{"textgrid.poem.36345": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Freier atmet schon die Brust,", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freier atmet schon die Brust,", "tokens": ["Frei\u00b7er", "at\u00b7met", "schon", "die", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00f6her schl\u00e4gt einsame Lust,", "tokens": ["H\u00f6\u00b7her", "schl\u00e4gt", "ein\u00b7sa\u00b7me", "Lust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "Friede ist es, was hier weht,", "tokens": ["Frie\u00b7de", "ist", "es", ",", "was", "hier", "weht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sanft zu innerm Herzen geht,", "tokens": ["Sanft", "zu", "in\u00b7nerm", "Her\u00b7zen", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df kein Schmerz da nimmer st\u00fcrmt,", "tokens": ["Da\u00df", "kein", "Schmerz", "da", "nim\u00b7mer", "st\u00fcrmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "ADV", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie sich Berg auf Berg ant\u00fcrmt,", "tokens": ["Wie", "sich", "Berg", "auf", "Berg", "an\u00b7t\u00fcrmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.7": {"text": "Hohes Schweigen uns ergreift,", "tokens": ["Ho\u00b7hes", "Schwei\u00b7gen", "uns", "er\u00b7greift", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Wildes Streben nicht mehr schweift,", "tokens": ["Wil\u00b7des", "Stre\u00b7ben", "nicht", "mehr", "schweift", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Hier auf stiller Alpenh\u00f6h',", "tokens": ["Hier", "auf", "stil\u00b7ler", "Al\u00b7pen\u00b7h\u00f6h'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Wo der fernen Gipfel Schnee,", "tokens": ["Wo", "der", "fer\u00b7nen", "Gip\u00b7fel", "Schnee", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "So die Sonne golden malt,", "tokens": ["So", "die", "Son\u00b7ne", "gol\u00b7den", "malt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Ernst zu uns hernieder strahlt.", "tokens": ["Ernst", "zu", "uns", "her\u00b7nie\u00b7der", "strahlt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPER", "PTKVZ", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Selig, wer da H\u00fctten baut,", "tokens": ["Se\u00b7lig", ",", "wer", "da", "H\u00fct\u00b7ten", "baut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWS", "ADV", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Einsam der Natur vertraut,", "tokens": ["Ein\u00b7sam", "der", "Na\u00b7tur", "ver\u00b7traut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Der Erinnerung nur lebt,", "tokens": ["Der", "E\u00b7rin\u00b7ne\u00b7rung", "nur", "lebt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.16": {"text": "Ganz sich selbst in sie vergr\u00e4bt,", "tokens": ["Ganz", "sich", "selbst", "in", "sie", "ver\u00b7gr\u00e4bt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Einzig auf das Lied nur denkt,", "tokens": ["Ein\u00b7zig", "auf", "das", "Lied", "nur", "denkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "Das ihm Gott ins Herz gesenkt,", "tokens": ["Das", "ihm", "Gott", "ins", "Herz", "ge\u00b7senkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Der den Dichter auserkor,", "tokens": ["Der", "den", "Dich\u00b7ter", "au\u00b7ser\u00b7kor", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.20": {"text": "Da\u00df er br\u00e4cht' ans Licht hervor", "tokens": ["Da\u00df", "er", "br\u00e4cht'", "ans", "Licht", "her\u00b7vor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "APPRART", "NN", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "Alten Heldengeistes Spur,", "tokens": ["Al\u00b7ten", "Hel\u00b7den\u00b7geis\u00b7tes", "Spur", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.22": {"text": "Stiller Sch\u00f6nheit Blumenflur,", "tokens": ["Stil\u00b7ler", "Sch\u00f6n\u00b7heit", "Blu\u00b7men\u00b7flur", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.23": {"text": "Fern von jener w\u00fcsten Welt,", "tokens": ["Fern", "von", "je\u00b7ner", "w\u00fcs\u00b7ten", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.24": {"text": "Die uns all' in Fesseln h\u00e4lt.", "tokens": ["Die", "uns", "all'", "in", "Fes\u00b7seln", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PIS", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.25": {"text": "M\u00f6cht' ich einst so gl\u00fccklich sein,", "tokens": ["M\u00f6cht'", "ich", "einst", "so", "gl\u00fcck\u00b7lich", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "ADJD", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.26": {"text": "Solchen Friedens mich zu freun,", "tokens": ["Sol\u00b7chen", "Frie\u00b7dens", "mich", "zu", "freun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.27": {"text": "Dieser sch\u00f6nen Berge H\u00f6hn", "tokens": ["Die\u00b7ser", "sch\u00f6\u00b7nen", "Ber\u00b7ge", "H\u00f6hn"], "token_info": ["word", "word", "word", "word"], "pos": ["PDAT", "ADJA", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.28": {"text": "Noch als Heimat wiedersehn!", "tokens": ["Noch", "als", "Hei\u00b7mat", "wie\u00b7der\u00b7sehn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}