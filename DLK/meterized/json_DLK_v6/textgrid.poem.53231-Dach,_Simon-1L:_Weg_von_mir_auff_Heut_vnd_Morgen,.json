{"textgrid.poem.53231": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Weg von mir auff Heut vnd Morgen,", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weg von mir auff Heut vnd Morgen,", "tokens": ["Weg", "von", "mir", "auff", "Heut", "vnd", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "O du bleiche Schaar der Sorgen,", "tokens": ["O", "du", "blei\u00b7che", "Schaar", "der", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Vnd du \u00f6de Trawrigheit!", "tokens": ["Vnd", "du", "\u00f6\u00b7de", "Traw\u00b7rig\u00b7heit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Anmuth, Liebe, Gn\u00fcge, Leben,", "tokens": ["An\u00b7muth", ",", "Lie\u00b7be", ",", "Gn\u00fc\u00b7ge", ",", "Le\u00b7ben", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Lachen vnd gew\u00fcnschte Zeit", "tokens": ["La\u00b7chen", "vnd", "ge\u00b7w\u00fcnschte", "Zeit"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "ADJA", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Sollen fr\u00f6lich vmb mich schweben,", "tokens": ["Sol\u00b7len", "fr\u00f6\u00b7lich", "vmb", "mich", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Weil der sechste Hornung mir", "tokens": ["Weil", "der", "sechs\u00b7te", "Hor\u00b7nung", "mir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Heilig soll gehalten werden,", "tokens": ["Hei\u00b7lig", "soll", "ge\u00b7hal\u00b7ten", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "VMFIN", "VVPP", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "So, der V\u00f6lcker Lust vnd Zier,", "tokens": ["So", ",", "der", "V\u00f6l\u00b7cker", "Lust", "vnd", "Zier", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Vnsern ", "tokens": ["Vn\u00b7sern"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.11": {"text": "Auff des Himmels trewen Raht", "tokens": ["Auff", "des", "Him\u00b7mels", "tre\u00b7wen", "Raht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Durch Geburt geschencket hat.", "tokens": ["Durch", "Ge\u00b7burt", "ge\u00b7schen\u00b7cket", "hat", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Prei\u00df der Tage, Wunsch der Frommen,", "tokens": ["Prei\u00df", "der", "Ta\u00b7ge", ",", "Wunsch", "der", "From\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meine Frewde, sey willkommen,", "tokens": ["Mei\u00b7ne", "Frew\u00b7de", ",", "sey", "will\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gr\u00f6sser noch ist deine Lust", "tokens": ["Gr\u00f6s\u00b7ser", "noch", "ist", "dei\u00b7ne", "Lust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "VAFIN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Heilger bist du recht zu sagen,", "tokens": ["Heil\u00b7ger", "bist", "du", "recht", "zu", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Als der Tag, der vor August", "tokens": ["Als", "der", "Tag", ",", "der", "vor", "Au\u00b7gust"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Hat an diese Welt getragen,", "tokens": ["Hat", "an", "die\u00b7se", "Welt", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PDAT", "NN", "VVPP", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.7": {"text": "Kein Gew\u00f6lcke, keine K\u00e4ltt'", "tokens": ["Kein", "Ge\u00b7w\u00f6l\u00b7cke", ",", "kei\u00b7ne", "K\u00e4ltt'"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Vnd kein Sturm soll dich betr\u00fcben,", "tokens": ["Vnd", "kein", "Sturm", "soll", "dich", "be\u00b7tr\u00fc\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Wenn du anbrichst, soll die Welt", "tokens": ["Wenn", "du", "an\u00b7brichst", ",", "soll", "die", "Welt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "VMFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Nichts sonst anders thun, als lieben,", "tokens": ["Nichts", "sonst", "an\u00b7ders", "thun", ",", "als", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "ADV", "ADV", "VVINF", "$,", "KOUS", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Weil du vnsre Lieb vnd Rast", "tokens": ["Weil", "du", "vns\u00b7re", "Lieb", "vnd", "Rast"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "An das Liecht gesetzest hast.", "tokens": ["An", "das", "Liecht", "ge\u00b7set\u00b7zest", "hast", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "O wie hat sich aller massen", "tokens": ["O", "wie", "hat", "sich", "al\u00b7ler", "mas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOKOM", "VAFIN", "PRF", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da der Himmel ausgelassen,", "tokens": ["Da", "der", "Him\u00b7mel", "aus\u00b7ge\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Seine Freundlicheit hat er", "tokens": ["Sei\u00b7ne", "Freund\u00b7li\u00b7cheit", "hat", "er"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Stromes-weis' vns zugegossen,", "tokens": ["Stro\u00b7mes\u00b7weis'", "vns", "zu\u00b7ge\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nectar ist von oben her", "tokens": ["Nec\u00b7tar", "ist", "von", "o\u00b7ben", "her"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "APPR", "ADV", "APZR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Auff die\u00df edle Land geflossen,", "tokens": ["Auff", "die\u00df", "ed\u00b7le", "Land", "ge\u00b7flos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDS", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Was ein Hertz auch w\u00fcnschen mag,", "tokens": ["Was", "ein", "Hertz", "auch", "w\u00fcn\u00b7schen", "mag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "ADV", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Balsam, Honig, Milch vnd Oele", "tokens": ["Bal\u00b7sam", ",", "Ho\u00b7nig", ",", "Milch", "vnd", "O\u00b7e\u00b7le"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.9": {"text": "Hat erquickt vmb selben Tag", "tokens": ["Hat", "er\u00b7quickt", "vmb", "sel\u00b7ben", "Tag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "VVPP", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Beydes vnser Leib und Seele,", "tokens": ["Bey\u00b7des", "vn\u00b7ser", "Leib", "und", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Wurden jung auff eine Zeit.", "tokens": ["Wur\u00b7den", "jung", "auff", "ei\u00b7ne", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Aller Wolstandt, der vns zieret,", "tokens": ["Al\u00b7ler", "Wol\u00b7standt", ",", "der", "vns", "zie\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Vnd vns t\u00e4glich new gebieret,", "tokens": ["Vnd", "vns", "t\u00e4g\u00b7lich", "new", "ge\u00b7bie\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJD", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Diese Rhue, der nichts gebricht,", "tokens": ["Die\u00b7se", "Rhue", ",", "der", "nichts", "ge\u00b7bricht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Vnd was sonst ist auserkohren", "tokens": ["Vnd", "was", "sonst", "ist", "au\u00b7ser\u00b7koh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "ADV", "VAFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ist mit vnserm Haupt vnd Liecht", "tokens": ["Ist", "mit", "vn\u00b7serm", "Haupt", "vnd", "Liecht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "PPOSAT", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "In was Hertzeleid' vnd Noht", "tokens": ["In", "was", "Hert\u00b7ze\u00b7leid'", "vnd", "Noht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "W\u00fcrden wir doch jetzund stecken,", "tokens": ["W\u00fcr\u00b7den", "wir", "doch", "je\u00b7tzund", "ste\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Drangsal, Dienstbarheit vnd Todt", "tokens": ["Dran\u00b7gsal", ",", "Dienst\u00b7bar\u00b7heit", "vnd", "Todt"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "W\u00fcrd' vns armes Volck erschrecken,", "tokens": ["W\u00fcrd'", "vns", "ar\u00b7mes", "Volck", "er\u00b7schre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "H\u00e4tte Gott vns dieses Pfandt", "tokens": ["H\u00e4t\u00b7te", "Gott", "vns", "die\u00b7ses", "Pfandt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "NN", "PPER", "PDAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Nicht aus Gnaden zugewandt.", "tokens": ["Nicht", "aus", "Gna\u00b7den", "zu\u00b7ge\u00b7wandt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wer nicht f\u00fcr Sein Heil vnd Leben", "tokens": ["Wer", "nicht", "f\u00fcr", "Sein", "Heil", "vnd", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PTKNEG", "APPR", "PPOSAT", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Heut dem H\u00f6chsten Danck wil geben,", "tokens": ["Heut", "dem", "H\u00f6chs\u00b7ten", "Danck", "wil", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Z\u00fcrnt auff diesen gutten Standt,", "tokens": ["Z\u00fcrnt", "auff", "die\u00b7sen", "gut\u00b7ten", "Standt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Hat an Angst vnd Noht gefallen", "tokens": ["Hat", "an", "Angst", "vnd", "Noht", "ge\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "NN", "KON", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Vnd ist vmb den Nilus-Strandt", "tokens": ["Vnd", "ist", "vmb", "den", "Ni\u00b7lus\u00b7Strandt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPR", "ART", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Wehrt im Elend vmbzuwallen,", "tokens": ["Wehrt", "im", "E\u00b7lend", "vmb\u00b7zu\u00b7wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Sol des T\u00fcrckschen Joches Last,", "tokens": ["Sol", "des", "T\u00fcrck\u00b7schen", "Jo\u00b7ches", "Last", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "NE", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Das sonst vnertr\u00e4glich, tragen,", "tokens": ["Das", "sonst", "vner\u00b7tr\u00e4g\u00b7lich", ",", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "ADV", "ADJD", "$,", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Da\u00df er nichts von Lieb vnd Rast,", "tokens": ["Da\u00df", "er", "nichts", "von", "Lieb", "vnd", "Rast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "So zwar vns kr\u00f6hnt, k\u00f6nne sagen,", "tokens": ["So", "zwar", "vns", "kr\u00b7\u00f6hnt", ",", "k\u00f6n\u00b7ne", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "VVFIN", "$,", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Vnd in wildster Barbarey", "tokens": ["Vnd", "in", "wilds\u00b7ter", "Bar\u00b7ba\u00b7rey"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Kenne weder Recht noch Trew.", "tokens": ["Ken\u00b7ne", "we\u00b7der", "Recht", "noch", "Trew", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "ADV", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Darumb, weil an diesem Tage", "tokens": ["Da\u00b7rumb", ",", "weil", "an", "die\u00b7sem", "Ta\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PAV", "$,", "KOUS", "APPR", "PDAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Seine Marck auch alle Klage", "to