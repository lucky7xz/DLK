: "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie ein Mensch gebohren werden,", "tokens": ["Wie", "ein", "Mensch", "ge\u00b7boh\u00b7ren", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVPP", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df sie dieser Sonnen Schein", "tokens": ["Da\u00df", "sie", "die\u00b7ser", "Son\u00b7nen", "Schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PDAT", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "M\u00f6chte sehn ohn Leid vnd Schmertzen,", "tokens": ["M\u00f6ch\u00b7te", "sehn", "ohn", "Leid", "vnd", "Schmert\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ihr Empf\u00e4ngnis w\u00fcrde seyn", "tokens": ["Ihr", "Emp\u00b7f\u00e4ng\u00b7nis", "w\u00fcr\u00b7de", "seyn"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "PPOSAT"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Vntter Deinem heilgen Hertzen.", "tokens": ["Vnt\u00b7ter", "Dei\u00b7nem", "heil\u00b7gen", "Hert\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Seelig ist wer sich ergetzt", "tokens": ["See\u00b7lig", "ist", "wer", "sich", "er\u00b7getzt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "PWS", "PRF", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Hie an Deinem strengen Leben,", "tokens": ["Hie", "an", "Dei\u00b7nem", "stren\u00b7gen", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Seelger der sich vorgesetzt", "tokens": ["Seel\u00b7ger", "der", "sich", "vor\u00b7ge\u00b7setzt"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "PRF", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Deiner Tugend nachzustreben,", "tokens": ["Dei\u00b7ner", "Tu\u00b7gend", "nach\u00b7zu\u00b7stre\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVIZU", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Die\u00df gesteh ich armer frey,", "tokens": ["Die\u00df", "ge\u00b7steh", "ich", "ar\u00b7mer", "frey", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Da\u00df es mir nicht m\u00fcglich sey.", "tokens": ["Da\u00df", "es", "mir", "nicht", "m\u00fcg\u00b7lich", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PTKNEG", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Jetzt la\u00df gn\u00e4digst Dir gefallen,", "tokens": ["Jetzt", "la\u00df", "gn\u00e4\u00b7digst", "Dir", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "VVFIN", "PPER", "VVPP", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Wenn das Schlos wird wiederschallen", "tokens": ["Wenn", "das", "Schlos", "wird", "wie\u00b7der\u00b7schal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "VAFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Von gew\u00fcnschtem Lust-geschrey,", "tokens": ["Von", "ge\u00b7w\u00fc