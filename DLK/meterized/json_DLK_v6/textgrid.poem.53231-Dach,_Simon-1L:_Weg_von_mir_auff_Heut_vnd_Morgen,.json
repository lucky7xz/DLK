 "measure": "trochaic.tetra"}, "line.5": {"text": "Vnd durch nicht gemeinen Thon", "tokens": ["Vnd", "durch", "nicht", "ge\u00b7mei\u00b7nen", "Thon"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PTKNEG", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Selbs die Wolcken zu erklimmen,", "tokens": ["Selbs", "die", "Wol\u00b7cken", "zu", "er\u00b7klim\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Mein Pohlihnchen trachtet sich", "tokens": ["Mein", "Poh\u00b7lihn\u00b7chen", "trach\u00b7tet", "sich"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Weis vnd sauber anzulegen,", "tokens": ["Weis", "vnd", "sau\u00b7ber", "an\u00b7zu\u00b7le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VVIZU", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Schm\u00fcckt die Kinder wie auch mich", "tokens": ["Schm\u00fcckt", "die", "Kin\u00b7der", "wie", "auch", "mich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "KOKOM", "ADV", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Dieses hohen Festes wegen,", "tokens": ["Die\u00b7ses", "ho\u00b7hen", "Fes\u00b7tes", "we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "APPR", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Ja, es putzt mein armes Hau\u00df", "tokens": ["Ja", ",", "es", "putzt", "mein", "ar\u00b7mes", "Hau\u00df"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Jetzt mit newem Pracht sich aus.", "tokens": ["Jetzt", "mit", "ne\u00b7wem", "Pracht", "sich", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "PRF", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.13": {"line.1": {"text": "Dir wird Gott zu seinen Ehren", "tokens": ["Dir", "wird", "Gott", "zu", "sei\u00b7nen", "Eh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deine Tag', o Held, vermehren,", "tokens": ["Dei\u00b7ne", "Tag'", ",", "o", "Held", ",", "ver\u00b7meh\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "FM", "NN", "$,", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Vnd Dein hohes Hau\u00df durch Dich,", "tokens": ["Vnd", "Dein", "ho\u00b7hes", "Hau\u00df", "durch", "Dich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn es Ihm gef\u00e4llt, auch bawen,", "tokens": ["Wenn", "es", "Ihm", "ge\u00b7f\u00e4llt", ",", "auch", "ba\u00b7wen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVPP", "$,", "ADV", "VVINF", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Wornach sehr vnd \u00e4ngstiglich", "tokens": ["Wor\u00b7nach", "sehr", "vnd", "\u00e4ngs\u00b7tig\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADV", "KON", "ADJD"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.6": {"text": "So viel tausend Seelen schawen,", "tokens": ["So", "viel", "tau\u00b7send", "See\u00b7len", "scha\u00b7wen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "CARD", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Dein Geburts-Fest, Herr, wirst Du", "tokens": ["Dein", "Ge\u00b7burts\u00b7Fest", ",", "Herr", ",", "wirst", "Du"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "NN", "$,", "VAFIN", "PPER"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.8": {"text": "Lang hernach in grawen Haaren", "tokens": ["Lang", "her\u00b7nach", "in", "gra\u00b7wen", "Haa\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "In gew\u00fcnschter Lust vnd Rhue", "tokens": ["In", "ge\u00b7w\u00fcnschter", "Lust", "vnd", "Rhue"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "NE"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.10": {"text": "Mitten in der Enckel Scharen", "tokens": ["Mit\u00b7ten", "in", "der", "En\u00b7ckel", "Scha\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "(gott la\u00df mich nicht schamroht stehn", "tokens": ["(", "gott", "la\u00df", "mich", "nicht", "scham\u00b7roht", "stehn"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "NN", "VVIMP", "PPER", "PTKNEG", "ADJD", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Vnd ein L\u00fcgner seyn!) begehn.", "tokens": ["Vnd", "ein", "L\u00fcg\u00b7ner", "seyn", "!", ")", "be\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["KON", "ART", "NN", "VAINF", "$.", "$(", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Zwar ich wei\u00df kein Band zu finden,", "tokens": ["Zwar", "ich", "wei\u00df", "kein", "Band", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PIAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deine gn\u00e4digst' Hand zu binden,", "tokens": ["Dei\u00b7ne", "gn\u00e4\u00b7digst'", "Hand", "zu", "bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn was ist Dein hoher Pracht,", "tokens": ["Denn", "was", "ist", "Dein", "ho\u00b7her", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Vnd dabey mein armes Wesen?", "tokens": ["Vnd", "da\u00b7bey", "mein", "ar\u00b7mes", "We\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Was aus Indien wird gebracht,", "tokens": ["Was", "aus", "In\u00b7di\u00b7en", "wird", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NE", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ist vor Dich nicht auserlesen,", "tokens": ["Ist", "vor", "Dich", "nicht", "au\u00b7ser\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Amor aber ist bem\u00fcht", "tokens": ["A\u00b7mor", "a\u00b7ber", "ist", "be\u00b7m\u00fcht"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ADV", "VAFIN", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Vmb ein Band vor Deinen Orden;", "tokens": ["Vmb", "ein", "Band", "vor", "Dei\u00b7nen", "Or\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Dieses fehlt nur, da\u00df er sieht,", "tokens": ["Die\u00b7ses", "fehlt", "nur", ",", "da\u00df", "er", "sieht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Ob er eines hol' aus Norden,", "tokens": ["Ob", "er", "ei\u00b7nes", "hol'", "aus", "Nor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Westen oder sonst wo her,", "tokens": ["Wes\u00b7ten", "o\u00b7der", "sonst", "wo", "her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADV", "PWAV", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Welches Deiner w\u00fcrdig wer.", "tokens": ["Wel\u00b7ches", "Dei\u00b7ner", "w\u00fcr\u00b7dig", "wer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "ADJD", "PWS", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}