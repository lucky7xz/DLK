{"textgrid.poem.33661": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Herbst", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was hab' ich nicht alles dem Himmel verziehn,", "tokens": ["Was", "hab'", "ich", "nicht", "al\u00b7les", "dem", "Him\u00b7mel", "ver\u00b7ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "PTKNEG", "PIS", "ART", "NN", "VVINF", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Nur weil er auf H\u00fcgeln die Rebe l\u00e4\u00dft bl\u00fchn!", "tokens": ["Nur", "weil", "er", "auf", "H\u00fc\u00b7geln", "die", "Re\u00b7be", "l\u00e4\u00dft", "bl\u00fchn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "APPR", "NN", "ART", "NN", "VVFIN", "VVINF", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Ich liebe den Herbst und im ersten Schnee", "tokens": ["Ich", "lie\u00b7be", "den", "Herbst", "und", "im", "ers\u00b7ten", "Schnee"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "APPRART", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Die Tannen im Nebel, im Berge den See.", "tokens": ["Die", "Tan\u00b7nen", "im", "Ne\u00b7bel", ",", "im", "Ber\u00b7ge", "den", "See", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Wo einsam im Winde die Blume sich neigt,", "tokens": ["Wo", "ein\u00b7sam", "im", "Win\u00b7de", "die", "Blu\u00b7me", "sich", "neigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "APPRART", "NN", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Die Heide nur dr\u00e4uende Wolken noch zeigt,", "tokens": ["Die", "Hei\u00b7de", "nur", "dr\u00e4u\u00b7en\u00b7de", "Wol\u00b7ken", "noch", "zeigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "ADV", "ADJA", "NN", "ADV", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Aus alternder Eichen zerkl\u00fcfteter Wucht", "tokens": ["Aus", "al\u00b7tern\u00b7der", "Ei\u00b7chen", "zer\u00b7kl\u00fcf\u00b7te\u00b7ter", "Wucht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Nur Raben umflattern die d\u00e4mmernde Schlucht,", "tokens": ["Nur", "Ra\u00b7ben", "um\u00b7flat\u00b7tern", "die", "d\u00e4m\u00b7mern\u00b7de", "Schlucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Da steigen mir \u00fcber den Felsen im Moor", "tokens": ["Da", "stei\u00b7gen", "mir", "\u00fc\u00b7ber", "den", "Fel\u00b7sen", "im", "Moor"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "APPRART", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Die G\u00f6tter der nordischen Sagen empor.", "tokens": ["Die", "G\u00f6t\u00b7ter", "der", "nor\u00b7di\u00b7schen", "Sa\u00b7gen", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "Sie schreiten und reiten um Berg und zu Tal,", "tokens": ["Sie", "schrei\u00b7ten", "und", "rei\u00b7ten", "um", "Berg", "und", "zu", "Tal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "In St\u00fcrmen auf Wolken zum Mitternachtmahl.", "tokens": ["In", "St\u00fcr\u00b7men", "auf", "Wol\u00b7ken", "zum", "Mit\u00b7ter\u00b7nacht\u00b7mahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Ich f\u00fcll' mir indessen den Becher mir Wein", "tokens": ["Ich", "f\u00fcll'", "mir", "in\u00b7des\u00b7sen", "den", "Be\u00b7cher", "mir", "Wein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ART", "NN", "PPER", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und lasse vergangen Vergangenheit sein!", "tokens": ["Und", "las\u00b7se", "ver\u00b7gan\u00b7gen", "Ver\u00b7gan\u00b7gen\u00b7heit", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJA", "NN", "VAINF", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Vor\u00fcber ist mancherlei, das uns gequ\u00e4lt,", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "ist", "man\u00b7cher\u00b7lei", ",", "das", "uns", "ge\u00b7qu\u00e4lt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Und war es auch bitter, es hat uns gest\u00e4hlt.", "tokens": ["Und", "war", "es", "auch", "bit\u00b7ter", ",", "es", "hat", "uns", "ge\u00b7st\u00e4hlt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ADJD", "$,", "PPER", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "Die st\u00fcrmischen Tage des Lebens vergehn,", "tokens": ["Die", "st\u00fcr\u00b7mi\u00b7schen", "Ta\u00b7ge", "des", "Le\u00b7bens", "ver\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.8": {"text": "Wie jagende Wolken am Himmel hin wehn.", "tokens": ["Wie", "ja\u00b7gen\u00b7de", "Wol\u00b7ken", "am", "Him\u00b7mel", "hin", "wehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "APPRART", "NN", "ADV", "VVINF", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.4": {"line.1": {"text": "Und alles sei freudig dem Himmel verziehn,", "tokens": ["Und", "al\u00b7les", "sei", "freu\u00b7dig", "dem", "Him\u00b7mel", "ver\u00b7ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Nur weil er auf H\u00fcgeln die Rebe l\u00e4\u00dft bl\u00fchn!", "tokens": ["Nur", "weil", "er", "auf", "H\u00fc\u00b7geln", "die", "Re\u00b7be", "l\u00e4\u00dft", "bl\u00fchn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "APPR", "NN", "ART", "NN", "VVFIN", "VVINF", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}}}}