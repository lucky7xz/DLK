{"textgrid.poem.37383": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Die Trud", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wahrlich, sagte meine Tante,", "tokens": ["Wahr\u00b7lich", ",", "sag\u00b7te", "mei\u00b7ne", "Tan\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die fast alle Geister kannte,", "tokens": ["Die", "fast", "al\u00b7le", "Geis\u00b7ter", "kann\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Keine T\u00e4uschung ist die Trud.", "tokens": ["Kei\u00b7ne", "T\u00e4u\u00b7schung", "ist", "die", "Trud", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wei\u00dft du nicht, da\u00df b\u00f6se Seelen", "tokens": ["Wei\u00dft", "du", "nicht", ",", "da\u00df", "b\u00f6\u00b7se", "See\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,", "KOUS", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "N\u00e4chtlich aus dem Leibe r\u00fccken,", "tokens": ["N\u00e4cht\u00b7lich", "aus", "dem", "Lei\u00b7be", "r\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Um den Menschen zu bedr\u00fccken", "tokens": ["Um", "den", "Men\u00b7schen", "zu", "be\u00b7dr\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und zu treten und zu qu\u00e4len,", "tokens": ["Und", "zu", "tre\u00b7ten", "und", "zu", "qu\u00e4\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wenn er auf dem R\u00fccken ruht?", "tokens": ["Wenn", "er", "auf", "dem", "R\u00fc\u00b7cken", "ruht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Lautlos durch verschloss'ne T\u00fcren", "tokens": ["Laut\u00b7los", "durch", "ver\u00b7schloss'\u00b7ne", "T\u00fc\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Immer n\u00e4her siehst du's kommen,", "tokens": ["Im\u00b7mer", "n\u00e4\u00b7her", "siehst", "du's", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Zauberhaft und wunderlich.", "tokens": ["Zau\u00b7ber\u00b7haft", "und", "wun\u00b7der\u00b7lich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Und dir graust es vor dem Dinge,", "tokens": ["Und", "dir", "graust", "es", "vor", "dem", "Din\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Und du kannst dich doch nicht r\u00fchren,", "tokens": ["Und", "du", "kannst", "dich", "doch", "nicht", "r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Und du f\u00fchlst dich so beklommen,", "tokens": ["Und", "du", "f\u00fchlst", "dich", "so", "be\u00b7klom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "M\u00f6chtest rufen, wenn's nur ginge,", "tokens": ["M\u00f6ch\u00b7test", "ru\u00b7fen", ",", "wenn's", "nur", "gin\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "$,", "KOUS", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Und auf einmal hat es dich.", "tokens": ["Und", "auf", "ein\u00b7mal", "hat", "es", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADV", "VAFIN", "PPER", "PRF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Doch wer klug, wei\u00df sich zu sch\u00fctzen:", "tokens": ["Doch", "wer", "klug", ",", "wei\u00df", "sich", "zu", "sch\u00fct\u00b7zen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADJD", "$,", "VVFIN", "PRF", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Abends beim Zurruhegehn", "tokens": ["A\u00b7bends", "beim", "Zur\u00b7ru\u00b7he\u00b7gehn"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Brauchst du blo\u00df darauf zu sehn,", "tokens": ["Brauchst", "du", "blo\u00df", "da\u00b7rauf", "zu", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PAV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.20": {"text": "Da\u00df die Schuhe mit den Spitzen", "tokens": ["Da\u00df", "die", "Schu\u00b7he", "mit", "den", "Spit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Abgewandt vom Bette stehn.", "tokens": ["Ab\u00b7ge\u00b7wandt", "vom", "Bet\u00b7te", "stehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.22": {"text": "Au\u00dferdem hab ich geh\u00f6rt:", "tokens": ["Au\u00b7\u00dfer\u00b7dem", "hab", "ich", "ge\u00b7h\u00f6rt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "VVFIN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.23": {"text": "Leichtes Herz und leichter Magen,", "tokens": ["Leich\u00b7tes", "Herz", "und", "leich\u00b7ter", "Ma\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Wie in andern Lebenslagen", "tokens": ["Wie", "in", "an\u00b7dern", "Le\u00b7bens\u00b7la\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Sind auch hier empfehlenswert.", "tokens": ["Sind", "auch", "hier", "emp\u00b7feh\u00b7lens\u00b7wert", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}