{"dta.poem.9320": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "F\u00fcnffte Handlung.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Bottes schlu\u00df bleibt ungebrochen/", "tokens": ["Bot\u00b7tes", "schlu\u00df", "bleibt", "un\u00b7ge\u00b7bro\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ADJD", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Er hat dir einmahl versprochen/", "tokens": ["Er", "hat", "dir", "ein\u00b7mahl", "ver\u00b7spro\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVINF", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Gn\u00e4dig und bereit zu seyn/", "tokens": ["Gn\u00e4\u00b7dig", "und", "be\u00b7reit", "zu", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "PTKZU", "VAINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Drum so klopffe mit den h\u00e4nden/", "tokens": ["Drum", "so", "klopf\u00b7fe", "mit", "den", "h\u00e4n\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "VVFIN", "APPR", "ART", "ADJA", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und nimm auch an allen enden", "tokens": ["Und", "nimm", "auch", "an", "al\u00b7len", "en\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "ADV", "APPR", "PIAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Fr\u00fcchte von dem seegen ein.", "tokens": ["Fr\u00fcch\u00b7te", "von", "dem", "see\u00b7gen", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Der himmel ist fr\u00f6lich und zeiget die blicke", "tokens": ["Der", "him\u00b7mel", "ist", "fr\u00f6\u00b7lich", "und", "zei\u00b7get", "die", "bli\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "VVFIN", "ART", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.8": {"text": "Mit dreyfach-ja tausendfach sch\u00f6nern gel\u00fccke.", "tokens": ["Mit", "dreyfach\u00b7ja", "tau\u00b7send\u00b7fach", "sch\u00f6\u00b7nern", "ge\u00b7l\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "ADV", "ADJA", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}}}}