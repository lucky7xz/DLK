{"textgrid.poem.53665": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Berliner Sonntag", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und Gott schickt \u00fcber uns das Wochenende.", "tokens": ["Und", "Gott", "schickt", "\u00fc\u00b7ber", "uns", "das", "Wo\u00b7chen\u00b7en\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da legt Berlin in manchen Scho\u00df die H\u00e4nde", "tokens": ["Da", "legt", "Ber\u00b7lin", "in", "man\u00b7chen", "Scho\u00df", "die", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NE", "APPR", "PIAT", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "und ruht sich aus.", "tokens": ["und", "ruht", "sich", "aus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Herr Stegerwald dehnt sich in seinen Kissen \u2013", "tokens": ["Herr", "Ste\u00b7ger\u00b7wald", "dehnt", "sich", "in", "sei\u00b7nen", "Kis\u00b7sen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "heut braucht er weniger noch als einst zu wissen", "tokens": ["heut", "braucht", "er", "we\u00b7ni\u00b7ger", "noch", "als", "einst", "zu", "wis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "KOKOM", "ADV", "PTKZU", "VVINF"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "vom Landtagshaus.", "tokens": ["vom", "Land\u00b7tags\u00b7haus", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Nur Ge\u00dfler will die Wochenarbeit machen:", "tokens": ["Nur", "Ge\u00df\u00b7ler", "will", "die", "Wo\u00b7chen\u00b7ar\u00b7beit", "ma\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "und schlummert sanft. Und alle Leutnants lachen.", "tokens": ["und", "schlum\u00b7mert", "sanft", ".", "Und", "al\u00b7le", "Leut\u00b7nants", "la\u00b7chen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$.", "KON", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "In eine Judenfahne siehst du schneuzen", "tokens": ["In", "ei\u00b7ne", "Ju\u00b7den\u00b7fah\u00b7ne", "siehst", "du", "schneu\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Herrn Wulle \u2013 und er ruht auf Hakenkreuzen.", "tokens": ["Herrn", "Wul\u00b7le", "\u2013", "und", "er", "ruht", "auf", "Ha\u00b7ken\u00b7kreu\u00b7zen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$(", "KON", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Kein Pressechef hat heut zu tun.", "tokens": ["Kein", "Pres\u00b7se\u00b7chef", "hat", "heut", "zu", "tun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Die Enten quaken fern:", "tokens": ["Die", "En\u00b7ten", "qua\u00b7ken", "fern", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Am heiligen Sonntag sollst du ruhn \u2013", "tokens": ["Am", "hei\u00b7li\u00b7gen", "Sonn\u00b7tag", "sollst", "du", "ruhn", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VMFIN", "PPER", "VVINF", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Dies ist der Tag des Herrn!", "tokens": ["Dies", "ist", "der", "Tag", "des", "Herrn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und alle Rotters sind zum Lunch versammelt.", "tokens": ["Und", "al\u00b7le", "Rot\u00b7ters", "sind", "zum", "Lunch", "ver\u00b7sam\u00b7melt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Denn ihre L\u00e4den sind mit Recht gerammelt", "tokens": ["Denn", "ih\u00b7re", "L\u00e4\u00b7den", "sind", "mit", "Recht", "ge\u00b7ram\u00b7melt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "APPR", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "und proppenvoll.", "tokens": ["und", "prop\u00b7pen\u00b7voll", "."], "token_info": ["word", "word", "punct"], "pos": ["KON", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Wer m\u00f6chte sonntags Mime in Berlin sein?", "tokens": ["Wer", "m\u00f6ch\u00b7te", "sonn\u00b7tags", "Mi\u00b7me", "in", "Ber\u00b7lin", "sein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "NN", "APPR", "NE", "VAINF", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Selbst Bassermann mu\u00df zwei Mal auf dem Kean sein \u2013", "tokens": ["Selbst", "Bas\u00b7ser\u00b7mann", "mu\u00df", "zwei", "Mal", "auf", "dem", "Ke\u00b7an", "sein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "VMFIN", "CARD", "NN", "APPR", "ART", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "verdammt, Apoll!", "tokens": ["ver\u00b7dammt", ",", "A\u00b7poll", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVPP", "$,", "NE", "$."], "meter": "--+-", "measure": "anapaest.init"}, "line.7": {"text": "Hollaender spricht, bis sich Amt Norden hei\u00df l\u00e4uft.", "tokens": ["Hol\u00b7la\u00b7en\u00b7der", "spricht", ",", "bis", "sich", "Amt", "Nor\u00b7den", "hei\u00df", "l\u00e4uft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "KOUS", "PRF", "NN", "NN", "ADJD", "VVFIN", "$."], "meter": "-+--+---+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Der dicke Nelson spielt bis ihm der Schwei\u00df l\u00e4uft.", "tokens": ["Der", "di\u00b7cke", "Nel\u00b7son", "spielt", "bis", "ihm", "der", "Schwei\u00df", "l\u00e4uft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Die Kinos schnurrn, da\u00df sich die Kassen biegen.", "tokens": ["Die", "Ki\u00b7nos", "schnurrn", ",", "da\u00df", "sich", "die", "Kas\u00b7sen", "bie\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KOUS", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Die blonde Emmy bleibt im Bett gleich liegen.", "tokens": ["Die", "blon\u00b7de", "Em\u00b7my", "bleibt", "im", "Bett", "gleich", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "VVFIN", "APPRART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Denn zuviel Liebe macht immun,", "tokens": ["Denn", "zu\u00b7viel", "Lie\u00b7be", "macht", "im\u00b7mun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "und deshalb schl\u00e4ft sie gern.", "tokens": ["und", "des\u00b7halb", "schl\u00e4ft", "sie", "gern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Am heiligen Sonntag sollst du ruhn \u2013", "tokens": ["Am", "hei\u00b7li\u00b7gen", "Sonn\u00b7tag", "sollst", "du", "ruhn", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VMFIN", "PPER", "VVINF", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Dies ist der Tag des Herrn!", "tokens": ["Dies", "ist", "der", "Tag", "des", "Herrn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "In allen H\u00e4usern pruzzelt Mittagessen.", "tokens": ["In", "al\u00b7len", "H\u00e4u\u00b7sern", "pruz\u00b7zelt", "Mit\u00b7ta\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das St\u00fcck Emilie hat das Obst vergessen \u2013", "tokens": ["Das", "St\u00fcck", "E\u00b7mi\u00b7lie", "hat", "das", "Obst", "ver\u00b7ges\u00b7sen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VAFIN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "die Hausfrau tobt.", "tokens": ["die", "Haus\u00b7frau", "tobt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Und unter ihren Tisch mit Sauers\u00fc\u00dfe", "tokens": ["Und", "un\u00b7ter", "ih\u00b7ren", "Tisch", "mit", "Sau\u00b7er\u00b7s\u00fc\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "streckt die Familie ihre gro\u00dfen F\u00fc\u00dfe \u2013", "tokens": ["streckt", "die", "Fa\u00b7mi\u00b7lie", "ih\u00b7re", "gro\u00b7\u00dfen", "F\u00fc\u00b7\u00dfe", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPOSAT", "ADJA", "NN", "$("], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Gott sei gelobt!", "tokens": ["Gott", "sei", "ge\u00b7lobt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Max schreit, weil Susi Onkel Hans verteidigt,", "tokens": ["Max", "schreit", ",", "weil", "Su\u00b7si", "On\u00b7kel", "Hans", "ver\u00b7tei\u00b7digt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "KOUS", "NE", "NN", "NE", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "die Tante Lo ist \u00fcberhaupt beleidigt . . .", "tokens": ["die", "Tan\u00b7te", "Lo", "ist", "\u00fc\u00b7ber\u00b7haupt", "be\u00b7lei\u00b7digt", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "NE", "VAFIN", "ADV", "VVPP", "$.", "$.", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Die Stimmung w\u00e4chst mit jedem Widerstande;", "tokens": ["Die", "Stim\u00b7mung", "w\u00e4chst", "mit", "je\u00b7dem", "Wi\u00b7der\u00b7stan\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "hier siehst du, was das hei\u00dft: Familienbande.", "tokens": ["hier", "siehst", "du", ",", "was", "das", "hei\u00dft", ":", "Fa\u00b7mi\u00b7li\u00b7en\u00b7ban\u00b7de", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWS", "PDS", "VVFIN", "$.", "NN", "$."], "meter": "-+-+---+--+-", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "So feiert diese Menschheit nun", "tokens": ["So", "fei\u00b7ert", "die\u00b7se", "Menschheit", "nun"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PDAT", "NN", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "auf unserm Erdenstern . . .", "tokens": ["auf", "un\u00b7serm", "Er\u00b7dens\u00b7tern", ".", ".", "."], "token_info": ["word", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$.", "$.", "$."], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.13": {"text": "Am heiligen Sonntag sollst du ruhn \u2013", "tokens": ["Am", "hei\u00b7li\u00b7gen", "Sonn\u00b7tag", "sollst", "du", "ruhn", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VMFIN", "PPER", "VVINF", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Dies ist der Tag des Herrn!", "tokens": ["Dies", "ist", "der", "Tag", "des", "Herrn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}