{"textgrid.poem.39797": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Zueignung des T\u00fcbinger \u00bbNeuen allgemein deutschen Commers- und Liederbuchs\u00ab von 1815", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du bist es, der wir diese Lieder bringen,", "tokens": ["Du", "bist", "es", ",", "der", "wir", "die\u00b7se", "Lie\u00b7der", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$,", "PRELS", "PPER", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wir gr\u00fc\u00dfen dich, du edle Burschenschaft!", "tokens": ["Wir", "gr\u00fc\u00b7\u00dfen", "dich", ",", "du", "ed\u00b7le", "Bur\u00b7schen\u00b7schaft", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Auch du bist wieder frei, darfst wieder singen", "tokens": ["Auch", "du", "bist", "wie\u00b7der", "frei", ",", "darfst", "wie\u00b7der", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PPER", "VAFIN", "ADV", "ADJD", "$,", "PAV", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein keckes Wort bei deutschem Rebensaft;", "tokens": ["Ein", "ke\u00b7ckes", "Wort", "bei", "deut\u00b7schem", "Re\u00b7ben\u00b7saft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Zechst du in Frieden, probst du deine Klingen,", "tokens": ["Zechst", "du", "in", "Frie\u00b7den", ",", "probst", "du", "dei\u00b7ne", "Klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$,", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Schl\u00e4gt dir das Herz in k\u00fchner Liebe Kraft,", "tokens": ["Schl\u00e4gt", "dir", "das", "Herz", "in", "k\u00fch\u00b7ner", "Lie\u00b7be", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "So soll dich unser freud'ger Klang umschweben,", "tokens": ["So", "soll", "dich", "un\u00b7ser", "freu\u00b7d'\u00b7ger", "Klang", "um\u00b7schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Begleiten durch dein freies Jugendleben.", "tokens": ["Be\u00b7glei\u00b7ten", "durch", "dein", "frei\u00b7es", "Ju\u00b7gend\u00b7le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Auch gilt es euch, die ihr dem Burschenorden", "tokens": ["Auch", "gilt", "es", "euch", ",", "die", "ihr", "dem", "Bur\u00b7schen\u00b7or\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$,", "PRELS", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "L\u00e4ngst abgesagt, in weiter Welt verstreut;", "tokens": ["L\u00e4ngst", "ab\u00b7ge\u00b7sagt", ",", "in", "wei\u00b7ter", "Welt", "ver\u00b7streut", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Was ihr auch seid, und was aus euch geworden,", "tokens": ["Was", "ihr", "auch", "seid", ",", "und", "was", "aus", "euch", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VAFIN", "$,", "KON", "PWS", "APPR", "PPER", "VAPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Staatsb\u00fcrger, K\u00e4mpfer in dem heil'gen Streit:", "tokens": ["Staats\u00b7b\u00fcr\u00b7ger", ",", "K\u00e4mp\u00b7fer", "in", "dem", "heil'\u00b7gen", "Streit", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Euch lieben Br\u00fcdern aus dem deutschen Norden,", "tokens": ["Euch", "lie\u00b7ben", "Br\u00fc\u00b7dern", "aus", "dem", "deut\u00b7schen", "Nor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Euch, die der Alpen Heimatland erfreut,", "tokens": ["Euch", ",", "die", "der", "Al\u00b7pen", "Hei\u00b7mat\u00b7land", "er\u00b7freut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Euch braven Franken, und euch tapfern Schwaben,", "tokens": ["Euch", "bra\u00b7ven", "Fran\u00b7ken", ",", "und", "euch", "tap\u00b7fern", "Schwa\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "KON", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die hier gek\u00e4mpft, geliebt, gesungen haben.", "tokens": ["Die", "hier", "ge\u00b7k\u00e4mpft", ",", "ge\u00b7liebt", ",", "ge\u00b7sun\u00b7gen", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADV", "VVPP", "$,", "VVPP", "$,", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Beschw\u00f6rt herauf die theuer werten Zeiten", "tokens": ["Be\u00b7schw\u00f6rt", "her\u00b7auf", "die", "theu\u00b7er", "wer\u00b7ten", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit dieses B\u00fcchleins mannigfachem Sang;", "tokens": ["Mit", "die\u00b7ses", "B\u00fcch\u00b7leins", "man\u00b7nig\u00b7fa\u00b7chem", "Sang", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "PIAT", "NN", "$."], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.3": {"text": "Den Bruderku\u00df, der Gl\u00e4ser helles L\u00e4uten,", "tokens": ["Den", "Bru\u00b7der\u00b7ku\u00df", ",", "der", "Gl\u00e4\u00b7ser", "hel\u00b7les", "L\u00e4u\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und auf dem Tisch der Schl\u00e4ger m\u00e4cht'gen Klang;", "tokens": ["Und", "auf", "dem", "Tisch", "der", "Schl\u00e4\u00b7ger", "m\u00e4cht'\u00b7gen", "Klang", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ja selbst die Menge s\u00fc\u00dfer Albernheiten,", "tokens": ["Ja", "selbst", "die", "Men\u00b7ge", "s\u00fc\u00b7\u00dfer", "Al\u00b7bern\u00b7hei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die trunkner Wahnsinn in die Runde sang:", "tokens": ["Die", "trunk\u00b7ner", "Wahn\u00b7sinn", "in", "die", "Run\u00b7de", "sang", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Kommt, alte Br\u00fcder, singt, was ihr gesungen;", "tokens": ["Kommt", ",", "al\u00b7te", "Br\u00fc\u00b7der", ",", "singt", ",", "was", "ihr", "ge\u00b7sun\u00b7gen", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$,", "VVFIN", "$,", "PWS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und ihr heran, und lernt den Brauch, ihr Jungen!", "tokens": ["Und", "ihr", "he\u00b7ran", ",", "und", "lernt", "den", "Brauch", ",", "ihr", "Jun\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "PTKVZ", "$,", "KON", "VVFIN", "ART", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}