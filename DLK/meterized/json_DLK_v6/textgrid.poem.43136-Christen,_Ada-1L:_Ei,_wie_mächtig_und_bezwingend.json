{"textgrid.poem.43136": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ei, wie m\u00e4chtig und bezwingend", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ei, wie m\u00e4chtig und bezwingend", "tokens": ["Ei", ",", "wie", "m\u00e4ch\u00b7tig", "und", "be\u00b7zwin\u00b7gend"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "ADJD", "KON", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "D\u00fcnkt Euch fast ein einzig Wort,", "tokens": ["D\u00fcnkt", "Euch", "fast", "ein", "ein\u00b7zig", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJD", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Glaubt Ihr wohl, es nehme pl\u00f6tzlich", "tokens": ["Glaubt", "Ihr", "wohl", ",", "es", "neh\u00b7me", "pl\u00f6tz\u00b7lich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "$,", "PPER", "VVFIN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jahrelanges Elend fort?", "tokens": ["Jah\u00b7re\u00b7lan\u00b7ges", "E\u00b7lend", "fort", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ei, versucht des Wortes Allmacht", "tokens": ["Ei", ",", "ver\u00b7sucht", "des", "Wor\u00b7tes", "All\u00b7macht"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "An dem Meer, das wild emp\u00f6rt,", "tokens": ["An", "dem", "Meer", ",", "das", "wild", "em\u00b7p\u00f6rt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Sturmgepeitscht so d\u00fcster grollet,", "tokens": ["Sturm\u00b7ge\u00b7peitscht", "so", "d\u00fcs\u00b7ter", "grol\u00b7let", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ob es Euer Wort beschw\u00f6rt.", "tokens": ["Ob", "es", "Eu\u00b7er", "Wort", "be\u00b7schw\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Und Ihr w\u00e4hnt, das Herz, das wilde,", "tokens": ["Und", "Ihr", "w\u00e4hnt", ",", "das", "Herz", ",", "das", "wil\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Das die Bitterkeit gest\u00e4hlt,", "tokens": ["Das", "die", "Bit\u00b7ter\u00b7keit", "ge\u00b7st\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Macht ein mildes Wort vergessen,", "tokens": ["Macht", "ein", "mil\u00b7des", "Wort", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Wie Ihr es gepeitscht \u2013 gequ\u00e4lt?!", "tokens": ["Wie", "Ihr", "es", "ge\u00b7peitscht", "\u2013", "ge\u00b7qu\u00e4lt", "?!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "VVPP", "$(", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}