{"dta.poem.20639": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Der sichere Fromme .  \n Aus einer Predigt des Herrn Ober-Consistorialrath  \n  Spalding .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer nie der sonnenhellen Wahrheit widerstrebt                 ", "tokens": ["Wer", "nie", "der", "son\u00b7nen\u00b7hel\u00b7len", "Wahr\u00b7heit", "wi\u00b7der\u00b7strebt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und stets in unver\u00e4nderlicher", "tokens": ["Und", "stets", "in", "un\u00b7ver\u00b7\u00e4n\u00b7der\u00b7li\u00b7cher"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Rechtschaffenheit und Tugend lebt,", "tokens": ["Recht\u00b7schaf\u00b7fen\u00b7heit", "und", "Tu\u00b7gend", "lebt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Derselbe Mensch lebt sicher.", "tokens": ["Der\u00b7sel\u00b7be", "Mensch", "lebt", "si\u00b7cher", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es m\u00f6gen \u00fcber ihm die Himmel, und umher", "tokens": ["Es", "m\u00f6\u00b7gen", "\u00fc\u00b7ber", "ihm", "die", "Him\u00b7mel", ",", "und", "um\u00b7her"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "PPER", "ART", "NN", "$,", "KON", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die feuerschwangern Berge krachen,", "tokens": ["Die", "feu\u00b7er\u00b7schwan\u00b7gern", "Ber\u00b7ge", "kra\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Es mag das ausgeri\u00dfne Meer", "tokens": ["Es", "mag", "das", "aus\u00b7ge\u00b7ri\u00df\u00b7ne", "Meer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Noch eine S\u00fcndfluth machen;", "tokens": ["Noch", "ei\u00b7ne", "S\u00fcnd\u00b7fluth", "ma\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der Sturmwind tobe, da\u00df der tausendj\u00e4hrge Wald", "tokens": ["Der", "Sturm\u00b7wind", "to\u00b7be", ",", "da\u00df", "der", "tau\u00b7send\u00b7j\u00e4hr\u00b7ge", "Wald"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und selbst die Felsen niedersinken;", "tokens": ["Und", "selbst", "die", "Fel\u00b7sen", "nie\u00b7der\u00b7sin\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das Erdreich berste Spalt an Spalt", "tokens": ["Das", "Er\u00b7dreich", "bers\u00b7te", "Spalt", "an", "Spalt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "APPR", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Zur Rechten und zur Linken;", "tokens": ["Zur", "Rech\u00b7ten", "und", "zur", "Lin\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Der Seuchen fliegend Gift und das gefr\u00e4\u00dfge Schwert", "tokens": ["Der", "Seu\u00b7chen", "flie\u00b7gend", "Gift", "und", "das", "ge\u00b7fr\u00e4\u00df\u00b7ge", "Schwert"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "NN", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des Krieges, m\u00f6gen pl\u00f6tzlich kommen:", "tokens": ["Des", "Krie\u00b7ges", ",", "m\u00f6\u00b7gen", "pl\u00f6tz\u00b7lich", "kom\u00b7men", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VMFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das Schrecken und die Zagheit f\u00e4hrt", "tokens": ["Das", "Schre\u00b7cken", "und", "die", "Zag\u00b7heit", "f\u00e4hrt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht in die Brust des Frommen.", "tokens": ["Nicht", "in", "die", "Brust", "des", "From\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Denn ihn besch\u00fctzet, st\u00e4rket, muntert und erh\u00e4lt", "tokens": ["Denn", "ihn", "be\u00b7sch\u00fct\u00b7zet", ",", "st\u00e4r\u00b7ket", ",", "mun\u00b7tert", "und", "er\u00b7h\u00e4lt"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gott, der des Christen Werke z\u00e4hlet,", "tokens": ["Gott", ",", "der", "des", "Chris\u00b7ten", "Wer\u00b7ke", "z\u00e4h\u00b7let", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Wenn ihm der Beyfall einer Welt", "tokens": ["Wenn", "ihm", "der", "Bey\u00b7fall", "ei\u00b7ner", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bey seiner Tugend fehlet.", "tokens": ["Bey", "sei\u00b7ner", "Tu\u00b7gend", "feh\u00b7let", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}