{"textgrid.poem.52729": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Sickingens Verm\u00e4chtni\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einst lag die Welt in Nacht befangen,", "tokens": ["Einst", "lag", "die", "Welt", "in", "Nacht", "be\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Kein hell Gestirn war aufgegangen,", "tokens": ["Kein", "hell", "Ge\u00b7stirn", "war", "auf\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJD", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nur eine matte Sternenpracht", "tokens": ["Nur", "ei\u00b7ne", "mat\u00b7te", "Ster\u00b7nen\u00b7pracht"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hinflimmerte durch tiefe Nacht.", "tokens": ["Hin\u00b7flim\u00b7mer\u00b7te", "durch", "tie\u00b7fe", "Nacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Jetzt ist die Leuchte angez\u00fcndet,", "tokens": ["Jetzt", "ist", "die", "Leuch\u00b7te", "an\u00b7ge\u00b7z\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Tag erstritten und verk\u00fcndet,", "tokens": ["Der", "Tag", "er\u00b7strit\u00b7ten", "und", "ver\u00b7k\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und nur ein Uebel wolkendicht", "tokens": ["Und", "nur", "ein", "Ue\u00b7bel", "wol\u00b7ken\u00b7dicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verd\u00e4mmert uns das Sonnenlicht.", "tokens": ["Ver\u00b7d\u00e4m\u00b7mert", "uns", "das", "Son\u00b7nen\u00b7licht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Jedweder sieht die Nacht gelichtet,", "tokens": ["Jed\u00b7we\u00b7der", "sieht", "die", "Nacht", "ge\u00b7lich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zur Sonn ist jeder Blick gerichtet,", "tokens": ["Zur", "Sonn", "ist", "je\u00b7der", "Blick", "ge\u00b7rich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wir harren, ob der Nebel sinkt,", "tokens": ["Wir", "har\u00b7ren", ",", "ob", "der", "Ne\u00b7bel", "sinkt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Strahl erw\u00e4rmend zu uns dringt.", "tokens": ["Der", "Strahl", "er\u00b7w\u00e4r\u00b7mend", "zu", "uns", "dringt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wir sp\u00e4hen, tappen, rathen, suchen,", "tokens": ["Wir", "sp\u00e4\u00b7hen", ",", "tap\u00b7pen", ",", "ra\u00b7then", ",", "su\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVINF", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Es geht ein Beten und ein Fluchen", "tokens": ["Es", "geht", "ein", "Be\u00b7ten", "und", "ein", "Flu\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Umsonst besteigen wir den Thurm,", "tokens": ["Um\u00b7sonst", "be\u00b7stei\u00b7gen", "wir", "den", "Thurm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Eins thut uns noth \u2013 es ist der Sturm.", "tokens": ["Eins", "thut", "uns", "noth", "\u2013", "es", "ist", "der", "Sturm", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "NN", "$(", "PPER", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}