{"dta.poem.11815": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "101.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Angler sitzt am Strom und angelt ohne Zahl", "tokens": ["Der", "Ang\u00b7ler", "sitzt", "am", "Strom", "und", "an\u00b7gelt", "oh\u00b7ne", "Zahl"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "KON", "VVPP", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was er erangeln kann von Fischen breit und schmal.", "tokens": ["Was", "er", "e\u00b7ran\u00b7geln", "kann", "von", "Fi\u00b7schen", "breit", "und", "schmal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVINF", "VMFIN", "APPR", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Er angelt sie heraus und zittert nicht einmal,", "tokens": ["Er", "an\u00b7gelt", "sie", "he\u00b7raus", "und", "zit\u00b7tert", "nicht", "ein\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn er sie zappeln sieht am Land im Sonnenstral.", "tokens": ["Wenn", "er", "sie", "zap\u00b7peln", "sieht", "am", "Land", "im", "Son\u00b7nen\u00b7stral", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVINF", "VVFIN", "APPRART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Da zittert in der Hand die Ruth' ihm doch einmal,", "tokens": ["Da", "zit\u00b7tert", "in", "der", "Hand", "die", "Ruth'", "ihm", "doch", "ein\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil angebissen hat am Fang ein Zitteraal.", "tokens": ["Weil", "an\u00b7ge\u00b7bis\u00b7sen", "hat", "am", "Fang", "ein", "Zit\u00b7te\u00b7raal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVPP", "VAFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Er ruft: du willst umsonst das Handwerk mir verbittern,", "tokens": ["Er", "ruft", ":", "du", "willst", "um\u00b7sonst", "das", "Hand\u00b7werk", "mir", "ver\u00b7bit\u00b7tern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VMFIN", "ADV", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "O Zitterer, du mu\u00dft heraus trotz allem Zittern.", "tokens": ["O", "Zit\u00b7te\u00b7rer", ",", "du", "mu\u00dft", "he\u00b7raus", "trotz", "al\u00b7lem", "Zit\u00b7tern", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "VMFIN", "ADV", "APPR", "PIS", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Das Sprichwort sagt: Es hilft kein Zittern vor dem Frost,", "tokens": ["Das", "Sprich\u00b7wort", "sagt", ":", "Es", "hilft", "kein", "Zit\u00b7tern", "vor", "dem", "Frost", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "PPER", "VVFIN", "PIAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und dir, o Zittrer, hilft kein Zittern vor dem Rost.", "tokens": ["Und", "dir", ",", "o", "Zitt\u00b7rer", ",", "hilft", "kein", "Zit\u00b7tern", "vor", "dem", "Rost", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "FM", "NN", "$,", "VVFIN", "PIAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}