{"dta.poem.19344": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "77.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die ihr, und zwar mit Recht, eur altes Recht umwacht,", "tokens": ["Die", "ihr", ",", "und", "zwar", "mit", "Recht", ",", "eur", "al\u00b7tes", "Recht", "um\u00b7wacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "$,", "KON", "ADV", "APPR", "NN", "$,", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Neurer schreckt ihr: la\u00df! denn so hats Gott gemacht.", "tokens": ["Den", "Neu\u00b7rer", "schreckt", "ihr", ":", "la\u00df", "!", "denn", "so", "hats", "Gott", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$(", "VVIMP", "$.", "KON", "ADV", "VAFIN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der Neurer, wenn er dis bestreiten will, ist dumm;", "tokens": ["Der", "Neu\u00b7rer", ",", "wenn", "er", "dis", "be\u00b7strei\u00b7ten", "will", ",", "ist", "dumm", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "PDS", "VVINF", "VMFIN", "$,", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn er gescheit ist, kehrt er keck die Waffen um.", "tokens": ["Wenn", "er", "ge\u00b7scheit", "ist", ",", "kehrt", "er", "keck", "die", "Waf\u00b7fen", "um", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "$,", "VVFIN", "PPER", "ADJD", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ja, Gott hat es gemacht, denn er macht alle Sachen;", "tokens": ["Ja", ",", "Gott", "hat", "es", "ge\u00b7macht", ",", "denn", "er", "macht", "al\u00b7le", "Sa\u00b7chen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NN", "VAFIN", "PPER", "VVPP", "$,", "KON", "PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Drum, weil ers so gemacht, kann ers auch anders machen.", "tokens": ["Drum", ",", "weil", "ers", "so", "ge\u00b7macht", ",", "kann", "ers", "auch", "an\u00b7ders", "ma\u00b7chen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "KOUS", "PIS", "ADV", "VVPP", "$,", "VMFIN", "PIS", "ADV", "ADV", "VVINF", "$."], "meter": "-++--+-+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Ein Werkzeug seiner Hand ist auch der Andersmacher,", "tokens": ["Ein", "Werk\u00b7zeug", "sei\u00b7ner", "Hand", "ist", "auch", "der", "An\u00b7ders\u00b7ma\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VAFIN", "ADV", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Werkzeug stark und neu, an alter statt und schwacher.", "tokens": ["Ein", "Werk\u00b7zeug", "stark", "und", "neu", ",", "an", "al\u00b7ter", "statt", "und", "schwa\u00b7cher", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "$,", "APPR", "ADJA", "NN", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}