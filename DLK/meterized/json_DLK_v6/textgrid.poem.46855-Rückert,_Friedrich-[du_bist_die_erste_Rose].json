{"textgrid.poem.46855": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[du bist die erste Rose]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du bist die erste Rose,", "tokens": ["Du", "bist", "die", "ers\u00b7te", "Ro\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die mir der Fr\u00fchling bringt,", "tokens": ["Die", "mir", "der", "Fr\u00fch\u00b7ling", "bringt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die letzte aus dem Mose", "tokens": ["Die", "letz\u00b7te", "aus", "dem", "Mo\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die noch im Herbste dringt;", "tokens": ["Die", "noch", "im", "Herbs\u00b7te", "dringt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Du bl\u00fchest vom Getose", "tokens": ["Du", "bl\u00fc\u00b7hest", "vom", "Ge\u00b7to\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Des Wintersturms umringt,", "tokens": ["Des", "Win\u00b7ter\u00b7sturms", "um\u00b7ringt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Von keiner Zeit bedingt,", "tokens": ["Von", "kei\u00b7ner", "Zeit", "be\u00b7dingt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Bl\u00fchst du mir als Zeitlose;", "tokens": ["Bl\u00fchst", "du", "mir", "als", "Zeit\u00b7lo\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "KOUS", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "O sch\u00f6ne hochzeitlose", "tokens": ["O", "sch\u00f6\u00b7ne", "hoch\u00b7zeit\u00b7lo\u00b7se"], "token_info": ["word", "word", "word"], "pos": ["NE", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Braut, die der Tod umschlingt,", "tokens": ["Braut", ",", "die", "der", "Tod", "um\u00b7schlingt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.11": {"text": "O duft'ge Hochzeitrose,", "tokens": ["O", "duft'\u00b7ge", "Hoch\u00b7zeit\u00b7ro\u00b7se", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Die aus dem Grab entspringt!", "tokens": ["Die", "aus", "dem", "Grab", "ent\u00b7springt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}