{"textgrid.poem.65802": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "76", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Warum ist mein Gesang so arm und stumm", "tokens": ["Wa\u00b7rum", "ist", "mein", "Ge\u00b7sang", "so", "arm", "und", "stumm"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPOSAT", "NN", "ADV", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "An jungem Prunk, an flinken Neuigkeiten?", "tokens": ["An", "jun\u00b7gem", "Prunk", ",", "an", "flin\u00b7ken", "Neu\u00b7ig\u00b7kei\u00b7ten", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was seh' ich mich nicht mit den Zeiten um", "tokens": ["Was", "seh'", "ich", "mich", "nicht", "mit", "den", "Zei\u00b7ten", "um"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "PRF", "PTKNEG", "APPR", "ART", "NN", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nach neuerfundnen fremden Ohrenweiden?", "tokens": ["Nach", "neu\u00b7er\u00b7fund\u00b7nen", "frem\u00b7den", "Oh\u00b7ren\u00b7wei\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Was schreib' ich immerfort dieselben Z\u00fcge,", "tokens": ["Was", "schreib'", "ich", "im\u00b7mer\u00b7fort", "die\u00b7sel\u00b7ben", "Z\u00fc\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "In dem gewohnten Kleid das alte Lied,", "tokens": ["In", "dem", "ge\u00b7wohn\u00b7ten", "Kleid", "das", "al\u00b7te", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Da\u00df jedes Wort fast meinen Namen tr\u00fcge,", "tokens": ["Da\u00df", "je\u00b7des", "Wort", "fast", "mei\u00b7nen", "Na\u00b7men", "tr\u00fc\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und jeder leicht, woher es k\u00e4m, erriet?", "tokens": ["Und", "je\u00b7der", "leicht", ",", "wo\u00b7her", "es", "k\u00e4m", ",", "er\u00b7riet", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PIS", "ADJD", "$,", "PWAV", "PPER", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "O wisse, s\u00fc\u00dfer Freund! Du bist allein", "tokens": ["O", "wis\u00b7se", ",", "s\u00fc\u00b7\u00dfer", "Freund", "!", "Du", "bist", "al\u00b7lein"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "$,", "ADJA", "NN", "$.", "PPER", "VAFIN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Mein Lied, und Lieb' und du mein einzig Wort.", "tokens": ["Mein", "Lied", ",", "und", "Lieb'", "und", "du", "mein", "ein\u00b7zig", "Wort", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KON", "NN", "KON", "PPER", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "So kann ich ewig Altes nur erneu'n,", "tokens": ["So", "kann", "ich", "e\u00b7wig", "Al\u00b7tes", "nur", "er\u00b7neu'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Und schon Gegebnes geb' ich wieder fort.", "tokens": ["Und", "schon", "Ge\u00b7geb\u00b7nes", "geb'", "ich", "wie\u00b7der", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Denn, wie die Sonne t\u00e4glich auf und nieder,", "tokens": ["Denn", ",", "wie", "die", "Son\u00b7ne", "t\u00e4g\u00b7lich", "auf", "und", "nie\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "ART", "NN", "ADJD", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Sagt meine Liebe stets Gesagtes wieder.", "tokens": ["Sagt", "mei\u00b7ne", "Lie\u00b7be", "stets", "Ge\u00b7sag\u00b7tes", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}