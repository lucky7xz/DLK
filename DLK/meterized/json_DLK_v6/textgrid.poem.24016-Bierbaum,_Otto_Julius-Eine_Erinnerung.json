{"textgrid.poem.24016": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Eine Erinnerung", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fr\u00fchling wars; ich war auf einem Kirchhof.", "tokens": ["Fr\u00fch\u00b7ling", "wars", ";", "ich", "war", "auf", "ei\u00b7nem", "Kirch\u00b7hof", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$.", "PPER", "VAFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Sa\u00df auf einem Grab ein blondes M\u00e4dchen,", "tokens": ["Sa\u00df", "auf", "ei\u00b7nem", "Grab", "ein", "blon\u00b7des", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Hatte blaue tr\u00e4umerische Augen;", "tokens": ["Hat\u00b7te", "blau\u00b7e", "tr\u00e4u\u00b7me\u00b7ri\u00b7sche", "Au\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Einen Fliederzweig hielt sie in H\u00e4nden,", "tokens": ["Ei\u00b7nen", "Flie\u00b7der\u00b7zweig", "hielt", "sie", "in", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Ihre Augen gingen in den Himmel,", "tokens": ["Ih\u00b7re", "Au\u00b7gen", "gin\u00b7gen", "in", "den", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Und es leuchteten die blauen Augen.", "tokens": ["Und", "es", "leuch\u00b7te\u00b7ten", "die", "blau\u00b7en", "Au\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "--+-+-+-+-", "measure": "anapaest.init"}}, "stanza.2": {"line.1": {"text": "Irgendwo einmal schon sah ich diese", "tokens": ["Ir\u00b7gend\u00b7wo", "ein\u00b7mal", "schon", "sah", "ich", "die\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADV", "VVFIN", "PPER", "PDS"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wundersch\u00f6nen tr\u00e4umerischen Augen,", "tokens": ["Wun\u00b7der\u00b7sch\u00f6\u00b7nen", "tr\u00e4u\u00b7me\u00b7ri\u00b7schen", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und ich sinne: wo?", "tokens": ["Und", "ich", "sin\u00b7ne", ":", "wo", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$.", "PWAV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Da h\u00f6r ichs klingen", "tokens": ["Da", "h\u00f6r", "ichs", "klin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Wie Klavier in einem Tingeltangel.", "tokens": ["Wie", "Kla\u00b7vier", "in", "ei\u00b7nem", "Tin\u00b7gel\u00b7tan\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Und ich sehe auf dem Gauklerbrette,", "tokens": ["Und", "ich", "se\u00b7he", "auf", "dem", "Gauk\u00b7ler\u00b7bret\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Seh im kurzen Kleid ein M\u00e4dchen tanzen,", "tokens": ["Seh", "im", "kur\u00b7zen", "Kleid", "ein", "M\u00e4d\u00b7chen", "tan\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Und sie singt dazu mit d\u00fcnnem Stimmchen", "tokens": ["Und", "sie", "singt", "da\u00b7zu", "mit", "d\u00fcn\u00b7nem", "Stimm\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PAV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Schrill ein Lied: Nur einmal bl\u00fcht im Jahr der", "tokens": ["Schrill", "ein", "Lied", ":", "Nur", "ein\u00b7mal", "bl\u00fcht", "im", "Jahr", "der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "ART", "NN", "$.", "ADV", "ADV", "VVFIN", "APPRART", "NN", "ART"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Mai.", "tokens": ["Mai", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}}, "stanza.3": {"line.1": {"text": "War so blond, blau\u00e4ugig jene T\u00e4nzerin", "tokens": ["War", "so", "blond", ",", "blau\u00b7\u00e4u\u00b7gig", "je\u00b7ne", "T\u00e4n\u00b7ze\u00b7rin"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ADJD", "$,", "ADJD", "PDAT", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Wie das M\u00e4dchen mit dem Fliederzweige", "tokens": ["Wie", "das", "M\u00e4d\u00b7chen", "mit", "dem", "Flie\u00b7der\u00b7zwei\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Aber ihre Wangen trugen Schminke,", "tokens": ["A\u00b7ber", "ih\u00b7re", "Wan\u00b7gen", "tru\u00b7gen", "Schmin\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und es lagen wie geduckte Schlangen", "tokens": ["Und", "es", "la\u00b7gen", "wie", "ge\u00b7duck\u00b7te", "Schlan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "KOKOM", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Schwarze Ringe um die blauen Augen.", "tokens": ["Schwar\u00b7ze", "Rin\u00b7ge", "um", "die", "blau\u00b7en", "Au\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Jenes M\u00e4dchen starb in meinen Armen,", "tokens": ["Je\u00b7nes", "M\u00e4d\u00b7chen", "starb", "in", "mei\u00b7nen", "Ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Krank und elend, aller L\u00fcste m\u00fcde,", "tokens": ["Krank", "und", "e\u00b7lend", ",", "al\u00b7ler", "L\u00fcs\u00b7te", "m\u00fc\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "$,", "PIAT", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Ihre Lippen pre\u00dften sich im Schmerze,", "tokens": ["Ih\u00b7re", "Lip\u00b7pen", "pre\u00df\u00b7ten", "sich", "im", "Schmer\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Die so hei\u00df gek\u00fc\u00dft und s\u00fc\u00df gel\u00e4chelt.", "tokens": ["Die", "so", "hei\u00df", "ge\u00b7k\u00fc\u00dft", "und", "s\u00fc\u00df", "ge\u00b7l\u00e4\u00b7chelt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "VVPP", "KON", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Aber als sie starb, da gingen ihre", "tokens": ["A\u00b7ber", "als", "sie", "starb", ",", "da", "gin\u00b7gen", "ih\u00b7re"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "PPOSAT"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Blauen Augen leuchtend in den Himmel,", "tokens": ["Blau\u00b7en", "Au\u00b7gen", "leuch\u00b7tend", "in", "den", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und ich dankte tief in meinem Herzen", "tokens": ["Und", "ich", "dank\u00b7te", "tief", "in", "mei\u00b7nem", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ihrem Heiland Tod, da\u00df er sie l\u00f6ste.", "tokens": ["Ih\u00b7rem", "Hei\u00b7land", "Tod", ",", "da\u00df", "er", "sie", "l\u00f6s\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": ". . . . . . . . . . . . . . . . . . . . . . . . . . .", "tokens": [".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", "."], "token_info": ["punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct"], "pos": ["$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$."]}}, "stanza.6": {"line.1": {"text": "Sieh, das M\u00e4dchen mit dem Fliederzweige", "tokens": ["Sieh", ",", "das", "M\u00e4d\u00b7chen", "mit", "dem", "Flie\u00b7der\u00b7zwei\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ist gegangen.", "tokens": ["Ist", "ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Dank f\u00fcr die Erinnerung,", "tokens": ["Dank", "f\u00fcr", "die", "E\u00b7rin\u00b7ne\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "$,"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.4": {"text": "Die mir deiner blauen Augen Leuchten", "tokens": ["Die", "mir", "dei\u00b7ner", "blau\u00b7en", "Au\u00b7gen", "Leuch\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "PPOSAT", "ADJA", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "G\u00fctig schenkte!", "tokens": ["G\u00fc\u00b7tig", "schenk\u00b7te", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Durch die Trauerweide", "tokens": ["Durch", "die", "Trau\u00b7er\u00b7wei\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Geht ein Wehn: Nur einmal bl\u00fcht im Jahr der", "tokens": ["Geht", "ein", "Wehn", ":", "Nur", "ein\u00b7mal", "bl\u00fcht", "im", "Jahr", "der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$.", "ADV", "ADV", "VVFIN", "APPRART", "NN", "ART"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Mai.", "tokens": ["Mai", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}}}}}