{"dta.poem.9611": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Abri\u00df eines verliebten.  \n C. H. v. H.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Er ist ein krancker/ den ein sinnlich fieber plaget/", "tokens": ["Er", "ist", "ein", "kran\u00b7cker", "/", "den", "ein", "sinn\u00b7lich", "fie\u00b7ber", "pla\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$(", "ART", "ART", "ADJD", "ADV", "VVFIN", "$("], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ein j\u00e4ger/ so allzeit auf einem hirsche jaget/", "tokens": ["Ein", "j\u00e4\u00b7ger", "/", "so", "all\u00b7zeit", "auf", "ei\u00b7nem", "hir\u00b7sche", "ja\u00b7get", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$(", "ADV", "ADV", "APPR", "ART", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein wetterhan/ der stets nach einem winde steht/", "tokens": ["Ein", "wet\u00b7ter\u00b7han", "/", "der", "stets", "nach", "ei\u00b7nem", "win\u00b7de", "steht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$(", "ART", "ADV", "APPR", "ART", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein schif/ so ungehemmt nach Cypris hafen geht.", "tokens": ["Ein", "schif", "/", "so", "un\u00b7ge\u00b7hemmt", "nach", "Cyp\u00b7ris", "ha\u00b7fen", "geht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "$(", "ADV", "ADJD", "APPR", "NE", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ein m\u00e4rterer der brunst/ den freund und feind belachet/", "tokens": ["Ein", "m\u00e4r\u00b7te\u00b7rer", "der", "brunst", "/", "den", "freund", "und", "feind", "be\u00b7la\u00b7chet", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$(", "ART", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ein Morpheus/ der ihm selbst bey tage tr\u00e4ume machet/", "tokens": ["Ein", "Mor\u00b7pheus", "/", "der", "ihm", "selbst", "bey", "ta\u00b7ge", "tr\u00e4u\u00b7me", "ma\u00b7chet", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$(", "ART", "PPER", "ADV", "APPR", "NN", "VVFIN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ein arm gefangener/ der seine fe\u00dfel liebt/", "tokens": ["Ein", "arm", "ge\u00b7fan\u00b7ge\u00b7ner", "/", "der", "sei\u00b7ne", "fe\u00b7\u00dfel", "liebt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "$(", "ART", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und seinen hencker ehrt/ wenn er ihm streiche giebt.", "tokens": ["Und", "sei\u00b7nen", "hen\u00b7cker", "ehrt", "/", "wenn", "er", "ihm", "strei\u00b7che", "giebt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$(", "KOUS", "PPER", "PPER", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ein Aetna/ der voll glut/ l\u00e4st flut und str\u00f6me flie\u00dfen/", "tokens": ["Ein", "A\u00b7et\u00b7na", "/", "der", "voll", "glut", "/", "l\u00e4st", "flut", "und", "str\u00f6\u00b7me", "flie\u00b7\u00dfen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$(", "ART", "ADJD", "NN", "$(", "VVFIN", "NN", "KON", "VVFIN", "VVINF", "$("], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Ein hungriger/ der blo\u00df wil rohes fleisch genie\u00dfen/", "tokens": ["Ein", "hung\u00b7ri\u00b7ger", "/", "der", "blo\u00df", "wil", "ro\u00b7hes", "fleisch", "ge\u00b7nie\u00b7\u00dfen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$(", "ART", "ADV", "VMFIN", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ein welt Sebastian/ den Venus sch\u00fctze trifft/", "tokens": ["Ein", "welt", "Se\u00b7bas\u00b7ti\u00b7an", "/", "den", "Ve\u00b7nus", "sch\u00fct\u00b7ze", "trifft", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$(", "ART", "NN", "VVFIN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ein rechter Adams-sohn/ den frauen-hand vergift.", "tokens": ["Ein", "rech\u00b7ter", "A\u00b7dams\u00b7sohn", "/", "den", "frau\u00b7en\u00b7hand", "ver\u00b7gift", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ein wirth/ ein ander kind/ l\u00e4st ernste sachen fahren/", "tokens": ["Ein", "wirth", "/", "ein", "an\u00b7der", "kind", "/", "l\u00e4st", "erns\u00b7te", "sa\u00b7chen", "fah\u00b7ren", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "$(", "ART", "ADJA", "NN", "$(", "VVFIN", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ein haar/ ein altes band/ sind seine besten wahren/", "tokens": ["Ein", "haar", "/", "ein", "al\u00b7tes", "band", "/", "sind", "sei\u00b7ne", "bes\u00b7ten", "wah\u00b7ren", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "ADJA", "NN", "$(", "VAFIN", "PPOSAT", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Jtzt baut er etwas auf/ itzt reist ers wieder ein.", "tokens": ["Jtzt", "baut", "er", "et\u00b7was", "auf", "/", "itzt", "reist", "ers", "wie\u00b7der", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "$(", "ADV", "VVFIN", "PIS", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Jtzt mu\u00df Democritus der sitten meister seyn/", "tokens": ["Jtzt", "mu\u00df", "De\u00b7mo\u00b7cri\u00b7tus", "der", "sit\u00b7ten", "meis\u00b7ter", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "NE", "ART", "ADJA", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Jtzt ist es Heraclyt. Das hertze/ so er f\u00fchret/", "tokens": ["Jtzt", "ist", "es", "Her\u00b7a\u00b7clyt", ".", "Das", "hert\u00b7ze", "/", "so", "er", "f\u00fch\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NE", "$.", "ART", "NN", "$(", "ADV", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Vergleicht sich dem metall/ das ein magnet ger\u00fchret.", "tokens": ["Ver\u00b7gleicht", "sich", "dem", "me\u00b7tall", "/", "das", "ein", "mag\u00b7net", "ge\u00b7r\u00fch\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "$(", "PDS", "ART", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Sein himmel ist ihr haupt/ die erd ist ihre schoos.", "tokens": ["Sein", "him\u00b7mel", "ist", "ihr", "haupt", "/", "die", "erd", "ist", "ih\u00b7re", "schoos", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "$(", "ART", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Hier anckert seine lust/ es wird der erden klo\u00df", "tokens": ["Hier", "an\u00b7ckert", "sei\u00b7ne", "lust", "/", "es", "wird", "der", "er\u00b7den", "klo\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$(", "PPER", "VAFIN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Der \u00fcberwei\u00dfte koth dem himmel vorgesetzet/", "tokens": ["Der", "\u00fc\u00b7berw\u00b7ei\u00df\u00b7te", "koth", "dem", "him\u00b7mel", "vor\u00b7ge\u00b7set\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Und ist ihr auge mehr als Venus selbst gesch\u00e4tzet.", "tokens": ["Und", "ist", "ihr", "au\u00b7ge", "mehr", "als", "Ve\u00b7nus", "selbst", "ge\u00b7sch\u00e4t\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "ADV", "KOUS", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "So wundre ich mich nicht/ da\u00df man das weib veracht/", "tokens": ["So", "wund\u00b7re", "ich", "mich", "nicht", "/", "da\u00df", "man", "das", "weib", "ver\u00b7acht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "PTKNEG", "$(", "KOUS", "PIS", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Weil sie die erste pein zu erst hat aufgebracht.", "tokens": ["Weil", "sie", "die", "ers\u00b7te", "pein", "zu", "erst", "hat", "auf\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "APPR", "ADV", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Sein essen ist ein ku\u00df/ sein tranck sind heisse thr\u00e4nen/", "tokens": ["Sein", "es\u00b7sen", "ist", "ein", "ku\u00df", "/", "sein", "tranck", "sind", "heis\u00b7se", "thr\u00e4\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$(", "PPOSAT", "NN", "VAFIN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Die zeit verjaget er mit seufftzen und mit stehnen.", "tokens": ["Die", "zeit", "ver\u00b7ja\u00b7get", "er", "mit", "seufft\u00b7zen", "und", "mit", "steh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "VVINF", "KON", "APPR", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Und wann ihm etwan tr\u00e4umt/ wie er die liebste find/", "tokens": ["Und", "wann", "ihm", "et\u00b7wan", "tr\u00e4umt", "/", "wie", "er", "die", "liebs\u00b7te", "find", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "VVFIN", "$(", "PWAV", "PPER", "ART", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "So hat er nichts als luft/ und k\u00fcsset nichts als wind.", "tokens": ["So", "hat", "er", "nichts", "als", "luft", "/", "und", "k\u00fcs\u00b7set", "nichts", "als", "wind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIS", "KOKOM", "VVFIN", "$(", "KON", "VVFIN", "PIS", "KOKOM", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Denn tr\u00e4ume/ buler/ wind sind gleiches thuns gesellen;", "tokens": ["Denn", "tr\u00e4u\u00b7me", "/", "bu\u00b7ler", "/", "wind", "sind", "glei\u00b7ches", "thuns", "ge\u00b7sel\u00b7len", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "NE", "$(", "NE", "VAFIN", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Sein schlaffen darf er nicht nach einem wecker stellen;", "tokens": ["Sein", "schlaf\u00b7fen", "darf", "er", "nicht", "nach", "ei\u00b7nem", "we\u00b7cker", "stel\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVINF", "VMFIN", "PPER", "PTKNEG", "APPR", "ART", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Indem die weckerin/ so in dem hertzen steckt/", "tokens": ["In\u00b7dem", "die", "we\u00b7cke\u00b7rin", "/", "so", "in", "dem", "hert\u00b7zen", "steckt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$(", "ADV", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Jhn besser/ als er w\u00fcnscht/ aus seinem schlaff erweckt/", "tokens": ["Jhn", "bes\u00b7ser", "/", "als", "er", "w\u00fcnscht", "/", "aus", "sei\u00b7nem", "schlaff", "er\u00b7weckt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "$(", "KOUS", "PPER", "VVFIN", "$(", "APPR", "PPOSAT", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Und seinen schmertzen r\u00fchrt. Zu dornen wird das bette/", "tokens": ["Und", "sei\u00b7nen", "schmert\u00b7zen", "r\u00fchrt", ".", "Zu", "dor\u00b7nen", "wird", "das", "bet\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$.", "PTKZU", "VVINF", "VAFIN", "PDS", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "So wachet er mit dem im lager in die wette/", "tokens": ["So", "wa\u00b7chet", "er", "mit", "dem", "im", "la\u00b7ger", "in", "die", "wet\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "APPRART", "NN", "APPR", "ART", "VVFIN", "$("], "meter": "-+---+-+-+-+-", "measure": "dactylic.init"}, "line.35": {"text": "Und f\u00fchrt der thr\u00e4nen strom um seine wangen her/", "tokens": ["Und", "f\u00fchrt", "der", "thr\u00e4\u00b7nen", "strom", "um", "sei\u00b7ne", "wan\u00b7gen", "her", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Bald wil er aus der welt/ bald wil er \u00fcber meer/", "tokens": ["Bald", "wil", "er", "aus", "der", "welt", "/", "bald", "wil", "er", "\u00fc\u00b7ber", "meer", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "ART", "NN", "$(", "ADV", "VMFIN", "PPER", "APPR", "NE", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Und mu\u00df doch wie zuvor in seinem hause bleiben/", "tokens": ["Und", "mu\u00df", "doch", "wie", "zu\u00b7vor", "in", "sei\u00b7nem", "hau\u00b7se", "blei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "KOKOM", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Mu\u00df lernen/ wie sein rath nicht stetig wil bekleiben.", "tokens": ["Mu\u00df", "ler\u00b7nen", "/", "wie", "sein", "rath", "nicht", "ste\u00b7tig", "wil", "be\u00b7klei\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "$(", "KOKOM", "PPOSAT", "NN", "PTKNEG", "ADJD", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Wie erstlich bulerey und die gew\u00f6lckte nacht", "tokens": ["Wie", "erst\u00b7lich", "bu\u00b7le\u00b7rey", "und", "die", "ge\u00b7w\u00f6lck\u00b7te", "nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "NN", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Auf anschlag/ aber nicht auf ausschlag ist bedacht;", "tokens": ["Auf", "an\u00b7schlag", "/", "a\u00b7ber", "nicht", "auf", "aus\u00b7schlag", "ist", "be\u00b7dacht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$(", "ADV", "PTKNEG", "APPR", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "So l\u00e4st er ohne ruh sich fremde sachen lencken/", "tokens": ["So", "l\u00e4st", "er", "oh\u00b7ne", "ruh", "sich", "frem\u00b7de", "sa\u00b7chen", "len\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "PRF", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "L\u00e4st in gesunder haut sich seine schwachheit kr\u00e4ncken/", "tokens": ["L\u00e4st", "in", "ge\u00b7sun\u00b7der", "haut", "sich", "sei\u00b7ne", "schwach\u00b7heit", "kr\u00e4n\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "VVFIN", "PRF", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Liebt nacht und finsterni\u00df bey sonne und bey licht;", "tokens": ["Liebt", "nacht", "und", "fins\u00b7ter\u00b7ni\u00df", "bey", "son\u00b7ne", "und", "bey", "licht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "VVFIN", "APPR", "NE", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Ist wie ein schweres schiff/ dem der compas gebricht.", "tokens": ["Ist", "wie", "ein", "schwe\u00b7res", "schiff", "/", "dem", "der", "com\u00b7pas", "ge\u00b7bricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "ART", "ADJA", "VVFIN", "$(", "ART", "ART", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Und da\u00df ich nicht zu viel von einer sache sage/", "tokens": ["Und", "da\u00df", "ich", "nicht", "zu", "viel", "von", "ei\u00b7ner", "sa\u00b7che", "sa\u00b7ge", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PTKNEG", "PTKA", "PIS", "APPR", "ART", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Die allen ist bekandt als allgemeine plage/", "tokens": ["Die", "al\u00b7len", "ist", "be\u00b7kandt", "als", "all\u00b7ge\u00b7mei\u00b7ne", "pla\u00b7ge", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VAFIN", "ADJD", "KOKOM", "PIS", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "So mu\u00df der vorhang weg. Das mahlwerck ist vollbracht/", "tokens": ["So", "mu\u00df", "der", "vor\u00b7hang", "weg", ".", "Das", "mahl\u00b7werck", "ist", "voll\u00b7bracht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "ADJD", "PTKVZ", "$.", "ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Hier hat der mahler selbst sein ebenbild gemacht.", "tokens": ["Hier", "hat", "der", "mah\u00b7ler", "selbst", "sein", "e\u00b7ben\u00b7bild", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "PPOSAT", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}