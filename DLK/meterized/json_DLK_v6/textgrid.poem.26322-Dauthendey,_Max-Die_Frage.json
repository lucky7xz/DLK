{"textgrid.poem.26322": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Die Frage", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Licht brennt auf dem Tische", "tokens": ["Ein", "Licht", "brennt", "auf", "dem", "Ti\u00b7sche"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die lange, lange Nacht.", "tokens": ["Die", "lan\u00b7ge", ",", "lan\u00b7ge", "Nacht", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADV", "$,", "ADV", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und in der Fensternische", "tokens": ["Und", "in", "der", "Fens\u00b7ter\u00b7ni\u00b7sche"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Steht bleich ein Weib und wacht.", "tokens": ["Steht", "bleich", "ein", "Weib", "und", "wacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sie wandert mit den Blicken", "tokens": ["Sie", "wan\u00b7dert", "mit", "den", "Bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nie m\u00fcd' am Himmel hin.", "tokens": ["Nie", "m\u00fcd'", "am", "Him\u00b7mel", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Himmelslichter nicken,", "tokens": ["Die", "Him\u00b7mels\u00b7lich\u00b7ter", "ni\u00b7cken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die langsam weiterziehn.", "tokens": ["Die", "lang\u00b7sam", "wei\u00b7ter\u00b7ziehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Kehrt er zur\u00fcck? Die Frage", "tokens": ["Kehrt", "er", "zu\u00b7r\u00fcck", "?", "Die", "Fra\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "PTKVZ", "$.", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Stellt sie still Nacht um Nacht.", "tokens": ["Stellt", "sie", "still", "Nacht", "um", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie wartet ohne Klage.", "tokens": ["Sie", "war\u00b7tet", "oh\u00b7ne", "Kla\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie wartet und sie wacht.", "tokens": ["Sie", "war\u00b7tet", "und", "sie", "wacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}