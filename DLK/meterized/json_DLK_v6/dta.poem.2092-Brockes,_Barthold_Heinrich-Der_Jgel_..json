{"dta.poem.2092": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Jgel .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Er macht sich selbst zum Spanschenreuter; er pflanzet ei-", "tokens": ["Er", "macht", "sich", "selbst", "zum", "Span\u00b7schen\u00b7reu\u00b7ter", ";", "er", "pflan\u00b7zet", "ei"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "APPRART", "NN", "$.", "PPER", "VVFIN", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "nen Zaun von Dornen", "tokens": ["nen", "Zaun", "von", "Dor\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPR", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Jm Augenblick um sich herum; er ist von hinten und", "tokens": ["Jm", "Au\u00b7gen\u00b7blick", "um", "sich", "he\u00b7rum", ";", "er", "ist", "von", "hin\u00b7ten", "und"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "PRF", "PTKVZ", "$.", "PPER", "VAFIN", "APPR", "ADV", "KON"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "von vornen", "tokens": ["von", "vor\u00b7nen"], "token_info": ["word", "word"], "pos": ["APPR", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "In seiner scharfen Schanze sicher, und er verl\u00e4sset sich", "tokens": ["In", "sei\u00b7ner", "schar\u00b7fen", "Schan\u00b7ze", "si\u00b7cher", ",", "und", "er", "ver\u00b7l\u00e4s\u00b7set", "sich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "ADJD", "$,", "KON", "PPER", "VVFIN", "PRF"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.6": {"text": "darauf.", "tokens": ["da\u00b7rauf", "."], "token_info": ["word", "punct"], "pos": ["PAV", "$."], "meter": "-+", "measure": "iambic.single"}, "line.7": {"text": "Droht ihm von au\u00dfen wo ein Feind, Thier oder Mensch,", "tokens": ["Droht", "ihm", "von", "au\u00b7\u00dfen", "wo", "ein", "Feind", ",", "Thier", "o\u00b7der", "Mensch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADV", "PWAV", "ART", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+--+-+-++--+", "measure": "iambic.hexa.invert"}, "line.8": {"text": "ihn anzufallen,", "tokens": ["ihn", "an\u00b7zu\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVIZU", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Ver\u00e4ndert er gleich seine Form und wird zu einem runden", "tokens": ["Ver\u00b7\u00e4n\u00b7dert", "er", "gleich", "sei\u00b7ne", "Form", "und", "wird", "zu", "ei\u00b7nem", "run\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN", "KON", "VAFIN", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.10": {"text": "Ballen.", "tokens": ["Bal\u00b7len", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.11": {"text": "Das, so an ihm verletzbar ist, den Kopf, die F\u00fc\u00dfe, zieht", "tokens": ["Das", ",", "so", "an", "ihm", "ver\u00b7letz\u00b7bar", "ist", ",", "den", "Kopf", ",", "die", "F\u00fc\u00b7\u00dfe", ",", "zieht"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PDS", "$,", "ADV", "APPR", "PPER", "ADJD", "VAFIN", "$,", "ART", "NN", "$,", "ART", "NN", "$,", "VVFIN"], "meter": "--+--+-+-+-+-+", "measure": "anapaest.di.plus"}, "line.12": {"text": "er ein,", "tokens": ["er", "ein", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.13": {"text": "Und dadurch wird er unerkannt, ja, auch erkannt doch", "tokens": ["Und", "da\u00b7durch", "wird", "er", "un\u00b7er\u00b7kannt", ",", "ja", ",", "auch", "er\u00b7kannt", "doch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KON", "PAV", "VAFIN", "PPER", "ADJD", "$,", "PTKANT", "$,", "ADV", "VVFIN", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "sicher seyn.", "tokens": ["si\u00b7cher", "seyn", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VAINF", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.15": {"text": "Was fast noch st\u00e4rker zu bewundern, so braucht er diese", "tokens": ["Was", "fast", "noch", "st\u00e4r\u00b7ker", "zu", "be\u00b7wun\u00b7dern", ",", "so", "braucht", "er", "die\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "ADV", "ADJD", "PTKZU", "VVINF", "$,", "ADV", "VVFIN", "PPER", "PDAT"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "seine Waffen,", "tokens": ["sei\u00b7ne", "Waf\u00b7fen", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.17": {"text": "Um, ohne H\u00e4nde, doch zu sammeln und sich die Nah-", "tokens": ["Um", ",", "oh\u00b7ne", "H\u00e4n\u00b7de", ",", "doch", "zu", "sam\u00b7meln", "und", "sich", "die", "Nah"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "$,", "KOUI", "NN", "$,", "ADV", "PTKZU", "VVINF", "KON", "PRF", "ART", "NN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "rung zu verschaffen.", "tokens": ["rung", "zu", "ver\u00b7schaf\u00b7fen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.19": {"text": "Wo abgefallne Fr\u00fcchte liegen, da welzt er sich; ein\u2019 jede", "tokens": ["Wo", "ab\u00b7ge\u00b7fall\u00b7ne", "Fr\u00fcch\u00b7te", "lie\u00b7gen", ",", "da", "welzt", "er", "sich", ";", "ein'", "je\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "PRF", "$.", "ART", "PIAT"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.20": {"text": "Spitz", "tokens": ["Spitz"], "token_info": ["word"], "pos": ["NE"], "meter": "+", "measure": "single.up"}, "line.21": {"text": "Ist dann, dieselben zu bekommen, so gut, als eine Hand,", "tokens": ["Ist", "dann", ",", "die\u00b7sel\u00b7ben", "zu", "be\u00b7kom\u00b7men", ",", "so", "gut", ",", "als", "ei\u00b7ne", "Hand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "$,", "PDS", "PTKZU", "VVINF", "$,", "ADV", "ADJD", "$,", "KOUS", "ART", "NN", "$,"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.22": {"text": "ihm n\u00fctz.", "tokens": ["ihm", "n\u00fctz", "."], "token_info": ["word", "word", "punct"], "pos": ["PPER", "ADJD", "$."], "meter": "-+", "measure": "iambic.single"}, "line.23": {"text": "Der Stachel dringet in die Frucht,", "tokens": ["Der", "Sta\u00b7chel", "drin\u00b7get", "in", "die", "Frucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Die Frucht bleibt auf derselben feste,", "tokens": ["Die", "Frucht", "bleibt", "auf", "der\u00b7sel\u00b7ben", "fes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PDAT", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Da wandert er, mit reicher Beute beladen, bald nach sei-", "tokens": ["Da", "wan\u00b7dert", "er", ",", "mit", "rei\u00b7cher", "Beu\u00b7te", "be\u00b7la\u00b7den", ",", "bald", "nach", "sei"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "APPR", "ADJD", "NN", "VVPP", "$,", "ADV", "APPR", "TRUNC"], "meter": "-+---+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.26": {"text": "nem Neste.", "tokens": ["nem", "Nes\u00b7te", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.27": {"text": "Uns wird von diesem kleinen Thier auch mancher Vor-", "tokens": ["Uns", "wird", "von", "die\u00b7sem", "klei\u00b7nen", "Thier", "auch", "man\u00b7cher", "Vor"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PDAT", "ADJA", "NN", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "theil noch gewehrt,", "tokens": ["theil", "noch", "ge\u00b7wehrt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.29": {"text": "Indem es M\u00e4use, Fr\u00f6sche, Kr\u00f6ten, und mancherley Ge-", "tokens": ["In\u00b7dem", "es", "M\u00e4u\u00b7se", ",", "Fr\u00f6\u00b7sche", ",", "Kr\u00f6\u00b7ten", ",", "und", "man\u00b7cher\u00b7ley", "Ge"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "NN", "$,", "NN", "$,", "NN", "$,", "KON", "PIAT", "NN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.30": {"text": "w\u00fcrm verzehrt.", "tokens": ["w\u00fcrm", "ver\u00b7zehrt", "."], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.31": {"text": "Sie wissen sich wohl zu verbergen, so da\u00df man sie nicht", "tokens": ["Sie", "wis\u00b7sen", "sich", "wohl", "zu", "ver\u00b7ber\u00b7gen", ",", "so", "da\u00df", "man", "sie", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "PTKZU", "VVINF", "$,", "ADV", "KOUS", "PIS", "PPER", "PTKNEG"], "meter": "-+--+--+--+--+", "measure": "amphibrach.penta.plus"}, "line.32": {"text": "leicht erkennet.", "tokens": ["leicht", "er\u00b7ken\u00b7net", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}