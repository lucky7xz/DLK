{"textgrid.poem.60878": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Sprache", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer sich zu dichten erk\u00fchnt und die Sprache verschm\u00e4ht und den Rhythmus,", "tokens": ["Wer", "sich", "zu", "dich\u00b7ten", "er\u00b7k\u00fchnt", "und", "die", "Spra\u00b7che", "ver\u00b7schm\u00e4ht", "und", "den", "Rhyth\u00b7mus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "APPR", "ADJA", "VVPP", "KON", "ART", "NN", "VVPP", "KON", "ART", "NN", "$,"], "meter": "-+-+--+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Gliche dem Plastiker, der Bilder gehaun in die Luft!", "tokens": ["Gli\u00b7che", "dem", "Plas\u00b7ti\u00b7ker", ",", "der", "Bil\u00b7der", "ge\u00b7haun", "in", "die", "Luft", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ART", "NN", "VVPP", "APPR", "ART", "NN", "$."], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Nicht der Gedanke gen\u00fcgt; die Gedanken geh\u00f6ren der Menschheit,", "tokens": ["Nicht", "der", "Ge\u00b7dan\u00b7ke", "ge\u00b7n\u00fcgt", ";", "die", "Ge\u00b7dan\u00b7ken", "ge\u00b7h\u00f6\u00b7ren", "der", "Menschheit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "VVPP", "$.", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+--+--+--+--+--+", "measure": "dactylic.tetra.plus"}, "line.4": {"text": "Die sie zerstreut und benutzt; aber die Sprache dem Volk:", "tokens": ["Die", "sie", "zer\u00b7streut", "und", "be\u00b7nutzt", ";", "a\u00b7ber", "die", "Spra\u00b7che", "dem", "Volk", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "KON", "VVPP", "$.", "ADV", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Der wird w\u00e4hren am l\u00e4ngsten von allen germanischen Dichtern,", "tokens": ["Der", "wird", "w\u00e4h\u00b7ren", "am", "l\u00e4ngs\u00b7ten", "von", "al\u00b7len", "ger\u00b7ma\u00b7ni\u00b7schen", "Dich\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "VVINF", "APPRART", "ADJA", "APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "--+--+--+-+-+-+-", "measure": "anapaest.tri.plus"}, "line.6": {"text": "Der des germanischen Worts Weisen am besten verstand.", "tokens": ["Der", "des", "ger\u00b7ma\u00b7ni\u00b7schen", "Worts", "Wei\u00b7sen", "am", "bes\u00b7ten", "ver\u00b7stand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "NN", "PTKA", "ADJD", "VVFIN", "$."], "meter": "-+--+--+--+--+", "measure": "amphibrach.penta.plus"}}}}}