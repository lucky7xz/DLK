{"textgrid.poem.45031": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "An eine hohe G\u00f6nnerin der Karlisten", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit Unrecht sch\u00fctzest du so schlechte Leute,", "tokens": ["Mit", "Un\u00b7recht", "sch\u00fct\u00b7zest", "du", "so", "schlech\u00b7te", "Leu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Schickst ihnen Geld mit eignen hohen H\u00e4nden,", "tokens": ["Schickst", "ih\u00b7nen", "Geld", "mit", "eig\u00b7nen", "ho\u00b7hen", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du kennst sie freilich nur von ihrer guten Seite:", "tokens": ["Du", "kennst", "sie", "frei\u00b7lich", "nur", "von", "ih\u00b7rer", "gu\u00b7ten", "Sei\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vom Glauben, Beten und vom Sch\u00e4nden.", "tokens": ["Vom", "Glau\u00b7ben", ",", "Be\u00b7ten", "und", "vom", "Sch\u00e4n\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "NN", "KON", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}