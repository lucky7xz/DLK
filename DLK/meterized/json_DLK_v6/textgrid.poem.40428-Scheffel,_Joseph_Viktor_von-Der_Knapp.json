{"textgrid.poem.40428": {"metadata": {"author": {"name": "Scheffel, Joseph Viktor von", "birth": "N.A.", "death": "N.A."}, "title": "Der Knapp", "genre": "verse", "period": "N.A.", "pub_year": 1856, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Herr vom Rodensteine", "tokens": ["Der", "Herr", "vom", "Ro\u00b7dens\u00b7tei\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sprach fiebrig und schabab:", "tokens": ["Sprach", "fieb\u00b7rig", "und", "scha\u00b7bab", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "VVFIN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "\u00bbungern duld' ich alleine,", "tokens": ["\u00bb", "un\u00b7gern", "duld'", "ich", "al\u00b7lei\u00b7ne", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "ADV", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Wo steckt mein treuer Knapp?", "tokens": ["Wo", "steckt", "mein", "treu\u00b7er", "Knapp", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich sp\u00fcr' in Haupt und Magen", "tokens": ["Ich", "sp\u00fcr'", "in", "Haupt", "und", "Ma\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Stechen und Geschlapp ...", "tokens": ["Ein", "Ste\u00b7chen", "und", "Ge\u00b7schlapp", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Diesmal geht mir's an Kragen,", "tokens": ["Dies\u00b7mal", "geht", "mir's", "an", "Kra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "APPR", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Wo steckt mein treuer Knapp?\u00ab", "tokens": ["Wo", "steckt", "mein", "treu\u00b7er", "Knapp", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "VVFIN", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der Reitersjungen viere", "tokens": ["Der", "Rei\u00b7ters\u00b7jun\u00b7gen", "vie\u00b7re"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Durchsuchten Weg und Steg:", "tokens": ["Durch\u00b7such\u00b7ten", "Weg", "und", "Steg", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Knapp sa\u00df fest beim Biere,", "tokens": ["Der", "Knapp", "sa\u00df", "fest", "beim", "Bie\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Juhei! im Bremeneck.", "tokens": ["Ju\u00b7hei", "!", "im", "Bre\u00b7me\u00b7neck", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$.", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Er trank und sprach mit Trauern:", "tokens": ["Er", "trank", "und", "sprach", "mit", "Trau\u00b7ern", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "\u00bbdu braver Rodenstein!", "tokens": ["\u00bb", "du", "bra\u00b7ver", "Ro\u00b7dens\u00b7tein", "!"], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Allein ich mu\u00df bedauern,", "tokens": ["Al\u00b7lein", "ich", "mu\u00df", "be\u00b7dau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich kann nicht bei dir sein!", "tokens": ["Ich", "kann", "nicht", "bei", "dir", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "APPR", "PPER", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ist ", "tokens": ["Ist"], "token_info": ["word"], "pos": ["VAFIN"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Auch ich hab' was erlebt:", "tokens": ["Auch", "ich", "hab'", "was", "er\u00b7lebt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "PIS", "VVPP", "$."], "meter": "-+---+", "measure": "dactylic.init"}, "line.3": {"text": "Ich bin mit Rock und Hosen", "tokens": ["Ich", "bin", "mit", "Rock", "und", "Ho\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hier v\u00f6llig festgeklebt.\u00ab", "tokens": ["Hier", "v\u00f6l\u00b7lig", "fest\u00b7ge\u00b7klebt", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Die Jungen meld'ten traurig", "tokens": ["Die", "Jun\u00b7gen", "meld'\u00b7ten", "trau\u00b7rig"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dem Kranken, was geschehn,", "tokens": ["Dem", "Kran\u00b7ken", ",", "was", "ge\u00b7schehn", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da sprach er fieberschaurig:", "tokens": ["Da", "sprach", "er", "fie\u00b7ber\u00b7schau\u00b7rig", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "\u00bbo Knapp, das ist nicht sch\u00f6n!", "tokens": ["\u00bb", "o", "Knapp", ",", "das", "ist", "nicht", "sch\u00f6n", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "NN", "$,", "PDS", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "L\u00e4ssest du dein'n Herren schwitzen", "tokens": ["L\u00e4s\u00b7sest", "du", "dein'n", "Her\u00b7ren", "schwit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In solcher Not und Plag',", "tokens": ["In", "sol\u00b7cher", "Not", "und", "Plag'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So sollt du \u00fcbersitzen", "tokens": ["So", "sollt", "du", "\u00fc\u00b7ber\u00b7sit\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bis an den J\u00fcngsten Tag!\u00ab", "tokens": ["Bis", "an", "den", "J\u00fcng\u00b7sten", "Tag", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Er sprach's und starb im Fieber,", "tokens": ["Er", "sprach's", "und", "starb", "im", "Fie\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sein letztes Wort traf zu,", "tokens": ["Sein", "letz\u00b7tes", "Wort", "traf", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Knapp sitzt heut noch \u00fcber,", "tokens": ["Der", "Knapp", "sitzt", "heut", "noch", "\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "APPR", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Es l\u00e4\u00dft ihm keine Ruh'.", "tokens": ["Es", "l\u00e4\u00dft", "ihm", "kei\u00b7ne", "Ruh'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Und nachts wie Sturmgewitter", "tokens": ["Und", "nachts", "wie", "Sturm\u00b7ge\u00b7wit\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "KOKOM", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Jagt's oft stra\u00dfauf, stra\u00dfab,", "tokens": ["Jagt's", "oft", "stra\u00df\u00b7auf", ",", "stra\u00df\u00b7ab", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "ADV", "PTKVZ", "$,", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das ist der alte Ritter,", "tokens": ["Das", "ist", "der", "al\u00b7te", "Rit\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Er ruft: \u00bbWo steckt mein Knapp?!\u00ab", "tokens": ["Er", "ruft", ":", "\u00bb", "Wo", "steckt", "mein", "Knapp", "?!", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "PWAV", "VVFIN", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}