{"textgrid.poem.53861": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Der Graben", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mutter, wozu hast du deinen aufgezogen?", "tokens": ["Mut\u00b7ter", ",", "wo\u00b7zu", "hast", "du", "dei\u00b7nen", "auf\u00b7ge\u00b7zo\u00b7gen", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "VAFIN", "PPER", "PPOSAT", "VVPP", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Hast dich zwanzig Jahr mit ihm gequ\u00e4lt?", "tokens": ["Hast", "dich", "zwan\u00b7zig", "Jahr", "mit", "ihm", "ge\u00b7qu\u00e4lt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "CARD", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wozu ist er dir in deinen Arm geflogen,", "tokens": ["Wo\u00b7zu", "ist", "er", "dir", "in", "dei\u00b7nen", "Arm", "ge\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PPER", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "und du hast ihm leise was erz\u00e4hlt?", "tokens": ["und", "du", "hast", "ihm", "lei\u00b7se", "was", "er\u00b7z\u00e4hlt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPER", "ADJD", "PWS", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Bis sie ihn dir weggenommen haben.", "tokens": ["Bis", "sie", "ihn", "dir", "weg\u00b7ge\u00b7nom\u00b7men", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PPER", "VVPP", "VAINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "F\u00fcr den Graben, Mutter, f\u00fcr den Graben.", "tokens": ["F\u00fcr", "den", "Gra\u00b7ben", ",", "Mut\u00b7ter", ",", "f\u00fcr", "den", "Gra\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Junge, kannst du noch an Vater denken?", "tokens": ["Jun\u00b7ge", ",", "kannst", "du", "noch", "an", "Va\u00b7ter", "den\u00b7ken", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VMFIN", "PPER", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Vater nahm dich oft auf seinen Arm.", "tokens": ["Va\u00b7ter", "nahm", "dich", "oft", "auf", "sei\u00b7nen", "Arm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und er wollt dir einen Groschen schenken,", "tokens": ["Und", "er", "wollt", "dir", "ei\u00b7nen", "Gro\u00b7schen", "schen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "und er spielte mit dir R\u00e4uber und Gendarm.", "tokens": ["und", "er", "spiel\u00b7te", "mit", "dir", "R\u00e4u\u00b7ber", "und", "Gen\u00b7darm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPER", "NN", "KON", "NN", "$."], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Bis sie ihn dir weggenommen haben.", "tokens": ["Bis", "sie", "ihn", "dir", "weg\u00b7ge\u00b7nom\u00b7men", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PPER", "VVPP", "VAINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "F\u00fcr den Graben, Junge, f\u00fcr den Graben.", "tokens": ["F\u00fcr", "den", "Gra\u00b7ben", ",", "Jun\u00b7ge", ",", "f\u00fcr", "den", "Gra\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Dr\u00fcben die franz\u00f6sischen Genossen", "tokens": ["Dr\u00fc\u00b7ben", "die", "fran\u00b7z\u00f6\u00b7si\u00b7schen", "Ge\u00b7nos\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "lagen dicht bei Englands Arbeitsmann.", "tokens": ["la\u00b7gen", "dicht", "bei", "En\u00b7glands", "Ar\u00b7beits\u00b7mann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "NE", "NE", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Alle haben sie ihr Blut vergossen,", "tokens": ["Al\u00b7le", "ha\u00b7ben", "sie", "ihr", "Blut", "ver\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "und zerschossen ruht heut Mann bei Mann.", "tokens": ["und", "zer\u00b7schos\u00b7sen", "ruht", "heut", "Mann", "bei", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "VVFIN", "ADV", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Alte Leute, M\u00e4nner, mancher Knabe", "tokens": ["Al\u00b7te", "Leu\u00b7te", ",", "M\u00e4n\u00b7ner", ",", "man\u00b7cher", "Kna\u00b7be"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "NN", "$,", "PIAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "in dem einen gro\u00dfen Massengrabe.", "tokens": ["in", "dem", "ei\u00b7nen", "gro\u00b7\u00dfen", "Mas\u00b7sen\u00b7gra\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Seid nicht stolz auf Orden und Geklunker!", "tokens": ["Seid", "nicht", "stolz", "auf", "Or\u00b7den", "und", "Ge\u00b7klun\u00b7ker", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "PTKNEG", "ADJD", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Seid nicht stolz auf Narben und die Zeit!", "tokens": ["Seid", "nicht", "stolz", "auf", "Nar\u00b7ben", "und", "die", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "PTKNEG", "ADJD", "APPR", "NN", "KON", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "In die Gr\u00e4ben schickten euch die Junker,", "tokens": ["In", "die", "Gr\u00e4\u00b7ben", "schick\u00b7ten", "euch", "die", "Jun\u00b7ker", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Staatswahn und der Fabrikantenneid.", "tokens": ["Staats\u00b7wahn", "und", "der", "Fab\u00b7ri\u00b7kan\u00b7ten\u00b7neid", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Ihr wart gut genug zum Fra\u00df f\u00fcr Raben,", "tokens": ["Ihr", "wart", "gut", "ge\u00b7nug", "zum", "Fra\u00df", "f\u00fcr", "Ra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ADV", "APPRART", "NN", "APPR", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "f\u00fcr das Grab, Kamraden, f\u00fcr den Graben!", "tokens": ["f\u00fcr", "das", "Grab", ",", "Kam\u00b7ra\u00b7den", ",", "f\u00fcr", "den", "Gra\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Werft die Fahnen fort!", "tokens": ["Werft", "die", "Fah\u00b7nen", "fort", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Die Milit\u00e4rkapellen", "tokens": ["Die", "Mi\u00b7li\u00b7t\u00e4r\u00b7ka\u00b7pel\u00b7len"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "spielen auf zu euerm Todestanz.", "tokens": ["spie\u00b7len", "auf", "zu", "eu\u00b7erm", "To\u00b7des\u00b7tanz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Seid ihr hin: ein Kranz von Immortellen \u2013", "tokens": ["Seid", "ihr", "hin", ":", "ein", "Kranz", "von", "Im\u00b7mor\u00b7tel\u00b7len", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "PPER", "PTKVZ", "$.", "ART", "NN", "APPR", "NN", "$("], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.5": {"text": "das ist dann der Dank des Vaterlands.", "tokens": ["das", "ist", "dann", "der", "Dank", "des", "Va\u00b7ter\u00b7lands", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Denkt an Todesr\u00f6cheln und Gest\u00f6hne.", "tokens": ["Denkt", "an", "To\u00b7des\u00b7r\u00f6\u00b7cheln", "und", "Ge\u00b7st\u00f6h\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Dr\u00fcben stehen V\u00e4ter, M\u00fctter, S\u00f6hne,", "tokens": ["Dr\u00fc\u00b7ben", "ste\u00b7hen", "V\u00e4\u00b7ter", ",", "M\u00fct\u00b7ter", ",", "S\u00f6h\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "schuften schwer, wie ihr, ums bi\u00dfchen Leben.", "tokens": ["schuf\u00b7ten", "schwer", ",", "wie", "ihr", ",", "ums", "bi\u00df\u00b7chen", "Le\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,", "PWAV", "PPER", "$,", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wollt ihr denen nicht die H\u00e4nde geben?", "tokens": ["Wollt", "ihr", "de\u00b7nen", "nicht", "die", "H\u00e4n\u00b7de", "ge\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PDS", "PTKNEG", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Reicht die Bruderhand als sch\u00f6nste aller Gaben", "tokens": ["Reicht", "die", "Bru\u00b7der\u00b7hand", "als", "sch\u00f6ns\u00b7te", "al\u00b7ler", "Ga\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "KOKOM", "VVFIN", "PIAT", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.6": {"text": "\u00fcbern Graben, Leute, \u00fcbern Graben \u2013!", "tokens": ["\u00fc\u00b7bern", "Gra\u00b7ben", ",", "Leu\u00b7te", ",", "\u00fc\u00b7bern", "Gra\u00b7ben", "\u2013", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "$,", "NN", "$,", "ADJA", "NN", "$(", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}