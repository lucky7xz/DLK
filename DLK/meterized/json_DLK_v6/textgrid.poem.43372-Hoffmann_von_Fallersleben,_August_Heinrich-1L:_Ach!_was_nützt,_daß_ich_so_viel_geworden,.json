{"textgrid.poem.43372": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ach! was n\u00fctzt, da\u00df ich so viel geworden,", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach! was n\u00fctzt, da\u00df ich so viel geworden,", "tokens": ["Ach", "!", "was", "n\u00fctzt", ",", "da\u00df", "ich", "so", "viel", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWS", "VVFIN", "$,", "KOUS", "PPER", "ADV", "ADV", "VAPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und da\u00df ich so vieles nenne mein?", "tokens": ["Und", "da\u00df", "ich", "so", "vie\u00b7les", "nen\u00b7ne", "mein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "PIS", "VVFIN", "PPOSAT", "$."], "meter": "-+------+", "measure": "dactylic.init"}, "line.3": {"text": "Gro\u00dfer Gott, mir fehlet noch ein Orden,", "tokens": ["Gro\u00b7\u00dfer", "Gott", ",", "mir", "feh\u00b7let", "noch", "ein", "Or\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "K\u00f6nntest du mir solchen doch verleihn!", "tokens": ["K\u00f6nn\u00b7test", "du", "mir", "sol\u00b7chen", "doch", "ver\u00b7leihn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "PIAT", "ADV", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Ja, und w\u00e4r's vielleicht auch nur ein kleiner,", "tokens": ["Ja", ",", "und", "w\u00e4r's", "viel\u00b7leicht", "auch", "nur", "ein", "klei\u00b7ner", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KON", "VAFIN", "ADV", "ADV", "ADV", "ART", "ADJA", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Den der kleinste Potentat ersann;", "tokens": ["Den", "der", "kleins\u00b7te", "Po\u00b7ten\u00b7tat", "er\u00b7sann", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Immer besser einer doch als keiner,", "tokens": ["Im\u00b7mer", "bes\u00b7ser", "ei\u00b7ner", "doch", "als", "kei\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "ADV", "KOUS", "PIS", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ziert der kleinste doch auch seinen Mann.", "tokens": ["Ziert", "der", "kleins\u00b7te", "doch", "auch", "sei\u00b7nen", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "ADV", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Sch\u00f6n' Erfindung, da\u00df ein kleines Zeichen", "tokens": ["Sch\u00f6n'", "Er\u00b7fin\u00b7dung", ",", "da\u00df", "ein", "klei\u00b7nes", "Zei\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "NN", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "So viel Ehre, Freud' und Gl\u00fcck umh\u00fcllt!", "tokens": ["So", "viel", "Eh\u00b7re", ",", "Freud'", "und", "Gl\u00fcck", "um\u00b7h\u00fcllt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$,", "NN", "KON", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Nichts auf Erden w\u00fcsst' ich dem zu gleichen,", "tokens": ["Nichts", "auf", "Er\u00b7den", "w\u00fcsst'", "ich", "dem", "zu", "glei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "NN", "VVFIN", "PPER", "ART", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Was so sinnig seinen Zweck erf\u00fcllt.", "tokens": ["Was", "so", "sin\u00b7nig", "sei\u00b7nen", "Zweck", "er\u00b7f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADJD", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Wenn die Engel einst mit mir entschweben,", "tokens": ["Wenn", "die", "En\u00b7gel", "einst", "mit", "mir", "ent\u00b7schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Stehn die Seel'gen da erstaunt und stumm,", "tokens": ["Stehn", "die", "Seel'\u00b7gen", "da", "er\u00b7staunt", "und", "stumm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Sonn' und Mond und alle Sterne beben,", "tokens": ["Sonn'", "und", "Mond", "und", "al\u00b7le", "Ster\u00b7ne", "be\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Meine Seele hat den Orden um.", "tokens": ["Mei\u00b7ne", "See\u00b7le", "hat", "den", "Or\u00b7den", "um", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}