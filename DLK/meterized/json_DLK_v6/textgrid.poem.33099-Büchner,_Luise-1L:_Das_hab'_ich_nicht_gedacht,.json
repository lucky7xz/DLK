{"textgrid.poem.33099": {"metadata": {"author": {"name": "B\u00fcchner, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Das hab' ich nicht gedacht,", "genre": "verse", "period": "N.A.", "pub_year": 1849, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das hab' ich nicht gedacht,", "tokens": ["Das", "hab'", "ich", "nicht", "ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Als Blatt und Bl\u00fcthe spro\u00dften,", "tokens": ["Als", "Blatt", "und", "Bl\u00fc\u00b7the", "spro\u00df\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und ich voll Fr\u00f6hlichkeit:", "tokens": ["Und", "ich", "voll", "Fr\u00f6h\u00b7lich\u00b7keit", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df mich die Sommerzeit", "tokens": ["Da\u00df", "mich", "die", "Som\u00b7mer\u00b7zeit"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Mein ganzes Gl\u00fcck soll kosten.", "tokens": ["Mein", "gan\u00b7zes", "Gl\u00fcck", "soll", "kos\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Das hab' ich nicht gedacht \u2013", "tokens": ["Das", "hab'", "ich", "nicht", "ge\u00b7dacht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "PTKNEG", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wie sollt' ich es auch meinen?", "tokens": ["Wie", "sollt'", "ich", "es", "auch", "mei\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Als diese Haide gr\u00fcn,", "tokens": ["Als", "die\u00b7se", "Hai\u00b7de", "gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df bis zu ihrem Bl\u00fch'n", "tokens": ["Da\u00df", "bis", "zu", "ih\u00b7rem", "Bl\u00fch'n"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ich so viel m\u00fc\u00dfte weinen.", "tokens": ["Ich", "so", "viel", "m\u00fc\u00df\u00b7te", "wei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Das hab' ich nicht gedacht,", "tokens": ["Das", "hab'", "ich", "nicht", "ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Es g\u00e4b' noch tief'res Leide,", "tokens": ["Es", "g\u00e4b'", "noch", "tie\u00b7f'\u00b7res", "Lei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Als sonst mein Herz empfand,", "tokens": ["Als", "sonst", "mein", "Herz", "emp\u00b7fand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wenn rings das ganze Land", "tokens": ["Wenn", "rings", "das", "gan\u00b7ze", "Land"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Sich barg im Winterkleide.", "tokens": ["Sich", "barg", "im", "Win\u00b7ter\u00b7klei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "O, Freude, gute Nacht!", "tokens": ["O", ",", "Freu\u00b7de", ",", "gu\u00b7te", "Nacht", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wie ich's auch mag bedenken,", "tokens": ["Wie", "ich's", "auch", "mag", "be\u00b7den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Meine Lust ist all' dahin \u2013", "tokens": ["Mei\u00b7ne", "Lust", "ist", "all'", "da\u00b7hin", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PIS", "PAV", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mag's schneien oder bl\u00fch'n,", "tokens": ["Mag's", "schnei\u00b7en", "o\u00b7der", "bl\u00fch'n", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Mich kann jetzt nichts mehr kr\u00e4nken!", "tokens": ["Mich", "kann", "jetzt", "nichts", "mehr", "kr\u00e4n\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PIS", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}