{"textgrid.poem.43541": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ward ein Bl\u00fcmchen mir geschenket,", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ward ein Bl\u00fcmchen mir geschenket,", "tokens": ["Ward", "ein", "Bl\u00fcm\u00b7chen", "mir", "ge\u00b7schen\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hab's gepflanzt und hab's getr\u00e4nket.", "tokens": ["Hab's", "ge\u00b7pflanzt", "und", "hab's", "ge\u00b7tr\u00e4n\u00b7ket", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVPP", "KON", "NE", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "V\u00f6gel, kommt und gebet Acht!", "tokens": ["V\u00f6\u00b7gel", ",", "kommt", "und", "ge\u00b7bet", "Acht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "KON", "NN", "CARD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Gelt, ich hab' es recht gemacht?", "tokens": ["Gelt", ",", "ich", "hab'", "es", "recht", "ge\u00b7macht", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sonne, la\u00df mein Bl\u00fcmchen sprie\u00dfen!", "tokens": ["Son\u00b7ne", ",", "la\u00df", "mein", "Bl\u00fcm\u00b7chen", "sprie\u00b7\u00dfen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wolke, komm, es zu begie\u00dfen!", "tokens": ["Wol\u00b7ke", ",", "komm", ",", "es", "zu", "be\u00b7gie\u00b7\u00dfen", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Richt' empor dein Angesicht,", "tokens": ["Richt'", "em\u00b7por", "dein", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Liebes Bl\u00fcmchen, f\u00fcrcht' dich nicht!", "tokens": ["Lie\u00b7bes", "Bl\u00fcm\u00b7chen", ",", "f\u00fcrcht'", "dich", "nicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und ich kann es kaum erwarten,", "tokens": ["Und", "ich", "kann", "es", "kaum", "er\u00b7war\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "T\u00e4glich geh' ich in den Garten,", "tokens": ["T\u00e4g\u00b7lich", "geh'", "ich", "in", "den", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "T\u00e4glich frag' ich: Bl\u00fcmchen, sprich,", "tokens": ["T\u00e4g\u00b7lich", "frag'", "ich", ":", "Bl\u00fcm\u00b7chen", ",", "sprich", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "$.", "NN", "$,", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Bl\u00fcmchen, bist du b\u00f6s' auf mich?", "tokens": ["Bl\u00fcm\u00b7chen", ",", "bist", "du", "b\u00f6s'", "auf", "mich", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "PPER", "ADJD", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Sonne lie\u00df mein Bl\u00fcmchen sprie\u00dfen,", "tokens": ["Son\u00b7ne", "lie\u00df", "mein", "Bl\u00fcm\u00b7chen", "sprie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wolke kam es zu begie\u00dfen;", "tokens": ["Wol\u00b7ke", "kam", "es", "zu", "be\u00b7gie\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Jedes hat sich brav gem\u00fcht,", "tokens": ["Je\u00b7des", "hat", "sich", "brav", "ge\u00b7m\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PRF", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und mein liebes Bl\u00fcmchen bl\u00fcht.", "tokens": ["Und", "mein", "lie\u00b7bes", "Bl\u00fcm\u00b7chen", "bl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wie's vor lauter Freuden weinet!", "tokens": ["Wie's", "vor", "lau\u00b7ter", "Freu\u00b7den", "wei\u00b7net", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Freut sich, da\u00df die Sonne scheinet.", "tokens": ["Freut", "sich", ",", "da\u00df", "die", "Son\u00b7ne", "schei\u00b7net", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "$,", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schmetterlinge, fliegt herbei,", "tokens": ["Schmet\u00b7ter\u00b7lin\u00b7ge", ",", "fliegt", "her\u00b7bei", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sagt ihm doch, wie sch\u00f6n es sei!", "tokens": ["Sagt", "ihm", "doch", ",", "wie", "sch\u00f6n", "es", "sei", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "PWAV", "ADJD", "PPER", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}