{"dta.poem.8662": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Im Fr\u00fchling.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "An ihren bunten Liedern klettert", "tokens": ["An", "ih\u00b7ren", "bun\u00b7ten", "Lie\u00b7dern", "klet\u00b7tert"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Lerche selig in die Luft;", "tokens": ["Die", "Ler\u00b7che", "se\u00b7lig", "in", "die", "Luft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Jubelchor von S\u00e4ngern schmettert", "tokens": ["Ein", "Ju\u00b7be\u00b7lchor", "von", "S\u00e4n\u00b7gern", "schmet\u00b7tert"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Im Walde, voller Bl\u00fcth' und Duft.", "tokens": ["Im", "Wal\u00b7de", ",", "vol\u00b7ler", "Bl\u00fcth'", "und", "Duft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Da sind, so weit die Blicke gleiten", "tokens": ["Da", "sind", ",", "so", "weit", "die", "Bli\u00b7cke", "glei\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "$,", "ADV", "ADJD", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Alt\u00e4re festlich aufgebaut,", "tokens": ["Al\u00b7t\u00e4\u00b7re", "fest\u00b7lich", "auf\u00b7ge\u00b7baut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und all' die tausend Herzen l\u00e4uten", "tokens": ["Und", "all'", "die", "tau\u00b7send", "Her\u00b7zen", "l\u00e4u\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "ART", "CARD", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zur Liebesfeier dringend laut.", "tokens": ["Zur", "Lie\u00b7be\u00b7sfei\u00b7er", "drin\u00b7gend", "laut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der Lenz hat Rosen angez\u00fcndet", "tokens": ["Der", "Lenz", "hat", "Ro\u00b7sen", "an\u00b7ge\u00b7z\u00fcn\u00b7det"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "An Leuchtern von Smaragd im Dom,", "tokens": ["An", "Leuch\u00b7tern", "von", "Sma\u00b7ragd", "im", "Dom", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "APPRART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Und jede Seele schwillt und m\u00fcndet", "tokens": ["Und", "je\u00b7de", "See\u00b7le", "schwillt", "und", "m\u00fcn\u00b7det"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hin\u00fcber in den Opferstrom.", "tokens": ["Hin\u00b7\u00fc\u00b7ber", "in", "den", "Op\u00b7fer\u00b7strom", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}