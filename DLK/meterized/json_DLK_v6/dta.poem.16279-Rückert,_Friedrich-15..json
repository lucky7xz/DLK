{"dta.poem.16279": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "15.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn dir das Himmelslicht durchs Fenster ist zuwider,", "tokens": ["Wenn", "dir", "das", "Him\u00b7mels\u00b7licht", "durchs", "Fens\u00b7ter", "ist", "zu\u00b7wi\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPRART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So z\u00fcnde Kerzen an, und la\u00df den Vorhang nieder!", "tokens": ["So", "z\u00fcn\u00b7de", "Ker\u00b7zen", "an", ",", "und", "la\u00df", "den", "Vor\u00b7hang", "nie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "PTKVZ", "$,", "KON", "VVIMP", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Leicht hast du dir zur Nacht den Stubentag gemacht,", "tokens": ["Leicht", "hast", "du", "dir", "zur", "Nacht", "den", "Stu\u00b7ben\u00b7tag", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "PPER", "APPRART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch drau\u00dfen in der Welt wird es davon nicht Nacht.", "tokens": ["Doch", "drau\u00b7\u00dfen", "in", "der", "Welt", "wird", "es", "da\u00b7von", "nicht", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "VAFIN", "PP