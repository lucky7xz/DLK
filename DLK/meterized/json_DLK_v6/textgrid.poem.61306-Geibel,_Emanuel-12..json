{"textgrid.poem.61306": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "12.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der sei noch nicht des Lorbeers wert gehalten,", "tokens": ["Der", "sei", "noch", "nicht", "des", "Lor\u00b7beers", "wert", "ge\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "PTKNEG", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zu dessen Wohllaut Ohr und Sinn sich neigen;", "tokens": ["Zu", "des\u00b7sen", "Wohl\u00b7laut", "Ohr", "und", "Sinn", "sich", "nei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "NN", "KON", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dem Dichter sei der Blick des Sehers eigen,", "tokens": ["Dem", "Dich\u00b7ter", "sei", "der", "Blick", "des", "Se\u00b7hers", "ei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der fromm vertraut ist mit des Schicksals Walten.", "tokens": ["Der", "fromm", "ver\u00b7traut", "ist", "mit", "des", "Schick\u00b7sals", "Wal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJD", "VAFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ihm mu\u00df im Kampf des Neuen sich und Alten", "tokens": ["Ihm", "mu\u00df", "im", "Kampf", "des", "Neu\u00b7en", "sich", "und", "Al\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPRART", "NN", "ART", "NN", "PRF", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Durch alle Zeit des Lebens Werkstatt zeigen,", "tokens": ["Durch", "al\u00b7le", "Zeit", "des", "Le\u00b7bens", "Werk\u00b7statt", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "An Schuld und S\u00fchnung mu\u00df sich ihm der Reigen", "tokens": ["An", "Schuld", "und", "S\u00fch\u00b7nung", "mu\u00df", "sich", "ihm", "der", "Rei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "VMFIN", "PRF", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der ew'gen Weltgesetze still entfalten.", "tokens": ["Der", "ew'\u00b7gen", "Welt\u00b7ge\u00b7set\u00b7ze", "still", "ent\u00b7fal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nur wenn er in sich tr\u00e4gt das Ma\u00df der Dinge,", "tokens": ["Nur", "wenn", "er", "in", "sich", "tr\u00e4gt", "das", "Ma\u00df", "der", "Din\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "APPR", "PRF", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Geb\u00fchrt es ihm, da\u00df er die Dinge schlichte,", "tokens": ["Ge\u00b7b\u00fchrt", "es", "ihm", ",", "da\u00df", "er", "die", "Din\u00b7ge", "schlich\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "PPER", "PPER", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Gelingt es ihm, da\u00df er die Sphinx bezwinge.", "tokens": ["Ge\u00b7lingt", "es", "ihm", ",", "da\u00df", "er", "die", "Sphinx", "be\u00b7zwin\u00b7ge", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Dann aber wird ihm alles zum Gedichte,", "tokens": ["Dann", "a\u00b7ber", "wird", "ihm", "al\u00b7les", "zum", "Ge\u00b7dich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "PIS", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Denn alles wirkt und deutet mit im Ringe,", "tokens": ["Denn", "al\u00b7les", "wirkt", "und", "deu\u00b7tet", "mit", "im", "Rin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "KON", "VVFIN", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und was er singt, ist wie die Weltgeschichte.", "tokens": ["Und", "was", "er", "singt", ",", "ist", "wie", "die", "Welt\u00b7ge\u00b7schich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "$,", "VAFIN", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}