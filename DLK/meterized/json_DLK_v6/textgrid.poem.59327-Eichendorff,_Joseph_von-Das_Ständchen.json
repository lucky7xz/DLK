{"textgrid.poem.59327": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Das St\u00e4ndchen", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf die D\u00e4cher zwischen blassen", "tokens": ["Auf", "die", "D\u00e4\u00b7cher", "zwi\u00b7schen", "blas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wolken scheint der Mond herf\u00fcr,", "tokens": ["Wol\u00b7ken", "scheint", "der", "Mond", "her\u00b7f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ein Student dort auf der Gassen", "tokens": ["Ein", "Stu\u00b7dent", "dort", "auf", "der", "Gas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "APPR", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Singt vor seiner Liebsten T\u00fcr.", "tokens": ["Singt", "vor", "sei\u00b7ner", "Liebs\u00b7ten", "T\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und die Brunnen rauschen wieder", "tokens": ["Und", "die", "Brun\u00b7nen", "rau\u00b7schen", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch die stille Einsamkeit,", "tokens": ["Durch", "die", "stil\u00b7le", "Ein\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Wald vom Berge nieder,", "tokens": ["Und", "der", "Wald", "vom", "Ber\u00b7ge", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Wie in alter, sch\u00f6ner Zeit.", "tokens": ["Wie", "in", "al\u00b7ter", ",", "sch\u00f6\u00b7ner", "Zeit", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "So in meinen jungen Tagen", "tokens": ["So", "in", "mei\u00b7nen", "jun\u00b7gen", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hab ich manche Sommernacht", "tokens": ["Hab", "ich", "man\u00b7che", "Som\u00b7mer\u00b7nacht"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPER", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Auch die Laute hier geschlagen", "tokens": ["Auch", "die", "Lau\u00b7te", "hier", "ge\u00b7schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ADV", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und manch lust'ges Lied erdacht.", "tokens": ["Und", "manch", "lust'\u00b7ges", "Lied", "er\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "VVPP", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.4": {"line.1": {"text": "Aber von der stillen Schwelle", "tokens": ["A\u00b7ber", "von", "der", "stil\u00b7len", "Schwel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Trugen sie mein Lieb zur Ruh \u2013", "tokens": ["Tru\u00b7gen", "sie", "mein", "Lieb", "zur", "Ruh", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "APPRART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und du, fr\u00f6hlicher Geselle,", "tokens": ["Und", "du", ",", "fr\u00f6h\u00b7li\u00b7cher", "Ge\u00b7sel\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Singe, sing nur immer zu!", "tokens": ["Sin\u00b7ge", ",", "sing", "nur", "im\u00b7mer", "zu", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ADV", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}