{"textgrid.poem.31708": {"metadata": {"author": {"name": "Weerth, Georg", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin noch gar so jung", "tokens": ["Ich", "bin", "noch", "gar", "so", "jung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADV", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und liebe schon den Trunk.", "tokens": ["Und", "lie\u00b7be", "schon", "den", "Trunk", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O heiliger Sankt Peter,", "tokens": ["O", "hei\u00b7li\u00b7ger", "Sankt", "Pe\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "VVFIN", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was wird aus mir erst sp\u00e4ter,", "tokens": ["Was", "wird", "aus", "mir", "erst", "sp\u00e4\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "APPR", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Was wird aus mir erst werden, ach,", "tokens": ["Was", "wird", "aus", "mir", "erst", "wer\u00b7den", ",", "ach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "VAFIN", "APPR", "PPER", "ADV", "VAINF", "$,", "ITJ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wohl \u00fcber Jahr und Tag!", "tokens": ["Wohl", "\u00fc\u00b7ber", "Jahr", "und", "Tag", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}