{"dta.poem.23713": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Mittel freundlich zu werden.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-200905197682", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Vater k&#252;\u00dft die Mutter,", "tokens": ["Mein", "Va\u00b7ter", "k", "&#252;", "\u00dft", "die", "Mut\u00b7ter", ","], "token_info": ["word", "word", "word", "XML_entity", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NE", "$(", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die Mutter k\u00fc\u00dft den Vater,", "tokens": ["Die", "Mut\u00b7ter", "k\u00fc\u00dft", "den", "Va\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und wenn sie beide k\u00fcssen,", "tokens": ["Und", "wenn", "sie", "bei\u00b7de", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PIS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So sind sie beide freundlich.", "tokens": ["So", "sind", "sie", "bei\u00b7de", "freund\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIS", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wie oft sagt meine Mutter:", "tokens": ["Wie", "oft", "sagt", "mei\u00b7ne", "Mut\u00b7ter", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mein Wilhelm werde freundlich!", "tokens": ["Mein", "Wil\u00b7helm", "wer\u00b7de", "freund\u00b7lich", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NE", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Nun will ich es schon werden;", "tokens": ["Nun", "will", "ich", "es", "schon", "wer\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "ADV", "VAINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Denn unsers Nachbars Tochter", "tokens": ["Denn", "un\u00b7sers", "Nach\u00b7bars", "Toch\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "L\u00e4\u00dft sich recht gerne k\u00fcssen.", "tokens": ["L\u00e4\u00dft", "sich", "recht", "ger\u00b7ne", "k\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}