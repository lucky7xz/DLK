{"dta.poem.5336": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der geschlagene Hund.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Neulich rannt ein grosser Hund, mit erb\u00e4rmlichem", "tokens": ["Neu\u00b7lich", "rannt", "ein", "gros\u00b7ser", "Hund", ",", "mit", "er\u00b7b\u00e4rm\u00b7li\u00b7chem"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,", "APPR", "ADJA"], "meter": "+-+-+-+--+--", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Geschrey,", "tokens": ["Ge\u00b7schrey", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Weil man ihn geschlagen hatte, Sporenstreichs mein Haus", "tokens": ["Weil", "man", "ihn", "ge\u00b7schla\u00b7gen", "hat\u00b7te", ",", "Spo\u00b7rens\u00b7treichs", "mein", "Haus"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PIS", "PPER", "VVPP", "VAFIN", "$,", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.4": {"text": "vorbey,", "tokens": ["vor\u00b7bey", ","], "token_info": ["word", "punct"], "pos": ["PTKVZ", "$,"], "meter": "++", "measure": "spondeus"}, "line.5": {"text": "Als ich an der Th\u00fcre stand. Dieser laute Ton durchdrang", "tokens": ["Als", "ich", "an", "der", "Th\u00fc\u00b7re", "stand", ".", "Die\u00b7ser", "lau\u00b7te", "Ton", "durch\u00b7drang"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$.", "PDAT", "ADJA", "NN", "VVFIN"], "meter": "--+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Nicht nur mein beleidigt Ohr, sondern der zu scharfe", "tokens": ["Nicht", "nur", "mein", "be\u00b7lei\u00b7digt", "Ohr", ",", "son\u00b7dern", "der", "zu", "schar\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "PPOSAT", "VVFIN", "NN", "$,", "KON", "ART", "PTKZU", "VVFIN"], "meter": "+-+-+-++-+-+-", "measure": "unknown.measure.septa"}, "line.7": {"text": "Klang", "tokens": ["Klang"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.8": {"text": "Drang mir durchs Geh\u00f6r ins Hertz. Da ich denn bewun-", "tokens": ["Drang", "mir", "durchs", "Ge\u00b7h\u00f6r", "ins", "Hertz", ".", "Da", "ich", "denn", "be\u00b7wun"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPER", "APPRART", "NN", "APPRART", "NN", "$.", "KOUS", "PPER", "ADV", "TRUNC"], "meter": "+---+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.9": {"text": "derte", "tokens": ["der\u00b7te"], "token_info": ["word"], "pos": ["NE"], "meter": "+-", "measure": "trochaic.single"}, "line.10": {"text": "Wie, durch wunderbare Wege, die Natur so gar den", "tokens": ["Wie", ",", "durch", "wun\u00b7der\u00b7ba\u00b7re", "We\u00b7ge", ",", "die", "Na\u00b7tur", "so", "gar", "den"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "$,", "APPR", "ADJA", "NN", "$,", "ART", "NN", "ADV", "ADV", "ART"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.11": {"text": "Thieren,", "tokens": ["Thie\u00b7ren", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.12": {"text": "Wenn sie Ungemach und Weh,", "tokens": ["Wenn", "sie", "Un\u00b7ge\u00b7mach", "und", "Weh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Welches ihren C\u00f6rpern sch\u00e4dlich, und beschwehrlich ist,", "tokens": ["Wel\u00b7ches", "ih\u00b7ren", "C\u00f6r\u00b7pern", "sch\u00e4d\u00b7lich", ",", "und", "be\u00b7schwehr\u00b7lich", "ist", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "ADJD", "$,", "KON", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.14": {"text": "versp\u00fchren,", "tokens": ["ver\u00b7sp\u00fch\u00b7ren", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.15": {"text": "Nicht nur einen Trieb zu schreyen, sondern Werck-Zeug\u2019", "tokens": ["Nicht", "nur", "ei\u00b7nen", "Trieb", "zu", "schre\u00b7yen", ",", "son\u00b7dern", "Wer\u00b7ck\u00b7Zeug'"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PTKNEG", "ADV", "ART", "NN", "PTKZU", "VVINF", "$,", "KON", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.16": {"text": "ihnen schenckt,", "tokens": ["ih\u00b7nen", "schenckt", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.17": {"text": "Wodurch laute T\u00f6n\u2019 erreget, und wir zur Aufmercksamkeit,", "tokens": ["Wo\u00b7durch", "lau\u00b7te", "T\u00f6n'", "er\u00b7re\u00b7get", ",", "und", "wir", "zur", "Auf\u00b7merck\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "NN", "VVFIN", "$,", "KON", "PPER", "APPRART", "NN", "$,"], "meter": "--+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "Ja zum Mitleid, wenigstens zur Verdrie\u00dflichkeit, gelencket,", "tokens": ["Ja", "zum", "Mit\u00b7leid", ",", "we\u00b7nigs\u00b7tens", "zur", "Ver\u00b7drie\u00df\u00b7lich\u00b7keit", ",", "ge\u00b7len\u00b7cket", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PTKANT", "APPRART", "NN", "$,", "ADV", "APPRART", "NN", "$,", "VVPP", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.19": {"text": "Ein so w\u00fcst Geschrey zu h\u00f6ren, wodurch sie denn oft", "tokens": ["Ein", "so", "w\u00fcst", "Ge\u00b7schrey", "zu", "h\u00f6\u00b7ren", ",", "wo\u00b7durch", "sie", "denn", "oft"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADV", "VVFIN", "NN", "PTKZU", "VVINF", "$,", "PWAV", "PPER", "ADV", "ADV"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.20": {"text": "befreit,", "tokens": ["be\u00b7freit", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.21": {"text": "Bald aus Mitleid zu uns selbst, bald aus Mitleid gegen", "tokens": ["Bald", "aus", "Mit\u00b7leid", "zu", "uns", "selbst", ",", "bald", "aus", "Mit\u00b7leid", "ge\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "APPR", "PPER", "ADV", "$,", "ADV", "APPR", "NN", "APPR"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "sie.", "tokens": ["sie", "."], "token_info": ["word", "punct"], "pos": ["PPER", "$."], "meter": "-", "measure": "single.down"}, "line.23": {"text": "Diesem Wunder in den T\u00f6nen, und den herrlichen Ge-", "tokens": ["Die\u00b7sem", "Wun\u00b7der", "in", "den", "T\u00f6\u00b7nen", ",", "und", "den", "herr\u00b7li\u00b7chen", "Ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "APPR", "ART", "NN", "$,", "KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.24": {"text": "setzen", "tokens": ["set\u00b7zen"], "token_info": ["word"], "pos": ["VVINF"], "meter": "+-", "measure": "trochaic.single"}, "line.25": {"text": "Der verst\u00e4ndigen Natur, dacht ich ferner, mit Erg\u00f6tzen", "tokens": ["Der", "ver\u00b7st\u00e4n\u00b7di\u00b7gen", "Na\u00b7tur", ",", "dacht", "ich", "fer\u00b7ner", ",", "mit", "Er\u00b7g\u00f6t\u00b7zen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "VVFIN", "PPER", "ADV", "$,", "APPR", "NN"], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.26": {"text": "Und mit Ehrfurcht, ernstlich nach. Letztlich kam ich von", "tokens": ["Und", "mit", "Ehr\u00b7furcht", ",", "ernst\u00b7lich", "nach", ".", "Letzt\u00b7lich", "kam", "ich", "von"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "$,", "ADJD", "PTKVZ", "$.", "ADV", "VVFIN", "PPER", "APPR"], "meter": "--+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.27": {"text": "dem Vieh", "tokens": ["dem", "Vieh"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+", "measure": "iambic.single"}}}}}