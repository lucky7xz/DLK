{"textgrid.poem.47935": {"metadata": {"author": {"name": "Stieler, Kaspar", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "verse", "period": "N.A.", "pub_year": 1669, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach ja! Es ist ein greiser Bahrt/", "tokens": ["Ach", "ja", "!", "Es", "ist", "ein", "grei\u00b7ser", "Bahrt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ITJ", "$.", "PPER", "VAFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "dem meine Venus nicht gef\u00e4llet/", "tokens": ["dem", "mei\u00b7ne", "Ve\u00b7nus", "nicht", "ge\u00b7f\u00e4l\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "PTKNEG", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "der ist von keuscher Art.", "tokens": ["der", "ist", "von", "keu\u00b7scher", "Art", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VAFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "die Keuschheit stekket in den Runzeln/", "tokens": ["die", "Keuschheit", "stek\u00b7ket", "in", "den", "Run\u00b7zeln", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.5": {"text": "ich habe keine Runzeln nicht/", "tokens": ["ich", "ha\u00b7be", "kei\u00b7ne", "Run\u00b7zeln", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "PTKNEG", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "ein schwarzes Haar erhellet mein Gesicht.", "tokens": ["ein", "schwar\u00b7zes", "Haar", "er\u00b7hel\u00b7let", "mein", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Die jungen Leute schmunzeln/", "tokens": ["Die", "jun\u00b7gen", "Leu\u00b7te", "schmun\u00b7zeln", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "wenn sie die Venus lesen:", "tokens": ["wenn", "sie", "die", "Ve\u00b7nus", "le\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "du Bleicher bleichst/ wenn du mein Singen h\u00f6rst/", "tokens": ["du", "Blei\u00b7cher", "bleichst", "/", "wenn", "du", "mein", "Sin\u00b7gen", "h\u00f6rst", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVFIN", "$(", "KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "das ist ein tolles Wesen.", "tokens": ["das", "ist", "ein", "tol\u00b7les", "We\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Iezt f\u00e4llt mirs ein/ woher es kommen mag:", "tokens": ["Iezt", "f\u00e4llt", "mirs", "ein", "/", "wo\u00b7her", "es", "kom\u00b7men", "mag", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "ART", "$(", "PWAV", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "du bl\u00e4ssest/ weil du f\u00fcrchtst den J\u00fcngsten Tag.", "tokens": ["du", "bl\u00e4s\u00b7sest", "/", "weil", "du", "f\u00fcrchtst", "den", "J\u00fcng\u00b7sten", "Tag", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}