{"textgrid.poem.59431": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Wacht auf!", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist ein Kirchlein zwischen Felsenbogen", "tokens": ["Es", "ist", "ein", "Kirch\u00b7lein", "zwi\u00b7schen", "Fel\u00b7sen\u00b7bo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So tief versteckt: wie in den alten Sagen", "tokens": ["So", "tief", "ver\u00b7steckt", ":", "wie", "in", "den", "al\u00b7ten", "Sa\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVPP", "$.", "PWAV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hat n\u00e4chtens drin die Glocke angeschlagen,", "tokens": ["Hat", "n\u00e4ch\u00b7tens", "drin", "die", "Glo\u00b7cke", "an\u00b7ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wei\u00df keiner, wer die Glocken hat gezogen.", "tokens": ["Wei\u00df", "kei\u00b7ner", ",", "wer", "die", "Glo\u00b7cken", "hat", "ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "PWS", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Erwache, Steuermann! hoch gehn die Wogen;", "tokens": ["Er\u00b7wa\u00b7che", ",", "Steu\u00b7er\u00b7mann", "!", "hoch", "gehn", "die", "Wo\u00b7gen", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$.", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ihr Hirten auf, die Herden nach euch fragen;", "tokens": ["Ihr", "Hir\u00b7ten", "auf", ",", "die", "Her\u00b7den", "nach", "euch", "fra\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKVZ", "$,", "ART", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ihr W\u00e4chter sollt an Schlo\u00df und H\u00fctten schlagen,", "tokens": ["Ihr", "W\u00e4ch\u00b7ter", "sollt", "an", "Schlo\u00df", "und", "H\u00fct\u00b7ten", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "APPR", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wacht auf, wacht auf, bevor der Klang verflogen!", "tokens": ["Wacht", "auf", ",", "wacht", "auf", ",", "be\u00b7vor", "der", "Klang", "ver\u00b7flo\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "VVFIN", "PTKVZ", "$,", "KOUS", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Denn Heerschau halten will in deutschen Gauen", "tokens": ["Denn", "Heer\u00b7schau", "hal\u00b7ten", "will", "in", "deut\u00b7schen", "Gau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVINF", "VMFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Herr und z\u00e4hlen, die ihm treu geblieben,", "tokens": ["Der", "Herr", "und", "z\u00e4h\u00b7len", ",", "die", "ihm", "treu", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "VVINF", "$,", "PRELS", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Eh er den Engel mit dem Schwerte sendet.", "tokens": ["Eh", "er", "den", "En\u00b7gel", "mit", "dem", "Schwer\u00b7te", "sen\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.4": {"line.1": {"text": "Schon bricht's so dunkelrot durchs Morgengrauen,", "tokens": ["Schon", "bricht's", "so", "dun\u00b7kel\u00b7rot", "durchs", "Mor\u00b7gen\u00b7grau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ob's Blut bedeutet oder feur'ges Lieben,", "tokens": ["Ob's", "Blut", "be\u00b7deu\u00b7tet", "o\u00b7der", "feur'\u00b7ges", "Lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "KON", "ADJA", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es steht in Gottes Hand, die niemand wendet.", "tokens": ["Es", "steht", "in", "Got\u00b7tes", "Hand", ",", "die", "nie\u00b7mand", "wen\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "NN", "$,", "PRELS", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}