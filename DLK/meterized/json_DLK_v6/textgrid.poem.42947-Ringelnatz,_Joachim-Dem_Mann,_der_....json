{"textgrid.poem.42947": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Dem Mann, der ...", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Mann, der meine Schuhe putzt", "tokens": ["Der", "Mann", ",", "der", "mei\u00b7ne", "Schu\u00b7he", "putzt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Am Bahnhofsplatz,", "tokens": ["Am", "Bahn\u00b7hofs\u00b7platz", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Hat abends, wenn er die Trambahn benutzt,", "tokens": ["Hat", "a\u00b7bends", ",", "wenn", "er", "die", "Tram\u00b7bahn", "be\u00b7nutzt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "$,", "KOUS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Neben sich einen Schatz.", "tokens": ["Ne\u00b7ben", "sich", "ei\u00b7nen", "Schatz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Wie gern w\u00fcrde ich diesem Kind", "tokens": ["Wie", "gern", "w\u00fcr\u00b7de", "ich", "die\u00b7sem", "Kind"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VAFIN", "PPER", "PDAT", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Auch mal die Schuhe reinigen.", "tokens": ["Auch", "mal", "die", "Schu\u00b7he", "rei\u00b7ni\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Jedoch sie sagt: \u00bbBaron, Sie sind", "tokens": ["Je\u00b7doch", "sie", "sagt", ":", "\u00bb", "Ba\u00b7ron", ",", "Sie", "sind"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "word"], "pos": ["ADV", "PPER", "VVFIN", "$.", "$(", "NN", "$,", "PPER", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein dickes Schweinigen.\u00ab", "tokens": ["Ein", "di\u00b7ckes", "Schwei\u00b7ni\u00b7gen", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Weil mir das Titelchen \u00bbBaron\u00ab", "tokens": ["Weil", "mir", "das", "Ti\u00b7tel\u00b7chen", "\u00bb", "Ba\u00b7ron", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "$(", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht zukommt noch mir nutzt,", "tokens": ["Nicht", "zu\u00b7kommt", "noch", "mir", "nutzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "ADV", "PPER", "VVFIN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Gab ich heute gro\u00dfen Extralohn", "tokens": ["Gab", "ich", "heu\u00b7te", "gro\u00b7\u00dfen", "Ext\u00b7ra\u00b7lohn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Dem Mann, der meine Schuhe putzt.", "tokens": ["Dem", "Mann", ",", "der", "mei\u00b7ne", "Schu\u00b7he", "putzt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}