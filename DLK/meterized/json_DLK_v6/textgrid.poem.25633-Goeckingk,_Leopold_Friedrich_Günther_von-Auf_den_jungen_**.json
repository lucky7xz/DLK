{"textgrid.poem.25633": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "Auf den jungen **", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich seh' ihn in Galopp durch alle Stra\u00dfen reiten,", "tokens": ["Ich", "seh'", "ihn", "in", "Ga\u00b7lopp", "durch", "al\u00b7le", "Stra\u00b7\u00dfen", "rei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NE", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch vor der Stadt h\u00e4lt er mit Jagen ein.", "tokens": ["Doch", "vor", "der", "Stadt", "h\u00e4lt", "er", "mit", "Ja\u00b7gen", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das ist doch sonderbar, ein Narr vor allen Leuten,", "tokens": ["Das", "ist", "doch", "son\u00b7der\u00b7bar", ",", "ein", "Narr", "vor", "al\u00b7len", "Leu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADJD", "$,", "ART", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vern\u00fcnftig nur, wenn's Niemand sieht, zu seyn.", "tokens": ["Ver\u00b7n\u00fcnf\u00b7tig", "nur", ",", "wenn's", "Nie\u00b7mand", "sieht", ",", "zu", "seyn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "ADV", "$,", "KOUS", "PIS", "VVFIN", "$,", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}