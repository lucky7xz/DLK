{"textgrid.poem.63181": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Ganymed", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zeus sandte seinen Adler,", "tokens": ["Zeus", "sand\u00b7te", "sei\u00b7nen", "Ad\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dass er den sch\u00f6nen Knaben Ganymed", "tokens": ["Dass", "er", "den", "sch\u00f6\u00b7nen", "Kna\u00b7ben", "Ga\u00b7ny\u00b7med"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In seinen F\u00e4ngen fange", "tokens": ["In", "sei\u00b7nen", "F\u00e4n\u00b7gen", "fan\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und zu ihm trage.", "tokens": ["Und", "zu", "ihm", "tra\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Der schoss aus dem Zenith", "tokens": ["Der", "schoss", "aus", "dem", "Ze\u00b7nith"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Des Mittags", "tokens": ["Des", "Mit\u00b7tags"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Herab auf die Narzissenwiese,", "tokens": ["Her\u00b7ab", "auf", "die", "Nar\u00b7zis\u00b7sen\u00b7wie\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wo Ganymed schlief,", "tokens": ["Wo", "Ga\u00b7ny\u00b7med", "schlief", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "NE", "VVFIN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Der Gelockte,", "tokens": ["Der", "Ge\u00b7lock\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.10": {"text": "Und von dem Adler tr\u00e4umte,", "tokens": ["Und", "von", "dem", "Ad\u00b7ler", "tr\u00e4um\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Der nach ihm stiess.", "tokens": ["Der", "nach", "ihm", "stiess", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "VVFIN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.12": {"text": "Er schrie im Traum.", "tokens": ["Er", "schrie", "im", "Traum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Der Adler mit dem gebogenen Horn des Schnabels", "tokens": ["Der", "Ad\u00b7ler", "mit", "dem", "ge\u00b7bo\u00b7ge\u00b7nen", "Horn", "des", "Schna\u00b7bels"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Den Knaben am G\u00fcrtel griff,", "tokens": ["Den", "Kna\u00b7ben", "am", "G\u00fcr\u00b7tel", "griff", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Am sch\u00f6n von der Mutter gestickten.", "tokens": ["Am", "sch\u00f6n", "von", "der", "Mut\u00b7ter", "ge\u00b7stick\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "\u00dcber Wolken und Winde und wehende Sterne", "tokens": ["\u00dc\u00b7ber", "Wol\u00b7ken", "und", "Win\u00b7de", "und", "we\u00b7hen\u00b7de", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "KON", "ADJA", "NN"], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.5": {"text": "Er flog mit ihm", "tokens": ["Er", "flog", "mit", "ihm"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Und legte ihn", "tokens": ["Und", "leg\u00b7te", "ihn"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Dem Gott zu F\u00fcssen.", "tokens": ["Dem", "Gott", "zu", "F\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Aber der Gott,", "tokens": ["A\u00b7ber", "der", "Gott", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Entz\u00fcndet von der Anmut,", "tokens": ["Ent\u00b7z\u00fcn\u00b7det", "von", "der", "An\u00b7mut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die er geschaffen,", "tokens": ["Die", "er", "ge\u00b7schaf\u00b7fen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Er neigte sich und nahm den Knaben in seine Arme", "tokens": ["Er", "neig\u00b7te", "sich", "und", "nahm", "den", "Kna\u00b7ben", "in", "sei\u00b7ne", "Ar\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "KON", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Und k\u00fcsste seine Wangen", "tokens": ["Und", "k\u00fcss\u00b7te", "sei\u00b7ne", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und k\u00fcsste seine Wimpern", "tokens": ["Und", "k\u00fcss\u00b7te", "sei\u00b7ne", "Wim\u00b7pern"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und k\u00fcsste seine Brust", "tokens": ["Und", "k\u00fcss\u00b7te", "sei\u00b7ne", "Brust"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Und k\u00fcsste seine Kniee", "tokens": ["Und", "k\u00fcss\u00b7te", "sei\u00b7ne", "Kni\u00b7ee"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Und k\u00fcsste seine Lippen", "tokens": ["Und", "k\u00fcss\u00b7te", "sei\u00b7ne", "Lip\u00b7pen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Und k\u00fcsste seinen Schoss.", "tokens": ["Und", "k\u00fcss\u00b7te", "sei\u00b7nen", "Schoss", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}