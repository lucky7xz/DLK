{"dta.poem.2814": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "15.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Soll tragen mit Geduld dein Lehrling Lernbeschwerden,", "tokens": ["Soll", "tra\u00b7gen", "mit", "Ge\u00b7duld", "dein", "Lehr\u00b7ling", "Lern\u00b7be\u00b7schwer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "APPR", "NN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So mu\u00dft du Lehrer selbst nicht ungeduldig werden.", "tokens": ["So", "mu\u00dft", "du", "Leh\u00b7rer", "selbst", "nicht", "un\u00b7ge\u00b7dul\u00b7dig", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "NN", "ADV", "PTKNEG", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Denn Schweres hat zu thun der Lehrling wie der Lehrer,", "tokens": ["Denn", "Schwe\u00b7res", "hat", "zu", "thun", "der", "Lehr\u00b7ling", "wie", "der", "Leh\u00b7rer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "PTKZU", "VVINF", "ART", "NN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das leichter durch Geduld, durch Ungeduld wird schwerer.", "tokens": ["Das", "leich\u00b7ter", "durch", "Ge\u00b7duld", ",", "durch", "Un\u00b7ge\u00b7duld", "wird", "schwe\u00b7rer", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "APPR", "NN", "$,", "APPR", "NN", "VAFIN", "ADJD",