{"dta.poem.20615": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Prinzessin !", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "als das eiserne Geschick", "tokens": ["als", "das", "ei\u00b7ser\u00b7ne", "Ge\u00b7schick"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+---+", "measure": "unknown.measure.tri"}, "line.2": {"text": "Der Atropos befahl: dem Leben", "tokens": ["Der", "A\u00b7tro\u00b7pos", "be\u00b7fahl", ":", "dem", "Le\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der deutschen Dichterin den letzten Augenblick", "tokens": ["Der", "deut\u00b7schen", "Dich\u00b7te\u00b7rin", "den", "letz\u00b7ten", "Au\u00b7gen\u00b7blick"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Durch ihrer Scheere Schnitt zu geben;", "tokens": ["Durch", "ih\u00b7rer", "Schee\u00b7re", "Schnitt", "zu", "ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Da flog die Muse her und seufzete: Apoll!", "tokens": ["Da", "flog", "die", "Mu\u00b7se", "her", "und", "seuf\u00b7ze\u00b7te", ":", "A\u00b7poll", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ", "KON", "VVFIN", "$.", "NE", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "O G\u00f6ttlichster! wer soll", "tokens": ["O", "G\u00f6tt\u00b7lichs\u00b7ter", "!", "wer", "soll"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "NN", "$.", "PWS", "VMFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Nun meiner S\u00e4ngerin bewahren ihre Leyer?", "tokens": ["Nun", "mei\u00b7ner", "S\u00e4n\u00b7ge\u00b7rin", "be\u00b7wah\u00b7ren", "ih\u00b7re", "Le\u00b7yer", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.8": {"text": "\u201edie Erste Grazie der Welt!\u201e rief er,", "tokens": ["\u201e", "die", "Ers\u00b7te", "Gra\u00b7zie", "der", "Welt", "!", "\u201e", "rief", "er", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "ART", "NN", "$.", "$(", "VVFIN", "PPER", "$,"], "meter": "-+-+--++-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "\u201everh\u00fclle sie in ihren Schleyer!\u201e", "tokens": ["\u201e", "ver\u00b7h\u00fcl\u00b7le", "sie", "in", "ih\u00b7ren", "Schle\u00b7yer", "!", "\u201e"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.10": {"text": "Die erste Grazie der Welt, sprach sie, o Sohn", "tokens": ["Die", "ers\u00b7te", "Gra\u00b7zie", "der", "Welt", ",", "sprach", "sie", ",", "o", "Sohn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,", "VVFIN", "PPER", "$,", "FM", "NN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Des Jupiters! schifft jetzt nach Albion \u2014", "tokens": ["Des", "Ju\u00b7pi\u00b7ters", "!", "schifft", "jetzt", "nach", "Al\u00b7bi\u00b7on"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "VVFIN", "ADV", "APPR", "NE", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Apollo wandte sich hinauf zum Sonnenthron \u2014", "tokens": ["A\u00b7pol\u00b7lo", "wand\u00b7te", "sich", "hin\u00b7auf", "zum", "Son\u00b7nen\u00b7thron"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "ADV", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Die Karschin sang als Schwan:", "tokens": ["Die", "Kar\u00b7schin", "sang", "als", "Schwan", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KOUS", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}