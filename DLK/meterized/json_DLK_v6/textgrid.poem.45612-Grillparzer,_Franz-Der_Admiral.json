{"textgrid.poem.45612": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Der Admiral", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In gar zu vielen F\u00e4chern bist du etwa t\u00e4tig,", "tokens": ["In", "gar", "zu", "vie\u00b7len", "F\u00e4\u00b7chern", "bist", "du", "et\u00b7wa", "t\u00e4\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "APPR", "PIAT", "NN", "VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Schwer pa\u00dft das Seemannswesen zur \u00c4sthetik.", "tokens": ["Schwer", "pa\u00dft", "das", "See\u00b7manns\u00b7we\u00b7sen", "zur", "\u00c4s\u00b7the\u00b7tik", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und gl\u00e4nzest du als Bauherr und Verfasser,", "tokens": ["Und", "gl\u00e4n\u00b7zest", "du", "als", "Bau\u00b7herr", "und", "Ver\u00b7fas\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "KOUS", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wird dir das bittre Meer gar leicht zum s\u00fc\u00dfen Wasser.", "tokens": ["Wird", "dir", "das", "bitt\u00b7re", "Meer", "gar", "leicht", "zum", "s\u00fc\u00b7\u00dfen", "Was\u00b7ser", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "ADV", "ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}