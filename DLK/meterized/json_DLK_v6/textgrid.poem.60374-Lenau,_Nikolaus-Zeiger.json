{"textgrid.poem.60374": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Zeiger", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Meiner Schwester liebe Sprossen,", "tokens": ["Mei\u00b7ner", "Schwes\u00b7ter", "lie\u00b7be", "Spros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ha, wie seid ihr aufgeschossen,", "tokens": ["Ha", ",", "wie", "seid", "ihr", "auf\u00b7ge\u00b7schos\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Seit ich \u00fcber Berg und Tal", "tokens": ["Seit", "ich", "\u00fc\u00b7ber", "Berg", "und", "Tal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Von euch schied das letzte Mal!", "tokens": ["Von", "euch", "schied", "das", "letz\u00b7te", "Mal", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da ihr wachset und euch dehnet,", "tokens": ["Da", "ihr", "wach\u00b7set", "und", "euch", "deh\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "KON", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sonnenzeiger unsrer Tage,", "tokens": ["Son\u00b7nen\u00b7zei\u00b7ger", "uns\u00b7rer", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Mahnt ihr, wie das Leben jage,", "tokens": ["Mahnt", "ihr", ",", "wie", "das", "Le\u00b7ben", "ja\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Das ihr fest und ewig w\u00e4hnet.", "tokens": ["Das", "ihr", "fest", "und", "e\u00b7wig", "w\u00e4h\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "PTKVZ", "KON", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Kinderwuchs und Abendschatten", "tokens": ["Kin\u00b7der\u00b7wuchs", "und", "A\u00b7bend\u00b7schat\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Zeigt dem Wandrer auf dem Steige", "tokens": ["Zeigt", "dem", "Wand\u00b7rer", "auf", "dem", "Stei\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Abgem\u00e4hter Blumenmatten,", "tokens": ["Ab\u00b7ge\u00b7m\u00e4h\u00b7ter", "Blu\u00b7men\u00b7mat\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Wie sich ihm die Sonne neige.", "tokens": ["Wie", "sich", "ihm", "die", "Son\u00b7ne", "nei\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}