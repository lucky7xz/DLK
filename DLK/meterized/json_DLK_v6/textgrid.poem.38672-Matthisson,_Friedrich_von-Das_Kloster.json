{"textgrid.poem.38672": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Das Kloster", "genre": "verse", "period": "N.A.", "pub_year": 1791, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Westgew\u00f6lke Purpursaum ergraut,", "tokens": ["Der", "West\u00b7ge\u00b7w\u00f6l\u00b7ke", "Pur\u00b7pur\u00b7saum", "er\u00b7graut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Aus Tannendunkel steigt der Mond empor,", "tokens": ["Aus", "Tan\u00b7nen\u00b7dun\u00b7kel", "steigt", "der", "Mond", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Winde seufzen bang im Haidekraut,", "tokens": ["Die", "Win\u00b7de", "seuf\u00b7zen", "bang", "im", "Hai\u00b7de\u00b7kraut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Elfen Tanz webt leis' am Weidenmoor.", "tokens": ["Der", "El\u00b7fen", "Tanz", "webt", "leis'", "am", "Wei\u00b7den\u00b7moor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Des hohen Pharus tr\u00fcbe Leucht' entglimmt", "tokens": ["Des", "ho\u00b7hen", "Pha\u00b7rus", "tr\u00fc\u00b7be", "Leucht'", "ent\u00b7glimmt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Am schroffen Vorgebirg' im Abendduft,", "tokens": ["Am", "schrof\u00b7fen", "Vor\u00b7ge\u00b7bir\u00b7g'", "im", "A\u00b7bend\u00b7duft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Des Eilands weisse Klippenreih' verschwimmt,", "tokens": ["Des", "Ei\u00b7lands", "weis\u00b7se", "Klip\u00b7pen\u00b7reih'", "ver\u00b7schwimmt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Gleich einem Nebelstreif, in Wog' und Luft.", "tokens": ["Gleich", "ei\u00b7nem", "Ne\u00b7bel\u00b7streif", ",", "in", "Wo\u00b7g'", "und", "Luft", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.3": {"line.1": {"text": "Die Th\u00fcrme der ver\u00f6deten Abtei", "tokens": ["Die", "Th\u00fcr\u00b7me", "der", "ver\u00b7\u00f6\u00b7de\u00b7ten", "Ab\u00b7tei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Entragen schauervoll im bleichen Licht", "tokens": ["Ent\u00b7ra\u00b7gen", "schau\u00b7er\u00b7voll", "im", "blei\u00b7chen", "Licht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "APPRART", "ADJA", "NN"], "meter": "---+-+-+-+", "measure": "zehnsilber"}, "line.3": {"text": "Dem wildernden Gestr\u00e4uch der Felsenbai,", "tokens": ["Dem", "wil\u00b7dern\u00b7den", "Ge\u00b7str\u00e4uch", "der", "Fel\u00b7sen\u00b7bai", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wo dumpfig sich die matte Woge bricht.", "tokens": ["Wo", "dump\u00b7fig", "sich", "die", "mat\u00b7te", "Wo\u00b7ge", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PRF", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wo R\u00fcstern dort ein heilig Dunkel streun,", "tokens": ["Wo", "R\u00fcs\u00b7tern", "dort", "ein", "hei\u00b7lig", "Dun\u00b7kel", "streun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADV", "ART", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und um des Doms Portal sich Efeu dehnt,", "tokens": ["Und", "um", "des", "Doms", "Por\u00b7tal", "sich", "E\u00b7feu", "dehnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "PRF", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Weilt die Melancholei im Vollmondschein,", "tokens": ["Weilt", "die", "Me\u00b7lan\u00b7cho\u00b7lei", "im", "Voll\u00b7mond\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "An Grabmaltr\u00fcmmer sinnend hingelehnt.", "tokens": ["An", "Grab\u00b7malt\u00b7r\u00fcm\u00b7mer", "sin\u00b7nend", "hin\u00b7ge\u00b7lehnt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Durch Eiben blickt ein Beinhaus halb zerst\u00f6rt;", "tokens": ["Durch", "Ei\u00b7ben", "blickt", "ein", "Be\u00b7in\u00b7haus", "halb", "zer\u00b7st\u00f6rt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+----+-+", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Die Distel wankt am grauen Tempelthor,", "tokens": ["Die", "Dis\u00b7tel", "wankt", "am", "grau\u00b7en", "Tem\u00b7pel\u00b7thor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das l\u00e4ngst nicht mehr dem Flug der Eule wehrt;", "tokens": ["Das", "l\u00e4ngst", "nicht", "mehr", "dem", "Flug", "der", "Eu\u00b7le", "wehrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "PTKNEG", "ADV", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Im Bildwerk baut die Schwalb' am hohen Chor.", "tokens": ["Im", "Bild\u00b7werk", "baut", "die", "Schwalb'", "am", "ho\u00b7hen", "Chor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Kaum deuten in der B\u00f6gen D\u00fcsternheit", "tokens": ["Kaum", "deu\u00b7ten", "in", "der", "B\u00f6\u00b7gen", "D\u00fcs\u00b7tern\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Geschw\u00e4rzter Scheiben Reste, dort und hier", "tokens": ["Ge\u00b7schw\u00e4rz\u00b7ter", "Schei\u00b7ben", "Res\u00b7te", ",", "dort", "und", "hier"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "NN", "NN", "$,", "ADV", "KON", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Im Blei der Fenster sparsam noch verstreut,", "tokens": ["Im", "Blei", "der", "Fens\u00b7ter", "spar\u00b7sam", "noch", "ver\u00b7streut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "ADJD", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Glasgem\u00e4lde gothischfromme Zier.", "tokens": ["Der", "Glas\u00b7ge\u00b7m\u00e4l\u00b7de", "go\u00b7thischfrom\u00b7me", "Zier", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Der Hochaltar, von d\u00fcrrem Gras' umrauscht,", "tokens": ["Der", "Hoc\u00b7hal\u00b7tar", ",", "von", "d\u00fcr\u00b7rem", "Gras'", "um\u00b7rauscht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Stufen ausgerundet vom Gebet,", "tokens": ["Die", "Stu\u00b7fen", "aus\u00b7ge\u00b7run\u00b7det", "vom", "Ge\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPRART", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Zeugt noch wie oft, von Seraphim belauscht,", "tokens": ["Zeugt", "noch", "wie", "oft", ",", "von", "Se\u00b7ra\u00b7phim", "be\u00b7lauscht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "KOKOM", "ADV", "$,", "APPR", "NE", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Andacht Flammenseufzer hier geweht.", "tokens": ["Der", "An\u00b7dacht", "Flam\u00b7men\u00b7seuf\u00b7zer", "hier", "ge\u00b7weht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Nun fl\u00fcstern einsam nur die Wind' im Dom;", "tokens": ["Nun", "fl\u00fcs\u00b7tern", "ein\u00b7sam", "nur", "die", "Wind'", "im", "Dom", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "ADV", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Beichtstuhl trauert von der Spinn' umflort;", "tokens": ["Der", "Beicht\u00b7stuhl", "trau\u00b7ert", "von", "der", "Spinn'", "um\u00b7flort", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Orgel w\u00e4lzt nicht mehr der T\u00f6ne Strom", "tokens": ["Die", "Or\u00b7gel", "w\u00e4lzt", "nicht", "mehr", "der", "T\u00f6\u00b7ne", "Strom"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Durch die Gew\u00f6lbe majest\u00e4tisch fort.", "tokens": ["Durch", "die", "Ge\u00b7w\u00f6l\u00b7be", "ma\u00b7jes\u00b7t\u00e4\u00b7tisch", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.9": {"line.1": {"text": "Der Hymnen Feierjubel sind verhallt;", "tokens": ["Der", "Hym\u00b7nen", "Fei\u00b7er\u00b7ju\u00b7bel", "sind", "ver\u00b7hallt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Kein Marmorbild gl\u00e4nzt mehr vom Opferduft", "tokens": ["Kein", "Mar\u00b7mor\u00b7bild", "gl\u00e4nzt", "mehr", "vom", "Op\u00b7fer\u00b7duft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Weihrauchwolke festlich \u00fcberwallt,", "tokens": ["Der", "Weih\u00b7rauch\u00b7wol\u00b7ke", "fest\u00b7lich", "\u00fc\u00b7berw\u00b7allt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und jene Beter sanken in die Gruft.", "tokens": ["Und", "je\u00b7ne", "Be\u00b7ter", "san\u00b7ken", "in", "die", "Gruft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "In dieser Blende flimmte schwermutsvoll", "tokens": ["In", "die\u00b7ser", "Blen\u00b7de", "flimm\u00b7te", "schwer\u00b7muts\u00b7voll"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die heilge Lampe, wenn der Chorgesang", "tokens": ["Die", "heil\u00b7ge", "Lam\u00b7pe", ",", "wenn", "der", "Chor\u00b7ge\u00b7sang"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Jungfraun durch die Mitternacht erscholl,", "tokens": ["Der", "Jung\u00b7fraun", "durch", "die", "Mit\u00b7ter\u00b7nacht", "er\u00b7scholl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", 