{"textgrid.poem.33851": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Ein n\u00e4chtliches Echo", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Blitzend", "tokens": ["Blit\u00b7zend"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "Zieh'n die Sterne auf am Himmelsrand,", "tokens": ["Zieh'n", "die", "Ster\u00b7ne", "auf", "am", "Him\u00b7mels\u00b7rand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Spritzend", "tokens": ["Sprit\u00b7zend"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Senkt der Thau sich auf das durst'ge Land.", "tokens": ["Senkt", "der", "Thau", "sich", "auf", "das", "dur\u00b7st'\u00b7ge", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PRF", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.2": {"line.1": {"text": "\u00bbliebe!\u00ab", "tokens": ["\u00bb", "lie\u00b7be", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "$.", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "Singt der Knabe in die Nacht hinein.", "tokens": ["Singt", "der", "Kna\u00b7be", "in", "die", "Nacht", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "\u00bbliebe!\u00ab", "tokens": ["\u00bb", "lie\u00b7be", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "$.", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Klingt es wieder aus dem Myrthenhain.", "tokens": ["Klingt", "es", "wie\u00b7der", "aus", "dem", "Myr\u00b7then\u00b7hain", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "S\u00e4uselnd", "tokens": ["S\u00e4u\u00b7selnd"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "Schleicht der Wind durch die gew\u00fcrzte Luft,", "tokens": ["Schleicht", "der", "Wind", "durch", "die", "ge\u00b7w\u00fcrz\u00b7te", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Kr\u00e4uselnd", "tokens": ["Kr\u00e4u\u00b7selnd"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Jeden Bl\u00fctenzweig voll Hauch und Duft.", "tokens": ["Je\u00b7den", "Bl\u00fc\u00b7ten\u00b7zweig", "voll", "Hauch", "und", "Duft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJD", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "\u00bbo Traum!\u00ab", "tokens": ["\u00bb", "o", "Traum", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "punct"], "pos": ["$(", "FM", "NN", "$.", "$("], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Ruft der Knabe aus in s\u00fc\u00dfem Schmerz.", "tokens": ["Ruft", "der", "Kna\u00b7be", "aus", "in", "s\u00fc\u00b7\u00dfem", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "\u00bbo Traum!\u00ab", "tokens": ["\u00bb", "o", "Traum", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "punct"], "pos": ["$(", "FM", "NN", "$.", "$("], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Hallt's zur\u00fcck, als h\u00e4tt' die Nacht ein Herz.", "tokens": ["Hallt's", "zu\u00b7r\u00fcck", ",", "als", "h\u00e4tt'", "die", "Nacht", "ein", "Herz", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "KOKOM", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Knabe", "tokens": ["Kna\u00b7be"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "Glaubt entz\u00fcckt, was Seel' und Sinn ihm f\u00fcllt,", "tokens": ["Glaubt", "ent\u00b7z\u00fcckt", ",", "was", "Seel'", "und", "Sinn", "ihm", "f\u00fcllt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VVPP", "$,", "PWS", "NN", "KON", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Habe", "tokens": ["Ha\u00b7be"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Schmeichelnd sich in Luft und Duft geh\u00fcllt.", "tokens": ["Schmei\u00b7chelnd", "sich", "in", "Luft", "und", "Duft", "ge\u00b7h\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PRF", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "\u00bbkomm! Komm!\u00ab", "tokens": ["\u00bb", "komm", "!", "Komm", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "$.", "VVFIN", "$.", "$("], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Quillt es ihm aus hei\u00dfer Brust hervor.", "tokens": ["Quillt", "es", "ihm", "aus", "hei\u00b7\u00dfer", "Brust", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "\u00bbkomm! Komm!\u00ab", "tokens": ["\u00bb", "komm", "!", "Komm", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "$.", "VVFIN", "$.", "$("], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Spielt es lind und weich ihm um das Ohr.", "tokens": ["Spielt", "es", "lind", "und", "weich", "ihm", "um", "das", "Ohr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "KON", "ADJD", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Seine", "tokens": ["Sei\u00b7ne"], "token_info": ["word"], "pos": ["PPOSAT"], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "Seufzer giebt der Wald ihm treu zur\u00fcck,", "tokens": ["Seuf\u00b7zer", "giebt", "der", "Wald", "ihm", "treu", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Keine", "tokens": ["Kei\u00b7ne"], "token_info": ["word"], "pos": ["PIAT"], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Himmlische Gestalt erscheint dem Blick.", "tokens": ["Himm\u00b7li\u00b7sche", "Ge\u00b7stalt", "er\u00b7scheint", "dem", "Blick", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "\u00bbnur Schall!\u00ab", "tokens": ["\u00bb", "nur", "Schall", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "NN", "$.", "$("], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Ruft er endlich, und er ruft nicht mehr.", "tokens": ["Ruft", "er", "end\u00b7lich", ",", "und", "er", "ruft", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "KON", "PPER", "VVFIN", "PTKNEG", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "\u00bbnur Schall!\u00ab", "tokens": ["\u00bb", "nur", "Schall", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "NN", "$.", "$("], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Klingt es hinter dem Verstummten her.", "tokens": ["Klingt", "es", "hin\u00b7ter", "dem", "Ver\u00b7stumm\u00b7ten", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}