winen drein,", "tokens": ["Schmel\u00b7ze", "drei", "La\u00b7wi\u00b7nen", "drein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "CARD", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Hochmut, Selbstsucht, Geld!", "tokens": ["Hoch\u00b7mut", ",", "Selbst\u00b7sucht", ",", "Geld", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}