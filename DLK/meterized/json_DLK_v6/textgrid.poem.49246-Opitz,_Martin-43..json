{"textgrid.poem.49246": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "43.", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O lieber Mensch, dein Leben ja betrachte,", "tokens": ["O", "lie\u00b7ber", "Mensch", ",", "dein", "Le\u00b7ben", "ja", "be\u00b7trach\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "NN", "$,", "PPOSAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hier liegt ein Mann, der Blaseb\u00e4lge machte,", "tokens": ["Hier", "liegt", "ein", "Mann", ",", "der", "Bla\u00b7se\u00b7b\u00e4l\u00b7ge", "mach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Jetzt aber nun zu letzt es doch sich findt,", "tokens": ["Jetzt", "a\u00b7ber", "nun", "zu", "letzt", "es", "doch", "sich", "findt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "PTKZU", "VVFIN", "PPER", "ADV", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dem Meister dem gebricht noch selber Wind.", "tokens": ["Dem", "Meis\u00b7ter", "dem", "ge\u00b7bricht", "noch", "sel\u00b7ber", "Wind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "VVFIN", "ADV", "ADV", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}