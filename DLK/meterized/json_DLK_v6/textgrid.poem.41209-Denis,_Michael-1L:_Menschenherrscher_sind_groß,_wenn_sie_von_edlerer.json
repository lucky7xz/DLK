{"textgrid.poem.41209": {"metadata": {"author": {"name": "Denis, Michael", "birth": "N.A.", "death": "N.A."}, "title": "1L: Menschenherrscher sind gro\u00df, wenn sie von edlerer", "genre": "verse", "period": "N.A.", "pub_year": 1764, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Menschenherrscher sind gro\u00df, wenn sie von edlerer", "tokens": ["Men\u00b7schen\u00b7herr\u00b7scher", "sind", "gro\u00df", ",", "wenn", "sie", "von", "ed\u00b7le\u00b7rer"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ADJD", "$,", "KOUS", "PPER", "APPR", "ADJA"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Lust nach Ruhme gereizt fr\u00fche den hohen Geist,", "tokens": ["Lust", "nach", "Ruh\u00b7me", "ge\u00b7reizt", "fr\u00fc\u00b7he", "den", "ho\u00b7hen", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "ADJD", "ADJA", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Und den m\u00e4chtigen Arm ewigkeitw\u00fcrdigen,", "tokens": ["Und", "den", "m\u00e4ch\u00b7ti\u00b7gen", "Arm", "e\u00b7wig\u00b7keit\u00b7w\u00fcr\u00b7di\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "--+--++-+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Folgerweckenden Thaten weih'n", "tokens": ["Fol\u00b7ger\u00b7we\u00b7cken\u00b7den", "Tha\u00b7ten", "weih'n"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVINF"], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Lieders\u00f6hne sind gro\u00df, wenn der gewaltige", "tokens": ["Lie\u00b7der\u00b7s\u00f6h\u00b7ne", "sind", "gro\u00df", ",", "wenn", "der", "ge\u00b7wal\u00b7ti\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VAFIN", "ADJD", "$,", "KOUS", "ART", "ADJA"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Zeugni\u00dfgeber, ihr Mund, Th\u00e4ter erhabener", "tokens": ["Zeug\u00b7ni\u00df\u00b7ge\u00b7ber", ",", "ihr", "Mund", ",", "Th\u00e4\u00b7ter", "er\u00b7ha\u00b7be\u00b7ner"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,", "NN", "ADJA"], "meter": "+----++--+--", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Thaten, wenn er den Ruhm ewigkeitw\u00fcrdiger", "tokens": ["Tha\u00b7ten", ",", "wenn", "er", "den", "Ruhm", "e\u00b7wig\u00b7keit\u00b7w\u00fcr\u00b7di\u00b7ger"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "PPER", "ART", "NN", "ADJA"], "meter": "+-+--+--+---", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Menschenherrscher dem Tod entrei\u00dft.", "tokens": ["Men\u00b7schen\u00b7herr\u00b7scher", "dem", "Tod", "ent\u00b7rei\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.3": {"line.1": {"text": "Menschenherrscher wie viel, ewigkeitw\u00fcrdiger,", "tokens": ["Men\u00b7schen\u00b7herr\u00b7scher", "wie", "viel", ",", "e\u00b7wig\u00b7keit\u00b7w\u00fcr\u00b7di\u00b7ger", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "KOKOM", "ADV", "$,", "ADJA", "$,"], "meter": "+-+-+-+-+---", "measure": "unknown.measure.penta"}, "line.2": {"text": "Selt'ner Thaten wie viel raffte der Tod hinweg,", "tokens": ["Selt'\u00b7ner", "Tha\u00b7ten", "wie", "viel", "raff\u00b7te", "der", "Tod", "hin\u00b7weg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KOKOM", "ADV", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.3": {"text": "Weil kein Retter aus Nacht, weil kein gewaltiger", "tokens": ["Weil", "kein", "Ret\u00b7ter", "aus", "Nacht", ",", "weil", "kein", "ge\u00b7wal\u00b7ti\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PIAT", "NN", "APPR", "NN", "$,", "KOUS", "PIAT", "ADJA"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Mund der Lieder sie leben hie\u00df!", "tokens": ["Mund", "der", "Lie\u00b7der", "sie", "le\u00b7ben", "hie\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PPER", "VVINF", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.4": {"line.1": {"text": "Flammen sind sie vor uns deines bewunderten", "tokens": ["Flam\u00b7men", "sind", "sie", "vor", "uns", "dei\u00b7nes", "be\u00b7wun\u00b7der\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "APPR", "PPER", "PPOSAT", "ADJA"], "meter": "+-+-+-+--+--", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Friedrichs Thaten, o Freund! Aber die Flamme sinkt", "tokens": ["Fried\u00b7richs", "Tha\u00b7ten", ",", "o", "Freund", "!", "A\u00b7ber", "die", "Flam\u00b7me", "sinkt"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "FM", "NN", "$.", "KON", "ART", "NN", "VVFIN"], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Langsam nieder und stirbt, wenn sie kein Fl\u00fcgelschlag", "tokens": ["Lang\u00b7sam", "nie\u00b7der", "und", "stirbt", ",", "wenn", "sie", "kein", "Fl\u00fc\u00b7gel\u00b7schlag"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "PTKVZ", "KON", "VVFIN", "$,", "KOUS", "PPER", "PIAT", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Fernhin streifender Winde trifft.", "tokens": ["Fern\u00b7hin", "strei\u00b7fen\u00b7der", "Win\u00b7de", "trifft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.5": {"line.1": {"text": "Friedrichs Thaten, sie traf, Barde! der Fl\u00fcgelschlag", "tokens": ["Fried\u00b7richs", "Tha\u00b7ten", ",", "sie", "traf", ",", "Bar\u00b7de", "!", "der", "Fl\u00fc\u00b7gel\u00b7schlag"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["NE", "NN", "$,", "PPER", "VVFIN", "$,", "NN", "$.", "ART", "NN"], "meter": "+-+---+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Deines Liedes. Hinan bis in entfernete", "tokens": ["Dei\u00b7nes", "Lie\u00b7des", ".", "Hi\u00b7nan", "bis", "in", "ent\u00b7fer\u00b7ne\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "ADV", "ADV", "APPR", "NE"], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Menschenalter ergeu\u00dft, Wogen auf Wogen, sich", "tokens": ["Men\u00b7schen\u00b7al\u00b7ter", "er\u00b7geu\u00dft", ",", "Wo\u00b7gen", "auf", "Wo\u00b7gen", ",", "sich"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["NN", "VVFIN", "$,", "NN", "APPR", "NN", "$,", "PRF"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Unaufhaltsam ihr Feuerstrom.", "tokens": ["Un\u00b7auf\u00b7halt\u00b7sam", "ihr", "Feu\u00b7er\u00b7strom", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.6": {"line.1": {"text": "Hub dein Herrscher den Spie\u00df, hubst du dein Harfenspiel;", "tokens": ["Hub", "dein", "Herr\u00b7scher", "den", "Spie\u00df", ",", "hubst", "du", "dein", "Har\u00b7fen\u00b7spiel", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ART", "NN", "$,", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Schlug er Schlachten der Held, schlugst du die Saiten an;", "tokens": ["Schlug", "er", "Schlach\u00b7ten", "der", "Held", ",", "schlugst", "du", "die", "Sai\u00b7ten", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "ART", "NN", "$,", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Kehrt' er Sieger zur\u00fcck, kehrtest du Sieger auch,", "tokens": ["Kehrt'", "er", "Sie\u00b7ger", "zu\u00b7r\u00fcck", ",", "kehr\u00b7test", "du", "Sie\u00b7ger", "auch", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "PTKVZ", "$,", "VVFIN", "PPER", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Er vom Felde, vom Liede du.", "tokens": ["Er", "vom", "Fel\u00b7de", ",", "vom", "Lie\u00b7de", "du", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "$,", "APPRART", "NN", "PPER", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.7": {"line.1": {"text": "K\u00fchn ist, Barde! dein Lied, wie der versuchteste", "tokens": ["K\u00fchn", "ist", ",", "Bar\u00b7de", "!", "dein", "Lied", ",", "wie", "der", "ver\u00b7such\u00b7tes\u00b7te"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VAFIN", "$,", "NN", "$.", "PPOSAT", "NN", "$,", "PWAV", "ART", "ADJA"], "meter": "+-+-++-+-+-+", "measure": "unknown.measure.septa"}, "line.2": {"text": "Krieger, feurig, wie Blitz, hoch wie der Adlerweg,", "tokens": ["Krie\u00b7ger", ",", "feu\u00b7rig", ",", "wie", "Blitz", ",", "hoch", "wie", "der", "Ad\u00b7ler\u00b7weg", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$,", "PWAV", "NN", "$,", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Brausend, wie die Gewalt himmlischer Wasser aus", "tokens": ["Brau\u00b7send", ",", "wie", "die", "Ge\u00b7walt", "himm\u00b7li\u00b7scher", "Was\u00b7ser", "aus"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "PWAV", "ART", "NN", "ADJA", "NN", "APPR"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.4": {"text": "Wolkenbr\u00fcchen herabst\u00fcrzt.", "tokens": ["Wol\u00b7ken\u00b7br\u00fc\u00b7chen", "her\u00b7ab\u00b7st\u00fcrzt", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.8": {"line.1": {"text": "Von dem Ruhme gelockt, welcher in Harfen wohnt,", "tokens": ["Von", "dem", "Ruh\u00b7me", "ge\u00b7lockt", ",", "wel\u00b7cher", "in", "Har\u00b7fen", "wohnt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,", "PRELS", "APPR", "NN", "VVFIN", "$,"], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Lie\u00dfest deines Geschlechts laute Versammlung du,", "tokens": ["Lie\u00b7\u00dfest", "dei\u00b7nes", "Ge\u00b7schlechts", "lau\u00b7te", "Ver\u00b7samm\u00b7lung", "du", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "VVFIN", "NN", "PPER", "$,"], "meter": "+-+--+---+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Noch ein J\u00fcngling, und gingst einsam entz\u00fcckenden", "tokens": ["Noch", "ein", "J\u00fcng\u00b7ling", ",", "und", "gingst", "ein\u00b7sam", "ent\u00b7z\u00fc\u00b7cken\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "$,", "KON", "VVFIN", "ADJD", "VVINF"], "meter": "--+--++--+--", "measure": "anapaest.di.plus"}, "line.4": {"text": "Nachtigallen im Haine nach.", "tokens": ["Nach\u00b7ti\u00b7gal\u00b7len", "im", "Hai\u00b7ne", "nach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.9": {"line.1": {"text": "Jeden Lispel des Quell's, welcher im Hine flo\u00df,", "tokens": ["Je\u00b7den", "Lis\u00b7pel", "des", "Quell's", ",", "wel\u00b7cher", "im", "Hi\u00b7ne", "flo\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+--+---+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Jeden Seufzer des West's, oben im heiteren", "tokens": ["Je\u00b7den", "Seuf\u00b7zer", "des", "West's", ",", "o\u00b7ben", "im", "hei\u00b7te\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "NN", "ART", "NE", "$,", "ADV", "APPRART", "ADJA"], "meter": "+-+---+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Eichenwipfel, verstand, Barde! dein Herz, und schwoll", "tokens": ["Ei\u00b7chen\u00b7wip\u00b7fel", ",", "ver\u00b7stand", ",", "Bar\u00b7de", "!", "dein", "Herz", ",", "und", "schwoll"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "VVFIN", "$,", "NN", "$.", "PPOSAT", "NN", "$,", "KON", "ADJD"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.4": {"text": "Hoch von werdenden Liedern auf.", "tokens": ["Hoch", "von", "wer\u00b7den\u00b7den", "Lie\u00b7dern", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.10": {"line.1": {"text": "Und ein Silbergew\u00f6lk' senkte vom Monde sich", "tokens": ["Und", "ein", "Sil\u00b7ber\u00b7ge\u00b7w\u00f6l\u00b7k'", "senk\u00b7te", "vom", "Mon\u00b7de", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "APPRART", "NE", "PRF"], "meter": "--+--+-+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Tr\u00e4chtig nieder. Wer war's? Geister der \u00e4ltesten", "tokens": ["Tr\u00e4ch\u00b7tig", "nie\u00b7der", ".", "Wer", "wa\u00b7r's", "?", "Geis\u00b7ter", "der", "\u00e4l\u00b7tes\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "PTKVZ", "$.", "PWS", "VAFIN", "$.", "NN", "ART", "ADJA"], "meter": "+-+--+-+--+--", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Barden deines Geschlechts, Geister der \u00e4ltesten", "tokens": ["Bar\u00b7den", "dei\u00b7nes", "Ge\u00b7schlechts", ",", "Geis\u00b7ter", "der", "\u00e4l\u00b7tes\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "$,", "NN", "ART", "ADJA"], "meter": "+-+---+--+--", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Barden fremder Geschlechter auch.", "tokens": ["Bar\u00b7den", "frem\u00b7der", "Ge\u00b7schlech\u00b7ter", "auch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "ADV", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.11": {"line.1": {"text": "Freundlich lehrten sie dich ihre Geheimnisse,", "tokens": ["Freund\u00b7lich", "lehr\u00b7ten", "sie", "dich", "ih\u00b7re", "Ge\u00b7heim\u00b7nis\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Jeden k\u00fcnstlichen Griff, welcher in Griechenland,", "tokens": ["Je\u00b7den", "k\u00fcnst\u00b7li\u00b7chen", "Griff", ",", "wel\u00b7cher", "in", "Grie\u00b7chen\u00b7land", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,", "PRELS", "APPR", "NE", "$,"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.3": {"text": "Und im herrschenden Rom jedes empfindenden", "tokens": ["Und", "im", "herr\u00b7schen\u00b7den", "Rom", "je\u00b7des", "emp\u00b7fin\u00b7den\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NE", "PIAT", "ADJA"], "meter": "--+--+-+-+--", "measure": "anapaest.di.plus"}, "line.4": {"text": "H\u00f6rers Seele bezauberte.", "tokens": ["H\u00f6\u00b7rers", "See\u00b7le", "be\u00b7zau\u00b7ber\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "$."], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}}, "stanza.12": {"line.1": {"text": "Also kamst du zur\u00fcck aus der geweihten Nacht,", "tokens": ["Al\u00b7so", "kamst", "du", "zu\u00b7r\u00fcck", "aus", "der", "ge\u00b7weih\u00b7ten", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Trat'st in deines Geschlechts laute Versammlungen,", "tokens": ["Trat'st", "in", "dei\u00b7nes", "Ge\u00b7schlechts", "lau\u00b7te", "Ver\u00b7samm\u00b7lun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "VVFIN", "NN", "$,"], "meter": "+-+--+--+-+-", "mea