{"textgrid.poem.56421": {"metadata": {"author": {"name": "Heym, Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: La\u00df rei\u00dfen. Denn die Fl\u00fcgel fassen Bahn,", "genre": "verse", "period": "N.A.", "pub_year": 1899, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00df rei\u00dfen. Denn die Fl\u00fcgel fassen Bahn,", "tokens": ["La\u00df", "rei\u00b7\u00dfen", ".", "Denn", "die", "Fl\u00fc\u00b7gel", "fas\u00b7sen", "Bahn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVFIN", "$.", "KON", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie Adler klafternd \u00fcber dunklem Grunde.", "tokens": ["Wie", "Ad\u00b7ler", "klaf\u00b7ternd", "\u00fc\u00b7ber", "dunk\u00b7lem", "Grun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVPP", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "H\u00f6r. H\u00f6r. Sie stimmen an mit lautem Munde", "tokens": ["H\u00f6r", ".", "H\u00f6r", ".", "Sie", "stim\u00b7men", "an", "mit", "lau\u00b7tem", "Mun\u00b7de"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "NE", "$.", "PPER", "VVFIN", "APPR", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den Kriegsgesang, den hallenden P\u00e4an.", "tokens": ["Den", "Kriegs\u00b7ge\u00b7sang", ",", "den", "hal\u00b7len\u00b7den", "P\u00e4\u00b7an", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die G\u00f6tter steigen in das Schlachtget\u00fcmmel,", "tokens": ["Die", "G\u00f6t\u00b7ter", "stei\u00b7gen", "in", "das", "Schlacht\u00b7ge\u00b7t\u00fcm\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Aus Griechenreihn des Ph\u00f6bus Pfeile sausen.", "tokens": ["Aus", "Grie\u00b7chen\u00b7reihn", "des", "Ph\u00f6\u00b7bus", "Pfei\u00b7le", "sau\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und Ares' Stimme f\u00fcllt mit lautem Brausen", "tokens": ["Und", "A\u00b7res'", "Stim\u00b7me", "f\u00fcllt", "mit", "lau\u00b7tem", "Brau\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NE", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des Meeres Tiefen, Erd und weiten Himmel.", "tokens": ["Des", "Mee\u00b7res", "Tie\u00b7fen", ",", "Erd", "und", "wei\u00b7ten", "Him\u00b7mel", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wie eine L\u00f6wenm\u00e4hne ragt sein Haupt.", "tokens": ["Wie", "ei\u00b7ne", "L\u00f6\u00b7wen\u00b7m\u00e4h\u00b7ne", "ragt", "sein", "Haupt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Er schlachtet mit dem Schwerte in den Horden.", "tokens": ["Er", "schlach\u00b7tet", "mit", "dem", "Schwer\u00b7te", "in", "den", "Hor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da fliehn die ersten, ihres Muts beraubt.", "tokens": ["Da", "fliehn", "die", "ers\u00b7ten", ",", "ih\u00b7res", "Muts", "be\u00b7raubt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "$,", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Da st\u00fcrzen viele zu der Schiffe Borden.", "tokens": ["Da", "st\u00fcr\u00b7zen", "vie\u00b7le", "zu", "der", "Schif\u00b7fe", "Bor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch Ares m\u00e4ht noch, blutig und bestaubt,", "tokens": ["Doch", "A\u00b7res", "m\u00e4ht", "noch", ",", "blu\u00b7tig", "und", "be\u00b7staubt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ADV", "$,", "ADJD", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und f\u00fchrt die Griechen an zu wildem Morden.", "tokens": ["Und", "f\u00fchrt", "die", "Grie\u00b7chen", "an", "zu", "wil\u00b7dem", "Mor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}