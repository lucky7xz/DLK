{"textgrid.poem.55569": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Elysium", "genre": "verse", "period": "N.A.", "pub_year": 1772, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Uns gaben die G\u00f6tter", "tokens": ["Uns", "ga\u00b7ben", "die", "G\u00f6t\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Auf Erden Elysium.", "tokens": ["Auf", "Er\u00b7den", "E\u00b7ly\u00b7si\u00b7um", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "NE", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Wie du das erste Mal", "tokens": ["Wie", "du", "das", "ers\u00b7te", "Mal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Liebahndend dem Fremdling", "tokens": ["Lie\u00b7bahn\u00b7dend", "dem", "Fremd\u00b7ling"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ART", "NN"], "meter": "++--+-", "measure": "trochaic.tri.relaxed"}, "line.3": {"text": "Entgegentratst", "tokens": ["Ent\u00b7ge\u00b7gent\u00b7ratst"], "token_info": ["word"], "pos": ["NN"], "meter": "+---", "measure": "dactylic.init"}, "line.4": {"text": "Und deine Hand ihm reichtest,", "tokens": ["Und", "dei\u00b7ne", "Hand", "ihm", "reich\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "F\u00fchlt' er alles voraus,", "tokens": ["F\u00fchlt'", "er", "al\u00b7les", "vo\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIS", "PTKVZ", "$,"], "meter": "+-+---", "measure": "unknown.measure.di"}, "line.6": {"text": "Was ihm f\u00fcr Seligkeit", "tokens": ["Was", "ihm", "f\u00fcr", "Se\u00b7lig\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "PPER", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Entgegenkeimte.", "tokens": ["Ent\u00b7ge\u00b7gen\u00b7keim\u00b7te", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Uns gaben die G\u00f6tter", "tokens": ["Uns", "ga\u00b7ben", "die", "G\u00f6t\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Auf Erden Elysium.", "tokens": ["Auf", "Er\u00b7den", "E\u00b7ly\u00b7si\u00b7um", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "NE", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Wie du den liebenden Arm", "tokens": ["Wie", "du", "den", "lie\u00b7ben\u00b7den", "Arm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Um den Freund schlangst,", "tokens": ["Um", "den", "Freund", "schlangst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Wie ihm Lilas Brust", "tokens": ["Wie", "ihm", "Li\u00b7las", "Brust"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PPER", "NE", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Entgegenbebte,", "tokens": ["Ent\u00b7ge\u00b7gen\u00b7beb\u00b7te", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Wie ihr, euch rings umfassend,", "tokens": ["Wie", "ihr", ",", "euch", "rings", "um\u00b7fas\u00b7send", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "In heil'ger Wonne schwebtet", "tokens": ["In", "heil'\u00b7ger", "Won\u00b7ne", "schweb\u00b7tet"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und ich, im Anschaun selig,", "tokens": ["Und", "ich", ",", "im", "An\u00b7schaun", "se\u00b7lig", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ohne sterblichen Neid", "tokens": ["Oh\u00b7ne", "sterb\u00b7li\u00b7chen", "Neid"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.9": {"text": "Darneben stand.", "tokens": ["Dar\u00b7ne\u00b7ben", "stand", "."], "token_info": ["word", "word", "punct"], "pos": ["PAV", "VVFIN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Uns gaben die G\u00f6tter", "tokens": ["Uns", "ga\u00b7ben", "die", "G\u00f6t\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Auf Erden Elysium.", "tokens": ["Auf", "Er\u00b7den", "E\u00b7ly\u00b7si\u00b7um", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "NE", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Wie durch heilige T\u00e4ler wir", "tokens": ["Wie", "durch", "hei\u00b7li\u00b7ge", "T\u00e4\u00b7ler", "wir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN", "PPER"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "H\u00e4nd in H\u00e4nde wandelten", "tokens": ["H\u00e4nd", "in", "H\u00e4n\u00b7de", "wan\u00b7del\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "VVFIN"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "Und des Fremdlings Treu", "tokens": ["Und", "des", "Fremd\u00b7lings", "Treu"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Sich euch versiegelte,", "tokens": ["Sich", "euch", "ver\u00b7sie\u00b7gel\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "PPER", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Da\u00df du dem liebenden,", "tokens": ["Da\u00df", "du", "dem", "lie\u00b7ben\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "$,"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.6": {"text": "Stille sehnenden", "tokens": ["Stil\u00b7le", "seh\u00b7nen\u00b7den"], "token_info": ["word", "word"], "pos": ["NN", "VVINF"], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.7": {"text": "Die Wange reichtest", "tokens": ["Die", "Wan\u00b7ge", "reich\u00b7test"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Zum himmlischen Ku\u00df.", "tokens": ["Zum", "himm\u00b7li\u00b7schen", "Ku\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.7": {"line.1": {"text": "Uns gaben die G\u00f6tter", "tokens": ["Uns", "ga\u00b7ben", "die", "G\u00f6t\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Auf Erden Elysium.", "tokens": ["Auf", "Er\u00b7den", "E\u00b7ly\u00b7si\u00b7um", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "NE", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Wenn du fern wandelst", "tokens": ["Wenn", "du", "fern", "wan\u00b7delst"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Am H\u00fcgelgeb\u00fcsch,", "tokens": ["Am", "H\u00fc\u00b7gel\u00b7ge\u00b7b\u00fcsch", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Wandeln Liebesgestalten", "tokens": ["Wan\u00b7deln", "Lie\u00b7bes\u00b7ge\u00b7stal\u00b7ten"], "token_info": ["word", "word"], "pos": ["NN", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Mit dir den Bach hinab;", "tokens": ["Mit", "dir", "den", "Bach", "hin\u00b7ab", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wenn mir auf dem Felsen", "tokens": ["Wenn", "mir", "auf", "dem", "Fel\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Die Sonne niedergeht,", "tokens": ["Die", "Son\u00b7ne", "nie\u00b7der\u00b7geht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Seh ich Freundegestalten", "tokens": ["Seh", "ich", "Freun\u00b7de\u00b7ge\u00b7stal\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.8": {"text": "Mir winken durch", "tokens": ["Mir", "win\u00b7ken", "durch"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Wehende Zweige", "tokens": ["We\u00b7hen\u00b7de", "Zwei\u00b7ge"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Des d\u00e4mmernden Hains.", "tokens": ["Des", "d\u00e4m\u00b7mern\u00b7den", "Hains", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.9": {"line.1": {"text": "Uns gaben die G\u00f6tter", "tokens": ["Uns", "ga\u00b7ben", "die", "G\u00f6t\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Auf Erden Elysium.", "tokens": ["Auf", "Er\u00b7den", "E\u00b7ly\u00b7si\u00b7um", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "NE", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.10": {"line.1": {"text": "Seh ich, verschlagen", "tokens": ["Seh", "ich", ",", "ver\u00b7schla\u00b7gen"], "token_info": ["word", "word", "punct", "word"], "pos": ["VVFIN", "PPER", "$,", "VVFIN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Unter schauernden Himmels", "tokens": ["Un\u00b7ter", "schau\u00b7ern\u00b7den", "Him\u00b7mels"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "\u00d6de Gestade,", "tokens": ["\u00d6\u00b7de", "Ge\u00b7sta\u00b7de", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "In der Vergangenheit", "tokens": ["In", "der", "Ver\u00b7gan\u00b7gen\u00b7heit"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Goldener Myrtenhainsd\u00e4mmerung", "tokens": ["Gol\u00b7de\u00b7ner", "Myr\u00b7ten\u00b7hains\u00b7d\u00e4m\u00b7me\u00b7rung"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+--+--", "measure": "dactylic.tri"}, "line.6": {"text": "Lilan an deiner Hand,", "tokens": ["Li\u00b7lan", "an", "dei\u00b7ner", "Hand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Seh mich Sch\u00fcchternen", "tokens": ["Seh", "mich", "Sch\u00fcch\u00b7ter\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "NN"], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.8": {"text": "Eure H\u00e4nde fassen,", "tokens": ["Eu\u00b7re", "H\u00e4n\u00b7de", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "Bittend blicken,", "tokens": ["Bit\u00b7tend", "bli\u00b7cken", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.10": {"text": "Eure H\u00e4nde k\u00fcssen \u2013", "tokens": ["Eu\u00b7re", "H\u00e4n\u00b7de", "k\u00fcs\u00b7sen", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.11": {"text": "Eure Augen sich begegnen,", "tokens": ["Eu\u00b7re", "Au\u00b7gen", "sich", "be\u00b7geg\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Auf mich blicken seh ich,", "tokens": ["Auf", "mich", "bli\u00b7cken", "seh", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVINF", "VVFIN", "PPER", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.13": {"text": "Werfe den hoffenden Blick", "tokens": ["Wer\u00b7fe", "den", "hof\u00b7fen\u00b7den", "Blick"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.14": {"text": "Auf Lila; sie n\u00e4hert sich mir,", "tokens": ["Auf", "Li\u00b7la", ";", "sie", "n\u00e4\u00b7hert", "sich", "mir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$.", "PPER", "VVFIN", "PRF", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Himmlische Lippe!", "tokens": ["Himm\u00b7li\u00b7sche", "Lip\u00b7pe", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.16": {"text": "Und ich wanke, nahe mich,", "tokens": ["Und", "ich", "wan\u00b7ke", ",", "na\u00b7he", "mich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Blicke, seufze, wanke \u2013", "tokens": ["Bli\u00b7cke", ",", "seuf\u00b7ze", ",", "wan\u00b7ke", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "VVFIN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.18": {"text": "Seligkeit! Seligkeit!", "tokens": ["Se\u00b7lig\u00b7keit", "!", "Se\u00b7lig\u00b7keit", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.19": {"text": "Eines Kusses Gef\u00fchl!", "tokens": ["Ei\u00b7nes", "Kus\u00b7ses", "Ge\u00b7f\u00fchl", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.11": {"line.1": {"text": "Mir gaben die G\u00f6tter", "tokens": ["Mir", "ga\u00b7ben", "die", "G\u00f6t\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Auf Erden Elysium!", "tokens": ["Auf", "Er\u00b7den", "E\u00b7ly\u00b7si\u00b7um", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "NE", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.12": {"line.1": {"text": "Ach, warum nur Elysium!", "tokens": ["Ach", ",", "wa\u00b7rum", "nur", "E\u00b7ly\u00b7si\u00b7um", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}