{"textgrid.poem.59755": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hinein mit vollen Segeln in den Sturm!", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hinein mit vollen Segeln in den Sturm!", "tokens": ["Hin\u00b7ein", "mit", "vol\u00b7len", "Se\u00b7geln", "in", "den", "Sturm", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Es denkt an m\u00f6gliche Kometenschwenkung,", "tokens": ["Es", "denkt", "an", "m\u00f6g\u00b7li\u00b7che", "Ko\u00b7me\u00b7ten\u00b7schwen\u00b7kung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Land und Meer wegfegt und Sonnenschein,", "tokens": ["Die", "Land", "und", "Meer", "weg\u00b7fegt", "und", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVPP", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Auf seinem gr\u00fcnen Grashalm kaum der Wurm \u2013", "tokens": ["Auf", "sei\u00b7nem", "gr\u00fc\u00b7nen", "Gras\u00b7halm", "kaum", "der", "Wurm", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Hinein denn, K\u00e4mpe Mensch, da voll hinein!", "tokens": ["Hin\u00b7ein", "denn", ",", "K\u00e4m\u00b7pe", "Mensch", ",", "da", "voll", "hin\u00b7ein", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "ADJA", "NN", "$,", "KOUS", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Dann steige wieder bei dir selber ein,", "tokens": ["Dann", "stei\u00b7ge", "wie\u00b7der", "bei", "dir", "sel\u00b7ber", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Hinein in deine tiefste Selbstversenkung,", "tokens": ["Hin\u00b7ein", "in", "dei\u00b7ne", "tiefs\u00b7te", "Selbst\u00b7ver\u00b7sen\u00b7kung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "In Platons Abgrund mutig dann hinein!", "tokens": ["In", "Pla\u00b7tons", "Ab\u00b7grund", "mu\u00b7tig", "dann", "hin\u00b7ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "ADJD", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Da schaust du in dem wilden Weltengraus", "tokens": ["Da", "schaust", "du", "in", "dem", "wil\u00b7den", "Wel\u00b7ten\u00b7graus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Mit G\u00f6tterblicken aus dir selbst heraus,", "tokens": ["Mit", "G\u00f6t\u00b7ter\u00b7bli\u00b7cken", "aus", "dir", "selbst", "he\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Da schaust du rechte Lenkung, rechte Schwenkung,", "tokens": ["Da", "schaust", "du", "rech\u00b7te", "Len\u00b7kung", ",", "rech\u00b7te", "Schwen\u00b7kung", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Da klingt aus stillster, innerster Bedenkung", "tokens": ["Da", "klingt", "aus", "stills\u00b7ter", ",", "in\u00b7ners\u00b7ter", "Be\u00b7den\u00b7kung"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Dein gro\u00dfes Ja, vernichtend jedes Nein:", "tokens": ["Dein", "gro\u00b7\u00dfes", "Ja", ",", "ver\u00b7nich\u00b7tend", "je\u00b7des", "Nein", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "PTKANT", "$,", "VVPP", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}