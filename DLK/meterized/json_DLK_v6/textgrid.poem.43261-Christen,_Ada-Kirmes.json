{"textgrid.poem.43261": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "Kirmes", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Brummba\u00df murrt und die Geige schreit,", "tokens": ["Der", "Brumm\u00b7ba\u00df", "murrt", "und", "die", "Gei\u00b7ge", "schreit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und die Trompete spektakelt!", "tokens": ["Und", "die", "Trom\u00b7pe\u00b7te", "spek\u00b7ta\u00b7kelt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$."], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Juchheisa! lustige Kirmeszeit! ...", "tokens": ["Juch\u00b7hei\u00b7sa", "!", "lus\u00b7ti\u00b7ge", "Kir\u00b7mes\u00b7zeit", "!", "..."], "token_info": ["word", "punct", "word", "word", "punct", "punct"], "pos": ["NE", "$.", "ADJA", "NN", "$.", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Da kommt der Pfarrer gewackelt.", "tokens": ["Da", "kommt", "der", "Pfar\u00b7rer", "ge\u00b7wa\u00b7ckelt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Juchheisa! seiner Dirn' einen Ku\u00df", "tokens": ["Juch\u00b7hei\u00b7sa", "!", "sei\u00b7ner", "Dirn'", "ei\u00b7nen", "Ku\u00df"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$.", "PPOSAT", "NN", "ART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Der Bursche giebt m\u00fchvergessen,", "tokens": ["Der", "Bur\u00b7sche", "giebt", "m\u00fch\u00b7ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sie tanzen! ... \u00bbWie ist der Lebensgenu\u00df", "tokens": ["Sie", "tan\u00b7zen", "!", "...", "\u00bb", "Wie", "ist", "der", "Le\u00b7bens\u00b7ge\u00b7nu\u00df"], "token_info": ["word", "word", "punct", "punct", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVINF", "$.", "$(", "$(", "PWAV", "VAFIN", "ART", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Dem Volk mit Scheffeln gemessen!\u00ab", "tokens": ["Dem", "Volk", "mit", "Schef\u00b7feln", "ge\u00b7mes\u00b7sen", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVPP", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "So brummt der Pfarrer und blinzelt hin", "tokens": ["So", "brummt", "der", "Pfar\u00b7rer", "und", "blin\u00b7zelt", "hin"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "KON", "VVFIN", "PTKVZ"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und grollt der Lust, der schlichten,", "tokens": ["Und", "grollt", "der", "Lust", ",", "der", "schlich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und qu\u00e4lt sich ab mit l\u00fcsternem Sinn,", "tokens": ["Und", "qu\u00e4lt", "sich", "ab", "mit", "l\u00fcs\u00b7ter\u00b7nem", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "PTKVZ", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Die S\u00fcnde hineinzudichten.", "tokens": ["Die", "S\u00fcn\u00b7de", "hin\u00b7ein\u00b7zu\u00b7dich\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}