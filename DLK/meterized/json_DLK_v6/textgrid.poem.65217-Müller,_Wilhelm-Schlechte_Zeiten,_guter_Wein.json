{"textgrid.poem.65217": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Schlechte Zeiten, guter Wein", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00dcber schlechte Zeiten", "tokens": ["\u00dc\u00b7ber", "schlech\u00b7te", "Zei\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Klag' ich nimmermehr,", "tokens": ["Klag'", "ich", "nim\u00b7mer\u00b7mehr", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Wird von gutem Weine", "tokens": ["Wird", "von", "gu\u00b7tem", "Wei\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Nur mein Fa\u00df nicht leer.", "tokens": ["Nur", "mein", "Fa\u00df", "nicht", "leer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "PTKNEG", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Willst die Zeitung lesen?", "tokens": ["Willst", "die", "Zei\u00b7tung", "le\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Bruder, geh' zu Bier!", "tokens": ["Bru\u00b7der", ",", "geh'", "zu", "Bier", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Zu dem Saft der Reben", "tokens": ["Zu", "dem", "Saft", "der", "Re\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Schmeckt kein L\u00f6schpapier.", "tokens": ["Schmeckt", "kein", "L\u00f6schpa\u00b7pier", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Ob auf dieser Erden", "tokens": ["Ob", "auf", "die\u00b7ser", "Er\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PDAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Auch von Tag zu Tag", "tokens": ["Auch", "von", "Tag", "zu", "Tag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "APPR", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Matter, k\u00e4lter, schw\u00e4cher", "tokens": ["Mat\u00b7ter", ",", "k\u00e4l\u00b7ter", ",", "schw\u00e4\u00b7cher"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["NE", "$,", "ADJD", "$,", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Alles werden mag:", "tokens": ["Al\u00b7les", "wer\u00b7den", "mag", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "VAINF", "VMFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Doch der Wein im Fasse", "tokens": ["Doch", "der", "Wein", "im", "Fas\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Trotzt der Macht der Zeit,", "tokens": ["Trotzt", "der", "Macht", "der", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "F\u00fchlet nichts vom Alter", "tokens": ["F\u00fch\u00b7let", "nichts", "vom", "Al\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "APPRART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Als die W\u00fcrdigkeit.", "tokens": ["Als", "die", "W\u00fcr\u00b7dig\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Was das Jahr dem Menschen", "tokens": ["Was", "das", "Jahr", "dem", "Men\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "ART", "NN", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Allgemach entrafft,", "tokens": ["All\u00b7ge\u00b7mach", "en\u00b7trafft", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Das, das giebt's dem Weine:", "tokens": ["Das", ",", "das", "giebt's", "dem", "Wei\u00b7ne", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PDS", "VVFIN", "ART", "NN", "$."], "meter": "--+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Gluth und Muth und Kraft.", "tokens": ["Gluth", "und", "Muth", "und", "Kraft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Wollen's wieder holen", "tokens": ["Wol\u00b7len's", "wie\u00b7der", "ho\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["NE", "ADV", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Aus dem Fa\u00df hervor,", "tokens": ["Aus", "dem", "Fa\u00df", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Was im Flug der Jahre", "tokens": ["Was", "im", "Flug", "der", "Jah\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Jeglicher verlor!", "tokens": ["Jeg\u00b7li\u00b7cher", "ver\u00b7lor", "!"], "token_info": ["word", "word", "punct"], "pos": ["PIS", "VVFIN", "$."], "meter": "+---+", "measure": "dactylic.init"}}, "stanza.7": {"line.1": {"text": "Und wer mit dem Leben", "tokens": ["Und", "wer", "mit", "dem", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "APPR", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Lebt in Leid und Streit,", "tokens": ["Lebt", "in", "Leid", "und", "Streit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Trink' aus altem Fasse", "tokens": ["Trink'", "aus", "al\u00b7tem", "Fas\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Alte gute Zeit!", "tokens": ["Al\u00b7te", "gu\u00b7te", "Zeit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}