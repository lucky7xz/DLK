{"textgrid.poem.46845": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[o ihr zu fr\u00fch entzognen]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O ihr zu fr\u00fch entzognen", "tokens": ["O", "ihr", "zu", "fr\u00fch", "ent\u00b7zo\u00b7gnen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "PPOSAT", "PTKA", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Vaterzucht,", "tokens": ["Der", "Va\u00b7ter\u00b7zucht", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Zum Himmel auf entflognen", "tokens": ["Zum", "Him\u00b7mel", "auf", "ent\u00b7flog\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In schneller Flucht!", "tokens": ["In", "schnel\u00b7ler", "Flucht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Ich wollte hier euch leiten", "tokens": ["Ich", "woll\u00b7te", "hier", "euch", "lei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Des Lebens Bahn,", "tokens": ["Des", "Le\u00b7bens", "Bahn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Nun wollet ihr mir schreiten", "tokens": ["Nun", "wol\u00b7let", "ihr", "mir", "schrei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zum Licht voran.", "tokens": ["Zum", "Licht", "vo\u00b7ran", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Und wir sind nicht geschieden", "tokens": ["Und", "wir", "sind", "nicht", "ge\u00b7schie\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "PTKNEG", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Durch Zeit und Ort,", "tokens": ["Durch", "Zeit", "und", "Ort", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Ihr lebt bei mir hienieden,", "tokens": ["Ihr", "lebt", "bei", "mir", "hien\u00b7ie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich bei euch dort.", "tokens": ["Ich", "bei", "euch", "dort", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPER", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}