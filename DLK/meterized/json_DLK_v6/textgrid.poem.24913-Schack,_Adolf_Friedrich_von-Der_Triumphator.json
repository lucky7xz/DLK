{"textgrid.poem.24913": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Der Triumphator", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Stolz im Triumph glorreicher Siege,", "tokens": ["Stolz", "im", "Tri\u00b7umph", "glor\u00b7rei\u00b7cher", "Sie\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ADJA", "NN", "$,"], "meter": "+----+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Wie keiner sie erk\u00e4mpft zuvor,", "tokens": ["Wie", "kei\u00b7ner", "sie", "er\u00b7k\u00e4mpft", "zu\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zieht auf der leuchtenden Quadrige", "tokens": ["Zieht", "auf", "der", "leuch\u00b7ten\u00b7den", "Quad\u00b7ri\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Aemilius Paulus durch das Thor;", "tokens": ["A\u00b7e\u00b7mi\u00b7lius", "Pau\u00b7lus", "durch", "das", "Thor", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Es wirbelt Duft aus goldnen Becken;", "tokens": ["Es", "wir\u00b7belt", "Duft", "aus", "gold\u00b7nen", "Be\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Roms Tempel sind mit Purpurdecken,", "tokens": ["Roms", "Tem\u00b7pel", "sind", "mit", "Pur\u00b7pur\u00b7de\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VAFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "So sch\u00f6n sie Tyrus beut, beh\u00e4ngt,", "tokens": ["So", "sch\u00f6n", "sie", "Ty\u00b7rus", "beut", ",", "be\u00b7h\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "NE", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und rauschend t\u00f6nt's, wie Meeresbranden,", "tokens": ["Und", "rau\u00b7schend", "t\u00f6nt's", ",", "wie", "Mee\u00b7res\u00b7bran\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "$,", "PWAV", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Wo sich das Volk in Festgewanden,", "tokens": ["Wo", "sich", "das", "Volk", "in", "Fest\u00b7ge\u00b7wan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Des Feierzuges harrend, dr\u00e4ngt.", "tokens": ["Des", "Fei\u00b7er\u00b7zu\u00b7ges", "har\u00b7rend", ",", "dr\u00e4ngt", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Auf Helmen, Schilden, Wurfgeschossen,", "tokens": ["Auf", "Hel\u00b7men", ",", "Schil\u00b7den", ",", "Wurf\u00b7ge\u00b7schos\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auf R\u00fcstungen von blankem Stahl,", "tokens": ["Auf", "R\u00fcs\u00b7tun\u00b7gen", "von", "blan\u00b7kem", "Stahl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf Marmorbildern, Erzkolossen", "tokens": ["Auf", "Mar\u00b7mor\u00b7bil\u00b7dern", ",", "Erz\u00b7ko\u00b7los\u00b7sen"], "token_info": ["word", "word", "punct", "word"], "pos": ["APPR", "NN", "$,", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Spielt wie verirrt der Sonnenstrahl;", "tokens": ["Spielt", "wie", "ver\u00b7irrt", "der", "Son\u00b7nen\u00b7strahl", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "VVPP", "ART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "J\u00fcnglinge nerv'gen Armes f\u00fchren", "tokens": ["J\u00fcng\u00b7lin\u00b7ge", "ner\u00b7v'\u00b7gen", "Ar\u00b7mes", "f\u00fch\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADJA", "NN", "VVINF"], "meter": "++--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Von des Clitumnus wei\u00dfen Stieren", "tokens": ["Von", "des", "Cli\u00b7tum\u00b7nus", "wei\u00b7\u00dfen", "Stie\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die sch\u00f6nsten hundert, kranzgeschm\u00fcckt;", "tokens": ["Die", "sch\u00f6ns\u00b7ten", "hun\u00b7dert", ",", "kranz\u00b7ge\u00b7schm\u00fcckt", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "CARD", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "In Reihen dann, ein Spott der Sieger,", "tokens": ["In", "Rei\u00b7hen", "dann", ",", "ein", "Spott", "der", "Sie\u00b7ger", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Nahn Macedoniens blasse Krieger,", "tokens": ["Nahn", "Ma\u00b7ce\u00b7do\u00b7ni\u00b7ens", "blas\u00b7se", "Krie\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Von eh'rner Ketten Wucht gedr\u00fcckt.", "tokens": ["Von", "eh'r\u00b7ner", "Ket\u00b7ten", "Wucht", "ge\u00b7dr\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Drauf er, dem bis zu Asiens Landen", "tokens": ["Drauf", "er", ",", "dem", "bis", "zu", "A\u00b7siens", "Lan\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PAV", "PPER", "$,", "PRELS", "ADV", "APPR", "NE", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sich gestern noch gedehnt das Reich,", "tokens": ["Sich", "ge\u00b7stern", "noch", "ge\u00b7dehnt", "das", "Reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "ADV", "VVPP", "ART", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Der K\u00f6nig selbst in Eisenbanden,", "tokens": ["Der", "K\u00f6\u00b7nig", "selbst", "in", "Ei\u00b7sen\u00b7ban\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dem niedersten der Sklaven gleich;", "tokens": ["Dem", "nie\u00b7ders\u00b7ten", "der", "Skla\u00b7ven", "gleich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.5": {"text": "An seiner Seite flehn zwei S\u00f6hne,", "tokens": ["An", "sei\u00b7ner", "Sei\u00b7te", "flehn", "zwei", "S\u00f6h\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "CARD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Fast Kinder noch, von holder Sch\u00f6ne,", "tokens": ["Fast", "Kin\u00b7der", "noch", ",", "von", "hol\u00b7der", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ADV", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Der stolzen R\u00f6mer Mitleid an;", "tokens": ["Der", "stol\u00b7zen", "R\u00f6\u00b7mer", "Mit\u00b7leid", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Dann siehe! durch die Ehrenbogen", "tokens": ["Dann", "sie\u00b7he", "!", "durch", "die", "Eh\u00b7ren\u00b7bo\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVIMP", "$.", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Der Legionen trunknes Wogen,", "tokens": ["Der", "Le\u00b7gi\u00b7o\u00b7nen", "trunk\u00b7nes", "Wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Des Siegers wei\u00dfes Ro\u00dfgespann!", "tokens": ["Des", "Sie\u00b7gers", "wei\u00b7\u00dfes", "Ro\u00df\u00b7ge\u00b7spann", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Beim Jauchzen der Triumphges\u00e4nge,", "tokens": ["Beim", "Jauch\u00b7zen", "der", "Tri\u00b7umph\u00b7ge\u00b7s\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das tausendstimmig rings erschallt,", "tokens": ["Das", "tau\u00b7sends\u00b7tim\u00b7mig", "rings", "er\u00b7schallt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Rollt die Quadriga durch die Menge", "tokens": ["Rollt", "die", "Quad\u00b7ri\u00b7ga", "durch", "die", "Men\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Und macht am Kapitole Halt.", "tokens": ["Und", "macht", "am", "Ka\u00b7pi\u00b7to\u00b7le", "Halt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Aemilius steigt durchs Jubelrufen", "tokens": ["A\u00b7e\u00b7mi\u00b7lius", "steigt", "durchs", "Ju\u00b7bel\u00b7ru\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Des Volkes die phorphyrnen Stufen", "tokens": ["Des", "Vol\u00b7kes", "die", "phor\u00b7phyr\u00b7nen", "Stu\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Zum Haus des Donnerers hinauf;", "tokens": ["Zum", "Haus", "des", "Don\u00b7ne\u00b7rers", "hin\u00b7auf", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Da, durch die Menschenwoge dringend,", "tokens": ["Da", ",", "durch", "die", "Men\u00b7schen\u00b7wo\u00b7ge", "drin\u00b7gend", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "St\u00fcrzt, bleich von Antlitz, h\u00e4nderingend,", "tokens": ["St\u00fcrzt", ",", "bleich", "von", "Ant\u00b7litz", ",", "h\u00e4n\u00b7de\u00b7rin\u00b7gend", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJD", "APPR", "NE", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Ein Sklav' ihm nach in hast'gem Lauf.", "tokens": ["Ein", "Skla\u00b7v'", "ihm", "nach", "in", "hast'\u00b7gem", "Lauf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPR", "APPR", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "\u00bbo Herr, vernimm die Trauerkunde!", "tokens": ["\u00bb", "o", "Herr", ",", "ver\u00b7nimm", "die", "Trau\u00b7er\u00b7kun\u00b7de", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "FM", "NN", "$,", "VVIMP", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was dir des Lebens Liebstes war,", "tokens": ["Was", "dir", "des", "Le\u00b7bens", "Liebs\u00b7tes", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "NN", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ward dir geraubt in ", "tokens": ["Ward", "dir", "ge\u00b7raubt", "in"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "VVPP", "APPR"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Der Zwillingss\u00f6hne bl\u00fchend Paar!", "tokens": ["Der", "Zwil\u00b7lings\u00b7s\u00f6h\u00b7ne", "bl\u00fc\u00b7hend", "Paar", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein Blitzstrahl hat die zwei erschlagen,", "tokens": ["Ein", "Blitz\u00b7strahl", "hat", "die", "zwei", "er\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "CARD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Als mittags sie entschlummert lagen", "tokens": ["Als", "mit\u00b7tags", "sie", "ent\u00b7schlum\u00b7mert", "la\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PPER", "VVPP", "VVFIN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.7": {"text": "Im Oelwald der Akademie;", "tokens": ["Im", "O\u00b7el\u00b7wald", "der", "A\u00b7ka\u00b7de\u00b7mie", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Her von Athen, damit die Laren", "tokens": ["Her", "von", "A\u00b7then", ",", "da\u00b7mit", "die", "La\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "APPR", "NE", "$,", "KOUS", "ART", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.9": {"text": "Der Heimat ihren Staub bewahren,", "tokens": ["Der", "Hei\u00b7mat", "ih\u00b7ren", "Staub", "be\u00b7wah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Im Sarkophage bring' ich sie.\u00ab", "tokens": ["Im", "Sar\u00b7ko\u00b7pha\u00b7ge", "bring'", "ich", "sie", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PPER", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Die rings die Botschaft h\u00f6ren, schauen", "tokens": ["Die", "rings", "die", "Bot\u00b7schaft", "h\u00f6\u00b7ren", ",", "schau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "ADV", "ART", "NN", "VVINF", "$,", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Voll Mitleid auf Aemilius:", "tokens": ["Voll", "Mit\u00b7leid", "auf", "A\u00b7e\u00b7mi\u00b7lius", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "APPR", "NE", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "\u00bbweh, da\u00df in Gram und Todesgrauen", "tokens": ["\u00bb", "weh", ",", "da\u00df", "in", "Gram", "und", "To\u00b7des\u00b7grau\u00b7en"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PTKVZ", "$,", "KOUS", "APPR", "NE", "KON", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Ihm der Triumphtag enden mu\u00df!\u00ab", "tokens": ["Ihm", "der", "Tri\u00b7umph\u00b7tag", "en\u00b7den", "mu\u00df", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "ART", "NN", "VVINF", "VMFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch er tritt, kaum entf\u00e4rbt die Wange,", "tokens": ["Doch", "er", "tritt", ",", "kaum", "ent\u00b7f\u00e4rbt", "die", "Wan\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}