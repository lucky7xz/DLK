{"textgrid.poem.38577": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Die Sommernacht", "genre": "verse", "period": "N.A.", "pub_year": 1784, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wann mir dein Fittig, duftende Sommernacht,", "tokens": ["Wann", "mir", "dein", "Fit\u00b7tig", ",", "duf\u00b7ten\u00b7de", "Som\u00b7mer\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Im Thal des mondbeschimmerten Silberquells,", "tokens": ["Im", "Thal", "des", "mond\u00b7be\u00b7schim\u00b7mer\u00b7ten", "Sil\u00b7ber\u00b7quells", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Auf hainumkr\u00e4nzten Blumenh\u00fcgeln", "tokens": ["Auf", "hai\u00b7num\u00b7kr\u00e4nz\u00b7ten", "Blu\u00b7men\u00b7h\u00fc\u00b7geln"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Oder im Schatten der Erle, s\u00e4uselt:", "tokens": ["O\u00b7der", "im", "Schat\u00b7ten", "der", "Er\u00b7le", ",", "s\u00e4u\u00b7selt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ART", "NN", "$,", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Dann bebt Entz\u00fcckungsschauer durch mein Gebein!", "tokens": ["Dann", "bebt", "Ent\u00b7z\u00fc\u00b7ckungs\u00b7schau\u00b7er", "durch", "mein", "Ge\u00b7bein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Dann tagt der Schwermuth t\u00e4uschende D\u00e4mmerung,", "tokens": ["Dann", "tagt", "der", "Schwer\u00b7muth", "t\u00e4u\u00b7schen\u00b7de", "D\u00e4m\u00b7me\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und mild, wie Abendsonnenblicke,", "tokens": ["Und", "mild", ",", "wie", "A\u00b7bend\u00b7son\u00b7nen\u00b7bli\u00b7cke", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "PWAV", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "L\u00e4chelt der Hofnung entw\u00f6lktes Auge!", "tokens": ["L\u00e4\u00b7chelt", "der", "Hof\u00b7nung", "ent\u00b7w\u00f6lk\u00b7tes", "Au\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}}}}