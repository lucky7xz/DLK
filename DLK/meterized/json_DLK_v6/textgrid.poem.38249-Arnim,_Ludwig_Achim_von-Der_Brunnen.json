{"textgrid.poem.38249": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Der Brunnen", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hab ein Br\u00fcnnlein mal gesehen,", "tokens": ["Hab", "ein", "Br\u00fcnn\u00b7lein", "mal", "ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Draus th\u00e4t fliessen lauter Gold,", "tokens": ["Draus", "th\u00e4t", "flies\u00b7sen", "lau\u00b7ter", "Gold", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Th\u00e4ten dort drei Jungfern stehen,", "tokens": ["Th\u00e4\u00b7ten", "dort", "drei", "Jung\u00b7fern", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "CARD", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gar so sch\u00f6n und gar so hold.", "tokens": ["Gar", "so", "sch\u00f6n", "und", "gar", "so", "hold", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "KON", "ADV", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Th\u00e4ten all so zu mir sprechen:", "tokens": ["Th\u00e4\u00b7ten", "all", "so", "zu", "mir", "spre\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "ADV", "APPR", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Trinkst du aus dem Br\u00fcnnelein,", "tokens": ["Trinkst", "du", "aus", "dem", "Br\u00fcn\u00b7nel\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kriegt dich einer bei dem Kragen,", "tokens": ["Kriegt", "dich", "ei\u00b7ner", "bei", "dem", "Kra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wirft dich in den Brunnen n'ein.", "tokens": ["Wirft", "dich", "in", "den", "Brun\u00b7nen", "n'\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+---+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Ihr sch\u00f6n Jungfern k\u00fchnlich glaubet,", "tokens": ["Ihr", "sch\u00f6n", "Jung\u00b7fern", "k\u00fchn\u00b7lich", "glau\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "NN", "ADJD", "VVFIN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Will den Durst nicht l\u00f6schen hier,", "tokens": ["Will", "den", "Durst", "nicht", "l\u00f6\u00b7schen", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PTKNEG", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn die sch\u00f6nste mir erlaubet", "tokens": ["Wenn", "die", "sch\u00f6ns\u00b7te", "mir", "er\u00b7lau\u00b7bet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einen zarten Ku\u00df allhier.", "tokens": ["Ei\u00b7nen", "zar\u00b7ten", "Ku\u00df", "all\u00b7hier", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Diese mit den schwarzen Augen", "tokens": ["Die\u00b7se", "mit", "den", "schwar\u00b7zen", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "APPR", "ART", "ADJA", "NN"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.2": {"text": "K\u00fc\u00df ich gern, trau aber nicht;", "tokens": ["K\u00fc\u00df", "ich", "gern", ",", "trau", "a\u00b7ber", "nicht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "VVFIN", "ADV", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sie kann nur zum Zanken taugen,", "tokens": ["Sie", "kann", "nur", "zum", "Zan\u00b7ken", "tau\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Aber zu der Liebe nicht.", "tokens": ["A\u00b7ber", "zu", "der", "Lie\u00b7be", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Diese mit den grauen Augen,", "tokens": ["Die\u00b7se", "mit", "den", "grau\u00b7en", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Diese falsche mag ich nicht;", "tokens": ["Die\u00b7se", "fal\u00b7sche", "mag", "ich", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "VMFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kann allein zum Roppen taugen,", "tokens": ["Kann", "al\u00b7lein", "zum", "Rop\u00b7pen", "tau\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Krazt den Buhlen ins Gesicht.", "tokens": ["Krazt", "den", "Buh\u00b7len", "ins", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Diese mit den blauen Augen,", "tokens": ["Die\u00b7se", "mit", "den", "blau\u00b7en", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Diese k\u00fc\u00df ich gar zu gern;", "tokens": ["Die\u00b7se", "k\u00fc\u00df", "ich", "gar", "zu", "gern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "PTKA", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Diese kann zur Liebe taugen,", "tokens": ["Die\u00b7se", "kann", "zur", "Lie\u00b7be", "tau\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Diese gleicht dem Morgenstern.", "tokens": ["Die\u00b7se", "gleicht", "dem", "Mor\u00b7gens\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+--", "measure": "unknown.measure.tri"}}}}}