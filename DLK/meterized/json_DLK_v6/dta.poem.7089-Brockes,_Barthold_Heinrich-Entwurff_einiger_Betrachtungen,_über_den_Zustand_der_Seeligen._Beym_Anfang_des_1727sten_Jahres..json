{"dta.poem.7089": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Entwurff einiger Betrachtungen,  \n \u00fcber den  \n  Zustand der Seeligen.  \n Beym Anfang des 1727sten Jahres.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Von der unendlichen Macht, Weisheit, Lieb und G\u00fcte", "tokens": ["Von", "der", "un\u00b7end\u00b7li\u00b7chen", "Macht", ",", "Weis\u00b7heit", ",", "Lieb", "und", "G\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des ew\u2019gen Ursprungs aller Wesen", "tokens": ["Des", "ew'\u00b7gen", "Ur\u00b7sprungs", "al\u00b7ler", "We\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wird ihr von seel\u2019ger Lust entz\u00fcckt Gem\u00fcthe", "tokens": ["Wird", "ihr", "von", "seel'\u00b7ger", "Lust", "ent\u00b7z\u00fcckt", "Ge\u00b7m\u00fc\u00b7the"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "ADJA", "NN", "VVPP", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den Inhalt, der sie mehr und mehr beseeligt, lesen.", "tokens": ["Den", "In\u00b7halt", ",", "der", "sie", "mehr", "und", "mehr", "be\u00b7see\u00b7ligt", ",", "le\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "KON", "ADV", "VVPP", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sie werden sich daran, zu ihres Sch\u00f6pfers Ehren,", "tokens": ["Sie", "wer\u00b7den", "sich", "da\u00b7ran", ",", "zu", "ih\u00b7res", "Sch\u00f6p\u00b7fers", "Eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "PAV", "$,", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "In seeliger Verwundrung, ewig n\u00e4hren.", "tokens": ["In", "see\u00b7li\u00b7ger", "Ver\u00b7wund\u00b7rung", ",", "e\u00b7wig", "n\u00e4h\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ein solch verhimmelndes Entz\u00fccken,", "tokens": ["Ein", "solch", "ver\u00b7him\u00b7meln\u00b7des", "Ent\u00b7z\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Das blo\u00df aus dem Begriff von GOTTES Werck entsteht,", "tokens": ["Das", "blo\u00df", "aus", "dem", "Be\u00b7griff", "von", "GoT\u00b7TES", "Werck", "ent\u00b7steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "APPR", "ART", "NN", "APPR", "NE", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ist abzubilden nicht, nicht auszudr\u00fccken.", "tokens": ["Ist", "ab\u00b7zu\u00b7bil\u00b7den", "nicht", ",", "nicht", "aus\u00b7zu\u00b7dr\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "VVIZU", "PTKNEG", "$,", "PTKNEG", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}