{"textgrid.poem.33032": {"metadata": {"author": {"name": "Flaischlen, C\u00e4sar", "birth": "N.A.", "death": "N.A."}, "title": "1L: Das aber ist das Schwere dann:", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das aber ist das Schwere dann:", "tokens": ["Das", "a\u00b7ber", "ist", "das", "Schwe\u00b7re", "dann", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VAFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "hinauszuwissen", "tokens": ["hin\u00b7aus\u00b7zu\u00b7wis\u00b7sen"], "token_info": ["word"], "pos": ["VVPP"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "\u00fcber ein erreichtes Ziel und:", "tokens": ["\u00fc\u00b7ber", "ein", "er\u00b7reich\u00b7tes", "Ziel", "und", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "KON", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "nicht stehen zu bleiben", "tokens": ["nicht", "ste\u00b7hen", "zu", "blei\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKNEG", "VVINF", "PTKZU", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "und sich bet\u00f6ren:", "tokens": ["und", "sich", "be\u00b7t\u00f6\u00b7ren", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PRF", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "nun sei's getan,", "tokens": ["nun", "sei's", "ge\u00b7tan", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "nun gehe alles seinen Gang,", "tokens": ["nun", "ge\u00b7he", "al\u00b7les", "sei\u00b7nen", "Gang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "nun habe alle Not ein Ende,", "tokens": ["nun", "ha\u00b7be", "al\u00b7le", "Not", "ein", "En\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "am Ziele anzukommen, sei genug!", "tokens": ["am", "Zie\u00b7le", "an\u00b7zu\u00b7kom\u00b7men", ",", "sei", "ge\u00b7nug", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVIZU", "$,", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich aber sage: es ist nicht genug!", "tokens": ["Ich", "a\u00b7ber", "sa\u00b7ge", ":", "es", "ist", "nicht", "ge\u00b7nug", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "$.", "PPER", "VAFIN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "und stehen bleiben rechnet \u00fcberhaupt nicht!", "tokens": ["und", "ste\u00b7hen", "blei\u00b7ben", "rech\u00b7net", "\u00fc\u00b7ber\u00b7haupt", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VVINF", "VVFIN", "ADV", "PTKNEG", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es gilt weit mehr, als nur ans Ziel zu kommen ...", "tokens": ["Es", "gilt", "weit", "mehr", ",", "als", "nur", "ans", "Ziel", "zu", "kom\u00b7men", "..."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ADV", "$,", "KOUS", "ADV", "APPRART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "im Gro\u00dfen wie im Kleinen,", "tokens": ["im", "Gro\u00b7\u00dfen", "wie", "im", "Klei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KOKOM", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "im Groben wie im Feinen:", "tokens": ["im", "Gro\u00b7ben", "wie", "im", "Fei\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KOKOM", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Es gilt: hinauszuwissen \u00fcber das Erreichte,", "tokens": ["Es", "gilt", ":", "hin\u00b7aus\u00b7zu\u00b7wis\u00b7sen", "\u00fc\u00b7ber", "das", "Er\u00b7reich\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "hinauszuringen \u00fcber das Errungne!", "tokens": ["hin\u00b7aus\u00b7zu\u00b7rin\u00b7gen", "\u00fc\u00b7ber", "das", "Er\u00b7rung\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "es gilt: von jedem erstrittenen Punkt", "tokens": ["es", "gilt", ":", "von", "je\u00b7dem", "er\u00b7strit\u00b7te\u00b7nen", "Punkt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "weiterzuwollen und weiterzusehn", "tokens": ["wei\u00b7ter\u00b7zu\u00b7wol\u00b7len", "und", "wei\u00b7ter\u00b7zu\u00b7sehn"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "KON", "VVINF"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.5": {"text": "und immer aufs neue Wege zu finden", "tokens": ["und", "im\u00b7mer", "aufs", "neu\u00b7e", "We\u00b7ge", "zu", "fin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "ADJA", "NN", "PTKZU", "VVINF"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "hochauf zu immer freieren H\u00f6hn!", "tokens": ["hoch\u00b7auf", "zu", "im\u00b7mer", "frei\u00b7e\u00b7ren", "H\u00f6hn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}}}}