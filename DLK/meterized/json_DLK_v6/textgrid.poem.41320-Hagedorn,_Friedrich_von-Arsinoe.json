{"textgrid.poem.41320": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Arsinoe", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Kennerin der Fehler und der S\u00fcnden,", "tokens": ["Die", "Ken\u00b7ne\u00b7rin", "der", "Feh\u00b7ler", "und", "der", "S\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Arsinoe, kann nichts unstr\u00e4flich finden,", "tokens": ["Ar\u00b7si\u00b7noe", ",", "kann", "nichts", "uns\u00b7tr\u00e4f\u00b7lich", "fin\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VMFIN", "PIS", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Nicht Chloens Witz, nicht Juliens Gestalt.", "tokens": ["Nicht", "Chloens", "Witz", ",", "nicht", "Ju\u00b7li\u00b7ens", "Ge\u00b7stalt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NE", "NN", "$,", "PTKNEG", "NE", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Sie ist mit sich, mit andern, unzufrieden;", "tokens": ["Sie", "ist", "mit", "sich", ",", "mit", "an\u00b7dern", ",", "un\u00b7zu\u00b7frie\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PRF", "$,", "APPR", "ADJA", "$,", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Nie wird ihr Mund im Unterricht erm\u00fcden.", "tokens": ["Nie", "wird", "ihr", "Mund", "im", "Un\u00b7ter\u00b7richt", "er\u00b7m\u00fc\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Fragt nicht warum; Arsinoe wird alt.", "tokens": ["Fragt", "nicht", "wa\u00b7rum", ";", "Ar\u00b7si\u00b7noe", "wird", "alt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "PWAV", "$.", "NE", "VAFIN", "ADJD", "$."], "meter": "----+-+-+", "measure": "unknown.measure.tri"}}}}}