{"textgrid.poem.60529": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Ein Epigramm", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Schwert zu f\u00fchren, die verschanzten Sitze", "tokens": ["Das", "Schwert", "zu", "f\u00fch\u00b7ren", ",", "die", "ver\u00b7schanz\u00b7ten", "Sit\u00b7ze"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des starken Feinds mit Pfeilen zu beschie\u00dfen,", "tokens": ["Des", "star\u00b7ken", "Feinds", "mit", "Pfei\u00b7len", "zu", "be\u00b7schie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "An seinem Fluch zu messen seine Wunde,", "tokens": ["An", "sei\u00b7nem", "Fluch", "zu", "mes\u00b7sen", "sei\u00b7ne", "Wun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ist meine Lust; und heut in m\u00fc\u00df'ger Stunde", "tokens": ["Ist", "mei\u00b7ne", "Lust", ";", "und", "heut", "in", "m\u00fc\u00df'\u00b7ger", "Stun\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "$.", "KON", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Freut michs, an Epigrammes Nadelspitze", "tokens": ["Freut", "michs", ",", "an", "E\u00b7pig\u00b7ram\u00b7mes", "Na\u00b7del\u00b7spit\u00b7ze"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PIS", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Zum Spa\u00df dich Eintagsfliege aufzuspie\u00dfen.", "tokens": ["Zum", "Spa\u00df", "dich", "Ein\u00b7tags\u00b7flie\u00b7ge", "auf\u00b7zu\u00b7spie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Dank mirs, so wirst du doch nicht gleich vergessen,", "tokens": ["Dank", "mirs", ",", "so", "wirst", "du", "doch", "nicht", "gleich", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "ADV", "VAFIN", "PPER", "ADV", "PTKNEG", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Nicht von der n\u00e4chsten Spinne aufgefressen.", "tokens": ["Nicht", "von", "der", "n\u00e4chs\u00b7ten", "Spin\u00b7ne", "auf\u00b7ge\u00b7fres\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}