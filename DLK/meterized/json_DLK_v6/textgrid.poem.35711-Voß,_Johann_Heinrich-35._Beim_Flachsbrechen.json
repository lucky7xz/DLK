{"textgrid.poem.35711": {"metadata": {"author": {"name": "Vo\u00df, Johann Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "35. Beim Flachsbrechen", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Plauderinnen, regt euch stracks!", "tokens": ["Plau\u00b7de\u00b7rin\u00b7nen", ",", "regt", "euch", "stracks", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Brecht den Flachs,", "tokens": ["Brecht", "den", "Flachs", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Da\u00df die Schebe springe,", "tokens": ["Da\u00df", "die", "Sche\u00b7be", "sprin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Und der Brechen Wechselklang", "tokens": ["Und", "der", "Bre\u00b7chen", "Wech\u00b7sel\u00b7klang"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mit Gesang", "tokens": ["Mit", "Ge\u00b7sang"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Fern das Dorf durchdringe!", "tokens": ["Fern", "das", "Dorf", "durch\u00b7drin\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Herbstlich rauscht im Fliederstrauch", "tokens": ["Herbst\u00b7lich", "rauscht", "im", "Flie\u00b7der\u00b7strauch"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Kalter Hauch,", "tokens": ["Kal\u00b7ter", "Hauch", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Und der Nachttau feuchtet!", "tokens": ["Und", "der", "Nacht\u00b7tau", "feuch\u00b7tet", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$."], "meter": "--+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Dennoch brecht mit blo\u00dfem Arm,", "tokens": ["Den\u00b7noch", "brecht", "mit", "blo\u00b7\u00dfem", "Arm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Brecht euch warm,", "tokens": ["Brecht", "euch", "warm", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Weil der Mond uns leuchtet!", "tokens": ["Weil", "der", "Mond", "uns", "leuch\u00b7tet", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Brich, du armer Flachs! dir droht", "tokens": ["Brich", ",", "du", "ar\u00b7mer", "Flachs", "!", "dir", "droht"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "$,", "PPER", "ADJA", "NN", "$.", "PPER", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "M\u00fch und Not,", "tokens": ["M\u00fch", "und", "Not", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Mehr denn je du tr\u00e4umtest,", "tokens": ["Mehr", "denn", "je", "du", "tr\u00e4um\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Als du gr\u00fcn im Sonnenschein,", "tokens": ["Als", "du", "gr\u00fcn", "im", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Junger Lein,", "tokens": ["Jun\u00b7ger", "Lein", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Blaue Blumen keimtest!", "tokens": ["Blau\u00b7e", "Blu\u00b7men", "ke\u00b7im\u00b7test", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "+-+----", "measure": "unknown.measure.di"}}, "stanza.4": {"line.1": {"text": "Ach, die harte Raufe hat", "tokens": ["Ach", ",", "die", "har\u00b7te", "Rau\u00b7fe", "hat"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "ART", "ADJA", "NN", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Gleich zur Saat", "tokens": ["Gleich", "zur", "Saat"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPRART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Dir die Boll' entrissen,", "tokens": ["Dir", "die", "Boll'", "ent\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Wochenlang dann auf der Au", "tokens": ["Wo\u00b7chen\u00b7lang", "dann", "auf", "der", "Au"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sonn' und Tau", "tokens": ["Sonn'", "und", "Tau"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "R\u00f6stend dich zerbissen!", "tokens": ["R\u00f6s\u00b7tend", "dich", "zer\u00b7bis\u00b7sen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Nun zerquetschen wir in Hast", "tokens": ["Nun", "zer\u00b7quet\u00b7schen", "wir", "in", "Hast"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dir den Bast,", "tokens": ["Dir", "den", "Bast", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Den die Schwinge reinigt;", "tokens": ["Den", "die", "Schwin\u00b7ge", "rei\u00b7nigt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Von der b\u00f6sen Hechel itzt,", "tokens": ["Von", "der", "b\u00f6\u00b7sen", "He\u00b7chel", "itzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Scharfgespitzt,", "tokens": ["Scharf\u00b7ge\u00b7spitzt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Wirst du durchgepeinigt!", "tokens": ["Wirst", "du", "durch\u00b7ge\u00b7pei\u00b7nigt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Doch dann prangst du glatt und sch\u00f6n;", "tokens": ["Doch", "dann", "prangst", "du", "glatt", "und", "sch\u00f6n", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Und wir drehn", "tokens": ["Und", "wir", "drehn"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPER", "CARD"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Dich in saubre Knocken:", "tokens": ["Dich", "in", "saub\u00b7re", "Kno\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Und getrillt mit flinkem Fu\u00df,", "tokens": ["Und", "ge\u00b7trillt", "mit", "flin\u00b7kem", "Fu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Feucht vom Ku\u00df,", "tokens": ["Feucht", "vom", "Ku\u00df", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "L\u00e4ufst du uns vom Rocken!", "tokens": ["L\u00e4ufst", "du", "uns", "vom", "Ro\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPRART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Schnell durch Spul' und Haspel eilt,", "tokens": ["Schnell", "durch", "Spul'", "und", "Has\u00b7pel", "eilt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sch\u00f6n gekn\u00e4ult,", "tokens": ["Sch\u00f6n", "ge\u00b7kn\u00e4ult", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVPP", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Drauf dein Garn zur Webe:", "tokens": ["Drauf", "dein", "Garn", "zur", "We\u00b7be", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Da\u00df die Leinwand, scharf gebeucht,", "tokens": ["Da\u00df", "die", "Lein\u00b7wand", ",", "scharf", "ge\u00b7beucht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und gebleicht,", "tokens": ["Und", "ge\u00b7bleicht", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVPP", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Hemd' und Laken gebe.", "tokens": ["Hemd'", "und", "La\u00b7ken", "ge\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Brich, o brich, du armer Flachs!", "tokens": ["Brich", ",", "o", "brich", ",", "du", "ar\u00b7mer", "Flachs", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "FM", "ADJD", "$,", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wei\u00df, wie Wachs,", "tokens": ["Wei\u00df", ",", "wie", "Wachs", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Prangst du angeschmieget,", "tokens": ["Prangst", "du", "an\u00b7ge\u00b7schmie\u00b7get", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Wann beim Br\u00e4utigam die Braut,", "tokens": ["Wann", "beim", "Br\u00e4u\u00b7ti\u00b7gam", "die", "Braut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Warm und traut,", "tokens": ["Warm", "und", "traut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Einst im Bette lieget!", "tokens": ["Einst", "im", "Bet\u00b7te", "lie\u00b7get", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}