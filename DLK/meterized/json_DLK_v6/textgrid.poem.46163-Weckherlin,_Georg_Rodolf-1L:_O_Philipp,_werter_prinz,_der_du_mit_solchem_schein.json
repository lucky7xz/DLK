{"textgrid.poem.46163": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "1L: O Philipp, werter prinz, der du mit solchem schein", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Philipp, werter prinz, der du mit solchem schein", "tokens": ["O", "Phi\u00b7lipp", ",", "wer\u00b7ter", "prinz", ",", "der", "du", "mit", "sol\u00b7chem", "schein"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "$,", "ADJA", "NN", "$,", "PRELS", "PPER", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "in deines alters ost die herzen \u00fcbergl\u00e4nzest", "tokens": ["in", "dei\u00b7nes", "al\u00b7ters", "ost", "die", "her\u00b7zen", "\u00fc\u00b7ber\u00b7gl\u00e4n\u00b7zest"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "wie v\u00f6llig wird dein glanz und unser lust wol sein,", "tokens": ["wie", "v\u00f6l\u00b7lig", "wird", "dein", "glanz", "und", "un\u00b7ser", "lust", "wol", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "ADV", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "wan deine tugend du in dem mittag erg\u00e4nzest.", "tokens": ["wan", "dei\u00b7ne", "tu\u00b7gend", "du", "in", "dem", "mit\u00b7tag", "er\u00b7g\u00e4n\u00b7zest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+--+-++-+--+-", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Wan schon der himmel sich durch unsers feinds kriegszeug", "tokens": ["Wan", "schon", "der", "him\u00b7mel", "sich", "durch", "un\u00b7sers", "feinds", "kriegs\u00b7zeug"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "NN", "PRF", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.6": {"text": "noch gegen dem Teutschland barmherzig nicht erzeiget,", "tokens": ["noch", "ge\u00b7gen", "dem", "Teutschland", "barm\u00b7her\u00b7zig", "nicht", "er\u00b7zei\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADJD", "PTKNEG", "VVFIN", "$,"], "meter": "++--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "erzeiget er doch schon durch so s\u00fc\u00dfedle zweig,", "tokens": ["er\u00b7zei\u00b7get", "er", "doch", "schon", "durch", "so", "s\u00fc\u00b7\u00dfed\u00b7le", "zweig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "als du und andre mehr, da\u00df er uns wol geneiget.", "tokens": ["als", "du", "und", "and\u00b7re", "mehr", ",", "da\u00df", "er", "uns", "wol", "ge\u00b7nei\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "KON", "PIS", "ADV", "$,", "KOUS", "PPER", "PRF", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Dan voll verstand dein herz, voll k\u00fchnheit deine hand,", "tokens": ["Dan", "voll", "ver\u00b7stand", "dein", "herz", ",", "voll", "k\u00fchn\u00b7heit", "dei\u00b7ne", "hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPOSAT", "NN", "$,", "ADJD", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "o jung und weiser held, nachfolgend den Moritzen,", "tokens": ["o", "jung", "und", "wei\u00b7ser", "held", ",", "nach\u00b7fol\u00b7gend", "den", "Mo\u00b7rit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["FM", "ADJD", "KON", "ADJD", "VVFIN", "$,", "VVPP", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "deren der ein mit macht, der ander mit verstand", "tokens": ["de\u00b7ren", "der", "ein", "mit", "macht", ",", "der", "an\u00b7der", "mit", "ver\u00b7stand"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "ART", "ART", "APPR", "VVFIN", "$,", "PRELS", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "der ganzen welt wol wert, soll das Teutschland besch\u00fctzen.", "tokens": ["der", "gan\u00b7zen", "welt", "wol", "wert", ",", "soll", "das", "Teutschland", "be\u00b7sch\u00fct\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ADJD", "$,", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Wan dan dein herz und hand, des einen kluge macht,", "tokens": ["Wan", "dan", "dein", "herz", "und", "hand", ",", "des", "ei\u00b7nen", "klu\u00b7ge", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPOSAT", "NN", "KON", "NN", "$,", "ART", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "der mutiger dan Mars, die starke lehr des andern,", "tokens": ["der", "mu\u00b7ti\u00b7ger", "dan", "Mars", ",", "die", "star\u00b7ke", "lehr", "des", "an\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "NN", "$,", "ART", "ADJA", "NN", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "dem Ph\u00f6bus selbs kaum gleich, nach wunsch in sich gebracht,", "tokens": ["dem", "Ph\u00f6\u00b7bus", "selbs", "kaum", "gleich", ",", "nach", "wunsch", "in", "sich", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "ADV", "$,", "APPR", "ADJD", "APPR", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "so seh ich, Philipp, dich so gro\u00df als Alexandern.", "tokens": ["so", "seh", "ich", ",", "Phi\u00b7lipp", ",", "dich", "so", "gro\u00df", "als", "A\u00b7lex\u00b7an\u00b7dern", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "NE", "$,", "PPER", "ADV", "ADJD", "KOKOM", "NE", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}