{"textgrid.poem.52566": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: In Garten bin ich gangen,", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In Garten bin ich gangen,", "tokens": ["In", "Gar\u00b7ten", "bin", "ich", "gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zu wandeln in dem Sonnenschein,", "tokens": ["Zu", "wan\u00b7deln", "in", "dem", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da f\u00e4llt mir mein lieb Liebchen ein", "tokens": ["Da", "f\u00e4llt", "mir", "mein", "lieb", "Lieb\u00b7chen", "ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "ADJD", "NN", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit ihren Rosenwangen", "tokens": ["Mit", "ih\u00b7ren", "Ro\u00b7sen\u00b7wan\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und klaren Aeugelein.", "tokens": ["Und", "kla\u00b7ren", "A\u00b7e\u00b7u\u00b7ge\u00b7lein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich hab zu mir gesprochen", "tokens": ["Ich", "hab", "zu", "mir", "ge\u00b7spro\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So recht im Stillen noch einmal:", "tokens": ["So", "recht", "im", "Stil\u00b7len", "noch", "ein\u00b7mal", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie litt um mich viel bittre Qual,", "tokens": ["Sie", "litt", "um", "mich", "viel", "bitt\u00b7re", "Qual", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr Herz ist schier gebrochen", "tokens": ["Ihr", "Herz", "ist", "schier", "ge\u00b7bro\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "In Thr\u00e4nen ohne Zahl.", "tokens": ["In", "Thr\u00e4\u00b7nen", "oh\u00b7ne", "Zahl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sie wollt an mir verzagen,", "tokens": ["Sie", "wollt", "an", "mir", "ver\u00b7za\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "An meiner Lieb an meiner Treu,", "tokens": ["An", "mei\u00b7ner", "Lieb", "an", "mei\u00b7ner", "Treu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und glaubte schon an Rank und Reu,", "tokens": ["Und", "glaub\u00b7te", "schon", "an", "Rank", "und", "Reu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "NN", "KON", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und f\u00fcrchtete, zu fragen,", "tokens": ["Und", "f\u00fcrch\u00b7te\u00b7te", ",", "zu", "fra\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Bek\u00fcmmert seelenscheu.", "tokens": ["Be\u00b7k\u00fcm\u00b7mert", "see\u00b7len\u00b7scheu", "."], "token_info": ["word", "word", "punct"], "pos": ["VVPP", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Sie aber kann nicht wanken.", "tokens": ["Sie", "a\u00b7ber", "kann", "nicht", "wan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sie ist so lieb, so treu, so gut,", "tokens": ["Sie", "ist", "so", "lieb", ",", "so", "treu", ",", "so", "gut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es lebt in ihr die alte Glut \u2013", "tokens": ["Es", "lebt", "in", "ihr", "die", "al\u00b7te", "Glut", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Lieb ist ohne Schranken,", "tokens": ["Die", "Lieb", "ist", "oh\u00b7ne", "Schran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sie ist ihr Lebensblut.", "tokens": ["Sie", "ist", "ihr", "Le\u00b7bens\u00b7blut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und kannst du es nicht fassen,", "tokens": ["Und", "kannst", "du", "es", "nicht", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wenn unsre Lieb sollt schlummern ein,", "tokens": ["Wenn", "uns\u00b7re", "Lieb", "sollt", "schlum\u00b7mern", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VMFIN", "VVINF", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So schw\u00f6r ichs hier beim Sonnenschein,", "tokens": ["So", "schw\u00f6r", "ichs", "hier", "beim", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich kann von dir nicht lassen,", "tokens": ["Ich", "kann", "von", "dir", "nicht", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ich bin noch immer dein!", "tokens": ["Ich", "bin", "noch", "im\u00b7mer", "dein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "O wundersch\u00f6ne Sonne,", "tokens": ["O", "wun\u00b7der\u00b7sch\u00f6\u00b7ne", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "O Sonne du des sch\u00f6nsten Mais!", "tokens": ["O", "Son\u00b7ne", "du", "des", "sch\u00f6ns\u00b7ten", "Mais", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als wie dein Strahl, so rein und hei\u00df", "tokens": ["Als", "wie", "dein", "Strahl", ",", "so", "rein", "und", "hei\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "KOKOM", "PPOSAT", "NN", "$,", "ADV", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist meines Herzens Wonne,", "tokens": ["Ist", "mei\u00b7nes", "Her\u00b7zens", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ist meine Lieb, Gott wei\u00df!", "tokens": ["Ist", "mei\u00b7ne", "Lieb", ",", "Gott", "wei\u00df", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$,", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}