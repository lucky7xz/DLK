{"textgrid.poem.34327": {"metadata": {"author": {"name": "Lenz, Jakob Michael Reinhold", "birth": "N.A.", "death": "N.A."}, "title": "54.", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich will, ich will den nagenden Beschwerden", "tokens": ["Ich", "will", ",", "ich", "will", "den", "na\u00b7gen\u00b7den", "Be\u00b7schwer\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "$,", "PPER", "VMFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Ende machen, will zur Quelle werden.", "tokens": ["Ein", "En\u00b7de", "ma\u00b7chen", ",", "will", "zur", "Quel\u00b7le", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "VMFIN", "APPRART", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Tief unterm Herzen diese Quaal", "tokens": ["Tief", "un\u00b7term", "Her\u00b7zen", "die\u00b7se", "Qua\u00b7al"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPRART", "NN", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ach Gott, verweint ich sie einmal!", "tokens": ["Ach", "Gott", ",", "ver\u00b7weint", "ich", "sie", "ein\u00b7mal", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$,", "VVFIN", "PPER", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vielleicht, vielleicht, vers\u00e4h sie sich,", "tokens": ["Viel\u00b7leicht", ",", "viel\u00b7leicht", ",", "ver\u00b7s\u00e4h", "sie", "sich", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "$,", "VVFIN", "PPER", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "O seelger Quell! und nehme mich f\u00fcr dich!", "tokens": ["O", "seel\u00b7ger", "Quell", "!", "und", "neh\u00b7me", "mich", "f\u00fcr", "dich", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$.", "KON", "VVFIN", "PRF", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}