{"dta.poem.3717": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Was m\u00f6chtest du nicht .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Buchsbaumes R\u00e4dle,               ", "tokens": ["Buchs\u00b7bau\u00b7mes", "R\u00e4d\u00b7le", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Ein' fl\u00e4chsene Schwing,", "tokens": ["Ein'", "fl\u00e4ch\u00b7se\u00b7ne", "Schwing", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Mein Schatz hei\u00dft Antonele,", "tokens": ["Mein", "Schatz", "hei\u00dft", "An\u00b7to\u00b7ne\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "NE", "$,"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.4": {"text": "Wie freut mich das Ding.", "tokens": ["Wie", "freut", "mich", "das", "Ding", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}