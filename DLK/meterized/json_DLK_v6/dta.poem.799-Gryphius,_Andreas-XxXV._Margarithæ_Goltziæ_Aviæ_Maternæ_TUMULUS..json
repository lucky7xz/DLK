{"dta.poem.799": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "XxXV.  \n Margarith\u00e6 Goltzi\u00e6 Avi\u00e6 Matern\u00e6  \n  TUMULUS.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Nvn jhr die Seelen noth/ nun jhr im dritten brand ", "tokens": ["Nvn", "jhr", "die", "See\u00b7len", "noth", "/", "nun", "jhr", "im", "drit\u00b7ten", "brand"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "PPER", "ART", "NN", "NN", "$(", "ADV", "PPOSAT", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hab\u2019t Ewre Stadt beweynt: nun euch de\u00df Himmels zei-", "tokens": ["Hab't", "Ew\u00b7re", "Stadt", "be\u00b7weynt", ":", "nun", "euch", "de\u00df", "Him\u00b7mels", "zei"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "VVPP", "$.", "ADV", "PPER", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "chen/", "tokens": ["chen", "/"], "token_info": ["word", "punct"], "pos": ["NE", "$("], "meter": "-", "measure": "single.down"}}, "stanza.2": {"line.1": {"text": "Der Erden fall/ erschreckt: nun jhr der Kinder leichen", "tokens": ["Der", "Er\u00b7den", "fall", "/", "er\u00b7schreckt", ":", "nun", "jhr", "der", "Kin\u00b7der", "lei\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$(", "VVPP", "$.", "ADV", "PPER", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Augen zu gedruckt/ mit schier erstarr\u2019ter hand.", "tokens": ["Die", "Au\u00b7gen", "zu", "ge\u00b7druckt", "/", "mit", "schier", "er\u00b7starr'\u00b7ter", "hand", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVPP", "$(", "APPR", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Nun was Euch lieb/ dahin! nun das betr\u00fcbte Land", "tokens": ["Nun", "was", "Euch", "lieb", "/", "da\u00b7hin", "!", "nun", "das", "be\u00b7tr\u00fcb\u00b7te", "Land"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PWS", "PPER", "ADJD", "$(", "PAV", "$.", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In flamm\u2019 vnd aschen f\u00e4llt: Nun alle trew sol weichen:", "tokens": ["In", "flamm'", "vnd", "asc\u00b7hen", "f\u00e4llt", ":", "Nun", "al\u00b7le", "trew", "sol", "wei\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "VVINF", "VVFIN", "$.", "ADV", "PIS", "ADJD", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nun Tugend selber stirb\u2019t; mu\u00df Ew\u2019r gesicht erbleichen/", "tokens": ["Nun", "Tu\u00b7gend", "sel\u00b7ber", "stir\u00b7b't", ";", "mu\u00df", "Ew'r", "ge\u00b7sicht", "er\u00b7blei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ADV", "VVFIN", "$.", "VMFIN", "NN", "VVPP", "VVINF", "$("], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Vnd man scharr\u2019t ewren Leib/ in/ von Blutt/ rotten sand.", "tokens": ["Vnd", "man", "scharr't", "ew\u00b7ren", "Leib", "/", "in", "/", "von", "Blutt", "/", "rot\u00b7ten", "sand", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPOSAT", "NN", "$(", "APPR", "$(", "APPR", "ADJD", "$(", "ADJA", "VVFIN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Jhr O betr\u00fcbte Fraw\u2019/ Jhr bringt ewr greises haar.", "tokens": ["Ihr", "O", "be\u00b7tr\u00fcb\u00b7te", "Fra\u00b7w'", "/", "Ihr", "bringt", "ewr", "grei\u00b7ses", "haar", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "VVFIN", "NE", "$(", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Nach tausendfachem Todt auff die beschwer\u2019te baar.", "tokens": ["Nach", "tau\u00b7send\u00b7fa\u00b7chem", "Todt", "auff", "die", "be\u00b7schwer'\u00b7te", "baar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vnd sucht die ware Ruh/ die ", "tokens": ["Vnd", "sucht", "die", "wa\u00b7re", "Ruh", "/", "die"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$(", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ach/ klag ich euch noch jtzt? da fiel ewer leben hin:", "tokens": ["Ach", "/", "klag", "ich", "euch", "noch", "jtzt", "?", "da", "fiel", "e\u00b7wer", "le\u00b7ben", "hin", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$(", "VVFIN", "PPER", "PPER", "ADV", "ADV", "$.", "ADV", "VVFIN", "ADJD", "VVFIN", "PTKVZ", "$."], "meter": "-+---+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Al\u00df Freund vnd Lust verging/ al\u00df ich geschieden bin.", "tokens": ["Al\u00df", "Freund", "vnd", "Lust", "ver\u00b7ging", "/", "al\u00df", "ich", "ge\u00b7schie\u00b7den", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "VVFIN", "$(", "KOUS", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Jhr seidt dem Land auch nicht/ das Land ist euch gestorben.", "tokens": ["Ihr", "seidt", "dem", "Land", "auch", "nicht", "/", "das", "Land", "ist", "euch", "ge\u00b7stor\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "PTKNEG", "$(", "ART", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "XxXVI. ", "tokens": ["XxX\u00b7VI", "."], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "An den vortrefflichen Philosophum vnd ", "tokens": ["An", "den", "vor\u00b7treff\u00b7li\u00b7chen", "Phi\u00b7lo\u00b7so\u00b7phum", "vnd"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "KON"], "meter": "+-++-+-+-+-", "measure": "unknown.measure.hexa"}, "line.3": {"text": "Mathematicum ", "tokens": ["Ma\u00b7the\u00b7ma\u00b7ti\u00b7cum"], "token_info": ["word"], "pos": ["NE"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "PeTRUM GRUGERUM. ", "tokens": ["PeT\u00b7RUM", "GrU\u00b7GE\u00b7RUM", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Vber den Tod seines Kindes Anno ", "tokens": ["Vber", "den", "Tod", "sei\u00b7nes", "Kin\u00b7des", "An\u00b7no"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "\u217d\u217c\u2184 \u217c\u2184\u217d ", "tokens": ["\u217d\u217c\u2184", "\u217c\u2184\u217d"], "token_info": ["word", "word"], "pos": ["FM.la", "FM.la"], "meter": "+-", "measure": "trochaic.single"}}, "stanza.7": {"line.1": {"text": "Odantzig! wil der Zorn der Deutschland gantz verzehret/ ", "tokens": ["O\u00b7dant\u00b7zig", "!", "wil", "der", "Zorn", "der", "Deutschland", "gantz", "ver\u00b7zeh\u00b7ret", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VMFIN", "ART", "NN", "ART", "NN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "So rasend auff dich zu? wil der/ der alle richt", "tokens": ["So", "ra\u00b7send", "auff", "dich", "zu", "?", "wil", "der", "/", "der", "al\u00b7le", "richt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "PPER", "PTKVZ", "$.", "VMFIN", "ART", "$(", "ART", "PIAT", "NN"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Durch seiner blitzen glutt/ die Thron\u2019 vnd Krone bricht/", "tokens": ["Durch", "sei\u00b7ner", "blit\u00b7zen", "glutt", "/", "die", "Thron'", "vnd", "Kro\u00b7ne", "bricht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "VVFIN", "ADJD", "$(", "ART", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So schrecklich \u00fcber dich/ al\u00df scharff er vn\u00df verheret?", "tokens": ["So", "schreck\u00b7lich", "\u00fc\u00b7ber", "dich", "/", "al\u00df", "scharff", "er", "vn\u00df", "ver\u00b7he\u00b7ret", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PPER", "$(", "ADV", "VVFIN", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Ist niemand der mit ernst den grossen Eyver wehret?", "tokens": ["Ist", "nie\u00b7mand", "der", "mit", "ernst", "den", "gros\u00b7sen", "Ey\u00b7ver", "weh\u00b7ret", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ART", "APPR", "ADJD", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der seine schl\u00e4g auffhelt? der Todt schont l\u00e4nger nicht.", "tokens": ["Der", "sei\u00b7ne", "schl\u00e4g", "auf\u00b7fhelt", "?", "der", "Todt", "schont", "l\u00e4n\u00b7ger", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJD", "VVPP", "$.", "ART", "NN", "VVFIN", "ADJD", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Er folgt der Blutt Trommet vnd rei\u00dft au\u00df diesem licht/", "tokens": ["Er", "folgt", "der", "Blutt", "Trom\u00b7met", "vnd", "rei\u00dft", "au\u00df", "die\u00b7sem", "licht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJD", "NN", "KON", "VVFIN", "APPR", "PDAT", "NN", "$("], "meter": "-+-++--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Die Kinder/ die ja Gott mit keiner schuld beschweret!", "tokens": ["Die", "Kin\u00b7der", "/", "die", "ja", "Gott", "mit", "kei\u00b7ner", "schuld", "be\u00b7schwe\u00b7ret", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PRELS", "ADV", "NN", "APPR", "PIS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Die Kinder:Gottes Lust! wie \u00fcbel ists bestellt/", "tokens": ["Die", "Kin\u00b7der", ":", "Got\u00b7tes", "Lust", "!", "wie", "\u00fc\u00b7bel", "ists", "be\u00b7stellt", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "NN", "NN", "$.", "PWAV", "ADJD", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn diese Brustwehr hin/ vnd wenn der hauffs f\u00e4lt", "tokens": ["Wenn", "die\u00b7se", "Brust\u00b7wehr", "hin", "/", "vnd", "wenn", "der", "hauffs", "f\u00e4lt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PDAT", "NN", "PTKVZ", "$(", "KON", "KOUS", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Der einig m\u00e4chtig ist den h\u00f6chsten zu bekrigen!", "tokens": ["Der", "ei\u00b7nig", "m\u00e4ch\u00b7tig", "ist", "den", "h\u00f6chs\u00b7ten", "zu", "be\u00b7kri\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJD", "VAFIN", "ART", "ADJA", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Trifft es zum Ersten Euch Herr Kriger O f\u00fcrwar", "tokens": ["Trifft", "es", "zum", "Ers\u00b7ten", "Euch", "Herr", "Kri\u00b7ger", "O", "f\u00fcr\u00b7war"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PPER", "NN", "NN", "NE", "NE"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Di\u00df Kind/ di\u00df bleiche Kind/ das jtzt ligt auff der bar/", "tokens": ["Di\u00df", "Kind", "/", "di\u00df", "blei\u00b7che", "Kind", "/", "das", "jtzt", "ligt", "auff", "der", "bar", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "$(", "PDS", "ADJA", "NN", "$(", "PDS", "ADV", "VVFIN", "APPR", "ART", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Di\u00df w\u00fcrde/ wenn es st\u00fcnd\u2019 alleine wol obsiegen.", "tokens": ["Di\u00df", "w\u00fcr\u00b7de", "/", "wenn", "es", "st\u00fcnd'", "al\u00b7lei\u00b7ne", "wol", "ob\u00b7sie\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$(", "KOUS", "PPER", "VVFIN", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}