{"textgrid.poem.39631": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Hofzeitung hat dein Pamphlet gepriesen,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Hofzeitung hat dein Pamphlet gepriesen,", "tokens": ["Die", "Hof\u00b7zei\u00b7tung", "hat", "dein", "Pam\u00b7ph\u00b7let", "ge\u00b7prie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Du habest dich als Patriot erwiesen.", "tokens": ["Du", "ha\u00b7best", "dich", "als", "Pat\u00b7ri\u00b7ot", "er\u00b7wie\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "KOUS", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den Tag darauf tr\u00e4gst du ein reines Hemd;", "tokens": ["Den", "Tag", "da\u00b7rauf", "tr\u00e4gst", "du", "ein", "rei\u00b7nes", "Hemd", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PAV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das ist an dir uns ungewohnt und fremd.", "tokens": ["Das", "ist", "an", "dir", "uns", "un\u00b7ge\u00b7wohnt", "und", "fremd", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "PPER", "PRF", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Freund! deine Schrift und W\u00e4sche l\u00e4\u00dft errathen,", "tokens": ["Freund", "!", "dei\u00b7ne", "Schrift", "und", "W\u00e4\u00b7sche", "l\u00e4\u00dft", "er\u00b7ra\u00b7then", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPOSAT", "NN", "KON", "NN", "VVFIN", "VVINF", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.6": {"text": "Du z\u00e4hlst auf Gunst bei hohen Potentaten.", "tokens": ["Du", "z\u00e4hlst", "auf", "Gunst", "bei", "ho\u00b7hen", "Po\u00b7ten\u00b7ta\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}