{"textgrid.poem.25550": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Soll ich noch vergessen? noch vergeben?", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Soll ich noch vergessen? noch vergeben?", "tokens": ["Soll", "ich", "noch", "ver\u00b7ges\u00b7sen", "?", "noch", "ver\u00b7ge\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "VVPP", "$.", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Soll ich, oder soll ich nicht?", "tokens": ["Soll", "ich", ",", "o\u00b7der", "soll", "ich", "nicht", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "KON", "VMFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Eins von beiden! Tod und Leben", "tokens": ["Eins", "von", "bei\u00b7den", "!", "Tod", "und", "Le\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "APPR", "PIAT", "$.", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Steht itzt in des Zweifels Wage", "tokens": ["Steht", "itzt", "in", "des", "Zwei\u00b7fels", "Wa\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "L\u00e4nger nicht im Gleichgewicht'.", "tokens": ["L\u00e4n\u00b7ger", "nicht", "im", "Gleich\u00b7ge\u00b7wicht'", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Mich verrathen hat sie, mich verrathen!", "tokens": ["Mich", "ver\u00b7ra\u00b7then", "hat", "sie", ",", "mich", "ver\u00b7ra\u00b7then", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVPP", "VAFIN", "PPER", "$,", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Hat verkauft mein Herz um Gold,", "tokens": ["Hat", "ver\u00b7kauft", "mein", "Herz", "um", "Gold", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Und vertauscht den Geist, der Thaten", "tokens": ["Und", "ver\u00b7tauscht", "den", "Geist", ",", "der", "Tha\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Sang und that, mit einem K\u00f6rper,", "tokens": ["Sang", "und", "that", ",", "mit", "ei\u00b7nem", "K\u00f6r\u00b7per", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Der nur i\u00dft und trinkt und schmollt.", "tokens": ["Der", "nur", "i\u00dft", "und", "trinkt", "und", "schmollt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Auf im Feuer sollst du Locke fliegen!", "tokens": ["Auf", "im", "Feu\u00b7er", "sollst", "du", "Lo\u00b7cke", "flie\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "NN", "VMFIN", "PPER", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Ich zertrete, was sie sang,", "tokens": ["Ich", "zer\u00b7tre\u00b7te", ",", "was", "sie", "sang", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Und zernichte ihre L\u00fcgen,", "tokens": ["Und", "zer\u00b7nich\u00b7te", "ih\u00b7re", "L\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Und zerrei\u00dfe mit den Z\u00e4hnen", "tokens": ["Und", "zer\u00b7rei\u00b7\u00dfe", "mit", "den", "Z\u00e4h\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Was aus ihrer Hand entsprang.", "tokens": ["Was", "aus", "ih\u00b7rer", "Hand", "ent\u00b7sprang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.16": {"text": "Dich, ihr Bild, will ich behalten,", "tokens": ["Dich", ",", "ihr", "Bild", ",", "will", "ich", "be\u00b7hal\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "NN", "$,", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Jedem sagen, der es schaut:", "tokens": ["Je\u00b7dem", "sa\u00b7gen", ",", "der", "es", "schaut", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVINF", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "Seht! das ist sie, die mit kalten", "tokens": ["Seht", "!", "das", "ist", "sie", ",", "die", "mit", "kal\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$.", "PDS", "VAFIN", "PPER", "$,", "PRELS", "APPR", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Blute mich verkaufen konnte!", "tokens": ["Blu\u00b7te", "mich", "ver\u00b7kau\u00b7fen", "konn\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Sehet! das war meine Braut!", "tokens": ["Se\u00b7het", "!", "das", "war", "mei\u00b7ne", "Braut", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "PDS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.21": {"text": "Sey verflucht, du ihre Lieblingslinde!", "tokens": ["Sey", "ver\u00b7flucht", ",", "du", "ih\u00b7re", "Lieb\u00b7lings\u00b7lin\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$,", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.22": {"text": "Dich zertr\u00fcmmere der Blitz!", "tokens": ["Dich", "zer\u00b7tr\u00fcm\u00b7me\u00b7re", "der", "Blitz", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.23": {"text": "Weggetilgt aus deiner Rinde", "tokens": ["Weg\u00b7ge\u00b7tilgt", "aus", "dei\u00b7ner", "Rin\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Werd' ihr Name! deine Wurzel", "tokens": ["Werd'", "ihr", "Na\u00b7me", "!", "dei\u00b7ne", "Wur\u00b7zel"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "$.", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Sey hinfort der Schlangen Sitz!", "tokens": ["Sey", "hin\u00b7fort", "der", "Schlan\u00b7gen", "Sitz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.26": {"text": "Sey verflucht, du Bach der Riesenh\u00f6hle!", "tokens": ["Sey", "ver\u00b7flucht", ",", "du", "Bach", "der", "Rie\u00b7sen\u00b7h\u00f6h\u00b7le", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$,", "PPER", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.27": {"text": "Wo aus ihren H\u00e4nden, ach!", "tokens": ["Wo", "aus", "ih\u00b7ren", "H\u00e4n\u00b7den", ",", "ach", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "APPR", "PPOSAT", "NN", "$,", "ITJ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.28": {"text": "Mehr das Lechzen meiner Seele,", "tokens": ["Mehr", "das", "Lech\u00b7zen", "mei\u00b7ner", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "Als der Durst der Zunge, schl\u00fcrfte;", "tokens": ["Als", "der", "Durst", "der", "Zun\u00b7ge", ",", "schl\u00fcrf\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "$,", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Sey verflucht! versieg', o Bach!", "tokens": ["Sey", "ver\u00b7flucht", "!", "ver\u00b7sieg'", ",", "o", "Bach", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$.", "VVFIN", "$,", "FM", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.31": {"text": "Sey verflucht, du ihre Rebenlaube!", "tokens": ["Sey", "ver\u00b7flucht", ",", "du", "ih\u00b7re", "Re\u00b7ben\u00b7lau\u00b7be", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$,", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.32": {"text": "Wo in N\u00e4chten sonder Ruh',", "tokens": ["Wo", "in", "N\u00e4ch\u00b7ten", "son\u00b7der", "Ruh'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.33": {"text": "Sie die Beeren aus der Traube", "tokens": ["Sie", "die", "Bee\u00b7ren", "aus", "der", "Trau\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Mir in ihren Lippen reichte;", "tokens": ["Mir", "in", "ih\u00b7ren", "Lip\u00b7pen", "reich\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.35": {"text": "Sey verflucht! verdorre du!", "tokens": ["Sey", "ver\u00b7flucht", "!", "ver\u00b7dor\u00b7re", "du", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$.", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.36": {"text": "Aber dennoch! \u2013 k\u00f6nnt' ich nur ihn finden!", "tokens": ["A\u00b7ber", "den\u00b7noch", "!", "\u2013", "k\u00f6nnt'", "ich", "nur", "ihn", "fin\u00b7den", "!"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$.", "$(", "VMFIN", "PPER", "ADV", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.37": {"text": "Hin durch L\u00e4nder weit und breit", "tokens": ["Hin", "durch", "L\u00e4n\u00b7der", "weit", "und", "breit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.38": {"text": "Rennt' ich, um des M\u00e4dchens S\u00fcnden", "tokens": ["Rennt'", "ich", ",", "um", "des", "M\u00e4d\u00b7chens", "S\u00fcn\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "KOUI", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.39": {"text": "Zu ers\u00e4ufen in dem Wasser", "tokens": ["Zu", "er\u00b7s\u00e4u\u00b7fen", "in", "dem", "Was\u00b7ser"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.40": {"text": "Deines Quells, Vergessenheit!", "tokens": ["Dei\u00b7nes", "Quells", ",", "Ver\u00b7ges\u00b7sen\u00b7heit", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.41": {"text": "Ach! ich s\u00e4he sie vielleicht von neuen,", "tokens": ["Ach", "!", "ich", "s\u00e4\u00b7he", "sie", "viel\u00b7leicht", "von", "neu\u00b7en", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPER", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.42": {"text": "Liebte, seufzt' und w\u00fcrd' erh\u00f6rt,", "tokens": ["Lieb\u00b7te", ",", "seufzt'", "und", "w\u00fcrd'", "er\u00b7h\u00f6rt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "KON", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.43": {"text": "F\u00e4nde statt der Ungetreuen,", "tokens": ["F\u00e4n\u00b7de", "statt", "der", "Un\u00b7ge\u00b7treu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.44": {"text": "Eine Seele, die die Erde", "tokens": ["Ei\u00b7ne", "See\u00b7le", ",", "die", "die", "Er\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.45": {"text": "Sonst zum Himmel umgekehrt.", "tokens": ["Sonst", "zum", "Him\u00b7mel", "um\u00b7ge\u00b7kehrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.46": {"text": "Weinen kann ich, ja sogar vergeben,", "tokens": ["Wei\u00b7nen", "kann", "ich", ",", "ja", "so\u00b7gar", "ver\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "$,", "ADV", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.47": {"text": "Aber ach! vergessen nicht!", "tokens": ["A\u00b7ber", "ach", "!", "ver\u00b7ges\u00b7sen", "nicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "$.", "VVFIN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.48": {"text": "Dieses Knirschen, dieses Beben,", "tokens": ["Die\u00b7ses", "Knir\u00b7schen", ",", "die\u00b7ses", "Be\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.49": {"text": "Will nicht Rache, will nur Liebe,", "tokens": ["Will", "nicht", "Ra\u00b7che", ",", "will", "nur", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "NN", "$,", "VMFIN", "ADV", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.50": {"text": "Der mein Busen widerspricht.", "tokens": ["Der", "mein", "Bu\u00b7sen", "wi\u00b7der\u00b7spricht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.51": {"text": "Da\u00df ich taub bei allem ihrem Flehen,", "tokens": ["Da\u00df", "ich", "taub", "bei", "al\u00b7lem", "ih\u00b7rem", "Fle\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "APPR", "PIS", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.52": {"text": "Blind vor ihren Thr\u00e4nen bin!", "tokens": ["Blind", "vor", "ih\u00b7ren", "Thr\u00e4\u00b7nen", "bin", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.53": {"text": "O! wer lehrt mich h\u00f6ren, sehen? \u2013", "tokens": ["O", "!", "wer", "lehrt", "mich", "h\u00f6\u00b7ren", ",", "se\u00b7hen", "?", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["NE", "$.", "PWS", "VVFIN", "PPER", "VVINF", "$,", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.54": {"text": "Nichts! die Lieb' ist hingestorben,", "tokens": ["Nichts", "!", "die", "Lieb'", "ist", "hin\u00b7ge\u00b7stor\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$.", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.55": {"text": "Stirb denn du mein Herz auch hin!", "tokens": ["Stirb", "denn", "du", "mein", "Herz", "auch", "hin", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "KON", "PPER", "PPOSAT", "NN", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}