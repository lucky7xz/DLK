{"dta.poem.6032": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Sonne, den Planeten,  \n und vom Firmament.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.57", "af:0.42"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Kan es wol anders seyn, da\u00df unsre Erde,", "tokens": ["Kan", "es", "wol", "an\u00b7ders", "seyn", ",", "da\u00df", "uns\u00b7re", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "VAINF", "$,", "KOUS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Jm Raum, der fl\u00fcssig ist, getrieben und gewiegt,", "tokens": ["Jm", "Raum", ",", "der", "fl\u00fcs\u00b7sig", "ist", ",", "ge\u00b7trie\u00b7ben", "und", "ge\u00b7wiegt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "ADJD", "VAFIN", "$,", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie andre C\u00f6rper auch, die so, wie sie, gef\u00fcgt,", "tokens": ["Wie", "and\u00b7re", "C\u00f6r\u00b7per", "auch", ",", "die", "so", ",", "wie", "sie", ",", "ge\u00b7f\u00fcgt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "ADV", "$,", "PRELS", "ADV", "$,", "PWAV", "PPER", "$,", "VVPP", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Nicht auch wie sie, mit fortgef\u00fchret werde?", "tokens": ["Nicht", "auch", "wie", "sie", ",", "mit", "fort\u00b7ge\u00b7f\u00fch\u00b7ret", "wer\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "KOKOM", "PPER", "$,", "APPR", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Indem des Himmels Trieb, der gr\u00f6ssre C\u00f6rper tr\u00e4get,", "tokens": ["In\u00b7dem", "des", "Him\u00b7mels", "Trieb", ",", "der", "gr\u00f6ss\u00b7re", "C\u00f6r\u00b7per", "tr\u00e4\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "$,", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Sie ja mit weniger Gewalt beweget.", "tokens": ["Sie", "ja", "mit", "we\u00b7ni\u00b7ger", "Ge\u00b7walt", "be\u00b7we\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Durch welche Ketten, welche Bande", "tokens": ["Durch", "wel\u00b7che", "Ket\u00b7ten", ",", "wel\u00b7che", "Ban\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PWAT", "NN", "$,", "PWAT", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Erhielte sie sich doch in einem festen Stande?", "tokens": ["Er\u00b7hiel\u00b7te", "sie", "sich", "doch", "in", "ei\u00b7nem", "fes\u00b7ten", "Stan\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ja, wie kan es doch nur den Schein der Wahrheit kriegen?", "tokens": ["Ja", ",", "wie", "kan", "es", "doch", "nur", "den", "Schein", "der", "Wahr\u00b7heit", "krie\u00b7gen", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PWAV", "VMFIN", "PPER", "ADV", "ADV", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+---+-+-+-+-", "measure": "dactylic.init"}, "line.10": {"text": "Die Sonne, die ein solch unme\u00dfbares Gef\u00e4sse,", "tokens": ["Die", "Son\u00b7ne", ",", "die", "ein", "solch", "un\u00b7me\u00df\u00b7ba\u00b7res", "Ge\u00b7f\u00e4s\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "PIAT", "ADJA", "NN", "$,"], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Soll in so kurtzer Zeit des Creyses Rund durchfliegen", "tokens": ["Soll", "in", "so", "kurt\u00b7zer", "Zeit", "des", "Crey\u00b7ses", "Rund", "durch\u00b7flie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "ADV", "ADJA", "NN", "ART", "NN", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Von ihres Wirbels Wunder-Gr\u00f6sse?", "tokens": ["Von", "ih\u00b7res", "Wir\u00b7bels", "Wun\u00b7der\u00b7Gr\u00f6s\u00b7se", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Sie soll, sammt den gestirnten Creysen,", "tokens": ["Sie", "soll", ",", "sammt", "den", "ge\u00b7stirn\u00b7ten", "Crey\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.14": {"text": "(ob ihren Raum kein Mensch gleich rechnen kan;)", "tokens": ["(", "ob", "ih\u00b7ren", "Raum", "kein", "Mensch", "gleich", "rech\u00b7nen", "kan", ";)"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "emoticon"], "pos": ["$(", "KOUS", "PPOSAT", "NN", "PIAT", "NN", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Auch der Planeten Heer,", "tokens": ["Auch", "der", "Pla\u00b7ne\u00b7ten", "Heer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.16": {"text": "Saturn und Jupiter,", "tokens": ["Sa\u00b7turn", "und", "Ju\u00b7pi\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "$,"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.17": {"text": "Durch eine t\u00e4gliche Bewegung nur allein", "tokens": ["Durch", "ei\u00b7ne", "t\u00e4g\u00b7li\u00b7che", "Be\u00b7we\u00b7gung", "nur", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ADV", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "In so gar wenigen Minuten um uns reisen?", "tokens": ["In", "so", "gar", "we\u00b7ni\u00b7gen", "Mi\u00b7nu\u00b7ten", "um", "uns", "rei\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADV", "PIAT", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "+-++-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.19": {"text": "Wie? hat denn in der Welt die Erde, die so klein,", "tokens": ["Wie", "?", "hat", "denn", "in", "der", "Welt", "die", "Er\u00b7de", ",", "die", "so", "klein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "VAFIN", "ADV", "APPR", "ART", "NN", "ART", "NN", "$,", "PRELS", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Als wie ein K\u00f6rnchen Sand,", "tokens": ["Als", "wie", "ein", "K\u00f6rn\u00b7chen", "Sand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.21": {"text": "Jm Raum, der fl\u00fcssig, einen Stand,", "tokens": ["Jm", "Raum", ",", "der", "fl\u00fcs\u00b7sig", ",", "ei\u00b7nen", "Stand", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "ADJD", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Der unver\u00e4nderlich, allein?", "tokens": ["Der", "un\u00b7ver\u00b7\u00e4n\u00b7der\u00b7lich", ",", "al\u00b7lein", "?"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJD", "$,", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Sie sieht, indem sie feste stehet,", "tokens": ["Sie", "sieht", ",", "in\u00b7dem", "sie", "fes\u00b7te", "ste\u00b7het", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Da\u00df die so grosse Welt um sie in Creyse gehet.", "tokens": ["Da\u00df", "die", "so", "gros\u00b7se", "Welt", "um", "sie", "in", "Crey\u00b7se", "ge\u00b7het", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADV", "ADJA", "NN", "APPR", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Schliesst man denn nicht mit mehr Wahrscheinlichkeit,", "tokens": ["Schliesst", "man", "denn", "nicht", "mit", "mehr", "Wahr\u00b7schein\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "PTKNEG", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.26": {"text": "Da\u00df sich die Erde allezeit", "tokens": ["Da\u00df", "sich", "die", "Er\u00b7de", "al\u00b7le\u00b7zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Um ihre kleine Axe drehet?", "tokens": ["Um", "ih\u00b7re", "klei\u00b7ne", "A\u00b7xe", "dre\u00b7het", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}