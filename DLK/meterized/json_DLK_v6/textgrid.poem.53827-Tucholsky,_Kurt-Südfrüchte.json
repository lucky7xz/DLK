{"textgrid.poem.53827": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "S\u00fcdfr\u00fcchte", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn einer und er i\u00dft Mittach \u2013", "tokens": ["Wenn", "ei\u00b7ner", "und", "er", "i\u00dft", "Mit\u00b7tach", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "KON", "PPER", "VVFIN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "feine Leute nennen di\u00df \u00bbfriehsticken\u00ab \u2013", "tokens": ["fei\u00b7ne", "Leu\u00b7te", "nen\u00b7nen", "di\u00df", "\u00bb", "frieh\u00b7sti\u00b7cken", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PDS", "$(", "VVINF", "$(", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "also: wenn er friehstickt unbestellt sich zum Schlu\u00df", "tokens": ["al\u00b7so", ":", "wenn", "er", "frieh\u00b7stickt", "un\u00b7be\u00b7stellt", "sich", "zum", "Schlu\u00df"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$.", "KOUS", "PPER", "VVFIN", "ADJD", "PRF", "APPRART", "NN"], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.4": {"text": "wat Sie\u00dfet: Appelsinenauflauf mit Gu\u00df,", "tokens": ["wat", "Sie\u00b7\u00dfet", ":", "Ap\u00b7pel\u00b7si\u00b7nen\u00b7auf\u00b7lauf", "mit", "Gu\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "$.", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "und nu wird ihm der Kellner eine Gurke bringen \u2013", "tokens": ["und", "nu", "wird", "ihm", "der", "Kell\u00b7ner", "ei\u00b7ne", "Gur\u00b7ke", "brin\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ART", "NN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "so sagt der: \u00bbNehm Sie das wieder fott!", "tokens": ["so", "sagt", "der", ":", "\u00bb", "Nehm", "Sie", "das", "wie\u00b7der", "fott", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "$.", "$(", "VVFIN", "PPER", "PDS", "ADV", "PTKVZ", "$."], "meter": "-+-++-+-+", "measure": "unknown.measure.penta"}, "line.7": {"text": "Denn, lieber Herr Ober:", "tokens": ["Denn", ",", "lie\u00b7ber", "Herr", "O\u00b7ber", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADV", "NN", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Saure Jurke ist keen Kompott!\u00ab", "tokens": ["Sau\u00b7re", "Jur\u00b7ke", "ist", "ke\u00b7en", "Kom\u00b7pott", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "VAFIN", "PIAT", "NN", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "So auch im Leben.", "tokens": ["So", "auch", "im", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Da glauben Biederbrillen und Fortschrittskneifer,", "tokens": ["Da", "glau\u00b7ben", "Bie\u00b7der\u00b7bril\u00b7len", "und", "Fort\u00b7schritts\u00b7knei\u00b7fer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "das deutsche politische Leben w\u00fcrde immer reifer und reifer.", "tokens": ["das", "deut\u00b7sche", "po\u00b7li\u00b7ti\u00b7sche", "Le\u00b7ben", "w\u00fcr\u00b7de", "im\u00b7mer", "rei\u00b7fer", "und", "rei\u00b7fer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+-+--+-", "measure": "iambic.octa.plus.relaxed"}, "line.4": {"text": "Und sprechen davon, bei Tage, abends und in der Nacht,", "tokens": ["Und", "spre\u00b7chen", "da\u00b7von", ",", "bei", "Ta\u00b7ge", ",", "a\u00b7bends", "und", "in", "der", "Nacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "$,", "APPR", "NN", "$,", "ADV", "KON", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "wie wir es doch so herrlich weit gebracht.", "tokens": ["wie", "wir", "es", "doch", "so", "herr\u00b7lich", "weit", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "ADV", "ADV", "ADJD", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und wir h\u00e4tten eine Republik und eine Verfassung", "tokens": ["Und", "wir", "h\u00e4t\u00b7ten", "ei\u00b7ne", "Re\u00b7pub\u00b7lik", "und", "ei\u00b7ne", "Ver\u00b7fas\u00b7sung"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "ART", "NN", "KON", "ART", "NN"], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.7": {"text": "und ein freies Wahlrecht dazu \u2013", "tokens": ["und", "ein", "frei\u00b7es", "Wahl\u00b7recht", "da\u00b7zu", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "PAV", "$("], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "wat sagste nu \u2013?", "tokens": ["wat", "sags\u00b7te", "nu", "\u2013", "?"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PWS", "VVFIN", "ADV", "$(", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Ich sage: Das Schild hat gewechselt, sag ich \u2013 im Laden der alte Trott:", "tokens": ["Ich", "sa\u00b7ge", ":", "Das", "Schild", "hat", "ge\u00b7wech\u00b7selt", ",", "sag", "ich", "\u2013", "im", "La\u00b7den", "der", "al\u00b7te", "Trott", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "ART", "NN", "VAFIN", "VVPP", "$,", "VVFIN", "PPER", "$(", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.10": {"text": "Saure Jurke is keen Kompott!", "tokens": ["Sau\u00b7re", "Jur\u00b7ke", "is", "ke\u00b7en", "Kom\u00b7pott", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "VVFIN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Und ich sage: Wenn ich so abends die Herren Ministersch seh,", "tokens": ["Und", "ich", "sa\u00b7ge", ":", "Wenn", "ich", "so", "a\u00b7bends", "die", "Her\u00b7ren", "Mi\u00b7nis\u00b7ter\u00b7sch", "seh", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$.", "KOUS", "PPER", "ADV", "ADV", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+---+-+--+--+--+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "und die Herren Staatssekat\u00e4re im Frack, im Bristoll und Theata pareh \u2013", "tokens": ["und", "die", "Her\u00b7ren", "Staats\u00b7se\u00b7ka\u00b7t\u00e4\u00b7re", "im", "Frack", ",", "im", "Bris\u00b7toll", "und", "Thea\u00b7ta", "pa\u00b7reh", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "APPRART", "NN", "$,", "APPRART", "NN", "KON", "XY", "XY", "$("], "meter": "--+-+--+--+-+--+--+", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "wie sie doch so gern m\u00f6chten mongd\u00e4n sein mit ihre Damens", "tokens": ["wie", "sie", "doch", "so", "gern", "m\u00f6ch\u00b7ten", "mong\u00b7d\u00e4n", "sein", "mit", "ih\u00b7re", "Da\u00b7mens"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "ADV", "ADV", "VMFIN", "ADJD", "VAINF", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "und vor jeden fremden Dippelmaten zusammenknicken,", "tokens": ["und", "vor", "je\u00b7den", "frem\u00b7den", "Dip\u00b7pel\u00b7ma\u00b7ten", "zu\u00b7sam\u00b7men\u00b7kni\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+--+-+-", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "wegen des Namens;", "tokens": ["we\u00b7gen", "des", "Na\u00b7mens", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "wie sie Fettlebe machen bei Rechtsanw\u00e4lte und bei Bankiers,", "tokens": ["wie", "sie", "Fett\u00b7le\u00b7be", "ma\u00b7chen", "bei", "Rechts\u00b7an\u00b7w\u00e4l\u00b7te", "und", "bei", "Ban\u00b7kiers", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "VVINF", "APPR", "NN", "KON", "APPR", "NE", "$,"], "meter": "--+--+-++-+-+++-", "measure": "anapaest.di.plus"}, "line.7": {"text": "wie sie so rumscharwenzeln auf die feinsten Modetees \u2013", "tokens": ["wie", "sie", "so", "rum\u00b7schar\u00b7wen\u00b7zeln", "auf", "die", "feins\u00b7ten", "Mo\u00b7de\u00b7tees", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVFIN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.8": {"text": "selig, wenn einer hinter sie herzischelt: \u00bbDi\u00df is Ge\u00dfler! Das ist Koch!\u00ab", "tokens": ["se\u00b7lig", ",", "wenn", "ei\u00b7ner", "hin\u00b7ter", "sie", "her\u00b7zi\u00b7schelt", ":", "\u00bb", "Di\u00df", "is", "Ge\u00df\u00b7ler", "!", "Das", "ist", "Koch", "!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "$,", "KOUS", "PIS", "APPR", "PPER", "VVPP", "$.", "$(", "PDS", "FM", "NE", "$.", "PDS", "VAFIN", "NE", "$.", "$("], "meter": "-+-+-+--+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.9": {"text": "wie manche fressen und spinkulieren noch und noch \u2013", "tokens": ["wie", "man\u00b7che", "fres\u00b7sen", "und", "spin\u00b7ku\u00b7lie\u00b7ren", "noch", "und", "noch", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVINF", "KON", "VVFIN", "ADV", "KON", "ADV", "$("], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "wenn ich das so sehe und denke: Die bilden sich nu ein,", "tokens": ["wenn", "ich", "das", "so", "se\u00b7he", "und", "den\u00b7ke", ":", "Die", "bil\u00b7den", "sich", "nu", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "ADV", "VVFIN", "KON", "VVFIN", "$.", "ART", "VVFIN", "PRF", "ADV", "PTKVZ", "$,"], "meter": "+-+-+--+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.11": {"text": "gro\u00dfe, internationale, politische Welt zu sein . . .", "tokens": ["gro\u00b7\u00dfe", ",", "in\u00b7ter\u00b7na\u00b7ti\u00b7o\u00b7na\u00b7le", ",", "po\u00b7li\u00b7ti\u00b7sche", "Welt", "zu", "sein", ".", ".", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADJA", "$,", "ADV", "$,", "ADJA", "NN", "PTKZU", "VAINF", "$.", "$.", "$."], "meter": "+-+-+--+-+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.12": {"text": "Dann sag ich bei mir: Die \u2013? Ach, du lieber Gott . . .", "tokens": ["Dann", "sag", "ich", "bei", "mir", ":", "Die", "\u2013", "?", "Ach", ",", "du", "lie\u00b7ber", "Gott", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "punct", "word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPER", "$.", "ART", "$(", "$.", "ITJ", "$,", "PPER", "ADV", "NN", "$.", "$.", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.13": {"text": "Lauter saure Jurken. Und kein Kompott.", "tokens": ["Lau\u00b7ter", "sau\u00b7re", "Jur\u00b7ken", ".", "Und", "kein", "Kom\u00b7pott", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "$.", "KON", "PIAT", "NN", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}}}}}