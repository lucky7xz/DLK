{"textgrid.poem.36409": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[6.]", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00e4ndlich, geehrter Baron, hast du Macbeth den hohen verschimpfet;", "tokens": ["Sch\u00e4nd\u00b7lich", ",", "ge\u00b7ehr\u00b7ter", "Ba\u00b7ron", ",", "hast", "du", "Mac\u00b7be\u00b7th", "den", "ho\u00b7hen", "ver\u00b7schimp\u00b7fet", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJA", "NN", "$,", "VAFIN", "PPER", "NE", "ART", "ADJA", "VVFIN", "$."], "meter": "+--+-+-+-+-+-+--+-", "measure": "iambic.octa.plus.invert"}, "line.2": {"text": "Doch was geschehn, ist geschehn, bleibe nur k\u00fcnftig davon.", "tokens": ["Doch", "was", "ge\u00b7schehn", ",", "ist", "ge\u00b7schehn", ",", "blei\u00b7be", "nur", "k\u00fcnf\u00b7tig", "da\u00b7von", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVPP", "$,", "VAFIN", "VVPP", "$,", "VVFIN", "ADV", "ADJD", "PAV", "$."], "meter": "-+-+--+---+--+", "measure": "iambic.penta.relaxed"}}}}}