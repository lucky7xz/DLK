{"dta.poem.12652": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Als sie wolte die sterne kennen  \n lernen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was reitzet vor ein stern zu dieser lust dich an,", "tokens": ["Was", "reit\u00b7zet", "vor", "ein", "stern", "zu", "die\u00b7ser", "lust", "dich", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "ART", "ADJA", "APPR", "PDAT", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Galante Margaris! wilstu die erde lassen,", "tokens": ["Ga\u00b7lan\u00b7te", "Mar\u00b7ga\u00b7ris", "!", "wils\u00b7tu", "die", "er\u00b7de", "las\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und jenes stern-revier in deine sinnen fassen?", "tokens": ["Und", "je\u00b7nes", "stern\u00b7re\u00b7vier", "in", "dei\u00b7ne", "sin\u00b7nen", "fas\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Was vor ein zug f\u00fchrt dich auf diese hohe bahn,", "tokens": ["Was", "vor", "ein", "zug", "f\u00fchrt", "dich", "auf", "die\u00b7se", "ho\u00b7he", "bahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "VVFIN", "PPER", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+---+-+", "measure": "unknown.measure.penta"}, "line.5": {"text": "Wo nichts als feuer ist, wo solche lichter brennen,", "tokens": ["Wo", "nichts", "als", "feu\u00b7er", "ist", ",", "wo", "sol\u00b7che", "lich\u00b7ter", "bren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "KOKOM", "ADJD", "VAFIN", "$,", "PWAV", "PIS", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die leicht durch einen fall die erde k\u00f6nten trennen?", "tokens": ["Die", "leicht", "durch", "ei\u00b7nen", "fall", "die", "er\u00b7de", "k\u00f6n\u00b7ten", "tren\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "ART", "NN", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Weil denn dein himmel-geist nicht zu erhalten ist,", "tokens": ["Weil", "denn", "dein", "him\u00b7mel\u00b7geist", "nicht", "zu", "er\u00b7hal\u00b7ten", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "PTKNEG", "PTKZU", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und an das sonnen-licht gedencket hinzuetlen,", "tokens": ["Und", "an", "das", "son\u00b7nen\u00b7licht", "ge\u00b7den\u00b7cket", "hin\u00b7zu\u00b7et\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Weil er sich itzund will mit erd und himmel theilen,", "tokens": ["Weil", "er", "sich", "it\u00b7zund", "will", "mit", "erd", "und", "him\u00b7mel", "thei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VMFIN", "APPR", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den himmel bey der nacht, die erd am tag erkiest;", "tokens": ["Den", "him\u00b7mel", "bey", "der", "nacht", ",", "die", "erd", "am", "tag", "er\u00b7kiest", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So f\u00e4hrt er gl\u00fccklich auf zu den gestirnten h\u00f6hen,", "tokens": ["So", "f\u00e4hrt", "er", "gl\u00fcck\u00b7lich", "auf", "zu", "den", "ge\u00b7stirn\u00b7ten", "h\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "APPR", "ART", "ADJA", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wo neue lichter stets in grossen circkeln stehen.", "tokens": ["Wo", "neu\u00b7e", "lich\u00b7ter", "stets", "in", "gros\u00b7sen", "cir\u00b7ckeln", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die sonne kennst du schon, des tages gro\u00dfes licht,", "tokens": ["Die", "son\u00b7ne", "kennst", "du", "schon", ",", "des", "ta\u00b7ges", "gro\u00b7\u00dfes", "licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$,", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wo du sie nicht kennst, so schau in deinen spiegel;", "tokens": ["Und", "wo", "du", "sie", "nicht", "kennst", ",", "so", "schau", "in", "dei\u00b7nen", "spie\u00b7gel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "PPER", "PTKNEG", "VVFIN", "$,", "ADV", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Was gilts? der l\u00f6st dir auf hier dieser reime siegel,", "tokens": ["Was", "gilts", "?", "der", "l\u00f6st", "dir", "auf", "hier", "die\u00b7ser", "rei\u00b7me", "sie\u00b7gel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$.", "ART", "VVFIN", "PPER", "APPR", "ADV", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.4": {"text": "Und sagt dir, da\u00df di\u00df wort sey selbst auf dich gericht:", "tokens": ["Und", "sagt", "dir", ",", "da\u00df", "di\u00df", "wort", "sey", "selbst", "auf", "dich", "ge\u00b7richt", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "KOUS", "PDS", "NN", "VAFIN", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Du bist die sonne selbst, und kanst mit deinem strahlen", "tokens": ["Du", "bist", "die", "son\u00b7ne", "selbst", ",", "und", "kanst", "mit", "dei\u00b7nem", "strah\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "$,", "KON", "VMFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Auch gantze gegenden zu aller freude mahlen.", "tokens": ["Auch", "gant\u00b7ze", "ge\u00b7gen\u00b7den", "zu", "al\u00b7ler", "freu\u00b7de", "mah\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Du brennst so starck als sie, dein heller augen-strahl", "tokens": ["Du", "brennst", "so", "starck", "als", "sie", ",", "dein", "hel\u00b7ler", "au\u00b7gen\u00b7strahl"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "KOKOM", "PPER", "$,", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kan seine flammen selbst in vieler hertzen treiben;", "tokens": ["Kan", "sei\u00b7ne", "flam\u00b7men", "selbst", "in", "vie\u00b7ler", "hert\u00b7zen", "trei\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "VVFIN", "ADV", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich selbsten will, und mu\u00df in diese zahl mich schreiben,", "tokens": ["Ich", "selbs\u00b7ten", "will", ",", "und", "mu\u00df", "in", "die\u00b7se", "zahl", "mich", "schrei\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "VMFIN", "$,", "KON", "VMFIN", "APPR", "PDAT", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der ich doch gegen dir ein klarheit-armes thal.", "tokens": ["Der", "ich", "doch", "ge\u00b7gen", "dir", "ein", "kla\u00b7rheit\u00b7ar\u00b7mes", "thal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und welcher lange dir zu nahe wolte stehen,", "tokens": ["Und", "wel\u00b7cher", "lan\u00b7ge", "dir", "zu", "na\u00b7he", "wol\u00b7te", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAT", "ADJA", "PPER", "PTKA", "ADJD", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der w\u00fcrde sicherlich vor hitz und licht vergehen.", "tokens": ["Der", "w\u00fcr\u00b7de", "si\u00b7cher\u00b7lich", "vor", "hitz", "und", "licht", "ver\u00b7ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PRF", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Der sonne folgt der mond an macht und w\u00fcrckung nach,", "tokens": ["Der", "son\u00b7ne", "folgt", "der", "mond", "an", "macht", "und", "w\u00fcr\u00b7ckung", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "APPR", "VVFIN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sein silber kennstu auch, und da\u00df er h\u00f6rner tr\u00e4get,", "tokens": ["Sein", "sil\u00b7ber", "kenns\u00b7tu", "auch", ",", "und", "da\u00df", "er", "h\u00f6r\u00b7ner", "tr\u00e4\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "$,", "KON", "KOUS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die zu gewisser zeit er zu verstecken pfleget,", "tokens": ["Die", "zu", "ge\u00b7wis\u00b7ser", "zeit", "er", "zu", "ver\u00b7ste\u00b7cken", "pfle\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKZU", "ADJA", "NN", "PPER", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bald wieder sehen l\u00e4st, wenn er sich allgemach", "tokens": ["Bald", "wie\u00b7der", "se\u00b7hen", "l\u00e4st", ",", "wenn", "er", "sich", "all\u00b7ge\u00b7mach"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVINF", "VVFIN", "$,", "KOUS", "PPER", "PRF", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der sonnen n\u00e4her weltzt, doch wirstu di\u00df schon wissen,", "tokens": ["Der", "son\u00b7nen", "n\u00e4\u00b7her", "weltzt", ",", "doch", "wirs\u00b7tu", "di\u00df", "schon", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "VVPP", "$,", "ADV", "VAFIN", "PDS", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Warum es offters mu\u00df das licht der sonnen missen.", "tokens": ["Wa\u00b7rum", "es", "off\u00b7ters", "mu\u00df", "das", "licht", "der", "son\u00b7nen", "mis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VMFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Nach diesem schau mit mir die irre-lichter an,", "tokens": ["Nach", "die\u00b7sem", "schau", "mit", "mir", "die", "ir\u00b7re\u00b7lich\u00b7ter", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "APPR", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die man planeten heist: Der Jupiter steht oben,", "tokens": ["Die", "man", "pla\u00b7ne\u00b7ten", "heist", ":", "Der", "Ju\u00b7pi\u00b7ter", "steht", "o\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "ADJD", "$.", "ART", "NN", "VVFIN", "ADV", "$,"], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Der w\u00fcrde, meyn ich, nach, weil nach der heyden loben", "tokens": ["Der", "w\u00fcr\u00b7de", ",", "meyn", "ich", ",", "nach", ",", "weil", "nach", "der", "hey\u00b7den", "lo\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "$,", "PPOSAT", "PPER", "$,", "APPR", "$,", "KOUS", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Jhm cron und scepter solgt; und was man weiter kan", "tokens": ["Jhm", "cron", "und", "scep\u00b7ter", "solgt", ";", "und", "was", "man", "wei\u00b7ter", "kan"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "NN", "KON", "ADJD", "VVPP", "$.", "KON", "PWS", "PIS", "ADV", "VMFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "An ihm vor wunder sehn, das lehren dich die tichter,", "tokens": ["An", "ihm", "vor", "wun\u00b7der", "sehn", ",", "das", "leh\u00b7ren", "dich", "die", "tich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "NN", "VVINF", "$,", "PDS", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Als in dem fabel-werck die allerbesten richter.", "tokens": ["Als", "in", "dem", "fa\u00b7bel\u00b7\u00b7werck", "die", "al\u00b7ler\u00b7bes\u00b7ten", "rich\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Wenn nun der himmel wird in deinem kopffe stehn,", "tokens": ["Wenn", "nun", "der", "him\u00b7mel", "wird", "in", "dei\u00b7nem", "kopf\u00b7fe", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VAFIN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn du die sterne wirst darein gefasset haben,", "tokens": ["Wenn", "du", "die", "ster\u00b7ne", "wirst", "da\u00b7rein", "ge\u00b7fas\u00b7set", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "VAFIN", "PAV", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und dessen circkel dem gehirue eingegraben,", "tokens": ["Und", "des\u00b7sen", "cir\u00b7ckel", "dem", "ge\u00b7hi\u00b7rue", "ein\u00b7ge\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRELAT", "NN", "ART", "ADJA", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So lasse deinen sinn sich nicht zu sehr erh\u00f6hn,", "tokens": ["So", "las\u00b7se", "dei\u00b7nen", "sinn", "sich", "nicht", "zu", "sehr", "er\u00b7h\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PRF", "PTKNEG", "PTKA", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Da\u00df er, was irrdisch ist, nicht alles bald verlachet,", "tokens": ["Da\u00df", "er", ",", "was", "irr\u00b7disch", "ist", ",", "nicht", "al\u00b7les", "bald", "ver\u00b7la\u00b7chet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PRELS", "ADJD", "VAFIN", "$,", "PTKNEG", "PIS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und also auch dadurch mich ungl\u00fcckseelig machet.", "tokens": ["Und", "al\u00b7so", "auch", "da\u00b7durch", "mich", "un\u00b7gl\u00fcck\u00b7see\u00b7lig", "ma\u00b7chet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "PAV", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Jhr sterne, fl\u00f6sset ihr ja andre sinnen ein!", "tokens": ["Ihr", "ster\u00b7ne", ",", "fl\u00f6s\u00b7set", "ihr", "ja", "and\u00b7re", "sin\u00b7nen", "ein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "PPER", "ADV", "PIS", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Saturnus m\u00fcsse ja nicht in ihr hertze kommen;", "tokens": ["Sa\u00b7tur\u00b7nus", "m\u00fcs\u00b7se", "ja", "nicht", "in", "ihr", "hert\u00b7ze", "kom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "ADV", "PTKNEG", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wo aber di\u00df vorher die Venus eingenommen,", "tokens": ["Wo", "a\u00b7ber", "di\u00