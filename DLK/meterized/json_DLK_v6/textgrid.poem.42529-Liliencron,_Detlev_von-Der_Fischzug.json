{"textgrid.poem.42529": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "Der Fischzug", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du h\u00f6rst der Schmetterlinge Fl\u00fcgelschlagen,", "tokens": ["Du", "h\u00f6rst", "der", "Schmet\u00b7ter\u00b7lin\u00b7ge", "Fl\u00fc\u00b7gel\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So still ruht Baum und Blatt im gro\u00dfen Parke.", "tokens": ["So", "still", "ruht", "Baum", "und", "Blatt", "im", "gro\u00b7\u00dfen", "Par\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "NE", "KON", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Auf fernen Steigen schurft des G\u00e4rtners Harke,", "tokens": ["Auf", "fer\u00b7nen", "Stei\u00b7gen", "schurft", "des", "G\u00e4rt\u00b7ners", "Har\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Spatz putzt auf der Sonnuhr sich den Kragen.", "tokens": ["Der", "Spatz", "putzt", "auf", "der", "Son\u00b7nuhr", "sich", "den", "Kra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Bewegung. Menschen. Und ein Fangnetz tragen", "tokens": ["Be\u00b7we\u00b7gung", ".", "Men\u00b7schen", ".", "Und", "ein", "Fang\u00b7netz", "tra\u00b7gen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "NN", "$.", "KON", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zum Teich hin Fischerarme, muskelstarke.", "tokens": ["Zum", "Teich", "hin", "Fi\u00b7scher\u00b7ar\u00b7me", ",", "mus\u00b7kels\u00b7tar\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Vom Pfahle l\u00f6sen sie die wei\u00dfe Barke;", "tokens": ["Vom", "Pfah\u00b7le", "l\u00f6\u00b7sen", "sie", "die", "wei\u00b7\u00dfe", "Bar\u00b7ke", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Zug beginnt, ganz wie zu Petri Tagen.", "tokens": ["Der", "Zug", "be\u00b7ginnt", ",", "ganz", "wie", "zu", "Pe\u00b7tri", "Ta\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ADV", "KOKOM", "APPR", "NE", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Indessen ist die F\u00fcrstin angekommen,", "tokens": ["In\u00b7des\u00b7sen", "ist", "die", "F\u00fcrs\u00b7tin", "an\u00b7ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hat in der Marmornische Platz genommen,", "tokens": ["Hat", "in", "der", "Mar\u00b7mor\u00b7ni\u00b7sche", "Platz", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Page kniet und legt die Schleppe nieder.", "tokens": ["Der", "Pa\u00b7ge", "kniet", "und", "legt", "die", "Schlep\u00b7pe", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Im Netze zappeln Karpfen und Karauschen.", "tokens": ["Im", "Net\u00b7ze", "zap\u00b7peln", "Karp\u00b7fen", "und", "Ka\u00b7rau\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Hoheit lacht; die Kavaliere lauschen.", "tokens": ["Die", "Ho\u00b7heit", "lacht", ";", "die", "Ka\u00b7va\u00b7lie\u00b7re", "lau\u00b7schen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Spa\u00df ist aus \u2013 und tiefe Ruhe wieder.", "tokens": ["Der", "Spa\u00df", "ist", "aus", "\u2013", "und", "tie\u00b7fe", "Ru\u00b7he", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "$(", "KON", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}