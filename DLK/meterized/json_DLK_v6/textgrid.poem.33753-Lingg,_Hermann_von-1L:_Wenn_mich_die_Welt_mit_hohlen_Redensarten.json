{"textgrid.poem.33753": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn mich die Welt mit hohlen Redensarten", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn mich die Welt mit hohlen Redensarten", "tokens": ["Wenn", "mich", "die", "Welt", "mit", "hoh\u00b7len", "Re\u00b7dens\u00b7ar\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Schon fast erstickt hat und es mir so bitter", "tokens": ["Schon", "fast", "er\u00b7stickt", "hat", "und", "es", "mir", "so", "bit\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVPP", "VAFIN", "KON", "PPER", "PPER", "ADV", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und elend wird, dann als mein Samariter", "tokens": ["Und", "e\u00b7lend", "wird", ",", "dann", "als", "mein", "Sa\u00b7ma\u00b7ri\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VAFIN", "$,", "ADV", "KOKOM", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sprichst du zu mir mit Worten, wunderzarten.", "tokens": ["Sprichst", "du", "zu", "mir", "mit", "Wor\u00b7ten", ",", "wun\u00b7derz\u00b7ar\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "APPR", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Mit Worten, die den s\u00fc\u00dfen Duft bewahrten,", "tokens": ["Mit", "Wor\u00b7ten", ",", "die", "den", "s\u00fc\u00b7\u00dfen", "Duft", "be\u00b7wahr\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den nur ein edles Herz hegt, keine Flitter,", "tokens": ["Den", "nur", "ein", "ed\u00b7les", "Herz", "hegt", ",", "kei\u00b7ne", "Flit\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Kein Falsch \u2013 die auch verstehen kann kein Dritter,", "tokens": ["Kein", "Falsch", "\u2013", "die", "auch", "ver\u00b7ste\u00b7hen", "kann", "kein", "Drit\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$(", "PRELS", "ADV", "VVINF", "VMFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Obwohl sie nichts als Wahrheit offenbarten.", "tokens": ["Ob\u00b7wohl", "sie", "nichts", "als", "Wahr\u00b7heit", "of\u00b7fen\u00b7bar\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "KOKOM", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Einst, wenn mich alle l\u00e4ngst vergessen haben,", "tokens": ["Einst", ",", "wenn", "mich", "al\u00b7le", "l\u00e4ngst", "ver\u00b7ges\u00b7sen", "ha\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "PIS", "ADV", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dann kommst doch du und legst mir Lorbeerzweige", "tokens": ["Dann", "kommst", "doch", "du", "und", "legst", "mir", "Lor\u00b7beer\u00b7zwei\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "KON", "VVFIN", "PPER", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Aufs Kreuz hin, unter dem ich bin begraben.", "tokens": ["Aufs", "Kreuz", "hin", ",", "un\u00b7ter", "dem", "ich", "bin", "be\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$,", "APPR", "PRELS", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Vergib, da\u00df deinen Preis ich nicht verschweige:", "tokens": ["Ver\u00b7gib", ",", "da\u00df", "dei\u00b7nen", "Preis", "ich", "nicht", "ver\u00b7schwei\u00b7ge", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "KOUS", "PPOSAT", "NN", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was tief ist, das allein ist auch erhaben;", "tokens": ["Was", "tief", "ist", ",", "das", "al\u00b7lein", "ist", "auch", "er\u00b7ha\u00b7ben", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "$,", "PRELS", "ADV", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was stolz zur\u00fcckh\u00e4lt, wert, da\u00df sich es zeige!", "tokens": ["Was", "stolz", "zu\u00b7r\u00fcck\u00b7h\u00e4lt", ",", "wert", ",", "da\u00df", "sich", "es", "zei\u00b7ge", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VVFIN", "$,", "ADJD", "$,", "KOUS", "PRF", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}