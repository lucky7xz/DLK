{"textgrid.poem.67699": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "12.", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr, Seligkeit und Himmel liegt", "tokens": ["Herr", ",", "Se\u00b7lig\u00b7keit", "und", "Him\u00b7mel", "liegt"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In jeder Deiner Gaben;", "tokens": ["In", "je\u00b7der", "Dei\u00b7ner", "Ga\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wer neidet und verscharret sie,", "tokens": ["Wer", "nei\u00b7det", "und", "ver\u00b7schar\u00b7ret", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "KON", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verdient er sie zu haben?", "tokens": ["Ver\u00b7di\u00b7ent", "er", "sie", "zu", "ha\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PTKZU", "VAINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Und wer sie hat, was hat er schon,", "tokens": ["Und", "wer", "sie", "hat", ",", "was", "hat", "er", "schon", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VAFIN", "$,", "PWS", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df er sich ewig freue!", "tokens": ["Da\u00df", "er", "sich", "e\u00b7wig", "freu\u00b7e", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "O Geber, gieb statt Allem mir", "tokens": ["O", "Ge\u00b7ber", ",", "gieb", "statt", "Al\u00b7lem", "mir"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "VVIMP", "APPR", "PIS", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nur im Geringsten Treue!", "tokens": ["Nur", "im", "Ge\u00b7rings\u00b7ten", "Treu\u00b7e", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}