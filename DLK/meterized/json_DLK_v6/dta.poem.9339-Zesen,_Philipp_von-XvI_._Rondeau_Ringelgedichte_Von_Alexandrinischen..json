{"dta.poem.9339": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "XvI .  \n Rondeau/ Ringelgedichte/  \n Von Alexandrinischen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ade zu guter nacht/ mein Sin\u0303 ist abzuscheiden/", "tokens": ["A\u00b7de", "zu", "gu\u00b7ter", "nacht", "/", "mein", "Si\u00f1", "ist", "ab\u00b7zu\u00b7schei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$(", "PPOSAT", "NN", "VAFIN", "VVIZU", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Ich mu\u00df dich edles Land mit widerwillen meiden/", "tokens": ["Ich", "mu\u00df", "dich", "ed\u00b7les", "Land", "mit", "wi\u00b7der\u00b7wil\u00b7len", "mei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADJA", "NN", "APPR", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "die Muld\u2019 ist selbst betr\u00fcbt ich bin auf nichts bedacht/", "tokens": ["die", "Muld'", "ist", "selbst", "be\u00b7tr\u00fcbt", "ich", "bin", "auf", "nichts", "be\u00b7dacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVFIN", "PPER", "VAFIN", "APPR", "PIS", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als blo\u00df auf diese wort/ ", "tokens": ["Als", "blo\u00df", "auf", "die\u00b7se", "wort", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "PDAT", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dein Briider/ trauter freund/ dein lieber und getreuer/", "tokens": ["Dein", "Bri\u00b7i\u00b7der", "/", "trau\u00b7ter", "freund", "/", "dein", "lie\u00b7ber", "und", "ge\u00b7treu\u00b7er", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "ADJA", "NN", "$(", "PPOSAT", "ADV", "KON", "ADJD", "$("], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "wie du ja nennest mich/ f\u00fchlt deiner Liebe-feuer/", "tokens": ["wie", "du", "ja", "nen\u00b7nest", "mich", "/", "f\u00fchlt", "dei\u00b7ner", "Lie\u00b7be\u00b7feu\u00b7er", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVFIN", "PPER", "$(", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "und nim\u0303t die feste treu und Freundes pflicht in acht/", "tokens": ["und", "nim\u0303t", "die", "fes\u00b7te", "treu", "und", "Freun\u00b7des", "pflicht", "in", "acht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "ADJD", "KON", "NN", "VVFIN", "APPR", "CARD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "und sagt zu letzt wie vor/ ", "tokens": ["und", "sagt", "zu", "letzt", "wie", "vor", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADV", "KOKOM", "APPR", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}