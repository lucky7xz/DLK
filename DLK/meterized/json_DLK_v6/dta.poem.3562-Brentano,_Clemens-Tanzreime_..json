{"dta.poem.3562": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Tanzreime .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Silberner Degen,               ", "tokens": ["Sil\u00b7ber\u00b7ner", "De\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Ein goldener Knopf;", "tokens": ["Ein", "gol\u00b7de\u00b7ner", "Knopf", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Die M\u00e4dle sind traurig,", "tokens": ["Die", "M\u00e4d\u00b7le", "sind", "trau\u00b7rig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Franzosen sind fort!", "tokens": ["Fran\u00b7zo\u00b7sen", "sind", "fort", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}