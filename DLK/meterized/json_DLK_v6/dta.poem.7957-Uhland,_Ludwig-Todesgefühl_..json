{"dta.poem.7957": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Todesgef\u00fchl .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Wie Sterbenden zu Muth, wer mag es sagen?", "tokens": ["Wie", "Ster\u00b7ben\u00b7den", "zu", "Muth", ",", "wer", "mag", "es", "sa\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "NN", "$,", "PWS", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch wunderbar ergriff mich\u2019s diese Nacht;", "tokens": ["Doch", "wun\u00b7der\u00b7bar", "er\u00b7griff", "mich's", "die\u00b7se", "Nacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PIS", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Glieder schienen schon in Todes Macht,", "tokens": ["Die", "Glie\u00b7der", "schie\u00b7nen", "schon", "in", "To\u00b7des", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Im Herzen f\u00fchlt\u2019 ich letztes Leben schlagen.", "tokens": ["Im", "Her\u00b7zen", "f\u00fchlt'", "ich", "letz\u00b7tes", "Le\u00b7ben", "schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Den Geist befiel ein ungewohntes Zagen,", "tokens": ["Den", "Geist", "be\u00b7fiel", "ein", "un\u00b7ge\u00b7wohn\u00b7tes", "Za\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Geist, der stets so sicher sich gedacht;", "tokens": ["Den", "Geist", ",", "der", "stets", "so", "si\u00b7cher", "sich", "ge\u00b7dacht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ADV", "ADJD", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Erl\u00f6schend jetzt, dann wieder angefacht,", "tokens": ["Er\u00b7l\u00f6\u00b7schend", "jetzt", ",", "dann", "wie\u00b7der", "an\u00b7ge\u00b7facht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "ADV", "$,", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein mattes Fl\u00e4mmchen, das die Winde jagen.", "tokens": ["Ein", "mat\u00b7tes", "Fl\u00e4mm\u00b7chen", ",", "das", "die", "Win\u00b7de", "ja\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wie? hielten schwere Tr\u00e4ume mich befangen?", "tokens": ["Wie", "?", "hiel\u00b7ten", "schwe\u00b7re", "Tr\u00e4u\u00b7me", "mich", "be\u00b7fan\u00b7gen", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "VVFIN", "ADJA", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Lerche singt, der rothe Morgen gl\u00fcht,", "tokens": ["Die", "Ler\u00b7che", "singt", ",", "der", "ro\u00b7the", "Mor\u00b7gen", "gl\u00fcht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In\u2019s rege Leben treibt mich neu Verlangen.", "tokens": ["In's", "re\u00b7ge", "Le\u00b7ben", "treibt", "mich", "neu", "Ver\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PPER", "ADJD", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.4": {"line.1": {"text": "Wie? oder gieng vorbei der Todesengel?", "tokens": ["Wie", "?", "o\u00b7der", "gieng", "vor\u00b7bei", "der", "To\u00b7de\u00b7sen\u00b7gel", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "KON", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Blumen, die am Abend frisch gebl\u00fcht,", "tokens": ["Die", "Blu\u00b7men", ",", "die", "am", "A\u00b7bend", "frisch", "ge\u00b7bl\u00fcht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie h\u00e4ngen hingewelket dort vom Stengel.", "tokens": ["Sie", "h\u00e4n\u00b7gen", "hin\u00b7ge\u00b7wel\u00b7ket", "dort", "vom", "Sten\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}