{"dta.poem.8470": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Mailied.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1783", "urn": "urn:nbn:de:kobv:b4-200905192568", "language": ["de:0.99"], "booktitle": "H\u00f6lty, Ludwig Christoph Heinrich: Gedichte. Hamburg, 1783."}, "poem": {"stanza.1": {"line.1": {"text": "Hold und br\u00e4utlich, die Natur;", "tokens": ["Hold", "und", "br\u00e4ut\u00b7lich", ",", "die", "Na\u00b7tur", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "$,", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Blumen wehn, vom West gef\u00e4chelt,", "tokens": ["Blu\u00b7men", "wehn", ",", "vom", "West", "ge\u00b7f\u00e4\u00b7chelt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gelb und roth, auf gr\u00fcner Flur;", "tokens": ["Gelb", "und", "roth", ",", "auf", "gr\u00fc\u00b7ner", "Flur", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$,", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Um die kleinen Nester h\u00fcpfend,", "tokens": ["Um", "die", "klei\u00b7nen", "Nes\u00b7ter", "h\u00fcp\u00b7fend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Singt der V\u00f6gel Chor im Hain;", "tokens": ["Singt", "der", "V\u00f6\u00b7gel", "Chor", "im", "Hain", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und der kalten Tief' entschl\u00fcpfend,", "tokens": ["Und", "der", "kal\u00b7ten", "Tie\u00b7f'", "ent\u00b7schl\u00fcp\u00b7fend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Spielt der Fisch im Sonnenschein.", "tokens": ["Spielt", "der", "Fisch", "im", "Son\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Blau und golden schwebt der Aether", "tokens": ["Blau", "und", "gol\u00b7den", "schwebt", "der", "A\u00b7e\u00b7ther"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "ADJD", "VVFIN", "ART", "NN"], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Im beb\u00fcschten Gartenteich;", "tokens": ["Im", "be\u00b7b\u00fcschten", "Gar\u00b7ten\u00b7teich", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "B\u00e4ume, weisser hier, dort r\u00f6ther,", "tokens": ["B\u00e4u\u00b7me", ",", "weis\u00b7ser", "hier", ",", "dort", "r\u00f6\u00b7ther", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADV", "$,", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Spiegeln ihren Bl\u00fctenzweig.", "tokens": ["Spie\u00b7geln", "ih\u00b7ren", "Bl\u00fc\u00b7ten\u00b7zweig", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Durch die Bl\u00fcten, durch das gr\u00fcne", "tokens": ["Durch", "die", "Bl\u00fc\u00b7ten", ",", "durch", "das", "gr\u00fc\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Blumenthal, vom Sonnenschein", "tokens": ["Blu\u00b7ment\u00b7hal", ",", "vom", "Son\u00b7nen\u00b7schein"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ueberstralet, summt die Biene,", "tokens": ["Ue\u00b7ber\u00b7stra\u00b7let", ",", "summt", "die", "Bie\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sammelt s\u00fcssen Nektar ein.", "tokens": ["Sam\u00b7melt", "s\u00fcs\u00b7sen", "Nek\u00b7tar", "ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}