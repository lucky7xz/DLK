{"textgrid.poem.58198": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "236. Was Gottes Wille sei", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fragst du, was Gottes Wille sei,", "tokens": ["Fragst", "du", ",", "was", "Got\u00b7tes", "Wil\u00b7le", "sei", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PRELS", "NN", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So sieh nicht um nach mancherlei!", "tokens": ["So", "sieh", "nicht", "um", "nach", "man\u00b7cher\u00b7lei", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PTKNEG", "KOUI", "APPR", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er ist nur eins in allen.", "tokens": ["Er", "ist", "nur", "eins", "in", "al\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PIS", "APPR", "PIAT", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Halt deinen Grund in Gott nur still!", "tokens": ["Halt", "dei\u00b7nen", "Grund", "in", "Gott", "nur", "still", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "APPR", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wer gar nichts aus sich selber will,", "tokens": ["Wer", "gar", "nichts", "aus", "sich", "sel\u00b7ber", "will", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PIS", "APPR", "PRF", "ADV", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Kann Gott gar leicht gefallen.", "tokens": ["Kann", "Gott", "gar", "leicht", "ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}