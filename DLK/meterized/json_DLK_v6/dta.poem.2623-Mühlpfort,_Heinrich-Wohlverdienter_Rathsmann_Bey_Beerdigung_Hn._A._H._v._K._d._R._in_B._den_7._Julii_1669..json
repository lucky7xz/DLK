{"dta.poem.2623": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Wohlverdienter Rathsmann/  \n Bey Beerdigung Hn. A. H. v. K. d. R. in B.  \n den 7. Julii 1669.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die allerschwerste Kunstist wei\u00dflich zu regieren/", "tokens": ["Die", "al\u00b7ler\u00b7schwers\u00b7te", "Kuns\u00b7tist", "wei\u00df\u00b7lich", "zu", "re\u00b7gie\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es darff kein niedrig Sinn und feige Seele seyn/", "tokens": ["Es", "darff", "kein", "nied\u00b7rig", "Sinn", "und", "fei\u00b7ge", "See\u00b7le", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIAT", "ADJD", "NN", "KON", "ADJA", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die \u00fcber Menschen soll Gewalt und Herrschafft f\u00fchren/", "tokens": ["Die", "\u00fc\u00b7ber", "Men\u00b7schen", "soll", "Ge\u00b7walt", "und", "Herr\u00b7schafft", "f\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "VMFIN", "NN", "KON", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil ihre Hertzen oft so hart als Kiesel-Stein:", "tokens": ["Weil", "ih\u00b7re", "Hert\u00b7zen", "oft", "so", "hart", "als", "Kie\u00b7sel\u00b7Stein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "ADV", "ADJD", "KOKOM", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der ungez\u00e4hmte Will/ ein Feind der guten Sitten/", "tokens": ["Der", "un\u00b7ge\u00b7z\u00e4hm\u00b7te", "Will", "/", "ein", "Feind", "der", "gu\u00b7ten", "Sit\u00b7ten", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die th\u00f6richte Vernunfft vor Frieden Unruh wehlt/", "tokens": ["Die", "th\u00f6\u00b7rich\u00b7te", "Ver\u00b7nunfft", "vor", "Frie\u00b7den", "Un\u00b7ruh", "wehlt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "NN", "VVFIN", "$("], "meter": "-+---+-+-+-+", "measure": "dactylic.init"}, "line.7": {"text": "Und der Gedancken Schlo\u00df vom Frevel wird bestritten/", "tokens": ["Und", "der", "Ge\u00b7dan\u00b7cken", "Schlo\u00df", "vom", "Fre\u00b7vel", "wird", "be\u00b7strit\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "APPRART", "NN", "VAFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der stets die Einigkeit mit der Emp\u00f6rung qu\u00e4lt.", "tokens": ["Der", "stets", "die", "Ei\u00b7nig\u00b7keit", "mit", "der", "Em\u00b7p\u00f6\u00b7rung", "qu\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die See ist nicht so wild/ und ihre stoltze Wellen/", "tokens": ["Die", "See", "ist", "nicht", "so", "wild", "/", "und", "ih\u00b7re", "stolt\u00b7ze", "Wel\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "ADV", "ADJD", "$(", "KON", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Als der gemeine Mann in seinem d\u00fcnckel-Witz/", "tokens": ["Als", "der", "ge\u00b7mei\u00b7ne", "Mann", "in", "sei\u00b7nem", "d\u00fcn\u00b7ckel\u00b7Witz", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NE", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und wo der P\u00f6fel will sein albers Urtheil f\u00e4llen/", "tokens": ["Und", "wo", "der", "P\u00f6\u00b7fel", "will", "sein", "al\u00b7bers", "Ur\u00b7theil", "f\u00e4l\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VMFIN", "PPOSAT", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Da folgt gemeiniglich des Ungel\u00fcckes Blitz/", "tokens": ["Da", "folgt", "ge\u00b7mei\u00b7nig\u00b7lich", "des", "Un\u00b7ge\u00b7l\u00fc\u00b7ckes", "Blitz", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Drumb mu\u00df ein hoher Geist von ungemeinen Gaben/", "tokens": ["Drumb", "mu\u00df", "ein", "ho\u00b7her", "Geist", "von", "un\u00b7ge\u00b7mei\u00b7nen", "Ga\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und scharffen Sinnen seyn/ dem man die Last vertraut/", "tokens": ["Und", "scharf\u00b7fen", "Sin\u00b7nen", "seyn", "/", "dem", "man", "die", "Last", "ver\u00b7traut", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "VAINF", "$(", "ART", "PIS", "ART", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Den Weisheit Lehr und Witz so ausgemustert haben/", "tokens": ["Den", "Weis\u00b7heit", "Lehr", "und", "Witz", "so", "aus\u00b7ge\u00b7mus\u00b7tert", "ha\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "ADV", "VVPP", "VAINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Da\u00df er/ dem Altas gleich/ das Wesen unterbaut:", "tokens": ["Da\u00df", "er", "/", "dem", "Al\u00b7tas", "gleich", "/", "das", "We\u00b7sen", "un\u00b7ter\u00b7baut", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$(", "ART", "NN", "ADV", "$(", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Bald mu\u00df Ulyssens Witz/ bald Carons Tapfferkeiten/", "tokens": ["Bald", "mu\u00df", "U\u00b7lys\u00b7sens", "Witz", "/", "bald", "Ca\u00b7rons", "Tapf\u00b7fer\u00b7kei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "NE", "NN", "$(", "ADV", "ADJA", "NN", "$("], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.18": {"text": "Bald Nestors g\u00fcldner Mund an statt der St\u00fctze seyn:", "tokens": ["Bald", "Nes\u00b7tors", "g\u00fcld\u00b7ner", "Mund", "an", "statt", "der", "St\u00fct\u00b7ze", "seyn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "ADJA", "NN", "APPR", "APPR", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Oft mu\u00df die Freundligkeit ein strenger Ernst begleiten;", "tokens": ["Oft", "mu\u00df", "die", "Freund\u00b7lig\u00b7keit", "ein", "stren\u00b7ger", "Ernst", "be\u00b7glei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Durch Schrecken mehret sich der Majest\u00e4ten Schein.", "tokens": ["Durch", "Schre\u00b7cken", "meh\u00b7ret", "sich", "der", "Ma\u00b7jes\u00b7t\u00e4\u00b7ten", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PRF", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Vergebens ist ja nicht der Purpur so erhoben/", "tokens": ["Ver\u00b7ge\u00b7bens", "ist", "ja", "nicht", "der", "Pur\u00b7pur", "so", "er\u00b7ho\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PTKNEG", "ART", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Die Krone so beperlt/ des Scepters Gold so schwer/", "tokens": ["Die", "Kro\u00b7ne", "so", "be\u00b7perlt", "/", "des", "Scep\u00b7ters", "Gold", "so", "schwer", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$(", "ART", "NN", "NN", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Als da\u00df sie Bilder sind der Allmacht von dort oben/", "tokens": ["Als", "da\u00df", "sie", "Bil\u00b7der", "sind", "der", "All\u00b7macht", "von", "dort", "o\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "NN", "VAFIN", "ART", "NN", "APPR", "ADV", "ADV", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Der tieff zu Fusse f\u00e4llt/ Lufft/ Erde/ Feuer/ Meer.", "tokens": ["Der", "tieff", "zu", "Fus\u00b7se", "f\u00e4llt", "/", "Lufft", "/", "Er\u00b7de", "/", "Feu\u00b7er", "/", "Meer", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "$(", "NN", "$(", "NN", "$(", "NN", "$(", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Kein Dorff ist ohne Haupt/ geschweige grosse Reiche/", "tokens": ["Kein", "Dorff", "ist", "oh\u00b7ne", "Haupt", "/", "ge\u00b7schwei\u00b7ge", "gros\u00b7se", "Rei\u00b7che", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "APPR", "NN", "$(", "ADJA", "ADJA", "NE", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Jedwedrer Stadt der sind Pfleg-V\u00e4ter f\u00fcr gesetzt;", "tokens": ["Jed\u00b7we\u00b7drer", "Stadt", "der", "sind", "Pfleg\u00b7V\u00e4\u00b7ter", "f\u00fcr", "ge\u00b7setzt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "VAFIN", "NN", "APPR", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Sonst w\u00fcrd\u2019 in einem Nun die Welt zu einer Leiche/", "tokens": ["Sonst", "w\u00fcrd'", "in", "ei\u00b7nem", "Nun", "die", "Welt", "zu", "ei\u00b7ner", "Lei\u00b7che", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "ART", "ADV", "ART", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und jeder Acker mit der Menschen Blut benetzt.", "tokens": ["Und", "je\u00b7der", "A\u00b7cker", "mit", "der", "Men\u00b7schen", "Blut", "be\u00b7netzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "APPR", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Wie seelig ist der Ort in dem Astr\u00e4a wohnet/", "tokens": ["Wie", "see\u00b7lig", "ist", "der", "Ort", "in", "dem", "A\u00b7str\u00e4a", "woh\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "NN", "APPR", "ART", "NE", "VVFIN", "$("], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.30": {"text": "Und zu der Obrigkeit geschickte R\u00e4the hat/", "tokens": ["Und", "zu", "der", "Ob\u00b7rig\u00b7keit", "ge\u00b7schick\u00b7te", "R\u00e4\u00b7the", "hat", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Wo man die Laster strafft/ und Tugenden belohnet/", "tokens": ["Wo", "man", "die", "Las\u00b7ter", "strafft", "/", "und", "Tu\u00b7gen\u00b7den", "be\u00b7loh\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ART", "NN", "VVFIN", "$(", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Wo die Gerechtigkeit f\u00fchrt ihre Hofe-Stadt;", "tokens": ["Wo", "die", "Ge\u00b7rech\u00b7tig\u00b7keit", "f\u00fchrt", "ih\u00b7re", "Ho\u00b7fe\u00b7Stadt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Und solche Leute sind des Himmels Meisterst\u00fccke/", "tokens": ["Und", "sol\u00b7che", "Leu\u00b7te", "sind", "des", "Him\u00b7mels", "Meis\u00b7ter\u00b7st\u00fc\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Und lichter derer Glut gemeinem Nutzen brennt/", "tokens": ["Und", "lich\u00b7ter", "de\u00b7rer", "Glut", "ge\u00b7mei\u00b7nem", "Nut\u00b7zen", "brennt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PDS", "NN", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Die durch Erfahrenheit und kluges Weltgeschicke/", "tokens": ["Die", "durch", "Er\u00b7fah\u00b7ren\u00b7heit", "und", "klu\u00b7ges", "Welt\u00b7ge\u00b7schi\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Die Art des besten Staats Haupts\u00e4chlich wol erkennt.", "tokens": ["Die", "Art", "des", "bes\u00b7ten", "Staats", "Haupt\u00b7s\u00e4ch\u00b7lich", "wol", "er\u00b7kennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Bey solchem konte sich mit grossem Ruhme weisen/", "tokens": ["Bey", "sol\u00b7chem", "kon\u00b7te", "sich", "mit", "gros\u00b7sem", "Ruh\u00b7me", "wei\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "VMFIN", "PRF", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Der Numa unsrer Stadt/ der nunmehr liegt erblast/", "tokens": ["Der", "Nu\u00b7ma", "uns\u00b7rer", "Stadt", "/", "der", "nun\u00b7mehr", "liegt", "er\u00b7blast", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$(", "ART", "ADV", "VVFIN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Der ob der Dienste Treu und Sorgfalt ist zu preisen/", "tokens": ["Der", "ob", "der", "Diens\u00b7te", "Treu", "und", "Sorg\u00b7falt", "ist", "zu", "prei\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOUS", "ART", "ADJA", "NN", "KON", "NN", "VAFIN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Und dessen Ehrenmahl bereits die Nachwelt fast.", "tokens": ["Und", "des\u00b7sen", "Eh\u00b7ren\u00b7mahl", "be\u00b7reits", "die", "Nach\u00b7welt", "fast", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRELAT", "NN", "ADV", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Er war zu Nutz und Heil des Vaterlands geboren/", "tokens": ["Er", "war", "zu", "Nutz", "und", "Heil", "des", "Va\u00b7ter\u00b7lands", "ge\u00b7bo\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "KON", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Bald von der W