{"textgrid.poem.32654": {"metadata": {"author": {"name": "Miller, Johann Martin", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich liebe dich, du kleines,", "genre": "verse", "period": "N.A.", "pub_year": 1771, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich liebe dich, du kleines,", "tokens": ["Ich", "lie\u00b7be", "dich", ",", "du", "klei\u00b7nes", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPER", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und stilles Veilchenthal", "tokens": ["Und", "stil\u00b7les", "Veil\u00b7chen\u00b7thal"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und dennoch schuf mir keines,", "tokens": ["Und", "den\u00b7noch", "schuf", "mir", "kei\u00b7nes", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PIS", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie du, so viele Qual.", "tokens": ["Wie", "du", ",", "so", "vie\u00b7le", "Qual", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Dich liebet auch Dorine,", "tokens": ["Dich", "lie\u00b7bet", "auch", "Do\u00b7ri\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die junge Sch\u00e4ferin,", "tokens": ["Die", "jun\u00b7ge", "Sch\u00e4\u00b7fe\u00b7rin", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit der bescheidnen Miene,", "tokens": ["Mit", "der", "be\u00b7scheid\u00b7nen", "Mie\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und mit dem spr\u00f6den Sinn.", "tokens": ["Und", "mit", "dem", "spr\u00f6\u00b7den", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$."], "meter": "---+-+", "measure": "unknown.measure.di"}}, "stanza.3": {"line.1": {"text": "J\u00fcngst sa\u00df ich hier und spielte", "tokens": ["J\u00fcngst", "sa\u00df", "ich", "hier", "und", "spiel\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im letzten Sonnenlicht,", "tokens": ["Im", "letz\u00b7ten", "Son\u00b7nen\u00b7licht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mein freier Busen f\u00fchlte", "tokens": ["Mein", "frei\u00b7er", "Bu\u00b7sen", "f\u00fchl\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Liebe Macht noch nicht.", "tokens": ["Der", "Lie\u00b7be", "Macht", "noch", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Als schnell, im leichten R\u00f6ckchen,", "tokens": ["Als", "schnell", ",", "im", "leich\u00b7ten", "R\u00f6ck\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vor mir ein M\u00e4dchen stand,", "tokens": ["Vor", "mir", "ein", "M\u00e4d\u00b7chen", "stand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das junge Maiengl\u00f6ckchen", "tokens": ["Das", "jun\u00b7ge", "Mai\u00b7en\u00b7gl\u00f6\u00b7ck\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.4": {"text": "Sich in die Haare wand.", "tokens": ["Sich", "in", "die", "Haa\u00b7re", "wand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wie bebt' ich nicht zur\u00fccke,", "tokens": ["Wie", "bebt'", "ich", "nicht", "zu\u00b7r\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Als sie mein Auge sah!", "tokens": ["Als", "sie", "mein", "Au\u00b7ge", "sah", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und, o, mit welchem Blicke", "tokens": ["Und", ",", "o", ",", "mit", "wel\u00b7chem", "Bli\u00b7cke"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KON", "$,", "FM", "$,", "APPR", "PWAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Stand sie, so reizend, da!", "tokens": ["Stand", "sie", ",", "so", "rei\u00b7zend", ",", "da", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADV", "VVPP", "$,", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Gern h\u00e4tt' ich sprechen wollen;", "tokens": ["Gern", "h\u00e4tt'", "ich", "spre\u00b7chen", "wol\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVINF", "VMINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Umsonst bem\u00fcht' ich mich;", "tokens": ["Um\u00b7sonst", "be\u00b7m\u00fcht'", "ich", "mich", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Kein Wort entwand dem vollen,", "tokens": ["Kein", "Wort", "ent\u00b7wand", "dem", "vol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Beklommnen Herzen sich.", "tokens": ["Be\u00b7klomm\u00b7nen", "Her\u00b7zen", "sich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PRF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Dann ging sie weg; ich klagte,", "tokens": ["Dann", "ging", "sie", "weg", ";", "ich", "klag\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$.", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und wu\u00dfte nicht warum;", "tokens": ["Und", "wu\u00df\u00b7te", "nicht", "wa\u00b7rum", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "PWAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Schlief wenig, und wenn's tagte,", "tokens": ["Schlief", "we\u00b7nig", ",", "und", "wenn's", "tag\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "KON", "PIS", "VVFIN", "$,"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.4": {"text": "War's tr\u00fcb um mich herum.", "tokens": ["Wa\u00b7r's", "tr\u00fcb", "um", "mich", "he\u00b7rum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Nun irr' ich stets alleine", "tokens": ["Nun", "irr'", "ich", "stets", "al\u00b7lei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den ganzen Tag umher;", "tokens": ["Den", "gan\u00b7zen", "Tag", "um\u00b7her", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und finde nirgend keine", "tokens": ["Und", "fin\u00b7de", "nir\u00b7gend", "kei\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "PIAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der alten Freuden mehr.", "tokens": ["Der", "al\u00b7ten", "Freu\u00b7den", "mehr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Oft wein' ich an der Stelle,", "tokens": ["Oft", "wein'", "ich", "an", "der", "Stel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und w\u00fcnsche mir mein Grab;", "tokens": ["Und", "w\u00fcn\u00b7sche", "mir", "mein", "Grab", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dann blickt zu mir der helle", "tokens": ["Dann", "blickt", "zu", "mir", "der", "hel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Geliebte Mond herab.", "tokens": ["Ge\u00b7lieb\u00b7te", "Mond", "her\u00b7ab", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Jetzt sieht er meine Z\u00e4hren:", "tokens": ["Jetzt", "sieht", "er", "mei\u00b7ne", "Z\u00e4h\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "O, s\u00e4h' er auch einmal,", "tokens": ["O", ",", "s\u00e4h'", "er", "auch", "ein\u00b7mal", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bei seinem Wiederkehren,", "tokens": ["Bei", "sei\u00b7nem", "Wie\u00b7der\u00b7keh\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das Ende meiner Qual!", "tokens": ["Das", "En\u00b7de", "mei\u00b7ner", "Qual", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "S\u00e4h' er, wie mich Seline", "tokens": ["S\u00e4h'", "er", ",", "wie", "mich", "Se\u00b7li\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "PPER", "NE"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.2": {"text": "Durch Liebe gl\u00fccklich macht,", "tokens": ["Durch", "Lie\u00b7be", "gl\u00fcck\u00b7lich", "macht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und aus bescheidner Miene", "tokens": ["Und", "aus", "be\u00b7scheid\u00b7ner", "Mie\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mir s\u00fc\u00df entgegen lacht!", "tokens": ["Mir", "s\u00fc\u00df", "ent\u00b7ge\u00b7gen", "lacht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PTKVZ", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Wie liebt' ich dann, o kleines,", "tokens": ["Wie", "liebt'", "ich", "dann", ",", "o", "klei\u00b7nes", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "$,", "FM", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und stilles Th\u00e4lchen dich;", "tokens": ["Und", "stil\u00b7les", "Th\u00e4l\u00b7chen", "dich", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es gliche dir sonst keines", "tokens": ["Es", "gli\u00b7che", "dir", "sonst", "kei\u00b7nes"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PIS"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "An Seligkeit f\u00fcr mich.", "tokens": ["An", "Se\u00b7lig\u00b7keit", "f\u00fcr", "mich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Hier baut' ich eine H\u00fctte", "tokens": ["Hier", "baut'", "ich", "ei\u00b7ne", "H\u00fct\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zum Angedenken hin,", "tokens": ["Zum", "An\u00b7ge\u00b7den\u00b7ken", "hin", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und scherzt' in ihrer Mitte", "tokens": ["Und", "scherzt'", "in", "ih\u00b7rer", "Mit\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit meiner Sch\u00e4ferin.", "tokens": ["Mit", "mei\u00b7ner", "Sch\u00e4\u00b7fe\u00b7rin", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}