{"textgrid.poem.28237": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "52.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einen Mohren wei\u00df erwaschen,", "tokens": ["Ei\u00b7nen", "Moh\u00b7ren", "wei\u00df", "er\u00b7wa\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Trincken au\u00df geleerten Flaschen,", "tokens": ["Trin\u00b7cken", "au\u00df", "ge\u00b7leer\u00b7ten", "Fla\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Einen Esel nackt beschehren,", "tokens": ["Ei\u00b7nen", "E\u00b7sel", "nackt", "be\u00b7scheh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eine Sackpfeiff abeh\u00e4ren,", "tokens": ["Ei\u00b7ne", "Sack\u00b7pfeiff", "a\u00b7be\u00b7h\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Einen Peltz im heissen baden,", "tokens": ["Ei\u00b7nen", "Peltz", "im", "heis\u00b7sen", "ba\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mit dem Siebe Wasser laden,", "tokens": ["Mit", "dem", "Sie\u00b7be", "Was\u00b7ser", "la\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Einem Tauben Lieder singen,", "tokens": ["Ei\u00b7nem", "Tau\u00b7ben", "Lie\u00b7der", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sand in ein Register bringen,", "tokens": ["Sand", "in", "ein", "Re\u00b7gis\u00b7ter", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "In den Wind und Wasser schreiben,", "tokens": ["In", "den", "Wind", "und", "Was\u00b7ser", "schrei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Flugwerck ohne Fl\u00fcgel treiben,", "tokens": ["Flug\u00b7werck", "oh\u00b7ne", "Fl\u00fc\u00b7gel", "trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Auff den Sand Pal\u00e4ste bauen,", "tokens": ["Auff", "den", "Sand", "Pa\u00b7l\u00e4s\u00b7te", "bau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Weibern auff die T\u00fccken schauen,", "tokens": ["Wei\u00b7bern", "auff", "die", "T\u00fc\u00b7cken", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Wind, Lufft, Lieb und Rauch verhalten,", "tokens": ["Wind", ",", "Lufft", ",", "Lieb", "und", "Rauch", "ver\u00b7hal\u00b7ten", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "J\u00fcnger machen einen Alten,", "tokens": ["J\u00fcn\u00b7ger", "ma\u00b7chen", "ei\u00b7nen", "Al\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Einen d\u00fcrren Wetzstein m\u00e4sten,", "tokens": ["Ei\u00b7nen", "d\u00fcr\u00b7ren", "Wetz\u00b7stein", "m\u00e4s\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Osten setzen zu dem Westen,", "tokens": ["Os\u00b7ten", "set\u00b7zen", "zu", "dem", "Wes\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Allen Leuten wol behagen,", "tokens": ["Al\u00b7len", "Leu\u00b7ten", "wol", "be\u00b7ha\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Allen was gef\u00e4llig sagen:", "tokens": ["Al\u00b7len", "was", "ge\u00b7f\u00e4l\u00b7lig", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "PWS", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Wer sich de\u00df wil unterstehen,", "tokens": ["Wer", "sich", "de\u00df", "wil", "un\u00b7ter\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "ART", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Mu\u00df mit Schimpff zur\u00fccke gehen.", "tokens": ["Mu\u00df", "mit", "Schimpff", "zu\u00b7r\u00fc\u00b7cke", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}