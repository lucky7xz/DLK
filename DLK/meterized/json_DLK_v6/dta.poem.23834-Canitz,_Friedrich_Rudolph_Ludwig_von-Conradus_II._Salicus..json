{"dta.poem.23834": {"metadata": {"author": {"name": "Canitz, Friedrich Rudolph Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "Conradus II. Salicus.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1700", "urn": "urn:nbn:de:kobv:b4-200905197532", "language": ["de:0.99"], "booktitle": "[Canitz, Friedrich Rudolph Ludwig von]: Neben-Stunden Unterschiedener Gedichte. [Hrsg. v. Joachim Lange]. Berlin, 1700."}, "poem": {"stanza.1": {"line.1": {"text": "Ich sah vor meinem Gl\u00fcck Gewalt und List zerrinnen/", "tokens": ["Ich", "sah", "vor", "mei\u00b7nem", "Gl\u00fcck", "Ge\u00b7walt", "und", "List", "zer\u00b7rin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "KON", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mir konte weder Sclav noch Ungar abgewinnen.", "tokens": ["Mir", "kon\u00b7te", "we\u00b7der", "Sclav", "noch", "Un\u00b7gar", "ab\u00b7ge\u00b7win\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "KON", "NE", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nachdem das Teutsche Volck zum K\u00e4yser mich ge-", "tokens": ["Nach\u00b7dem", "das", "Teut\u00b7sche", "Volck", "zum", "K\u00e4y\u00b7ser", "mich", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPRART", "NN", "PPER", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Hab ich Burgundien ihm wieder zugebracht.", "tokens": ["Hab", "ich", "Bur\u00b7gun\u00b7di\u00b7en", "ihm", "wie\u00b7der", "zu\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "NE", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-