{"textgrid.poem.52220": {"metadata": {"author": {"name": "Freiligrath, Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Trotz alledem!", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ob Armut euer Los auch sei,", "tokens": ["Ob", "Ar\u00b7mut", "eu\u00b7er", "Los", "auch", "sei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPOSAT", "NN", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hebt hoch die Stirn, trotz alledem!", "tokens": ["Hebt", "hoch", "die", "Stirn", ",", "trotz", "al\u00b7le\u00b7dem", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "$,", "APPR", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Geht k\u00fchn den feigen Knecht vorbei;", "tokens": ["Geht", "k\u00fchn", "den", "fei\u00b7gen", "Knecht", "vor\u00b7bei", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wagt's, arm zu sein trotz alledem!", "tokens": ["Wagt's", ",", "arm", "zu", "sein", "trotz", "al\u00b7le\u00b7dem", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "PTKZU", "VAINF", "APPR", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Trotz alledem und alledem,", "tokens": ["Trotz", "al\u00b7le\u00b7dem", "und", "al\u00b7le\u00b7dem", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "KON", "PIS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Trotz niederm Plack und alledem,", "tokens": ["Trotz", "nie\u00b7derm", "Plack", "und", "al\u00b7le\u00b7dem", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "PIS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der Rang ist das Gepr\u00e4ge nur,", "tokens": ["Der", "Rang", "ist", "das", "Ge\u00b7pr\u00e4\u00b7ge", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der Mann das Gold trotz alledem!", "tokens": ["Der", "Mann", "das", "Gold", "trotz", "al\u00b7le\u00b7dem", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und sitzt ihr auch beim kargen Mahl", "tokens": ["Und", "sitzt", "ihr", "auch", "beim", "kar\u00b7gen", "Mahl"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Zwilch und Lein und alledem,", "tokens": ["In", "Zwilch", "und", "Lein", "und", "al\u00b7le\u00b7dem", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "PIS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "G\u00f6nnt Schurken Samt und Goldpokal \u2013", "tokens": ["G\u00f6nnt", "Schur\u00b7ken", "Samt", "und", "Gold\u00b7po\u00b7kal", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Mann ist Mann trotz alledem!", "tokens": ["Ein", "Mann", "ist", "Mann", "trotz", "al\u00b7le\u00b7dem", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "APPR", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Trotz alledem und alledem,", "tokens": ["Trotz", "al\u00b7le\u00b7dem", "und", "al\u00b7le\u00b7dem", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "KON", "PIS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Trotz Prunk und Pracht und alledem!", "tokens": ["Trotz", "Prunk", "und", "Pracht", "und", "al\u00b7le\u00b7dem", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der brave Mann, wie d\u00fcrftig auch,", "tokens": ["Der", "bra\u00b7ve", "Mann", ",", "wie", "d\u00fcrf\u00b7tig", "auch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PWAV", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ist K\u00f6nig doch trotz alledem!", "tokens": ["Ist", "K\u00f6\u00b7nig", "doch", "trotz", "al\u00b7le\u00b7dem", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADV", "APPR", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Hei\u00dft \u00bbgn\u00e4d'ger Herr\u00ab das B\u00fcrschchen dort,", "tokens": ["Hei\u00dft", "\u00bb", "gn\u00e4d'\u00b7ger", "Herr", "\u00ab", "das", "B\u00fcr\u00b7schchen", "dort", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "ADJA", "NN", "$(", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Man sieht's am Stolz und alledem;", "tokens": ["Man", "sieht's", "am", "Stolz", "und", "al\u00b7le\u00b7dem", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPRART", "NN", "KON", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch lenkt auch Hunderte sein Wort,", "tokens": ["Doch", "lenkt", "auch", "Hun\u00b7der\u00b7te", "sein", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "'s ist nur ein Tropf trotz alledem!", "tokens": ["'s", "ist", "nur", "ein", "Tropf", "trotz", "al\u00b7le\u00b7dem", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "APPR", "PIS", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Trotz alledem und alledem!", "tokens": ["Trotz", "al\u00b7le\u00b7dem", "und", "al\u00b7le\u00b7dem", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "KON", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Trotz Band und Stern und alledem!", "tokens": ["Trotz", "Band", "und", "Stern", "und", "al\u00b7le\u00b7dem", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der Mann von unabh\u00e4ngigem Sinn", "tokens": ["Der", "Mann", "von", "un\u00b7ab\u00b7h\u00e4n\u00b7gi\u00b7gem", "Sinn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Sieht zu und lacht zu alledem!", "tokens": ["Sieht", "zu", "und", "lacht", "zu", "al\u00b7le\u00b7dem", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "KON", "VVFIN", "APPR", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ein F\u00fcrst macht Ritter wenn er spricht,", "tokens": ["Ein", "F\u00fcrst", "macht", "Rit\u00b7ter", "wenn", "er", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Sporn und Schild und alledem:", "tokens": ["Mit", "Sporn", "und", "Schild", "und", "al\u00b7le\u00b7dem", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den braven Mann kreiert er nicht,", "tokens": ["Den", "bra\u00b7ven", "Mann", "krei\u00b7ert", "er", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der steht zu hoch trotz alledem:", "tokens": ["Der", "steht", "zu", "hoch", "trotz", "al\u00b7le\u00b7dem", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKA", "ADJD", "APPR", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Trotz alledem und alledem!", "tokens": ["Trotz", "al\u00b7le\u00b7dem", "und", "al\u00b7le\u00b7dem", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "KON", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Trotz W\u00fcrdenschnack und alledem \u2013", "tokens": ["Trotz", "W\u00fcr\u00b7den\u00b7schnack", "und", "al\u00b7le\u00b7dem", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "PIS", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Des innern Wertes stolz Gef\u00fchl", "tokens": ["Des", "in\u00b7nern", "Wer\u00b7tes", "stolz", "Ge\u00b7f\u00fchl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "L\u00e4uft doch den Rang ab alledem!", "tokens": ["L\u00e4uft", "doch", "den", "Rang", "ab", "al\u00b7le\u00b7dem", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "APPR", "PIS", "$."], "meter": "++-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Drum jeder fleh', da\u00df es gescheh',", "tokens": ["Drum", "je\u00b7der", "fleh'", ",", "da\u00df", "es", "ge\u00b7scheh'", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "PIS", "VVFIN", "$,", "KOUS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie es geschieht trotz alledem,", "tokens": ["Wie", "es", "ge\u00b7schieht", "trotz", "al\u00b7le\u00b7dem", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "APPR", "PIS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df Wert und Kern, so nah wie fern,", "tokens": ["Da\u00df", "Wert", "und", "Kern", ",", "so", "nah", "wie", "fern", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "KON", "NN", "$,", "ADV", "ADJD", "KOKOM", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Sieg erringt trotz alledem!", "tokens": ["Den", "Sieg", "er\u00b7ringt", "trotz", "al\u00b7le\u00b7dem", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Trotz alledem und alledem!", "tokens": ["Trotz", "al\u00b7le\u00b7dem", "und", "al\u00b7le\u00b7dem", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "KON", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Es kommt dazu trotz alledem,", "tokens": ["Es", "kommt", "da\u00b7zu", "trotz", "al\u00b7le\u00b7dem", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "APPR", "PIS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da\u00df rings der Mensch die Bruderhand", "tokens": ["Da\u00df", "rings", "der", "Mensch", "die", "Bru\u00b7der\u00b7hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Dem Menschen reicht trotz alledem!", "tokens": ["Dem", "Men\u00b7schen", "reicht", "trotz", "al\u00b7le\u00b7dem", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}