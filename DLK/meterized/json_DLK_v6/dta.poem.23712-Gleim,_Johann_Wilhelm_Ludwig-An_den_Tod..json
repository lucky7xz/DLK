{"dta.poem.23712": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "An den Tod.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-200905197682", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Freier, als mein ", "tokens": ["Frei\u00b7er", ",", "als", "mein"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "KOUS", "PPOSAT"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Oder singe seine Lieder,", "tokens": ["O\u00b7der", "sin\u00b7ge", "sei\u00b7ne", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die den lieben Weingott loben!", "tokens": ["Die", "den", "lie\u00b7ben", "Wein\u00b7gott", "lo\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tod, du kanst den Wein nicht schmekken!", "tokens": ["Tod", ",", "du", "kanst", "den", "Wein", "nicht", "schmek\u00b7ken", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VMFIN", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Br\u00fcder, seht doch das Gerippe,", "tokens": ["Br\u00fc\u00b7der", ",", "seht", "doch", "das", "Ge\u00b7rip\u00b7pe", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Seht, es fehlen Lefz und Zunge!", "tokens": ["Seht", ",", "es", "feh\u00b7len", "Lefz", "und", "Zun\u00b7ge", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "VVFIN", "NE", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Br\u00fcder trinkt, und schmekkt den Rheinwein!", "tokens": ["Br\u00fc\u00b7der", "trinkt", ",", "und", "schmekkt", "den", "Rhein\u00b7wein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "KON", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Seid ihr einst, wie dis Gerippe,", "tokens": ["Seid", "ihr", "einst", ",", "wie", "dis", "Ge\u00b7rip\u00b7pe", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAIMP", "PPER", "ADV", "$,", "PWAV", "PDS", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Ohne Lefz und ohne Zunge,", "tokens": ["Oh\u00b7ne", "Lefz", "und", "oh\u00b7ne", "Zun\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Dann k\u00f6nnt ihr ihn nicht mehr schmekken.", "tokens": ["Dann", "k\u00f6nnt", "ihr", "ihn", "nicht", "mehr", "schmek\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}