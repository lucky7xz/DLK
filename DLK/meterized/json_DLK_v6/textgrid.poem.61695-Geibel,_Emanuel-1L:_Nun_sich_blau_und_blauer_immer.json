{"textgrid.poem.61695": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nun sich blau und blauer immer", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun sich blau und blauer immer", "tokens": ["Nun", "sich", "blau", "und", "blau\u00b7er", "im\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PRF", "ADJD", "KON", "ADJD", "ADV"], "meter": "--+-+-++", "measure": "anapaest.init"}, "line.2": {"text": "\u00dcber mir der Himmel tieft,", "tokens": ["\u00dc\u00b7ber", "mir", "der", "Him\u00b7mel", "tieft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Goldner stets des Herbstes Schimmer", "tokens": ["Gold\u00b7ner", "stets", "des", "Herbs\u00b7tes", "Schim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Durch die roten Wipfel trieft,", "tokens": ["Durch", "die", "ro\u00b7ten", "Wip\u00b7fel", "trieft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Nun empfind' ich's, wie ein Schleier", "tokens": ["Nun", "emp\u00b7find'", "ich's", ",", "wie", "ein", "Schlei\u00b7er"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "$,", "PWAV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schwer mir von der Seele f\u00e4llt,", "tokens": ["Schwer", "mir", "von", "der", "See\u00b7le", "f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und mein Auge wandelt freier", "tokens": ["Und", "mein", "Au\u00b7ge", "wan\u00b7delt", "frei\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Durch den lichten Reiz der Welt.", "tokens": ["Durch", "den", "lich\u00b7ten", "Reiz", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ja, getaucht in Sonnenstille,", "tokens": ["Ja", ",", "ge\u00b7taucht", "in", "Son\u00b7nen\u00b7stil\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVPP", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00dcberstr\u00f6mt von Sonnenkraft,", "tokens": ["\u00dc\u00b7ber\u00b7str\u00f6mt", "von", "Son\u00b7nen\u00b7kraft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Badet sich der kranke Wille", "tokens": ["Ba\u00b7det", "sich", "der", "kran\u00b7ke", "Wil\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Rein vom Schmerz der Leidenschaft.", "tokens": ["Rein", "vom", "Schmerz", "der", "Lei\u00b7den\u00b7schaft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und so leb' ich wunschlos wieder", "tokens": ["Und", "so", "leb'", "ich", "wun\u00b7schlos", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADJD", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Leichtgewobne Tage hin,", "tokens": ["Leicht\u00b7ge\u00b7wob\u00b7ne", "Ta\u00b7ge", "hin", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ein Nachwuchs heitrer Lieder", "tokens": ["Und", "ein", "Nach\u00b7wuchs", "hei\u00b7trer", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "B\u00fcrgt, da\u00df ich genesen bin.", "tokens": ["B\u00fcrgt", ",", "da\u00df", "ich", "ge\u00b7ne\u00b7sen", "bin", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Nur durch meine N\u00e4chte schwimmen", "tokens": ["Nur", "durch", "mei\u00b7ne", "N\u00e4ch\u00b7te", "schwim\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Manchmal, eh' mich Schlaf befiel,", "tokens": ["Manch\u00b7mal", ",", "eh'", "mich", "Schlaf", "be\u00b7fiel", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Noch der alten Sehnsucht Stimmen", "tokens": ["Noch", "der", "al\u00b7ten", "Sehn\u00b7sucht", "Stim\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie verhallend Harfenspiel.", "tokens": ["Wie", "ver\u00b7hal\u00b7lend", "Har\u00b7fen\u00b7spiel", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}