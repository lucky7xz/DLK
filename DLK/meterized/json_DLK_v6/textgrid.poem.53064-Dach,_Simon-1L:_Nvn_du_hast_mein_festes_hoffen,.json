{"textgrid.poem.53064": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nvn du hast mein festes hoffen,", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nvn du hast mein festes hoffen,", "tokens": ["Nvn", "du", "hast", "mein", "fes\u00b7tes", "hof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "VAFIN", "PPOSAT", "ADJA", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Liebste Seel, einmahl gestillt,", "tokens": ["Liebs\u00b7te", "Seel", ",", "ein\u00b7mahl", "ge\u00b7stillt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Komm, mein Hertze steht dir offen", "tokens": ["Komm", ",", "mein", "Hert\u00b7ze", "steht", "dir", "of\u00b7fen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PPOSAT", "VVFIN", "VVFIN", "PPER", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Au\u00dfgeschm\u00fcckt vnd angef\u00fcllt", "tokens": ["Au\u00df\u00b7ge\u00b7schm\u00fcckt", "vnd", "an\u00b7ge\u00b7f\u00fcllt"], "token_info": ["word", "word", "word"], "pos": ["VVPP", "KON", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mit standhaffter Flammen,", "tokens": ["Mit", "stand\u00b7haff\u00b7ter", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Die so lange Wehrt,", "tokens": ["Die", "so", "lan\u00b7ge", "Wehrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Bi\u00df vns Gott Zusammen", "tokens": ["Bi\u00df", "vns", "Gott", "Zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "NN", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Durch den Todt begehrt.", "tokens": ["Durch", "den", "Todt", "be\u00b7gehrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Dir, O Christe, wahres Leben,", "tokens": ["Dir", ",", "O", "Chris\u00b7te", ",", "wah\u00b7res", "Le\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "NE", "NE", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Heyrahts Stiffter, Breutigam,", "tokens": ["Hey\u00b7rahts", "Stiff\u00b7ter", ",", "Breu\u00b7ti\u00b7gam", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADV", "NN", "$,", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sey von Hertzen danck gegeben,", "tokens": ["Sey", "von", "Hert\u00b7zen", "danck", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "APPR", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das du, als ich Zu dir kam", "tokens": ["Das", "du", ",", "als", "ich", "Zu", "dir", "kam"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "$,", "KOUS", "PPER", "APPR", "PPER", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Vmb ein Lieb zu bitten,", "tokens": ["Vmb", "ein", "Lieb", "zu", "bit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Eine mir geschenckt", "tokens": ["Ei\u00b7ne", "mir", "ge\u00b7schenckt"], "token_info": ["word", "word", "word"], "pos": ["ART", "PPER", "VVPP"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Von so sch\u00f6nen Sitten,", "tokens": ["Von", "so", "sch\u00f6\u00b7nen", "Sit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Als mein Hertz gedenckt.", "tokens": ["Als", "mein", "Hertz", "ge\u00b7denckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Wer auff deinen Nahmen bawet", "tokens": ["Wer", "auff", "dei\u00b7nen", "Nah\u00b7men", "ba\u00b7wet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Als der niemals hat gefehlt,", "tokens": ["Als", "der", "nie\u00b7mals", "hat", "ge\u00b7fehlt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADV", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Vnd sich dir in Hoffnung trawet,", "tokens": ["Vnd", "sich", "dir", "in", "Hoff\u00b7nung", "tra\u00b7wet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Hat das allerbest' erwehlt,", "tokens": ["Hat", "das", "al\u00b7ler\u00b7best'", "er\u00b7wehlt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "PIS", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wird's jhm sawr' vnd Bitter,", "tokens": ["Wird's", "jhm", "sa\u00b7wr'", "vnd", "Bit\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVFIN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Bleibt er doch bestehn,", "tokens": ["Bleibt", "er", "doch", "be\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Bi\u00df das Vngewitter", "tokens": ["Bi\u00df", "das", "Vn\u00b7ge\u00b7wit\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Mus vor\u00fcber gehn.", "tokens": ["Mus", "vor\u00b7\u00fc\u00b7ber", "gehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Darumb wil ich allem gleuben,", "tokens": ["Da\u00b7rumb", "wil", "ich", "al\u00b7lem", "gleu\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "PIS", "VVINF", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Was mir, Gott, dein Wort f\u00fcrhelt,", "tokens": ["Was", "mir", ",", "Gott", ",", "dein", "Wort", "f\u00fcr\u00b7helt", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "$,", "NN", "$,", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Du kanst, was du sagest, treiben,", "tokens": ["Du", "kanst", ",", "was", "du", "sa\u00b7gest", ",", "trei\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PWS", "PPER", "VVFIN", "$,", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Da\u00df es dir zu Willen fellt,", "tokens": ["Da\u00df", "es", "dir", "zu", "Wil\u00b7len", "fellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Alles mus geschehen", "tokens": ["Al\u00b7les", "mus", "ge\u00b7sche\u00b7hen"], "token_info": ["word", "word", "word"], "pos": ["PIS", "VMFIN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Auff dein Wort allein,", "tokens": ["Auff", "dein", "Wort", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Ob gleich wir nicht sehen,", "tokens": ["Ob", "gleich", "wir", "nicht", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Wie es k\u00f6nne sein.", "tokens": ["Wie", "es", "k\u00f6n\u00b7ne", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VMFIN", "VAINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Alles Wesen, was wir kennen", "tokens": ["Al\u00b7les", "We\u00b7sen", ",", "was", "wir", "ken\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "NN", "$,", "PRELS", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In dem grossen Weltgebew,", "tokens": ["In", "dem", "gros\u00b7sen", "Welt\u00b7ge\u00b7bew", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wird vnd mu\u00df zuletzt verbrennen,", "tokens": ["Wird", "vnd", "mu\u00df", "zu\u00b7letzt", "ver\u00b7bren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KON", "VMFIN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn du wirst ein Feldgeschrey", "tokens": ["Wenn", "du", "wirst", "ein", "Feld\u00b7ge\u00b7schrey"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VAFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "In den Wolcken machen,", "tokens": ["In", "den", "Wol\u00b7cken", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Da der Erdenkreis", "tokens": ["Da", "der", "Er\u00b7den\u00b7kreis"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Au\u00df dem Grab' erwachen", "tokens": ["Au\u00df", "dem", "Grab'", "er\u00b7wa\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Wird auff dein gehei\u00df.", "tokens": ["Wird", "auff", "dein", "ge\u00b7hei\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Welches auch die kl\u00fcgsten Heyden", "tokens": ["Wel\u00b7ches", "auch", "die", "kl\u00fcgs\u00b7ten", "Hey\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wiederstritten vnd verneint,", "tokens": ["Wie\u00b7der\u00b7strit\u00b7ten", "vnd", "ver\u00b7neint", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die nat\u00fcrlich gar nicht leiden,", "tokens": ["Die", "na\u00b7t\u00fcr\u00b7lich", "gar", "nicht", "lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn dein Wort was anders meint,", "tokens": ["Wenn", "dein", "Wort", "was", "an\u00b7ders", "meint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PWS", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Des sich doch ein Christe", "tokens": ["Des", "sich", "doch", "ein", "Chris\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PRF", "ADV", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Tr\u00f6stet in der Pein,", "tokens": ["Tr\u00f6s\u00b7tet", "in", "der", "Pein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Oder sonsten m\u00fcste", "tokens": ["O\u00b7der", "sons\u00b7ten", "m\u00fcs\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADV", "VMFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Gar kein Gott nicht sein.", "tokens": ["Gar", "kein", "Gott", "nicht", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "PTKNEG", "VAINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}