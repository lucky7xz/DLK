{"textgrid.poem.65428": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1. Was wir bringen", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Rosen pfl\u00fcckten einst die Weisen in der Musen Gartenflur,", "tokens": ["Ro\u00b7sen", "pfl\u00fcck\u00b7ten", "einst", "die", "Wei\u00b7sen", "in", "der", "Mu\u00b7sen", "Gar\u00b7ten\u00b7flur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ART", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Sp\u00f6tter kamen hinter ihnen, und sie brachen Dornen nur.", "tokens": ["Sp\u00f6t\u00b7ter", "ka\u00b7men", "hin\u00b7ter", "ih\u00b7nen", ",", "und", "sie", "bra\u00b7chen", "Dor\u00b7nen", "nur", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPER", "$,", "KON", "PPER", "VVFIN", "NN", "ADV", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Als sie Dornen nicht mehr fanden, suchten Disteln sie zum Stich:", "tokens": ["Als", "sie", "Dor\u00b7nen", "nicht", "mehr", "fan\u00b7den", ",", "such\u00b7ten", "Dis\u00b7teln", "sie", "zum", "Stich", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "PTKNEG", "ADV", "VVFIN", "$,", "VVFIN", "NN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Dornenlose Rosen bring' ich. Lieber Leser, willst du mich?", "tokens": ["Dor\u00b7nen\u00b7lo\u00b7se", "Ro\u00b7sen", "bring'", "ich", ".", "Lie\u00b7ber", "Le\u00b7ser", ",", "willst", "du", "mich", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "PPER", "$.", "ADJD", "NN", "$,", "VMFIN", "PPER", "PRF", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}