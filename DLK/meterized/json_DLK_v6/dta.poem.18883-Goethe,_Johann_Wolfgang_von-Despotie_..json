{"dta.poem.18883": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "Despotie .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Denjenigen, der meine Scheitel", "tokens": ["Den\u00b7je\u00b7ni\u00b7gen", ",", "der", "mei\u00b7ne", "Schei\u00b7tel"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PDS", "$,", "PRELS", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie Staub zertritt mit F\u00fcssen,", "tokens": ["Wie", "Staub", "zer\u00b7tritt", "mit", "F\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Will ich zum Kaiser machen,", "tokens": ["Will", "ich", "zum", "Kai\u00b7ser", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wenn er zu mir zur\u00fcckkommt.", "tokens": ["Wenn", "er", "zu", "mir", "zu\u00b7r\u00fcck\u00b7kommt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}