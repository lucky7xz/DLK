{"dta.poem.4005": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Der gefesselte Strom .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905197104", "language": ["de:0.99"], "booktitle": "H\u00f6lderlin, Friedrich: Gedichte. Stuttgart u. a., 1826."}, "poem": {"stanza.1": {"line.1": {"text": "Was schl\u00e4fst und tr\u00e4umst Du, J\u00fcngling! geh\u00fcllt", "tokens": ["Was", "schl\u00e4fst", "und", "tr\u00e4umst", "Du", ",", "J\u00fcng\u00b7ling", "!", "ge\u00b7h\u00fcllt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["PWS", "VVFIN", "KON", "VVFIN", "PPER", "$,", "NN", "$.", "VVPP"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "in Dich,", "tokens": ["in", "Dich", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "PPER", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Und s\u00e4umst am kalten Ufer, Geduldiger,", "tokens": ["Und", "s\u00e4umst", "am", "kal\u00b7ten", "U\u00b7fer", ",", "Ge\u00b7dul\u00b7di\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "NN", "$,", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und achtest nicht des Ursprungs, Du, des", "tokens": ["Und", "ach\u00b7test", "nicht", "des", "Ur\u00b7sprungs", ",", "Du", ",", "des"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["KON", "VVFIN", "PTKNEG", "ART", "NN", "$,", "PPER", "$,", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Oceans Sohn, des Titanenfreundes?", "tokens": ["O\u00b7ceans", "Sohn", ",", "des", "Ti\u00b7ta\u00b7nen\u00b7freun\u00b7des", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ART", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Die Liebesboten, welche der Vater schickt,", "tokens": ["Die", "Lie\u00b7bes\u00b7bo\u00b7ten", ",", "wel\u00b7che", "der", "Va\u00b7ter", "schickt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Kennst Du die lebenathmenden L\u00fcfte nicht?", "tokens": ["Kennst", "Du", "die", "le\u00b7be\u00b7nath\u00b7men\u00b7den", "L\u00fcf\u00b7te", "nicht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "PTKNEG", "$."], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Und trifft das Wort Dich nicht, das hell von", "tokens": ["Und", "trifft", "das", "Wort", "Dich", "nicht", ",", "das", "hell", "von"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "PPER", "PTKNEG", "$,", "PRELS", "ADJD", "APPR"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Oben der wachende Gott Dir sendet? &#8212;", "tokens": ["O\u00b7ben", "der", "wa\u00b7chen\u00b7de", "Gott", "Dir", "sen\u00b7det", "?", "&#8212;"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "XML_entity"], "pos": ["ADV", "ART", "ADJA", "NN", "PPER", "VVFIN", "$.", "$("], "meter": "+--+--++--", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Schon t\u00f6nt, schon t\u00f6nt es ihm in der Brust! es", "tokens": ["Schon", "t\u00f6nt", ",", "schon", "t\u00f6nt", "es", "ihm", "in", "der", "Brust", "!", "es"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "PPER", "APPR", "ART", "NN", "$.", "PPER"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "quillt,", "tokens": ["quillt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Wie da er noch im Schoose der Felsen spielt',", "tokens": ["Wie", "da", "er", "noch", "im", "Schoo\u00b7se", "der", "Fel\u00b7sen", "spielt'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "KOUS", "PPER", "ADV", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Ihm auf; und nun gedenkt er seiner", "tokens": ["Ihm", "auf", ";", "und", "nun", "ge\u00b7denkt", "er", "sei\u00b7ner"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "PTKVZ", "$.", "KON", "ADV", "VVFIN", "PPER", "PPOSAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Kraft, der Gewaltige, nun, nun eilt er,", "tokens": ["Kraft", ",", "der", "Ge\u00b7wal\u00b7ti\u00b7ge", ",", "nun", ",", "nun", "eilt", "er", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "$,", "ADV", "$,", "ADV", "VVFIN", "PPER", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Der Zauderer, er spottet der Fesseln nun,", "tokens": ["Der", "Zau\u00b7de\u00b7rer", ",", "er", "spot\u00b7tet", "der", "Fes\u00b7seln", "nun", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und nimmt und bricht und wirft die zerbrochenen", "tokens": ["Und", "nimmt", "und", "bricht", "und", "wirft", "die", "zer\u00b7bro\u00b7che\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "ART", "ADJA"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Im Zorne, spielend, da und dort zum", "tokens": ["Im", "Zor\u00b7ne", ",", "spie\u00b7lend", ",", "da", "und", "dort", "zum"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "ADJD", "$,", "ADV", "KON", "ADV", "APPRART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schallenden Ufer; und von der Stimme", "tokens": ["Schal\u00b7len\u00b7den", "U\u00b7fer", ";", "und", "von", "der", "Stim\u00b7me"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "KON", "APPR", "ART", "NN"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}}, "stanza.5": {"line.1": {"text": "Des G\u00f6ttersohns erwachen die Berge rings,", "tokens": ["Des", "G\u00f6t\u00b7ter\u00b7sohns", "er\u00b7wa\u00b7chen", "die", "Ber\u00b7ge", "rings", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Es regen sich die W\u00e4lder, es h\u00f6rt die Kluft", "tokens": ["Es", "re\u00b7gen", "sich", "die", "W\u00e4l\u00b7der", ",", "es", "h\u00f6rt", "die", "Kluft"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Den Herold fern, und schaudernd regt im", "tokens": ["Den", "He\u00b7rold", "fern", ",", "und", "schau\u00b7dernd", "regt", "im"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "$,", "KON", "ADJD", "VVFIN", "APPRART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Busen der Erde sich Freude wieder.", "tokens": ["Bu\u00b7sen", "der", "Er\u00b7de", "sich", "Freu\u00b7de", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PRF", "NN", "ADV", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Der neue Fr\u00fchling d\u00e4mmert, es bl\u00fcht um ihn;", "tokens": ["Der", "neu\u00b7e", "Fr\u00fch\u00b7ling", "d\u00e4m\u00b7mert", ",", "es", "bl\u00fcht", "um", "ihn", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "PPER", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Er aber wandelt hin zu Unsterblichen;", "tokens": ["Er", "a\u00b7ber", "wan\u00b7delt", "hin", "zu", "U\u00b7nsterb\u00b7li\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+---", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Denn nirgend darf er bleiben, als wo", "tokens": ["Denn", "nir\u00b7gend", "darf", "er", "blei\u00b7ben", ",", "als", "wo"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "VMFIN", "PPER", "VVINF", "$,", "KOUS", "PWAV"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Ihn in die Arme der Vater aufnimmt.", "tokens": ["Ihn", "in", "die", "Ar\u00b7me", "der", "Va\u00b7ter", "auf\u00b7nimmt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}}}}