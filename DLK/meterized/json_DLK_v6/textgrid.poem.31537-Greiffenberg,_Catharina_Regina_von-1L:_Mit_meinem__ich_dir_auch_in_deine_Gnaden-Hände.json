{"textgrid.poem.31537": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mit meinem / ich dir auch in deine Gnaden-H\u00e4nde", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit meinem / ich dir auch in deine Gnaden-H\u00e4nde", "tokens": ["Mit", "mei\u00b7nem", "/", "ich", "dir", "auch", "in", "dei\u00b7ne", "Gna\u00b7den\u00b7H\u00e4n\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "$(", "PPER", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "gib' aller Christen-Geist. Mein Sterben sie belebt:", "tokens": ["gib'", "al\u00b7ler", "Chris\u00b7ten\u00b7Geist", ".", "Mein", "Ster\u00b7ben", "sie", "be\u00b7lebt", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$.", "PPOSAT", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "mein Leib'-Eingrabung sie in deine Scho\u00df erhebt;", "tokens": ["mein", "Leib'\u00b7Ein\u00b7gra\u00b7bung", "sie", "in", "dei\u00b7ne", "Scho\u00df", "er\u00b7hebt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "mein' H\u00f6ll' ihr' Himmel-fahrt und Paradei\u00df anl\u00e4nde.", "tokens": ["mein'", "H\u00f6ll'", "ih\u00b7r'", "Him\u00b7mel\u00b7fahrt", "und", "Pa\u00b7ra\u00b7dei\u00df", "an\u00b7l\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPOSAT", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Auf da\u00df in deiner Hand mein Geist / ich ihm hersende /", "tokens": ["Auf", "da\u00df", "in", "dei\u00b7ner", "Hand", "mein", "Geist", "/", "ich", "ihm", "her\u00b7sen\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "APPR", "PPOSAT", "NN", "PPOSAT", "NN", "$(", "PPER", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "den ihren Ruh bereit: wornach ich lang gestrebt /", "tokens": ["den", "ih\u00b7ren", "Ruh", "be\u00b7reit", ":", "wor\u00b7nach", "ich", "lang", "ge\u00b7strebt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADJD", "$.", "PWAV", "PPER", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "in Herz-und H\u00f6llen-Pein / im Blut und Creutz geschwebt /", "tokens": ["in", "Her\u00b7z\u00b7und", "H\u00f6l\u00b7len\u00b7Pein", "/", "im", "Blut", "und", "Creutz", "ge\u00b7schwebt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$(", "APPRART", "NN", "KON", "NN", "VVPP", "$("], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "|leidens|", "tokens": ["|lei\u00b7dens|"], "token_info": ["word"], "pos": ["NE"], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "bi\u00df endlich ich erlangt di\u00df meines \u203a \u2039 Ende.", "tokens": ["bi\u00df", "end\u00b7lich", "ich", "er\u00b7langt", "di\u00df", "mei\u00b7nes", "\u203a", "\u2039", "En\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["KON", "ADV", "PPER", "VVFIN", "PDS", "PPOSAT", "$(", "$(", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "|endes |", "tokens": ["|en\u00b7des", "|"], "token_info": ["word", "word"], "pos": ["NE", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.11": {"text": "Dieweil ja meine Lieb' am Leiden nicht vergn\u00fcgt:", "tokens": ["Die\u00b7weil", "ja", "mei\u00b7ne", "Lieb'", "am", "Lei\u00b7den", "nicht", "ver\u00b7gn\u00fcgt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "APPRART", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "so will ich sterben auch / auf da\u00df unsterblich werde", "tokens": ["so", "will", "ich", "ster\u00b7ben", "auch", "/", "auf", "da\u00df", "uns\u00b7terb\u00b7lich", "wer\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "ADV", "$(", "APPR", "KOUS", "PPER", "VAFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "die selbste Sterblichkeit. Mein Tod den Tod besiegt.", "tokens": ["die", "selbs\u00b7te", "Sterb\u00b7lich\u00b7keit", ".", "Mein", "Tod", "den", "Tod", "be\u00b7siegt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "PPOSAT", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die Auferstehung bring' mit mir ich in die Erde.", "tokens": ["Die", "Auf\u00b7er\u00b7ste\u00b7hung", "bring'", "mit", "mir", "ich", "in", "die", "Er\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ihr meinet / ihr verschlingt das Leben / Erd' und Tod!", "tokens": ["Ihr", "mei\u00b7net", "/", "ihr", "ver\u00b7schlingt", "das", "Le\u00b7ben", "/", "Erd'", "und", "Tod", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PPER", "VVFIN", "ART", "NN", "$(", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Nein! es hat minder nie mit ihm / als sterbend Noht.", "tokens": ["Nein", "!", "es", "hat", "min\u00b7der", "nie", "mit", "ihm", "/", "als", "ster\u00b7bend", "Noht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PPER", "VAFIN", "ADV", "ADV", "APPR", "PPER", "$(", "KOUS", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}