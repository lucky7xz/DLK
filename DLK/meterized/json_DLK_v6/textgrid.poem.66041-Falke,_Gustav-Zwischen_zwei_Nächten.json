{"textgrid.poem.66041": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Zwischen zwei N\u00e4chten", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Morgen steigt und gl\u00fcht und steigt,", "tokens": ["Der", "Mor\u00b7gen", "steigt", "und", "gl\u00fcht", "und", "steigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und frohe Herzen beben;", "tokens": ["Und", "fro\u00b7he", "Her\u00b7zen", "be\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein Tag, und \u00fcberschauert schweigt", "tokens": ["Ein", "Tag", ",", "und", "\u00fc\u00b7bersc\u00b7hau\u00b7ert", "schweigt"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KON", "VVPP", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das trunken reiche Leben.", "tokens": ["Das", "trun\u00b7ken", "rei\u00b7che", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und zwischen Auf- und Niedergang", "tokens": ["Und", "zwi\u00b7schen", "Auf", "und", "Nie\u00b7der\u00b7gang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "TRUNC", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Blutwellenhei\u00dfes Schlagen,", "tokens": ["Blut\u00b7wel\u00b7len\u00b7hei\u00b7\u00dfes", "Schla\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Ein Hoffen tausend Leben lang,", "tokens": ["Ein", "Hof\u00b7fen", "tau\u00b7send", "Le\u00b7ben", "lang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "CARD", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Schmerz und ein Entsagen.", "tokens": ["Ein", "Schmerz", "und", "ein", "Ent\u00b7sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und ists nur einen Sonnenblitz,", "tokens": ["Und", "ists", "nur", "ei\u00b7nen", "Son\u00b7nen\u00b7blitz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df uns ein Gl\u00fcck bereitet,", "tokens": ["Da\u00df", "uns", "ein", "Gl\u00fcck", "be\u00b7rei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nur einen kurzen Sattelsitz,", "tokens": ["Nur", "ei\u00b7nen", "kur\u00b7zen", "Sat\u00b7tel\u00b7sitz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df Freude uns begleitet:", "tokens": ["Da\u00df", "Freu\u00b7de", "uns", "be\u00b7glei\u00b7tet", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Freiweg durchs Leben! Sprung und Sporn!", "tokens": ["Frei\u00b7weg", "durchs", "Le\u00b7ben", "!", "Sprung", "und", "Sporn", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$.", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und Schwert und Schlacht und Scherben,", "tokens": ["Und", "Schwert", "und", "Schlacht", "und", "Scher\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und Gl\u00fcck und T\u00fcck und Kranz und Dorn,", "tokens": ["Und", "Gl\u00fcck", "und", "T\u00fcck", "und", "Kranz", "und", "Dorn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und rauscht der Tod durchs reife Korn,", "tokens": ["Und", "rauscht", "der", "Tod", "durchs", "rei\u00b7fe", "Korn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein L\u00e4cheln noch im Sterben.", "tokens": ["Ein", "L\u00e4\u00b7cheln", "noch", "im", "Ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}