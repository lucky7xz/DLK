{"textgrid.poem.36016": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "[es fliegt mit struppigem Gefieder]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es fliegt mit struppigem Gefieder", "tokens": ["Es", "fliegt", "mit", "strup\u00b7pi\u00b7gem", "Ge\u00b7fie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein armer Vogel in die Luft.", "tokens": ["Ein", "ar\u00b7mer", "Vo\u00b7gel", "in", "die", "Luft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der arme Vogel singt nicht Lieder", "tokens": ["Der", "ar\u00b7me", "Vo\u00b7gel", "singt", "nicht", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NE", "VVFIN", "PTKNEG", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und freut sich nicht am Blumenduft.", "tokens": ["Und", "freut", "sich", "nicht", "am", "Blu\u00b7men\u00b7duft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "PTKNEG", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ach! andres Werk hat er zu treiben,", "tokens": ["Ach", "!", "and\u00b7res", "Werk", "hat", "er", "zu", "trei\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADJA", "NN", "VAFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Er sucht sich einen stillen Ort", "tokens": ["Er", "sucht", "sich", "ei\u00b7nen", "stil\u00b7len", "Ort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und darf an keiner Stelle bleiben,", "tokens": ["Und", "darf", "an", "kei\u00b7ner", "Stel\u00b7le", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und mu\u00df best\u00e4ndig wieder fort.", "tokens": ["Und", "mu\u00df", "be\u00b7st\u00e4n\u00b7dig", "wie\u00b7der", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADJD", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Er m\u00f6chte gern auf Erden wohnen,", "tokens": ["Er", "m\u00f6ch\u00b7te", "gern", "auf", "Er\u00b7den", "woh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Er hat die Erde gar so lieb,", "tokens": ["Er", "hat", "die", "Er\u00b7de", "gar", "so", "lieb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und sehnt sich nach den Regionen,", "tokens": ["Und", "sehnt", "sich", "nach", "den", "Re\u00b7gi\u00b7o\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wo man ihn aus dem Neste trieb.", "tokens": ["Wo", "man", "ihn", "aus", "dem", "Nes\u00b7te", "trieb", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Drum, soll er nicht an Heimweh sterben,", "tokens": ["Drum", ",", "soll", "er", "nicht", "an", "Heim\u00b7weh", "ster\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "VMFIN", "PPER", "PTKNEG", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So fang' den Vogel, eins, zwei, drei!", "tokens": ["So", "fang'", "den", "Vo\u00b7gel", ",", "eins", ",", "zwei", ",", "drei", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NE", "$,", "PIS", "$,", "CARD", "$,", "CARD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und soll der Freund ihn von dir erben,", "tokens": ["Und", "soll", "der", "Freund", "ihn", "von", "dir", "er\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "PPER", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So la\u00df ihn hurtig wieder frei.", "tokens": ["So", "la\u00df", "ihn", "hur\u00b7tig", "wie\u00b7der", "frei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "ADJD", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Doch soll der Vogel l\u00e4nger schweben,", "tokens": ["Doch", "soll", "der", "Vo\u00b7gel", "l\u00e4n\u00b7ger", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NE", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So greif' ihn nicht mit blo\u00dfer Hand,", "tokens": ["So", "greif'", "ihn", "nicht", "mit", "blo\u00b7\u00dfer", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sonst t\u00f6test du sein zartes Leben", "tokens": ["Sonst", "t\u00f6\u00b7test", "du", "sein", "zar\u00b7tes", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und brichst der Federn leichtes Band.", "tokens": ["Und", "brichst", "der", "Fe\u00b7dern", "leich\u00b7tes", "Band", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Wie mancher war in seinem Leben", "tokens": ["Wie", "man\u00b7cher", "war", "in", "sei\u00b7nem", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein solches armes V\u00f6gelein,", "tokens": ["Ein", "sol\u00b7ches", "ar\u00b7mes", "V\u00f6\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und mu\u00dfte wider Willen schweben", "tokens": ["Und", "mu\u00df\u00b7te", "wi\u00b7der", "Wil\u00b7len", "schwe\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und immer in der Hetze sein.", "tokens": ["Und", "im\u00b7mer", "in", "der", "Het\u00b7ze", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Bis er nicht l\u00e4nger konnte fliegen", "tokens": ["Bis", "er", "nicht", "l\u00e4n\u00b7ger", "konn\u00b7te", "flie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "ADJD", "VMFIN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und auf die Erde niederfiel;", "tokens": ["Und", "auf", "die", "Er\u00b7de", "nie\u00b7der\u00b7fiel", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dort lie\u00df das Gl\u00fcck ihn endlich liegen", "tokens": ["Dort", "lie\u00df", "das", "Gl\u00fcck", "ihn", "end\u00b7lich", "lie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "ADV", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und suchte sich ein andres Spiel.", "tokens": ["Und", "such\u00b7te", "sich", "ein", "and\u00b7res", "Spiel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}