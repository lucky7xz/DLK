{"dta.poem.9700": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "An Lisetten.  \n E. N.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So wilstu nun durchaus ins nonnen-closter gehn/", "tokens": ["So", "wils\u00b7tu", "nun", "durc\u00b7haus", "ins", "non\u00b7nen\u00b7clos\u00b7ter", "gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "ADV", "APPRART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und in der heiligkeit als eine schwester stehn?", "tokens": ["Und", "in", "der", "hei\u00b7lig\u00b7keit", "als", "ei\u00b7ne", "schwes\u00b7ter", "stehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "KOKOM", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nun gut/ ich habe mir den m\u00fcnchs-stand vorgenommen/", "tokens": ["Nun", "gut", "/", "ich", "ha\u00b7be", "mir", "den", "m\u00fcnchs\u00b7stand", "vor\u00b7ge\u00b7nom\u00b7men", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$(", "PPER", "VAFIN", "PPER", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So darf ein bruder wol zu einer schwester kommen.", "tokens": ["So", "darf", "ein", "bru\u00b7der", "wol", "zu", "ei\u00b7ner", "schwes\u00b7ter", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "ADV", "APPR", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Lisette la\u00df mich nur in deine zelle nein/", "tokens": ["Li\u00b7set\u00b7te", "la\u00df", "mich", "nur", "in", "dei\u00b7ne", "zel\u00b7le", "nein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVIMP", "PPER", "ADV", "APPR", "PPOSAT", "NN", "PTKANT", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Jm beten solstu selbst mein paternoster seyn.", "tokens": ["Jm", "be\u00b7ten", "sols\u00b7tu", "selbst", "mein", "pa\u00b7ter\u00b7nos\u00b7ter", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "VVINF", "VMFIN", "ADV", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}