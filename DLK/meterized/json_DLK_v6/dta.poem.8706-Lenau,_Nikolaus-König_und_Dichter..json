{"dta.poem.8706": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "K\u00f6nig und Dichter.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Stolz flammt ein K\u00f6nig dort auf erhabnem Thron,", "tokens": ["Stolz", "flammt", "ein", "K\u00f6\u00b7nig", "dort", "auf", "er\u00b7hab\u00b7nem", "Thron", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "++-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Befehl den V\u00f6lkern winkt in die Fernen er,", "tokens": ["Be\u00b7fehl", "den", "V\u00f6l\u00b7kern", "winkt", "in", "die", "Fer\u00b7nen", "er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "PPER", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Denn scheu vor ihm zur\u00fcck stets weiter", "tokens": ["Denn", "scheu", "vor", "ihm", "zu\u00b7r\u00fcck", "stets", "wei\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "PPER", "ADV", "ADV", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Weichen die Gr\u00e4nzen des Reichs, und weiter.", "tokens": ["Wei\u00b7chen", "die", "Gr\u00e4n\u00b7zen", "des", "Reichs", ",", "und", "wei\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "$,", "KON", "ADV", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Zum nahen Flug jezt l\u00fcftet der schnelle Tod", "tokens": ["Zum", "na\u00b7hen", "Flug", "jezt", "l\u00fcf\u00b7tet", "der", "schnel\u00b7le", "Tod"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Den Fittig, und \u2014 was flammte, das glimmt nur mehr;", "tokens": ["Den", "Fit\u00b7tig", ",", "und", "was", "flamm\u00b7te", ",", "das", "glimmt", "nur", "mehr", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "$(", "PWS", "VVFIN", "$,", "PDS", "VVFIN", "ADV", "ADV", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Er rauscht heran \u2014 sein starker Fl\u00fcgel", "tokens": ["Er", "rauscht", "he\u00b7ran", "sein", "star\u00b7ker", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$(", "PPOSAT", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "F\u00e4chelt vom Throne herab die Asche. \u2014", "tokens": ["F\u00e4\u00b7chelt", "vom", "Thro\u00b7ne", "her\u00b7ab", "die", "A\u00b7sche", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "APPRART", "NN", "ADV", "ART", "NN", "$.", "$("], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Dort singt ein S\u00e4nger hohe Begeisterung:", "tokens": ["Dort", "singt", "ein", "S\u00e4n\u00b7ger", "ho\u00b7he", "Be\u00b7geis\u00b7te\u00b7rung", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Welle horcht, Wald, Th\u00e4ler und Berge, selbst", "tokens": ["Die", "Wel\u00b7le", "horcht", ",", "Wald", ",", "Th\u00e4\u00b7ler", "und", "Ber\u00b7ge", ",", "selbst"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "NN", "$,", "NN", "KON", "NN", "$,", "ADV"], "meter": "-+---+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die G\u00f6tter horchen, seliger, und", "tokens": ["Die", "G\u00f6t\u00b7ter", "hor\u00b7chen", ",", "se\u00b7li\u00b7ger", ",", "und"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["ART", "NN", "VVINF", "$,", "ADJD", "$,", "KON"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sehnen vom hohen Olymp herab sich; \u2014", "tokens": ["Seh\u00b7nen", "vom", "ho\u00b7hen", "O\u00b7lymp", "her\u00b7ab", "sich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "ADV", "PRF", "$.", "$("], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Du winkst, o Tod; \u2014 er schweigt; der erstarrten Hand", "tokens": ["Du", "winkst", ",", "o", "Tod", ";", "er", "schweigt", ";", "der", "er\u00b7starr\u00b7ten", "Hand"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "FM", "NN", "$.", "$(", "PPER", "VVFIN", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Entsinkt die Leier; doch im Triumfe f\u00fchrt", "tokens": ["Ent\u00b7sinkt", "die", "Lei\u00b7er", ";", "doch", "im", "Tri\u00b7um\u00b7fe", "f\u00fchrt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$.", "ADV", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Die Ewigkeit sein Lied davon, das", "tokens": ["Die", "E\u00b7wig\u00b7keit", "sein", "Lied", "da\u00b7von", ",", "das"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "PPOSAT", "NN", "PAV", "$,", "PRELS"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Z\u00fcrnend die St\u00e4rkere dir entrissen.", "tokens": ["Z\u00fcr\u00b7nend", "die", "St\u00e4r\u00b7ke\u00b7re", "dir", "ent\u00b7ris\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "ART", "NN", "PPER", "VVPP", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}}}}