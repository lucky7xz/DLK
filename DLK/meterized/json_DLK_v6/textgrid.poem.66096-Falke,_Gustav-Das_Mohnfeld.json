{"textgrid.poem.66096": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Das Mohnfeld", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es war einmal, ich wei\u00df nicht wann", "tokens": ["Es", "war", "ein\u00b7mal", ",", "ich", "wei\u00df", "nicht", "wann"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "$,", "PPER", "VVFIN", "PTKNEG", "PWAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und wei\u00df nicht wo. Vielleicht ein Traum.", "tokens": ["Und", "wei\u00df", "nicht", "wo", ".", "Viel\u00b7leicht", "ein", "Traum", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "PWAV", "$.", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich trat aus einem schwarzen Tann", "tokens": ["Ich", "trat", "aus", "ei\u00b7nem", "schwar\u00b7zen", "Tann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An einen stillen Wiesensaum.", "tokens": ["An", "ei\u00b7nen", "stil\u00b7len", "Wie\u00b7sen\u00b7saum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und auf der stillen Wiese stand", "tokens": ["Und", "auf", "der", "stil\u00b7len", "Wie\u00b7se", "stand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Rings Mohn bei Mohn und unbewegt,", "tokens": ["Rings", "Mohn", "bei", "Mohn", "und", "un\u00b7be\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "NN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und war bis an den fernsten Rand", "tokens": ["Und", "war", "bis", "an", "den", "ferns\u00b7ten", "Rand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der rote Teppich hingelegt.", "tokens": ["Der", "ro\u00b7te", "Tep\u00b7pich", "hin\u00b7ge\u00b7legt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und auf dem roten Teppich lag,", "tokens": ["Und", "auf", "dem", "ro\u00b7ten", "Tep\u00b7pich", "lag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von tausend Blumen angeblickt,", "tokens": ["Von", "tau\u00b7send", "Blu\u00b7men", "an\u00b7ge\u00b7blickt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein sch\u00f6ner, m\u00fcder Sommertag,", "tokens": ["Ein", "sch\u00f6\u00b7ner", ",", "m\u00fc\u00b7der", "Som\u00b7mer\u00b7tag", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im ersten Schlummer eingenickt.", "tokens": ["Im", "ers\u00b7ten", "Schlum\u00b7mer", "ein\u00b7ge\u00b7nickt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ein Hase kam im Sprung. Erschreckt", "tokens": ["Ein", "Ha\u00b7se", "kam", "im", "Sprung", ".", "Er\u00b7schreckt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$.", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hat er sich tief ins Kraut geduckt,", "tokens": ["Hat", "er", "sich", "tief", "ins", "Kraut", "ge\u00b7duckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "ADJD", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bis an die L\u00f6ffel zugedeckt,", "tokens": ["Bis", "an", "die", "L\u00f6f\u00b7fel", "zu\u00b7ge\u00b7deckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nur einer hat herausgeguckt.", "tokens": ["Nur", "ei\u00b7ner", "hat", "her\u00b7aus\u00b7ge\u00b7guckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Kein Hauch. Kein Laut. Ein Vogelflug", "tokens": ["Kein", "Hauch", ".", "Kein", "Laut", ".", "Ein", "Vo\u00b7gel\u00b7flug"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PIAT", "NN", "$.", "PIAT", "NN", "$.", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bewegte kaum die Abendluft.", "tokens": ["Be\u00b7weg\u00b7te", "kaum", "die", "A\u00b7ben\u00b7dluft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich sah kaum, wie der Fl\u00fcgel schlug,", "tokens": ["Ich", "sah", "kaum", ",", "wie", "der", "Fl\u00fc\u00b7gel", "schlug", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein schwarzer Strich im D\u00e4mmerduft.", "tokens": ["Ein", "schwar\u00b7zer", "Strich", "im", "D\u00e4m\u00b7mer\u00b7duft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Es war einmal, ich wei\u00df nicht wo.", "tokens": ["Es", "war", "ein\u00b7mal", ",", "ich", "wei\u00df", "nicht", "wo", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$,", "PPER", "VVFIN", "PTKNEG", "PWAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Traum vielleicht. Lang ist es her.", "tokens": ["Ein", "Traum", "viel\u00b7leicht", ".", "Lang", "ist", "es", "her", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$.", "NN", "VAFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+++-+", "measure": "unknown.measure.penta"}, "line.3": {"text": "Ich seh nur noch, und immer so,", "tokens": ["Ich", "seh", "nur", "noch", ",", "und", "im\u00b7mer", "so", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$,", "KON", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das stille, rote Blumenmeer.", "tokens": ["Das", "stil\u00b7le", ",", "ro\u00b7te", "Blu\u00b7men\u00b7meer", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}