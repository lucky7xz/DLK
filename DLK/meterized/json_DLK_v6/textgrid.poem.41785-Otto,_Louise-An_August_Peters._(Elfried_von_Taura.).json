{"textgrid.poem.41785": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "An August Peters. (Elfried von Taura.)", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie lag ich gern am Bl\u00fctenhag", "tokens": ["Wie", "lag", "ich", "gern", "am", "Bl\u00fc\u00b7ten\u00b7hag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von Veilchenduft umflossen,", "tokens": ["Von", "Veil\u00b7chen\u00b7duft", "um\u00b7flos\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bei Lerchentriller, Finkenschlag", "tokens": ["Bei", "Ler\u00b7chent\u00b7ril\u00b7ler", ",", "Fin\u00b7ken\u00b7schlag"], "token_info": ["word", "word", "punct", "word"], "pos": ["APPR", "NE", "$,", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und tausend jungen Sprossen.", "tokens": ["Und", "tau\u00b7send", "jun\u00b7gen", "Spros\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wie lauscht ich da in Fr\u00fchlingslust", "tokens": ["Wie", "lauscht", "ich", "da", "in", "Fr\u00fch\u00b7lings\u00b7lust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den D\u00fcften und den Kl\u00e4ngen,", "tokens": ["Den", "D\u00fcf\u00b7ten", "und", "den", "Kl\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wie ward das all', mir kaum bewu\u00dft", "tokens": ["Wie", "ward", "das", "all'", ",", "mir", "kaum", "be\u00b7wu\u00dft"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "ART", "PIS", "$,", "PPER", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Zu t\u00f6nenden Ges\u00e4ngen.", "tokens": ["Zu", "t\u00f6\u00b7nen\u00b7den", "Ge\u00b7s\u00e4n\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wie tr\u00e4umt ich von der Muse Ku\u00df", "tokens": ["Wie", "tr\u00e4umt", "ich", "von", "der", "Mu\u00b7se", "Ku\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und gab mich ihr zu eigen", "tokens": ["Und", "gab", "mich", "ihr", "zu", "ei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PPER", "PTKA", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und lie\u00df der Lieder lauten Gru\u00df,", "tokens": ["Und", "lie\u00df", "der", "Lie\u00b7der", "lau\u00b7ten", "Gru\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In alle Winde steigen!", "tokens": ["In", "al\u00b7le", "Win\u00b7de", "stei\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wie schwor ich stolz, wie schwor ich k\u00fchn:", "tokens": ["Wie", "schwor", "ich", "stolz", ",", "wie", "schwor", "ich", "k\u00fchn", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADJD", "$,", "PWAV", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Muse nur zu leben,", "tokens": ["Der", "Mu\u00b7se", "nur", "zu", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und meines Herzens lodernd Gl\u00fchn,", "tokens": ["Und", "mei\u00b7nes", "Her\u00b7zens", "lo\u00b7dernd", "Gl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVPP", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ihr ungeteilt zu geben!", "tokens": ["Ihr", "un\u00b7ge\u00b7teilt", "zu", "ge\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wie sang ich trotzig in die Welt", "tokens": ["Wie", "sang", "ich", "trot\u00b7zig", "in", "die", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vom heil'gen Frauenrechte,", "tokens": ["Vom", "heil'\u00b7gen", "Frau\u00b7en\u00b7rech\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Stand fest, auf mich allein gestellt,", "tokens": ["Stand", "fest", ",", "auf", "mich", "al\u00b7lein", "ge\u00b7stellt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "APPR", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im brausenden Gefechte.", "tokens": ["Im", "brau\u00b7sen\u00b7den", "Ge\u00b7fech\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wie arg verkannt' ich wankte nicht,", "tokens": ["Wie", "arg", "ver\u00b7kannt'", "ich", "wank\u00b7te", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich blieb auf meiner Stelle.", "tokens": ["Ich", "blieb", "auf", "mei\u00b7ner", "Stel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Erbleichte auch mein Angesicht", "tokens": ["Er\u00b7bleich\u00b7te", "auch", "mein", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das Auge blieb doch helle.", "tokens": ["Das", "Au\u00b7ge", "blieb", "doch", "hel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und sing ich jetzt, so sing ich ", "tokens": ["Und", "sing", "ich", "jetzt", ",", "so", "sing", "ich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,", "ADV", "VVFIN", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dem\u00fctig mich Dir neigend,", "tokens": ["De\u00b7m\u00fc\u00b7tig", "mich", "Dir", "nei\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "PPER", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Lieb' ist meines Liedes Zier,", "tokens": ["Die", "Lieb'", "ist", "mei\u00b7nes", "Lie\u00b7des", "Zier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zum Himmel jubelnd steigend.", "tokens": ["Zum", "Him\u00b7mel", "ju\u00b7belnd", "stei\u00b7gend", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und denk ich an den stolzen Eid:", "tokens": ["Und", "denk", "ich", "an", "den", "stol\u00b7zen", "Eid", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Muse nur zu leben \u2013", "tokens": ["Der", "Mu\u00b7se", "nur", "zu", "le\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Noch stolzer ist die Seligkeit", "tokens": ["Noch", "stol\u00b7zer", "ist", "die", "Se\u00b7lig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mich Dir zur Muse geben.", "tokens": ["Mich", "Dir", "zur", "Mu\u00b7se", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und stolzer jetzt Dein Weib zu sein,", "tokens": ["Und", "stol\u00b7zer", "jetzt", "Dein", "Weib", "zu", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "ADV", "PPOSAT", "NN", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und Dich mit Lorbern schm\u00fccken", "tokens": ["Und", "Dich", "mit", "Lor\u00b7bern", "schm\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPR", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Als wollten Andre ", "tokens": ["Als", "woll\u00b7ten", "And\u00b7re"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "VMFIN", "PIS"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Auf ", "tokens": ["Auf"], "token_info": ["word"], "pos": ["APPR"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Und wenn ein Zweig davon mir blieb,", "tokens": ["Und", "wenn", "ein", "Zweig", "da\u00b7von", "mir", "blieb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den reich' ich Dir mit Kosen,", "tokens": ["Den", "reich'", "ich", "Dir", "mit", "Ko\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Du giebst daf\u00fcr in Deiner Lieb'", "tokens": ["Du", "giebst", "da\u00b7f\u00fcr", "in", "Dei\u00b7ner", "Lieb'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PAV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mir Myrten ja und Rosen!", "tokens": ["Mir", "Myr\u00b7ten", "ja", "und", "Ro\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADV", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}