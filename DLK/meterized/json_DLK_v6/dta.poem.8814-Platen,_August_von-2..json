{"dta.poem.8814": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Habt ihr nie gesehn im Walde, da\u00df auf tr\u00fcbem Wasser-", "tokens": ["Habt", "ihr", "nie", "ge\u00b7sehn", "im", "Wal\u00b7de", ",", "da\u00df", "auf", "tr\u00fc\u00b7bem", "Was\u00b7ser"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "VVPP", "APPRART", "NN", "$,", "KOUS", "APPR", "ADJA", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "Eine Lilie bescheiden, mit unz\u00e4hl'gen Bl\u00fcthen, schwamm?", "tokens": ["Ei\u00b7ne", "Li\u00b7lie", "be\u00b7schei\u00b7den", ",", "mit", "un\u00b7z\u00e4hl'\u00b7gen", "Bl\u00fc\u00b7then", ",", "schwamm", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "APPR", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "+-+--+--+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Dieses Volks geschw\u00e4tz'ge Leere gleicht gestand'nem, trock-", "tokens": ["Die\u00b7ses", "Volks", "ge\u00b7schw\u00e4tz'\u00b7ge", "Lee\u00b7re", "gleicht", "ge\u00b7stan\u00b7d'\u00b7nem", ",", "trock"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PDAT", "NN", "ADJA", "NN", "VVFIN", "ADJA", "$,", "TRUNC"], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.4": {"text": "Deines Wesens ew'ge Jugend ist des Lebens gr\u00fcner", "tokens": ["Dei\u00b7nes", "We\u00b7sens", "ew'\u00b7ge", "Ju\u00b7gend", "ist", "des", "Le\u00b7bens", "gr\u00fc\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "VAFIN", "ART", "NN", "ADJA"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}}}}}