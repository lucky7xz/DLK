{"dta.poem.16062": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "116.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was ich geworden bin, bin ich durch dich geworden;", "tokens": ["Was", "ich", "ge\u00b7wor\u00b7den", "bin", ",", "bin", "ich", "durch", "dich", "ge\u00b7wor\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VAPP", "VAFIN", "$,", "VAFIN", "PPER", "APPR", "PPER", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du ordnest um dich her nach Wahl der Geister Orden.", "tokens": ["Du", "ord\u00b7nest", "um", "dich", "her", "nach", "Wahl", "der", "Geis\u00b7ter", "Or\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PRF", "APZR", "APPR", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Den einen ziehst du vor, und stellest den zur\u00fcck,", "tokens": ["Den", "ei\u00b7nen", "ziehst", "du", "vor", ",", "und", "stel\u00b7lest", "den", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "VVFIN", "ART", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und dieser auch entbehrt nicht sein bescheidnes Gl\u00fcck.", "tokens": ["Und", "die\u00b7ser", "auch", "ent\u00b7behrt", "nicht", "sein", "be\u00b7scheid\u00b7nes", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "ADV", "VVFIN", "PTKNEG", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Der, welchen du erh\u00f6hst, wird von der Welt erhoben,", "tokens": ["Der", ",", "wel\u00b7chen", "du", "er\u00b7h\u00f6hst", ",", "wird", "von", "der", "Welt", "er\u00b7ho\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PWAT", "PPER", "VVFIN", "$,", "VAFIN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und der am tiefsten steht, kann dich den H\u00f6chsten loben.", "tokens": ["Und", "der", "am", "tiefs\u00b7ten", "steht", ",", "kann", "dich", "den", "H\u00f6chs\u00b7ten", "lo\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPRART", "ADJA", "VVFIN", "$,", "VMFIN", "PRF", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Den einen f\u00fchrest du des Kampfes rauhe Bahn,", "tokens": ["Den", "ei\u00b7nen", "f\u00fch\u00b7rest", "du", "des", "Kamp\u00b7fes", "rau\u00b7he", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "PPER", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den andern hebest du auf Fl\u00fcgeln leicht hinan.", "tokens": ["Den", "an\u00b7dern", "he\u00b7best", "du", "auf", "Fl\u00fc\u00b7geln", "leicht", "hi\u00b7nan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PPER", "APPR", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Nicht soll sich der des Kampfs, noch der des Fluges br\u00fcsten;", "tokens": ["Nicht", "soll", "sich", "der", "des", "Kampfs", ",", "noch", "der", "des", "Flu\u00b7ges", "br\u00fcs\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VMFIN", "PRF", "ART", "ART", "NN", "$,", "ADV", "ART", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du mu\u00dftest den mit Kraft, und den mit Schwingen r\u00fcsten.", "tokens": ["Du", "mu\u00df\u00b7test", "den", "mit", "Kraft", ",", "und", "den", "mit", "Schwin\u00b7gen", "r\u00fcs\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "APPR", "NN", "$,", "KON", "ART", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Und keiner br\u00fcsten soll vor keinem sich der beiden;", "tokens": ["Und", "kei\u00b7ner", "br\u00fcs\u00b7ten", "soll", "vor", "kei\u00b7nem", "sich", "der", "bei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVINF", "VMFIN", "APPR", "PIAT", "PRF", "ART", "PIAT", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bewundern will ich den, und diesen nicht beneiden.", "tokens": ["Be\u00b7wun\u00b7dern", "will", "ich", "den", ",", "und", "die\u00b7sen", "nicht", "be\u00b7nei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ART", "$,", "KON", "PDS", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Ich seh gleichhoch gestellt sie auf verschiednen H\u00f6hn;", "tokens": ["Ich", "seh", "gleich\u00b7hoch", "ge\u00b7stellt", "sie", "auf", "ver\u00b7schied\u00b7nen", "H\u00f6hn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "VVPP", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erhaben ist der Kampf, und G\u00f6ttergl\u00fcck ist sch\u00f6n.", "tokens": ["Er\u00b7ha\u00b7ben", "ist", "der", "Kampf", ",", "und", "G\u00f6t\u00b7ter\u00b7gl\u00fcck", "ist", "sch\u00f6n", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$,", "KON", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Preis dem, der seine Kraft, dem, der sein Gl\u00fcck erkennt,", "tokens": ["Preis", "dem", ",", "der", "sei\u00b7ne", "Kraft", ",", "dem", ",", "der", "sein", "Gl\u00fcck", "er\u00b7kennt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "$,", "PRELS", "PPOSAT", "NN", "$,", "PRELS", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und sie nicht sein, sie dein, dankbar erkennend, nennt.", "tokens": ["Und", "sie", "nicht", "sein", ",", "sie", "dein", ",", "dank\u00b7bar", "er\u00b7ken\u00b7nend", ",", "nennt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "PTKNEG", "VAINF", "$,", "PPER", "PPOSAT", "$,", "ADJD", "VVPP", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}