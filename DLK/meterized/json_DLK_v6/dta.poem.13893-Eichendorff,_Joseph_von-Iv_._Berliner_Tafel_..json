{"dta.poem.13893": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Iv .  \n  Berliner Tafel .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.71", "af:0.28"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "So sind viele hier gesellt:", "tokens": ["So", "sind", "vie\u00b7le", "hier", "ge\u00b7sellt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "R\u00fcstige Gesellen,", "tokens": ["R\u00fcs\u00b7ti\u00b7ge", "Ge\u00b7sel\u00b7len", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Die ihr' Sach auf Klang gestellt,", "tokens": ["Die", "ih\u00b7r'", "Sach", "auf", "Klang", "ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schauspiel und Novellen,", "tokens": ["Schau\u00b7spiel", "und", "No\u00b7vel\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Viele dann, die recht sich freun,", "tokens": ["Vie\u00b7le", "dann", ",", "die", "recht", "sich", "freun", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "$,", "PRELS", "ADJD", "PRF", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wenn wir's l\u00f6blich machen,", "tokens": ["Wenn", "wir's", "l\u00f6b\u00b7lich", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Und, greift einer falsch darein,", "tokens": ["Und", ",", "greift", "ei\u00b7ner", "falsch", "da\u00b7rein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "ART", "ADJD", "PAV", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Auch von Herzen lachen.", "tokens": ["Auch", "von", "Her\u00b7zen", "la\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}