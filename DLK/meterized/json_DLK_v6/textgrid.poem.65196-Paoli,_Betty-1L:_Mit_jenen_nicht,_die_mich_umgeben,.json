{"textgrid.poem.65196": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mit jenen nicht, die mich umgeben,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit jenen nicht, die mich umgeben,", "tokens": ["Mit", "je\u00b7nen", "nicht", ",", "die", "mich", "um\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PDS", "PTKNEG", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Verbring' ich diesen Rest von Leben,", "tokens": ["Ver\u00b7bring'", "ich", "die\u00b7sen", "Rest", "von", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PDAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nein! mit der Heimgegang'nen Schaar.", "tokens": ["Nein", "!", "mit", "der", "Heim\u00b7ge\u00b7gang'\u00b7nen", "Schaar", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit ihnen, die in fernen Tagen", "tokens": ["Mit", "ih\u00b7nen", ",", "die", "in", "fer\u00b7nen", "Ta\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Mich sah'n in meiner Bl\u00fcte ragen,", "tokens": ["Mich", "sah'n", "in", "mei\u00b7ner", "Bl\u00fc\u00b7te", "ra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und deren Zeit die meine war!", "tokens": ["Und", "de\u00b7ren", "Zeit", "die", "mei\u00b7ne", "war", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRELAT", "NN", "ART", "PPOSAT", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Beim Fest, im dicht gedr\u00e4ngten Saale,", "tokens": ["Beim", "Fest", ",", "im", "dicht", "ge\u00b7dr\u00e4ng\u00b7ten", "Saa\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Im stillen Wald beim Mondesstrahle", "tokens": ["Im", "stil\u00b7len", "Wald", "beim", "Mon\u00b7des\u00b7strah\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Verfolg' ich tr\u00e4umend ihre Spur;", "tokens": ["Ver\u00b7fol\u00b7g'", "ich", "tr\u00e4u\u00b7mend", "ih\u00b7re", "Spur", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und hier wie dort, auf allen Wegen", "tokens": ["Und", "hier", "wie", "dort", ",", "auf", "al\u00b7len", "We\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "KOKOM", "ADV", "$,", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Tritt mir vertraut ihr Bild entgegen,", "tokens": ["Tritt", "mir", "ver\u00b7traut", "ihr", "Bild", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Nur reiner und verkl\u00e4rter nur!", "tokens": ["Nur", "rei\u00b7ner", "und", "ver\u00b7kl\u00e4r\u00b7ter", "nur", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "KON", "ADJD", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und aus dem Mund der teuren Schemen", "tokens": ["Und", "aus", "dem", "Mund", "der", "teu\u00b7ren", "Sche\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mein' ich die Frage zu vernehmen,", "tokens": ["Mein'", "ich", "die", "Fra\u00b7ge", "zu", "ver\u00b7neh\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die mir im eig'nen Herzen brennt:", "tokens": ["Die", "mir", "im", "eig'\u00b7nen", "Her\u00b7zen", "brennt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u00bballein, allein auf dieser Erde,", "tokens": ["\u00bb", "al\u00b7lein", ",", "al\u00b7lein", "auf", "die\u00b7ser", "Er\u00b7de", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$,", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "-+-----+-", "measure": "dactylic.init"}, "line.5": {"text": "Was hoffst du wohl, da\u00df dir noch werde?", "tokens": ["Was", "hoffst", "du", "wohl", ",", "da\u00df", "dir", "noch", "wer\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Was h\u00e4lt dich noch von uns getrennt?\u00ab", "tokens": ["Was", "h\u00e4lt", "dich", "noch", "von", "uns", "ge\u00b7trennt", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "APPR", "PPER", "VVPP", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}