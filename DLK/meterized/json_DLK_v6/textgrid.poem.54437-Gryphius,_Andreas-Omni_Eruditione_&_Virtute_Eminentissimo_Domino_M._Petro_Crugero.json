{"textgrid.poem.54437": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "Omni Eruditione & Virtute Eminentissimo Domino M. Petro Crugero", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nembt wieder jtzund an/ was Ihr mir habt vortrawet/", "tokens": ["Nembt", "wie\u00b7der", "jt\u00b7zund", "an", "/", "was", "Ihr", "mir", "habt", "vor\u00b7tra\u00b7wet", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "PTKVZ", "$(", "PWS", "PPER", "PPER", "VAFIN", "VVPP", "$("], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "(ihr Wonhau\u00df trewer Gunst/ Ihr Richtschnur aller Zeit/", "tokens": ["(", "ihr", "Won\u00b7hau\u00df", "tre\u00b7wer", "Gunst", "/", "Ihr", "Richt\u00b7schnur", "al\u00b7ler", "Zeit", "/"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "ADJA", "NN", "$(", "PPOSAT", "NN", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Ihr der Sonnen selbst auch eine Sonne seyd/)", "tokens": ["Der", "Ihr", "der", "Son\u00b7nen", "selbst", "auch", "ei\u00b7ne", "Son\u00b7ne", "seyd", "/", ")"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "PPER", "ART", "NN", "ADV", "ADV", "ART", "NN", "VAFIN", "$(", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Di\u00df/ was man hier vnd dar/ auff manchem Grabe schawet/", "tokens": ["Di\u00df", "/", "was", "man", "hier", "vnd", "dar", "/", "auff", "man\u00b7chem", "Gra\u00b7be", "scha\u00b7wet", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$(", "PWS", "PIS", "ADV", "KON", "PTKVZ", "$(", "APPR", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+--+--+--+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Ewr Wei\u00dfheit/ welcher nicht vorm End vnd Sterben grawet/", "tokens": ["Ewr", "Wei\u00df\u00b7heit", "/", "wel\u00b7cher", "nicht", "vorm", "End", "vnd", "Ster\u00b7ben", "gra\u00b7wet", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "PRELS", "PTKNEG", "APPRART", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.6": {"text": "Ewr Ehre/ die der Ruhm an allem Ort vmbstrewt/", "tokens": ["Ewr", "Eh\u00b7re", "/", "die", "der", "Ruhm", "an", "al\u00b7lem", "Ort", "vmbstrewt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "PRELS", "ART", "NN", "APPR", "PIS", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ewr Tugend/ der vmbsonst des Todes stachel drewt/", "tokens": ["Ewr", "Tu\u00b7gend", "/", "der", "vmbsonst", "des", "To\u00b7des", "sta\u00b7chel", "drewt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "ART", "NN", "ART", "NN", "NE", "VVFIN", "$("], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "V\u00f1 Ewre Freundligkeit find hier kein Grab gebawet/", "tokens": ["V\u00f1", "Ew\u00b7re", "Freund\u00b7lig\u00b7keit", "find", "hier", "kein", "Grab", "ge\u00b7ba\u00b7wet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "VVFIN", "ADV", "PIAT", "NN", "VVPP", "$("], "meter": "-+-+--+--+--+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Ihn wird kein Stein gesetzt/ weil vber sie das Rad", "tokens": ["Ihn", "wird", "kein", "Stein", "ge\u00b7setzt", "/", "weil", "vber", "sie", "das", "Rad"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "VVPP", "$(", "KOUS", "APPR", "PPER", "ART", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.10": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.11": {"text": "Wo aber doch mein Sinn so sehr verkehrt mag werden/", "tokens": ["Wo", "a\u00b7ber", "doch", "mein", "Sinn", "so", "sehr", "ver\u00b7kehrt", "mag", "wer\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "PPOSAT", "NN", "ADV", "ADV", "ADJD", "VMFIN", "VAINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Da\u00df Ich mich vntersteh zu \u00f6ffnen Ihn die Grufft/", "tokens": ["Da\u00df", "Ich", "mich", "vn\u00b7ter\u00b7steh", "zu", "\u00f6ff\u00b7nen", "Ihn", "die", "Grufft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "PTKZU", "VVINF", "PPER", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "In welcher Vndanckt sitzt/ zu dem Vergessen rufft/", "tokens": ["In", "wel\u00b7cher", "Vn\u00b7danckt", "sitzt", "/", "zu", "dem", "Ver\u00b7ges\u00b7sen", "rufft", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "VVFIN", "$(", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "So mu\u00df Ich lebendig vergraben sein mit Erden.", "tokens": ["So", "mu\u00df", "Ich", "le\u00b7ben\u00b7dig", "ver\u00b7gra\u00b7ben", "sein", "mit", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "VVPP", "VAINF", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}