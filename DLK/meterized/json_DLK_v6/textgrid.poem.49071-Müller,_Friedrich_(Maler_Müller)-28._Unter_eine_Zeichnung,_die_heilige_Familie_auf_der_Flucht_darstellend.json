{"textgrid.poem.49071": {"metadata": {"author": {"name": "M\u00fcller, Friedrich (Maler M\u00fcller)", "birth": "N.A.", "death": "N.A."}, "title": "28. Unter eine Zeichnung, die heilige Familie auf der Flucht darstellend", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Umringt von Feindesschlingen schreitet", "tokens": ["Um\u00b7ringt", "von", "Fein\u00b7des\u00b7schlin\u00b7gen", "schrei\u00b7tet"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Noch ruhig der Gerechte seine Bahn.", "tokens": ["Noch", "ru\u00b7hig", "der", "Ge\u00b7rech\u00b7te", "sei\u00b7ne", "Bahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+---+-+-+", "measure": "dactylic.init"}, "line.3": {"text": "In Not reicht Schutz und Labung", "tokens": ["In", "Not", "reicht", "Schutz", "und", "La\u00b7bung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der schroffe Fels ihm mild, und Liebe", "tokens": ["Der", "schrof\u00b7fe", "Fels", "ihm", "mild", ",", "und", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "PPER", "ADJD", "$,", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Gepaart mit Menschlichkeit begegnen", "tokens": ["Ge\u00b7paart", "mit", "Menschlich\u00b7keit", "be\u00b7geg\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Auf \u00f6der Haide ihm, ihn leitend", "tokens": ["Auf", "\u00f6\u00b7der", "Hai\u00b7de", "ihm", ",", "ihn", "lei\u00b7tend"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "NN", "PPER", "$,", "PPER", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Am steilen Abgrund hin, denn \u00fcberall", "tokens": ["Am", "stei\u00b7len", "Ab\u00b7grund", "hin", ",", "denn", "\u00fc\u00b7be\u00b7rall"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "PTKVZ", "$,", "KON", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Deckt freundlich ihn der Vorsicht sicherer Fl\u00fcgel.", "tokens": ["Deckt", "freund\u00b7lich", "ihn", "der", "Vor\u00b7sicht", "si\u00b7che\u00b7rer", "Fl\u00fc\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}}}}