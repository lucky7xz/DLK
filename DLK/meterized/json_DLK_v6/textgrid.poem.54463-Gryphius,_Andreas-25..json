{"textgrid.poem.54463": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "25.", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ob gleich der weisse schnee itzt thall' vnd berge decket/", "tokens": ["Ob", "gleich", "der", "weis\u00b7se", "schnee", "itzt", "thall'", "vnd", "ber\u00b7ge", "de\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "ADV", "VVFIN", "KON", "VVFIN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ob mancher schneller flus zeucht einen harnisch an/", "tokens": ["Ob", "man\u00b7cher", "schnel\u00b7ler", "flus", "zeucht", "ei\u00b7nen", "har\u00b7nisch", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "ADJA", "NN", "VVFIN", "ART", "ADJD", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "In dem er sich des zorns der k\u00e4lt erwehren kan/", "tokens": ["In", "dem", "er", "sich", "des", "zorns", "der", "k\u00e4lt", "er\u00b7weh\u00b7ren", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PRF", "ART", "NN", "ART", "VVFIN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vor welcher jeder baum steht bis in tod erschrecket;", "tokens": ["Vor", "wel\u00b7cher", "je\u00b7der", "baum", "steht", "bis", "in", "tod", "er\u00b7schre\u00b7cket", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PIS", "ADV", "VVFIN", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ob gleich der bleiche frost die scharffe seens au\u00dfstrecket", "tokens": ["Ob", "gleich", "der", "blei\u00b7che", "frost", "die", "scharf\u00b7fe", "seens", "au\u00df\u00b7str\u00b7e\u00b7cket"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "ART", "VVFIN", "ADV", "VVFIN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Vnd alle blumen f\u00e4llt/ die ", "tokens": ["Vnd", "al\u00b7le", "blu\u00b7men", "f\u00e4llt", "/", "die"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["KON", "PIAT", "NN", "VVFIN", "$(", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Der Erden schawen lies/ hat liebe doch gethan", "tokens": ["Der", "Er\u00b7den", "scha\u00b7wen", "lies", "/", "hat", "lie\u00b7be", "doch", "ge\u00b7than"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "VVFIN", "$(", "VAFIN", "VVFIN", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Mehr den die sonne selbst/ vnd hitz vnd lust erwecket/", "tokens": ["Mehr", "den", "die", "son\u00b7ne", "selbst", "/", "vnd", "hitz", "vnd", "lust", "er\u00b7we\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ART", "NN", "ADV", "$(", "KON", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Das ihr des winters sch\u00e4rff vnd rasen sicher lacht.", "tokens": ["Das", "ihr", "des", "win\u00b7ters", "sch\u00e4rff", "vnd", "ra\u00b7sen", "si\u00b7cher", "lacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ART", "NN", "VVFIN", "KON", "VVFIN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die lieb' Herr Godfrid hatt euch diese rose bracht.", "tokens": ["Die", "lieb'", "Herr", "God\u00b7frid", "hatt", "euch", "die\u00b7se", "ro\u00b7se", "bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NN", "NE", "VAFIN", "PPER", "PDAT", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Woll euch! vnd mehr den woll! was m\u00f6gt ihr noch begehren?", "tokens": ["Woll", "euch", "!", "vnd", "mehr", "den", "woll", "!", "was", "m\u00f6gt", "ihr", "noch", "be\u00b7geh\u00b7ren", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$.", "KON", "ADV", "ART", "ADV", "$.", "PWS", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Woll euch vnd mehr den woll! wen diese rawe zeit", "tokens": ["Woll", "euch", "vnd", "mehr", "den", "woll", "!", "wen", "die\u00b7se", "ra\u00b7we", "zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "KON", "ADV", "ART", "ADV", "$.", "PWS", "PDAT", "ADJA", "NN"], "meter": "+-----+-+-+-", "measure": "dactylic.init"}, "line.13": {"text": "So sch\u00f6ne blumen gibt vnd solche lust bereitt", "tokens": ["So", "sch\u00f6\u00b7ne", "blu\u00b7men", "gibt", "vnd", "sol\u00b7che", "lust", "be\u00b7reitt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "KON", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Was wird' euch nicht der herbst f\u00fcr s\u00fcsse frucht gewehren.", "tokens": ["Was", "wird'", "euch", "nicht", "der", "herbst", "f\u00fcr", "s\u00fcs\u00b7se", "frucht", "ge\u00b7weh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "PTKNEG", "ART", "ADV", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}