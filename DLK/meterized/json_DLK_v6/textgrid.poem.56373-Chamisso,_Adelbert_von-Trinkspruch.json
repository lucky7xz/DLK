{"textgrid.poem.56373": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "Trinkspruch", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O lasset uns in dieser d\u00fcstern, bangen Zeit,", "tokens": ["O", "las\u00b7set", "uns", "in", "die\u00b7ser", "d\u00fcs\u00b7tern", ",", "ban\u00b7gen", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "APPR", "PDAT", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo hochanschwellend, donnernd der Geschichte Strom", "tokens": ["Wo", "hoch\u00b7an\u00b7schwel\u00b7lend", ",", "don\u00b7nernd", "der", "Ge\u00b7schich\u00b7te", "Strom"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "VVPP", "$,", "ADJD", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die starren langgehegten Eisesfesseln sprengt,", "tokens": ["Die", "star\u00b7ren", "lang\u00b7ge\u00b7heg\u00b7ten", "Ei\u00b7ses\u00b7fes\u00b7seln", "sprengt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das neue Leben unter Tr\u00fcmmern bricht hervor,", "tokens": ["Das", "neu\u00b7e", "Le\u00b7ben", "un\u00b7ter", "Tr\u00fcm\u00b7mern", "bricht", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und sich in St\u00fcrmen umgestalten will die Welt;", "tokens": ["Und", "sich", "in", "St\u00fcr\u00b7men", "um\u00b7ge\u00b7stal\u00b7ten", "will", "die", "Welt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "APPR", "NN", "VVINF", "VMFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "O lasset uns, ihr Freunde, \u2013 rings verhallt das Lied", "tokens": ["O", "las\u00b7set", "uns", ",", "ihr", "Freun\u00b7de", ",", "\u2013", "rings", "ver\u00b7hallt", "das", "Lied"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "$(", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und unserm heitern Saitenspiele lauscht kein Ohr, \u2013", "tokens": ["Und", "un\u00b7serm", "hei\u00b7tern", "Sai\u00b7ten\u00b7spie\u00b7le", "lauscht", "kein", "Ohr", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "PIAT", "NN", "$,", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Dennoch die Gottesgabe des Gesanges treu", "tokens": ["Den\u00b7noch", "die", "Got\u00b7tes\u00b7ga\u00b7be", "des", "Ge\u00b7san\u00b7ges", "treu"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ART", "NN", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Im reinen Busen hegen, wahren; da\u00df vielleicht", "tokens": ["Im", "rei\u00b7nen", "Bu\u00b7sen", "he\u00b7gen", ",", "wah\u00b7ren", ";", "da\u00df", "viel\u00b7leicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$,", "VVINF", "$.", "KOUS", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wir, hochergraute Barden, einst die Sonne noch", "tokens": ["Wir", ",", "ho\u00b7cher\u00b7grau\u00b7te", "Bar\u00b7den", ",", "einst", "die", "Son\u00b7ne", "noch"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "ADJA", "NN", "$,", "ADV", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Mit Hochgesang begr\u00fc\u00dfen, welche das Gew\u00f6lk", "tokens": ["Mit", "Hoch\u00b7ge\u00b7sang", "be\u00b7gr\u00fc\u00b7\u00dfen", ",", "wel\u00b7che", "das", "Ge\u00b7w\u00f6lk"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "VVINF", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Zerteilend die verj\u00fcngte Welt bescheinen wird.", "tokens": ["Zer\u00b7tei\u00b7lend", "die", "ver\u00b7j\u00fcng\u00b7te", "Welt", "be\u00b7schei\u00b7nen", "wird", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "ART", "ADJA", "NN", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Prophetisch, Freunde, bring ich dieses volle Glas", "tokens": ["Pro\u00b7phe\u00b7tisch", ",", "Freun\u00b7de", ",", "bring", "ich", "die\u00b7ses", "vol\u00b7le", "Glas"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "VVFIN", "PPER", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Der fernen Zukunft einer andern Liederzeit!", "tokens": ["Der", "fer\u00b7nen", "Zu\u00b7kunft", "ei\u00b7ner", "an\u00b7dern", "Lie\u00b7der\u00b7zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}