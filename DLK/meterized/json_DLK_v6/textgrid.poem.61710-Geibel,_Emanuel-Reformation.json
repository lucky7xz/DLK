{"textgrid.poem.61710": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Reformation", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Woll' uns deinen Tr\u00f6ster senden,", "tokens": ["Woll'", "uns", "dei\u00b7nen", "Tr\u00f6s\u00b7ter", "sen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Herr, in dieser schweren Zeit,", "tokens": ["Herr", ",", "in", "die\u00b7ser", "schwe\u00b7ren", "Zeit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da die Welt an allen Enden", "tokens": ["Da", "die", "Welt", "an", "al\u00b7len", "En\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Durstig nach Erl\u00f6sung schreit!", "tokens": ["Durs\u00b7tig", "nach", "Er\u00b7l\u00f6\u00b7sung", "schreit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn es geht ein heilig Sehnen", "tokens": ["Denn", "es", "geht", "ein", "hei\u00b7lig", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Durch der V\u00f6lker bangen Sinn,", "tokens": ["Durch", "der", "V\u00f6l\u00b7ker", "ban\u00b7gen", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und sie seufzen unter Tr\u00e4nen:", "tokens": ["Und", "sie", "seuf\u00b7zen", "un\u00b7ter", "Tr\u00e4\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "H\u00fcter, ist die Nacht bald hin?", "tokens": ["H\u00fc\u00b7ter", ",", "ist", "die", "Nacht", "bald", "hin", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ach, sie f\u00fchlen's: Alles Wissen,", "tokens": ["Ach", ",", "sie", "f\u00fch\u00b7len's", ":", "Al\u00b7les", "Wis\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "$.", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ob's den Stoff der Welt umfa\u00dft,", "tokens": ["Ob's", "den", "Stoff", "der", "Welt", "um\u00b7fa\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bringt, vom Ew'gen losgerissen,", "tokens": ["Bringt", ",", "vom", "Ew'\u00b7gen", "los\u00b7ge\u00b7ris\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kein Gen\u00fcgen, keine Rast.", "tokens": ["Kein", "Ge\u00b7n\u00fc\u00b7gen", ",", "kei\u00b7ne", "Rast", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch die Suchenden, Beschwerten", "tokens": ["Doch", "die", "Su\u00b7chen\u00b7den", ",", "Be\u00b7schwer\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["KON", "ART", "NN", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Treibt levitisch Schwertgez\u00fcck,", "tokens": ["Treibt", "le\u00b7vi\u00b7tisch", "Schwert\u00b7ge\u00b7z\u00fcck", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Treibt der Spruch der Schriftgelehrten", "tokens": ["Treibt", "der", "Spruch", "der", "Schrift\u00b7ge\u00b7lehr\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Hart und eng in sich zur\u00fcck.", "tokens": ["Hart", "und", "eng", "in", "sich", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "APPR", "PRF", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Was einst Trost und Heil den Massen,", "tokens": ["Was", "einst", "Trost", "und", "Heil", "den", "Mas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "NN", "KON", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ward zur Satzung dumpf und schwer;", "tokens": ["Ward", "zur", "Sat\u00b7zung", "dumpf", "und", "schwer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dieser Kirche Formen fassen", "tokens": ["Die\u00b7ser", "Kir\u00b7che", "For\u00b7men", "fas\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PDAT", "NN", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dein Geheimnis, Herr, nicht mehr.", "tokens": ["Dein", "Ge\u00b7heim\u00b7nis", ",", "Herr", ",", "nicht", "mehr", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "$,", "PTKNEG", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Tausenden, die fromm dich rufen,", "tokens": ["Tau\u00b7sen\u00b7den", ",", "die", "fromm", "dich", "ru\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADJD", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Weigert sie den Gnadenscho\u00df,", "tokens": ["Wei\u00b7gert", "sie", "den", "Gna\u00b7den\u00b7scho\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wandle denn, was Menschen schufen,", "tokens": ["Wand\u00b7le", "denn", ",", "was", "Men\u00b7schen", "schu\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PWS", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Denn nur du bist wandellos.", "tokens": ["Denn", "nur", "du", "bist", "wan\u00b7del\u00b7los", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Aus dem dunkeln Schriftbuchstaben,", "tokens": ["Aus", "dem", "dun\u00b7keln", "Schrift\u00b7buch\u00b7sta\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus der Lehr' erstarrter Haft,", "tokens": ["Aus", "der", "Lehr'", "er\u00b7starr\u00b7ter", "Haft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Drin der heil'ge Geist begraben,", "tokens": ["Drin", "der", "heil'\u00b7ge", "Geist", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "La\u00df ihn auferstehn in Kraft!", "tokens": ["La\u00df", "ihn", "auf\u00b7er\u00b7stehn", "in", "Kraft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVIZU", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "La\u00df ihn \u00fcbers Rund der Erde", "tokens": ["La\u00df", "ihn", "\u00fc\u00b7bers", "Rund", "der", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "ADJD", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wieder fluten froh und frei,", "tokens": ["Wie\u00b7der", "flu\u00b7ten", "froh", "und", "frei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Da\u00df das Glauben Leben werde,", "tokens": ["Da\u00df", "das", "Glau\u00b7ben", "Le\u00b7ben", "wer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und die Tat Bekenntnis sei!", "tokens": ["Und", "die", "Tat", "Be\u00b7kennt\u00b7nis", "sei", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Flammend zeug' er, was vereinigt", "tokens": ["Flam\u00b7mend", "zeug'", "er", ",", "was", "ver\u00b7ei\u00b7nigt"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "$,", "PRELS", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einst der Boten Mund get\u00f6nt,", "tokens": ["Einst", "der", "Bo\u00b7ten", "Mund", "ge\u00b7t\u00f6nt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie's, vom Zeitlichen gereinigt,", "tokens": ["Wie's", ",", "vom", "Zeit\u00b7li\u00b7chen", "ge\u00b7rei\u00b7nigt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich dem Menschengeist vers\u00f6hnt!", "tokens": ["Sich", "dem", "Men\u00b7schen\u00b7geist", "ver\u00b7s\u00f6hnt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Zeug' es, bis vor solcher Kunde", "tokens": ["Zeug'", "es", ",", "bis", "vor", "sol\u00b7cher", "Kun\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "$,", "KOUS", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Jede Zweifelstimme schweigt,", "tokens": ["Je\u00b7de", "Zwei\u00b7fel\u00b7stim\u00b7me", "schweigt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und empor vom alten Grunde", "tokens": ["Und", "em\u00b7por", "vom", "al\u00b7ten", "Grun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PTKVZ", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Frei die neue Kirche steigt.", "tokens": ["Frei", "die", "neu\u00b7e", "Kir\u00b7che", "steigt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}