{"textgrid.poem.39670": {"metadata": {"author": {"name": "G\u00fcnderode, Karoline von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mora, seine Geliebte.", "genre": "verse", "period": "N.A.", "pub_year": 1793, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mora, seine Geliebte.", "tokens": ["Mo\u00b7ra", ",", "sei\u00b7ne", "Ge\u00b7lieb\u00b7te", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.2": {"line.1": {"text": "Karmor, ein Krieger.", "tokens": ["Kar\u00b7mor", ",", "ein", "Krie\u00b7ger", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Thormod,", "tokens": ["Thor\u00b7mod", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}}, "stanza.4": {"line.1": {"text": "Carul, Barden.", "tokens": ["Ca\u00b7rul", ",", "Bar\u00b7den", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.5": {"line.1": {"text": "Gr\u00e4\u00dflich rollt der Donner, die Erde zittert, aber Frothal zittert nicht.", "tokens": ["Gr\u00e4\u00df\u00b7lich", "rollt", "der", "Don\u00b7ner", ",", "die", "Er\u00b7de", "zit\u00b7tert", ",", "a\u00b7ber", "Frot\u00b7hal", "zit\u00b7tert", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$,", "KON", "NN", "VVFIN", "PTKNEG", "$."], "meter": "+-+-+--+-+-+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.6": {"line.1": {"text": "Angenehm ist meinem Ohre euer Gesang, ihr Barden des Liedes.", "tokens": ["An\u00b7ge\u00b7nehm", "ist", "mei\u00b7nem", "Oh\u00b7re", "eu\u00b7er", "Ge\u00b7sang", ",", "ihr", "Bar\u00b7den", "des", "Lie\u00b7des", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPOSAT", "NN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+--+-+--+-", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.7": {"line.1": {"text": "Stirbst du mit Ruhm und Liebe, so starbst du doch Frothal f\u00fcr mich.", "tokens": ["Stirbst", "du", "mit", "Ruhm", "und", "Lie\u00b7be", ",", "so", "starbst", "du", "doch", "Frot\u00b7hal", "f\u00fcr", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$,", "ADV", "VVFIN", "PPER", "ADV", "NN", "APPR", "PPER", "$."], "meter": "+--+-+--+--+--+", "measure": "iambic.hexa.invert"}}, "stanza.8": {"line.1": {"text": "Ja er mu\u00df hier seyn, hier in der H\u00f6hle. Frothal! komm!", "tokens": ["Ja", "er", "mu\u00df", "hier", "seyn", ",", "hier", "in", "der", "H\u00f6h\u00b7le", ".", "Frot\u00b7hal", "!", "komm", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PTKANT", "PPER", "VMFIN", "ADV", "VAINF", "$,", "ADV", "APPR", "ART", "NN", "$.", "NN", "$.", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}}, "stanza.9": {"line.1": {"text": "Was willst du von Frothal? Warum verscheucht deine Stimme den Schlummer?", "tokens": ["Was", "willst", "du", "von", "Frot\u00b7hal", "?", "Wa\u00b7rum", "ver\u00b7scheucht", "dei\u00b7ne", "Stim\u00b7me", "den", "Schlum\u00b7mer", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "APPR", "NE", "$.", "PWAV", "VVFIN", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+--+--+-+--+--+-", "measure": "amphibrach.tri.plus"}}, "stanza.10": {"line.1": {"text": "Ich rufe den K\u00f6nig zum Zweikampf.", "tokens": ["Ich", "ru\u00b7fe", "den", "K\u00f6\u00b7nig", "zum", "Zwei\u00b7kampf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.11": {"line.1": {"text": "Warum rufst du ihn!", "tokens": ["Wa\u00b7rum", "rufst", "du", "ihn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.12": {"line.1": {"text": "Er hat mir die Seele meines Busens geraubt, ich liebte die Tochter von Torlat, und sie w\u00e4hlt ihn.", "tokens": ["Er", "hat", "mir", "die", "See\u00b7le", "mei\u00b7nes", "Bu\u00b7sens", "ge\u00b7raubt", ",", "ich", "lieb\u00b7te", "die", "Toch\u00b7ter", "von", "Tor\u00b7lat", ",", "und", "sie", "w\u00e4hlt", "ihn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ART", "NN", "PPOSAT", "NN", "VVPP", "$,", "PPER", "VVFIN", "ART", "NN", "APPR", "NE", "$,", "KON", "PPER", "VVFIN", "PPER", "$."], "meter": "-+--+-+-+--+-+--+--+-+-+-", "measure": "iambic.octa.plus.relaxed"}}, "stanza.13": {"line.1": {"text": "Sie w\u00e4hlt ihn, und nicht dich. Was nutzt dir der Kampf? was hilft dir der Sieg?", "tokens": ["Sie", "w\u00e4hlt", "ihn", ",", "und", "nicht", "dich", ".", "Was", "nutzt", "dir", "der", "Kampf", "?", "was", "hilft", "dir", "der", "Sieg", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KON", "PTKNEG", "PPER", "$.", "PWS", "VVFIN", "PPER", "ART", "NN", "$.", "PWS", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+---+-+--+-+--+", "measure": "iambic.hexa.relaxed"}}, "stanza.14": {"line.1": {"text": "Komm, mich d\u00fcrstet nach Kampf, mein Muth jauchzt der Gefahr entgegen, komm!", "tokens": ["Komm", ",", "mich", "d\u00fcrs\u00b7tet", "nach", "Kampf", ",", "mein", "Muth", "jauchzt", "der", "Ge\u00b7fahr", "ent\u00b7ge\u00b7gen", ",", "komm", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "VVFIN", "APPR", "NN", "$,", "PPOSAT", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,", "VVFIN", "$."], "meter": "+-+--+-+-+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.15": {"line.1": {"text": "Mora kommt nicht zu dir, o K\u00f6nig der Speere!", "tokens": ["Mo\u00b7ra", "kommt", "nicht", "zu", "dir", ",", "o", "K\u00f6\u00b7nig", "der", "Spee\u00b7re", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKNEG", "APPR", "PPER", "$,", "FM", "NN", "ART", "NN", "$."], "meter": "---+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.16": {"line.1": {"text": "Trauer umnachtet meine Seele, ihr S\u00f6hne des Gesangs! ewige Trauer umarmt mich.", "tokens": ["Trau\u00b7er", "um\u00b7nach\u00b7tet", "mei\u00b7ne", "See\u00b7le", ",", "ihr", "S\u00f6h\u00b7ne", "des", "Ge\u00b7sangs", "!", "e\u00b7wi\u00b7ge", "Trau\u00b7er", "um\u00b7armt", "mich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "ART", "NN", "$.", "ADJA", "NN", "VVFIN", "PPER", "$."], "meter": "+--+-+-+--+-+-+-+-+--+-", "measure": "iambic.octa.plus.invert"}}}}}