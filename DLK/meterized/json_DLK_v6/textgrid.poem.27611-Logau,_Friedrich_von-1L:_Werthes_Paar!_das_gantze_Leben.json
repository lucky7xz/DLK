{"textgrid.poem.27611": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Werthes Paar! das gantze Leben", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Werthes Paar! das gantze Leben", "tokens": ["Wert\u00b7hes", "Paar", "!", "das", "gant\u00b7ze", "Le\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sey bey euch ein steter Krieg,", "tokens": ["Sey", "bey", "euch", "ein", "ste\u00b7ter", "Krieg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "So da\u00df Beyden sey gegeben", "tokens": ["So", "da\u00df", "Bey\u00b7den", "sey", "ge\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PIS", "VAFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gleiche Beut und gleicher Sieg.", "tokens": ["Glei\u00b7che", "Beut", "und", "glei\u00b7cher", "Sieg", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "K\u00e4mpfft mit Liebe gegen Liebe,", "tokens": ["K\u00e4mpfft", "mit", "Lie\u00b7be", "ge\u00b7gen", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und mit Treu k\u00e4mpfft gegen Treu;", "tokens": ["Und", "mit", "Treu", "k\u00e4mpfft", "ge\u00b7gen", "Treu", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Da\u00df euch Zwiespalt nie betr\u00fcbe,", "tokens": ["Da\u00df", "euch", "Zwies\u00b7palt", "nie", "be\u00b7tr\u00fc\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Niemals auch der Kauff bereu.", "tokens": ["Nie\u00b7mals", "auch", "der", "Kauff", "be\u00b7reu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Zwar es wird wol offt geschehen,", "tokens": ["Zwar", "es", "wird", "wol", "offt", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "ADV", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Da\u00df die Braut zu seiner Zeit", "tokens": ["Da\u00df", "die", "Braut", "zu", "sei\u00b7ner", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Bey sechs Wochen nicht wird sehen,", "tokens": ["Bey", "sechs", "Wo\u00b7chen", "nicht", "wird", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "PTKNEG", "VAFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Wie die Wirthschafft sey bereit;", "tokens": ["Wie", "die", "Wirth\u00b7schafft", "sey", "be\u00b7reit", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Doch bedeut es nichts denn Beute;", "tokens": ["Doch", "be\u00b7deut", "es", "nichts", "denn", "Beu\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIS", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Wenn das Ziel f\u00fcrbey wird seyn,", "tokens": ["Wenn", "das", "Ziel", "f\u00fcr\u00b7bey", "wird", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "VAFIN", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Werden euren Schatz die Leute", "tokens": ["Wer\u00b7den", "eu\u00b7ren", "Schatz", "die", "Leu\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "H\u00f6ren au\u00df der Wiege schreyn.", "tokens": ["H\u00f6\u00b7ren", "au\u00df", "der", "Wie\u00b7ge", "schreyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}