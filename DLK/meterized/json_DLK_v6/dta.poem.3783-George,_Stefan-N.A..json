{"dta.poem.3783": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1892", "urn": "urn:nbn:de:kobv:b4-200905191524", "language": ["de:0.99"], "booktitle": "George, Stefan: Algabal. Paris u. a., 1892."}, "poem": {"stanza.1": {"line.1": {"text": "Sind es h\u00f6rner sind es harfen", "tokens": ["Sind", "es", "h\u00f6r\u00b7ner", "sind", "es", "har\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADJD", "VAFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die mich hoben", "tokens": ["Die", "mich", "ho\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["ART", "PPER", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Und in gr\u00fcfte niederwarfen?", "tokens": ["Und", "in", "gr\u00fcf\u00b7te", "nie\u00b7der\u00b7war\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie betreten", "tokens": ["Wie", "be\u00b7tre\u00b7ten"], "token_info": ["word", "word"], "pos": ["PWAV", "VVPP"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Und als ob ein gott mich zw\u00e4nge", "tokens": ["Und", "als", "ob", "ein", "gott", "mich", "zw\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOKOM", "KOUS", "ART", "NN", "PPER", "VVFIN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Muss ich beten", "tokens": ["Muss", "ich", "be\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "PPER", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Syrer w\u00e4hrend eurer s\u00e4nge", "tokens": ["Sy\u00b7rer", "w\u00e4h\u00b7rend", "eu\u00b7rer", "s\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Leise triller \u00b7 verj\u00fcngen gesunden", "tokens": ["Lei\u00b7se", "tril\u00b7ler", "ver\u00b7j\u00fcn\u00b7gen", "ge\u00b7sun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["XY", "XY", "XY", "VVINF", "VVPP"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Laute st\u00f6sse \u00b7 mit lachen vergeuden", "tokens": ["Lau\u00b7te", "st\u00f6s\u00b7se", "mit", "la\u00b7chen", "ver\u00b7geu\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "XY", "APPR", "VVINF", "VVINF"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Gelle striche \u00b7 die bohrenden wunden", "tokens": ["Gel\u00b7le", "stri\u00b7che", "die", "boh\u00b7ren\u00b7den", "wun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "XY", "ART", "ADJA", "ADJA"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Helle schl\u00e4ge \u00b7 die brennenden freuden", "tokens": ["Hel\u00b7le", "schl\u00e4\u00b7ge", "die", "bren\u00b7nen\u00b7den", "freu\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "XY", "ART", "ADJA", "NN"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Weise Syrer", "tokens": ["Wei\u00b7se", "Sy\u00b7rer"], "token_info": ["word", "word"], "pos": ["NN", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Werd ich dankend euch vertreiben?", "tokens": ["Werd", "ich", "dan\u00b7kend", "euch", "ver\u00b7trei\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihr verf\u00fchrer", "tokens": ["Ihr", "ver\u00b7f\u00fch\u00b7rer"], "token_info": ["word", "word"], "pos": ["PPOSAT", "ADJA"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Noch im leben zu verbleiben", "tokens": ["Noch", "im", "le\u00b7ben", "zu", "ver\u00b7blei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "VVINF", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}