{"textgrid.poem.46438": {"metadata": {"author": {"name": "Engelke, Gerrit", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nicht Raum, nicht Zeit, nur Nacht und Nacht.", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht Raum, nicht Zeit, nur Nacht und Nacht.", "tokens": ["Nicht", "Raum", ",", "nicht", "Zeit", ",", "nur", "Nacht", "und", "Nacht", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "$,", "PTKNEG", "NN", "$,", "ADV", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nur Nacht, von Nacht noch \u00fcberdacht.", "tokens": ["Nur", "Nacht", ",", "von", "Nacht", "noch", "\u00fc\u00b7berd\u00b7acht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "APPR", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein tr\u00e4chtig Sausen wogend schwoll \u2013", "tokens": ["Ein", "tr\u00e4ch\u00b7tig", "Sau\u00b7sen", "wo\u00b7gend", "schwoll", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "ADJD", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Da! pl\u00f6tzlichgro\u00df ein donnernd \u00bbIch\u00ab! erscholl \u2013", "tokens": ["Da", "!", "pl\u00f6tz\u00b7lich\u00b7gro\u00df", "ein", "don\u00b7nernd", "\u00bb", "Ich", "\u00ab", "!", "er\u00b7scholl", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "punct", "word", "punct"], "pos": ["ADV", "$.", "ADJD", "ART", "ADJD", "$(", "PPER", "$(", "$.", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da: Er! \u2013 Er sa\u00df in Nacht,", "tokens": ["Da", ":", "Er", "!", "\u2013", "Er", "sa\u00df", "in", "Nacht", ","], "token_info": ["word", "punct", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PPER", "$.", "$(", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und Er \u2013 Er war die Nacht.", "tokens": ["Und", "Er", "\u2013", "Er", "war", "die", "Nacht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$(", "PPER", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der Anfang war erwacht.", "tokens": ["Der", "An\u00b7fang", "war", "er\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er sa\u00df im Anfangsnacht-Getreibe", "tokens": ["Er", "sa\u00df", "im", "An\u00b7fangs\u00b7nacht\u00b7Ge\u00b7trei\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mit schwangerem Hirn und Leibe,", "tokens": ["Mit", "schwan\u00b7ge\u00b7rem", "Hirn", "und", "Lei\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Um Seinen K\u00f6rper rauchte Schwei\u00df.", "tokens": ["Um", "Sei\u00b7nen", "K\u00f6r\u00b7per", "rauch\u00b7te", "Schwei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ein helles Strahlen ging aus Seinem Kopf \u2013", "tokens": ["Ein", "hel\u00b7les", "Strah\u00b7len", "ging", "aus", "Sei\u00b7nem", "Kopf", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Und wurde dicht und hell: zum Silber-Mond-Kreis,", "tokens": ["Und", "wur\u00b7de", "dicht", "und", "hell", ":", "zum", "Sil\u00b7ber\u00b7Mon\u00b7dKreis", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADJD", "KON", "ADJD", "$.", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Aus Seinen Augen fiel ein Lichtgetropf:", "tokens": ["Aus", "Sei\u00b7nen", "Au\u00b7gen", "fiel", "ein", "Licht\u00b7ge\u00b7tropf", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Und irrte wirr im Dunkel:", "tokens": ["Und", "irr\u00b7te", "wirr", "im", "Dun\u00b7kel", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Sterngefunkel.", "tokens": ["Stern\u00b7ge\u00b7fun\u00b7kel", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Da scholl es wieder f\u00fcrchterlich:", "tokens": ["Da", "scholl", "es", "wie\u00b7der", "f\u00fcrch\u00b7ter\u00b7lich", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das All-Geb\u00e4r-Gebr\u00fcll: \u00bbIch\u00ab!", "tokens": ["Das", "All\u00b7Ge\u00b7b\u00e4r\u00b7Ge\u00b7br\u00fcll", ":", "\u00bb", "Ich", "\u00ab", "!"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "punct"], "pos": ["ART", "NN", "$.", "$(", "PPER", "$(", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da ri\u00df Er auf mit H\u00e4ndekrallen Seine Stirn:", "tokens": ["Da", "ri\u00df", "Er", "auf", "mit", "H\u00e4n\u00b7de\u00b7kral\u00b7len", "Sei\u00b7ne", "Stirn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "APPR", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und offen lag in Dampf: das rote Feuer-Hirn!", "tokens": ["Und", "of\u00b7fen", "lag", "in", "Dampf", ":", "das", "ro\u00b7te", "Feu\u00b7er\u00b7Hirn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "APPR", "NN", "$.", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Er ri\u00df ein St\u00fcck heraus:", "tokens": ["Er", "ri\u00df", "ein", "St\u00fcck", "he\u00b7raus", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Er ballte eine Kugel draus", "tokens": ["Er", "ball\u00b7te", "ei\u00b7ne", "Ku\u00b7gel", "draus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "PAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und hielt das Gl\u00fchen in die Nacht,", "tokens": ["Und", "hielt", "das", "Gl\u00fc\u00b7hen", "in", "die", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Er hing es in den Braus:", "tokens": ["Er", "hing", "es", "in", "den", "Braus", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Die Sonne war erwacht!", "tokens": ["Die", "Son\u00b7ne", "war", "er\u00b7wacht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Ein Gl\u00fchgezisch, das Funken spr\u00fchte,", "tokens": ["Ein", "Gl\u00fch\u00b7ge\u00b7zisch", ",", "das", "Fun\u00b7ken", "spr\u00fch\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Das hei\u00df die schwere Nacht durchgl\u00fchte,", "tokens": ["Das", "hei\u00df", "die", "schwe\u00b7re", "Nacht", "durch\u00b7gl\u00fch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Da\u00df Mond und jeder Stern verbl\u00fchte", "tokens": ["Da\u00df", "Mond", "und", "je\u00b7der", "Stern", "ver\u00b7bl\u00fch\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "KON", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Und alles Dunkel schwand.", "tokens": ["Und", "al\u00b7les", "Dun\u00b7kel", "schwand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Hochoben hing der Sonne-Brand.", "tokens": ["Hoch\u00b7o\u00b7ben", "hing", "der", "Son\u00b7ne\u00b7Brand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Da ri\u00df Er mit den H\u00e4ndekrallen", "tokens": ["Da", "ri\u00df", "Er", "mit", "den", "H\u00e4n\u00b7de\u00b7kral\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Aus Seinem Leib das Alles-Herz!", "tokens": ["Aus", "Sei\u00b7nem", "Leib", "das", "Al\u00b7les\u00b7Herz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Schrie \u00bbIch\u00ab! und \u00bbIch\u00ab! in Dampf und Schmerz \u2013", "tokens": ["Schrie", "\u00bb", "Ich", "\u00ab", "!", "und", "\u00bb", "Ich", "\u00ab", "!", "in", "Dampf", "und", "Schmerz", "\u2013"], "token_info": ["word", "punct", "word", "punct", "punct", "word", "punct", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PPER", "$(", "$.", "KON", "$(", "PPER", "$(", "$.", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und lie\u00df es in die Tiefe fallen \u2013", "tokens": ["Und", "lie\u00df", "es", "in", "die", "Tie\u00b7fe", "fal\u00b7len", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Er lie\u00df es in die Tiefe fallen", "tokens": ["Er", "lie\u00df", "es", "in", "die", "Tie\u00b7fe", "fal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und setzte Seinen Fu\u00df darauf.", "tokens": ["Und", "setz\u00b7te", "Sei\u00b7nen", "Fu\u00df", "da\u00b7rauf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und setzte Seinen Fu\u00df auf diese Welt", "tokens": ["Und", "setz\u00b7te", "Sei\u00b7nen", "Fu\u00df", "auf", "die\u00b7se", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Auf Seine, Seine Welt,", "tokens": ["Auf", "Sei\u00b7ne", ",", "Sei\u00b7ne", "Welt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Von Sonne \u00fcberhellt.", "tokens": ["Von", "Son\u00b7ne", "\u00fc\u00b7berh\u00b7ellt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Zum Letzten rief er wieder \u00bbIch\u00ab:", "tokens": ["Zum", "Letz\u00b7ten", "rief", "er", "wie\u00b7der", "\u00bb", "Ich", "\u00ab", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADV", "$(", "PPER", "$(", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gedehnt und G0\u00e4terlich beschlie\u00dfend,", "tokens": ["Ge\u00b7dehnt", "und", "G0\u00b7\u00e4\u00b7ter\u00b7lich", "be\u00b7schlie\u00b7\u00dfend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADJD", "VVPP", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Als erster Wohlklang aus Ihm flie\u00dfend,", "tokens": ["Als", "ers\u00b7ter", "Wohl\u00b7klang", "aus", "Ihm", "flie\u00b7\u00dfend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und lie\u00df ein Teilchen Zeugungs-Hirn aus Seiner Hand", "tokens": ["Und", "lie\u00df", "ein", "Teil\u00b7chen", "Zeu\u00b7gungs\u00b7Hirn", "aus", "Sei\u00b7ner", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Tief abw\u00e4rts fallen auf das neue, runde Land:", "tokens": ["Tief", "ab\u00b7w\u00e4rts", "fal\u00b7len", "auf", "das", "neu\u00b7e", ",", "run\u00b7de", "Land", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "ADV", "VVFIN", "APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Und da! und da: der Same quoll;", "tokens": ["Und", "da", "!", "und", "da", ":", "der", "Sa\u00b7me", "quoll", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$.", "KON", "ADV", "$.", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Wesen, neues Wesen schwoll:", "tokens": ["Ein", "We\u00b7sen", ",", "neu\u00b7es", "We\u00b7sen", "schwoll", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und stieg \u2013 und stand auf dem Geroll: \u2013", "tokens": ["Und", "stieg", "\u2013", "und", "stand", "auf", "dem", "Ge\u00b7roll", ":", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "$(", "KON", "VVFIN", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Mensch! der Mensch! der Mensch!", "tokens": ["Der", "Mensch", "!", "der", "Mensch", "!", "der", "Mensch", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "ART", "NN", "$.", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Der sah den All-Geb\u00e4rer nicht!", "tokens": ["Der", "sah", "den", "All\u00b7Ge\u00b7b\u00e4\u00b7rer", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er sah das Licht, nur Licht und Licht!", "tokens": ["Er", "sah", "das", "Licht", ",", "nur", "Licht", "und", "Licht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ADV", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er hob ergriffen seine H\u00e4nde hoch,", "tokens": ["Er", "hob", "er\u00b7grif\u00b7fen", "sei\u00b7ne", "H\u00e4n\u00b7de", "hoch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVPP", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein sch\u00e4umend Stammeln seinem Mund entflog,", "tokens": ["Ein", "sch\u00e4u\u00b7mend", "Stam\u00b7meln", "sei\u00b7nem", "Mund", "ent\u00b7flog", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Das gro\u00dfe Leuchten bog", "tokens": ["Das", "gro\u00b7\u00dfe", "Leuch\u00b7ten", "bog"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"