{"textgrid.poem.60983": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Einer jungen Freundin", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es kommt dies B\u00fcchlein zu dir fein", "tokens": ["Es", "kommt", "dies", "B\u00fcch\u00b7lein", "zu", "dir", "fein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PDS", "NN", "APPR", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und m\u00f6chte gern dein Garten sein.", "tokens": ["Und", "m\u00f6ch\u00b7te", "gern", "dein", "Gar\u00b7ten", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zwischen den Blumen, die ihn zieren,", "tokens": ["Zwi\u00b7schen", "den", "Blu\u00b7men", ",", "die", "ihn", "zie\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "F\u00fchr' deine Gedanken h\u00fcbsch spazieren.", "tokens": ["F\u00fchr'", "dei\u00b7ne", "Ge\u00b7dan\u00b7ken", "h\u00fcbsch", "spa\u00b7zie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ADJD", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Wirst manches finden, was dich freut:", "tokens": ["Wirst", "man\u00b7ches", "fin\u00b7den", ",", "was", "dich", "freut", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "VVINF", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Rosen im dunkeln Gr\u00fcn verstreut,", "tokens": ["Ro\u00b7sen", "im", "dun\u00b7keln", "Gr\u00fcn", "ver\u00b7streut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Nelk', Apfelbl\u00fct' und Rosmarin", "tokens": ["Nel\u00b7k'", ",", "Ap\u00b7fel\u00b7bl\u00fct'", "und", "Ros\u00b7ma\u00b7rin"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Und Falter, die dazwischen ziehn;", "tokens": ["Und", "Fal\u00b7ter", ",", "die", "da\u00b7zwi\u00b7schen", "ziehn", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PRELS", "PAV", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Auch alte Wipfel leis und lind", "tokens": ["Auch", "al\u00b7te", "Wip\u00b7fel", "leis", "und", "lind"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "ADJD", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Ger\u00fchrt vom lauen Sommerwind.", "tokens": ["Ge\u00b7r\u00fchrt", "vom", "lau\u00b7en", "Som\u00b7mer\u00b7wind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Und kommt dir's manchmal vor beim Lauschen,", "tokens": ["Und", "kommt", "dir's", "manch\u00b7mal", "vor", "beim", "Lau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "ADV", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Als sei dir wohlbekannt das Rauschen,", "tokens": ["Als", "sei", "dir", "wohl\u00b7be\u00b7kannt", "das", "Rau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "So denk', was rauscht und klingt und bl\u00fcht,", "tokens": ["So", "denk'", ",", "was", "rauscht", "und", "klingt", "und", "bl\u00fcht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PWS", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Das ist am Ende mein Gem\u00fct.", "tokens": ["Das", "ist", "am", "En\u00b7de", "mein", "Ge\u00b7m\u00fct", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Und bist du gr\u00f6\u00dfer, wirst du sehn,", "tokens": ["Und", "bist", "du", "gr\u00f6\u00b7\u00dfer", ",", "wirst", "du", "sehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "$,", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Da\u00df zwischen den Rosen auch Disteln stehn.", "tokens": ["Da\u00df", "zwi\u00b7schen", "den", "Ro\u00b7sen", "auch", "Dis\u00b7teln", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ADV", "NN", "VVINF", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.17": {"text": "Z\u00fcrn' aber drum dem G\u00e4rtner nicht;", "tokens": ["Z\u00fcrn'", "a\u00b7ber", "drum", "dem", "G\u00e4rt\u00b7ner", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PAV", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Er lie\u00df sie bei den Blumen licht,", "tokens": ["Er", "lie\u00df", "sie", "bei", "den", "Blu\u00b7men", "licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Damit die Esel und Rezensenten", "tokens": ["Da\u00b7mit", "die", "E\u00b7sel", "und", "Re\u00b7zen\u00b7sen\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "ART", "NN", "KON", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.20": {"text": "F\u00fcr sich doch auch was finden k\u00f6nnten.", "tokens": ["F\u00fcr", "sich", "doch", "auch", "was", "fin\u00b7den", "k\u00f6nn\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ADV", "ADV", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}