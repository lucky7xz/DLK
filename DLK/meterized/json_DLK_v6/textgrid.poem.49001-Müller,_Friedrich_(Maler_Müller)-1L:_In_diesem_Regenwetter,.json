{"textgrid.poem.49001": {"metadata": {"author": {"name": "M\u00fcller, Friedrich (Maler M\u00fcller)", "birth": "N.A.", "death": "N.A."}, "title": "1L: In diesem Regenwetter,", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In diesem Regenwetter,", "tokens": ["In", "die\u00b7sem", "Re\u00b7gen\u00b7wet\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was schw\u00e4rmt ihr um den Hain?", "tokens": ["Was", "schw\u00e4rmt", "ihr", "um", "den", "Hain", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr liebsten kleinen G\u00f6tter,", "tokens": ["Ihr", "liebs\u00b7ten", "klei\u00b7nen", "G\u00f6t\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Kommt doch zu mir herein!", "tokens": ["Kommt", "doch", "zu", "mir", "her\u00b7ein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Horcht, wie die St\u00fcrme heulen,", "tokens": ["Horcht", ",", "wie", "die", "St\u00fcr\u00b7me", "heu\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "ART", "NN", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Durch jene Felsenkluft!", "tokens": ["Durch", "je\u00b7ne", "Fel\u00b7sen\u00b7kluft", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die grauen Lerchen eilen", "tokens": ["Die", "grau\u00b7en", "Ler\u00b7chen", "ei\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gebadet aus der Luft.", "tokens": ["Ge\u00b7ba\u00b7det", "aus", "der", "Luft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Kommt hurtig doch geflogen,", "tokens": ["Kommt", "hur\u00b7tig", "doch", "ge\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Damit der Regen nicht", "tokens": ["Da\u00b7mit", "der", "Re\u00b7gen", "nicht"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "ART", "NN", "PTKNEG"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Erschlaffe euren Bogen,", "tokens": ["Er\u00b7schlaf\u00b7fe", "eu\u00b7ren", "Bo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Euch eure Pfeilchen bricht!", "tokens": ["Euch", "eu\u00b7re", "Pfeil\u00b7chen", "bricht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Kommt, h\u00e4ngt zu meiner Leier", "tokens": ["Kommt", ",", "h\u00e4ngt", "zu", "mei\u00b7ner", "Lei\u00b7er"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den goldnen K\u00f6cher hin,", "tokens": ["Den", "gold\u00b7nen", "K\u00f6\u00b7cher", "hin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den Bogen auch! Zum Feuer", "tokens": ["Den", "Bo\u00b7gen", "auch", "!", "Zum", "Feu\u00b7er"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ADV", "$.", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Setzt euch um den Kamin.", "tokens": ["Setzt", "euch", "um", "den", "Ka\u00b7min", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.5": {"line.1": {"text": "Und singt mit s\u00fc\u00dfer Kehle", "tokens": ["Und", "singt", "mit", "s\u00fc\u00b7\u00dfer", "Keh\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mir meinen blonden Freund,", "tokens": ["Mir", "mei\u00b7nen", "blon\u00b7den", "Freund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In dessen gro\u00dfer Seele", "tokens": ["In", "des\u00b7sen", "gro\u00b7\u00dfer", "See\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PRELAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sich Kunst und Geist vereint.", "tokens": ["Sich", "Kunst", "und", "Geist", "ver\u00b7eint", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Was brauch' ich ihn zu nennen?", "tokens": ["Was", "brauch'", "ich", "ihn", "zu", "nen\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihn nennt die ganze Welt:", "tokens": ["Ihn", "nennt", "die", "gan\u00b7ze", "Welt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den Kobell m\u00fc\u00dft ihr kennen!", "tokens": ["Den", "Ko\u00b7bell", "m\u00fc\u00dft", "ihr", "ken\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sonst, Knaben, w\u00e4r's gefehlt.", "tokens": ["Sonst", ",", "Kna\u00b7ben", ",", "w\u00e4r's", "ge\u00b7fehlt", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "$,", "VAFIN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "In Cypris dunklen Hainen", "tokens": ["In", "Cyp\u00b7ris", "dunk\u00b7len", "Hai\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Steht er in hoher Ehr;", "tokens": ["Steht", "er", "in", "ho\u00b7her", "Ehr", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Die Grazien', ihr Kleinen,", "tokens": ["Die", "Gra\u00b7zi\u00b7en'", ",", "ihr", "Klei\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sind immer um in her.", "tokens": ["Sind", "im\u00b7mer", "um", "in", "her", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "APPR", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "O, der hat hohe Gaben!", "tokens": ["O", ",", "der", "hat", "ho\u00b7he", "Ga\u00b7ben", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "VAFIN", "ADJA", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Der malt euch eine Flur,", "tokens": ["Der", "malt", "euch", "ei\u00b7ne", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein' Wasserfall, ihr Knaben,", "tokens": ["Ein'", "Was\u00b7ser\u00b7fall", ",", "ihr", "Kna\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So sch\u00f6n als die Natur.", "tokens": ["So", "sch\u00f6n", "als", "die", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Ihr h\u00f6rt die Weste wehen", "tokens": ["Ihr", "h\u00f6rt", "die", "Wes\u00b7te", "we\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Herab in's k\u00fchle Tal;", "tokens": ["Her\u00b7ab", "in's", "k\u00fch\u00b7le", "Tal", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr schw\u00f6rt, die Sonn' zu sehen", "tokens": ["Ihr", "schw\u00f6rt", ",", "die", "Sonn'", "zu", "se\u00b7hen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und f\u00fchlet ihren Strahl.", "tokens": ["Und", "f\u00fch\u00b7let", "ih\u00b7ren", "Strahl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Auch ehret er die Weisen", "tokens": ["Auch", "eh\u00b7ret", "er", "die", "Wei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und liebet Scherz und Wein.", "tokens": ["Und", "lie\u00b7bet", "Scherz", "und", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr m\u00fc\u00dft, ihr m\u00fc\u00dft ihn preisen,", "tokens": ["Ihr", "m\u00fc\u00dft", ",", "ihr", "m\u00fc\u00dft", "ihn", "prei\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PPER", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wenn ihr mir lieb wollt sein.", "tokens": ["Wenn", "ihr", "mir", "lieb", "wollt", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "VMFIN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Denn, goldgelockte Kleinen,", "tokens": ["Denn", ",", "gold\u00b7ge\u00b7lock\u00b7te", "Klei\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["KON", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sehr z\u00e4rtlich lieb ich ihn.", "tokens": ["Sehr", "z\u00e4rt\u00b7lich", "lieb", "ich", "ihn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ach, ach! Ich m\u00f6chte weinen,", "tokens": ["Ach", ",", "ach", "!", "Ich", "m\u00f6ch\u00b7te", "wei\u00b7nen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ITJ", "$.", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df ich nicht bei ihm bin ...", "tokens": ["Da\u00df", "ich", "nicht", "bei", "ihm", "bin", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "APPR", "PPER", "VAFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Ein Kranz wollt' ich ihm winden", "tokens": ["Ein", "Kranz", "wollt'", "ich", "ihm", "win\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "PPER", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von Rosen, Balsamin", "tokens": ["Von", "Ro\u00b7sen", ",", "Bal\u00b7sa\u00b7min"], "token_info": ["word", "word", "punct", "word"], "pos": ["APPR", "NN", "$,", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und s\u00fc\u00dfen Hyacinthen", "tokens": ["Und", "s\u00fc\u00b7\u00dfen", "Hya\u00b7cin\u00b7then"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Und duftenden Jasmin,", "tokens": ["Und", "duf\u00b7ten\u00b7den", "Jas\u00b7min", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.13": {"line.1": {"text": "Da\u00df er in hei\u00dfen Tagen", "tokens": ["Da\u00df", "er", "in", "hei\u00b7\u00dfen", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Um seine Stirne weht,", "tokens": ["Um", "sei\u00b7ne", "Stir\u00b7ne", "weht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn \u00fcber ihn der Wagen", "tokens": ["Wenn", "\u00fc\u00b7ber", "ihn", "der", "Wa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der goldnen Sonne steht.", "tokens": ["Der", "gold\u00b7nen", "Son\u00b7ne", "steht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}