{"textgrid.poem.66848": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbwir sind die \u203amodernen Barbaren\u2039,", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbwir sind die \u203amodernen Barbaren\u2039,", "tokens": ["\u00bb", "wir", "sind", "die", "\u203a", "mo\u00b7der\u00b7nen", "Bar\u00b7ba\u00b7ren", "\u2039", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VAFIN", "ART", "$(", "ADJA", "NN", "$(", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wir r\u00fccken Mann f\u00fcr Mann", "tokens": ["Wir", "r\u00fc\u00b7cken", "Mann", "f\u00fcr", "Mann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In un\u00fcberwindlichen Scharen,", "tokens": ["In", "un\u00b7\u00fc\u00b7berw\u00b7ind\u00b7li\u00b7chen", "Scha\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "In schwellenden Scharen heran.", "tokens": ["In", "schwel\u00b7len\u00b7den", "Scha\u00b7ren", "he\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Wir kommen mit Hammer und Mei\u00dfel,", "tokens": ["Wir", "kom\u00b7men", "mit", "Ham\u00b7mer", "und", "Mei\u00b7\u00dfel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Wir kommen mit Letter und Buch", "tokens": ["Wir", "kom\u00b7men", "mit", "Let\u00b7ter", "und", "Buch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Wider der Menschheit Gei\u00dfel,", "tokens": ["Wi\u00b7der", "der", "Menschheit", "Gei\u00b7\u00dfel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Wider des Mammons Fluch.", "tokens": ["Wi\u00b7der", "des", "Mam\u00b7mons", "Fluch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Wir sind die \u203amodernen Vandalen\u2039,", "tokens": ["Wir", "sind", "die", "\u203a", "mo\u00b7der\u00b7nen", "Van\u00b7da\u00b7len", "\u2039", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "ART", "$(", "ADJA", "NN", "$(", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Wir wandeln wuchtig und schwer", "tokens": ["Wir", "wan\u00b7deln", "wuch\u00b7tig", "und", "schwer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "ADJD"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "In eisenbeschlagnen Sandalen", "tokens": ["In", "ei\u00b7sen\u00b7be\u00b7schlag\u00b7nen", "San\u00b7da\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Die Pfade der Zukunft daher.", "tokens": ["Die", "Pfa\u00b7de", "der", "Zu\u00b7kunft", "da\u00b7her", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PAV", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Wir schreiten mit dr\u00f6hnendem Schritte", "tokens": ["Wir", "schrei\u00b7ten", "mit", "dr\u00f6h\u00b7nen\u00b7dem", "Schrit\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Durch die goldenen Pforten der Zeit,", "tokens": ["Durch", "die", "gol\u00b7de\u00b7nen", "Pfor\u00b7ten", "der", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Wir wandeln Ordnung und Sitte,", "tokens": ["Wir", "wan\u00b7deln", "Ord\u00b7nung", "und", "Sit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Gesetz und Gerechtigkeit.", "tokens": ["Ge\u00b7setz", "und", "Ge\u00b7rech\u00b7tig\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Wo wir kommen, zischen die Wogen", "tokens": ["Wo", "wir", "kom\u00b7men", ",", "zi\u00b7schen", "die", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PPER", "VVINF", "$,", "VVFIN", "ART", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Einer untergehenden Welt,", "tokens": ["Ei\u00b7ner", "un\u00b7ter\u00b7ge\u00b7hen\u00b7den", "Welt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Wo wir kommen, werden im Bogen", "tokens": ["Wo", "wir", "kom\u00b7men", ",", "wer\u00b7den", "im", "Bo\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PPER", "VVINF", "$,", "VAFIN", "APPRART", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Die Himmel des Lebens erhellt.", "tokens": ["Die", "Him\u00b7mel", "des", "Le\u00b7bens", "er\u00b7hellt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Es zittert und \u00e4chzt durch die Weiten", "tokens": ["Es", "zit\u00b7tert", "und", "\u00e4chzt", "durch", "die", "Wei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Der verwahrlost \u00f6den Kultur,", "tokens": ["Der", "ver\u00b7wahr\u00b7lost", "\u00f6\u00b7den", "Kul\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Es donnert und blitzt, wo wir schreiten,", "tokens": ["Es", "don\u00b7nert", "und", "blitzt", ",", "wo", "wir", "schrei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Und Befruchtung dampft unsre Spur.", "tokens": ["Und", "Be\u00b7fruch\u00b7tung", "dampft", "uns\u00b7re", "Spur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Wir sind die \u203aVandalen\u2039 der Milde,", "tokens": ["Wir", "sind", "die", "\u203a", "Van\u00b7da\u00b7len", "\u2039", "der", "Mil\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$(", "ART", "NN", "$,"], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.2": {"text": "Wir sind die \u203aBarbaren\u2039 des Rechts,", "tokens": ["Wir", "sind", "die", "\u203a", "Bar\u00b7ba\u00b7ren", "\u2039", "des", "Rechts", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$(", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir f\u00fchren die Freiheit im Schilde,", "tokens": ["Wir", "f\u00fch\u00b7ren", "die", "Frei\u00b7heit", "im", "Schil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Die Freiheit des Menschengeschlechts.", "tokens": ["Die", "Frei\u00b7heit", "des", "Men\u00b7schen\u00b7ge\u00b7schlechts", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Moderne Barbaren? O nein!", "tokens": ["Mo\u00b7der\u00b7ne", "Bar\u00b7ba\u00b7ren", "?", "O", "nein", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "NE", "PTKANT", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Wir wollen die roten Husaren,", "tokens": ["Wir", "wol\u00b7len", "die", "ro\u00b7ten", "Hu\u00b7sa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Husaren der Menschheit sein.\u00ab", "tokens": ["Hu\u00b7sa\u00b7ren", "der", "Menschheit", "sein", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", 