{"dta.poem.7130": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtung des Schlafs,  \n als  \n eine G\u00f6ttliche Wohlthat,  \n bey dem 1728sten Jahres-Wechsel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du f\u00e4hrst vielleicht noch fort, und wirffst mir ferner ein:", "tokens": ["Du", "f\u00e4hrst", "viel\u00b7leicht", "noch", "fort", ",", "und", "wirffst", "mir", "fer\u00b7ner", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "PTKVZ", "$,", "KON", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201ees k\u00f6nne sich ein Traum hierher durchaus nicht schicken,", "tokens": ["\u201e", "es", "k\u00f6n\u00b7ne", "sich", "ein", "Traum", "hier\u00b7her", "durc\u00b7haus", "nicht", "schi\u00b7cken", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "PRF", "ART", "NN", "PAV", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201eals der ein Gegen-Satz der W\u00fcrcklichkeit ja sey:", "tokens": ["\u201e", "als", "der", "ein", "Ge\u00b7gen\u00b7Satz", "der", "W\u00fcrck\u00b7lich\u00b7keit", "ja", "sey", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "ART", "ART", "NN", "ART", "NN", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So nenn\u2019 es ein Gesicht, so nenn\u2019 es ein Entz\u00fccken,", "tokens": ["So", "nenn'", "es", "ein", "Ge\u00b7sicht", ",", "so", "nenn'", "es", "ein", "Ent\u00b7z\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,", "ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Genung, da\u00df allenfalls die Tr\u00e4ume mancherley.", "tokens": ["Ge\u00b7nung", ",", "da\u00df", "al\u00b7len\u00b7falls", "die", "Tr\u00e4u\u00b7me", "man\u00b7cher\u00b7ley", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "ADV", "ART", "NN", "PIAT", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die W\u00fcrcklichkeit, die du so starck begehrest,", "tokens": ["Die", "W\u00fcrck\u00b7lich\u00b7keit", ",", "die", "du", "so", "starck", "be\u00b7geh\u00b7rest", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ist etwas ohnedem, das dir gantz unbekannt.", "tokens": ["Ist", "et\u00b7was", "oh\u00b7ne\u00b7dem", ",", "das", "dir", "gantz", "un\u00b7be\u00b7kannt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "PAV", "$,", "PRELS", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Was du nicht greiffen kannst, nicht schmeckest, siehst noch h\u00f6rest,", "tokens": ["Was", "du", "nicht", "greif\u00b7fen", "kannst", ",", "nicht", "schme\u00b7ckest", ",", "siehst", "noch", "h\u00f6\u00b7rest", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PTKNEG", "VVINF", "VMFIN", "$,", "PTKNEG", "VVFIN", "$,", "VVFIN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "H\u00e4lt dein von Vor-Urtheil verfinsterter Verstand", "tokens": ["H\u00e4lt", "dein", "von", "Vor\u00b7Urt\u00b7heil", "ver\u00b7fins\u00b7ter\u00b7ter", "Ver\u00b7stand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "APPR", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "So gut, als w\u00e4r\u2019 es nicht.", "tokens": ["So", "gut", ",", "als", "w\u00e4r'", "es", "nicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOKOM", "VAFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Gerad, als k\u00f6nnten dich die Sinnen lehren,", "tokens": ["Ge\u00b7rad", ",", "als", "k\u00f6nn\u00b7ten", "dich", "die", "Sin\u00b7nen", "leh\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "VMFIN", "PRF", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Gerad, als ob sie Richter w\u00e4ren", "tokens": ["Ge\u00b7rad", ",", "als", "ob", "sie", "Rich\u00b7ter", "w\u00e4\u00b7ren"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "KOKOM", "KOUS", "PPER", "NN", "VAFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Von wahrer W\u00fcrcklichkeit: da doch die Seel allein,", "tokens": ["Von", "wah\u00b7rer", "W\u00fcrck\u00b7lich\u00b7keit", ":", "da", "doch", "die", "Seel", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$.", "ADV", "ADV", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Von dem, was wesentlich, von einem wahren Seyn,", "tokens": ["Von", "dem", ",", "was", "we\u00b7sent\u00b7lich", ",", "von", "ei\u00b7nem", "wah\u00b7ren", "Seyn", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PRELS", "ADJD", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Das, was man wei\u00df, begreifft.", "tokens": ["Das", ",", "was", "man", "wei\u00df", ",", "be\u00b7greifft", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "PIS", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.16": {"text": "Wie k\u00f6nnten Engel, Geister, Seelen,", "tokens": ["Wie", "k\u00f6nn\u00b7ten", "En\u00b7gel", ",", "Geis\u00b7ter", ",", "See\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWAV", "VMFIN", "NE", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Als welchen ja die Sinne fehlen,", "tokens": ["Als", "wel\u00b7chen", "ja", "die", "Sin\u00b7ne", "feh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PWAT", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Was w\u00fcrckliches empfinden und verstehn,", "tokens": ["Was", "w\u00fcrck\u00b7li\u00b7ches", "emp\u00b7fin\u00b7den", "und", "ver\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Wenn sie die W\u00fcrcklichkeit der Dinge, die geschehn,", "tokens": ["Wenn", "sie", "die", "W\u00fcrck\u00b7lich\u00b7keit", "der", "Din\u00b7ge", ",", "die", "ge\u00b7schehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "$,", "PRELS", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Nicht, als durch Sinnen nur, empf\u00fcnden.", "tokens": ["Nicht", ",", "als", "durch", "Sin\u00b7nen", "nur", ",", "emp\u00b7f\u00fcn\u00b7den", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PTKNEG", "$,", "KOUS", "APPR", "NN", "ADV", "$,", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Es w\u00fcrde, wenn sie sonst nicht f\u00fchlten und verst\u00fcnden,", "tokens": ["Es", "w\u00fcr\u00b7de", ",", "wenn", "sie", "sonst", "nicht", "f\u00fchl\u00b7ten", "und", "ver\u00b7st\u00fcn\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "PPER", "ADV", "PTKNEG", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Jhr\u2019 eigne W\u00fcrcklichkeit, zusammt der W\u00fcrcklichkeit", "tokens": ["Ih\u00b7r'", "eig\u00b7ne", "W\u00fcrck\u00b7lich\u00b7keit", ",", "zu\u00b7sammt", "der", "W\u00fcrck\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.23": {"text": "Der Dinge, in-und nach der Zeit,", "tokens": ["Der", "Din\u00b7ge", ",", "in\u00b7\u00b7und", "nach", "der", "Zeit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Nach unserem Begriff, verschwinden.", "tokens": ["Nach", "un\u00b7se\u00b7rem", "Be\u00b7griff", ",", "ver\u00b7schwin\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}