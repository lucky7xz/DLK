{"dta.poem.20317": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Sonnet.  \n \u00fcber Calistens bildni\u00df.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es hat des k\u00fcnstlers hand allhier zu frey gethan/", "tokens": ["Es", "hat", "des", "k\u00fcnst\u00b7lers", "hand", "all\u00b7hier", "zu", "frey", "ge\u00b7than", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "ADV", "PTKA", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich tadle seine kunst/ und schelte sein beginnen/", "tokens": ["Ich", "tad\u00b7le", "sei\u00b7ne", "kunst", "/", "und", "schel\u00b7te", "sein", "be\u00b7gin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$(", "KON", "VVFIN", "PPOSAT", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein mensch der kan ja nur auff etwas menschlichs sinnen/", "tokens": ["Ein", "mensch", "der", "kan", "ja", "nur", "auff", "et\u00b7was", "menschlichs", "sin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "VMFIN", "ADV", "ADV", "APPR", "PIS", "PIS", "VVINF", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Wie f\u00e4nat die bl\u00f6de faust bey einer g\u00f6ttin an?", "tokens": ["Wie", "f\u00e4\u00b7nat", "die", "bl\u00f6\u00b7de", "faust", "bey", "ei\u00b7ner", "g\u00f6t\u00b7tin", "an", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "ADJA", "NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Was Gott und die natur aus allen kr\u00e4fften kan/", "tokens": ["Was", "Gott", "und", "die", "na\u00b7tur", "aus", "al\u00b7len", "kr\u00e4ff\u00b7ten", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "KON", "ART", "NN", "APPR", "PIS", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Das wird kein schlechtes blat mit wei\u00df und roth gewinnen/", "tokens": ["Das", "wird", "kein", "schlech\u00b7tes", "blat", "mit", "wei\u00df", "und", "roth", "ge\u00b7win\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PIAT", "ADJA", "NN", "APPR", "VVFIN", "KON", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Sein vorsatz wird auch noch bey dieser glut zerrinnen/", "tokens": ["Sein", "vor\u00b7satz", "wird", "auch", "noch", "bey", "die\u00b7ser", "glut", "zer\u00b7rin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADV", "APPR", "PDAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Denn wer das feuer scheut/ der trete nicht heran.", "tokens": ["Denn", "wer", "das", "feu\u00b7er", "scheut", "/", "der", "tre\u00b7te", "nicht", "he\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "VVFIN", "$(", "ART", "VVFIN", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der sie aus wachs gemacht/ dem ist die kunst gelungen/", "tokens": ["Der", "sie", "aus", "wachs", "ge\u00b7macht", "/", "dem", "ist", "die", "kunst", "ge\u00b7lun\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NE", "VVPP", "$(", "PDS", "VAFIN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Da\u00df er den hefft\u2019gen reitz nur halb und halb gezwungen/", "tokens": ["Da\u00df", "er", "den", "hefft'\u00b7gen", "reitz", "nur", "halb", "und", "halb", "ge\u00b7zwun\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "ADV", "ADJD", "KON", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Sonst m\u00fcst es vor sich selbst wenns \u00e4hnlich w\u00e4r zergehn.", "tokens": ["Sonst", "m\u00fcst", "es", "vor", "sich", "selbst", "wenns", "\u00e4hn\u00b7lich", "w\u00e4r", "zer\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "PRF", "ADV", "KOUS", "ADJD", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die sonne kan sich nur in schlechtes wasser mahlen/", "tokens": ["Die", "son\u00b7ne", "kan", "sich", "nur", "in", "schlech\u00b7tes", "was\u00b7ser", "mah\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PRF", "ADV", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Verlangt man einen ri\u00df von dieser sch\u00f6nheit strahlen/", "tokens": ["Ver\u00b7langt", "man", "ei\u00b7nen", "ri\u00df", "von", "die\u00b7ser", "sch\u00f6n\u00b7heit", "strah\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PIS", "VVFIN", "APPR", "PDAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "So mu\u00df ihr bild von glut in hertz und seelen stehn.", "tokens": ["So", "mu\u00df", "ihr", "bild", "von", "glut", "in", "hertz", "und", "see\u00b7len", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "APPR", "NN", "APPR", "NN", "KON", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}