{"dta.poem.19628": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Die Karyatide.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Im Hof des Louvre tr\u00e4gt ein Weib", "tokens": ["Im", "Hof", "des", "Louv\u00b7re", "tr\u00e4gt", "ein", "Weib"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Zinne mit dem Marmorhaupt,", "tokens": ["Die", "Zin\u00b7ne", "mit", "dem", "Mar\u00b7mor\u00b7haupt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit einem allerliebsten Haupt.", "tokens": ["Mit", "ei\u00b7nem", "al\u00b7ler\u00b7liebs\u00b7ten", "Haupt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als Meister Goujon sie geformt", "tokens": ["Als", "Meis\u00b7ter", "Gou\u00b7jon", "sie", "ge\u00b7formt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "NE", "PPER", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "In feinen Linien, \u00fcberschlank,", "tokens": ["In", "fei\u00b7nen", "Li\u00b7ni\u00b7en", ",", "\u00fc\u00b7bersc\u00b7hlank", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Und stehend auf dem Bauger\u00fcst", "tokens": ["Und", "ste\u00b7hend", "auf", "dem", "Bau\u00b7ge\u00b7r\u00fcst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die letzte Locke mei\u00dfelte,", "tokens": ["Die", "letz\u00b7te", "Lo\u00b7cke", "mei\u00b7\u00dfel\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Erscho\u00df den Meister hinterr\u00fccks", "tokens": ["Er\u00b7scho\u00df", "den", "Meis\u00b7ter", "hin\u00b7ter\u00b7r\u00fccks"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "(am Tag der Saint-Barth", "tokens": ["(", "am", "Tag", "der", "Saint\u00b7Barth"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "APPRART", "NN", "ART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.10": {"text": "Ein \u00fcberzeugter Katholik.", "tokens": ["Ein", "\u00fc\u00b7berz\u00b7eug\u00b7ter", "Ka\u00b7tho\u00b7lik", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Vorst\u00fcrzend \u00fcberflutet' er", "tokens": ["Vor\u00b7st\u00fcr\u00b7zend", "\u00fc\u00b7ber\u00b7flu\u00b7tet'", "er"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Den feinen Busen ganz mit Blut,", "tokens": ["Den", "fei\u00b7nen", "Bu\u00b7sen", "ganz", "mit", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Dann sank er r\u00fccklings in den Hof.", "tokens": ["Dann", "sank", "er", "r\u00fcck\u00b7lings", "in", "den", "Hof", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Die Marmormagd entschlummerte", "tokens": ["Die", "Mar\u00b7mor\u00b7magd", "ent\u00b7schlum\u00b7mer\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Und schlief dreihundert Jahre lang,", "tokens": ["Und", "schlief", "drei\u00b7hun\u00b7dert", "Jah\u00b7re", "lang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "CARD", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Ein Feuerschein erw\u00e4rmte sie", "tokens": ["Ein", "Feu\u00b7er\u00b7schein", "er\u00b7w\u00e4rm\u00b7te", "sie"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "(am Tag da die Comm\u00fcne focht)", "tokens": ["(", "am", "Tag", "da", "die", "Com\u00b7m\u00fc\u00b7ne", "focht", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPRART", "NN", "ADV", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Sie g\u00e4hnt' und blickte rings sich um:", "tokens": ["Sie", "g\u00e4hnt'", "und", "blick\u00b7te", "rings", "sich", "um", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ADV", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Wo bin ich denn? In welcher Stadt?", "tokens": ["Wo", "bin", "ich", "denn", "?", "In", "wel\u00b7cher", "Stadt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "$.", "APPR", "PWAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Sie morden sich. Es ist Paris.", "tokens": ["Sie", "mor\u00b7den", "sich", ".", "Es", "ist", "Pa\u00b7ris", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "$.", "PPER", "VAFIN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}