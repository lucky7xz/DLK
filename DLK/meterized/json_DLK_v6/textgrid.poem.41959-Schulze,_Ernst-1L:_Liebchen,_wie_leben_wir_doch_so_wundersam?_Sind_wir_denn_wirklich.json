{"textgrid.poem.41959": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Liebchen, wie leben wir doch so wundersam? Sind wir denn wirklich", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Liebchen, wie leben wir doch so wundersam? Sind wir denn wirklich", "tokens": ["Lieb\u00b7chen", ",", "wie", "le\u00b7ben", "wir", "doch", "so", "wun\u00b7der\u00b7sam", "?", "Sind", "wir", "denn", "wirk\u00b7lich"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "PWAV", "VVFIN", "PPER", "ADV", "ADV", "ADJD", "$.", "VAFIN", "PPER", "ADV", "ADJD"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Eins in das Andre verliebt, oder betr\u00fcgt uns der Schein?", "tokens": ["Eins", "in", "das", "And\u00b7re", "ver\u00b7liebt", ",", "o\u00b7der", "be\u00b7tr\u00fcgt", "uns", "der", "Schein", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "PIS", "VVPP", "$,", "KON", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Traulich sitzen wir oft, und es scherzt muthwillig der Leichtsinn", "tokens": ["Trau\u00b7lich", "sit\u00b7zen", "wir", "oft", ",", "und", "es", "scherzt", "mut\u00b7hwil\u00b7lig", "der", "Leicht\u00b7sinn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "ADV", "$,", "KON", "PPER", "VVFIN", "ADJD", "ART", "NN"], "meter": "+-+--++-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Ueber das tiefe Gef\u00fchl, \u00fcber ein schw\u00e4rmendes Paar;", "tokens": ["Ue\u00b7ber", "das", "tie\u00b7fe", "Ge\u00b7f\u00fchl", ",", "\u00fc\u00b7ber", "ein", "schw\u00e4r\u00b7men\u00b7des", "Paar", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.5": {"text": "Dichten vereint Spottliederchen oft auf den kindischen Amor,", "tokens": ["Dich\u00b7ten", "ver\u00b7eint", "Spott\u00b7lie\u00b7der\u00b7chen", "oft", "auf", "den", "kin\u00b7di\u00b7schen", "A\u00b7mor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "NN", "ADV", "APPR", "ART", "ADJA", "NE", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.6": {"text": "Necken mit stechendem Dorn sicher den trotzigen Gott,", "tokens": ["Ne\u00b7cken", "mit", "ste\u00b7chen\u00b7dem", "Dorn", "si\u00b7cher", "den", "trot\u00b7zi\u00b7gen", "Gott", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.7": {"text": "Und doch lieg' ich so oft zu deinen F\u00fc\u00dfen und fl\u00fcstre,", "tokens": ["Und", "doch", "lieg'", "ich", "so", "oft", "zu", "dei\u00b7nen", "F\u00fc\u00b7\u00dfen", "und", "fl\u00fcst\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "ADV", "APPR", "PPOSAT", "NN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Was mich das Herz allein, was nur die Liebe mich lehrt,", "tokens": ["Was", "mich", "das", "Herz", "al\u00b7lein", ",", "was", "nur", "die", "Lie\u00b7be", "mich", "lehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "NN", "ADV", "$,", "PRELS", "ADV", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.9": {"text": "Und du beugst dich herab, und ein gl\u00fchender Ku\u00df, der des Leichtsinns", "tokens": ["Und", "du", "beugst", "dich", "her\u00b7ab", ",", "und", "ein", "gl\u00fc\u00b7hen\u00b7der", "Ku\u00df", ",", "der", "des", "Leicht\u00b7sinns"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV", "$,", "KON", "ART", "ADJA", "NN", "$,", "PRELS", "ART", "NN"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.10": {"text": "L\u00fcge bestraft und des Spotts Dornen, begl\u00fccket den Freund.", "tokens": ["L\u00fc\u00b7ge", "be\u00b7straft", "und", "des", "Spotts", "Dor\u00b7nen", ",", "be\u00b7gl\u00fc\u00b7cket", "den", "Freund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "ART", "NN", "NN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.11": {"text": "Froh dann scheid' ich von dir und schwelg' in s\u00fc\u00dfer Erinnrung,", "tokens": ["Froh", "dann", "scheid'", "ich", "von", "dir", "und", "schwel\u00b7g'", "in", "s\u00fc\u00b7\u00dfer", "E\u00b7rinn\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "VVFIN", "PPER", "APPR", "PPER", "KON", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.12": {"text": "W\u00e4hn' ein ewiges Band kette mein fl\u00fcchtiges Herz:", "tokens": ["W\u00e4hn'", "ein", "e\u00b7wi\u00b7ges", "Band", "ket\u00b7te", "mein", "fl\u00fcch\u00b7ti\u00b7ges", "Herz", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+--+---+--+", "measure": "trochaic.penta.relaxed"}, "line.13": {"text": "Doch kaum flieht der Moment, so umstrickt arglistig den Frevler,", "tokens": ["Doch", "kaum", "flieht", "der", "Mo\u00b7ment", ",", "so", "um\u00b7strickt", "arg\u00b7lis\u00b7tig", "den", "Frev\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "$,", "ADV", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.14": {"text": "Welcher die Fessel so gern duldet ein anderes Netz,", "tokens": ["Wel\u00b7cher", "die", "Fes\u00b7sel", "so", "gern", "dul\u00b7det", "ein", "an\u00b7de\u00b7res", "Netz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "ART", "NN", "ADV", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}, "line.15": {"text": "Ach, und jeglichen Schwur, den ich dir that, tilgt die Bezaubrung,", "tokens": ["Ach", ",", "und", "jeg\u00b7li\u00b7chen", "Schwur", ",", "den", "ich", "dir", "that", ",", "tilgt", "die", "Be\u00b7zaub\u00b7rung", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KON", "PIAT", "NN", "$,", "PRELS", "PPER", "PPER", "VVFIN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+-+-++--+-", "measure": "trochaic.septa.relaxed"}, "line.16": {"text": "Gleich dem Gew\u00f6lk, das rasch gaukelnde Winde zerstreun.", "tokens": ["Gleich", "dem", "Ge\u00b7w\u00f6lk", ",", "das", "rasch", "gau\u00b7keln\u00b7de", "Win\u00b7de", "zer\u00b7streun", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PRELS", "ADJD", "ADJA", "NN", "VVINF", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.17": {"text": "Doch noch nimmer verga\u00df ich dich selbst, und, bin ich auch treulos,", "tokens": ["Doch", "noch", "nim\u00b7mer", "ver\u00b7ga\u00df", "ich", "dich", "selbst", ",", "und", ",", "bin", "ich", "auch", "treu\u00b7los", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VVFIN", "PPER", "PRF", "ADV", "$,", "KON", "$,", "VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "+-+--+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.18": {"text": "Nur dein eigener Reiz trage des Irrenden Schuld.", "tokens": ["Nur", "dein", "ei\u00b7ge\u00b7ner", "Reiz", "tra\u00b7ge", "des", "Ir\u00b7ren\u00b7den", "Schuld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "--+--+---+--+", "measure": "anapaest.di.plus"}, "line.19": {"text": "Ach, du fesseltest mich mit ewigen Banden der Sehnsucht,", "tokens": ["Ach", ",", "du", "fes\u00b7sel\u00b7test", "mich", "mit", "e\u00b7wi\u00b7gen", "Ban\u00b7den", "der", "Sehn\u00b7sucht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "PRF", "APPR", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.20": {"text": "Jegliches holde Gesicht zeigt dem Get\u00e4uschten dein Bild:", "tokens": ["Jeg\u00b7li\u00b7ches", "hol\u00b7de", "Ge\u00b7sicht", "zeigt", "dem", "Ge\u00b7t\u00e4uschten", "dein", "Bild", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+--+--+-+-+-+", "measure": "dactylic.di.plus"}, "line.21": {"text": "Doch kaum bin ich dir wieder genaht, so beugt mich gewaltsam", "tokens": ["Doch", "kaum", "bin", "ich", "dir", "wie\u00b7der", "ge\u00b7naht", ",", "so", "beugt", "mich", "ge\u00b7walt\u00b7sam"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PPER", "PPER", "ADV", "VVPP", "$,", "ADV", "VVFIN", "PPER", "ADJD"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.22": {"text": "Dein allm\u00e4chtiger Blick wieder in's vorige Joch.", "tokens": ["Dein", "all\u00b7m\u00e4ch\u00b7ti\u00b7ger", "Blick", "wie\u00b7der", "in's", "vo\u00b7ri\u00b7ge", "Joch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "--+--+---+--+", "measure": "anapaest.di.plus"}, "line.23": {"text": "Wahrlich, dir schenkte Cythere gewi\u00df den magischen G\u00fcrtel,", "tokens": ["Wahr\u00b7lich", ",", "dir", "schenk\u00b7te", "Cy\u00b7the\u00b7re", "ge\u00b7wi\u00df", "den", "ma\u00b7gi\u00b7schen", "G\u00fcr\u00b7tel", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "VVFIN", "NE", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-+--+-", "measure": "iambic.septa.invert"}, "line.24": {"text": "Welchen mit Zaubergeflecht webte der Grazien Hand.", "tokens": ["Wel\u00b7chen", "mit", "Zau\u00b7ber\u00b7ge\u00b7flecht", "web\u00b7te", "der", "Gra\u00b7zi\u00b7en", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "APPR", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.25": {"text": "Was du auch thust, du thust es mit Reiz, und heimliche Anmuth", "tokens": ["Was", "du", "auch", "thust", ",", "du", "thust", "es", "mit", "Reiz", ",", "und", "heim\u00b7li\u00b7che", "An\u00b7muth"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "PPER", "ADV", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "APPR", "NN", "$,", "KON", "ADJA", "NN"], "meter": "-+-+-+--+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.26": {"text": "Schwebt im Blick dir und schwebt rings um die ganze Gestalt.", "tokens": ["Schwebt", "im", "Blick", "dir", "und", "schwebt", "rings", "um", "die", "gan\u00b7ze", "Ge\u00b7stalt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "PPER", "KON", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.27": {"text": "Kehr' ich dir wieder zur\u00fcck, ja dann gesteh' ich dir Alles,", "tokens": ["Kehr'", "ich", "dir", "wie\u00b7der", "zu\u00b7r\u00fcck", ",", "ja", "dann", "ge\u00b7steh'", "ich", "dir", "Al\u00b7les", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPER", "ADV", "PTKVZ", "$,", "ADV", "ADV", "VVFIN", "PPER", "PPER", "PIS", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.28": {"text": "Preise die Sch\u00f6nen sogar, welche mich gestern besiegt,", "tokens": ["Prei\u00b7se", "die", "Sch\u00f6\u00b7nen", "so\u00b7gar", ",", "wel\u00b7che", "mich", "ge\u00b7stern", "be\u00b7siegt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "$,", "PRELS", "PPER", "ADV", "VVPP", "$,"], "meter": "+--+-+-+-+-+-+", "measure": "iambic.septa.invert"}, "line.29": {"text": "Und, bei Gott, kaum trau' ich mir selbst, du h\u00f6rst es geduldig,", "tokens": ["Und", ",", "bei", "Gott", ",", "kaum", "trau'", "ich", "mir", "selbst", ",", "du", "h\u00f6rst", "es", "ge\u00b7dul\u00b7dig", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "NN", "$,", "ADV", "VVFIN", "PPER", "PPER", "ADV", "$,", "PPER", "VVFIN", "PPER", "ADJD", "$,"], "meter": "--+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.30": {"text": "Stimmst in's schmeichelnde Lob gern und gef\u00e4llig mit ein,", "tokens": ["Stimmst", "in's", "schmei\u00b7cheln\u00b7de", "Lob", "gern", "und", "ge\u00b7f\u00e4l\u00b7lig", "mit", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "ADV", "KON", "ADJD", "APPR", "ART", "$,"], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.31": {"text": "Lobst den beweglichen Geist, der nur die Gen\u00fcsse des Lebens", "tokens": ["Lobst", "den", "be\u00b7weg\u00b7li\u00b7chen", "Geist", ",", "der", "nur", "die", "Ge\u00b7n\u00fcs\u00b7se", "des", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN", "$,", "PRELS", "ADV", "ART", "NN", "ART", "NN"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.32": {"text": "Leis' umschwebt und den Schmerz unter die Winde verstreut,", "tokens": ["Leis'", "um\u00b7schwebt", "und", "den", "Schmerz", "un\u00b7ter", "die", "Win\u00b7de", "ver\u00b7streut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "KON", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.33": {"text": "Neckst nur zuweilen mit stechendem Scherz den Armen, der Dornen", "tokens": ["Neckst", "nur", "zu\u00b7wei\u00b7len", "mit", "ste\u00b7chen\u00b7dem", "Scherz", "den", "Ar\u00b7men", ",", "der", "Dor\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ADV", "ADV", "APPR", "ADJA", "NN", "ART", "NN", "$,", "ART", "NN"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.34": {"text": "Statt der Rosen empfing, die er zu pfl\u00fccken gehofft.", "tokens": ["Statt", "der", "Ro\u00b7sen", "emp\u00b7fing", ",", "die", "er", "zu", "pfl\u00fc\u00b7cken", "ge\u00b7hofft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "$,", "PRELS", "PPER", "PTKZU", "VVINF", "VVPP", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.35": {"text": "Schalkhaft zeigst du ein Briefchen mir dann, das du eben empfangen,", "tokens": ["Schalk\u00b7haft", "zeigst", "du", "ein", "Brief\u00b7chen", "mir", "dann", ",", "das", "du", "e\u00b7ben", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "PPER", "ADV", "$,", "PRELS", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+--+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.36": {"text": "Aber die Unterschrift deckst du mit sorglicher Hand,", "tokens": ["A\u00b7ber", "die", "Un\u00b7ter\u00b7schrift", "deckst", "du", "mit", "sorg\u00b7li\u00b7cher", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.37": {"text": "R\u00fchmst mir die Blume, die j\u00fcngst ein Unbekannter dir sandte,", "tokens": ["R\u00fchmst", "mir", "die", "Blu\u00b7me", ",", "die", "j\u00fcngst", "ein", "Un\u00b7be\u00b7kann\u00b7ter", "dir", "sand\u00b7te", ","], "token_info": ["word", "word", "word