{"dta.poem.742": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "XxVIII.  An  P\u0153tum. Anno \u217d\u217c\u2184 \u217c\u2184 \u217d  cxxxvii.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Dv P&#339;tus/ du beginst die Musen denn zu hassen/ ", "tokens": ["Dv", "P", "&#339;", "tus", "/", "du", "be\u00b7ginst", "die", "Mu\u00b7sen", "denn", "zu", "has\u00b7sen", "/"], "token_info": ["word", "word", "XML_entity", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$(", "PPER", "VVFIN", "ART", "NN", "KON", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vnd ", "tokens": ["Vnd"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Die Phillis die nichts weis von Ehr vnd Redligkeit: ", "tokens": ["Die", "Phil\u00b7lis", "die", "nichts", "weis", "von", "Ehr", "vnd", "Red\u00b7lig\u00b7keit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "ART", "PIS", "PTKVZ", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vnd die sich ohne Mann\u2019 hat Mutter nennen lassen.", "tokens": ["Vnd", "die", "sich", "oh\u00b7ne", "Mann'", "hat", "Mut\u00b7ter", "nen\u00b7nen", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRELS", "PRF", "APPR", "NN", "VAFIN", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Du Thor/ du glaubest no", "tokens": ["Du", "Thor", "/", "du", "glau\u00b7best", "no"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "NN", "$(", "PPER", "VVFIN", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ein jeder nur von dier/ vnd deinem gl\u00fccke s", "tokens": ["Ein", "je\u00b7der", "nur", "von", "dier", "/", "vnd", "dei\u00b7nem", "gl\u00fc\u00b7cke", "s"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PIS", "ADV", "APPR", "PDAT", "$(", "KON", "PPOSAT", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ja wol der Vogel selbst/ der vns die heisse zeit", "tokens": ["Ja", "wol", "der", "Vo\u00b7gel", "selbst", "/", "der", "vns", "die", "heis\u00b7se", "zeit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "ADV", "ART", "NN", "ADV", "$(", "PRELS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "An meldet/ wird dir auch ein Hochzeit Lied verfassen.", "tokens": ["An", "mel\u00b7det", "/", "wird", "dir", "auch", "ein", "Hoch\u00b7zeit", "Lied", "ver\u00b7fas\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "$(", "VAFIN", "PPER", "ADV", "ART", "NN", "NN", "VVPP", "$."], "meter": "---+-+-+-+-+-", "measure": "unknown.measure.penta"}}, "stanza.3": {"line.1": {"text": "Dein gl\u00fcck ist freylich gro\u00df/ du nimbst f\u00fcr eigen a&#241; ", "tokens": ["Dein", "gl\u00fcck", "ist", "frey\u00b7lich", "gro\u00df", "/", "du", "nimbst", "f\u00fcr", "ei\u00b7gen", "a", "&#241;"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "XML_entity"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "$(", "PPER", "ADV", "APPR", "ADJD", "NE", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was ", "tokens": ["Was"], "token_info": ["word"], "pos": ["PWS"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Lucullus seet/ dir bleibt der Acker mit den Fr\u00fcchten. ", "tokens": ["Lu\u00b7cul\u00b7lus", "seet", "/", "dir", "bleibt", "der", "A\u00b7cker", "mit", "den", "Fr\u00fcch\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$(", "PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Stell\u2019 alle sorgen eyn/ wirff ", "tokens": ["Stell'", "al\u00b7le", "sor\u00b7gen", "eyn", "/", "wirff"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["NE", "PIAT", "NN", "ART", "$(", "XY"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Wenn ", "tokens": ["Wenn"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Als wenn du gantze Jahr wilst part\u2019 vnd h\u00e4ndel schlichten.", "tokens": ["Als", "wenn", "du", "gant\u00b7ze", "Jahr", "wilst", "part'", "vnd", "h\u00e4n\u00b7del", "schlich\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "ADJA", "NN", "VMFIN", "VVPP", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}