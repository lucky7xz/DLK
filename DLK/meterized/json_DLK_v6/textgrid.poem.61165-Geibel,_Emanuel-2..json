{"textgrid.poem.61165": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es fliegt manch V\u00f6glein in das Nest", "tokens": ["Es", "fliegt", "manch", "V\u00f6\u00b7glein", "in", "das", "Nest"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und fliegt auch wied'r heraus;", "tokens": ["Und", "fliegt", "auch", "wie\u00b7d'r", "he\u00b7raus", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und bist du mal mein Schatz gewest,", "tokens": ["Und", "bist", "du", "mal", "mein", "Schatz", "ge\u00b7west", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So ist die Liebschaft aus.", "tokens": ["So", "ist", "die", "Lieb\u00b7schaft", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Du hast mich schlimm betrogen", "tokens": ["Du", "hast", "mich", "schlimm", "be\u00b7tro\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Um schn\u00f6den Geldgewinn -", "tokens": ["Um", "schn\u00f6\u00b7den", "Geld\u00b7ge\u00b7winn"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Viel Gl\u00fcck, viel Gl\u00fcck zum reichen Mann!", "tokens": ["Viel", "Gl\u00fcck", ",", "viel", "Gl\u00fcck", "zum", "rei\u00b7chen", "Mann", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Geh du nur immer hin!", "tokens": ["Geh", "du", "nur", "im\u00b7mer", "hin", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Viel Bl\u00fcmlein stehn im hohen Korn", "tokens": ["Viel", "Bl\u00fcm\u00b7lein", "stehn", "im", "ho\u00b7hen", "Korn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von rot und blauer Zier,", "tokens": ["Von", "rot", "und", "blau\u00b7er", "Zier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und hast du eins davon verlorn,", "tokens": ["Und", "hast", "du", "eins", "da\u00b7von", "ver\u00b7lorn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PIS", "PAV", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So such' ein andres dir.", "tokens": ["So", "such'", "ein", "and\u00b7res", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "PIS", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Glaub' nicht, da\u00df ich mich gr\u00e4me", "tokens": ["Glaub'", "nicht", ",", "da\u00df", "ich", "mich", "gr\u00e4\u00b7me"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "PRF", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Um deinen falschen Sinn -", "tokens": ["Um", "dei\u00b7nen", "fal\u00b7schen", "Sinn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ich find' schon einen andern Schatz;", "tokens": ["Ich", "find'", "schon", "ei\u00b7nen", "an\u00b7dern", "Schatz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Geh du nur immer hin!", "tokens": ["Geh", "du", "nur", "im\u00b7mer", "hin", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}