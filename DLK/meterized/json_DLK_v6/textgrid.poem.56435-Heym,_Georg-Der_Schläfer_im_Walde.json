{"textgrid.poem.56435": {"metadata": {"author": {"name": "Heym, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Der Schl\u00e4fer im Walde", "genre": "verse", "period": "N.A.", "pub_year": 1899, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seit Morgen ruht er. Da die Sonne rot", "tokens": ["Seit", "Mor\u00b7gen", "ruht", "er", ".", "Da", "die", "Son\u00b7ne", "rot"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PPER", "$.", "KOUS", "ART", "NN", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Durch Regenwolken seine Wunde traf.", "tokens": ["Durch", "Re\u00b7gen\u00b7wol\u00b7ken", "sei\u00b7ne", "Wun\u00b7de", "traf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das Laub tropft langsam noch. Der Wald liegt tot.", "tokens": ["Das", "Laub", "tropft", "lang\u00b7sam", "noch", ".", "Der", "Wald", "liegt", "tot", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "ADV", "$.", "ART", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Im Baume ruft ein V\u00f6gelchen im Schlaf.", "tokens": ["Im", "Bau\u00b7me", "ruft", "ein", "V\u00f6\u00b7gel\u00b7chen", "im", "Schlaf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der Tote schl\u00e4ft im ewigen Vergessen,", "tokens": ["Der", "To\u00b7te", "schl\u00e4ft", "im", "e\u00b7wi\u00b7gen", "Ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Umrauscht vom Walde. Und die W\u00fcrmer singen,", "tokens": ["Um\u00b7rauscht", "vom", "Wal\u00b7de", ".", "Und", "die", "W\u00fcr\u00b7mer", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$.", "KON", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die in des Sch\u00e4dels H\u00f6hle tief sich fressen,", "tokens": ["Die", "in", "des", "Sch\u00e4\u00b7dels", "H\u00f6h\u00b7le", "tief", "sich", "fres\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "NN", "ADJD", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In seine Tr\u00e4ume ihn mit Fl\u00fcgelklingen.", "tokens": ["In", "sei\u00b7ne", "Tr\u00e4u\u00b7me", "ihn", "mit", "Fl\u00fc\u00b7gel\u00b7klin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wie s\u00fc\u00df ist es, zu tr\u00e4umen nach den Leiden", "tokens": ["Wie", "s\u00fc\u00df", "ist", "es", ",", "zu", "tr\u00e4u\u00b7men", "nach", "den", "Lei\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "$,", "PTKZU", "VVINF", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Traum, in Licht und Erde zu zerfallen,", "tokens": ["Den", "Traum", ",", "in", "Licht", "und", "Er\u00b7de", "zu", "zer\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "NN", "KON", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nichts mehr zu sein, von allem abzuscheiden,", "tokens": ["Nichts", "mehr", "zu", "sein", ",", "von", "al\u00b7lem", "ab\u00b7zu\u00b7schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "PTKZU", "VAINF", "$,", "APPR", "PIS", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und wie ein Hauch der Nacht hinabzuwallen,", "tokens": ["Und", "wie", "ein", "Hauch", "der", "Nacht", "hin\u00b7ab\u00b7zu\u00b7wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Zum Reich der Schl\u00e4fer. Zu den Hetairien", "tokens": ["Zum", "Reich", "der", "Schl\u00e4\u00b7fer", ".", "Zu", "den", "He\u00b7tai\u00b7ri\u00b7en"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "$.", "APPR", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der Toten unten. Zu den hohen Pal\u00e4sten,", "tokens": ["Der", "To\u00b7ten", "un\u00b7ten", ".", "Zu", "den", "ho\u00b7hen", "Pa\u00b7l\u00e4s\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$.", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Davon die Bilder in dem Strome ziehen,", "tokens": ["Da\u00b7von", "die", "Bil\u00b7der", "in", "dem", "Stro\u00b7me", "zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zu ihren Tafeln, zu den langen Festen.", "tokens": ["Zu", "ih\u00b7ren", "Ta\u00b7feln", ",", "zu", "den", "lan\u00b7gen", "Fes\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Wo in den Schalen dunkle Flammen schwellen,", "tokens": ["Wo", "in", "den", "Scha\u00b7len", "dunk\u00b7le", "Flam\u00b7men", "schwel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo golden klingen vieler Leiern Saiten.", "tokens": ["Wo", "gol\u00b7den", "klin\u00b7gen", "vie\u00b7ler", "Lei\u00b7ern", "Sai\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Durch hohe Fenster schaun sie auf die Wellen,", "tokens": ["Durch", "ho\u00b7he", "Fens\u00b7ter", "schaun", "sie", "auf", "die", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auf gr\u00fcne Wiesen in den blassen Weiten.", "tokens": ["Auf", "gr\u00fc\u00b7ne", "Wie\u00b7sen", "in", "den", "blas\u00b7sen", "Wei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Er scheint zu l\u00e4cheln aus des Sch\u00e4dels Leere,", "tokens": ["Er", "scheint", "zu", "l\u00e4\u00b7cheln", "aus", "des", "Sch\u00e4\u00b7dels", "Lee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKZU", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Er schl\u00e4ft, ein Gott, den s\u00fc\u00dfer Traum bezwang.", "tokens": ["Er", "schl\u00e4ft", ",", "ein", "Gott", ",", "den", "s\u00fc\u00b7\u00dfer", "Traum", "be\u00b7zwang", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "$,", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die W\u00fcrmer bl\u00e4hen sich in seiner Schw\u00e4re,", "tokens": ["Die", "W\u00fcr\u00b7mer", "bl\u00e4\u00b7hen", "sich", "in", "sei\u00b7ner", "Schw\u00e4\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie kriechen satt die rote Stirn entlang.", "tokens": ["Sie", "krie\u00b7chen", "satt", "die", "ro\u00b7te", "Stirn", "ent\u00b7lang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Ein Falter kommt die Schlucht herab. Er ruht", "tokens": ["Ein", "Fal\u00b7ter", "kommt", "die", "Schlucht", "her\u00b7ab", ".", "Er", "ruht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ADV", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Auf Blumen. Und er senkt sich m\u00fcd", "tokens": ["Auf", "Blu\u00b7men", ".", "Und", "er", "senkt", "sich", "m\u00fcd"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$.", "KON", "PPER", "VVFIN", "PRF", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Wunde zu, dem gro\u00dfen Kelch von Blut,", "tokens": ["Der", "Wun\u00b7de", "zu", ",", "dem", "gro\u00b7\u00dfen", "Kelch", "von", "Blut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der wie die Sammetrose dunkel gl\u00fcht.", "tokens": ["Der", "wie", "die", "Sam\u00b7me\u00b7tro\u00b7se", "dun\u00b7kel", "gl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOKOM", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}