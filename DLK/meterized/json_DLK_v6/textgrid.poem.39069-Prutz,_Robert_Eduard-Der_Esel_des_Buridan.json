{"textgrid.poem.39069": {"metadata": {"author": {"name": "Prutz, Robert Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Der Esel des Buridan", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Rechts Heu und Klee, links Heu und Klee!", "tokens": ["Rechts", "Heu", "und", "Klee", ",", "links", "Heu", "und", "Klee", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "$,", "ADV", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die allerfettsten Weiden \u2013", "tokens": ["Die", "al\u00b7ler\u00b7fetts\u00b7ten", "Wei\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dem Esel tut das W\u00e4hlen weh,", "tokens": ["Dem", "E\u00b7sel", "tut", "das", "W\u00e4h\u00b7len", "weh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "er kann sich nicht entscheiden.", "tokens": ["er", "kann", "sich", "nicht", "ent\u00b7schei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Er schnopert rechts, er schnopert links", "tokens": ["Er", "schno\u00b7pert", "rechts", ",", "er", "schno\u00b7pert", "links"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PPER", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "und dreht sich dreimal um \u2013", "tokens": ["und", "dreht", "sich", "drei\u00b7mal", "um", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADV", "APPR", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "O Buridan, o Buridan,", "tokens": ["O", "Bu\u00b7ri\u00b7dan", ",", "o", "Bu\u00b7ri\u00b7dan", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "FM", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "was ist dein Esel dumm!", "tokens": ["was", "ist", "dein", "E\u00b7sel", "dumm", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPOSAT", "NN", "ADJD", "$."], "meter": "---+-+", "measure": "unknown.measure.di"}}, "stanza.2": {"line.1": {"text": "Rechts Gras und Korn, links Gras und Korn,", "tokens": ["Rechts", "Gras", "und", "Korn", ",", "links", "Gras", "und", "Korn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "$,", "ADV", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "wie knurrt es ihm im Magen!", "tokens": ["wie", "knurrt", "es", "ihm", "im", "Ma\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und immer wieder geht's von vorn,", "tokens": ["Und", "im\u00b7mer", "wie\u00b7der", "geht's", "von", "vorn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VVFIN", "APPR", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "er mag die Wahl nicht wagen.", "tokens": ["er", "mag", "die", "Wahl", "nicht", "wa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So zwischen beiden bleibt er stehn", "tokens": ["So", "zwi\u00b7schen", "bei\u00b7den", "bleibt", "er", "stehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PIS", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "und f\u00e4llt vor Hunger um \u2013", "tokens": ["und", "f\u00e4llt", "vor", "Hun\u00b7ger", "um", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "APPR", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "O Buridan, o Buridan,", "tokens": ["O", "Bu\u00b7ri\u00b7dan", ",", "o", "Bu\u00b7ri\u00b7dan", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "FM", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "was war dein Esel dumm! \u2013", "tokens": ["was", "war", "dein", "E\u00b7sel", "dumm", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VAFIN", "PPOSAT", "NN", "ADJD", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Rechts freie Presse, links Zensur,", "tokens": ["Rechts", "frei\u00b7e", "Pres\u00b7se", ",", "links", "Zen\u00b7sur", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "rechts Wahrheit, links die L\u00fcge \u2013", "tokens": ["rechts", "Wahr\u00b7heit", ",", "links", "die", "L\u00fc\u00b7ge", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Was stehen wir und gr\u00fcbeln nur", "tokens": ["Was", "ste\u00b7hen", "wir", "und", "gr\u00fc\u00b7beln", "nur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "KON", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und haben's nicht Gen\u00fcge?", "tokens": ["und", "ha\u00b7ben's", "nicht", "Ge\u00b7n\u00fc\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PTKNEG", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wir horchen rechts, wir horchen links", "tokens": ["Wir", "hor\u00b7chen", "rechts", ",", "wir", "hor\u00b7chen", "links"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PPER", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "und fragen fern und nah \u2013", "tokens": ["und", "fra\u00b7gen", "fern", "und", "nah", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "KON", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "O Buridan, o Buridan,", "tokens": ["O", "Bu\u00b7ri\u00b7dan", ",", "o", "Bu\u00b7ri\u00b7dan", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "FM", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "w\u00e4r' doch dein Esel da!", "tokens": ["w\u00e4r'", "doch", "dein", "E\u00b7sel", "da", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-++-+", "measure": "unknown.measure.tetra"}}, "stanza.4": {"line.1": {"text": "Die Freiheit rechts, links Sklaverei,", "tokens": ["Die", "Frei\u00b7heit", "rechts", ",", "links", "Skla\u00b7ve\u00b7rei", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "wer k\u00f6nnt' es sich verhehlen!", "tokens": ["wer", "k\u00f6nnt'", "es", "sich", "ver\u00b7heh\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wir aber stehn und stehn dabei", "tokens": ["Wir", "a\u00b7ber", "stehn", "und", "stehn", "da\u00b7bei"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVINF", "KON", "VVFIN", "PAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und wissen nicht zu w\u00e4hlen.", "tokens": ["und", "wis\u00b7sen", "nicht", "zu", "w\u00e4h\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So sind wir doch weit \u00e4rger noch", "tokens": ["So", "sind", "wir", "doch", "weit", "\u00e4r\u00b7ger", "noch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJD", "ADJD", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "und dummer noch f\u00fcrwahr,", "tokens": ["und", "dum\u00b7mer", "noch", "f\u00fcr\u00b7wahr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "ADV", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "o Buridan, o Buridan,", "tokens": ["o", "Bu\u00b7ri\u00b7dan", ",", "o", "Bu\u00b7ri\u00b7dan", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["FM", "NE", "$,", "FM", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "als wie dein Esel war!", "tokens": ["als", "wie", "dein", "E\u00b7sel", "war", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}