{"textgrid.poem.52070": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Indem die gantze Welt itzt Alexandrisirt,", "tokens": ["In\u00b7dem", "die", "gant\u00b7ze", "Welt", "itzt", "A\u00b7le\u00b7xand\u00b7ri\u00b7sirt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "ADV", "NE", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So wil ich auch mein Fa\u00df von Ort auff Ort bewegen,", "tokens": ["So", "wil", "ich", "auch", "mein", "Fa\u00df", "von", "Ort", "auff", "Ort", "be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "PPOSAT", "NN", "APPR", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "In dem nichts anders hat als dis Papier gelegen,", "tokens": ["In", "dem", "nichts", "an\u00b7ders", "hat", "als", "dis", "Pa\u00b7pier", "ge\u00b7le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIS", "ADV", "VAFIN", "KOKOM", "PDS", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das sein Diogenes und Lasterst\u00fcrmer f\u00fchrt.", "tokens": ["Das", "sein", "Dio\u00b7ge\u00b7nes", "und", "Las\u00b7ter\u00b7st\u00fcr\u00b7mer", "f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NE", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.2": {"line.1": {"text": "Es z\u00fcrnen meine Freund auff diesen Tag mit mir,", "tokens": ["Es", "z\u00fcr\u00b7nen", "mei\u00b7ne", "Freund", "auff", "die\u00b7sen", "Tag", "mit", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "PDAT", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil dies' in Krieg, in Rath mich jene wolten haben,", "tokens": ["Weil", "dies'", "in", "Krieg", ",", "in", "Rath", "mich", "je\u00b7ne", "wol\u00b7ten", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "APPR", "NN", "$,", "APPR", "NN", "PPER", "PDS", "VMFIN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df auch vor mir nicht sol ein Zug von Sechsen draben,", "tokens": ["Da\u00df", "auch", "vor", "mir", "nicht", "sol", "ein", "Zug", "von", "Sech\u00b7sen", "dra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "PPER", "PTKNEG", "VMFIN", "ART", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Auch nicht sol gleich wie sie vor andern ragen f\u00fcr.", "tokens": ["Auch", "nicht", "sol", "gleich", "wie", "sie", "vor", "an\u00b7dern", "ra\u00b7gen", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "VMFIN", "ADV", "KOKOM", "PPER", "APPR", "PIS", "VVFIN", "APPR", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Verzeiht mir es, ihr Herrn, ich wil viel lieber schaun,", "tokens": ["Ver\u00b7zeiht", "mir", "es", ",", "ihr", "Herrn", ",", "ich", "wil", "viel", "lie\u00b7ber", "schaun", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$,", "PPOSAT", "NN", "$,", "PPER", "VMFIN", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie auff den Schauplatz Ihr k\u00f6nnt Ammt und Ansehn st\u00e4rcken,", "tokens": ["Wie", "auff", "den", "Schau\u00b7platz", "Ihr", "k\u00f6nnt", "Ammt", "und", "An\u00b7sehn", "st\u00e4r\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "PPER", "VVFIN", "NE", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es suchte niemand nicht die Fehltritt anzumercken", "tokens": ["Es", "such\u00b7te", "nie\u00b7mand", "nicht", "die", "Fehl\u00b7tritt", "an\u00b7zu\u00b7mer\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIS", "PTKNEG", "ART", "NN", "VVIZU"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Solt ich mitspieln und auch mich frembden Urtheln traun.", "tokens": ["Solt", "ich", "mit\u00b7spieln", "und", "auch", "mich", "fremb\u00b7den", "Ur\u00b7theln", "traun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "KON", "ADV", "PPER", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Jedoch, wenn ich ein Buch mit Versen voll gekleckt,", "tokens": ["Je\u00b7doch", ",", "wenn", "ich", "ein", "Buch", "mit", "Ver\u00b7sen", "voll", "ge\u00b7kleckt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "ART", "NN", "APPR", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So hab ich nichts davon: Ich mu\u00df umbsonsten lachen,", "tokens": ["So", "hab", "ich", "nichts", "da\u00b7von", ":", "Ich", "mu\u00df", "um\u00b7bsons\u00b7ten", "la\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIS", "PAV", "$.", "PPER", "VMFIN", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie lustig sich dabey der Leser wei\u00df zu machen,", "tokens": ["Wie", "lus\u00b7tig", "sich", "da\u00b7bey", "der", "Le\u00b7ser", "wei\u00df", "zu", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PRF", "PAV", "ART", "NN", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der meinen Stachel f\u00fchlt und seine Galle schmeckt.", "tokens": ["Der", "mei\u00b7nen", "Sta\u00b7chel", "f\u00fchlt", "und", "sei\u00b7ne", "Gal\u00b7le", "schmeckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Mein Rent Ammt ist ja schlecht: Doch sprecht ihr bey mir ein,", "tokens": ["Mein", "Rent", "Ammt", "ist", "ja", "schlecht", ":", "Doch", "sprecht", "ihr", "bey", "mir", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NE", "VAFIN", "ADV", "ADJD", "$.", "KON", "VVFIN", "PPER", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wird Jeder meine Ruh vor seinen Sorgen preisen;", "tokens": ["Wird", "Je\u00b7der", "mei\u00b7ne", "Ruh", "vor", "sei\u00b7nen", "Sor\u00b7gen", "prei\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Er war selbst Herr der Welt, schaut, im Vor\u00fcber Reisen", "tokens": ["Er", "war", "selbst", "Herr", "der", "Welt", ",", "schaut", ",", "im", "Vor\u00b7\u00fc\u00b7ber", "Rei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "NN", "ART", "NN", "$,", "VVFIN", "$,", "APPRART", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "W\u00fcnscht Alexander doch Diogenes zu seyn.", "tokens": ["W\u00fcnscht", "A\u00b7lex\u00b7an\u00b7der", "doch", "Dio\u00b7ge\u00b7nes", "zu", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "ADV", "NE", "PTKZU", "VAINF", "$."], "meter": "-+-+--+---+", "measure": "iambic.tetra.relaxed"}}}}}