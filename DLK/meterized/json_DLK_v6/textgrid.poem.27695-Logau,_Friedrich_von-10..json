{"textgrid.poem.27695": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "10.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Alten sind ietzund der Jungen ihre Kinder;", "tokens": ["Die", "Al\u00b7ten", "sind", "ie\u00b7tzund", "der", "Jun\u00b7gen", "ih\u00b7re", "Kin\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn diese wissen mehr, und jene wissen minder,", "tokens": ["Denn", "die\u00b7se", "wis\u00b7sen", "mehr", ",", "und", "je\u00b7ne", "wis\u00b7sen", "min\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "ADV", "$,", "KON", "PDS", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wol gut! Weil Gott sein Reich den Kindern anverspricht,", "tokens": ["Wol", "gut", "!", "Weil", "Gott", "sein", "Reich", "den", "Kin\u00b7dern", "an\u00b7ver\u00b7spricht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$.", "KOUS", "NN", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Erbt jene Welt allein, und diese Welt erbt nicht.", "tokens": ["Erbt", "je\u00b7ne", "Welt", "al\u00b7lein", ",", "und", "die\u00b7se", "Welt", "erbt", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "NN", "ADV", "$,", "KON", "PDAT", "NN", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}