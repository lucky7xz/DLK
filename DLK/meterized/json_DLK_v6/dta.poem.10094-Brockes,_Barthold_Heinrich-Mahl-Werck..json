{"dta.poem.10094": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Mahl-Werck.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie herrlich mahlt die bildende Natur!", "tokens": ["Wie", "herr\u00b7lich", "mahlt", "die", "bil\u00b7den\u00b7de", "Na\u00b7tur", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie zierlich zeichnet sie so mancherley Figur!", "tokens": ["Wie", "zier\u00b7lich", "zeich\u00b7net", "sie", "so", "man\u00b7cher\u00b7ley", "Fi\u00b7gur", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie lieblich stellt sie uns, in bunter Harmonie,", "tokens": ["Wie", "lieb\u00b7lich", "stellt", "sie", "uns", ",", "in", "bun\u00b7ter", "Har\u00b7mo\u00b7nie", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die sch\u00f6nste Landschaft vor, voll Th\u00e4ler, Berg\u2019 und W\u00e4lder;", "tokens": ["Die", "sch\u00f6ns\u00b7te", "Land\u00b7schaft", "vor", ",", "voll", "Th\u00e4\u00b7ler", ",", "Ber\u00b7g'", "und", "W\u00e4l\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,", "ADJD", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Hier Fl\u00fcsse wie Crystall, dort Aehren-reiche Felder;", "tokens": ["Hier", "Fl\u00fcs\u00b7se", "wie", "Crys\u00b7tall", ",", "dort", "A\u00b7eh\u00b7ren\u00b7rei\u00b7che", "Fel\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KOKOM", "NN", "$,", "ADV", "ADJA", "NN", "$."], "meter": "-+--++--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Bald B\u00e4che voller Fisch\u2019 und Wiesen voller Vieh!", "tokens": ["Bald", "B\u00e4\u00b7che", "vol\u00b7ler", "Fisch'", "und", "Wie\u00b7sen", "vol\u00b7ler", "Vieh", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ADJA", "NN", "KON", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Das Mahl-Werck ist so sch\u00f6n, so sch\u00f6n die Schilderey,", "tokens": ["Das", "Mahl\u00b7\u00b7Werck", "ist", "so", "sch\u00f6n", ",", "so", "sch\u00f6n", "die", "Schil\u00b7de\u00b7rey", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$,", "ADV", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da\u00df, wenn man es nur recht betrachten wollte;", "tokens": ["Da\u00df", ",", "wenn", "man", "es", "nur", "recht", "be\u00b7trach\u00b7ten", "woll\u00b7te", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUS", "PIS", "PPER", "ADV", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Man, halb erstaunt, fast dencken solte,", "tokens": ["Man", ",", "halb", "er\u00b7staunt", ",", "fast", "den\u00b7cken", "sol\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "ADJD", "VVPP", "$,", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Da\u00df es ein w\u00fcrcklich St\u00fcck vom Paradiese sey.", "tokens": ["Da\u00df", "es", "ein", "w\u00fcrck\u00b7lich", "St\u00fcck", "vom", "Pa\u00b7ra\u00b7die\u00b7se", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJD", "NN", "APPRART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch sieht so mancher Mensch die\u00df sch\u00f6ne Mahl-Werck an", "tokens": ["Doch", "sieht", "so", "man\u00b7cher", "Mensch", "die\u00df", "sch\u00f6\u00b7ne", "Mahl\u00b7\u00b7Werck", "an"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "PIAT", "NN", "PDS", "ADJA", "NN", "APPR"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie ", "tokens": ["Wie"], "token_info": ["word"], "pos": ["PWAV"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Ein w\u00fcrcklich Wunder heissen kann,", "tokens": ["Ein", "w\u00fcrck\u00b7lich", "Wun\u00b7der", "heis\u00b7sen", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und welches recht, als wenn es lebt\u2019 und spr\u00e4che, lie\u00df.", "tokens": ["Und", "wel\u00b7ches", "recht", ",", "als", "wenn", "es", "lebt'", "und", "spr\u00e4\u00b7che", ",", "lie\u00df", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PWS", "ADJD", "$,", "KOKOM", "KOUS", "PPER", "VVFIN", "KON", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Denn, wie wir an zu lauschen fingen,", "tokens": ["Denn", ",", "wie", "wir", "an", "zu", "lau\u00b7schen", "fin\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "PPER", "PTKVZ", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was er doch immer w\u00fcrde bringen", "tokens": ["Was", "er", "doch", "im\u00b7mer", "w\u00fcr\u00b7de", "brin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "ADV", "ADV", "VAFIN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Zu dieses grossen Meisters Ehr;", "tokens": ["Zu", "die\u00b7ses", "gros\u00b7sen", "Meis\u00b7ters", "Ehr", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So sagt er anders nichts als die\u00df:", "tokens": ["So", "sagt", "er", "an\u00b7ders", "nichts", "als", "die\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PIS", "KOKOM", "PDS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}