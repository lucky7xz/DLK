{"textgrid.poem.62035": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "[kennst Du nicht das Licht des Lebens]", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kennst Du nicht das Licht des Lebens,", "tokens": ["Kennst", "Du", "nicht", "das", "Licht", "des", "Le\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kennst Du seine Schatten nur,", "tokens": ["Kennst", "Du", "sei\u00b7ne", "Schat\u00b7ten", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nicht des Lebens goldne Sonne,", "tokens": ["Nicht", "des", "Le\u00b7bens", "gold\u00b7ne", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nur des d\u00fcstern Nebels Spur?", "tokens": ["Nur", "des", "d\u00fcs\u00b7tern", "Ne\u00b7bels", "Spur", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Zage nicht, die Truggestalten", "tokens": ["Za\u00b7ge", "nicht", ",", "die", "Trug\u00b7ge\u00b7stal\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "PTKNEG", "$,", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schwinden hin gleich eitlem Schein,", "tokens": ["Schwin\u00b7den", "hin", "gleich", "eit\u00b7lem", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dorten wird die Tugend leuchten,", "tokens": ["Dor\u00b7ten", "wird", "die", "Tu\u00b7gend", "leuch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und das Blendwerk dunkel sein!", "tokens": ["Und", "das", "Blend\u00b7werk", "dun\u00b7kel", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}