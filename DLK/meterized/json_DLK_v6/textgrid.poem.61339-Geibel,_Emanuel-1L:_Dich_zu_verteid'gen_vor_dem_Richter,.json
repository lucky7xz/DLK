{"textgrid.poem.61339": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dich zu verteid'gen vor dem Richter,", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dich zu verteid'gen vor dem Richter,", "tokens": ["Dich", "zu", "ver\u00b7tei\u00b7d'\u00b7gen", "vor", "dem", "Rich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "APPR", "ART", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "F\u00fchrst deine Lieder du herein?", "tokens": ["F\u00fchrst", "dei\u00b7ne", "Lie\u00b7der", "du", "her\u00b7ein", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O Freund, man kann ein lyrischer Dichter", "tokens": ["O", "Freund", ",", "man", "kann", "ein", "ly\u00b7ri\u00b7scher", "Dich\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "PIS", "VMFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und doch ein dummer Teufel sein.", "tokens": ["Und", "doch", "ein", "dum\u00b7mer", "Teu\u00b7fel", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}