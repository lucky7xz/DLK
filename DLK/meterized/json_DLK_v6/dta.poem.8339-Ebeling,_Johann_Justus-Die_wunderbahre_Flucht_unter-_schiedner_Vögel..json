"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "Wer sagt den Aufbruch an, wer stellt die Ordre", "tokens": ["Wer", "sagt", "den", "Auf\u00b7bruch", "an", ",", "wer", "stellt", "die", "Ord\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ART", "NN", "PTKVZ", "$,", "PWS", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.54": {"text": "Jhr Zug wird anberamt, sie gehn mit starken Braus,", "tokens": ["Ihr", "Zug", "wird", "an\u00b7be\u00b7ramt", ",", "sie", "gehn", "mit", "star\u00b7ken", "Braus", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.55": {"text": "Sie reisen durch die Lufft zum warmen Erdenkreise,", "tokens": ["Sie", "rei\u00b7sen", "durch", "die", "Lufft", "zum", "war\u00b7men", "Er\u00b7den\u00b7krei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.56": {"text": "Durchziehn die leichte Bahn, und wandern Trup-", "tokens": ["Durch\u00b7ziehn", "die", "leich\u00b7te", "Bahn", ",", "und", "wan\u00b7dern", "Trup"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "KON", "VVFIN", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.57": {"text": "O! ewge Vorsehung! du lenkst sie nur allein,", "tokens": ["O", "!", "ew\u00b7ge", "Vor\u00b7se\u00b7hung", "!", "du", "lenkst", "sie", "nur", "al\u00b7lein", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "w