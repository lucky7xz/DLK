{"dta.poem.8339": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Die  \n wunderbahre Flucht unter-  \n schiedner V\u00f6gel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198782", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das wundernsw\u00fcrdigste, das sich an Thie-\nren zeigt,", "tokens": ["Das", "wun\u00b7derns\u00b7w\u00fcr\u00b7digs\u00b7te", ",", "das", "sich", "an", "Thie", "ren", "zeigt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PRELS", "PRF", "APPR", "TRUNC", "NN", "VVFIN", "$,"], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Das \u00fcber dem Begriff der weisen Men-", "tokens": ["Das", "\u00fc\u00b7ber", "dem", "Be\u00b7griff", "der", "wei\u00b7sen", "Men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "APPR", "ART", "NN", "ART", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ist der verborgne Trieb, der in der Brust", "tokens": ["Ist", "der", "ver\u00b7borg\u00b7ne", "Trieb", ",", "der", "in", "der", "Brust"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der ihre Handelung zum weisen Zwek bestimmet.", "tokens": ["Der", "ih\u00b7re", "Han\u00b7de\u00b7lung", "zum", "wei\u00b7sen", "Zwek", "be\u00b7stim\u00b7met", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Man seh die Schwalben Flucht, der Wachteln Bei-", "tokens": ["Man", "seh", "die", "Schwal\u00b7ben", "Flucht", ",", "der", "Wach\u00b7teln", "Bei"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ART", "NN", "NN", "$,", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und auch den Krannichs Zug, woran man lesen", "tokens": ["Und", "auch", "den", "Kran\u00b7nichs", "Zug", ",", "wo\u00b7ran", "man", "le\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "NN", "$,", "PWAV", "PIS", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Da\u00df jede seine Zeit, wie uns die Schrifft gelehret,", "tokens": ["Da\u00df", "je\u00b7de", "sei\u00b7ne", "Zeit", ",", "wie", "uns", "die", "Schrifft", "ge\u00b7leh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "PPOSAT", "NN", "$,", "PWAV", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Jm Herbst und Fr\u00fchling hat, da sie hin, wieder", "tokens": ["Jm", "Herbst", "und", "Fr\u00fch\u00b7ling", "hat", ",", "da", "sie", "hin", ",", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["APPRART", "NN", "KON", "NN", "VAFIN", "$,", "KOUS", "PPER", "PTKVZ", "$,", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Wenn der bestimmte Tag, im rauhen Herbst erscheint,", "tokens": ["Wenn", "der", "be\u00b7stimm\u00b7te", "Tag", ",", "im", "rau\u00b7hen", "Herbst", "er\u00b7scheint", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$,", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Sind diese V\u00f6gelein zu ihrer Flucht vereint.", "tokens": ["Sind", "die\u00b7se", "V\u00f6\u00b7ge\u00b7lein", "zu", "ih\u00b7rer", "Flucht", "ver\u00b7eint", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Sie sammlen sich zu Hauff und ziehn mit grossen", "tokens": ["Sie", "samm\u00b7len", "sich", "zu", "Hauff", "und", "ziehn", "mit", "gros\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "KON", "VVFIN", "APPR", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Jm Fluge durch die Lufft, und \u00fcber Erd und Meere,", "tokens": ["Jm", "Flu\u00b7ge", "durch", "die", "Lufft", ",", "und", "\u00fc\u00b7ber", "Erd", "und", "Mee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "$,", "KON", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "In andre Gegend hin. Sie ziehen schleunig fort,", "tokens": ["In", "and\u00b7re", "Ge\u00b7gend", "hin", ".", "Sie", "zie\u00b7hen", "schleu\u00b7nig", "fort", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$.", "PPER", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "In andre L\u00e4nder hin, an einem warmen Ort;", "tokens": ["In", "and\u00b7re", "L\u00e4n\u00b7der", "hin", ",", "an", "ei\u00b7nem", "war\u00b7men", "Ort", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$,", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Sie kommen bei dem Schein der warmen Fr\u00fchlings", "tokens": ["Sie", "kom\u00b7men", "bei", "dem", "Schein", "der", "war\u00b7men", "Fr\u00fch\u00b7lings"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Aus ihren Auffenthalt der Winterszeit zur\u00fckke.", "tokens": ["Aus", "ih\u00b7ren", "Auf\u00b7fent\u00b7halt", "der", "Win\u00b7ter\u00b7szeit", "zu\u00b7r\u00fck\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Zwar einge finden sich, die das in Zweiffel ziehn,", "tokens": ["Zwar", "ein\u00b7ge", "fin\u00b7den", "sich", ",", "die", "das", "in", "Zweif\u00b7fel", "ziehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PRF", "$,", "PRELS", "ART", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Da\u00df Schwalben ganz hinweg in fremde Gegend", "tokens": ["Da\u00df", "Schwal\u00b7ben", "ganz", "hin\u00b7weg", "in", "frem\u00b7de", "Ge\u00b7gend"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "ADV", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Sie meinen, da\u00df sie sich in tieffen Grund verstek-", "tokens": ["Sie", "mei\u00b7nen", ",", "da\u00df", "sie", "sich", "in", "tief\u00b7fen", "Grund", "vers\u00b7tek"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "PRF", "APPR", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Bis sie sich durch den Strahl der Sonnen aufer-", "tokens": ["Bis", "sie", "sich", "durch", "den", "Strahl", "der", "Son\u00b7nen", "auf\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "APPR", "ART", "NN", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Dem sey nun, wie ihm sey; gnug zu der Herbstes", "tokens": ["Dem", "sey", "nun", ",", "wie", "ihm", "sey", ";", "gnug", "zu", "der", "Herbs\u00b7tes"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ADV", "$,", "PWAV", "PPER", "VAFIN", "$.", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Sind sie wie Kraniche zu ihrer Flucht bereit;", "tokens": ["Sind", "sie", "wie", "Kra\u00b7ni\u00b7che", "zu", "ih\u00b7rer", "Flucht", "be\u00b7reit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "KOKOM", "NN", "APPR", "PPOSAT", "NN", "ADJD", "$."], "meter": "+-++-+-+-+-+", "measure": "unknown.measure.septa"}, "line.23": {"text": "Sie werden durch den Reiz in der Natur bewogen,", "tokens": ["Sie", "wer\u00b7den", "durch", "den", "Reiz", "in", "der", "Na\u00b7tur", "be\u00b7wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Sie sind uns dem Gesicht im tr\u00fcben Herbst entflo-", "tokens": ["Sie", "sind", "uns", "dem", "Ge\u00b7sicht", "im", "tr\u00fc\u00b7ben", "Herbst", "ent\u00b7flo"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ART", "NN", "APPRART", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Bei Wachteln, Kranichen ists g\u00e4nzlich ausgemacht,", "tokens": ["Bei", "Wach\u00b7teln", ",", "Kra\u00b7ni\u00b7chen", "ists", "g\u00e4nz\u00b7lich", "aus\u00b7ge\u00b7macht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Da\u00df wenn der Jahreskreis, die Herbstzeit herge-", "tokens": ["Da\u00df", "wenn", "der", "Jah\u00b7res\u00b7kreis", ",", "die", "Herbst\u00b7zeit", "her\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "KOUS", "ART", "NN", "$,", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Sie nach dem warmen Strich, zu denen S\u00fcdens", "tokens": ["Sie", "nach", "dem", "war\u00b7men", "Strich", ",", "zu", "de\u00b7nen", "S\u00fc\u00b7dens"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "$,", "APPR", "PRELS", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Gleichsam, als Truppenweis mit schnellen Fl\u00fcgeln", "tokens": ["Gleich\u00b7sam", ",", "als", "Trup\u00b7pen\u00b7weis", "mit", "schnel\u00b7len", "Fl\u00fc\u00b7geln"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "KOUS", "NN", "APPR", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.29": {"text": "Der F\u00fchrer fliegt vorher, durchstreicht die d\u00fcnne", "tokens": ["Der", "F\u00fch\u00b7rer", "fliegt", "vor\u00b7her", ",", "durch\u00b7streicht", "die", "d\u00fcn\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Und zeigt den rechten Weg in fremde L\u00e4nder an,", "tokens": ["Und", "zeigt", "den", "rech\u00b7ten", "Weg", "in", "frem\u00b7de", "L\u00e4n\u00b7der", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Die andern folgen nach, in wollgeorndten Z\u00fcgen,", "tokens": ["Die", "an\u00b7dern", "fol\u00b7gen", "nach", ",", "in", "woll\u00b7ge\u00b7ornd\u00b7ten", "Z\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "PTKVZ", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Da sie wie Linien, als leichte Heere fliegen.", "tokens": ["Da", "sie", "wie", "Li\u00b7ni\u00b7en", ",", "als", "leich\u00b7te", "Hee\u00b7re", "flie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "KOKOM", "NE", "$,", "KOUS", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Ob ihnen gleich die Welt im Abris nicht bekannt;", "tokens": ["Ob", "ih\u00b7nen", "gleich", "die", "Welt", "im", "Ab\u00b7ris", "nicht", "be\u00b7kannt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "APPRART", "NE", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "So finden sie doch stets, das ausgesuchte Land,", "tokens": ["So", "fin\u00b7den", "sie", "doch", "stets", ",", "das", "aus\u00b7ge\u00b7such\u00b7te", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Kein Compas leitet sie, wenn sie bei Sturm und", "tokens": ["Kein", "Com\u00b7pas", "lei\u00b7tet", "sie", ",", "wenn", "sie", "bei", "Sturm", "und"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "$,", "KOUS", "PPER", "APPR", "NN", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.36": {"text": "Bei tr\u00fcber Finsternis etwan von ihren Wegen,", "tokens": ["Bei", "tr\u00fc\u00b7ber", "Fins\u00b7ter\u00b7nis", "et\u00b7wan", "von", "ih\u00b7ren", "We\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Von rechter Strasse irrn. Und dennoch k\u00f6nnen sie,", "tokens": ["Von", "rech\u00b7ter", "Stras\u00b7se", "irrn", ".", "Und", "den\u00b7noch", "k\u00f6n\u00b7nen", "sie", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$.", "KON", "ADV", "VMFIN", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Ohn eine K\u00fcmmernis, ohn eine saure M\u00fch,", "tokens": ["Ohn", "ei\u00b7ne", "K\u00fcm\u00b7mer\u00b7nis", ",", "ohn", "ei\u00b7ne", "sau\u00b7re", "M\u00fch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "KOUI", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "In einer freien Lufft, in den geraden Gleisen,", "tokens": ["In", "ei\u00b7ner", "frei\u00b7en", "Lufft", ",", "in", "den", "ge\u00b7ra\u00b7den", "Glei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Wohin sie immer wolln, ohn eine Charte reisen.", "tokens": ["Wo\u00b7hin", "sie", "im\u00b7mer", "wolln", ",", "ohn", "ei\u00b7ne", "Char\u00b7te", "rei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VMFIN", "$,", "KOUI", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Ist das nicht wunderbahr, ists nicht betrachtens", "tokens": ["Ist", "das", "nicht", "wun\u00b7der\u00b7bahr", ",", "ists", "nicht", "be\u00b7trach\u00b7tens"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PDS", "PTKNEG", "ADJD", "$,", "VAFIN", "PTKNEG", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.42": {"text": "Wer hat das Federvolk, das ohn Vernunfft, be-", "tokens": ["Wer", "hat", "das", "Fe\u00b7der\u00b7volk", ",", "das", "ohn", "Ver\u00b7nunfft", ",", "be"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PWS", "VAFIN", "ART", "NN", "$,", "PRELS", "APPR", "NN", "$,", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.43": {"text": "Da\u00df in dem Kreis der Welt, auch heisse L\u00e4nder", "tokens": ["Da\u00df", "in", "dem", "Kreis", "der", "Welt", ",", "auch", "heis\u00b7se", "L\u00e4n\u00b7der"], "token_info": ["word", "word", "word", "word"