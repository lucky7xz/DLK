{"textgrid.poem.62220": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "[kalt ist's, eine trockene K\u00e4lte]", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kalt ist's, eine trockene K\u00e4lte,", "tokens": ["Kalt", "ist's", ",", "ei\u00b7ne", "tro\u00b7cke\u00b7ne", "K\u00e4l\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Aus modernen Burgen schallt", "tokens": ["Aus", "mo\u00b7der\u00b7nen", "Bur\u00b7gen", "schallt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Tadel f\u00fcr das Holz, den Heizer", "tokens": ["Ta\u00b7del", "f\u00fcr", "das", "Holz", ",", "den", "Hei\u00b7zer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "$,", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Durch die weiten S\u00e4le bald.", "tokens": ["Durch", "die", "wei\u00b7ten", "S\u00e4\u00b7le", "bald", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Aber in des Armen H\u00fctte", "tokens": ["A\u00b7ber", "in", "des", "Ar\u00b7men", "H\u00fct\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist von Tadel keine Spur,", "tokens": ["Ist", "von", "Ta\u00b7del", "kei\u00b7ne", "Spur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Eingefroren ist das Wasser", "tokens": ["Ein\u00b7ge\u00b7fro\u00b7ren", "ist", "das", "Was\u00b7ser"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und man weint und zittert nur.", "tokens": ["Und", "man", "weint", "und", "zit\u00b7tert", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "KON", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}