{"textgrid.poem.41318": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Goldoni", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von vielen, die sich jetzt Thalien zugesellen,", "tokens": ["Von", "vie\u00b7len", ",", "die", "sich", "jetzt", "Tha\u00b7li\u00b7en", "zu\u00b7ge\u00b7sel\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "$,", "PRELS", "PRF", "ADV", "NN", "VVPP", "$,"], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Kennt keiner, so wie er, was bessert und gef\u00e4llt.", "tokens": ["Kennt", "kei\u00b7ner", ",", "so", "wie", "er", ",", "was", "bes\u00b7sert", "und", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "ADV", "KOKOM", "PPER", "$,", "PRELS", "VVPP", "KON", "VVPP", "$."], "meter": "-+---+-+-+-+", "measure": "dactylic.init"}, "line.3": {"text": "Der Schauplatz und die heut'ge Welt", "tokens": ["Der", "Schau\u00b7platz", "und", "die", "heut'\u00b7ge", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sind seiner Fabeln stete Quellen.", "tokens": ["Sind", "sei\u00b7ner", "Fa\u00b7beln", "ste\u00b7te", "Quel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wie lehrreich r\u00fchren uns, durch ihn,", "tokens": ["Wie", "lehr\u00b7reich", "r\u00fch\u00b7ren", "uns", ",", "durch", "ihn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "$,", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bettina, und ihr Pasqualin!", "tokens": ["Bet\u00b7ti\u00b7na", ",", "und", "ihr", "Pas\u00b7qua\u00b7lin", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KON", "PPOSAT", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.7": {"text": "Die Kleinigkeiten selbst, die nur zu spielen scheinen,", "tokens": ["Die", "Klei\u00b7nig\u00b7kei\u00b7ten", "selbst", ",", "die", "nur", "zu", "spie\u00b7len", "schei\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "PRELS", "ADV", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Auch die sieht man von ihm empfindlich angebracht,", "tokens": ["Auch", "die", "sieht", "man", "von", "ihm", "emp\u00b7find\u00b7lich", "an\u00b7ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "VVFIN", "PIS", "APPR", "PPER", "ADJD", "VVPP", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.9": {"text": "Und wer nicht beim Goldoni lacht,", "tokens": ["Und", "wer", "nicht", "beim", "Gol\u00b7do\u00b7ni", "lacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PTKNEG", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Der kann beim Holberg weinen.", "tokens": ["Der", "kann", "beim", "Hol\u00b7berg", "wei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}