{"textgrid.poem.32323": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "9. Der Eintritt des Jahres 1754 in Berlin", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wem t\u00f6nt dies k\u00fchnre Lied? dies Lied, zu wessen Lobe,", "tokens": ["Wem", "t\u00f6nt", "dies", "k\u00fchn\u00b7re", "Lied", "?", "dies", "Lied", ",", "zu", "wes\u00b7sen", "Lo\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PDS", "ADJA", "NN", "$.", "PDS", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "H\u00f6rt es noch manche sp\u00e4te Welt?", "tokens": ["H\u00f6rt", "es", "noch", "man\u00b7che", "sp\u00e4\u00b7te", "Welt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "PIAT", "ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Hier steh' ich, sinne nach, und gl\u00fch' und stampf' und tobe,", "tokens": ["Hier", "steh'", "ich", ",", "sin\u00b7ne", "nach", ",", "und", "gl\u00fch'", "und", "stampf'", "und", "to\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "VVFIN", "PTKVZ", "$,", "KON", "ADJD", "KON", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und suche meiner Hymnen Held.", "tokens": ["Und", "su\u00b7che", "mei\u00b7ner", "Hym\u00b7nen", "Held", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wer wird es sein? Vielleicht im blut'gen Panzerkleide", "tokens": ["Wer", "wird", "es", "sein", "?", "Viel\u00b7leicht", "im", "blut'\u00b7gen", "Pan\u00b7zer\u00b7klei\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "PPER", "VAINF", "$.", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des Krieges f\u00fcrchterlicher Gott?", "tokens": ["Des", "Krie\u00b7ges", "f\u00fcrch\u00b7ter\u00b7li\u00b7cher", "Gott", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Um ihn t\u00f6nt durch das Feld gedungner Krieger Freude,", "tokens": ["Um", "ihn", "t\u00f6nt", "durch", "das", "Feld", "ge\u00b7dung\u00b7ner", "Krie\u00b7ger", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "VVFIN", "APPR", "ART", "NN", "ADJA", "NN", "NN", "$,"], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Und der Erw\u00fcrgten lauter Tod.", "tokens": ["Und", "der", "Er\u00b7w\u00fcrg\u00b7ten", "lau\u00b7ter", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie, oder ists vielmehr in fabellosen Zeiten", "tokens": ["Wie", ",", "o\u00b7der", "ists", "viel\u00b7mehr", "in", "fa\u00b7bel\u00b7lo\u00b7sen", "Zei\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "$,", "KON", "VAFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein neuer g\u00f6ttlicher Apoll,", "tokens": ["Ein", "neu\u00b7er", "g\u00f6tt\u00b7li\u00b7cher", "A\u00b7poll", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Der, schwer entbehrt, mit schnell zur\u00fcckberufnen Saiten", "tokens": ["Der", ",", "schwer", "ent\u00b7behrt", ",", "mit", "schnell", "zu\u00b7r\u00fcck\u00b7be\u00b7ruf\u00b7nen", "Sai\u00b7ten"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "$,", "ADJD", "VVPP", "$,", "APPR", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den Himmel wieder f\u00fcllen soll?", "tokens": ["Den", "Him\u00b7mel", "wie\u00b7der", "f\u00fcl\u00b7len", "soll", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wo nicht, so werde der der Vorwurf meiner Lieder,", "tokens": ["Wo", "nicht", ",", "so", "wer\u00b7de", "der", "der", "Vor\u00b7wurf", "mei\u00b7ner", "Lie\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "$,", "ADV", "VAFIN", "ART", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der sich als Themis R\u00e4cher wies,", "tokens": ["Der", "sich", "als", "The\u00b7mis", "R\u00e4\u00b7cher", "wies", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "KOUS", "NE", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und dessen frommes Schwert der gift'gen Zanksucht Hyder", "tokens": ["Und", "des\u00b7sen", "from\u00b7mes", "Schwert", "der", "gift'\u00b7gen", "Zank\u00b7sucht", "Hy\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "ADJA", "NN", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nur drei von tausend K\u00f6pfen lie\u00df.", "tokens": ["Nur", "drei", "von", "tau\u00b7send", "K\u00f6p\u00b7fen", "lie\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "APPR", "CARD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Doch ihn, Apoll und Mars, in ", "tokens": ["Doch", "ihn", ",", "A\u00b7poll", "und", "Mars", ",", "in"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KON", "PPER", "$,", "NE", "KON", "NN", "$,", "APPR"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Vereine, mein Gesang, auch du!", "tokens": ["Ver\u00b7ei\u00b7ne", ",", "mein", "Ge\u00b7sang", ",", "auch", "du", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,", "ADV", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wann einst ein junger Held bei seinem Grabe weinet,", "tokens": ["Wann", "einst", "ein", "jun\u00b7ger", "Held", "bei", "sei\u00b7nem", "Gra\u00b7be", "wei\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So z\u00e4hl' ihm seine Taten zu!", "tokens": ["So", "z\u00e4hl'", "ihm", "sei\u00b7ne", "Ta\u00b7ten", "zu", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Fang an von jenem Tag' \u2013 Doch, welch ein neues Feuer", "tokens": ["Fang", "an", "von", "je\u00b7nem", "Tag'", "\u2013", "Doch", ",", "welch", "ein", "neu\u00b7es", "Feu\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "APPR", "APPR", "PDAT", "NN", "$(", "KON", "$,", "PWAT", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Rei\u00dft mich vom niedern Staub' empor?", "tokens": ["Rei\u00dft", "mich", "vom", "nie\u00b7dern", "Staub'", "em\u00b7por", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auch K\u00f6nige sind Staub! Seid ihnen treu; dem treuer,", "tokens": ["Auch", "K\u00f6\u00b7ni\u00b7ge", "sind", "Staub", "!", "Seid", "ih\u00b7nen", "treu", ";", "dem", "treu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "NN", "VAFIN", "NN", "$.", "VAIMP", "PPER", "ADJD", "$.", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der sie zu besserm Staub' erkor.", "tokens": ["Der", "sie", "zu", "bes\u00b7serm", "Staub'", "er\u00b7kor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Wer wird, voll seines Geists, mir seinen Namen melden?", "tokens": ["Wer", "wird", ",", "voll", "sei\u00b7nes", "Geists", ",", "mir", "sei\u00b7nen", "Na\u00b7men", "mel\u00b7den", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "$,", "ADJD", "PPOSAT", "NN", "$,", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sein Nam' ist ihm allein bewu\u00dft.", "tokens": ["Sein", "Nam'", "ist", "ihm", "al\u00b7lein", "be\u00b7wu\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er ist der F\u00fcrsten F\u00fcrst, er ist der Held der Helden;", "tokens": ["Er", "ist", "der", "F\u00fcrs\u00b7ten", "F\u00fcrst", ",", "er", "ist", "der", "Held", "der", "Hel\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "$,", "PPER", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Er f\u00fcllt die Welt und meine Brust.", "tokens": ["Er", "f\u00fcllt", "die", "Welt", "und", "mei\u00b7ne", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Er rief sie aus des Nichts nur ihm folgsamen Schlunde;", "tokens": ["Er", "rief", "sie", "aus", "des", "Nichts", "nur", "ihm", "folg\u00b7sa\u00b7men", "Schlun\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "ADV", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Er ruft sie noch, da\u00df sie besteht.", "tokens": ["Er", "ruft", "sie", "noch", ",", "da\u00df", "sie", "be\u00b7steht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie bebt, sie wankt, so oft ein Hauch aus seinem Munde", "tokens": ["Sie", "bebt", ",", "sie", "wankt", ",", "so", "oft", "ein", "Hauch", "aus", "sei\u00b7nem", "Mun\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$,", "ADV", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den Fluch in ihre Sph\u00e4ren weht.", "tokens": ["Den", "Fluch", "in", "ih\u00b7re", "Sph\u00e4\u00b7ren", "weht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "O dreimal Schrecklicher! \u2013 \u2013 doch voller Quell des Guten,", "tokens": ["O", "drei\u00b7mal", "Schreck\u00b7li\u00b7cher", "!", "\u2013", "\u2013", "doch", "vol\u00b7ler", "Quell", "des", "Gu\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "NN", "$.", "$(", "$(", "ADV", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du bist der Schreckliche nicht gern.", "tokens": ["Du", "bist", "der", "Schreck\u00b7li\u00b7che", "nicht", "gern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den weiten Orient zerfleischen deine Ruten;", "tokens": ["Den", "wei\u00b7ten", "O\u00b7rient", "zer\u00b7flei\u00b7schen", "dei\u00b7ne", "Ru\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Uns, Vater, zeigst du sie von fern.", "tokens": ["Uns", ",", "Va\u00b7ter", ",", "zeigst", "du", "sie", "von", "fern", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "VVFIN", "PPER", "PPER", "APPR", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Wie, da\u00df des Undanks Frost die tr\u00e4gen Lippen bindet,", "tokens": ["Wie", ",", "da\u00df", "des", "Un\u00b7danks", "Frost", "die", "tr\u00e4\u00b7gen", "Lip\u00b7pen", "bin\u00b7det", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "KOUS", "ART", "NN", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Volk, dem er Heil, wie Flocken, gibt!", "tokens": ["Volk", ",", "dem", "er", "Heil", ",", "wie", "Flo\u00b7cken", ",", "gibt", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "NN", "$,", "PWAV", "NN", "$,", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Ihm dank' es, wenn ein Jahr in s\u00fc\u00dfer Ruh verschwindet;", "tokens": ["Ihm", "dank'", "es", ",", "wenn", "ein", "Jahr", "in", "s\u00fc\u00b7\u00dfer", "Ruh", "ver\u00b7schwin\u00b7det", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ihm dank' es, da\u00df dich ", "tokens": ["Ihm", "dank'", "es", ",", "da\u00df", "dich"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "PPER"], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}