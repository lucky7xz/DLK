{"textgrid.poem.59947": {"metadata": {"author": {"name": "Bodenstedt, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "37.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zwei Arten h\u00f6h'rer Geister schuf Natur:", "tokens": ["Zwei", "Ar\u00b7ten", "h\u00f6h'\u00b7rer", "Geis\u00b7ter", "schuf", "Na\u00b7tur", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "ADJA", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die einen, sch\u00f6n zu denken und zu handeln,", "tokens": ["Die", "ei\u00b7nen", ",", "sch\u00f6n", "zu", "den\u00b7ken", "und", "zu", "han\u00b7deln", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "$,", "ADJD", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die andern, voll Empf\u00e4nglichkeit der Spur", "tokens": ["Die", "an\u00b7dern", ",", "voll", "Emp\u00b7f\u00e4ng\u00b7lich\u00b7keit", "der", "Spur"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "$,", "ADJD", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Des Wahren und des Sch\u00f6nen nachzuwandeln.", "tokens": ["Des", "Wah\u00b7ren", "und", "des", "Sch\u00f6\u00b7nen", "nach\u00b7zu\u00b7wan\u00b7deln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}