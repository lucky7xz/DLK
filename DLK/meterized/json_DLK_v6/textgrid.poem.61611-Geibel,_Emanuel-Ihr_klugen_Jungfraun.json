{"textgrid.poem.61611": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Ihr klugen Jungfraun", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr klugen Jungfraun", "tokens": ["Ihr", "klu\u00b7gen", "Jung\u00b7fraun"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+---", "measure": "dactylic.init"}, "line.2": {"text": "In eurer Kammer,", "tokens": ["In", "eu\u00b7rer", "Kam\u00b7mer", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "O schlummert nimmer,", "tokens": ["O", "schlum\u00b7mert", "nim\u00b7mer", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "R\u00fcstet die Lampen!", "tokens": ["R\u00fcs\u00b7tet", "die", "Lam\u00b7pen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "L\u00e4ngst schwand in der Ferne", "tokens": ["L\u00e4ngst", "schwand", "in", "der", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Des Abendrots Pracht;", "tokens": ["Des", "A\u00b7ben\u00b7drots", "Pracht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Schon k\u00fcnden die Sterne", "tokens": ["Schon", "k\u00fcn\u00b7den", "die", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Die Mitte der Nacht.", "tokens": ["Die", "Mit\u00b7te", "der", "Nacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Seid munter und wacht!", "tokens": ["Seid", "mun\u00b7ter", "und", "wacht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "KON", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Wie lang wird es w\u00e4hren,", "tokens": ["Wie", "lang", "wird", "es", "w\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "So nahet der Br\u00e4utigam,", "tokens": ["So", "na\u00b7het", "der", "Br\u00e4u\u00b7ti\u00b7gam", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NE", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Der K\u00f6nig der Ehren.", "tokens": ["Der", "K\u00f6\u00b7nig", "der", "Eh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Drum schlummert nimmer,", "tokens": ["Drum", "schlum\u00b7mert", "nim\u00b7mer", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADV", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "R\u00fcstet die Lampen!", "tokens": ["R\u00fcs\u00b7tet", "die", "Lam\u00b7pen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Nun salbt euch mit D\u00fcften!", "tokens": ["Nun", "salbt", "euch", "mit", "D\u00fcf\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Legt an das Geschmeid'!", "tokens": ["Legt", "an", "das", "Ge\u00b7schmeid'", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Umg\u00fcrtet die H\u00fcften", "tokens": ["Um\u00b7g\u00fcr\u00b7tet", "die", "H\u00fcf\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Mit purpurnem Kleid!", "tokens": ["Mit", "pur\u00b7pur\u00b7nem", "Kleid", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Der Herr ist nicht weit.", "tokens": ["Der", "Herr", "ist", "nicht", "weit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Auf g\u00fcldnen Geschirren", "tokens": ["Auf", "g\u00fcld\u00b7nen", "Ge\u00b7schir\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Bald gilt's ihm zu z\u00fcnden", "tokens": ["Bald", "gilt's", "ihm", "zu", "z\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKZU", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Weihrauch und Myrrhen.", "tokens": ["Weih\u00b7rauch", "und", "Myr\u00b7rhen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Drum schlummert nimmer,", "tokens": ["Drum", "schlum\u00b7mert", "nim\u00b7mer", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADV", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "R\u00fcstet die Lampen!", "tokens": ["R\u00fcs\u00b7tet", "die", "Lam\u00b7pen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Durch W\u00e4lder und Wogen,", "tokens": ["Durch", "W\u00e4l\u00b7der", "und", "Wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Durchs finstere Land,", "tokens": ["Durchs", "fins\u00b7te\u00b7re", "Land", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Still kommt er gezogen,", "tokens": ["Still", "kommt", "er", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "VVPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Die Kron' in der Hand.", "tokens": ["Die", "Kron'", "in", "der", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Sein Herz ist entbrannt", "tokens": ["Sein", "Herz", "ist", "ent\u00b7brannt"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Von himmlischer Minne;", "tokens": ["Von", "himm\u00b7li\u00b7scher", "Min\u00b7ne", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Doch forschend verzieht er,", "tokens": ["Doch", "for\u00b7schend", "ver\u00b7zieht", "er", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Zu pr\u00fcfen die Sinne.", "tokens": ["Zu", "pr\u00fc\u00b7fen", "die", "Sin\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Drum schlummert nimmer,", "tokens": ["Drum", "schlum\u00b7mert", "nim\u00b7mer", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADV", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "R\u00fcstet die Lampen!", "tokens": ["R\u00fcs\u00b7tet", "die", "Lam\u00b7pen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "Weh denen, die liegen,", "tokens": ["Weh", "de\u00b7nen", ",", "die", "lie\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PDS", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Vom Schlaf unterjocht,", "tokens": ["Vom", "Schlaf", "un\u00b7ter\u00b7jocht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Wenn endlich, die Stiegen", "tokens": ["Wenn", "end\u00b7lich", ",", "die", "Stie\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KOUS", "ADV", "$,", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Beschreitend, er pocht.", "tokens": ["Be\u00b7schrei\u00b7tend", ",", "er", "pocht", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVPP", "$,", "PPER", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Verl\u00f6scht ist ihr Docht.", "tokens": ["Ver\u00b7l\u00f6scht", "ist", "ihr", "Docht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Versto\u00dfen vom Funkeln", "tokens": ["Ver\u00b7sto\u00b7\u00dfen", "vom", "Fun\u00b7keln"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPRART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Des Festes dann gehn sie", "tokens": ["Des", "Fes\u00b7tes", "dann", "gehn", "sie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVFIN", "PPER"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Und weinen im Dunkeln.", "tokens": ["Und", "wei\u00b7nen", "im", "Dun\u00b7keln", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "APPRART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Drum schlummert nimmer,", "tokens": ["Drum", "schlum\u00b7mert", "nim\u00b7mer", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADV", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "R\u00fcstet die Lampen!", "tokens": ["R\u00fcs\u00b7tet", "die", "Lam\u00b7pen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.6": {"line.1": {"text": "Doch die da sich schm\u00fcckten", "tokens": ["Doch", "die", "da", "sich", "schm\u00fcck\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "KOUS", "PRF", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Und warteten sein,", "tokens": ["Und", "war\u00b7te\u00b7ten", "sein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VAINF", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Er f\u00fchrt die Entz\u00fcckten", "tokens": ["Er", "f\u00fchrt", "die", "Ent\u00b7z\u00fcck\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Zur Herrlichkeit ein.", "tokens": ["Zur", "Herr\u00b7lich\u00b7keit", "ein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Holdseliger Schein", "tokens": ["Hold\u00b7se\u00b7li\u00b7ger", "Schein"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+---+", "measure": "dactylic.init"}, "line.6": {"text": "Ist drinnen ergossen,", "tokens": ["Ist", "drin\u00b7nen", "er\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVPP", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Wo hoch um das Lager", "tokens": ["Wo", "hoch", "um", "das", "La\u00b7ger"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "APPR", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Die Lilien sprossen.", "tokens": ["Die", "Li\u00b7li\u00b7en", "spros\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Drum schlummert nimmer!", "tokens": ["Drum", "schlum\u00b7mert", "nim\u00b7mer", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADV", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "R\u00fcstet die Lampen!", "tokens": ["R\u00fcs\u00b7tet", "die", "Lam\u00b7pen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}