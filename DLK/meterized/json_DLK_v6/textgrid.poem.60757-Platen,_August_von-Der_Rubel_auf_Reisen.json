{"textgrid.poem.60757": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Der Rubel auf Reisen", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Rubel reist im deutschen Land,", "tokens": ["Der", "Ru\u00b7bel", "reist", "im", "deut\u00b7schen", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der frommen Leuten frommt,", "tokens": ["Der", "from\u00b7men", "Leu\u00b7ten", "frommt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und jeder \u00f6ffnet schnell die Hand,", "tokens": ["Und", "je\u00b7der", "\u00f6ff\u00b7net", "schnell", "die", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sobald der Rubel kommt.", "tokens": ["So\u00b7bald", "der", "Ru\u00b7bel", "kommt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ihn speichert selbst der Pietist,", "tokens": ["Ihn", "spei\u00b7chert", "selbst", "der", "Pie\u00b7tist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Und gibt den Armen mehr:", "tokens": ["Und", "gibt", "den", "Ar\u00b7men", "mehr", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Seit au\u00dfer Kurs die Tugend ist,", "tokens": ["Seit", "au\u00b7\u00dfer", "Kurs", "die", "Tu\u00b7gend", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "NN", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kursiert der Rubel sehr.", "tokens": ["Kur\u00b7siert", "der", "Ru\u00b7bel", "sehr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Der Tugend wird blo\u00df Ruhm zuteil,", "tokens": ["Der", "Tu\u00b7gend", "wird", "blo\u00df", "Ruhm", "zu\u00b7teil", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es ist ein hohler Schall;", "tokens": ["Es", "ist", "ein", "hoh\u00b7ler", "Schall", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch wem die Welt um Rubel feil,", "tokens": ["Doch", "wem", "die", "Welt", "um", "Ru\u00b7bel", "feil", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem klingt ein rein Metall!", "tokens": ["Dem", "klingt", "ein", "rein", "Me\u00b7tall", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Da wird die Nacht gescholten Tag,", "tokens": ["Da", "wird", "die", "Nacht", "ge\u00b7schol\u00b7ten", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Teufel wird so gut!", "tokens": ["Der", "Teu\u00b7fel", "wird", "so", "gut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was nicht ein heller Klang vermag,", "tokens": ["Was", "nicht", "ein", "hel\u00b7ler", "Klang", "ver\u00b7mag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was nicht ein Rubel tut!", "tokens": ["Was", "nicht", "ein", "Ru\u00b7bel", "tut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Des Nordens Sternbild wird bekr\u00e4nzt", "tokens": ["Des", "Nor\u00b7dens", "Stern\u00b7bild", "wird", "be\u00b7kr\u00e4nzt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vom S\u00e4ngerchor des Teut:", "tokens": ["Vom", "S\u00e4n\u00b7ge\u00b7rchor", "des", "Teut", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es ist der Rubel, der so Gl\u00e4nzt,", "tokens": ["Es", "ist", "der", "Ru\u00b7bel", ",", "der", "so", "Gl\u00e4nzt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der so das Aug erfreut!", "tokens": ["Der", "so", "das", "Aug", "er\u00b7freut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Wohl ist er ein an jedem Strand", "tokens": ["Wohl", "ist", "er", "ein", "an", "je\u00b7dem", "Strand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ART", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "S\u00fc\u00dfangegrinster Gast:", "tokens": ["S\u00fc\u00b7\u00df\u00b7an\u00b7ge\u00b7grins\u00b7ter", "Gast", ":"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Verkaufe nur dein Vaterland,", "tokens": ["Ver\u00b7kau\u00b7fe", "nur", "dein", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wofern du eines hast!", "tokens": ["Wo\u00b7fern", "du", "ei\u00b7nes", "hast", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Doch ist dir auch, welch eine Kraft", "tokens": ["Doch", "ist", "dir", "auch", ",", "welch", "ei\u00b7ne", "Kraft"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADV", "$,", "PWAT", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du t\u00f6ten sollst, bewu\u00dft?", "tokens": ["Du", "t\u00f6\u00b7ten", "sollst", ",", "be\u00b7wu\u00dft", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVINF", "VMFIN", "$,", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gymnastik ist's und Wissenschaft,", "tokens": ["Gym\u00b7nas\u00b7tik", "ist's", "und", "Wis\u00b7sen\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die du verleumden mu\u00dft.", "tokens": ["Die", "du", "ver\u00b7leum\u00b7den", "mu\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Es wird, und sind nur diese Zwo", "tokens": ["Es", "wird", ",", "und", "sind", "nur", "die\u00b7se", "Zwo"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "KON", "VAFIN", "ADV", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zum Land gepeitscht hinaus,", "tokens": ["Zum", "Land", "ge\u00b7peitscht", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit Rubeln ach, mit Rubeln oh,", "tokens": ["Mit", "Ru\u00b7beln", "ach", ",", "mit", "Ru\u00b7beln", "oh", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "XY", "$,", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gepflastert jedes Haus.", "tokens": ["Ge\u00b7pflas\u00b7tert", "je\u00b7des", "Haus", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Und sind verjagt nur diese Zwei,", "tokens": ["Und", "sind", "ver\u00b7jagt", "nur", "die\u00b7se", "Zwei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "VVPP", "ADV", "PDAT", "CARD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dann herrscht Tyrann und Pfaff,", "tokens": ["Dann", "herrscht", "Ty\u00b7rann", "und", "Pfaff", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "KON", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und jede Nerve wird wie Brei,", "tokens": ["Und", "je\u00b7de", "Ner\u00b7ve", "wird", "wie", "Brei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und jede Sehne schlaff.", "tokens": ["Und", "je\u00b7de", "Seh\u00b7ne", "schlaff", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Der Rubel klirrt, der Rubel f\u00e4llt,", "tokens": ["Der", "Ru\u00b7bel", "klirrt", ",", "der", "Ru\u00b7bel", "f\u00e4llt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was ist der Mensch? Ein Schuft!", "tokens": ["Was", "ist", "der", "Mensch", "?", "Ein", "Schuft", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "$.", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wenn die Welt dir nicht gef\u00e4llt,", "tokens": ["Und", "wenn", "die", "Welt", "dir", "nicht", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So steig in deine Gruft!", "tokens": ["So", "steig", "in", "dei\u00b7ne", "Gruft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Erst gab's nur Einen Kotzebue,", "tokens": ["Erst", "gab's", "nur", "Ei\u00b7nen", "Kot\u00b7ze\u00b7bue", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Jetzt gibt's ein ganzes Schock;", "tokens": ["Jetzt", "gibt's", "ein", "gan\u00b7zes", "Schock", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und sch\u00fcttelst du das Haupt dazu,", "tokens": ["Und", "sch\u00fct\u00b7telst", "du", "das", "Haupt", "da\u00b7zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So leg es auf den Block!", "tokens": ["So", "leg", "es", "auf", "den", "Block", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Der Teufel siegt, der Gott verliert,", "tokens": ["Der", "Teu\u00b7fel", "siegt", ",", "der", "Gott", "ver\u00b7liert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der blanke Rubel reist:", "tokens": ["Der", "blan\u00b7ke", "Ru\u00b7bel", "reist", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So ward von je die Welt regiert,", "tokens": ["So", "ward", "von", "je", "die", "Welt", "re\u00b7giert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So lang die Sonne kreist.", "tokens": ["So", "lang", "die", "Son\u00b7ne", "kreist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}