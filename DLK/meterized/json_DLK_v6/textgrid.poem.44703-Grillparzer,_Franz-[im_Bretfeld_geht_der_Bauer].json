{"textgrid.poem.44703": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[im Bretfeld geht der Bauer]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Bretfeld geht der Bauer,", "tokens": ["Im", "Bret\u00b7feld", "geht", "der", "Bau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und geht er immer nur,", "tokens": ["Und", "geht", "er", "im\u00b7mer", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Trifft er zuletzt die Stelle,", "tokens": ["Trifft", "er", "zu\u00b7letzt", "die", "Stel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So wird er zur Figur;", "tokens": ["So", "wird", "er", "zur", "Fi\u00b7gur", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Wird Springer, L\u00e4ufer, Roche,", "tokens": ["Wird", "Sprin\u00b7ger", ",", "L\u00e4u\u00b7fer", ",", "Ro\u00b7che", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gibt gar zuletzt noch matt,", "tokens": ["Gibt", "gar", "zu\u00b7letzt", "noch", "matt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und ist doch nur ein Bauer,", "tokens": ["Und", "ist", "doch", "nur", "ein", "Bau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den man geadelt hat.", "tokens": ["Den", "man", "ge\u00b7a\u00b7delt", "hat", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}