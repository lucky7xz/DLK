{"textgrid.poem.26117": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wir horchten aus den Heckenwegen ins Tal,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir horchten aus den Heckenwegen ins Tal,", "tokens": ["Wir", "horch\u00b7ten", "aus", "den", "He\u00b7cken\u00b7we\u00b7gen", "ins", "Tal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Dort kamen die Abendlieder der V\u00f6gel dir entgegen,", "tokens": ["Dort", "ka\u00b7men", "die", "A\u00b7ben\u00b7dlie\u00b7der", "der", "V\u00f6\u00b7gel", "dir", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+--+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Vervielf\u00e4ltigt wie aus einem Echosaal.", "tokens": ["Ver\u00b7viel\u00b7f\u00e4l\u00b7tigt", "wie", "aus", "ei\u00b7nem", "E\u00b7cho\u00b7saal", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KOKOM", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.2": {"line.1": {"text": "Du hieltest deine Wangen dem Himmel hell hin,", "tokens": ["Du", "hiel\u00b7test", "dei\u00b7ne", "Wan\u00b7gen", "dem", "Him\u00b7mel", "hell", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ART", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wie ein W\u00f6lklein kam der Mond hergegangen,", "tokens": ["Wie", "ein", "W\u00f6lk\u00b7lein", "kam", "der", "Mond", "her\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "ART", "NN", "VVPP", "$,"], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Als w\u00e4r' er dein Bild in dem Spiegelglas drin.", "tokens": ["Als", "w\u00e4r'", "er", "dein", "Bild", "in", "dem", "Spie\u00b7gel\u00b7glas", "drin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "PPOSAT", "NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Ich seh' dich jetzt immer am Heckenweg stehn", "tokens": ["Ich", "seh'", "dich", "jetzt", "im\u00b7mer", "am", "He\u00b7cken\u00b7weg", "stehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "APPRART", "NN", "VVINF"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Wie das W\u00f6lklein Mond mit unschuldigem Schimmer,", "tokens": ["Wie", "das", "W\u00f6lk\u00b7lein", "Mond", "mit", "un\u00b7schul\u00b7di\u00b7gem", "Schim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "--+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und ich mu\u00df dir wie den Liedern im Abend nachgehn.", "tokens": ["Und", "ich", "mu\u00df", "dir", "wie", "den", "Lie\u00b7dern", "im", "A\u00b7bend", "nach\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPER", "KOKOM", "ART", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "----+-+--+--+", "measure": "iambic.tetra.relaxed"}}}}}