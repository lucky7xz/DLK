{"textgrid.poem.66937": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nun schlie\u00dft euch zusammen", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun schlie\u00dft euch zusammen", "tokens": ["Nun", "schlie\u00dft", "euch", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Zum einigen Kreis:", "tokens": ["Zum", "ei\u00b7ni\u00b7gen", "Kreis", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Das Wort soll entflammen,", "tokens": ["Das", "Wort", "soll", "ent\u00b7flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Die Tat sei der Preis!", "tokens": ["Die", "Tat", "sei", "der", "Preis", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "War doch der Geist so fremd gebunden,", "tokens": ["War", "doch", "der", "Geist", "so", "fremd", "ge\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die \u00dcbung ward zur welken Pflicht \u2013", "tokens": ["Die", "\u00dc\u00b7bung", "ward", "zur", "wel\u00b7ken", "Pflicht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPRART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir m\u00fcssen an dem Quell gesunden,", "tokens": ["Wir", "m\u00fcs\u00b7sen", "an", "dem", "Quell", "ge\u00b7sun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der aus dem Grund des Lebens bricht.", "tokens": ["Der", "aus", "dem", "Grund", "des", "Le\u00b7bens", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Aus morschen Gem\u00e4uern", "tokens": ["Aus", "mor\u00b7schen", "Ge\u00b7m\u00e4u\u00b7ern"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Weht modrig Gebot,", "tokens": ["Weht", "mod\u00b7rig", "Ge\u00b7bot", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Kommt, la\u00dft uns erneuern", "tokens": ["Kommt", ",", "la\u00dft", "uns", "er\u00b7neu\u00b7ern"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "VVIMP", "PPER", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Zum Leben den Tod!", "tokens": ["Zum", "Le\u00b7ben", "den", "Tod", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Wir opferten die Kraft dem Brauche,", "tokens": ["Wir", "op\u00b7fer\u00b7ten", "die", "Kraft", "dem", "Brau\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Der ohne zeugenden Segen war,", "tokens": ["Der", "oh\u00b7ne", "zeu\u00b7gen\u00b7den", "Se\u00b7gen", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und sehnten uns mit jedem Hauche", "tokens": ["Und", "sehn\u00b7ten", "uns", "mit", "je\u00b7dem", "Hau\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nach dem, was zukunftsoffenbar.", "tokens": ["Nach", "dem", ",", "was", "zu\u00b7kunf\u00b7tsof\u00b7fen\u00b7bar", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PRELS", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Der Geist sprach in Zungen", "tokens": ["Der", "Geist", "sprach", "in", "Zun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Voll Sehergewalt,", "tokens": ["Voll", "Se\u00b7her\u00b7ge\u00b7walt", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Jetzt dr\u00e4ngt in den Jungen", "tokens": ["Jetzt", "dr\u00e4ngt", "in", "den", "Jun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Er k\u00fchn zur Gestalt.", "tokens": ["Er", "k\u00fchn", "zur", "Ge\u00b7stalt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPRART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.6": {"line.1": {"text": "Wie wandelte mit Sturmesdr\u00f6hnen", "tokens": ["Wie", "wan\u00b7del\u00b7te", "mit", "Stur\u00b7mes\u00b7dr\u00f6h\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Welt ihr Schreckbild weit und breit!", "tokens": ["Die", "Welt", "ihr", "Schreck\u00b7bild", "weit", "und", "breit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zu M\u00e4nnern, Frauen, T\u00f6chtern, S\u00f6hnen", "tokens": ["Zu", "M\u00e4n\u00b7nern", ",", "Frau\u00b7en", ",", "T\u00f6ch\u00b7tern", ",", "S\u00f6h\u00b7nen"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Erscholl der Schicksalsruf der Zeit.", "tokens": ["Er\u00b7scholl", "der", "Schick\u00b7sals\u00b7ruf", "der", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Verloren die Lehre,", "tokens": ["Ver\u00b7lo\u00b7ren", "die", "Leh\u00b7re", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Verleugnet vom Sein!", "tokens": ["Ver\u00b7leug\u00b7net", "vom", "Sein", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Wer Ohren hat, h\u00f6re,", "tokens": ["Wer", "Oh\u00b7ren", "hat", ",", "h\u00f6\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "NN", "VAFIN", "$,", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Die Herzen, sie schrein:", "tokens": ["Die", "Her\u00b7zen", ",", "sie", "schrein", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.8": {"line.1": {"text": "Sie schreien nach dem neuen Buche,", "tokens": ["Sie", "schrei\u00b7en", "nach", "dem", "neu\u00b7en", "Bu\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das anders bindet, anders l\u00f6st,", "tokens": ["Das", "an\u00b7ders", "bin\u00b7det", ",", "an\u00b7ders", "l\u00f6st", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "ADV", "VVFIN", "$,", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Welt befreit vom Wahnsinnsfluche,", "tokens": ["Die", "Welt", "be\u00b7freit", "vom", "Wahn\u00b7sinns\u00b7flu\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Geister sucht und nicht verst\u00f6\u00dft.", "tokens": ["Die", "Geis\u00b7ter", "sucht", "und", "nicht", "ver\u00b7st\u00f6\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Von unten nach oben", "tokens": ["Von", "un\u00b7ten", "nach", "o\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADV", "APPR", "ADV"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Erschlossen den Schacht!", "tokens": ["Er\u00b7schlos\u00b7sen", "den", "Schacht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Den Volksschatz gehoben", "tokens": ["Den", "Volks\u00b7schatz", "ge\u00b7ho\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVPP"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Urspr\u00fcnglicher Macht!", "tokens": ["Ur\u00b7spr\u00fcng\u00b7li\u00b7cher", "Macht", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.10": {"line.1": {"text": "Frei sei dem mutterm\u00e4chtigen Triebe", "tokens": ["Frei", "sei", "dem", "mut\u00b7ter\u00b7m\u00e4ch\u00b7ti\u00b7gen", "Trie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Zum Bund des Friedens Weg und Bahn,", "tokens": ["Zum", "Bund", "des", "Frie\u00b7dens", "Weg", "und", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Sonne gl\u00fchender Menschenliebe", "tokens": ["Die", "Son\u00b7ne", "gl\u00fc\u00b7hen\u00b7der", "Men\u00b7schen\u00b7lie\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Erf\u00fclle siegreich Land und Plan!", "tokens": ["Er\u00b7f\u00fcl\u00b7le", "sieg\u00b7reich", "Land", "und", "Plan", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "So schlie\u00dft euch zusammen", "tokens": ["So", "schlie\u00dft", "euch", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Zum einigen Kreis,", "tokens": ["Zum", "ei\u00b7ni\u00b7gen", "Kreis", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Das Wort soll entflammen,", "tokens": ["Das", "Wort", "soll", "ent\u00b7flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Die Tat sei der Preis!", "tokens": ["Die", "Tat", "sei", "der", "Preis", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.12": {"line.1": {"text": "Wir wollen nicht das Licht verscherzen,", "tokens": ["Wir", "wol\u00b7len", "nicht", "das", "Licht", "ver\u00b7scher\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das unsrer F\u00fchrer Pfad erhellt,", "tokens": ["Das", "uns\u00b7rer", "F\u00fch\u00b7rer", "Pfad", "er\u00b7hellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und s\u00e4en in der Jugend Herzen", "tokens": ["Und", "s\u00e4\u00b7en", "in", "der", "Ju\u00b7gend", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den Samen einer h\u00f6hern Welt.", "tokens": ["Den", "Sa\u00b7men", "ei\u00b7ner", "h\u00f6\u00b7hern", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}