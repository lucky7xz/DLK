{"textgrid.poem.50156": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "44. Nulla calamitas sola", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wahr ists, ", "tokens": ["Wahr", "ists", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "VAFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "In ungest\u00fcmen N\u00e4chten pflegen", "tokens": ["In", "un\u00b7ge\u00b7st\u00fc\u00b7men", "N\u00e4ch\u00b7ten", "pfle\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Gemeiniglich der ", "tokens": ["Ge\u00b7mei\u00b7nig\u00b7lich", "der"], "token_info": ["word", "word"], "pos": ["ADV", "ART"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Nicht weit von sich entfernt zu sein:", "tokens": ["Nicht", "weit", "von", "sich", "ent\u00b7fernt", "zu", "sein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "APPR", "PRF", "ADJD", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Jedoch vermindert ", "tokens": ["Je\u00b7doch", "ver\u00b7min\u00b7dert"], "token_info": ["word", "word"], "pos": ["ADV", "VVPP"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}}}}}