{"textgrid.poem.57053": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Die Priesterin", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nachdenklich nickt im D\u00e4mmer die Pagode ...", "tokens": ["Nach\u00b7denk\u00b7lich", "nickt", "im", "D\u00e4m\u00b7mer", "die", "Pa\u00b7go\u00b7de", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Daneben tritt aus ihres Hauses Pforte", "tokens": ["Da\u00b7ne\u00b7ben", "tritt", "aus", "ih\u00b7res", "Hau\u00b7ses", "Pfor\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "T'ang-ku-ei-i, die H\u00fcterin der Orte", "tokens": ["T'\u00b7ang\u00b7ku\u00b7ei\u00b7\u00b7i", ",", "die", "H\u00fc\u00b7te\u00b7rin", "der", "Or\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "ART", "NN", "ART", "NN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "vom krausen Leben und vom grausen Tode.", "tokens": ["vom", "krau\u00b7sen", "Le\u00b7ben", "und", "vom", "grau\u00b7sen", "To\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "KON", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Aus ihrem Munde h\u00e4ngt die Mondschein-Ode", "tokens": ["Aus", "ih\u00b7rem", "Mun\u00b7de", "h\u00e4ngt", "die", "Mond\u00b7schein\u00b7O\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Tang-Wangs, des Kaisers, mit gebl\u00fcmter Borte,", "tokens": ["Tang\u00b7Wangs", ",", "des", "Kai\u00b7sers", ",", "mit", "ge\u00b7bl\u00fcm\u00b7ter", "Bor\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "in ihren H\u00e4nden tr\u00e4gt sie eine Torte,", "tokens": ["in", "ih\u00b7ren", "H\u00e4n\u00b7den", "tr\u00e4gt", "sie", "ei\u00b7ne", "Tor\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "gekr\u00f6nt von einer winzigen Kommode.", "tokens": ["ge\u00b7kr\u00f6nt", "von", "ei\u00b7ner", "win\u00b7zi\u00b7gen", "Kom\u00b7mo\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So wandelt sie die sieben \u00e4ngstlich schmalen", "tokens": ["So", "wan\u00b7delt", "sie", "die", "sie\u00b7ben", "\u00e4ngst\u00b7lich", "schma\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "CARD", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "aus Fl\u00f6tenholz geschwungnen Tempelbr\u00fccken", "tokens": ["aus", "Fl\u00f6\u00b7ten\u00b7holz", "ge\u00b7schwung\u00b7nen", "Tem\u00b7pel\u00b7br\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "zum Grabe des vom Mond erschlagnen Hundes \u2013", "tokens": ["zum", "Gra\u00b7be", "des", "vom", "Mond", "er\u00b7schlag\u00b7nen", "Hun\u00b7des", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "APPRART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "und brockt den Kuchen in die Opferschalen \u2013", "tokens": ["und", "brockt", "den", "Ku\u00b7chen", "in", "die", "Op\u00b7fer\u00b7scha\u00b7len", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "und lockt den Mond, sich auf den Schrein zu b\u00fccken,", "tokens": ["und", "lockt", "den", "Mond", ",", "sich", "auf", "den", "Schrein", "zu", "b\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "PRF", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "und reicht ihm ihr Gedicht gespitzten Mundes ...", "tokens": ["und", "reicht", "ihm", "ihr", "Ge\u00b7dicht", "ge\u00b7spitz\u00b7ten", "Mun\u00b7des", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}