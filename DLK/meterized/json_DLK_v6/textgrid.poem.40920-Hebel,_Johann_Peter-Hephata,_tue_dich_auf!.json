{"textgrid.poem.40920": {"metadata": {"author": {"name": "Hebel, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Hephata, tue dich auf!", "genre": "verse", "period": "N.A.", "pub_year": 1793, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Woni am Sunntig fr\u00fceih in mine Gidanke dohi gang,", "tokens": ["Wo\u00b7ni", "am", "Sunn\u00b7tig", "fr\u00fc\u00b7eih", "in", "mi\u00b7ne", "Gi\u00b7dan\u00b7ke", "do\u00b7hi", "gang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+--+--+--++", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "'s isch so lieb und heimlig gsi, und d'Sunne het gschiene", "tokens": ["'s", "isch", "so", "lieb", "und", "heim\u00b7lig", "gsi", ",", "und", "d'\u00b7Sun\u00b7ne", "het", "gschie\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADJD", "ADV", "ADJD", "KON", "ADJD", "NE", "$,", "KON", "PDS", "VAFIN", "ADJA"], "meter": "-+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "rechts und links an d'D\u00f6rfer und an die gwiisgete Chilcht\u00fcrn,", "tokens": ["rechts", "und", "links", "an", "d'\u00b7D\u00f6r\u00b7fer", "und", "an", "die", "gwiis\u00b7ge\u00b7te", "Chilch\u00b7t\u00fcrn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "ADV", "APPR", "NE", "KON", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "--+--+--+-+-+-+", "measure": "anapaest.tri.plus"}, "line.4": {"text": "und die Chilcht\u00fcrn st\u00f6hn und bschauen enander vo witem", "tokens": ["und", "die", "Chilch\u00b7t\u00fcrn", "st\u00f6hn", "und", "bschau\u00b7en", "en\u00b7an\u00b7der", "vo", "wi\u00b7tem"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVINF", "KON", "ADJA", "FM", "FM", "FM"], "meter": "--+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "\u00fcbers Weizefeld und \u00fcber die duftige Matte", "tokens": ["\u00fc\u00b7bers", "Wei\u00b7ze\u00b7feld", "und", "\u00fc\u00b7ber", "die", "duf\u00b7ti\u00b7ge", "Mat\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "KON", "APPR", "ART", "ADJA", "NN"], "meter": "--+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "und 's will ken der Afang mache: \u00bbNochber fang du a!", "tokens": ["und", "'s", "will", "ken", "der", "A\u00b7fang", "ma\u00b7che", ":", "\u00bb", "Noch\u00b7ber", "fang", "du", "a", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "VVINF", "ART", "NN", "VVFIN", "$.", "$(", "ADV", "VVFIN", "PPER", "NE", "$."], "meter": "-++--+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.7": {"text": "Bisch du nit der \u00e4ltst und hesch die chr\u00e4ftigste Glocke?\u00ab", "tokens": ["Bisch", "du", "nit", "der", "\u00e4ltst", "und", "hesch", "die", "chr\u00e4f\u00b7tigs\u00b7te", "Glo\u00b7cke", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ART", "ADJD", "KON", "ADJD", "ART", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.8": {"text": "\u00bb's het jo no nit N\u00fcni gschlage\u00ab, seit er zum Nochber,", "tokens": ["\u00bb", "'s", "het", "jo", "no", "nit", "N\u00fc\u00b7ni", "gschla\u00b7ge", "\u00ab", ",", "seit", "er", "zum", "Noch\u00b7ber", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "NE", "NE", "PTKNEG", "NE", "VVFIN", "$(", "$,", "KOUS", "PPER", "APPRART", "NN", "$,"], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}, "line.9": {"text": "\u00bbund d\u00f6rt stoht e Burst im Feld, und lueget an d'Birb\u00e4um,", "tokens": ["\u00bb", "und", "d\u00f6rt", "stoht", "e", "Burst", "im", "Feld", ",", "und", "lue\u00b7get", "an", "d'\u00b7Bir\u00b7b\u00e4\u00b7um", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "ADJD", "VVFIN", "NE", "NE", "APPRART", "NN", "$,", "KON", "VVFIN", "APPR", "NE", "$,"], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.10": {"text": "denkwol i will warte, se bringi 'n au no in d'Chilche.\u00ab", "tokens": ["denk\u00b7wol", "i", "will", "war\u00b7te", ",", "se", "brin\u00b7gi", "'n", "au", "no", "in", "d'\u00b7Chil\u00b7che", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "NE", "VMFIN", "VVFIN", "$,", "FM", "FM", "FM", "FM", "FM", "APPR", "NE", "$.", "$("], "meter": "+-+-+-----+-+-+-", "measure": "unknown.measure.hexa"}, "line.11": {"text": "Drum es het e V\u00f6geli pfiffen uffeme Birbaum,", "tokens": ["Drum", "es", "het", "e", "V\u00f6\u00b7ge\u00b7li", "pfif\u00b7fen", "uf\u00b7fe\u00b7me", "Bir\u00b7baum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+--+--+-+--+-", "measure": "amphibrach.tri.plus"}, "line.12": {"text": "woni gstande bi, druf denki, woni em zuelos:", "tokens": ["wo\u00b7ni", "gstan\u00b7de", "bi", ",", "druf", "den\u00b7ki", ",", "wo\u00b7ni", "em", "zu\u00b7e\u00b7los", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$,", "FM.la", "FM.la", "$,", "FM.la", "FM.la", "FM.la", "$."], "meter": "+-+--+-+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.13": {"text": "Predigt echt der Fink uf siner laubige Chanzle.", "tokens": ["Pre\u00b7digt", "echt", "der", "Fink", "uf", "si\u00b7ner", "lau\u00b7bi\u00b7ge", "Chanz\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.14": {"text": "'s chunnt eim schier so vor, und d'Bl\u00fcmli sitzen und lose.", "tokens": ["'s", "chunnt", "eim", "schier", "so", "vor", ",", "und", "d'\u00b7B\u00b7l\u00fcm\u00b7li", "sit\u00b7zen", "und", "lo\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "ADV", "PTKVZ", "$,", "KON", "NE", "VVINF", "KON", "VVFIN", "$."], "meter": "-+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.15": {"text": "Nei, wie lost das Glockebl\u00fcmli, weger es schnuft nit,", "tokens": ["Nei", ",", "wie", "lost", "das", "Gloc\u00b7ke\u00b7bl\u00fcm\u00b7li", ",", "we\u00b7ger", "es", "schnuft", "nit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PWAV", "VVFIN", "ART", "NN", "$,", "PWS", "PPER", "VVFIN", "PTKNEG", "$,"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.16": {"text": "wenni 's nummen au verst\u00fcnd! Er wirdene sage,", "tokens": ["wen\u00b7ni", "'s", "num\u00b7men", "au", "ver\u00b7st\u00fcnd", "!", "Er", "wir\u00b7de\u00b7ne", "sa\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "NE", "VVPP", "$.", "PPER", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.17": {"text": "wie sie der himmlisch, Vater do usem saftigen Erdrich", "tokens": ["wie", "sie", "der", "himm\u00b7lisch", ",", "Va\u00b7ter", "do", "u\u00b7sem", "saf\u00b7ti\u00b7gen", "Erd\u00b7rich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ART", "ADJD", "$,", "NN", "ADV", "ADJA", "ADJA", "NN"], "meter": "-+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "n\u00e4hrt und chleidet und puzt mit allerlei lieblige Farbe,", "tokens": ["n\u00e4hrt", "und", "chlei\u00b7det", "und", "puzt", "mit", "al\u00b7ler\u00b7lei", "lieb\u00b7li\u00b7ge", "Far\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "KON", "VVFIN", "APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+--+-+-++--+-", "measure": "trochaic.septa.relaxed"}, "line.19": {"text": "wenn sie scho nit spinnen und \u00fcberbindlige neihe;", "tokens": ["wenn", "sie", "scho", "nit", "spin\u00b7nen", "und", "\u00fc\u00b7ber\u00b7bind\u00b7li\u00b7ge", "nei\u00b7he", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKNEG", "VVINF", "KON", "ADJA", "NN", "$."], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.20": {"text": "und es gangem selber so. Si R\u00f6ckli seig gwachse,", "tokens": ["und", "es", "gan\u00b7gem", "sel\u00b7ber", "so", ".", "Si", "R\u00f6ck\u00b7li", "seig", "gwach\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJA", "ADV", "ADV", "$.", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.21": {"text": "wiener gr\u00f6\u00dfer worde seig, er trag's doch afange", "tokens": ["wie\u00b7ner", "gr\u00f6\u00b7\u00dfer", "wor\u00b7de", "seig", ",", "er", "trag's", "doch", "a\u00b7fan\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "ADJA", "NN", "VVFIN", "$,", "PPER", "VVFIN", "ADV", "NN"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.22": {"text": "menge Monet Tag und Nacht und Sunntig und Werchtig,", "tokens": ["men\u00b7ge", "Mo\u00b7net", "Tag", "und", "Nacht", "und", "Sunn\u00b7tig", "und", "Werch\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "KON", "NN", "KON", "ADJD", "KON", "ADJD", "$,"], "meter": "--+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.23": {"text": "und es seig no nagelneu, wie ehnen am Schilfmeer", "tokens": ["und", "es", "seig", "no", "na\u00b7gel\u00b7neu", ",", "wie", "eh\u00b7nen", "am", "Schilf\u00b7meer"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "NE", "ADJD", "$,", "PWAV", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.24": {"text": "'s Plunder blibe seig, wo d'Chinder Israel treit hen,", "tokens": ["'s", "Plun\u00b7der", "bli\u00b7be", "seig", ",", "wo", "d'\u00b7Chin\u00b7der", "Is\u00b7rael", "treit", "hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVFIN", "ADJD", "$,", "PWAV", "ART", "NE", "NN", "VVINF", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.25": {"text": "d'Schnider seigen all verlumpt, wo unterne gsi sin,", "tokens": ["d'\u00b7Sch\u00b7ni\u00b7der", "sei\u00b7gen", "all", "ver\u00b7lumpt", ",", "wo", "un\u00b7ter\u00b7ne", "gsi", "sin", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIAT", "VVFIN", "$,", "PWAV", "FM.fr", "FM.fr", "FM.fr", "$,"], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.26": {"text": "und er heig kei Sch\u00fcren und heig kei Zehnten im Etter,", "tokens": ["und", "er", "heig", "kei", "Sch\u00fc\u00b7ren", "und", "heig", "kei", "Zehn\u00b7ten", "im", "Et\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJD", "PIAT", "NN", "KON", "ADJD", "PIAT", "NN", "APPRART", "NN", "$,"], "meter": "--+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.27": {"text": "und kei Burgergob; doch gang der Vater im Himmel", "tokens": ["und", "kei", "Bur\u00b7ger\u00b7gob", ";", "doch", "gang", "der", "Va\u00b7ter", "im", "Him\u00b7mel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "$.", "ADV", "ADV", "ART", "NN", "APPRART", "NN"], "meter": "-+--+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.28": {"text": "nie verbei, er geb em n\u00e4umis z'Morgen und z'Mittag;", "tokens": ["nie", "ver\u00b7bei", ",", "er", "geb", "em", "n\u00e4u\u00b7mis", "z'\u00b7Mor\u00b7gen", "und", "z'\u00b7Mit\u00b7tag", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+-+-+--+-+--++-", "measure": "trochaic.septa.relaxed"}, "line.29": {"text": "het er nit so gseit, se hani mer's eso vorgstellt.", "tokens": ["het", "er", "nit", "so", "gseit", ",", "se", "ha\u00b7ni", "mer's", "e\u00b7so", "vorgs\u00b7tellt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADV", "ADJD", "$,", "FM", "FM", "FM", "FM", "VVPP", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.30": {"text": "Woner ufgh\u00f6rt het und woner's Schn\u00e4beli puzt het,", "tokens": ["Wo\u00b7ner", "uf\u00b7gh\u00f6rt", "het", "und", "wo\u00b7ner's", "Schn\u00e4\u00b7be\u00b7li", "puzt", "het", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "VAFIN", "KON", "NE", "NE", "VVFIN", "VAFIN", "$,"], "meter": "+--+--+-+--+-", "measure": "dactylic.di.plus"}, "line.31": {"text": "d' Immli hen scho Orgle gspilt, se denki, jez gangi", "tokens": ["d'", "I\u00b7mmli", "hen", "scho", "Org\u00b7le", "gs\u00b7pilt", ",", "se", "den\u00b7ki", ",", "jez", "gan\u00b7gi"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["FM", "FM", "FM", "FM", "NN", "VVFIN", "$,", "FM", "FM", "$,", "XY", "XY"], "meter": "--+--+-+-+-+-+-", "measure": "anapaest.di.plus"}, "line.32": {"text": "do dur d'Rebberg uf, und woni oben am Gupf bi,", "tokens": ["do", "dur", "d'\u00b7Reb\u00b7berg", "uf", ",", "und", "wo\u00b7ni", "o\u00b7ben", "am", "Gupf", "bi", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$,", "KON", "PWAV", "ADV", "APPRART", "NN", "NE", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.33": {"text": "l\u00fctet's \u00fcbersmol mit alle Glocken in d'Chilche.", "tokens": ["l\u00fc\u00b7tet's", "\u00fc\u00b7bers\u00b7mol", "mit", "al\u00b7le", "Glo\u00b7cken", "in", "d'\u00b7Chil\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PIAT", "NN", "APPR", "NE", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.34": {"text": "Jo do bini, denki, 's isch ordli, a\u00df der au wartet,", "tokens": ["Jo", "do", "bi\u00b7ni", ",", "den\u00b7ki", ",", "'s", "isch", "ord\u00b7li", ",", "a\u00df", "der", "au", "war\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$,", "PRELS", "$,", "PPER", "ADJD", "VVFIN", "$,", "VVFIN", "ART", "NE", "VVFIN", "$,"], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.35": {"text": "bis me chunnt, und gang in d'Chilche. Was i drin gh\u00f6rt ha,", "tokens": ["bis", "me", "chunnt", ",", "und", "gang", "in", "d'\u00b7Chil\u00b7che", ".", "Was", "i", "drin", "gh\u00f6rt", "ha", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.fr", "FM.fr", "FM.fr", "$,", "KON", "ADJD", "APPR", "NE", "$.", "PWS", "NE", "ADV", "VVFIN", "NE", "$,"], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.36": {"text": "will i jez verzehle. \u2013 Gang, Vreni, leng mer e Stuhl her! \u2013", "tokens": ["will", "i", "jez", "ver\u00b7zeh\u00b7le", ".", "\u2013", "Gang", ",", "Vre\u00b7ni", ",", "leng", "mer", "e", "Stuhl", "her", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "NE", "NE", "VVFIN", "$.", "$(", "NN", "$,", "NE", "$,", "VVFIN", "ADV", "ADJA", "NN", "PTKVZ", "$.", "$("], "meter": "+-+-+-+--+-++-", "measure": "trochaic.septa.relaxed"}, "line.37": {"text": "Chani 's nit sage, wie er, se willi 's sage, wie i 's cha.", "tokens": ["Cha\u00b7ni", "'s", "nit", "sa\u00b7ge", ",", "wie", "er", ",", "se", "wil\u00b7li", "'s", "sa\u00b7ge", ",", "wie", "i", "'s", "cha", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PTKNEG", "VVFIN", "$,", "PWAV", "PPER", "$,", "FM", "FM", "PPER", "VVFIN", "$,", "PWAV", "NE", "PPER", "NE", "$."], "meter": "+--+--+--+-+-+-+-+", "measure": "dactylic.tri.plus"}, "line.38": {"t