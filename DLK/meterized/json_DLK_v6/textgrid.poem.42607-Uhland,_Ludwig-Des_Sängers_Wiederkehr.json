{"textgrid.poem.42607": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Des S\u00e4ngers Wiederkehr", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dort liegt der S\u00e4nger auf der Bahre,", "tokens": ["Dort", "liegt", "der", "S\u00e4n\u00b7ger", "auf", "der", "Bah\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des bleicher Mund kein Lied beginnt,", "tokens": ["Des", "blei\u00b7cher", "Mund", "kein", "Lied", "be\u00b7ginnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es kr\u00e4nzen Daphnes falbe Haare", "tokens": ["Es", "kr\u00e4n\u00b7zen", "Daph\u00b7nes", "fal\u00b7be", "Haa\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Stirne, die nichts mehr ersinnt.", "tokens": ["Die", "Stir\u00b7ne", ",", "die", "nichts", "mehr", "er\u00b7sinnt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIS", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Man legt zu ihm in schmucken Rollen", "tokens": ["Man", "legt", "zu", "ihm", "in", "schmu\u00b7cken", "Rol\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "APPR", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die letzten Lieder, die er sang;", "tokens": ["Die", "letz\u00b7ten", "Lie\u00b7der", ",", "die", "er", "sang", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Leier, die so hell erschollen,", "tokens": ["Die", "Lei\u00b7er", ",", "die", "so", "hell", "er\u00b7schol\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Liegt ihm in Armen sonder Klang.", "tokens": ["Liegt", "ihm", "in", "Ar\u00b7men", "son\u00b7der", "Klang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "So schlummert er den tiefen Schlummer.", "tokens": ["So", "schlum\u00b7mert", "er", "den", "tie\u00b7fen", "Schlum\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sein Lied umweht noch jedes Ohr,", "tokens": ["Sein", "Lied", "um\u00b7weht", "noch", "je\u00b7des", "Ohr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch n\u00e4hrt es stets den herben Kummer,", "tokens": ["Doch", "n\u00e4hrt", "es", "stets", "den", "her\u00b7ben", "Kum\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df man den Herrlichen verlor.", "tokens": ["Da\u00df", "man", "den", "Herr\u00b7li\u00b7chen", "ver\u00b7lor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.4": {"line.1": {"text": "Wohl Monden, Jahre sind verschwunden,", "tokens": ["Wohl", "Mon\u00b7den", ",", "Jah\u00b7re", "sind", "ver\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zypressen wuchsen um sein Grab;", "tokens": ["Zyp\u00b7res\u00b7sen", "wuch\u00b7sen", "um", "sein", "Grab", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Die seinen Tod so herb empfunden,", "tokens": ["Die", "sei\u00b7nen", "Tod", "so", "herb", "emp\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie sanken alle selbst hinab.", "tokens": ["Sie", "san\u00b7ken", "al\u00b7le", "selbst", "hin\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Doch wie der Fr\u00fchling wiederkehret", "tokens": ["Doch", "wie", "der", "Fr\u00fch\u00b7ling", "wie\u00b7der\u00b7keh\u00b7ret"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit frischer Kraft und Regsamkeit", "tokens": ["Mit", "fri\u00b7scher", "Kraft", "und", "Reg\u00b7sam\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So wandelt jetzt, verj\u00fcngt, verkl\u00e4ret,", "tokens": ["So", "wan\u00b7delt", "jetzt", ",", "ver\u00b7j\u00fcngt", ",", "ver\u00b7kl\u00e4\u00b7ret", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der S\u00e4nger in der neuen Zeit.", "tokens": ["Der", "S\u00e4n\u00b7ger", "in", "der", "neu\u00b7en", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Er ist den Lebenden vereinet,", "tokens": ["Er", "ist", "den", "Le\u00b7ben\u00b7den", "ver\u00b7ei\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vom Hauch des Grabes keine Spur!", "tokens": ["Vom", "Hauch", "des", "Gra\u00b7bes", "kei\u00b7ne", "Spur", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Vorwelt, die ihn tot gemeinet,", "tokens": ["Die", "Vor\u00b7welt", ",", "die", "ihn", "tot", "ge\u00b7mei\u00b7net", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Lebt selbst in seinem Liede nur.", "tokens": ["Lebt", "selbst", "in", "sei\u00b7nem", "Lie\u00b7de", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}