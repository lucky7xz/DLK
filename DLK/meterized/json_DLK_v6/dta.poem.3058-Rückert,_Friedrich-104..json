{"dta.poem.3058": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "104.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So oft du wieder treibst, was du einmal getrieben,", "tokens": ["So", "oft", "du", "wie\u00b7der", "treibst", ",", "was", "du", "ein\u00b7mal", "ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "ADV", "VVFIN", "$,", "PWS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So oft du wieder schreibst, was du einmal geschrieben;", "tokens": ["So", "oft", "du", "wie\u00b7der", "schreibst", ",", "was", "du", "ein\u00b7mal", "ge\u00b7schrie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "ADV", "VVFIN", "$,", "PWS", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Scheint ein Verst\u00e4ndnis erst der Sache dir erstanden,", "tokens": ["Scheint", "ein", "Ver\u00b7st\u00e4nd\u00b7nis", "erst", "der", "Sa\u00b7che", "dir", "er\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als h\u00e4ttest du sie gar vom Anfang nicht verstanden.", "tokens": ["Als", "h\u00e4t\u00b7test", "du", "sie", "gar", "vom", "An\u00b7fang", "nicht", "ver\u00b7stan\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "PPER", "ADV", "APPRART", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Verstehst du wirklich sie nun erst, und damals nicht?", "tokens": ["Ver\u00b7stehst", "du", "wirk\u00b7lich", "sie", "nun", "erst", ",", "und", "da\u00b7mals", "nicht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PPER", "ADV", "ADV", "$,", "KON", "ADV", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich denke sie erscheint dir nur im neuen Licht.", "tokens": ["Ich", "den\u00b7ke", "sie", "er\u00b7scheint", "dir", "nur", "im", "neu\u00b7en", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "PPER", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Thu denn nicht Unrecht dem, was du gewesen bist,", "tokens": ["Thu", "denn", "nicht", "Un\u00b7recht", "dem", ",", "was", "du", "ge\u00b7we\u00b7sen", "bist", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKNEG", "NN", "ART", "$,", "PWS", "PPER", "VAPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Noch zuviel Ehre dem, was draus geworden ist!", "tokens": ["Noch", "zu\u00b7viel", "Eh\u00b7re", "dem", ",", "was", "draus", "ge\u00b7wor\u00b7den", "ist", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "ART", "$,", "PRELS", "PAV", "VAPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Und mache dann von dir auf andre die Anwendung:", "tokens": ["Und", "ma\u00b7che", "dann", "von", "dir", "auf", "and\u00b7re", "die", "An\u00b7wen\u00b7dung", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "PPER", "APPR", "PIS", "ART", "NN", "$."], "meter": "-+-+-+-+-+++-", "measure": "unknown.measure.septa"}, "line.2": {"text": "Steh auch das Licht, in dem sie sehn, nicht an f\u00fcr Blendung!", "tokens": ["Steh", "auch", "das", "Licht", ",", "in", "dem", "sie", "sehn", ",", "nicht", "an", "f\u00fcr", "Blen\u00b7dung", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "VVINF", "$,", "PTKNEG", "APPR", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}