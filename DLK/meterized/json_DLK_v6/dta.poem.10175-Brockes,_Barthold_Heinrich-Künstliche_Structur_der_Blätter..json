{"dta.poem.10175": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "K\u00fcnstliche Structur der Bl\u00e4tter.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich habe j\u00fcngst ein Eichen-Blat gefunden,", "tokens": ["Ich", "ha\u00b7be", "j\u00fcngst", "ein", "Ei\u00b7chen\u00b7Blat", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das, durch der kleinen W\u00fcrmer Schaar,", "tokens": ["Das", ",", "durch", "der", "klei\u00b7nen", "W\u00fcr\u00b7mer", "Schaar", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So k\u00fcnstlich ausgefressen war;", "tokens": ["So", "k\u00fcnst\u00b7lich", "aus\u00b7ge\u00b7fres\u00b7sen", "war", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df alle Aederchen darin in netter Ordnung stunden.", "tokens": ["Da\u00df", "al\u00b7le", "A\u00b7e\u00b7der\u00b7chen", "da\u00b7rin", "in", "net\u00b7ter", "Ord\u00b7nung", "stun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "PAV", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+--+-+-+-+-", "measure": "iambic.septa.relaxed"}}, "stanza.2": {"line.1": {"text": "Unzehlig war der zarten G\u00e4nge", "tokens": ["Un\u00b7zeh\u00b7lig", "war", "der", "zar\u00b7ten", "G\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ver\u00e4ndrung, Unterschied und Menge.", "tokens": ["Ver\u00b7\u00e4n\u00b7drung", ",", "Un\u00b7ter\u00b7schied", "und", "Men\u00b7ge", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Es kam die\u00df Blat mir recht nat\u00fcrlich vor,", "tokens": ["Es", "kam", "die\u00df", "Blat", "mir", "recht", "na\u00b7t\u00fcr\u00b7lich", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDS", "NN", "PPER", "ADV", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Als wie der allerz\u00e4rtste Flor.", "tokens": ["Als", "wie", "der", "al\u00b7ler\u00b7z\u00e4rts\u00b7te", "Flor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Man kunte nun, wie sie so zierlich, zart und sch\u00f6n,", "tokens": ["Man", "kun\u00b7te", "nun", ",", "wie", "sie", "so", "zier\u00b7lich", ",", "zart", "und", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ADV", "$,", "PWAV", "PPER", "ADV", "ADJD", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Bewunderns-wehrt geordnet, stehn,", "tokens": ["Be\u00b7wun\u00b7derns\u00b7wehrt", "ge\u00b7ord\u00b7net", ",", "stehn", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NN", "VVPP", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Erst, durch der W\u00fcrmer H\u00fclffe, sehn:", "tokens": ["Erst", ",", "durch", "der", "W\u00fcr\u00b7mer", "H\u00fclf\u00b7fe", ",", "sehn", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "$,", "APPR", "ART", "NN", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So da\u00df, da uns so sch\u00f6n-sonst ungesehne Sachen", "tokens": ["So", "da\u00df", ",", "da", "uns", "so", "sch\u00f6n\u00b7sonst", "un\u00b7ge\u00b7seh\u00b7ne", "Sa\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "$,", "KOUS", "PPER", "ADV", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die kleinen W\u00fcrmer sichtbar machen;", "tokens": ["Die", "klei\u00b7nen", "W\u00fcr\u00b7mer", "sicht\u00b7bar", "ma\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Die kleinen W\u00fcrmer selbst, zu unsers Sch\u00f6pfers Ehren,", "tokens": ["Die", "klei\u00b7nen", "W\u00fcr\u00b7mer", "selbst", ",", "zu", "un\u00b7sers", "Sch\u00f6p\u00b7fers", "Eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$,", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "F\u00fcr unser solche Kunst betrachtendes Gesicht,", "tokens": ["F\u00fcr", "un\u00b7ser", "sol\u00b7che", "Kunst", "be\u00b7trach\u00b7ten\u00b7des", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "PIAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die Zahlen Seiner Wunder mehren.", "tokens": ["Die", "Zah\u00b7len", "Sei\u00b7ner", "Wun\u00b7der", "meh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "So schweige denn, ger\u00fchrte Seele, nicht!", "tokens": ["So", "schwei\u00b7ge", "denn", ",", "ge\u00b7r\u00fchr\u00b7te", "See\u00b7le", ",", "nicht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$,", "ADJA", "NN", "$,", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Bewundre, freue dich, besinge", "tokens": ["Be\u00b7wund\u00b7re", ",", "freu\u00b7e", "dich", ",", "be\u00b7sin\u00b7ge"], "token_info": ["word", "punct", "word", "word", "punct", "word"], "pos": ["VVFIN", "$,", "VVFIN", "PPER", "$,", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die weise Macht des Sch\u00f6pfers aller Dinge,", "tokens": ["Die", "wei\u00b7se", "Macht", "des", "Sch\u00f6p\u00b7fers", "al\u00b7ler", "Din\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der k\u00fcnstlich wirckenden Natur,", "tokens": ["Der", "k\u00fcnst\u00b7lich", "wir\u00b7cken\u00b7den", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die alle Kunst weit \u00fcbersteiget,", "tokens": ["Die", "al\u00b7le", "Kunst", "weit", "\u00fc\u00b7bers\u00b7tei\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So dem geschicktesten Zergliedrer in der Welt,", "tokens": ["So", "dem", "ge\u00b7schick\u00b7tes\u00b7ten", "Zer\u00b7glied\u00b7rer", "in", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Trotz aller Kunst, zu zeigen m\u00f6glich f\u00e4llt;", "tokens": ["Trotz", "al\u00b7ler", "Kunst", ",", "zu", "zei\u00b7gen", "m\u00f6g\u00b7lich", "f\u00e4llt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "PTKZU", "VVINF", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Durch kleiner W\u00fcrmer Z\u00e4hne, zeiget.", "tokens": ["Durch", "klei\u00b7ner", "W\u00fcr\u00b7mer", "Z\u00e4h\u00b7ne", ",", "zei\u00b7get", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}