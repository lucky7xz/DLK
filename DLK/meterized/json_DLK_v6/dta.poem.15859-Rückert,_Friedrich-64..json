{"dta.poem.15859": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "64.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das ist das Wetter nicht, das, als sie mich gebar,", "tokens": ["Das", "ist", "das", "Wet\u00b7ter", "nicht", ",", "das", ",", "als", "sie", "mich", "ge\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "PTKNEG", "$,", "PDS", "$,", "KOUS", "PPER", "PRF", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Mutter mir versprach, bald ists nun funfzig Jahr,", "tokens": ["Die", "Mut\u00b7ter", "mir", "ver\u00b7sprach", ",", "bald", "ists", "nun", "funf\u00b7zig", "Jahr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$,", "ADV", "VAFIN", "ADV", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Als einen Monatlang sie die Geburt verschoben,", "tokens": ["Als", "ei\u00b7nen", "Mo\u00b7nat\u00b7lang", "sie", "die", "Ge\u00b7burt", "ver\u00b7scho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df sie erst den April lie\u00df seine Laun' austoben:", "tokens": ["Da\u00df", "sie", "erst", "den", "Ap\u00b7ril", "lie\u00df", "sei\u00b7ne", "Laun'", "aus\u00b7to\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VVFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Im warmen Scho\u00dfe ward ich z\u00e4rtlich aufgehoben,", "tokens": ["Im", "war\u00b7men", "Scho\u00b7\u00dfe", "ward", "ich", "z\u00e4rt\u00b7lich", "auf\u00b7ge\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bis v\u00f6llig auf der Flur der Wintersturm verschnoben:", "tokens": ["Bis", "v\u00f6l\u00b7lig", "auf", "der", "Flur", "der", "Win\u00b7ter\u00b7sturm", "ver\u00b7schno\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "APPR", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Als am sechzehnten Mai war aller Frost vorbei,", "tokens": ["Als", "am", "sech\u00b7zehn\u00b7ten", "Mai", "war", "al\u00b7ler", "Frost", "vor\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "ADJA", "NN", "VAFIN", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Schiens da\u00df ihr erster Sohn ihr zu geb\u00e4ren sei.", "tokens": ["Schiens", "da\u00df", "ihr", "ers\u00b7ter", "Sohn", "ihr", "zu", "ge\u00b7b\u00e4\u00b7ren", "sei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPOSAT", "ADJA", "NN", "PPER", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Sie l\u00e4chelte dabei und sprach: Dein Leben sei", "tokens": ["Sie", "l\u00e4\u00b7chel\u00b7te", "da\u00b7bei", "und", "sprach", ":", "Dein", "Le\u00b7ben", "sei"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PAV", "KON", "VVFIN", "$.", "PPOSAT", "NN", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von Kummerfr\u00f6sten frei stets ein sechzehnter Mai.", "tokens": ["Von", "Kum\u00b7mer\u00b7fr\u00f6s\u00b7ten", "frei", "stets", "ein", "sech\u00b7zehn\u00b7ter", "Mai", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "O h\u00e4tte sie's vermocht, die nun im Grabe ruht,", "tokens": ["O", "h\u00e4t\u00b7te", "sie's", "ver\u00b7mocht", ",", "die", "nun", "im", "Gra\u00b7be", "ruht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "VVPP", "$,", "PRELS", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mir zeigte die Natur stets m\u00fctterlichen Muth,", "tokens": ["Mir", "zeig\u00b7te", "die", "Na\u00b7tur", "stets", "m\u00fct\u00b7ter\u00b7li\u00b7chen", "Muth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Die so stiefm\u00fctterlich sich leider nun erweiset,", "tokens": ["Die", "so", "stief\u00b7m\u00fct\u00b7ter\u00b7lich", "sich", "lei\u00b7der", "nun", "er\u00b7wei\u00b7set", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "PRF", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df mein Geburtstag sich mit Winterfrost umeiset.", "tokens": ["Da\u00df", "mein", "Ge\u00b7burts\u00b7tag", "sich", "mit", "Win\u00b7ter\u00b7frost", "um\u00b7ei\u00b7set", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PRF", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Das hat, so ahnungsreich, die Mutter auch geahnt,", "tokens": ["Das", "hat", ",", "so", "ah\u00b7nungs\u00b7reich", ",", "die", "Mut\u00b7ter", "auch", "ge\u00b7ahnt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$,", "ADV", "ADJD", "$,", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die mit Sprichw\u00f6rtern mich daran als Kind gemahnt.", "tokens": ["Die", "mit", "Sprich\u00b7w\u00f6r\u00b7tern", "mich", "da\u00b7ran", "als", "Kind", "ge\u00b7mahnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "PRF", "PAV", "KOUS", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Das eine war: Der Mai, der Mai ist nichts zu gut,", "tokens": ["Das", "ei\u00b7ne", "war", ":", "Der", "Mai", ",", "der", "Mai", "ist", "nichts", "zu", "gut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VAFIN", "$.", "ART", "NN", "$,", "ART", "NN", "VAFIN", "PIS", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er schneit dem Sch\u00e4fer wol zuweilen auf den Hut.", "tokens": ["Er", "schneit", "dem", "Sch\u00e4\u00b7fer", "wol", "zu\u00b7wei\u00b7len", "auf", "den", "Hut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Das andre Sprichwort klang noch frostiger: Im Mai,", "tokens": ["Das", "and\u00b7re", "Sprich\u00b7wort", "klang", "noch", "fros\u00b7ti\u00b7ger", ":", "Im", "Mai", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "ADJD", "$.", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Im Mai erfrieret oft der Vogel selbst im Ei.", "tokens": ["Im", "Mai", "er\u00b7frie\u00b7ret", "oft", "der", "Vo\u00b7gel", "selbst", "im", "Ei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ADV", "ART", "NN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Und wenn ich feiern mein Geburtsfest m\u00fc\u00dft' im Freien,", "tokens": ["Und", "wenn", "ich", "fei\u00b7ern", "mein", "Ge\u00b7burts\u00b7fest", "m\u00fc\u00dft'", "im", "Frei\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "PPOSAT", "NN", "VMFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So w\u00fcrde auf den Hut Herr Mai dem Sch\u00e4fer schneien.", "tokens": ["So", "w\u00fcr\u00b7de", "auf", "den", "Hut", "Herr", "Mai", "dem", "Sch\u00e4\u00b7fer", "schnei\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "ART", "NN", "NN", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.12": {"line.1": {"text": "Und h\u00e4tt' ich nicht ein Nest ein warmes mir erkoren,", "tokens": ["Und", "h\u00e4tt'", "ich", "nicht", "ein", "Nest", "ein", "war\u00b7mes", "mir", "er\u00b7ko\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PTKNEG", "ART", "NN", "ART", "ADJA", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So w\u00e4r' im Mai im Ei der Vogel gar erfroren.", "tokens": ["So", "w\u00e4r'", "im", "Mai", "im", "Ei", "der", "Vo\u00b7gel", "gar", "er\u00b7fro\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPRART", "NN", "APPRART", "NN", "ART", "NE", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}