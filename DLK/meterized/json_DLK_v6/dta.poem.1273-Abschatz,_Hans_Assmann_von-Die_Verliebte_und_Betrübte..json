{"dta.poem.1273": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Die Verliebte und Betr\u00fcbte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Betr\u00fcbte Nacht/ in der mich Furcht und Schrecken", "tokens": ["Be\u00b7tr\u00fcb\u00b7te", "Nacht", "/", "in", "der", "mich", "Furcht", "und", "Schre\u00b7cken"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$(", "APPR", "PRELS", "PPER", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ohn Unterla\u00df von meiner Ruhe wecken/", "tokens": ["Ohn", "Un\u00b7ter\u00b7la\u00df", "von", "mei\u00b7ner", "Ru\u00b7he", "we\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn k\u00f6mmt ein mahl die lange Mitternacht/", "tokens": ["Wenn", "k\u00f6mmt", "ein", "mahl", "die", "lan\u00b7ge", "Mit\u00b7ter\u00b7nacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "ART", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die meiner Pein ein endlich Ende macht?", "tokens": ["Die", "mei\u00b7ner", "Pein", "ein", "end\u00b7lich", "En\u00b7de", "macht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ART", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Du gehst vorbey/ mein Leyden bleibt zu r\u00fccke/", "tokens": ["Du", "gehst", "vor\u00b7bey", "/", "mein", "Ley\u00b7den", "bleibt", "zu", "r\u00fc\u00b7cke", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$(", "PPOSAT", "NN", "VVFIN", "PTKZU", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Stunden fliehn/ doch nicht mein Ungel\u00fccke.", "tokens": ["Die", "Stun\u00b7den", "fliehn", "/", "doch", "nicht", "mein", "Un\u00b7ge\u00b7l\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$(", "ADV", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dein k\u00fchler Thau erfrischt den trocknen Klee/", "tokens": ["Dein", "k\u00fch\u00b7ler", "Thau", "er\u00b7frischt", "den", "trock\u00b7nen", "Klee", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mich \u00fcberschwemmt der Thr\u00e4nen heisse See.", "tokens": ["Mich", "\u00fc\u00b7bersc\u00b7hwemmt", "der", "Thr\u00e4\u00b7nen", "heis\u00b7se", "See", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Es ruht die Welt in sanfften Schlaff gewieget/", "tokens": ["Es", "ruht", "die", "Welt", "in", "sanff\u00b7ten", "Schlaff", "ge\u00b7wie\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn meine Seel in tausend Aengsten lieget/", "tokens": ["Wenn", "mei\u00b7ne", "Seel", "in", "tau\u00b7send", "A\u00b7engs\u00b7ten", "lie\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "CARD", "NN", "VVFIN", "$("], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ich werffe mich mit Seuffzen hin und her/", "tokens": ["Ich", "werf\u00b7fe", "mich", "mit", "Seuff\u00b7zen", "hin", "und", "her", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "PTKVZ", "KON", "PTKVZ", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das leichte Bett ist mir als Bley zu schwer.", "tokens": ["Das", "leich\u00b7te", "Bett", "ist", "mir", "als", "Bley", "zu", "schwer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "KOUS", "NN", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Die stille Glutt durchkocht die d\u00fcrre Seele/", "tokens": ["Die", "stil\u00b7le", "Glutt", "durch\u00b7kocht", "die", "d\u00fcr\u00b7re", "See\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Hertze brennt wie Etnens Schwefel-H\u00f6le/", "tokens": ["Das", "Hert\u00b7ze", "brennt", "wie", "Et\u00b7nens", "Schwe\u00b7fel\u00b7H\u00f6le", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "VVFIN", "KOKOM", "NE", "NE", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mein Wange zeigt der rothen Flamme Schein/", "tokens": ["Mein", "Wan\u00b7ge", "zeigt", "der", "ro\u00b7then", "Flam\u00b7me", "Schein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wird aber bald voll bleicher Asche seyn.", "tokens": ["Wird", "a\u00b7ber", "bald", "voll", "blei\u00b7cher", "A\u00b7sche", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADJD", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Kein schwerer Traum darff mich bek\u00fcmmert machen/", "tokens": ["Kein", "schwe\u00b7rer", "Traum", "darff", "mich", "be\u00b7k\u00fcm\u00b7mert", "ma\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VMFIN", "PRF", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich habe Qual genung bey hellem Wachen.", "tokens": ["Ich", "ha\u00b7be", "Qual", "ge\u00b7nung", "bey", "hel\u00b7lem", "Wa\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mein Leben ist ein Traum und Gauckel-Spil/", "tokens": ["Mein", "Le\u00b7ben", "ist", "ein", "Traum", "und", "Gau\u00b7ckel\u00b7Spil", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Damit mich Gl\u00fcck und Zeit beth\u00f6ren will.", "tokens": ["Da\u00b7mit", "mich", "Gl\u00fcck", "und", "Zeit", "be\u00b7th\u00f6\u00b7ren", "will", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "KON", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Komm/ blasser Mond/ und leuchte mir zu Grabe:", "tokens": ["Komm", "/", "blas\u00b7ser", "Mond", "/", "und", "leuch\u00b7te", "mir", "zu", "Gra\u00b7be", ":"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "ADJA", "NN", "$(", "KON", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da ich forthin die beste Ruhstatt habe.", "tokens": ["Da", "ich", "for\u00b7thin", "die", "bes\u00b7te", "Ruh\u00b7statt", "ha\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Erreich\u2019 ich gleich des jungen Tages Licht/", "tokens": ["Er\u00b7reich'", "ich", "gleich", "des", "jun\u00b7gen", "Ta\u00b7ges", "Licht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So \u00fcberleb\u2019 ich doch die Sonne nicht.", "tokens": ["So", "\u00fc\u00b7ber\u00b7leb'", "ich", "doch", "die", "Son\u00b7ne", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}