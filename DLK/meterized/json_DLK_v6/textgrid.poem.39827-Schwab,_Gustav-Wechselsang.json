{"textgrid.poem.39827": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Wechselsang", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.85", "af:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Sie singen Wettgesang,", "tokens": ["Sie", "sin\u00b7gen", "Wett\u00b7ge\u00b7sang", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Glocke hell im Tone,", "tokens": ["Die", "Glo\u00b7cke", "hell", "im", "To\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das St\u00fcck mit rauhem Klang.", "tokens": ["Das", "St\u00fcck", "mit", "rau\u00b7hem", "Klang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "\u00bbkommt her, frohlockt die Glocke,", "tokens": ["\u00bb", "kommt", "her", ",", "froh\u00b7lockt", "die", "Glo\u00b7cke", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PTKVZ", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Kommt her aus Stadt und Land!", "tokens": ["Kommt", "her", "aus", "Stadt", "und", "Land", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Herbei im Friedensrocke,", "tokens": ["Her\u00b7bei", "im", "Frie\u00b7dens\u00b7ro\u00b7cke", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den Segen in der Hand!\u00ab", "tokens": ["Den", "Se\u00b7gen", "in", "der", "Hand", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Da donnert die Kanone:", "tokens": ["Da", "don\u00b7nert", "die", "Ka\u00b7no\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "\u00bbnein, kommt im Panzerkleid!", "tokens": ["\u00bb", "nein", ",", "kommt", "im", "Pan\u00b7zer\u00b7kleid", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PTKANT", "$,", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Stellt auf am Heldenthrone", "tokens": ["Stellt", "auf", "am", "Hel\u00b7den\u00b7thro\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das schimmernde Geleit!\u00ab", "tokens": ["Das", "schim\u00b7mern\u00b7de", "Ge\u00b7leit", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die Glocke klagt: \u00bbZum Kampfe", "tokens": ["Die", "Glo\u00b7cke", "klagt", ":", "\u00bb", "Zum", "Kamp\u00b7fe"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Riefst du die Schaaren gern?", "tokens": ["Riefst", "du", "die", "Schaa\u00b7ren", "gern", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du deckst mit Pulverdampfe", "tokens": ["Du", "deckst", "mit", "Pul\u00b7ver\u00b7damp\u00b7fe"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Triumph aus N\u00e4h' und Fern?\u00ab", "tokens": ["Tri\u00b7umph", "aus", "N\u00e4h'", "und", "Fern", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Kanone schnaubt: \u00bbDem Tr\u00e4gen", "tokens": ["Ka\u00b7no\u00b7ne", "schnaubt", ":", "\u00bb", "Dem", "Tr\u00e4\u00b7gen"], "token_info": ["word", "word", "punct", "punct", "word", "word"], "pos": ["NE", "VVFIN", "$.", "$(", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hilft keine Friedenszier.", "tokens": ["Hilft", "kei\u00b7ne", "Frie\u00b7dens\u00b7zier", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zu schirmen euern Segen,", "tokens": ["Zu", "schir\u00b7men", "eu\u00b7ern", "Se\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu drohen blitz' ich hier!\u00ab", "tokens": ["Zu", "dro\u00b7hen", "blitz'", "ich", "hier", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PTKZU", "VVINF", "VVFIN", "PPER", "ADV", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Die Glocke singt: \u00bbVerletze", "tokens": ["Die", "Glo\u00b7cke", "singt", ":", "\u00bb", "Ver\u00b7let\u00b7ze"], "token_info": ["word", "word", "word", "punct", "punct", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nicht mit so wildem Laut", "tokens": ["Nicht", "mit", "so", "wil\u00b7dem", "Laut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vertrag, Altar, Gesetze,", "tokens": ["Ver\u00b7trag", ",", "Al\u00b7tar", ",", "Ge\u00b7set\u00b7ze", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was furchtlos Treue baut!\u00ab", "tokens": ["Was", "furcht\u00b7los", "Treu\u00b7e", "baut", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "ADJD", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Kanone grollt: \u00bbJa singe", "tokens": ["Ka\u00b7no\u00b7ne", "grollt", ":", "\u00bb", "Ja", "sin\u00b7ge"], "token_info": ["word", "word", "punct", "punct", "word", "word"], "pos": ["NE", "VVFIN", "$.", "$(", "PTKANT", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dein Liedchen, da\u00df es gellt!", "tokens": ["Dein", "Lied\u00b7chen", ",", "da\u00df", "es", "gellt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KOUS", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich wei\u00df doch: auf der Klinge", "tokens": ["Ich", "wei\u00df", "doch", ":", "auf", "der", "Klin\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$.", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Des Degens ruht die Welt!\u00ab", "tokens": ["Des", "De\u00b7gens", "ruht", "die", "Welt", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Die Glocke ruft: \u00bbWir fechten", "tokens": ["Die", "Glo\u00b7cke", "ruft", ":", "\u00bb", "Wir", "fech\u00b7ten"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nicht mit Gesch\u00fctz und Stahl!", "tokens": ["Nicht", "mit", "Ge\u00b7sch\u00fctz", "und", "Stahl", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "KON", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Nein, giebt es Eins zu rechten,", "tokens": ["Nein", ",", "giebt", "es", "Eins", "zu", "rech\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "PPER", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So hall's im St\u00e4ndesaal!\u00ab", "tokens": ["So", "hall's", "im", "St\u00e4n\u00b7de\u00b7saal", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "\u00bbso will ich,\u00ab murrt Kanone,", "tokens": ["\u00bb", "so", "will", "ich", ",", "\u00ab", "murrt", "Ka\u00b7no\u00b7ne", ","], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["$(", "ADV", "VMFIN", "PPER", "$,", "$(", "VVFIN", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "\u00bbdoch sein der Widerhall,", "tokens": ["\u00bb", "doch", "sein", "der", "Wi\u00b7der\u00b7hall", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "PPOSAT", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der absingt \u00fcberm Throne", "tokens": ["Der", "ab\u00b7singt", "\u00fc\u00b7berm", "Thro\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der deutschen Siege Schall!\u00ab", "tokens": ["Der", "deut\u00b7schen", "Sie\u00b7ge", "Schall", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Da jauchzt die Glocke: \u00bbHeute,", "tokens": ["Da", "jauchzt", "die", "Glo\u00b7cke", ":", "\u00bb", "Heu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "$(", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ja, heute sing davon!", "tokens": ["Ja", ",", "heu\u00b7te", "sing", "da\u00b7von", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "VVFIN", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Thut's wieder Not, ich l\u00e4ute", "tokens": ["Thut's", "wie\u00b7der", "Not", ",", "ich", "l\u00e4u\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NE", "ADV", "NN", "$,", "PPER", "VVFIN"], "meter": "++-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zum Sturm f\u00fcr Volk und Thron!\u00ab \u2013", "tokens": ["Zum", "Sturm", "f\u00fcr", "Volk", "und", "Thron", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPRART", "NN", "APPR", "NN", "KON", "NN", "$.", "$(", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Nun klingt in mildem Tone", "tokens": ["Nun", "klingt", "in", "mil\u00b7dem", "To\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Glocke Lied voll Ruh',", "tokens": ["Der", "Glo\u00b7cke", "Lied", "voll", "Ruh'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und brausend streicht Kanone", "tokens": ["Und", "brau\u00b7send", "streicht", "Ka\u00b7no\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den tiefen Ba\u00df dazu.", "tokens": ["Den", "tie\u00b7fen", "Ba\u00df", "da\u00b7zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Sie singen ungeschieden", "tokens": ["Sie", "sin\u00b7gen", "un\u00b7ge\u00b7schie\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den Helden und den Herrn,", "tokens": ["Den", "Hel\u00b7den", "und", "den", "Herrn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Des Landes Lieb' im Frieden,", "tokens": ["Des", "Lan\u00b7des", "Lieb'", "im", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und einst im Kriege Stern.", "tokens": ["Und", "einst", "im", "Krie\u00b7ge", "Stern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Sing Glocke, sing Kanone,", "tokens": ["Sing", "Glo\u00b7cke", ",", "sing", "Ka\u00b7no\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df es ein Volk erquickt,", "tokens": ["Da\u00df", "es", "ein", "Volk", "er\u00b7quickt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das heut zur goldnen Krone", "tokens": ["Das", "heut", "zur", "gold\u00b7nen", "Kro\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Viel tausend W\u00fcnsche schickt!", "tokens": ["Viel", "tau\u00b7send", "W\u00fcn\u00b7sche", "schickt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Sing Glocke, sing Kanone,", "tokens": ["Sing", "Glo\u00b7cke", ",", "sing", "Ka\u00b7no\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df es den Schalk verdrie\u00dft,", "tokens": ["Da\u00df", "es", "den", "Schalk", "ver\u00b7drie\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der tr\u00fcben die Patrone", "tokens": ["Der", "tr\u00fc\u00b7ben", "die", "Pat\u00b7ro\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im K\u00f6nigsmord verschie\u00dft!", "tokens": ["Im", "K\u00f6\u00b7nigs\u00b7mord", "ver\u00b7schie\u00dft", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}