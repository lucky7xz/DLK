{"textgrid.poem.46634": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[sei in deinem Trauerfall]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sei in deinem Trauerfall", "tokens": ["Sei", "in", "dei\u00b7nem", "Trau\u00b7er\u00b7fall"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Doch nicht so unduldsam all,", "tokens": ["Doch", "nicht", "so", "un\u00b7duld\u00b7sam", "all", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADV", "ADJD", "PIAT", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Wenn sie tr\u00f6sten dich, so gut", "tokens": ["Wenn", "sie", "tr\u00f6s\u00b7ten", "dich", ",", "so", "gut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "PPER", "$,", "ADV", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sie's verstehn, mit Wortgelall!", "tokens": ["Sie's", "ver\u00b7stehn", ",", "mit", "Wort\u00b7ge\u00b7lall", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VVINF", "$,", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wenn man dir dein Herzgef\u00fchl", "tokens": ["Wenn", "man", "dir", "dein", "Herz\u00b7ge\u00b7f\u00fchl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Gibt zur\u00fcck als leeren Schall;", "tokens": ["Gibt", "zu\u00b7r\u00fcck", "als", "lee\u00b7ren", "Schall", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "KOKOM", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "H\u00f6rst du doch von Felsen auch", "tokens": ["H\u00f6rst", "du", "doch", "von", "Fel\u00b7sen", "auch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Deiner Klage Widerhall;", "tokens": ["Dei\u00b7ner", "Kla\u00b7ge", "Wi\u00b7der\u00b7hall", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Und dich kann beschwichtigen", "tokens": ["Und", "dich", "kann", "be\u00b7schwich\u00b7ti\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ein geschw\u00e4tz'ger Wasserfall.", "tokens": ["Ein", "ge\u00b7schw\u00e4tz'\u00b7ger", "Was\u00b7ser\u00b7fall", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Und wenn ein empfundnes Wort", "tokens": ["Und", "wenn", "ein", "emp\u00b7fund\u00b7nes", "Wort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Einmal mitkommt unterm Schwall;", "tokens": ["Ein\u00b7mal", "mit\u00b7kommt", "un\u00b7term", "Schwall", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Ist's, als ob dir unverhofft", "tokens": ["Ist's", ",", "als", "ob", "dir", "un\u00b7ver\u00b7hofft"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "KOKOM", "KOUS", "PPER", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "S\u00e4ng' im Busch die Nachtigall.", "tokens": ["S\u00e4ng'", "im", "Busch", "die", "Nach\u00b7ti\u00b7gall", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}