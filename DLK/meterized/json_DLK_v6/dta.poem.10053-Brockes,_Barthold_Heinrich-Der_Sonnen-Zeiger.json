{"dta.poem.10053": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Sonnen-Zeiger", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.85", "nl:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sehet dort den Sonnen-Zeiger! merckt! er zeigt den", "tokens": ["Se\u00b7het", "dort", "den", "Son\u00b7nen\u00b7Zei\u00b7ger", "!", "merckt", "!", "er", "zeigt", "den"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN", "$.", "VVFIN", "$.", "PPER", "VVFIN", "ART"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "hellen Schein", "tokens": ["hel\u00b7len", "Schein"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Durch den Schatten, zeigt die Ordnung, zeigt die Gegen-", "tokens": ["Durch", "den", "Schat\u00b7ten", ",", "zeigt", "die", "Ord\u00b7nung", ",", "zeigt", "die", "Ge\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "VVFIN", "ART", "NN", "$,", "VVFIN", "ART", "TRUNC"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "wart und Zeit.", "tokens": ["wart", "und", "Zeit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "M\u00f6gt\u2019 ich doch in meinen Schriften auch ein solcher Schat-", "tokens": ["M\u00f6gt'", "ich", "doch", "in", "mei\u00b7nen", "Schrif\u00b7ten", "auch", "ein", "sol\u00b7cher", "Schat"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "ADV", "ART", "PIAT", "TRUNC"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.6": {"text": "ten seyn", "tokens": ["ten", "seyn"], "token_info": ["word", "word"], "pos": ["VVFIN", "PPOSAT"], "meter": "-+", "measure": "iambic.single"}, "line.7": {"text": "Von der Sonnen-Sonnen Strahle, Gegenwart und Herr-", "tokens": ["Von", "der", "Son\u00b7nen\u00b7Son\u00b7nen", "Strah\u00b7le", ",", "Ge\u00b7gen\u00b7wart", "und", "Herr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "$,", "NN", "KON", "TRUNC"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.8": {"text": "lichkeit!", "tokens": ["lich\u00b7keit", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+", "measure": "iambic.single"}}}}}