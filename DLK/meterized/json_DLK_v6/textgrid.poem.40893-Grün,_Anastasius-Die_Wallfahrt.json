{"textgrid.poem.40893": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "Die Wallfahrt", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht fern von Terouanne hebt sich ein stattlich Schlo\u00df,", "tokens": ["Nicht", "fern", "von", "Te\u00b7rou\u00b7an\u00b7ne", "hebt", "sich", "ein", "statt\u00b7lich", "Schlo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "APPR", "NE", "VVFIN", "PRF", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Da sa\u00df nun Max beim Mahle, mit ihm manch treuer Geno\u00df,", "tokens": ["Da", "sa\u00df", "nun", "Max", "beim", "Mah\u00b7le", ",", "mit", "ihm", "manch", "treu\u00b7er", "Ge\u00b7no\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "NE", "APPRART", "NN", "$,", "APPR", "PPER", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-++--+", "measure": "iambic.septa.chol"}, "line.3": {"text": "Von Dendermond' der Abbas, des Kaisers alter Freund,", "tokens": ["Von", "Den\u00b7der\u00b7mond'", "der", "Ab\u00b7bas", ",", "des", "Kai\u00b7sers", "al\u00b7ter", "Freund", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ART", "NN", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und Hofmann, Narr und Krieger sa\u00df da gar froh vereint.", "tokens": ["Und", "Hof\u00b7mann", ",", "Narr", "und", "Krie\u00b7ger", "sa\u00df", "da", "gar", "froh", "ver\u00b7eint", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "$,", "NN", "KON", "NN", "VVFIN", "ADV", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Die waren just gekommen vom heitren Jagen heim,", "tokens": ["Die", "wa\u00b7ren", "just", "ge\u00b7kom\u00b7men", "vom", "hei\u00b7tren", "Ja\u00b7gen", "heim", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "VVPP", "APPRART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Da ward erz\u00e4hlt manch Waidst\u00fcck, da klang manch Waidmannsreim,", "tokens": ["Da", "ward", "er\u00b7z\u00e4hlt", "manch", "Waid\u00b7st\u00fcck", ",", "da", "klang", "manch", "Waid\u00b7manns\u00b7reim", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "VVFIN", "PIAT", "NN", "$,", "ADV", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Mit lust'gen J\u00e4gerschw\u00e4nken ward reich das Mahl gespickt,", "tokens": ["Mit", "lust'\u00b7gen", "J\u00e4\u00b7ger\u00b7schw\u00e4n\u00b7ken", "ward", "reich", "das", "Mahl", "ge\u00b7spickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "ADJD", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Auf l\u00e4ngst verdautes Wildpret aufs Neu' der Spie\u00df gez\u00fcckt.", "tokens": ["Auf", "l\u00e4ngst", "ver\u00b7dau\u00b7tes", "Wild\u00b7pret", "aufs", "Neu'", "der", "Spie\u00df", "ge\u00b7z\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "APPRART", "NN", "ART", "NN", "VVPP", "$."], "meter": "---+-+--+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Horch! horch! da t\u00f6nt ein Liedlein vom Grund des Thalesstegs,", "tokens": ["Horch", "!", "horch", "!", "da", "t\u00f6nt", "ein", "Lied\u00b7lein", "vom", "Grund", "des", "Tha\u00b7less\u00b7tegs", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VVIMP", "$.", "ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Wie Wallfahrtspilger pflegen zu singen unterwegs,", "tokens": ["Wie", "Wall\u00b7fahrts\u00b7pil\u00b7ger", "pfle\u00b7gen", "zu", "sin\u00b7gen", "un\u00b7ter\u00b7wegs", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVINF", "PTKZU", "VVFIN", "ADV", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Dazwischen klingt ein Gl\u00f6ckchen zum Schlosse sanft herauf,", "tokens": ["Da\u00b7zwi\u00b7schen", "klingt", "ein", "Gl\u00f6ck\u00b7chen", "zum", "Schlos\u00b7se", "sanft", "her\u00b7auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "APPRART", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Da\u00df Max von seinem Sitze fuhr leise horchend auf.", "tokens": ["Da\u00df", "Max", "von", "sei\u00b7nem", "Sit\u00b7ze", "fuhr", "lei\u00b7se", "hor\u00b7chend", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "APPR", "PPOSAT", "NN", "VVFIN", "ADJD", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Da stie\u00df Herr Kunze \u00e4ngstlich am Arm den Nebenmann:", "tokens": ["Da", "stie\u00df", "Herr", "Kun\u00b7ze", "\u00e4ngst\u00b7lich", "am", "Arm", "den", "Ne\u00b7ben\u00b7mann", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "NN", "ADJD", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "\u00bbsto\u00dft schnell, um Gotteswillen, die Gl\u00e4ser zum Vivat an,", "tokens": ["\u00bb", "sto\u00dft", "schnell", ",", "um", "Got\u00b7tes\u00b7wil\u00b7len", ",", "die", "Gl\u00e4\u00b7ser", "zum", "Vi\u00b7vat", "an", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ADJD", "$,", "KOUI", "NN", "$,", "ART", "NN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Damit es \u00fcbert\u00e4ube die\u00df Teufelspsalmodein,", "tokens": ["Da\u00b7mit", "es", "\u00fc\u00b7bert\u00b7\u00e4u\u00b7be", "die\u00df", "Teu\u00b7fel\u00b7spsal\u00b7mo\u00b7de\u00b7in", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "PDS", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Denn h\u00f6rt Herr Max solch Gl\u00f6cklein, gleich treibt's ihn hinterdrein.\u00ab", "tokens": ["Denn", "h\u00f6rt", "Herr", "Max", "solch", "Gl\u00f6c\u00b7klein", ",", "gleich", "treibt's", "ihn", "hin\u00b7ter\u00b7drein", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "NN", "NE", "PIAT", "NN", "$,", "ADV", "VVFIN", "PPER", "PTKVZ", "$.", "$("], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "Da klangen die Becher zusammen so hell und grell mit Macht:", "tokens": ["Da", "klan\u00b7gen", "die", "Be\u00b7cher", "zu\u00b7sam\u00b7men", "so", "hell", "und", "grell", "mit", "Macht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "ADV", "ADJD", "KON", "ADJD", "APPR", "NN", "$."], "meter": "-+--+--+--+-+-+", "measure": "amphibrach.tetra.plus"}, "line.2": {"text": "\u00bbhoch lebe der tapfere Sieger in Guinegate's Schlacht!\u00ab", "tokens": ["\u00bb", "hoch", "le\u00b7be", "der", "tap\u00b7fe\u00b7re", "Sie\u00b7ger", "in", "Gu\u00b7i\u00b7ne\u00b7ga\u00b7te's", "Schlacht", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJD", "VVFIN", "ART", "ADJA", "NN", "APPR", "NE", "NN", "$.", "$("], "meter": "+---+--+--+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Den drohenden Finger l\u00e4chelnd hebt Max gen Kunz empor,", "tokens": ["Den", "dro\u00b7hen\u00b7den", "Fin\u00b7ger", "l\u00e4\u00b7chelnd", "hebt", "Max", "gen", "Kunz", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVFIN", "NE", "APPR", "NE", "PTKVZ", "$,"], "meter": "-+--+-+-++-+-+", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Sein Antlitz still verneigend dankt er dem Jubelchor:", "tokens": ["Sein", "Ant\u00b7litz", "still", "ver\u00b7nei\u00b7gend", "dankt", "er", "dem", "Ju\u00b7be\u00b7lchor", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "VVPP", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.6": {"line.1": {"text": "\u00bbihr ehrt den Sieg im Sieger, jedoch verge\u00dft drob nicht", "tokens": ["\u00bb", "ihr", "ehrt", "den", "Sieg", "im", "Sie\u00b7ger", ",", "je\u00b7doch", "ver\u00b7ge\u00dft", "drob", "nicht"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$,", "ADV", "VVFIN", "ADV", "PTKNEG"], "meter": "-+-+-+----+-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Des Starken, der ihn spendet und f\u00fcr uns Schwache ficht:", "tokens": ["Des", "Star\u00b7ken", ",", "der", "ihn", "spen\u00b7det", "und", "f\u00fcr", "uns", "Schwa\u00b7che", "ficht", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "KON", "APPR", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Seht, Pilger ziehn fromm singend dort gegen Sankt Alban,", "tokens": ["Seht", ",", "Pil\u00b7ger", "ziehn", "fromm", "sin\u00b7gend", "dort", "ge\u00b7gen", "Sankt", "Al\u00b7ban", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "VVFIN", "ADJD", "ADJD", "ADV", "APPR", "VVFIN", "NE", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Drum meint' ich, Freund und Br\u00fcder, wir schlie\u00dfen dem Zug uns an.\u00ab", "tokens": ["Drum", "meint'", "ich", ",", "Freund", "und", "Br\u00fc\u00b7der", ",", "wir", "schlie\u00b7\u00dfen", "dem", "Zug", "uns", "an", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PAV", "VVFIN", "PPER", "$,", "NN", "KON", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$.", "$("], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.7": {"line.1": {"text": "Da sprach Kunz von der Rosen: \u00bbVerzeiht, ich kann kaum gehn!", "tokens": ["Da", "sprach", "Kunz", "von", "der", "Ro\u00b7sen", ":", "\u00bb", "Ver\u00b7zeiht", ",", "ich", "kann", "kaum", "gehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "APPR", "ART", "NN", "$.", "$(", "VVFIN", "$,", "PPER", "VMFIN", "ADV", "VVINF", "$."], "meter": "-+---+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Als ich von jenem H\u00fcgel der Schlacht j\u00fcngst zugesehn,", "tokens": ["Als", "ich", "von", "je\u00b7nem", "H\u00fc\u00b7gel", "der", "Schlacht", "j\u00fcngst", "zu\u00b7ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PDAT", "NN", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Hab' ich vom langen Stehen das rechte Bein verstaucht,", "tokens": ["Hab'", "ich", "vom", "lan\u00b7gen", "Ste\u00b7hen", "das", "rech\u00b7te", "Bein", "ver\u00b7staucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "ADJA", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Auch hat der Dampf des Pulvers mein Aug' fast blind geraucht.\u00ab", "tokens": ["Auch", "hat", "der", "Dampf", "des", "Pul\u00b7vers", "mein", "Aug'", "fast", "blind", "ge\u00b7raucht", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NN", "PPOSAT", "NN", "ADV", "ADJD", "VVPP", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.8": {"line.1": {"text": "Stallmeister Emershofen hob nun halb gr\u00e4mlich an:", "tokens": ["Stall\u00b7meis\u00b7ter", "E\u00b7mers\u00b7ho\u00b7fen", "hob", "nun", "halb", "gr\u00e4m\u00b7lich", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ADV", "ADJD", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "\u00bberlaubt nur, da\u00df ich fr\u00fcher die Pferde satteln kann;", "tokens": ["\u00bb", "er\u00b7laubt", "nur", ",", "da\u00df", "ich", "fr\u00fc\u00b7her", "die", "Pfer\u00b7de", "sat\u00b7teln", "kann", ";"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ADV", "$,", "KOUS", "PPER", "ADJD", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Denn wenn zu Fu\u00df wir gehen in J\u00e4gerstiefeln und Sporn,", "tokens": ["Denn", "wenn", "zu", "Fu\u00df", "wir", "ge\u00b7hen", "in", "J\u00e4\u00b7gers\u00b7tie\u00b7feln", "und", "Sporn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPR", "NN", "PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Verwickeln wir uns schm\u00e4hlich in Buschwerk, Gras und Dorn.\u00ab", "tokens": ["Ver\u00b7wi\u00b7ckeln", "wir", "uns", "schm\u00e4h\u00b7lich", "in", "Busc\u00b7hwerk", ",", "Gras", "und", "Dorn", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADJD", "APPR", "NN", "$,", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.9": {"line.1": {"text": "Aus seiner rechten Tasche zog drauf der Abt ein Buch:", "tokens": ["Aus", "sei\u00b7ner", "rech\u00b7ten", "Ta\u00b7sche", "zog", "drauf", "der", "Abt", "ein", "Buch", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "PAV", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "\u00bbdie Wallfahrt widerrath' ich! Les't hier den weisen Spruch;", "tokens": ["\u00bb", "die", "Wall\u00b7fahrt", "wi\u00b7der\u00b7ra\u00b7th'", "ich", "!", "Les't", "hier", "den", "wei\u00b7sen", "Spruch", ";"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "PPER", "$.", "NE", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Da hei\u00dft's: ", "tokens": ["Da", "hei\u00dft's", ":"], "token_info": ["word", "word", "punct"], "pos": ["KOUS", "NE", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.10": {"line.1": {"text": "\u00bbihr Herren,\u00ab sprach der Kaiser, \u00bbei, la\u00dft doch euren Schwank!", "tokens": ["\u00bb", "ihr", "Her\u00b7ren", ",", "\u00ab", "sprach", "der", "Kai\u00b7ser", ",", "\u00bb", "ei", ",", "la\u00dft", "doch", "eu\u00b7ren", "Schwank", "!"], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "$(", "VVFIN", "ART", "NN", "$,", "$(", "ITJ", "$,", "VVIMP", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Hat man denn je vernommen, da\u00df wer vom Beten krank?", "tokens": ["Hat", "man", "denn", "je", "ver\u00b7nom\u00b7men", ",", "da\u00df", "wer", "vom", "Be\u00b7ten", "krank", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADV", "ADV", "VVPP", "$,", "KOUS", "PWS", "APPRART", "NN", "ADJD", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Wer trabte je zu Rosse ins Gotteshaus hinein?", "tokens": ["W