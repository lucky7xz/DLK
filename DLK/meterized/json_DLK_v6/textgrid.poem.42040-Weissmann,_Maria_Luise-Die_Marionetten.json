{"textgrid.poem.42040": {"metadata": {"author": {"name": "Weissmann, Maria Luise", "birth": "N.A.", "death": "N.A."}, "title": "Die Marionetten", "genre": "verse", "period": "N.A.", "pub_year": 1914, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir lieben unsre schlanken Puppen sehr", "tokens": ["Wir", "lie\u00b7ben", "uns\u00b7re", "schlan\u00b7ken", "Pup\u00b7pen", "sehr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "\u2013 Ihre wei\u00dfen Gesichter sind einsam", "tokens": ["\u2013", "Ih\u00b7re", "wei\u00b7\u00dfen", "Ge\u00b7sich\u00b7ter", "sind", "ein\u00b7sam"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PPOSAT", "ADJA", "NN", "VAFIN", "ADJD"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "\u00dcber Leibern, fromm von Zerbrechlichkeit \u2013", "tokens": ["\u00dc\u00b7ber", "Lei\u00b7bern", ",", "fromm", "von", "Zer\u00b7brech\u00b7lich\u00b7keit", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADJD", "APPR", "NN", "$("], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und spielen gern die seltsamen Verkettungen", "tokens": ["Und", "spie\u00b7len", "gern", "die", "selt\u00b7sa\u00b7men", "Ver\u00b7ket\u00b7tun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-++-", "measure": "unknown.measure.hexa"}}, "stanza.2": {"line.1": {"text": "Ihrer Arme, wenn sie sich lieben.", "tokens": ["Ih\u00b7rer", "Ar\u00b7me", ",", "wenn", "sie", "sich", "lie\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KOUS", "PPER", "PRF", "VVINF", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Tragische Geb\u00e4rde ist ihnen zuteil,", "tokens": ["Tra\u00b7gi\u00b7sche", "Ge\u00b7b\u00e4r\u00b7de", "ist", "ih\u00b7nen", "zu\u00b7teil", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "PPER", "PTKVZ", "$,"], "meter": "+---+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Ihr dunkles Schicksal verst\u00f6\u00dft uns,", "tokens": ["Ihr", "dunk\u00b7les", "Schick\u00b7sal", "ver\u00b7st\u00f6\u00dft", "uns", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Sie sind unter fremden Sternen.", "tokens": ["Sie", "sind", "un\u00b7ter", "frem\u00b7den", "Ster\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Manchmal greift uns ihr Schweigen ans Herz.", "tokens": ["Manch\u00b7mal", "greift", "uns", "ihr", "Schwei\u00b7gen", "ans", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Dann springt \u2013 hei\u00dfe Verwunderung! \u2013", "tokens": ["Dann", "springt", "\u2013", "hei\u00b7\u00dfe", "Ver\u00b7wun\u00b7de\u00b7rung", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "$(", "ADJA", "NN", "$.", "$("], "meter": "--+--+--", "measure": "anapaest.di.plus"}, "line.3": {"text": "T\u00f6nender Schrei unsrer Inbrunst", "tokens": ["T\u00f6\u00b7nen\u00b7der", "Schrei", "uns\u00b7rer", "In\u00b7brunst"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "NN", "PPOSAT", "NN"], "meter": "+--+---+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Von ihrem entfalteten Mund.", "tokens": ["Von", "ih\u00b7rem", "ent\u00b7fal\u00b7te\u00b7ten", "Mund", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}