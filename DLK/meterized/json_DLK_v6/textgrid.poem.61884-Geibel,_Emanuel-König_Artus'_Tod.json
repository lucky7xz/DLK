{"textgrid.poem.61884": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "K\u00f6nig Artus' Tod", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Durch Wolken schien der Mond aufs Meer", "tokens": ["Durch", "Wol\u00b7ken", "schien", "der", "Mond", "aufs", "Meer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und auf den wei\u00dfen Strand am Sund;", "tokens": ["Und", "auf", "den", "wei\u00b7\u00dfen", "Strand", "am", "Sund", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Erschlagen lagen ringsumher", "tokens": ["Er\u00b7schla\u00b7gen", "la\u00b7gen", "rings\u00b7um\u00b7her"], "token_info": ["word", "word", "word"], "pos": ["NN", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Ritter von der Tafelrund'.", "tokens": ["Die", "Rit\u00b7ter", "von", "der", "Ta\u00b7fel\u00b7rund'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Am D\u00fcnenhange, wund zum Tod,", "tokens": ["Am", "D\u00fc\u00b7nen\u00b7han\u00b7ge", ",", "wund", "zum", "Tod", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Lag K\u00f6nig Artus, ihre Zier,", "tokens": ["Lag", "K\u00f6\u00b7nig", "Ar\u00b7tus", ",", "ih\u00b7re", "Zier", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NE", "NE", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und bei ihm kniet in seiner Not", "tokens": ["Und", "bei", "ihm", "kniet", "in", "sei\u00b7ner", "Not"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der tapfre Schenk, Herr Bedivere.", "tokens": ["Der", "tapf\u00b7re", "Schenk", ",", "Herr", "Be\u00b7di\u00b7ve\u00b7re", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Er sprach: \u00bbO Herr, wie seht Ihr bla\u00df!", "tokens": ["Er", "sprach", ":", "\u00bb", "O", "Herr", ",", "wie", "seht", "Ihr", "bla\u00df", "!"], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "NE", "NN", "$,", "PWAV", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gewi\u00df, die Wunde schmerzt Euch sehr.\u00ab", "tokens": ["Ge\u00b7wi\u00df", ",", "die", "Wun\u00b7de", "schmerzt", "Euch", "sehr", ".", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PTKANT", "$,", "ART", "NN", "VVFIN", "PPER", "ADV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der K\u00f6nig sprach: \u00bbEs ist nicht das,", "tokens": ["Der", "K\u00f6\u00b7nig", "sprach", ":", "\u00bb", "Es", "ist", "nicht", "das", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "PPER", "VAFIN", "PTKNEG", "PDS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Wunde brennt, doch andres mehr.", "tokens": ["Die", "Wun\u00b7de", "brennt", ",", "doch", "and\u00b7res", "mehr", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ADV", "PIS", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wie fragt' ich viel um meinen Leib,", "tokens": ["Wie", "fragt'", "ich", "viel", "um", "mei\u00b7nen", "Leib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn keinen Fleck mein Schild gewann!", "tokens": ["Wenn", "kei\u00b7nen", "Fleck", "mein", "Schild", "ge\u00b7wann", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch mich betrog das sch\u00f6nste Weib,", "tokens": ["Doch", "mich", "be\u00b7trog", "das", "sch\u00f6ns\u00b7te", "Weib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch mich verriet der beste Mann.", "tokens": ["Doch", "mich", "ver\u00b7riet", "der", "bes\u00b7te", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "O Ginover, o Lanzelot,", "tokens": ["O", "Gi\u00b7no\u00b7ver", ",", "o", "Lan\u00b7ze\u00b7lot", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich hegt' euch, wie die Brut der Schwan,", "tokens": ["Ich", "hegt'", "euch", ",", "wie", "die", "Brut", "der", "Schwan", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PWAV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "An meiner Brust. Verzeih' euch Gott,", "tokens": ["An", "mei\u00b7ner", "Brust", ".", "Ver\u00b7zeih'", "euch", "Gott", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$.", "NN", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was ihr an eurem Herrn getan!", "tokens": ["Was", "ihr", "an", "eu\u00b7rem", "Herrn", "ge\u00b7tan", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Weh, da ihr bracht die Treu' an mir,", "tokens": ["Weh", ",", "da", "ihr", "bracht", "die", "Treu'", "an", "mir", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "VVFIN", "ART", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erlosch das Sternbild unsres Ruhms,", "tokens": ["Er\u00b7losch", "das", "Stern\u00b7bild", "uns\u00b7res", "Ruhms", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Ehr' ist tot, und \u00fcber ihr", "tokens": ["Die", "Ehr'", "ist", "tot", ",", "und", "\u00fc\u00b7ber", "ihr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "KON", "APPR", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "St\u00fcrzt ein die Welt des Rittertums.", "tokens": ["St\u00fcrzt", "ein", "die", "Welt", "des", "Rit\u00b7ter\u00b7tums", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Wildwuchernd um den Tr\u00fcmmergraus", "tokens": ["Wild\u00b7wu\u00b7chernd", "um", "den", "Tr\u00fcm\u00b7mer\u00b7graus"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Schie\u00dft auf Gewalttat, Lug und List;", "tokens": ["Schie\u00dft", "auf", "Ge\u00b7walt\u00b7tat", ",", "Lug", "und", "List", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$,", "NE", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich f\u00fchl' es, meine Zeit ist aus,", "tokens": ["Ich", "f\u00fchl'", "es", ",", "mei\u00b7ne", "Zeit", "ist", "aus", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "VAFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und bettle nicht um Lebenfrist.", "tokens": ["Und", "bett\u00b7le", "nicht", "um", "Le\u00b7ben\u00b7frist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "So fahr denn wohl, du treuer Mann!", "tokens": ["So", "fahr", "denn", "wohl", ",", "du", "treu\u00b7er", "Mann", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ha! Siehst du dort das Schiff der Fei?", "tokens": ["Ha", "!", "Siehst", "du", "dort", "das", "Schiff", "der", "Fei", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "PPER", "ADV", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bekr\u00e4nzt mit Lilien schwebt's heran,", "tokens": ["Be\u00b7kr\u00e4nzt", "mit", "Li\u00b7li\u00b7en", "schwebt's", "he\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und Rosen gl\u00fchn, als w\u00e4r' es Mai.", "tokens": ["Und", "Ro\u00b7sen", "gl\u00fchn", ",", "als", "w\u00e4r'", "es", "Mai", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "$,", "KOKOM", "VAFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Im Winde klingt ein s\u00fc\u00dfes Wort", "tokens": ["Im", "Win\u00b7de", "klingt", "ein", "s\u00fc\u00b7\u00dfes", "Wort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und lullt mich ein wie Harfenton;", "tokens": ["Und", "lullt", "mich", "ein", "wie", "Har\u00b7fen\u00b7ton", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "KOKOM", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "An Bord! An Bord! Nun geht es fort", "tokens": ["An", "Bord", "!", "An", "Bord", "!", "Nun", "geht", "es", "fort"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$.", "APPR", "NN", "$.", "ADV", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ins stille Land, nach Avalon!\u00ab", "tokens": ["Ins", "stil\u00b7le", "Land", ",", "nach", "A\u00b7va\u00b7lon", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "APPR", "NE", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}