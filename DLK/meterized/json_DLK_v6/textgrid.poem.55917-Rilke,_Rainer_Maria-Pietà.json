{"textgrid.poem.55917": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Piet\u00e0", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So seh ich, Jesus, deine F\u00fc\u00dfe wieder,", "tokens": ["So", "seh", "ich", ",", "Je\u00b7sus", ",", "dei\u00b7ne", "F\u00fc\u00b7\u00dfe", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "NE", "$,", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "die damals eines J\u00fcnglings F\u00fc\u00dfe waren,", "tokens": ["die", "da\u00b7mals", "ei\u00b7nes", "J\u00fcng\u00b7lings", "F\u00fc\u00b7\u00dfe", "wa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "da ich sie bang entkleidete und wusch;", "tokens": ["da", "ich", "sie", "bang", "ent\u00b7klei\u00b7de\u00b7te", "und", "wusch", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "ADJA", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "wie standen sie verwirrt in meinen Haaren", "tokens": ["wie", "stan\u00b7den", "sie", "ver\u00b7wirrt", "in", "mei\u00b7nen", "Haa\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "und wie ein wei\u00dfes Wild im Dornenbusch.", "tokens": ["und", "wie", "ein", "wei\u00b7\u00dfes", "Wild", "im", "Dor\u00b7nen\u00b7busch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "So seh ich deine niegeliebten Glieder", "tokens": ["So", "seh", "ich", "dei\u00b7ne", "nie\u00b7ge\u00b7lieb\u00b7ten", "Glie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "zum erstenmal in dieser Liebesnacht.", "tokens": ["zum", "ers\u00b7ten\u00b7mal", "in", "die\u00b7ser", "Lie\u00b7bes\u00b7nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADV", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wir legten uns noch nie zusammen nieder,", "tokens": ["Wir", "leg\u00b7ten", "uns", "noch", "nie", "zu\u00b7sam\u00b7men", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "und nun wird nur bewundert und gewacht.", "tokens": ["und", "nun", "wird", "nur", "be\u00b7wun\u00b7dert", "und", "ge\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ADV", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch, siehe, deine H\u00e4nde sind zerrissen \u2013:", "tokens": ["Doch", ",", "sie\u00b7he", ",", "dei\u00b7ne", "H\u00e4n\u00b7de", "sind", "zer\u00b7ris\u00b7sen", "\u2013", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "$,", "VVIMP", "$,", "PPOSAT", "NN", "VAFIN", "VVPP", "$(", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Geliebter, nicht von mir, von meinen Bissen.", "tokens": ["Ge\u00b7lieb\u00b7ter", ",", "nicht", "von", "mir", ",", "von", "mei\u00b7nen", "Bis\u00b7sen", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PTKNEG", "APPR", "PPER", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dein Herz steht offen und man kann hinein:", "tokens": ["Dein", "Herz", "steht", "of\u00b7fen", "und", "man", "kann", "hin\u00b7ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "KON", "PIS", "VMFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "das h\u00e4tte d\u00fcrfen nur mein Eingang sein.", "tokens": ["das", "h\u00e4t\u00b7te", "d\u00fcr\u00b7fen", "nur", "mein", "Ein\u00b7gang", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "VMFIN", "ADV", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Nun bist du m\u00fcde, und dein m\u00fcder Mund", "tokens": ["Nun", "bist", "du", "m\u00fc\u00b7de", ",", "und", "dein", "m\u00fc\u00b7der", "Mund"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$,", "KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "hat keine Lust zu meinem wehen Munde \u2013.", "tokens": ["hat", "kei\u00b7ne", "Lust", "zu", "mei\u00b7nem", "we\u00b7hen", "Mun\u00b7de", "\u2013", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PIAT", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$(", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "O Jesus, Jesus, wann war unsre Stunde?", "tokens": ["O", "Je\u00b7sus", ",", "Je\u00b7sus", ",", "wann", "war", "uns\u00b7re", "Stun\u00b7de", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "NE", "$,", "PWAV", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wie gehn wir beide wunderlich zugrund.", "tokens": ["Wie", "gehn", "wir", "bei\u00b7de", "wun\u00b7der\u00b7lich", "zu\u00b7grund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PIS", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}