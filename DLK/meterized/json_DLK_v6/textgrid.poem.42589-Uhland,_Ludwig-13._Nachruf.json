{"textgrid.poem.42589": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "13. Nachruf", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Noch ist kein F\u00fcrst so hochgef\u00fcrstet,", "tokens": ["Noch", "ist", "kein", "F\u00fcrst", "so", "hoch\u00b7ge\u00b7f\u00fcrs\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So auserw\u00e4hlt kein ird'scher Mann,", "tokens": ["So", "au\u00b7ser\u00b7w\u00e4hlt", "kein", "ird'\u00b7scher", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df, wenn die Welt nach Freiheit d\u00fcrstet,", "tokens": ["Da\u00df", ",", "wenn", "die", "Welt", "nach", "Frei\u00b7heit", "d\u00fcrs\u00b7tet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUS", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Er sie mit Freiheit tr\u00e4nken kann,", "tokens": ["Er", "sie", "mit", "Frei\u00b7heit", "tr\u00e4n\u00b7ken", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "APPR", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df er allein in seinen H\u00e4nden", "tokens": ["Da\u00df", "er", "al\u00b7lein", "in", "sei\u00b7nen", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Den Reichtum alles Rechtes h\u00e4lt,", "tokens": ["Den", "Reich\u00b7tum", "al\u00b7les", "Rech\u00b7tes", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Um an die V\u00f6lker auszuspenden", "tokens": ["Um", "an", "die", "V\u00f6l\u00b7ker", "aus\u00b7zu\u00b7spen\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "So viel, so wenig ihm gef\u00e4llt.", "tokens": ["So", "viel", ",", "so", "we\u00b7nig", "ihm", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "ADV", "PIS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Das ", "tokens": ["Das"], "token_info": ["word"], "pos": ["PDS"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Es liegt in jedem Erdensohne,", "tokens": ["Es", "liegt", "in", "je\u00b7dem", "Er\u00b7den\u00b7soh\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Es quillt in uns wie Herzensblut;", "tokens": ["Es", "quillt", "in", "uns", "wie", "Her\u00b7zens\u00b7blut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "KOKOM", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und wann sich M\u00e4nner frei erheben", "tokens": ["Und", "wann", "sich", "M\u00e4n\u00b7ner", "frei", "er\u00b7he\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PRF", "NN", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und treulich schlagen Hand in Hand,", "tokens": ["Und", "treu\u00b7lich", "schla\u00b7gen", "Hand", "in", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dann tritt das innre Recht ins Leben", "tokens": ["Dann", "tritt", "das", "inn\u00b7re", "Recht", "ins", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und der ", "tokens": ["Und", "der"], "token_info": ["word", "word"], "pos": ["KON", "ART"], "meter": "+-", "measure": "trochaic.single"}}, "stanza.3": {"line.1": {"text": "Vertrag! es ging auch hierzulande", "tokens": ["Ver\u00b7trag", "!", "es", "ging", "auch", "hier\u00b7zu\u00b7lan\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "PPER", "VVFIN", "ADV", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von ihm der Rechte Satzung aus,", "tokens": ["Von", "ihm", "der", "Rech\u00b7te", "Sat\u00b7zung", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es kn\u00fcpfen seine heil'gen Bande", "tokens": ["Es", "kn\u00fcp\u00b7fen", "sei\u00b7ne", "heil'\u00b7gen", "Ban\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den Volksstamm an das F\u00fcrstenhaus.", "tokens": ["Den", "Volks\u00b7stamm", "an", "das", "F\u00fcrs\u00b7ten\u00b7haus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ob einer im Palast geboren,", "tokens": ["Ob", "ei\u00b7ner", "im", "Pa\u00b7last", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "In F\u00fcrstenwiege sei gewiegt,", "tokens": ["In", "F\u00fcrs\u00b7ten\u00b7wie\u00b7ge", "sei", "ge\u00b7wiegt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Als Herrscher wird ihm erst geschworen,", "tokens": ["Als", "Herr\u00b7scher", "wird", "ihm", "erst", "ge\u00b7schwo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wenn der Vertrag besiegelt liegt.", "tokens": ["Wenn", "der", "Ver\u00b7trag", "be\u00b7sie\u00b7gelt", "liegt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Solch teure Wahrheit ward verfochten,", "tokens": ["Solch", "teu\u00b7re", "Wahr\u00b7heit", "ward", "ver\u00b7foch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und \u00fcberwunden ist sie nicht.", "tokens": ["Und", "\u00fc\u00b7berw\u00b7un\u00b7den", "ist", "sie", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "VAFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Euch, K\u00e4mpfer, ist kein Kranz geflochten,", "tokens": ["Euch", ",", "K\u00e4mp\u00b7fer", ",", "ist", "kein", "Kranz", "ge\u00b7floch\u00b7ten", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "VAFIN", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie der begl\u00fcckte Sieg ihn flicht;", "tokens": ["Wie", "der", "be\u00b7gl\u00fcck\u00b7te", "Sieg", "ihn", "flicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nein! wie ein F\u00e4hnrich, wund und blutig,", "tokens": ["Nein", "!", "wie", "ein", "F\u00e4hn\u00b7rich", ",", "wund", "und", "blu\u00b7tig", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PWAV", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sein Banner rettet im Gefecht,", "tokens": ["Sein", "Ban\u00b7ner", "ret\u00b7tet", "im", "Ge\u00b7fecht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So blickt ihr, tief gekr\u00e4nkt, doch mutig", "tokens": ["So", "blickt", "ihr", ",", "tief", "ge\u00b7kr\u00e4nkt", ",", "doch", "mu\u00b7tig"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADJD", "VVPP", "$,", "ADV", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und stolz auf das gewahrte Recht.", "tokens": ["Und", "stolz", "auf", "das", "ge\u00b7wahr\u00b7te", "Recht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Kein Herold wird's den V\u00f6lkern k\u00fcnden", "tokens": ["Kein", "He\u00b7rold", "wird's", "den", "V\u00f6l\u00b7kern", "k\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VAFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Pauken- und Trommetenschall,", "tokens": ["Mit", "Pau\u00b7ken", "und", "Trom\u00b7me\u00b7ten\u00b7schall", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "TRUNC", "KON", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Und dennoch wird es Wurzel gr\u00fcnden", "tokens": ["Und", "den\u00b7noch", "wird", "es", "Wur\u00b7zel", "gr\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PPER", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In deutschen Gauen \u00fcberall:", "tokens": ["In", "deut\u00b7schen", "Gau\u00b7en", "\u00fc\u00b7be\u00b7rall", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df Weisheit nicht das Recht begraben", "tokens": ["Da\u00df", "Weis\u00b7heit", "nicht", "das", "Recht", "be\u00b7gra\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "PTKNEG", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Noch Wohlfahrt es ersetzen mag,", "tokens": ["Noch", "Wohl\u00b7fahrt", "es", "er\u00b7set\u00b7zen", "mag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da\u00df bei dem biedern Volk in Schwaben", "tokens": ["Da\u00df", "bei", "dem", "bie\u00b7dern", "Volk", "in", "Schwa\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Das ", "tokens": ["Das"], "token_info": ["word"], "pos": ["PDS"], "meter": "-", "measure": "single.down"}}}}}