{"textgrid.poem.25084": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "8.", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Nacht ist schaurig und finster,", "tokens": ["Die", "Nacht", "ist", "schau\u00b7rig", "und", "fins\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der Friedhof mit wei\u00dfen Flocken bestreut;", "tokens": ["Der", "Fried\u00b7hof", "mit", "wei\u00b7\u00dfen", "Flo\u00b7cken", "be\u00b7streut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Hernieder vom alten M\u00fcnster", "tokens": ["Her\u00b7nie\u00b7der", "vom", "al\u00b7ten", "M\u00fcns\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Im Winde wallt der Glocken Gel\u00e4ut.", "tokens": ["Im", "Win\u00b7de", "wallt", "der", "Glo\u00b7cken", "Ge\u00b7l\u00e4ut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "Sie alle, die oft mir erklungen,", "tokens": ["Sie", "al\u00b7le", ",", "die", "oft", "mir", "er\u00b7klun\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "$,", "PRELS", "ADV", "PPER", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Wie t\u00f6nen mir ihre Stimmen vertraut!", "tokens": ["Wie", "t\u00f6\u00b7nen", "mir", "ih\u00b7re", "Stim\u00b7men", "ver\u00b7traut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}}, "stanza.3": {"line.1": {"text": "Und unter den steinernen Platten", "tokens": ["Und", "un\u00b7ter", "den", "stei\u00b7ner\u00b7nen", "Plat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Quillt es hervor wie Leichenduft;", "tokens": ["Quillt", "es", "her\u00b7vor", "wie", "Lei\u00b7chen\u00b7duft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "KOKOM", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Geschwundener Stunden Schatten", "tokens": ["Ge\u00b7schwun\u00b7de\u00b7ner", "Stun\u00b7den", "Schat\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Entschweben bei den Kl\u00e4ngen der Gruft.", "tokens": ["Ent\u00b7schwe\u00b7ben", "bei", "den", "Kl\u00e4n\u00b7gen", "der", "Gruft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.4": {"line.1": {"text": "Err\u00f6tend, dann neu sich entf\u00e4rbend,", "tokens": ["Er\u00b7r\u00f6\u00b7tend", ",", "dann", "neu", "sich", "ent\u00b7f\u00e4r\u00b7bend", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADV", "ADJD", "PRF", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Von sel'ger Zeit mir fl\u00fcstern sie;", "tokens": ["Von", "sel'\u00b7ger", "Zeit", "mir", "fl\u00fcs\u00b7tern", "sie", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Um ihre Lippen hallt sterbend", "tokens": ["Um", "ih\u00b7re", "Lip\u00b7pen", "hallt", "ster\u00b7bend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "PPOSAT", "NN", "VVFIN", "ADJD"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Verschollener Lieder Melodie.", "tokens": ["Ver\u00b7schol\u00b7le\u00b7ner", "Lie\u00b7der", "Me\u00b7lo\u00b7die", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NE", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Von wei\u00dfen Rosen umwunden", "tokens": ["Von", "wei\u00b7\u00dfen", "Ro\u00b7sen", "um\u00b7wun\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sind ihre Stirnen; sie rei\u00dfen sie ab", "tokens": ["Sind", "ih\u00b7re", "Stir\u00b7nen", ";", "sie", "rei\u00b7\u00dfen", "sie", "ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und zeigen mir blutende Wunden \u2013", "tokens": ["Und", "zei\u00b7gen", "mir", "blu\u00b7ten\u00b7de", "Wun\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJA", "NN", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Ich sinke bebend hin auf ein Grab.", "tokens": ["Ich", "sin\u00b7ke", "be\u00b7bend", "hin", "auf", "ein", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Hernieder durch st\u00e4ubende Flocken", "tokens": ["Her\u00b7nie\u00b7der", "durch", "st\u00e4u\u00b7ben\u00b7de", "Flo\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Bricht matt des Mondes Glanz,", "tokens": ["Bricht", "matt", "des", "Mon\u00b7des", "Glanz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und fort beim Schalle der Glocken", "tokens": ["Und", "fort", "beim", "Schal\u00b7le", "der", "Glo\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PTKVZ", "APPRART", "NN", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wallt mir zu H\u00e4upten der Geistertanz.", "tokens": ["Wallt", "mir", "zu", "H\u00e4up\u00b7ten", "der", "Geis\u00b7ter\u00b7tanz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "ART", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}}}}