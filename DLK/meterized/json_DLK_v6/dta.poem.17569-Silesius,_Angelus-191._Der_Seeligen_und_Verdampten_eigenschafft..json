{"dta.poem.17569": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "191.  Der Seeligen und Verdampten  \n eigenschafft.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Der Seelgen Eigenschafft ist gantz nach GOtte leben?", "tokens": ["Der", "Seel\u00b7gen", "Ei\u00b7gen\u00b7schafft", "ist", "gantz", "nach", "Got\u00b7te", "le\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und der Verdampten art Jhm g\u00e4ntzlich wiederstreben.", "tokens": ["Und", "der", "Ver\u00b7damp\u00b7ten", "art", "Jhm", "g\u00e4ntz\u00b7lich", "wie\u00b7der\u00b7stre\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}