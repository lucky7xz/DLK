{"dta.poem.6140": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Schwere und der Leichte,  \n von der Fluht und Ebbe  \n des Meeres.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ist gleich die Welt gantz voll; wird durch die F\u00fclle doch,", "tokens": ["Ist", "gleich", "die", "Welt", "gantz", "voll", ";", "wird", "durch", "die", "F\u00fcl\u00b7le", "doch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ADV", "ADJD", "$.", "VAFIN", "APPR", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Statt, da\u00df sie dem Bewegen", "tokens": ["Statt", ",", "da\u00df", "sie", "dem", "Be\u00b7we\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "PPER", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Der C\u00f6rper w\u00e4r\u2019 entgegen,", "tokens": ["Der", "C\u00f6r\u00b7per", "w\u00e4r'", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von der best\u00e4ndigen Ver\u00e4nderung annoch", "tokens": ["Von", "der", "be\u00b7st\u00e4n\u00b7di\u00b7gen", "Ver\u00b7\u00e4n\u00b7de\u00b7rung", "an\u00b7noch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Harmonie gemehrt. Dadurch nun m\u00fcssem", "tokens": ["Die", "Har\u00b7mo\u00b7nie", "ge\u00b7mehrt", ".", "Da\u00b7durch", "nun", "m\u00fcs\u00b7sem"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "$.", "PAV", "ADV", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Von allen C\u00f6rpern, die sich r\u00fchren,", "tokens": ["Von", "al\u00b7len", "C\u00f6r\u00b7pern", ",", "die", "sich", "r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "PRELS", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die nahgelegenen denselben Druck versp\u00fcren.", "tokens": ["Die", "nah\u00b7ge\u00b7le\u00b7ge\u00b7nen", "den\u00b7sel\u00b7ben", "Druck", "ver\u00b7sp\u00fc\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PDAT", "NN", "VVINF", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Ein C\u00f6rper presst den Druck dem andern ein.", "tokens": ["Ein", "C\u00f6r\u00b7per", "presst", "den", "Druck", "dem", "an\u00b7dern", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ART", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Ein eintzigs Feder-Werck kan sie zusammen schliessen.", "tokens": ["Ein", "eint\u00b7zigs", "Fe\u00b7der\u00b7\u00b7Werck", "kan", "sie", "zu\u00b7sam\u00b7men", "schlies\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Jm nah\u2019 und fernem ist die Wirckung allgemein,", "tokens": ["Jm", "nah'", "und", "fer\u00b7nem", "ist", "die", "Wir\u00b7ckung", "all\u00b7ge\u00b7mein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "ADJA", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Blo\u00df durch ein eintzig Rad, das sich best\u00e4ndig drehet.", "tokens": ["Blo\u00df", "durch", "ein", "eint\u00b7zig", "Rad", ",", "das", "sich", "be\u00b7st\u00e4n\u00b7dig", "dre\u00b7het", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJD", "NN", "$,", "PRELS", "PRF", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ein C\u00f6rper dr\u00fccket stets, und zieht hingegen", "tokens": ["Ein", "C\u00f6r\u00b7per", "dr\u00fc\u00b7cket", "stets", ",", "und", "zieht", "hin\u00b7ge\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,", "KON", "VVFIN", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Nie etwas nach sich her.", "tokens": ["Nie", "et\u00b7was", "nach", "sich", "her", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PRF", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "In dieser grossen Uhr stimmt alles \u00fcberein,", "tokens": ["In", "die\u00b7ser", "gros\u00b7sen", "Uhr", "stimmt", "al\u00b7les", "\u00fc\u00b7be\u00b7re\u00b7in", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "VVFIN", "PIS", "PTKVZ", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.15": {"text": "Mit dem gleichf\u00f6rmigen Bewegen,", "tokens": ["Mit", "dem", "gleich\u00b7f\u00f6r\u00b7mi\u00b7gen", "Be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.16": {"text": "Wodurch die gantze Welt", "tokens": ["Wo\u00b7durch", "die", "gant\u00b7ze", "Welt"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "Nach ihren Regeln geht, sich in der Ordnung h\u00e4lt.", "tokens": ["Nach", "ih\u00b7ren", "Re\u00b7geln", "geht", ",", "sich", "in", "der", "Ord\u00b7nung", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,", "PRF", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}