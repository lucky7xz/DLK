{"textgrid.poem.24816": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Noch einen mir, der Kraft mir leihe!", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Noch einen mir, der Kraft mir leihe!", "tokens": ["Noch", "ei\u00b7nen", "mir", ",", "der", "Kraft", "mir", "lei\u00b7he", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "PPER", "$,", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gieb, Weib, bevor ich scheiden mu\u00df,", "tokens": ["Gieb", ",", "Weib", ",", "be\u00b7vor", "ich", "schei\u00b7den", "mu\u00df", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "NN", "$,", "KOUS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "F\u00fcr Leben mir und Tod die Weihe", "tokens": ["F\u00fcr", "Le\u00b7ben", "mir", "und", "Tod", "die", "Wei\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "PPER", "KON", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In einem langen, heil'gen Ku\u00df!", "tokens": ["In", "ei\u00b7nem", "lan\u00b7gen", ",", "heil'\u00b7gen", "Ku\u00df", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "La\u00df brennend ihn von deinem Munde", "tokens": ["La\u00df", "bren\u00b7nend", "ihn", "von", "dei\u00b7nem", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "VVPP", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mir bis ins Herz des Herzens gl\u00fchn,", "tokens": ["Mir", "bis", "ins", "Herz", "des", "Her\u00b7zens", "gl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPRART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und duftend gl\u00e4nze diese Stunde", "tokens": ["Und", "duf\u00b7tend", "gl\u00e4n\u00b7ze", "die\u00b7se", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gleich Rosen, die auf Gr\u00e4bern bl\u00fchn!", "tokens": ["Gleich", "Ro\u00b7sen", ",", "die", "auf", "Gr\u00e4\u00b7bern", "bl\u00fchn", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "PRELS", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Um unsre selig-s\u00fc\u00dfen Schmerzen", "tokens": ["Um", "uns\u00b7re", "se\u00b7lig\u00b7s\u00fc\u00b7\u00dfen", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Soll sie, und um des Abschieds Qual,", "tokens": ["Soll", "sie", ",", "und", "um", "des", "Ab\u00b7schieds", "Qual", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "KON", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Aufflammen halb wie Hochzeitskerzen", "tokens": ["Auf\u00b7flam\u00b7men", "halb", "wie", "Hoch\u00b7zeits\u00b7ker\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADJD", "KOKOM", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und halb wie Leichenfackelstrahl;", "tokens": ["Und", "halb", "wie", "Lei\u00b7chen\u00b7fa\u00b7ckel\u00b7strahl", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KOKOM", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und fern noch in der Trennung Wehe", "tokens": ["Und", "fern", "noch", "in", "der", "Tren\u00b7nung", "We\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADV", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mir leuchte sie, wenn ich verirrt", "tokens": ["Mir", "leuch\u00b7te", "sie", ",", "wenn", "ich", "ver\u00b7irrt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Am Rand des j\u00e4hen Abgrunds stehe", "tokens": ["Am", "Rand", "des", "j\u00e4\u00b7hen", "Ab\u00b7grunds", "ste\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und alles um mich finster wird.", "tokens": ["Und", "al\u00b7les", "um", "mich", "fins\u00b7ter", "wird", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "APPR", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}