{"textgrid.poem.49547": {"metadata": {"author": {"name": "G\u00f6tz, Nicolaus", "birth": "N.A.", "death": "N.A."}, "title": "Nach dem Rousseau", "genre": "verse", "period": "N.A.", "pub_year": 1751, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es setzt' in ihren alten Tagen", "tokens": ["Es", "setzt'", "in", "ih\u00b7ren", "al\u00b7ten", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Cythere dich und mich zu gleichen Erben ein", "tokens": ["Cy\u00b7the\u00b7re", "dich", "und", "mich", "zu", "glei\u00b7chen", "Er\u00b7ben", "ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "KON", "PPER", "APPR", "ADJA", "NN", "ART"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Und gieng in ein Convent hinein.", "tokens": ["Und", "gieng", "in", "ein", "Con\u00b7vent", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Amor aber ward die Theilung aufgetragen;", "tokens": ["Dem", "A\u00b7mor", "a\u00b7ber", "ward", "die", "Thei\u00b7lung", "auf\u00b7ge\u00b7tra\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "ADV", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Doch er besorgte sie nicht fein:", "tokens": ["Doch", "er", "be\u00b7sorg\u00b7te", "sie", "nicht", "fein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Denn durch dein Augenpaar besiegt, das immer sieget,", "tokens": ["Denn", "durch", "dein", "Au\u00b7gen\u00b7paar", "be\u00b7siegt", ",", "das", "im\u00b7mer", "sie\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVPP", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Beschied er dir allein,", "tokens": ["Be\u00b7schied", "er", "dir", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Was in der Gratien ber\u00fchmten G\u00fcrtel lieget,", "tokens": ["Was", "in", "der", "Gra\u00b7ti\u00b7en", "be\u00b7r\u00fchm\u00b7ten", "G\u00fcr\u00b7tel", "lie\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Reizt, \u00fcberredet und vergn\u00fcget;", "tokens": ["Reizt", ",", "\u00fc\u00b7berr\u00b7e\u00b7det", "und", "ver\u00b7gn\u00fc\u00b7get", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVPP", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Und mir allein", "tokens": ["Und", "mir", "al\u00b7lein"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPER", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Die Thr\u00e4nen und die Pein.", "tokens": ["Die", "Thr\u00e4\u00b7nen", "und", "die", "Pein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}