{"dta.poem.1137": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "23. Die Sch\u00f6ne Weinende.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Du l\u00e4st/ mein werthes Kind/ die heissen Z\u00e4hren schiessen: ", "tokens": ["Du", "l\u00e4st", "/", "mein", "wert\u00b7hes", "Kind", "/", "die", "heis\u00b7sen", "Z\u00e4h\u00b7ren", "schies\u00b7sen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PPOSAT", "ADJA", "NN", "$(", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mein treues Auge sch\u00e4zt dein Weinen lauter Wein.", "tokens": ["Mein", "treu\u00b7es", "Au\u00b7ge", "sch\u00e4zt", "dein", "Wei\u00b7nen", "lau\u00b7ter", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich w\u00fcnschte/ da\u00df du m\u00f6chtst ein andrer Argus seyn/", "tokens": ["Ich", "w\u00fcnschte", "/", "da\u00df", "du", "m\u00f6chtst", "ein", "an\u00b7drer", "Ar\u00b7gus", "seyn", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "PPER", "VMFIN", "ART", "ADJA", "NN", "VAINF", "$("], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und solche Thr\u00e4nen-Bach aus hundert Augen giessen.", "tokens": ["Und", "sol\u00b7che", "Thr\u00e4\u00b7nen\u00b7Bach", "aus", "hun\u00b7dert", "Au\u00b7gen", "gies\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "APPR", "CARD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Du edles Seelen-Blutt k\u00f6ntstu in Str\u00f6me fl\u00fcssen/", "tokens": ["Du", "ed\u00b7les", "See\u00b7len\u00b7Blutt", "k\u00f6nts\u00b7tu", "in", "Str\u00f6\u00b7me", "fl\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "VMFIN", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wie wolt ich so mit Lust mich in dir tauchen ein.", "tokens": ["Wie", "wolt", "ich", "so", "mit", "Lust", "mich", "in", "dir", "tau\u00b7chen", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "APPR", "NN", "PRF", "APPR", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die Perlen Indiens sind nicht wie du/ so rein.", "tokens": ["Die", "Per\u00b7len", "In\u00b7di\u00b7ens", "sind", "nicht", "wie", "du", "/", "so", "rein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VAFIN", "PTKNEG", "PWAV", "PPER", "$(", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Jedweder Tropffen ist statt Spiegels zu gen\u00fcssen.", "tokens": ["Jed\u00b7we\u00b7der", "Tropf\u00b7fen", "ist", "statt", "Spie\u00b7gels", "zu", "ge\u00b7n\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "O Brunnen/ zugericht ein Blumen-Feld zu netzen/", "tokens": ["O", "Brun\u00b7nen", "/", "zu\u00b7ge\u00b7richt", "ein", "Blu\u00b7men\u00b7Feld", "zu", "net\u00b7zen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$(", "VVPP", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Man mu\u00df eur heilsam Saltz vor kluge Wei\u00dfheit sch\u00e4tzen;", "tokens": ["Man", "mu\u00df", "eur", "heil\u00b7sam", "Saltz", "vor", "klu\u00b7ge", "Wei\u00df\u00b7heit", "sch\u00e4t\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPOSAT", "ADJD", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ihr wascht/ was eurer Zier den mindsten Flecken gab.", "tokens": ["Ihr", "wascht", "/", "was", "eu\u00b7rer", "Zier", "den", "minds\u00b7ten", "Fle\u00b7cken", "gab", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PWS", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "So wird der Liebe Brand gel\u00f6scht und ausgeschwizt:", "tokens": ["So", "wird", "der", "Lie\u00b7be", "Brand", "ge\u00b7l\u00f6scht", "und", "aus\u00b7ge\u00b7schwizt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "NN", "VVPP", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "So k\u00fchlt/ was Ph\u00f6bus Glutt den langen Tag erhizt/", "tokens": ["So", "k\u00fchlt", "/", "was", "Ph\u00f6\u00b7bus", "Glutt", "den", "lan\u00b7gen", "Tag", "er\u00b7hizt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$(", "PWS", "NE", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die sch\u00f6ne Morgen-R\u00f6th in linden Thr\u00e4nen ab.", "tokens": ["Die", "sch\u00f6\u00b7ne", "Mor\u00b7gen\u00b7R\u00f6\u00b7th", "in", "lin\u00b7den", "Thr\u00e4\u00b7nen", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}}}}}