{"textgrid.poem.34862": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "9.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Blumen erreicht der Fu\u00df so leicht,", "tokens": ["Die", "Blu\u00b7men", "er\u00b7reicht", "der", "Fu\u00df", "so", "leicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Auch werden zertreten die meisten;", "tokens": ["Auch", "wer\u00b7den", "zer\u00b7tre\u00b7ten", "die", "meis\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAINF", "VVFIN", "ART", "VVFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Man geht vorbei und tritt entzwei", "tokens": ["Man", "geht", "vor\u00b7bei", "und", "tritt", "ent\u00b7zwei"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PTKVZ", "KON", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die bl\u00f6den wie die dreisten.", "tokens": ["Die", "bl\u00f6\u00b7den", "wie", "die", "dreis\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KOKOM", "ART", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Perlen ruhn in Meerestruhn,", "tokens": ["Die", "Per\u00b7len", "ruhn", "in", "Mee\u00b7rest\u00b7ruhn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch wei\u00df man sie aufzusp\u00fcren;", "tokens": ["Doch", "wei\u00df", "man", "sie", "auf\u00b7zu\u00b7sp\u00fc\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "PPER", "VVIZU", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Man bohrt ein Loch und spannt sie ins Joch,", "tokens": ["Man", "bohrt", "ein", "Loch", "und", "spannt", "sie", "ins", "Joch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "KON", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Ins Joch von seidenen Schn\u00fcren.", "tokens": ["Ins", "Joch", "von", "sei\u00b7de\u00b7nen", "Schn\u00fc\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Die Sterne sind klug, sie halten mit Fug", "tokens": ["Die", "Ster\u00b7ne", "sind", "klug", ",", "sie", "hal\u00b7ten", "mit", "Fug"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "PPER", "VVFIN", "APPR", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Von unserer Erde sich ferne;", "tokens": ["Von", "un\u00b7se\u00b7rer", "Er\u00b7de", "sich", "fer\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PRF", "ADV", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Am Himmelszelt, als Lichter der Welt,", "tokens": ["Am", "Him\u00b7mels\u00b7zelt", ",", "als", "Lich\u00b7ter", "der", "Welt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KOUS", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Stehn ewig sicher die Sterne.", "tokens": ["Stehn", "e\u00b7wig", "si\u00b7cher", "die", "Ster\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADJD", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}