{"textgrid.poem.44163": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du meintest nechster Zeit, getreu- und edler Freund,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du meintest nechster Zeit, getreu- und edler Freund,", "tokens": ["Du", "mein\u00b7test", "nechs\u00b7ter", "Zeit", ",", "ge\u00b7treu", "und", "ed\u00b7ler", "Freund", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$,", "TRUNC", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich scherzte gar zu viel mit meinen Castalinnen", "tokens": ["Ich", "scherz\u00b7te", "gar", "zu", "viel", "mit", "mei\u00b7nen", "Cas\u00b7ta\u00b7lin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PTKA", "PIS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und sollte, da ohndem mein Gl\u00fccksstern dunckel scheint,", "tokens": ["Und", "soll\u00b7te", ",", "da", "ohn\u00b7dem", "mein", "Gl\u00fccks\u00b7stern", "dun\u00b7ckel", "scheint", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "$,", "KOUS", "PAV", "PPOSAT", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+---+-+-+-+", "measure": "dactylic.init"}, "line.4": {"text": "Nunmehr auch mit Bedacht auf Ernst und Nachdruck sinnen.", "tokens": ["Nun\u00b7mehr", "auch", "mit", "Be\u00b7dacht", "auf", "Ernst", "und", "Nach\u00b7druck", "sin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "APPR", "NE", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dies war ein guter Rath, den Lieb und Freundschaft gab.", "tokens": ["Dies", "war", "ein", "gu\u00b7ter", "Rath", ",", "den", "Lieb", "und", "Freund\u00b7schaft", "gab", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,", "ART", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wohlan, ich leg an dir die ersten Proben ab", "tokens": ["Wo\u00b7hlan", ",", "ich", "leg", "an", "dir", "die", "ers\u00b7ten", "Pro\u00b7ben", "ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PPER", "NE", "APPR", "PPER", "ART", "ADJA", "NN", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und gebe dir hiermit durch Ku\u00df und Abschiedslieder", "tokens": ["Und", "ge\u00b7be", "dir", "hier\u00b7mit", "durch", "Ku\u00df", "und", "Ab\u00b7schieds\u00b7lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PAV", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Das Zeugn\u00fc\u00df und den Ruhm getreu- und edler Br\u00fcder.", "tokens": ["Das", "Zeug\u00b7n\u00fc\u00df", "und", "den", "Ruhm", "ge\u00b7treu", "und", "ed\u00b7ler", "Br\u00fc\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "TRUNC", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Warheit, so jezt kommt, entspringt aus Redligkeit,", "tokens": ["Die", "War\u00b7heit", ",", "so", "jezt", "kommt", ",", "ent\u00b7springt", "aus", "Red\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ADV", "VVFIN", "$,", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Neid mag noch so sehr das hohe Gleichn\u00fc\u00df schelten:", "tokens": ["Der", "Neid", "mag", "noch", "so", "sehr", "das", "ho\u00b7he", "Gleich\u00b7n\u00fc\u00df", "schel\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "ADV", "ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Augustus gab vor dem den Dichtern g\u00fcldne Zeit", "tokens": ["Au\u00b7gus\u00b7tus", "gab", "vor", "dem", "den", "Dich\u00b7tern", "g\u00fcld\u00b7ne", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "ART", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und lies ihr Saythenspiel gewis nicht wenig gelten.", "tokens": ["Und", "lies", "ihr", "Say\u00b7then\u00b7spiel", "ge\u00b7wis", "nicht", "we\u00b7nig", "gel\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "ADV", "PTKNEG", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Das Dancklied, so Horaz vor solche Gnade sang,", "tokens": ["Das", "Danck\u00b7lied", ",", "so", "Ho\u00b7raz", "vor", "sol\u00b7che", "Gna\u00b7de", "sang", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "NE", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Das wiederholt auch jezt mein schlechter Fl\u00f6thenklang;", "tokens": ["Das", "wie\u00b7der\u00b7holt", "auch", "jezt", "mein", "schlech\u00b7ter", "Fl\u00f6\u00b7then\u00b7klang", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Denn prahlt gleich Beuchelts Haupt mit keiner Kaysercrone,", "tokens": ["Denn", "prahlt", "gleich", "Beu\u00b7chelts", "Haupt", "mit", "kei\u00b7ner", "Kay\u00b7ser\u00b7cro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "NE", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So fodert doch sein Herz nicht minder Lob zum Lohne,", "tokens": ["So", "fo\u00b7dert", "doch", "sein", "Herz", "nicht", "min\u00b7der", "Lob", "zum", "Loh\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT", "NN", "PTKNEG", "ADV", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Sein Herz, sein ehrlich Herz, das mich ganz fremden Gast", "tokens": ["Sein", "Herz", ",", "sein", "ehr\u00b7lich", "Herz", ",", "das", "mich", "ganz", "frem\u00b7den", "Gast"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "ADJD", "NN", "$,", "PRELS", "PPER", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit gutem Rath und Schuz so angenehm empfangen,", "tokens": ["Mit", "gu\u00b7tem", "Rath", "und", "Schuz", "so", "an\u00b7ge\u00b7nehm", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Musen auf der Flucht mit H\u00fclf und Trost umfast", "tokens": ["Die", "Mu\u00b7sen", "auf", "der", "Flucht", "mit", "H\u00fclf", "und", "Trost", "um\u00b7fast"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "APPR", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und ihnen \u00fcberall mit Liebe nachgegangen.", "tokens": ["Und", "ih\u00b7nen", "\u00fc\u00b7be\u00b7rall", "mit", "Lie\u00b7be", "nach\u00b7ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und wie M\u00e4cenas dort der Wi\u00dfenschaften Preis", "tokens": ["Und", "wie", "M\u00e4\u00b7ce\u00b7nas", "dort", "der", "Wi\u00b7\u00dfen\u00b7schaf\u00b7ten", "Preis"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "NE", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "In seines Kaysers Brust klug zu erhalten weis,", "tokens": ["In", "sei\u00b7nes", "Kay\u00b7sers", "Brust", "klug", "zu", "er\u00b7hal\u00b7ten", "weis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "ADJD", "PTKZU", "VVINF", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So bistu, edler Freund, vor meinen Musenorden", "tokens": ["So", "bis\u00b7tu", ",", "ed\u00b7ler", "Freund", ",", "vor", "mei\u00b7nen", "Mu\u00b7se\u00b7nor\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "ADJA", "NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Bey Beuchelts Herz und Haus auch oft M\u00e4cenas worden.", "tokens": ["Bey", "Beu\u00b7chelts", "Herz", "und", "Haus", "auch", "oft", "M\u00e4\u00b7ce\u00b7nas", "wor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "KON", "NN", "ADV", "ADV", "NN", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Verbleib es auch forthin und liebe meine Kunst;", "tokens": ["Ver\u00b7bleib", "es", "auch", "for\u00b7thin", "und", "lie\u00b7be", "mei\u00b7ne", "Kunst", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "PTKVZ", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie ist ein armes Kind und mu\u00df sich furchtsam schmiegen.", "tokens": ["Sie", "ist", "ein", "ar\u00b7mes", "Kind", "und", "mu\u00df", "sich", "furcht\u00b7sam", "schmie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "KON", "VMFIN", "PRF", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Verdien ich in der Zeit von Gott noch eine Gunst,", "tokens": ["Ver\u00b7di\u00b7en", "ich", "in", "der", "Zeit", "von", "Gott", "noch", "ei\u00b7ne", "Gunst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "APPR", "NN", "ADV", "ART", "NN", "$,"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "So wird es sein Entschlu\u00df vor mich so gl\u00fccklich f\u00fcgen,", "tokens": ["So", "wird", "es", "sein", "Ent\u00b7schlu\u00df", "vor", "mich", "so", "gl\u00fcck\u00b7lich", "f\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "APPR", "PPER", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Da\u00df Beuchelt, du, die Welt und selbst die Warheit seh,", "tokens": ["Da\u00df", "Beu\u00b7chelt", ",", "du", ",", "die", "Welt", "und", "selbst", "die", "War\u00b7heit", "seh", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "PPER", "$,", "ART", "NN", "KON", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wie weh ins dritte Jahr mir durch mich selbst gescheh", "tokens": ["Wie", "weh", "ins", "drit\u00b7te", "Jahr", "mir", "durch", "mich", "selbst", "ge\u00b7scheh"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "APPRART", "ADJA", "NN", "PPER", "APPR", "PPER", "ADV", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und wie so wenig ich das, was ich andern g\u00f6nne,", "tokens": ["Und", "wie", "so", "we\u00b7nig", "ich", "das", ",", "was", "ich", "an\u00b7dern", "g\u00f6n\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADV", "PIS", "PPER", "PDS", "$,", "PWS", "PPER", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Mit allem Wuntsch und Flehn vom Gl\u00fcck erhalten k\u00f6nne.", "tokens": ["Mit", "al\u00b7lem", "Wunt\u00b7sch", "und", "Flehn", "vom", "Gl\u00fcck", "er\u00b7hal\u00b7ten", "k\u00f6n\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "NN", "KON", "VVFIN", "APPRART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "Inde\u00dfen, da ich auch dein Schuldner bleiben mu\u00df", "tokens": ["In\u00b7de\u00b7\u00dfen", ",", "da", "ich", "auch", "dein", "Schuld\u00b7ner", "blei\u00b7ben", "mu\u00df"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "PPER", "ADV", "PPOSAT", "NN", "VVINF", "VMFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und nicht verm\u00f6gend bin, das Capital zu legen,", "tokens": ["Und", "nicht", "ver\u00b7m\u00f6\u00b7gend", "bin", ",", "das", "Ca\u00b7pi\u00b7tal", "zu", "le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "VVPP", "VAFIN", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So nimm hier vor den Zins des Seegens \u00dcberflu\u00df;", "tokens": ["So", "nimm", "hier", "vor", "den", "Zins", "des", "See\u00b7gens", "\u00dc\u00b7berf\u00b7lu\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "ADV", "APPR", "ART", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mein Wuntsch begleitet dich auf allen deinen Wegen.", "tokens": ["Mein", "Wunt\u00b7sch", "be\u00b7glei\u00b7tet", "dich", "auf", "al\u00b7len", "dei\u00b7nen", "We\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "APPR", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Sieh meiner Schwachheit nach, wozu die Noth oft zwingt,", "tokens": ["Sieh", "mei\u00b7ner", "Schwach\u00b7heit", "nach", ",", "wo\u00b7zu", "die", "Noth", "oft", "zwingt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "PTKVZ", "$,", "PWAV", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und wi\u00dfe, wo uns noch ein Stern zusammenbringt,", "tokens": ["Und", "wi\u00b7\u00dfe", ",", "wo", "uns", "noch", "ein", "Stern", "zu\u00b7sam\u00b7men\u00b7bringt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Da soll manch Abend uns den alten Gram vertreiben.", "tokens": ["Da", "soll", "manch", "A\u00b7bend", "uns", "den", "al\u00b7ten", "Gram", "ver\u00b7trei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIAT", "NN", "PPER", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wo dies nicht ernstlich ist, so weis ich nichts zu schreiben.", "tokens": ["Wo", "dies", "nicht", "ernst\u00b7lich", "ist", ",", "so", "weis", "ich", "nichts", "zu", "schrei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "PTKNEG", "ADJD", "VAFIN", "$,", "ADV", "PTKVZ", "PPER", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}