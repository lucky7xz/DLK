{"textgrid.poem.61424": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Genesung", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nach dumpfer Schw\u00fcle", "tokens": ["Nach", "dum\u00b7pfer", "Schw\u00fc\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Was mir so frisch", "tokens": ["Was", "mir", "so", "frisch"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "PPER", "ADV", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Mit unsichtbarem Fittich", "tokens": ["Mit", "un\u00b7sicht\u00b7ba\u00b7rem", "Fit\u00b7tich"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Stirne r\u00fchrt,", "tokens": ["Die", "Stir\u00b7ne", "r\u00fchrt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Bist du's endlich,", "tokens": ["Bist", "du's", "end\u00b7lich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADV", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Himmelstochter Genesung?", "tokens": ["Him\u00b7mel\u00b7stoch\u00b7ter", "Ge\u00b7ne\u00b7sung", "?"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+--", "measure": "unknown.measure.tri"}}, "stanza.2": {"line.1": {"text": "Leise sinkt's wie Gew\u00f6lk", "tokens": ["Lei\u00b7se", "sinkt's", "wie", "Ge\u00b7w\u00f6lk"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "KOKOM", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Zerrinnender Nebel", "tokens": ["Zer\u00b7rin\u00b7nen\u00b7der", "Ne\u00b7bel"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Mir von den Sinnen!", "tokens": ["Mir", "von", "den", "Sin\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Klarer, tiefer", "tokens": ["Kla\u00b7rer", ",", "tie\u00b7fer"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$,", "ADJD"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "D\u00fcnkt mir der Himmel,", "tokens": ["D\u00fcnkt", "mir", "der", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "++-+-", "measure": "iambic.di"}, "line.6": {"text": "Der Quellen Wogen", "tokens": ["Der", "Quel\u00b7len", "Wo\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "R\u00fchrt wie ferne Musik", "tokens": ["R\u00fchrt", "wie", "fer\u00b7ne", "Mu\u00b7sik"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "KOKOM", "ADJA", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "Mein erwachend Ohr,", "tokens": ["Mein", "er\u00b7wa\u00b7chend", "Ohr", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "VVPP", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "Und von den Wipfeln", "tokens": ["Und", "von", "den", "Wip\u00b7feln"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Der schwarzen Tannen", "tokens": ["Der", "schwar\u00b7zen", "Tan\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "Auf mich hernieder", "tokens": ["Auf", "mich", "her\u00b7nie\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPER", "PTKVZ"], "meter": "-+-+-", "measure": "iambic.di"}, "line.12": {"text": "D\u00e4mmern Gedanken.", "tokens": ["D\u00e4m\u00b7mern", "Ge\u00b7dan\u00b7ken", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.13": {"text": "Ach, noch kann ich dich nicht", "tokens": ["Ach", ",", "noch", "kann", "ich", "dich", "nicht"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "ADV", "VMFIN", "PPER", "PRF", "PTKNEG"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Fassen, o Muse,", "tokens": ["Fas\u00b7sen", ",", "o", "Mu\u00b7se", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.15": {"text": "Noch versagst du", "tokens": ["Noch", "ver\u00b7sagst", "du"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER"], "meter": "+-+-", "measure": "trochaic.di"}, "line.16": {"text": "Dem irrenden Finger", "tokens": ["Dem", "ir\u00b7ren\u00b7den", "Fin\u00b7ger"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.17": {"text": "Dein Saitenspiel;", "tokens": ["Dein", "Sai\u00b7ten\u00b7spiel", ";"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.18": {"text": "Aber schon sp\u00fcr' ich", "tokens": ["A\u00b7ber", "schon", "sp\u00fcr'", "ich"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.19": {"text": "In ahnender Seele", "tokens": ["In", "ah\u00b7nen\u00b7der", "See\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.20": {"text": "Dein tr\u00f6stlich Nahen,", "tokens": ["Dein", "tr\u00f6st\u00b7lich", "Na\u00b7hen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.21": {"text": "Im Windesodem", "tokens": ["Im", "Win\u00b7de\u00b7so\u00b7dem"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.22": {"text": "Flattert dein Hauch schon,", "tokens": ["Flat\u00b7tert", "dein", "Hauch", "schon", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.23": {"text": "Und seh' ich fern durch die St\u00e4mme", "tokens": ["Und", "seh'", "ich", "fern", "durch", "die", "St\u00e4m\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.24": {"text": "Auf Waldeswiesen", "tokens": ["Auf", "Wal\u00b7des\u00b7wie\u00b7sen"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.25": {"text": "Des Sonnenstrahls", "tokens": ["Des", "Son\u00b7nen\u00b7strahls"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.26": {"text": "Bewegtes Spielen,", "tokens": ["Be\u00b7weg\u00b7tes", "Spie\u00b7len", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.27": {"text": "So ist mir's oft,", "tokens": ["So", "ist", "mir's", "oft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NE", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.28": {"text": "Es sei das Wallen", "tokens": ["Es", "sei", "das", "Wal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.29": {"text": "Deines wei\u00dfen Gewandes.", "tokens": ["Dei\u00b7nes", "wei\u00b7\u00dfen", "Ge\u00b7wan\u00b7des", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}}}}