{"textgrid.poem.63102": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Die Infantriekaserne", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hinter diesem kleinen Feldchen", "tokens": ["Hin\u00b7ter", "die\u00b7sem", "klei\u00b7nen", "Feld\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Steht ein grau verhutzelt W\u00e4ldchen,", "tokens": ["Steht", "ein", "grau", "ver\u00b7hut\u00b7zelt", "W\u00e4ld\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJD", "VVPP", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00dcber seinen Gipfeln ferne", "tokens": ["\u00dc\u00b7ber", "sei\u00b7nen", "Gip\u00b7feln", "fer\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Blinkt die Infantriekaserne.", "tokens": ["Blinkt", "die", "In\u00b7fant\u00b7rie\u00b7ka\u00b7ser\u00b7ne", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Viele sch\u00f6ne rote D\u00e4cher", "tokens": ["Vie\u00b7le", "sch\u00f6\u00b7ne", "ro\u00b7te", "D\u00e4\u00b7cher"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Streckt sie in die Luft wie F\u00e4cher. \u2013", "tokens": ["Streckt", "sie", "in", "die", "Luft", "wie", "F\u00e4\u00b7cher", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "KOKOM", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach, der sch\u00f6nen Wanderin", "tokens": ["Ach", ",", "der", "sch\u00f6\u00b7nen", "Wan\u00b7de\u00b7rin"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ward ein wenig schw\u00fcl zu Sinn.", "tokens": ["Ward", "ein", "we\u00b7nig", "schw\u00fcl", "zu", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "PIS", "ADJD", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ja sie trippelte und hetzte,", "tokens": ["Ja", "sie", "trip\u00b7pel\u00b7te", "und", "hetz\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weil sie was in Glut versetzte,", "tokens": ["Weil", "sie", "was", "in", "Glut", "ver\u00b7setz\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ihr Auge, hei\u00df und gier,", "tokens": ["Und", "ihr", "Au\u00b7ge", ",", "hei\u00df", "und", "gier", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "ADJD", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sp\u00e4ht nach einem Musketier.", "tokens": ["Sp\u00e4ht", "nach", "ei\u00b7nem", "Mus\u00b7ke\u00b7tier", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Dieser hockt im Fenster tr\u00e4ge,", "tokens": ["Die\u00b7ser", "hockt", "im", "Fens\u00b7ter", "tr\u00e4\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eine Pfeife im Gehege.", "tokens": ["Ei\u00b7ne", "Pfei\u00b7fe", "im", "Ge\u00b7he\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.3": {"text": "Ach, wie w\u00e4r er doch so gerne", "tokens": ["Ach", ",", "wie", "w\u00e4r", "er", "doch", "so", "ger\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PWAV", "VAFIN", "PPER", "ADV", "ADV", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}