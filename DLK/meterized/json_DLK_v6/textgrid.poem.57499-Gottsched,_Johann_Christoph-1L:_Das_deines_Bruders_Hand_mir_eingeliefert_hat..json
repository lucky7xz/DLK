{"textgrid.poem.57499": {"metadata": {"author": {"name": "Gottsched, Johann Christoph", "birth": "N.A.", "death": "N.A."}, "title": "1L: Das deines Bruders Hand mir eingeliefert hat.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das deines Bruders Hand mir eingeliefert hat.", "tokens": ["Das", "dei\u00b7nes", "Bru\u00b7ders", "Hand", "mir", "ein\u00b7ge\u00b7lie\u00b7fert", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "NN", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nun bin ich wieder froh, da\u00df sich dein Kiel bequemet,", "tokens": ["Nun", "bin", "ich", "wie\u00b7der", "froh", ",", "da\u00df", "sich", "dein", "Kiel", "be\u00b7que\u00b7met", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJD", "$,", "KOUS", "PRF", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und sich der deutschen Art im Schreiben nicht gesch\u00e4met.", "tokens": ["Und", "sich", "der", "deut\u00b7schen", "Art", "im", "Schrei\u00b7ben", "nicht", "ge\u00b7sch\u00e4\u00b7met", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "ART", "ADJA", "NN", "APPRART", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Worte flie\u00dfen dir so sanft, so zart und rein,", "tokens": ["Die", "Wor\u00b7te", "flie\u00b7\u00dfen", "dir", "so", "sanft", ",", "so", "zart", "und", "rein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "ADJD", "$,", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So munter, deutlich, voll, so sch\u00f6n und ungemein,", "tokens": ["So", "mun\u00b7ter", ",", "deut\u00b7lich", ",", "voll", ",", "so", "sch\u00f6n", "und", "un\u00b7ge\u00b7mein", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADJD", "$,", "ADJD", "$,", "ADV", "ADJD", "KON", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Als h\u00e4tte ", "tokens": ["Als", "h\u00e4t\u00b7te"], "token_info": ["word", "word"], "pos": ["KOUS", "VAFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "In ", "tokens": ["In"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.8": {"text": "Die Franzen werden stolz, wenn du franz\u00f6sisch schreibst,", "tokens": ["Die", "Fran\u00b7zen", "wer\u00b7den", "stolz", ",", "wenn", "du", "fran\u00b7z\u00f6\u00b7sisch", "schreibst", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "KOUS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und zu der Missethat auch deinen Diener treibst,", "tokens": ["Und", "zu", "der", "Mis\u00b7se\u00b7that", "auch", "dei\u00b7nen", "Die\u00b7ner", "treibst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Der doch auch deutsch versteht. Verspare doch dein Wissen", "tokens": ["Der", "doch", "auch", "deutsch", "ver\u00b7steht", ".", "Ver\u00b7spa\u00b7re", "doch", "dein", "Wis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADV", "ADJD", "VVFIN", "$.", "NN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "F\u00fcr jene, die vieleicht, wie Hunde, bellen m\u00fcssen,", "tokens": ["F\u00fcr", "je\u00b7ne", ",", "die", "vie\u00b7leicht", ",", "wie", "Hun\u00b7de", ",", "bel\u00b7len", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PDS", "$,", "PRELS", "ADV", "$,", "PWAV", "NN", "$,", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wenn man sich nicht bequemt und ihre Mundart spricht:", "tokens": ["Wenn", "man", "sich", "nicht", "be\u00b7quemt", "und", "ih\u00b7re", "Mund\u00b7art", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PRF", "PTKNEG", "ADJD", "KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Bey Deutschen schimpfe nur durchaus das Deutsche nicht.", "tokens": ["Bey", "Deut\u00b7schen", "schimp\u00b7fe", "nur", "durc\u00b7haus", "das", "Deut\u00b7sche", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "ADV", "ADV", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ich selber hab es schon dem Himmel abgebethen,", "tokens": ["Ich", "sel\u00b7ber", "hab", "es", "schon", "dem", "Him\u00b7mel", "ab\u00b7ge\u00b7be\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Da\u00df ich ", "tokens": ["Da\u00df", "ich"], "token_info": ["word", "word"], "pos": ["KOUS", "PPER"], "meter": "-+", "measure": "iambic.single"}}, "stanza.2": {"line.1": {"text": "Allein, ", "tokens": ["Al\u00b7lein", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Den sonst gef\u00fchrten Krieg, und giebest dich zur Ruh?", "tokens": ["Den", "sonst", "ge\u00b7f\u00fchr\u00b7ten", "Krieg", ",", "und", "gie\u00b7best", "dich", "zur", "Ruh", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "$,", "KON", "VVFIN", "PRF", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du bist ja stark genug, an Muth und Kraft zu sch\u00e4tzen,", "tokens": ["Du", "bist", "ja", "stark", "ge\u00b7nug", ",", "an", "Muth", "und", "Kraft", "zu", "sch\u00e4t\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "ADV", "$,", "APPR", "NN", "KON", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Was dir so wohl gelung, noch ferner fortzusetzen.", "tokens": ["Was", "dir", "so", "wohl", "ge\u00b7lung", ",", "noch", "fer\u00b7ner", "fort\u00b7zu\u00b7set\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADV", "NN", "$,", "ADV", "ADV", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich bin der Gegner nicht, der dich besiegen kann,", "tokens": ["Ich", "bin", "der", "Geg\u00b7ner", "nicht", ",", "der", "dich", "be\u00b7sie\u00b7gen", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PTKNEG", "$,", "PRELS", "PRF", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Du triffst f\u00fcrwahr so leicht nicht deinesgleichen an:", "tokens": ["Du", "triffst", "f\u00fcr\u00b7wahr", "so", "leicht", "nicht", "dei\u00b7nes\u00b7glei\u00b7chen", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADJD", "PTKNEG", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "An Sch\u00f6nheit bist du leicht den Sch\u00f6nen \u00fcberlegen,", "tokens": ["An", "Sch\u00f6n\u00b7heit", "bist", "du", "leicht", "den", "Sch\u00f6\u00b7nen", "\u00fc\u00b7berl\u00b7e\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und M\u00e4nner scheuen dich, um deines Geistes wegen.", "tokens": ["Und", "M\u00e4n\u00b7ner", "scheu\u00b7en", "dich", ",", "um", "dei\u00b7nes", "Geis\u00b7tes", "we\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "$,", "KOUI", "PPOSAT", "NN", "APPR", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Von beyden hat mich l\u00e4ngst die volle Kraft ger\u00fchrt:", "tokens": ["Von", "bey\u00b7den", "hat", "mich", "l\u00e4ngst", "die", "vol\u00b7le", "Kraft", "ge\u00b7r\u00fchrt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VAFIN", "PPER", "ADV", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wie schnell war ich besiegt! Wie bald hab ich gesp\u00fcrt,", "tokens": ["Wie", "schnell", "war", "ich", "be\u00b7siegt", "!", "Wie", "bald", "hab", "ich", "ge\u00b7sp\u00fcrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "VVPP", "$.", "PWAV", "ADV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-++--+", "measure": "iambic.hexa.chol"}, "line.11": {"text": "Da\u00df auch ein starkes Herz, das nicht so leicht verzaget,", "tokens": ["Da\u00df", "auch", "ein", "star\u00b7kes", "Herz", ",", "das", "nicht", "so", "leicht", "ver\u00b7za\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "$,", "PRELS", "PTKNEG", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Sich niemals ungestraft an deine Gr\u00f6\u00dfe waget.", "tokens": ["Sich", "nie\u00b7mals", "un\u00b7ge\u00b7straft", "an", "dei\u00b7ne", "Gr\u00f6\u00b7\u00dfe", "wa\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Was ", "tokens": ["Was"], "token_info": ["word"], "pos": ["PWS"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Da\u00df sie mir selbst durch dich die Antwort schreiben lie\u00df.", "tokens": ["Da\u00df", "sie", "mir", "selbst", "durch", "dich", "die", "Ant\u00b7wort", "schrei\u00b7ben", "lie\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "APPR", "PPER", "ART", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sie lebt ja noch in dir, an Geist, Gestalt und Gaben,", "tokens": ["Sie", "lebt", "ja", "noch", "in", "dir", ",", "an", "Geist", ",", "Ge\u00b7stalt", "und", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "APPR", "PPER", "$,", "APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Stand gebricht dir nur: den darfst du auch nicht haben;", "tokens": ["Der", "Stand", "ge\u00b7bricht", "dir", "nur", ":", "den", "darfst", "du", "auch", "nicht", "ha\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$.", "ART", "VVFIN", "PPER", "ADV", "PTKNEG", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ja du verlangst ihn nicht, seit dem du mit Verstand,", "tokens": ["Ja", "du", "ver\u00b7langst", "ihn", "nicht", ",", "seit", "dem", "du", "mit", "Ver\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "VVFIN", "PPER", "PTKNEG", "$,", "APPR", "PRELS", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Nach Philosophenart, der St\u00e4nde Werth erkannt.", "tokens": ["Nach", "Phi\u00b7lo\u00b7so\u00b7phen\u00b7art", ",", "der", "St\u00e4n\u00b7de", "Werth", "er\u00b7kannt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Du hast ja eingesehn, da\u00df sonder Witz und Tugend", "tokens": ["Du", "hast", "ja", "ein\u00b7ge\u00b7sehn", ",", "da\u00df", "son\u00b7der", "Witz", "und", "Tu\u00b7gend"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "VVINF", "$,", "KOUS", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Auch F\u00fcrsten Sclaven sind; und deine weise Jugend,", "tokens": ["Auch", "F\u00fcrs\u00b7ten", "Scla\u00b7ven", "sind", ";", "und", "dei\u00b7ne", "wei\u00b7se", "Ju\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "NN", "VAFIN", "$.", "KON", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Durch Zucht und Artigkeit und vieler Gaben Pracht,", "tokens": ["Durch", "Zucht", "und", "Ar\u00b7tig\u00b7keit", "und", "vie\u00b7ler", "Ga\u00b7ben", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Weit mehr, als edel ist, ja F\u00fcrsten schamroth macht.", "tokens": ["Weit", "mehr", ",", "als", "e\u00b7del", "ist", ",", "ja", "F\u00fcrs\u00b7ten", "scham\u00b7roth", "macht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "$,", "KOUS", "ADJD", "VAFIN", "$,", "ADV", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Doch hab ich mit Bedacht, was du befahlst, gelesen:", "tokens": ["Doch", "hab", "ich", "mit", "Be\u00b7dacht", ",", "was", "du", "be\u00b7fahlst", ",", "ge\u00b7le\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "APPR", "NN", "$,", "PWS", "PPER", "VVFIN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und wie besch\u00e4mte mich dein witzerf\u00fclltes Wesen,", "tokens": ["Und", "wie", "be\u00b7sch\u00e4m\u00b7te", "mich", "dein", "wit\u00b7zer\u00b7f\u00fcll\u00b7tes", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Womit du mich ber\u00fcckt! Indessen, wie mich deucht,", "tokens": ["Wo\u00b7mit", "du", "mich", "be\u00b7r\u00fcckt", "!", "In\u00b7des\u00b7sen", ",", "wie", "mich", "deucht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "VVPP", "$.", "NN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "F\u00e4llt dir die Antwort selbst, in meinem Namen leicht.", "tokens": ["F\u00e4llt", "dir", "die", "Ant\u00b7wort", "selbst", ",", "in", "mei\u00b7nem", "Na\u00b7men", "leicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "$,", "APPR", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ich bin kein ", "tokens": ["Ich", "bin", "kein"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "PIAT"], "meter": "+-+", "measure": "trochaic.di"}, "line.16": {"text": "(vom H\u00f6cker sag ich nichts) und hab es oft erfahren,", "tokens": ["(", "vom", "H\u00f6\u00b7cker", "sag", "ich", "nichts", ")", "und", "hab", "es", "oft", "er\u00b7fah\u00b7ren", ","], "token_info": ["punct", "word", "word