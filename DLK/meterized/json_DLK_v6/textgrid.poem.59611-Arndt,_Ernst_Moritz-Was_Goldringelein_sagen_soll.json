{"textgrid.poem.59611": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Was Goldringelein sagen soll", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Geh hin, geh hin, Goldringelein,", "tokens": ["Geh", "hin", ",", "geh", "hin", ",", "Gold\u00b7rin\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "VVFIN", "PTKVZ", "$,", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Und sage meinem trauten Kinde,", "tokens": ["Und", "sa\u00b7ge", "mei\u00b7nem", "trau\u00b7ten", "Kin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Treufest wie Berge soll sie sein", "tokens": ["Treu\u00b7fest", "wie", "Ber\u00b7ge", "soll", "sie", "sein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "KOKOM", "NN", "VMFIN", "PPER", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und lieblich wie die Fr\u00fchlingswinde,", "tokens": ["Und", "lieb\u00b7lich", "wie", "die", "Fr\u00fch\u00b7lings\u00b7win\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Doch nicht wie Zephirs Fl\u00fcgel leicht,", "tokens": ["Doch", "nicht", "wie", "Ze\u00b7phirs", "Fl\u00fc\u00b7gel", "leicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "KOKOM", "NE", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der alles k\u00fcssend weiter fleucht.", "tokens": ["Der", "al\u00b7les", "k\u00fcs\u00b7send", "wei\u00b7ter", "fleucht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADJD", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Geh hin, geh hin, Goldringelein,", "tokens": ["Geh", "hin", ",", "geh", "hin", ",", "Gold\u00b7rin\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "VVFIN", "PTKVZ", "$,", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Und sage meiner h\u00fcbschen Feinen,", "tokens": ["Und", "sa\u00b7ge", "mei\u00b7ner", "h\u00fcb\u00b7schen", "Fei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie soll in meines Herzens Schrein", "tokens": ["Sie", "soll", "in", "mei\u00b7nes", "Her\u00b7zens", "Schrein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hell leuchtend wie Karfunkel scheinen,", "tokens": ["Hell", "leuch\u00b7tend", "wie", "Kar\u00b7fun\u00b7kel", "schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVPP", "KOKOM", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Womit man heil'ge Bilder schm\u00fcckt,", "tokens": ["Wo\u00b7mit", "man", "heil'\u00b7ge", "Bil\u00b7der", "schm\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df sich der Frommen Herz entz\u00fcckt.", "tokens": ["Da\u00df", "sich", "der", "From\u00b7men", "Herz", "ent\u00b7z\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Geh hin, geh hin, Goldringelein,", "tokens": ["Geh", "hin", ",", "geh", "hin", ",", "Gold\u00b7rin\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "VVFIN", "PTKVZ", "$,", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Und sage meiner s\u00fc\u00dfen Holden,", "tokens": ["Und", "sa\u00b7ge", "mei\u00b7ner", "s\u00fc\u00b7\u00dfen", "Hol\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wann ostw\u00e4rts geht der Sonne Schein", "tokens": ["Wann", "ost\u00b7w\u00e4rts", "geht", "der", "Son\u00b7ne", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Sterne H\u00f6llennacht vergolden,", "tokens": ["Und", "Ster\u00b7ne", "H\u00f6l\u00b7len\u00b7nacht", "ver\u00b7gol\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wann B\u00e4che rinnen berghinauf,", "tokens": ["Wann", "B\u00e4\u00b7che", "rin\u00b7nen", "berg\u00b7hin\u00b7auf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dann h\u00f6ret meine Liebe auf.", "tokens": ["Dann", "h\u00f6\u00b7ret", "mei\u00b7ne", "Lie\u00b7be", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Geh hin, geh hin, Goldringelein,", "tokens": ["Geh", "hin", ",", "geh", "hin", ",", "Gold\u00b7rin\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "VVFIN", "PTKVZ", "$,", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Das sollst du noch zuletzt ihr sagen:", "tokens": ["Das", "sollst", "du", "noch", "zu\u00b7letzt", "ihr", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "ADV", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nichts s\u00fc\u00dfer ist als Liebespein,", "tokens": ["Nichts", "s\u00fc\u00b7\u00dfer", "ist", "als", "Lie\u00b7be\u00b7spein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADJD", "VAFIN", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nichts lustiger als Liebesklagen,", "tokens": ["Nichts", "lus\u00b7ti\u00b7ger", "als", "Lie\u00b7bes\u00b7kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ADJD", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Nichts fr\u00f6hlicher als Liebesnot,", "tokens": ["Nichts", "fr\u00f6h\u00b7li\u00b7cher", "als", "Lie\u00b7bes\u00b7not", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ADJD", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nichts seliger als Liebestod.", "tokens": ["Nichts", "se\u00b7li\u00b7ger", "als", "Lie\u00b7be\u00b7stod", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ADJD", "KOKOM", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}}}}