{"dta.poem.10006": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Die Vergn\u00fcgungen  \n  der Melancholey.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20674-3", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Andre m\u00f6gen am l\u00e4chelnden Abend des Sommers", "tokens": ["And\u00b7re", "m\u00f6\u00b7gen", "am", "l\u00e4\u00b7cheln\u00b7den", "A\u00b7bend", "des", "Som\u00b7mers"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "APPRART", "ADJA", "NN", "ART", "NN"], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Wenn sie am dumpfen Ger\u00e4usch des murmelnden Ba-", "tokens": ["Wenn", "sie", "am", "dum\u00b7pfen", "Ge\u00b7r\u00e4usch", "des", "mur\u00b7meln\u00b7den", "Ba"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "ADJA", "NN", "ART", "ADJA", "TRUNC"], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Oder das sanftere Roth des streifichten Westens be-", "tokens": ["O\u00b7der", "das", "sanf\u00b7te\u00b7re", "Roth", "des", "strei\u00b7fich\u00b7ten", "Wes\u00b7tens", "be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "TRUNC"], "meter": "+--+--+-+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Mich ergetzt nur Nebel und Dunkel des blassen De-", "tokens": ["Mich", "er\u00b7getzt", "nur", "Ne\u00b7bel", "und", "Dun\u00b7kel", "des", "blas\u00b7sen", "De"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "NN", "KON", "NN", "ART", "ADJA", "TRUNC"], "meter": "+-+-+--+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Wenn die Schatten sich dann des langen Abends ge-", "tokens": ["Wenn", "die", "Schat\u00b7ten", "sich", "dann", "des", "lan\u00b7gen", "A\u00b7bends", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PRF", "ADV", "ART", "ADJA", "NN", "TRUNC"], "meter": "--+--+-+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Und ein schimmernder Stral der matten sterbenden", "tokens": ["Und", "ein", "schim\u00b7mern\u00b7der", "Stral", "der", "mat\u00b7ten", "ster\u00b7ben\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "--+--+-+-+--", "measure": "anapaest.di.plus"}, "line.7": {"text": "Durch den d\u00e4mmernden Raum, sich bricht: dann la\u00df", "tokens": ["Durch", "den", "d\u00e4m\u00b7mern\u00b7den", "Raum", ",", "sich", "bricht", ":", "dann", "la\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PRF", "VVFIN", "$.", "ADV", "VVFIN"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "Von dem Jauchzen des Unsinns, das ietzo mit festlichem", "tokens": ["Von", "dem", "Jauch\u00b7zen", "des", "Un\u00b7sinns", ",", "das", "iet\u00b7zo", "mit", "fest\u00b7li\u00b7chem"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,", "PRELS", "ADV", "APPR", "ADJA"], "meter": "--+--+--+--+--", "measure": "anapaest.tetra.plus"}, "line.9": {"text": "Durch die erleuchteten Zimmer ert\u00f6nt, dann la\u00df mich", "tokens": ["Durch", "die", "er\u00b7leuch\u00b7te\u00b7ten", "Zim\u00b7mer", "er\u00b7t\u00f6nt", ",", "dann", "la\u00df", "mich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$,", "ADV", "VVIMP", "PPER"], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.10": {"text": "Sitzen, allein nur vergn\u00fcgt an der niedern klagenden", "tokens": ["Sit\u00b7zen", ",", "al\u00b7lein", "nur", "ver\u00b7gn\u00fcgt", "an", "der", "nie\u00b7dern", "kla\u00b7gen\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADV", "ADV", "VVPP", "APPR", "ART", "ADJA", "NN"], "meter": "+--++-+--+-+--", "measure": "trochaic.hexa.relaxed"}, "line.11": {"text": "Schlummer erweckendem Lied; und la\u00df mich mit mei-", "tokens": ["Schlum\u00b7mer", "er\u00b7we\u00b7cken\u00b7dem", "Lied", ";", "und", "la\u00df", "mich", "mit", "mei"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "ADJA", "NN", "$.", "KON", "VVIMP", "PPER", "APPR", "TRUNC"], "meter": "+--+--+-+--+", "measure": "dactylic.di.plus"}, "line.12": {"text": "In mich gekehrt, den Wechsel der Dinge, die leeren", "tokens": ["In", "mich", "ge\u00b7kehrt", ",", "den", "Wech\u00b7sel", "der", "Din\u00b7ge", ",", "die", "lee\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPER", "VVPP", "$,", "ART", "NN", "ART", "NN", "$,", "ART", "ADJA"], "meter": "+--+-+--+--+-", "measure": "iambic.penta.invert"}, "line.13": {"text": "Und die vergebliche M\u00fche betrachten, die unsrer Er-", "tokens": ["Und", "die", "ver\u00b7geb\u00b7li\u00b7che", "M\u00fc\u00b7he", "be\u00b7trach\u00b7ten", ",", "die", "uns\u00b7rer", "Er"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF", "$,", "PRELS", "PPOSAT", "NN"], "meter": "-+-+--+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Forschen vereitelt, so wie wir die W\u00fcste des Lebens", "tokens": ["For\u00b7schen", "ver\u00b7ei\u00b7telt", ",", "so", "wie", "wir", "die", "W\u00fcs\u00b7te", "des", "Le\u00b7bens"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVPP", "$,", "ADV", "KOKOM", "PPER", "ART", "NN", "ART", "NN"], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}, "line.15": {"text": "Diese gesegnete Stunde der Stille wird alles das L\u00e4-", "tokens": ["Die\u00b7se", "ge\u00b7se\u00b7gne\u00b7te", "Stun\u00b7de", "der", "Stil\u00b7le", "wird", "al\u00b7les", "das", "L\u00e4"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDAT", "ADJA", "NN", "ART", "NN", "VAFIN", "PIS", "ART", "TRUNC"], "meter": "+---+-+--+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.16": {"text": "Schimmernder Thorheit entdecken, das, gleich des li-", "tokens": ["Schim\u00b7mern\u00b7der", "Thor\u00b7heit", "ent\u00b7de\u00b7cken", ",", "das", ",", "gleich", "des", "li"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "VVINF", "$,", "PDS", "$,", "ADV", "ART", "TRUNC"], "meter": "+--+--+--+--", "measure": "dactylic.tetra"}, "line.17": {"text": "Falscher zaubrischer Kunst, die allzusicheren Augen", "tokens": ["Fal\u00b7scher", "zaub\u00b7ri\u00b7scher", "Kunst", ",", "die", "all\u00b7zu\u00b7si\u00b7che\u00b7ren", "Au\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "ADJA", "NN", "$,", "ART", "ADJA", "NN"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.18": {"text": "Mit der verborgnen Verblendung get\u00e4uscht; den bezau-", "tokens": ["Mit", "der", "ver\u00b7borg\u00b7nen", "Ver\u00b7blen\u00b7dung", "ge\u00b7t\u00e4uscht", ";", "den", "be\u00b7zau"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$.", "ART", "TRUNC"], "meter": "---+--+--+--+", "measure": "iambic.tetra.relaxed"}, "line.19": {"text": "Uns zu trinken verf\u00fchrt, wodurch die Seele berauschet,", "tokens": ["Uns", "zu", "trin\u00b7ken", "ver\u00b7f\u00fchrt", ",", "wo\u00b7durch", "die", "See\u00b7le", "be\u00b7rau\u00b7schet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "VVPP", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.20": {"text": "Ganz sich vergi\u00dft, und der Mensch zum Ungeheuer", "tokens": ["Ganz", "sich", "ver\u00b7gi\u00dft", ",", "und", "der", "Mensch", "zum", "Un\u00b7ge\u00b7heu\u00b7er"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "PRF", "VVFIN", "$,", "KON", "ART", "NN", "APPRART", "NN"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.21": {"text": "Gierig kosten wir ihn, doch in dem frohen Genu\u00dfe", "tokens": ["Gie\u00b7rig", "kos\u00b7ten", "wir", "ihn", ",", "doch", "in", "dem", "fro\u00b7hen", "Ge\u00b7nu\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "PPER", "$,", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "+-+---+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.22": {"text": "Merken wir nicht die giftigen Hefen, die mit ihm ge-", "tokens": ["Mer\u00b7ken", "wir", "nicht", "die", "gif\u00b7ti\u00b7gen", "He\u00b7fen", ",", "die", "mit", "ihm", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "ART", "ADJA", "NN", "$,", "PRELS", "APPR", "PPER", "TRUNC"], "meter": "+--+-+--+--+-+", "measure": "iambic.hexa.invert"}}}}}