{"dta.poem.9628": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du blume Schlestens/ du sonne dieser welt/", "tokens": ["Du", "blu\u00b7me", "Schles\u00b7tens", "/", "du", "son\u00b7ne", "die\u00b7ser", "welt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$(", "PPER", "VVFIN", "PDAT", "NN", "$("], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die die annehmligkeit auff purpur-bl\u00e4ttern tr\u00e4get/", "tokens": ["Die", "die", "an\u00b7nehm\u00b7lig\u00b7keit", "auff", "pur\u00b7pur\u00b7bl\u00e4t\u00b7tern", "tr\u00e4\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Auf welche Venus selbst ihr ebenbild gepr\u00e4get/", "tokens": ["Auf", "wel\u00b7che", "Ve\u00b7nus", "selbst", "ihr", "e\u00b7ben\u00b7bild", "ge\u00b7pr\u00e4\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "ADV", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als/ irrdsche g\u00f6ttin/ sie dich kaum ans licht gestellt.", "tokens": ["Als", "/", "irrd\u00b7sche", "g\u00f6t\u00b7tin", "/", "sie", "dich", "kaum", "ans", "licht", "ge\u00b7stellt", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$(", "ADJA", "NN", "$(", "PPER", "PRF", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Du bist der sch\u00f6nheit prei\u00df/ ein auszug aller zierden;", "tokens": ["Du", "bist", "der", "sch\u00f6n\u00b7heit", "prei\u00df", "/", "ein", "aus\u00b7zug", "al\u00b7ler", "zier\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVFIN", "$(", "ART", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Doch auch ein marmolstein an f\u00fchlen und begierden.", "tokens": ["Doch", "auch", "ein", "mar\u00b7mol\u00b7stein", "an", "f\u00fch\u00b7len", "und", "be\u00b7gier\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "APPR", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wirff itzt von deinem blitz auch einen schlechten blick", "tokens": ["Wirff", "itzt", "von", "dei\u00b7nem", "blitz", "auch", "ei\u00b7nen", "schlech\u00b7ten", "blick"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "APPR", "PPOSAT", "NN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auff deinen armen knecht/ und la\u00df ihn einmahl wissen/", "tokens": ["Auff", "dei\u00b7nen", "ar\u00b7men", "knecht", "/", "und", "la\u00df", "ihn", "ein\u00b7mahl", "wis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$(", "KON", "VVIMP", "PPER", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df deine hohe gunst will auff die knechtschafft fl\u00fcssen/", "tokens": ["Da\u00df", "dei\u00b7ne", "ho\u00b7he", "gunst", "will", "auff", "die", "knecht\u00b7schafft", "fl\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "VMFIN", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So lebet er in ruh/ so lebet er in gl\u00fcck.", "tokens": ["So", "le\u00b7bet", "er", "in", "ruh", "/", "so", "le\u00b7bet", "er", "in", "gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "$(", "ADV", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+---+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Es wird die hohe gunst ihn ewig dir verbinden/", "tokens": ["Es", "wird", "die", "ho\u00b7he", "gunst", "ihn", "e\u00b7wig", "dir", "ver\u00b7bin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "PPER", "ADJD", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wenn dein erh\u00e4rter sinn aus seel und leib wird schwinden.", "tokens": ["Wenn", "dein", "er\u00b7h\u00e4r\u00b7ter", "sinn", "aus", "seel", "und", "leib", "wird", "schwin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "APPR", "NE", "KON", "NN", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch alles ist umsonst/ es ist um mich geschehn/", "tokens": ["Doch", "al\u00b7les", "ist", "um\u00b7sonst", "/", "es", "ist", "um", "mich", "ge\u00b7schehn", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "ADV", "$(", "PPER", "VAFIN", "APPR", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die scharlach-lippen sind entgeisterte corallen/", "tokens": ["Die", "scha\u00b7rlach\u00b7\u00b7lip\u00b7pen", "sind", "ent\u00b7geis\u00b7ter\u00b7te", "co\u00b7ral\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der kugeln sch\u00f6nes paar/ nur alabaster-ballen/", "tokens": ["Der", "ku\u00b7geln", "sch\u00f6\u00b7nes", "paar", "/", "nur", "a\u00b7la\u00b7bas\u00b7ter\u00b7bal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$(", "ADV", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Auff welchen man noch nie die regung hat gesehn.", "tokens": ["Auff", "wel\u00b7chen", "man", "noch", "nie", "die", "re\u00b7gung", "hat", "ge\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "PIS", "ADV", "ADV", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sol der granaten schmuck auf deinen amber-lippen", "tokens": ["Sol", "der", "gra\u00b7na\u00b7ten", "schmuck", "auf", "dei\u00b7nen", "am\u00b7ber\u00b7\u00b7lip\u00b7pen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Fast mehr entseelet seyn/ als berge/ fel\u00df und klippen?", "tokens": ["Fast", "mehr", "ent\u00b7see\u00b7let", "seyn", "/", "als", "ber\u00b7ge", "/", "fel\u00df", "und", "klip\u00b7pen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "VAINF", "$(", "KOKOM", "VVFIN", "$(", "NE", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Bedencke sch\u00f6nes kind/ wilstu ein bild ja seyn/", "tokens": ["Be\u00b7den\u00b7cke", "sch\u00f6\u00b7nes", "kind", "/", "wils\u00b7tu", "ein", "bild", "ja", "seyn", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$(", "VMFIN", "ART", "NN", "ADV", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df todte bilder auch annehmligkeiten geben/", "tokens": ["Da\u00df", "tod\u00b7te", "bil\u00b7der", "auch", "an\u00b7nehm\u00b7lig\u00b7kei\u00b7ten", "ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "ADV", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dein eigen conterfait sah ich nechst um mich schweben/", "tokens": ["Dein", "ei\u00b7gen", "con\u00b7ter\u00b7fait", "sah", "ich", "nechst", "um", "mich", "schwe\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NE", "VVFIN", "PPER", "ADV", "APPR", "PPER", "VVFIN", "$("], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Als sich ein s\u00fcsser schlaff bey mir gestellet ein.", "tokens": ["Als", "sich", "ein", "s\u00fcs\u00b7ser", "schlaff", "bey", "mir", "ge\u00b7stel\u00b7let", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "ADJA", "VVFIN", "APPR", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es wolt die liebligkeit sich dar auch lassen finden/", "tokens": ["Es", "wolt", "die", "lieb\u00b7lig\u00b7keit", "sich", "dar", "auch", "las\u00b7sen", "fin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PRF", "PTKVZ", "ADV", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und mit der sch\u00f6nheit sich verschwestern und verbinden.", "tokens": ["Und", "mit", "der", "sch\u00f6n\u00b7heit", "sich", "ver\u00b7schwes\u00b7tern", "und", "ver\u00b7bin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PRF", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}