{"textgrid.poem.67916": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "11. Das nu\u00dfbraune M\u00e4dchen", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Falsch oder wahr, man sagt es klar:", "tokens": ["Falsch", "o\u00b7der", "wahr", ",", "man", "sagt", "es", "klar", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$,", "PIS", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbwer traut auf Weibertreu,", "tokens": ["\u00bb", "wer", "traut", "auf", "Wei\u00b7ber\u00b7treu", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der tr\u00fcgt sich sehr, der b\u00fc\u00dft es schwer", "tokens": ["Der", "tr\u00fcgt", "sich", "sehr", ",", "der", "b\u00fc\u00dft", "es", "schwer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PRF", "ADV", "$,", "PRELS", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit mancher sp\u00e4ten Reu.\u00ab", "tokens": ["Mit", "man\u00b7cher", "sp\u00e4\u00b7ten", "Reu", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NE", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "So spricht die Welt, doch, wenns gef\u00e4llt,", "tokens": ["So", "spricht", "die", "Welt", ",", "doch", ",", "wenns", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADV", "$,", "KOUS", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "H\u00f6rt ein Geschichtchen an;", "tokens": ["H\u00f6rt", "ein", "Ge\u00b7schicht\u00b7chen", "an", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Vom M\u00e4dchen braun, die fest und traun!", "tokens": ["Vom", "M\u00e4d\u00b7chen", "braun", ",", "die", "fest", "und", "traun", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "$,", "PRELS", "ADJD", "KON", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Liebt, wie man lieben kann.", "tokens": ["Liebt", ",", "wie", "man", "lie\u00b7ben", "kann", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es kam zu ihr, leis' an die Th\u00fcr,", "tokens": ["Es", "kam", "zu", "ihr", ",", "leis'", "an", "die", "Th\u00fcr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$,", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr Lieb zu Mitternacht,", "tokens": ["Ihr", "Lieb", "zu", "Mit\u00b7ter\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Thu, M\u00e4dchen, auf im schnellen Lauf,", "tokens": ["Thu", ",", "M\u00e4d\u00b7chen", ",", "auf", "im", "schnel\u00b7len", "Lauf", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "APPR", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Eh jemand hier erwacht.", "tokens": ["Eh", "je\u00b7mand", "hier", "er\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie that ihm auf in schnellem Lauf:", "tokens": ["Sie", "that", "ihm", "auf", "in", "schnel\u00b7lem", "Lauf", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "\u00bbich mu\u00df, ich mu\u00df von hier,", "tokens": ["\u00bb", "ich", "mu\u00df", ",", "ich", "mu\u00df", "von", "hier", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "$,", "PPER", "VMFIN", "APPR", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Zum Tod verdammt, vom Richteramt,", "tokens": ["Zum", "Tod", "ver\u00b7dammt", ",", "vom", "Rich\u00b7ter\u00b7amt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Nehm Abschied ich von dir. \u2013", "tokens": ["Nehm", "Ab\u00b7schied", "ich", "von", "dir", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "NN", "PPER", "APPR", "PPER", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich mu\u00df gar bald in wilden Wald;", "tokens": ["Ich", "mu\u00df", "gar", "bald", "in", "wil\u00b7den", "Wald", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sonst ists um mich geschehn.\u00ab", "tokens": ["Sonst", "ists", "um", "mich", "ge\u00b7schehn", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "APPR", "PPER", "VVPP", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbo nein, o nein! es kann nicht seyn! \u2013", "tokens": ["\u00bb", "o", "nein", ",", "o", "nein", "!", "es", "kann", "nicht", "seyn", "!", "\u2013"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "FM", "PTKANT", "$,", "FM", "PTKANT", "$.", "PPER", "VMFIN", "PTKNEG", "VAINF", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auch ich will mit dir gehn.\u00ab", "tokens": ["Auch", "ich", "will", "mit", "dir", "gehn", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "PPER", "VMFIN", "APPR", "PPER", "VVINF", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "\u00bbwas ist der Zeit Gl\u00fcckseligkeit?", "tokens": ["\u00bb", "was", "ist", "der", "Zeit", "Gl\u00fcck\u00b7se\u00b7lig\u00b7keit", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sie wandelt Lieb' in Noth.\u00ab", "tokens": ["Sie", "wan\u00b7delt", "Lieb'", "in", "Noth", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "\u00bbo Lieber nein! es kann nicht seyn,", "tokens": ["\u00bb", "o", "Lie\u00b7ber", "nein", "!", "es", "kann", "nicht", "seyn", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "NN", "PTKANT", "$.", "PPER", "VMFIN", "PTKNEG", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Uns scheidet nur der Tod.\u00ab", "tokens": ["Uns", "schei\u00b7det", "nur", "der", "Tod", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "\u00bbdu kannst nicht mit! H\u00f6r' an, ich bitt',", "tokens": ["\u00bb", "du", "kannst", "nicht", "mit", "!", "H\u00f6r'", "an", ",", "ich", "bitt'", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "PTKNEG", "PTKVZ", "$.", "NE", "PTKVZ", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "H\u00f6r an und la\u00df es seyn.", "tokens": ["H\u00f6r", "an", "und", "la\u00df", "es", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "KON", "VVIMP", "PPER", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was ist der Wald f\u00fcr Aufenthalt", "tokens": ["Was", "ist", "der", "Wald", "f\u00fcr", "Auf\u00b7ent\u00b7halt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr dich, du Liebe mein!", "tokens": ["F\u00fcr", "dich", ",", "du", "Lie\u00b7be", "mein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PPER", "NN", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "In Frost und Schnee, in Durst und Weh,", "tokens": ["In", "Frost", "und", "Schnee", ",", "in", "Durst", "und", "Weh", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In Hunger, Furcht und Schmerz;", "tokens": ["In", "Hun\u00b7ger", ",", "Furcht", "und", "Schmerz", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Nein, Liebe, nein! es kann nicht seyn,", "tokens": ["Nein", ",", "Lie\u00b7be", ",", "nein", "!", "es", "kann", "nicht", "seyn", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NN", "$,", "PTKANT", "$.", "PPER", "VMFIN", "PTKNEG", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Bleib' hier und still dein Herz.\u00ab", "tokens": ["Bleib'", "hier", "und", "still", "dein", "Herz", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ADV", "KON", "ADJD", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "\u00bbnein, Lieber, nein! geh nicht allein!", "tokens": ["\u00bb", "nein", ",", "Lie\u00b7ber", ",", "nein", "!", "geh", "nicht", "al\u00b7lein", "!"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PTKANT", "$,", "ADJD", "$,", "PTKANT", "$.", "VVFIN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich mu\u00df, ich mu\u00df mit dir!", "tokens": ["Ich", "mu\u00df", ",", "ich", "mu\u00df", "mit", "dir", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PPER", "VMFIN", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Entfliehest du, wo find' ich Ruh?", "tokens": ["Ent\u00b7flie\u00b7hest", "du", ",", "wo", "find'", "ich", "Ruh", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was bleibt f\u00fcr Leben mir?", "tokens": ["Was", "bleibt", "f\u00fcr", "Le\u00b7ben", "mir", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "NN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "In Frost und Schnee, in Durst und Weh,", "tokens": ["In", "Frost", "und", "Schnee", ",", "in", "Durst", "und", "Weh", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In Hunger, Furcht und Schmerz;", "tokens": ["In", "Hun\u00b7ger", ",", "Furcht", "und", "Schmerz", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Nichts ficht mich an, gehst du voran", "tokens": ["Nichts", "ficht", "mich", "an", ",", "gehst", "du", "vo\u00b7ran"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "PTKVZ", "$,", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und stillst mein armes Herz.\u00ab", "tokens": ["Und", "stillst", "mein", "ar\u00b7mes", "Herz", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "\u00bbach, Liebe, nein! Ich mu\u00df allein,", "tokens": ["\u00bb", "ach", ",", "Lie\u00b7be", ",", "nein", "!", "Ich", "mu\u00df", "al\u00b7lein", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$,", "NN", "$,", "PTKANT", "$.", "PPER", "VMFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bleib' hier und tr\u00f6ste dich;", "tokens": ["Bleib'", "hier", "und", "tr\u00f6s\u00b7te", "dich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "KON", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es stillt die Zeit ja alles Leid,", "tokens": ["Es", "stillt", "die", "Zeit", "ja", "al\u00b7les", "Leid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie stillt dirs sicherlich.", "tokens": ["Sie", "stillt", "dirs", "si\u00b7cher\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "PRF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Was wird die Stadt, die Zungen hat,", "tokens": ["Was", "wird", "die", "Stadt", ",", "die", "Zun\u00b7gen", "hat", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "$,", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So scharf wie Spie\u00df und Schwert;", "tokens": ["So", "scharf", "wie", "Spie\u00df", "und", "Schwert", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "F\u00fcr bittre Schmach dir reden nach,", "tokens": ["F\u00fcr", "bitt\u00b7re", "Schmach", "dir", "re\u00b7den", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wenn sie die Flucht erf\u00e4hrt?\u00ab", "tokens": ["Wenn", "sie", "die", "Flucht", "er\u00b7f\u00e4hrt", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "\u00bbnein, Lieber, nein! es kann nicht seyn,", "tokens": ["\u00bb", "nein", ",", "Lie\u00b7ber", ",", "nein", "!", "es", "kann", "nicht", "seyn", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKANT", "$,", "ADJD", "$,", "PTKANT", "$.", "PPER", "VMFIN", "PTKNEG", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mich tr\u00f6stet keine Zeit;", "tokens": ["Mich", "tr\u00f6s\u00b7tet", "kei\u00b7ne", "Zeit", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein jeder Tag, der kommen mag,", "tokens": ["Ein", "je\u00b7der", "Tag", ",", "der", "kom\u00b7men", "mag", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "$,", "PRELS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Macht neu mir Herzeleid.", "tokens": ["Macht", "neu", "mir", "Her\u00b7ze\u00b7leid", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PPER", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Was geht die Stadt, die Zungen hat,", "tokens": ["Was", "geht", "die", "Stadt", ",", "die", "Zun\u00b7gen", "hat", ","], "token_info": ["word",