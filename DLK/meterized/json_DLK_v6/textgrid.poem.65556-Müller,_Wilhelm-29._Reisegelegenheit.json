{"textgrid.poem.65556": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "29. Reisegelegenheit", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie schnell und leicht durchreist sich jetzt die Welt!", "tokens": ["Wie", "schnell", "und", "leicht", "durc\u00b7hreist", "sich", "jetzt", "die", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "KON", "ADJD", "VVFIN", "PRF", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Drum reitet auch die Kunst schon als Kurier.", "tokens": ["Drum", "rei\u00b7tet", "auch", "die", "Kunst", "schon", "als", "Ku\u00b7rier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADV", "ART", "NN", "ADV", "KOUS", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wer sich an ihres Pferdes Schweife h\u00e4lt,", "tokens": ["Wer", "sich", "an", "ih\u00b7res", "Pfer\u00b7des", "Schwei\u00b7fe", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "APPR", "PPOSAT", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Reist frei mit ihr als blinder Passagier.", "tokens": ["Reist", "frei", "mit", "ihr", "als", "blin\u00b7der", "Pas\u00b7sa\u00b7gier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "PPOSAT", "KOUS", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}