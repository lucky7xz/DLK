{"textgrid.poem.61568": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die M\u00f6we flog zu Nest,", "tokens": ["Die", "M\u00f6\u00b7we", "flog", "zu", "Nest", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Der Mond h\u00e4lt oben Wacht,", "tokens": ["Der", "Mond", "h\u00e4lt", "o\u00b7ben", "Wacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Des Meeres Brausen kommt", "tokens": ["Des", "Mee\u00b7res", "Brau\u00b7sen", "kommt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Von ferne durch die Nacht.", "tokens": ["Von", "fer\u00b7ne", "durch", "die", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich schreit' hinab zum Strand,", "tokens": ["Ich", "schreit'", "hin\u00b7ab", "zum", "Strand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die Seeluft streift mein Haar,", "tokens": ["Die", "See\u00b7luft", "streift", "mein", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da kommt mir's ins Gem\u00fct,", "tokens": ["Da", "kommt", "mir's", "ins", "Ge\u00b7m\u00fct", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Was jemals s\u00fc\u00df mir war.", "tokens": ["Was", "je\u00b7mals", "s\u00fc\u00df", "mir", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "PPER", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und wie die Wolken dort", "tokens": ["Und", "wie", "die", "Wol\u00b7ken", "dort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Sich rasch verwandelnd ziehn,", "tokens": ["Sich", "rasch", "ver\u00b7wan\u00b7delnd", "ziehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "VVPP", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ziehn durch die Seele mir", "tokens": ["Ziehn", "durch", "die", "See\u00b7le", "mir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Erinnrungstr\u00e4ume hin.", "tokens": ["E\u00b7rinn\u00b7rungs\u00b7tr\u00e4u\u00b7me", "hin", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Sie wechseln f\u00fcr und f\u00fcr,", "tokens": ["Sie", "wech\u00b7seln", "f\u00fcr", "und", "f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "KON", "APPR", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Sie gr\u00fc\u00dfen und zergehn;", "tokens": ["Sie", "gr\u00fc\u00b7\u00dfen", "und", "zer\u00b7gehn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dein Bild nur, wie der Mond,", "tokens": ["Dein", "Bild", "nur", ",", "wie", "der", "Mond", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Bleibt klar inmitten stehn.", "tokens": ["Bleibt", "klar", "in\u00b7mit\u00b7ten", "stehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADJA", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}