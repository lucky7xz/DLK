{"dta.poem.7114": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtung des Schlafs,  \n als  \n eine G\u00f6ttliche Wohlthat,  \n bey dem 1728sten Jahres-Wechsel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ach GOTT! Unendlichs Licht! bey Dessen Herrlichkeit", "tokens": ["Ach", "GoTT", "!", "Un\u00b7end\u00b7lichs", "Licht", "!", "bey", "Des\u00b7sen", "Herr\u00b7lich\u00b7keit"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "NE", "$.", "ADJA", "NN", "$.", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die allerdickste Dunckelheit", "tokens": ["Die", "al\u00b7ler\u00b7dicks\u00b7te", "Dun\u00b7ckel\u00b7heit"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nicht schwartz, nicht dunckel ist; bey Dem die Finsterni\u00df", "tokens": ["Nicht", "schwartz", ",", "nicht", "dun\u00b7ckel", "ist", ";", "bey", "Dem", "die", "Fins\u00b7ter\u00b7ni\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADJD", "$,", "PTKNEG", "ADJD", "VAFIN", "$.", "APPR", "ART", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nicht finster, und bey Dem die Nacht selbst, wie der Tag", "tokens": ["Nicht", "fins\u00b7ter", ",", "und", "bey", "Dem", "die", "Nacht", "selbst", ",", "wie", "der", "Tag"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "ADJD", "$,", "KON", "APPR", "ART", "ART", "NN", "ADV", "$,", "PWAV", "ART", "NN"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Verkl\u00e4rt ist, leuchtet, gl\u00e4ntzt. Gieb, da\u00df ich lehren mag", "tokens": ["Ver\u00b7kl\u00e4rt", "ist", ",", "leuch\u00b7tet", ",", "gl\u00e4ntzt", ".", "Gieb", ",", "da\u00df", "ich", "leh\u00b7ren", "mag"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "$,", "VVFIN", "$,", "VVFIN", "$.", "NN", "$,", "KOUS", "PPER", "VVINF", "VMFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Was Dir gef\u00e4llig ist! Ach! st\u00e4rcke meine Augen,", "tokens": ["Was", "Dir", "ge\u00b7f\u00e4l\u00b7lig", "ist", "!", "Ach", "!", "st\u00e4r\u00b7cke", "mei\u00b7ne", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADJD", "VAFIN", "$.", "ITJ", "$.", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Da\u00df sie, was uns bisher verholen, ungewi\u00df,", "tokens": ["Da\u00df", "sie", ",", "was", "uns", "bis\u00b7her", "ver\u00b7ho\u00b7len", ",", "un\u00b7ge\u00b7wi\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PRELS", "PPER", "ADV", "VVINF", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und kaum bemercket war, zu sehn, zu mercken taugen!", "tokens": ["Und", "kaum", "be\u00b7mer\u00b7cket", "war", ",", "zu", "sehn", ",", "zu", "mer\u00b7cken", "tau\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "VAFIN", "$,", "PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ach! la\u00df die Wunder in der Nacht,", "tokens": ["Ach", "!", "la\u00df", "die", "Wun\u00b7der", "in", "der", "Nacht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVIMP", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Absonderlich den Schlaf, wodurch Du uns begl\u00fcckest,", "tokens": ["Ab\u00b7son\u00b7der\u00b7lich", "den", "Schlaf", ",", "wo\u00b7durch", "Du", "uns", "be\u00b7gl\u00fc\u00b7ckest", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PWAV", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und fast aufs neu belebst, erfrischest und erquickest,", "tokens": ["Und", "fast", "aufs", "neu", "be\u00b7lebst", ",", "er\u00b7fri\u00b7schest", "und", "er\u00b7qui\u00b7ckest", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "ADJD", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Mir einen Spiegel seyn von Deiner Lieb und Macht!", "tokens": ["Mir", "ei\u00b7nen", "Spie\u00b7gel", "seyn", "von", "Dei\u00b7ner", "Lieb", "und", "Macht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VAINF", "APPR", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}