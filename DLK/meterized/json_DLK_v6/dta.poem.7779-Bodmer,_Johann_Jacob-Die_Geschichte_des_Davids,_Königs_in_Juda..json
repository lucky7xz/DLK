{"dta.poem.7779": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Die Geschichte des Davids,  \n K\u00f6nigs in Juda.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198429", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der David war best\u00fcrzt, die\u00df Lied mit anzuh\u00f6ren,", "tokens": ["Der", "Da\u00b7vid", "war", "be\u00b7st\u00fcrzt", ",", "die\u00df", "Lied", "mit", "an\u00b7zu\u00b7h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VAFIN", "ADJD", "$,", "PDS", "NN", "APPR", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So seine Arbeit war, und ware sein Begehreu", "tokens": ["So", "sei\u00b7ne", "Ar\u00b7beit", "war", ",", "und", "wa\u00b7re", "sein", "Be\u00b7geh\u00b7reu"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "VAFIN", "$,", "KON", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.3": {"text": "Noch gr\u00f6sser als f\u00fcrhin, zu kennen seine G\u00e4st,", "tokens": ["Noch", "gr\u00f6s\u00b7ser", "als", "f\u00fcr\u00b7hin", ",", "zu", "ken\u00b7nen", "sei\u00b7ne", "G\u00e4st", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ADV", "$,", "PTKZU", "VVINF", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Von denen eine sich also vernehmen l\u00e4st:", "tokens": ["Von", "de\u00b7nen", "ei\u00b7ne", "sich", "al\u00b7so", "ver\u00b7neh\u00b7men", "l\u00e4st", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "PRF", "ADV", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wo hast du dieses Lied, o Schwester, herbekommen?", "tokens": ["Wo", "hast", "du", "die\u00b7ses", "Lied", ",", "o", "Schwes\u00b7ter", ",", "her\u00b7be\u00b7kom\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PDAT", "NN", "$,", "FM", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ich hab es, sagte sie, dem Jonathan genommen.", "tokens": ["Ich", "hab", "es", ",", "sag\u00b7te", "sie", ",", "dem", "Jo\u00b7na\u00b7than", "ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$,", "VVFIN", "PPER", "$,", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wie? fragt die erste bald, kommt es von meinem Mann?", "tokens": ["Wie", "?", "fragt", "die", "ers\u00b7te", "bald", ",", "kommt", "es", "von", "mei\u00b7nem", "Mann", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "VVFIN", "ART", "ADJA", "ADV", "$,", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Weist du dann nicht von wem er dieses haben kan?", "tokens": ["Weist", "du", "dann", "nicht", "von", "wem", "er", "die\u00b7ses", "ha\u00b7ben", "kan", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PTKNEG", "APPR", "PWS", "PPER", "PDAT", "VAINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Des Jsai sein Sohn (sprach sie) wie er berichtet,", "tokens": ["Des", "Jsai", "sein", "Sohn", "(", "sprach", "sie", ")", "wie", "er", "be\u00b7rich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$(", "VVFIN", "PPER", "$(", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Der sch\u00f6ne David hat die\u00df Kunstgeb\u00e4nd gedichtet,", "tokens": ["Der", "sch\u00f6\u00b7ne", "Da\u00b7vid", "hat", "die\u00df", "Kunst\u00b7ge\u00b7b\u00e4nd", "ge\u00b7dich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "VAFIN", "PDS", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Der unserm Vater dient in seiner Traurigkeit,", "tokens": ["Der", "un\u00b7serm", "Va\u00b7ter", "dient", "in", "sei\u00b7ner", "Trau\u00b7rig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und den jedweder liebt, um seiner Lieblichkeit.", "tokens": ["Und", "den", "jed\u00b7we\u00b7der", "liebt", ",", "um", "sei\u00b7ner", "Lieb\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PIAT", "VVFIN", "$,", "KOUI", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Du seufzest tief, sprach die, so da bisher geschwiegen,", "tokens": ["Du", "seuf\u00b7zest", "tief", ",", "sprach", "die", ",", "so", "da", "bis\u00b7her", "ge\u00b7schwie\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "VVFIN", "ART", "$,", "ADV", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Entdeck uns deine Lieb, gelt dieser kan besiegen", "tokens": ["Ent\u00b7deck", "uns", "dei\u00b7ne", "Lieb", ",", "gelt", "die\u00b7ser", "kan", "be\u00b7sie\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "$,", "ADJD", "PDS", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Dein vorhin stoltzes Hertz, was Phalti nicht gekunt,", "tokens": ["Dein", "vor\u00b7hin", "stolt\u00b7zes", "Hertz", ",", "was", "Phal\u00b7ti", "nicht", "ge\u00b7kunt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADV", "ADJA", "NN", "$,", "PRELS", "NE", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Kan Jsai sein Sohn. Mein Seuftzen geht itzund,", "tokens": ["Kan", "Jsai", "sein", "Sohn", ".", "Mein", "Seuft\u00b7zen", "geht", "it\u00b7zund", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NE", "PPOSAT", "NN", "$.", "PPOSAT", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Die andere gab f\u00fcr, hin nach dem Trauerwesen,", "tokens": ["Die", "an\u00b7de\u00b7re", "gab", "f\u00fcr", ",", "hin", "nach", "dem", "Trau\u00b7er\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "APPR", "$,", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.18": {"text": "Das unser Vater treibt. Der, der bringt sein Genesen,", "tokens": ["Das", "un\u00b7ser", "Va\u00b7ter", "treibt", ".", "Der", ",", "der", "bringt", "sein", "Ge\u00b7ne\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "VVFIN", "$.", "ART", "$,", "PRELS", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+----", "measure": "iambic.tetra.relaxed"}, "line.19": {"text": "Die erste sprach, der ist ein wenig Ursach mit,", "tokens": ["Die", "ers\u00b7te", "sprach", ",", "der", "ist", "ein", "we\u00b7nig", "Ur\u00b7sach", "mit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "$,", "PRELS", "VAFIN", "ART", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Da\u00df deine Seuftzer gehn. Wann ich dich flei\u00dfig bitt,", "tokens": ["Da\u00df", "dei\u00b7ne", "Seuft\u00b7zer", "gehn", ".", "Wann", "ich", "dich", "flei\u00b7\u00dfig", "bitt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVINF", "$.", "PWAV", "PPER", "PRF", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Wirst du mir bergen nicht, was wir bereits versp\u00fchret,", "tokens": ["Wirst", "du", "mir", "ber\u00b7gen", "nicht", ",", "was", "wir", "be\u00b7reits", "ver\u00b7sp\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "VVFIN", "PTKNEG", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Du liebest nichts, was dir zu lieben nicht geb\u00fchret;", "tokens": ["Du", "lie\u00b7best", "nichts", ",", "was", "dir", "zu", "lie\u00b7ben", "nicht", "ge\u00b7b\u00fch\u00b7ret", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "$,", "PWS", "PPER", "PTKZU", "VVINF", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Der David zeiget schon, was aus ihm werden wird,", "tokens": ["Der", "Da\u00b7vid", "zei\u00b7get", "schon", ",", "was", "aus", "ihm", "wer\u00b7den", "wird", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "ADV", "$,", "PRELS", "APPR", "PPER", "VAINF", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Bald wird er seyn ein Held, ist er gleich schon ein Hirt.", "tokens": ["Bald", "wird", "er", "seyn", "ein", "Held", ",", "ist", "er", "gleich", "schon", "ein", "Hirt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "ART", "NN", "$,", "VAFIN", "PPER", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.25": {"text": "Der K\u00f6nig will ihm wohl, mein Jonathan ihn liebet,", "tokens": ["Der", "K\u00f6\u00b7nig", "will", "ihm", "wohl", ",", "mein", "Jo\u00b7na\u00b7than", "ihn", "lie\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "ADV", "$,", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Kein Ritter ist am Hof, der ihm nicht Zeugni\u00df giebet,", "tokens": ["Kein", "Rit\u00b7ter", "ist", "am", "Hof", ",", "der", "ihm", "nicht", "Zeug\u00b7ni\u00df", "gie\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "APPRART", "NN", "$,", "PRELS", "PPER", "PTKNEG", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Da\u00df er so k\u00fchn als sch\u00f6n, so tapfer und so wei\u00df,", "tokens": ["Da\u00df", "er", "so", "k\u00fchn", "als", "sch\u00f6n", ",", "so", "tap\u00b7fer", "und", "so", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "KOKOM", "ADJD", "$,", "ADV", "ADJD", "KON", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Da\u00df er von Jahren jung, von Witz vor Alter grei\u00df.", "tokens": ["Da\u00df", "er", "von", "Jah\u00b7ren", "jung", ",", "von", "Witz", "vor", "Al\u00b7ter", "grei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "ADJD", "$,", "APPR", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Wer wei\u00df, was er jetzund im Feld vor Thaten \u00fcbet,", "tokens": ["Wer", "wei\u00df", ",", "was", "er", "je\u00b7tzund", "im", "Feld", "vor", "Tha\u00b7ten", "\u00fc\u00b7bet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "PWS", "PPER", "ADV", "APPRART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Wie der Philister Heer sich vor ihm fl\u00fcchtig giebet,", "tokens": ["Wie", "der", "Phi\u00b7lis\u00b7ter", "Heer", "sich", "vor", "ihm", "fl\u00fcch\u00b7tig", "gie\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "PRF", "APPR", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Wie sein die Siegeskron mit Ehren schon verlangt,", "tokens": ["Wie", "sein", "die", "Sie\u00b7ges\u00b7kron", "mit", "Eh\u00b7ren", "schon", "ver\u00b7langt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ART", "NN", "APPR", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Wie er gleich als ein Held in sch\u00f6nen Waffen prangt.", "tokens": ["Wie", "er", "gleich", "als", "ein", "Held", "in", "sch\u00f6\u00b7nen", "Waf\u00b7fen", "prangt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "KOKOM", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-++--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.33": {"text": "Ach! sprach die andre drauf, Thalmais ich m\u00fcst liegen,", "tokens": ["Ach", "!", "sprach", "die", "and\u00b7re", "drauf", ",", "Thal\u00b7mais", "ich", "m\u00fcst", "lie\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "ART", "PIS", "PTKVZ", "$,", "NN", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.34": {"text": "Wann ich dir l\u00e4ugnen wolt was ich bisher verschwiegen.", "tokens": ["Wann", "ich", "dir", "l\u00e4ug\u00b7nen", "wolt", "was", "ich", "bis\u00b7her", "ver\u00b7schwie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "VVINF", "VMFIN", "PWS", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Ach es ist nur zu wahr, da\u00df David mir gef\u00e4llt,", "tokens": ["Ach", "es", "ist", "nur", "zu", "wahr", ",", "da\u00df", "Da\u00b7vid", "mir", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PPER", "VAFIN", "ADV", "PTKA", "ADJD", "$,", "KOUS", "NE", "PPER", "VVPP", "$,"], "meter": "---+-+-+-+-+", "measure": "unknown.measure.penta"}, "line.36": {"text": "Kein Mann in Jsrael, ja keinen in der Welt", "tokens": ["Kein", "Mann", "in", "Js\u00b7rael", ",", "ja", "kei\u00b7nen", "in", "der", "Welt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "APPR", "NE", "$,", "ADV", "PIAT", "APPR", "ART", "NN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.37": {"text": "Kan ich in meinem Sinn dem edeln David gleichen,", "tokens": ["Kan", "ich", "in", "mei\u00b7nem", "Sinn", "dem", "e\u00b7deln", "Da\u00b7vid", "glei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PPOSAT", "NN", "ART", "ADJA", "NE", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Er ist allein das Feur, das mein Hertz kan erweichen.", "tokens": ["Er", "ist", "al\u00b7lein", "das", "Feur", ",", "das", "mein", "Hertz", "kan", "er\u00b7wei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.39": {"text": "Mein Uebermuth ist hin, bin ich des K\u00f6nigs Kind,", "tokens": ["Mein", "Ue\u00b7ber\u00b7muth", "ist", "hin", ",", "bin", "ich", "des", "K\u00f6\u00b7nigs", "Kind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "$,", "VAFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "So ist ein Hirtenknab, bey dem mein Hertze findt", "tokens": ["So", "ist", "ein", "Hir\u00b7ten\u00b7knab", ",", "bey", "dem", "mein", "Hert\u00b7ze", "findt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "APPR", "ART", "PPOSAT", "VVFIN", "VVFIN"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.41": {"text": "Den Thron, die Ehr, die Kron. Wie gerne wolltich weiden", "tokens": ["Den", "Thron", ",", "die", "Ehr", ",", "die", "Kron", ".", "Wie", "ger\u00b7ne", "woll\u00b7tich", "wei\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$.", "PWAV", "ADV", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Mit ihm die Schaf, und mich als eine Hirtin kleiden,", "tokens": ["Mit", "ihm", "die", "Schaf", ",", "und", "mich", "als", "ei\u00b7ne", "Hir\u00b7tin", "klei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "$,", "KON", "PPER", "KOUS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "In meinen bunten Rock, ja meine K\u00f6nigszier", "tokens": ["In", "mei\u00b7nen", "bun\u00b7ten", "Rock", ",", "ja", "mei\u00b7ne", "K\u00f6\u00b7nigs\u00b7zier"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Wollt ich ohn alles Leid gleich legen weg von mir.", "tokens": ["Wollt", "ich", "ohn", 