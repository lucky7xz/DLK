{"dta.poem.9673": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "K\u00fcssen.  \n * * v. L.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jm k\u00fcssen trifft ein mensch nicht leicht das rechte ziel/", "tokens": ["Jm", "k\u00fcs\u00b7sen", "trifft", "ein", "mensch", "nicht", "leicht", "das", "rech\u00b7te", "ziel", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "VVINF", "VVFIN", "ART", "NN", "PTKNEG", "ADJD", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Je mehr man hat gek\u00fcst/ je mehr man k\u00fcssen will/", "tokens": ["Je", "mehr", "man", "hat", "ge\u00b7k\u00fcst", "/", "je", "mehr", "man", "k\u00fcs\u00b7sen", "will", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIS", "VAFIN", "VVPP", "$(", "ADV", "ADV", "PIS", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Je mehr man wird gek\u00fcst/ je mehr man w\u00fcnscht di\u00df spiel.", "tokens": ["Je", "mehr", "man", "wird", "ge\u00b7k\u00fcst", "/", "je", "mehr", "man", "w\u00fcnscht", "di\u00df", "spiel", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIS", "VAFIN", "VVPP", "$(", "ADV", "ADV", "PIS", "VVFIN", "PDS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}