{"textgrid.poem.39474": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wer Freundschaft f\u00fcr den Sch\u00f6nen konnte f\u00fchlen,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer Freundschaft f\u00fcr den Sch\u00f6nen konnte f\u00fchlen,", "tokens": ["Wer", "Freund\u00b7schaft", "f\u00fcr", "den", "Sch\u00f6\u00b7nen", "konn\u00b7te", "f\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "APPR", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die, gleich der Lieb', in Sehnsucht oft erbla\u00dfte;", "tokens": ["Die", ",", "gleich", "der", "Lieb'", ",", "in", "Sehn\u00b7sucht", "oft", "er\u00b7bla\u00df\u00b7te", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ADV", "ART", "NN", "$,", "APPR", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wer je den Ha\u00df geliebt und Liebe ha\u00dfte,", "tokens": ["Wer", "je", "den", "Ha\u00df", "ge\u00b7liebt", "und", "Lie\u00b7be", "ha\u00df\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "VVPP", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "W\u00e4hlt' Untreu buhlend ihn zu ihren Zielen:", "tokens": ["W\u00e4hlt'", "Un\u00b7treu", "buh\u00b7lend", "ihn", "zu", "ih\u00b7ren", "Zie\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADJD", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der finde hier sein Bild, und sanft entlaste", "tokens": ["Der", "fin\u00b7de", "hier", "sein", "Bild", ",", "und", "sanft", "ent\u00b7las\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADV", "PPOSAT", "NN", "$,", "KON", "ADJD", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Worte Scherz den Ernst in den Gef\u00fchlen;", "tokens": ["Der", "Wor\u00b7te", "Scherz", "den", "Ernst", "in", "den", "Ge\u00b7f\u00fch\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Gern schau er dich in zarter Lieder Spielen,", "tokens": ["Gern", "schau", "er", "dich", "in", "zar\u00b7ter", "Lie\u00b7der", "Spie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des Schauspiel gro\u00df der Welt Schauspiel umfa\u00dfte.", "tokens": ["Des", "Schau\u00b7spiel", "gro\u00df", "der", "Welt", "Schau\u00b7spiel", "um\u00b7fa\u00df\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "S\u00fc\u00df Klagen ist der Laut und Odem aller,", "tokens": ["S\u00fc\u00df", "Kla\u00b7gen", "ist", "der", "Laut", "und", "O\u00b7dem", "al\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VAFIN", "ART", "NN", "KON", "NN", "PIAT", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie meinen Perlen um Adonis Jugend,", "tokens": ["Sie", "mei\u00b7nen", "Per\u00b7len", "um", "A\u00b7do\u00b7nis", "Ju\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Lucretia's Huld, die Tod und Raub verbittert.", "tokens": ["Lu\u00b7cre\u00b7tia's", "Huld", ",", "die", "Tod", "und", "Raub", "ver\u00b7bit\u00b7tert", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ART", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Hier bist du nur ein Stab verliebter Waller,", "tokens": ["Hier", "bist", "du", "nur", "ein", "Stab", "ver\u00b7lieb\u00b7ter", "Wal\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Noch nicht der Speer, des wunderbare Tugend", "tokens": ["Noch", "nicht", "der", "Speer", ",", "des", "wun\u00b7der\u00b7ba\u00b7re", "Tu\u00b7gend"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PTKNEG", "ART", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Verwundend heilt, wenn er die B\u00fchn' ersch\u00fcttert.", "tokens": ["Ver\u00b7wun\u00b7dend", "heilt", ",", "wenn", "er", "die", "B\u00fchn'", "er\u00b7sch\u00fct\u00b7tert", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}