{"textgrid.poem.57932": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Nach Recht und Billigkeit", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer soll denn die Lasten tragen?", "tokens": ["Wer", "soll", "denn", "die", "Las\u00b7ten", "tra\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Na, doch immer der Interessent!", "tokens": ["Na", ",", "doch", "im\u00b7mer", "der", "In\u00b7ter\u00b7es\u00b7sent", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "ADV", "ART", "NN", "$."], "meter": "---+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Der Worthalter hat es gesprochen,", "tokens": ["Der", "Wort\u00b7hal\u00b7ter", "hat", "es", "ge\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-++-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Nun hat die Geschichte ein End'.", "tokens": ["Nun", "hat", "die", "Ge\u00b7schich\u00b7te", "ein", "End'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Das ging ja auch nicht mehr so weiter,", "tokens": ["Das", "ging", "ja", "auch", "nicht", "mehr", "so", "wei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADV", "PTKNEG", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da war ja das Ende von weg,", "tokens": ["Da", "war", "ja", "das", "En\u00b7de", "von", "weg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ART", "NN", "APPR", "PTKVZ", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Von dem Ein-f\u00fcr-den-andern-bezahlen,", "tokens": ["Von", "dem", "Ein\u00b7f\u00fcr\u00b7den\u00b7an\u00b7dern\u00b7be\u00b7zah\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Dabei kriegt die B\u00f6rse ein Leck.", "tokens": ["Da\u00b7bei", "kriegt", "die", "B\u00f6r\u00b7se", "ein", "Leck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "----+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "La\u00dft die Toten die Toten begraben,", "tokens": ["La\u00dft", "die", "To\u00b7ten", "die", "To\u00b7ten", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Die Armut ein Armenhaus baun,", "tokens": ["Die", "Ar\u00b7mut", "ein", "Ar\u00b7men\u00b7haus", "baun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Die Schulkinder bauen die Schulen,", "tokens": ["Die", "Schul\u00b7kin\u00b7der", "bau\u00b7en", "die", "Schu\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Die Stifte die \u00e4rmlichen Fraun.", "tokens": ["Die", "Stif\u00b7te", "die", "\u00e4rm\u00b7li\u00b7chen", "Fraun", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "Niemand soll f\u00fcrder Wassergeld zahlen,", "tokens": ["Nie\u00b7mand", "soll", "f\u00fcr\u00b7der", "Was\u00b7ser\u00b7geld", "zah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ADJA", "NN", "VVINF", "$,"], "meter": "+----+--+-", "measure": "trochaic.tri.relaxed"}, "line.2": {"text": "Wer notorisch nur Alkohol sauft,", "tokens": ["Wer", "no\u00b7to\u00b7risch", "nur", "Al\u00b7ko\u00b7hol", "sauft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "ADV", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "F\u00fcr das Rathaus hat aufzukommen,", "tokens": ["F\u00fcr", "das", "Rat\u00b7haus", "hat", "auf\u00b7zu\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Wer dahin von Amts wegen lauft.", "tokens": ["Wer", "da\u00b7hin", "von", "Amts", "we\u00b7gen", "lauft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PAV", "APPR", "NN", "APPR", "VVFIN", "$."], "meter": "-++-+--+", "measure": "iambic.tetra.chol"}}, "stanza.5": {"line.1": {"text": "Vor allem die Interessenten,", "tokens": ["Vor", "al\u00b7lem", "die", "In\u00b7ter\u00b7es\u00b7sen\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Die m\u00fcssen blechen und wie,", "tokens": ["Die", "m\u00fcs\u00b7sen", "ble\u00b7chen", "und", "wie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "VVINF", "KON", "PWAV", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Die Herren Vertreter der Firma", "tokens": ["Die", "Her\u00b7ren", "Ver\u00b7tre\u00b7ter", "der", "Fir\u00b7ma"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "ART", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Eggert, K\u00fcster und Kompanie.", "tokens": ["Eg\u00b7gert", ",", "K\u00fcs\u00b7ter", "und", "Kom\u00b7pa\u00b7nie", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "KON", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}}}}