{"dta.poem.7875": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Brautgesang .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Das Haus benedei\u2019 ich und preis\u2019 es laut,", "tokens": ["Das", "Haus", "be\u00b7ne\u00b7dei'", "ich", "und", "preis'", "es", "laut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "KON", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Das empfangen hat eine liebliche Braut;", "tokens": ["Das", "emp\u00b7fan\u00b7gen", "hat", "ei\u00b7ne", "lieb\u00b7li\u00b7che", "Braut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVPP", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Zum Garten mu\u00df es erbl\u00fchen.", "tokens": ["Zum", "Gar\u00b7ten", "mu\u00df", "es", "er\u00b7bl\u00fc\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Aus dem Brautgemach tritt eine herrliche Sonn\u2019;", "tokens": ["Aus", "dem", "Braut\u00b7ge\u00b7mach", "tritt", "ei\u00b7ne", "herr\u00b7li\u00b7che", "Sonn'", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "--+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Wie Nachtigalln locket die Fl\u00f6te,", "tokens": ["Wie", "Nach\u00b7ti\u00b7galln", "lo\u00b7cket", "die", "Fl\u00f6\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Die Tische wuchern wie Beete,", "tokens": ["Die", "Ti\u00b7sche", "wu\u00b7chern", "wie", "Bee\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KOKOM", "NE", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und es springet des Weines goldener Bronn.", "tokens": ["Und", "es", "sprin\u00b7get", "des", "Wei\u00b7nes", "gol\u00b7de\u00b7ner", "Bronn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Die Frauen ergl\u00fchen", "tokens": ["Die", "Frau\u00b7en", "er\u00b7gl\u00fc\u00b7hen"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Zu Lilien und Rosen;", "tokens": ["Zu", "Li\u00b7li\u00b7en", "und", "Ro\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wie die L\u00fcfte, die losen,", "tokens": ["Wie", "die", "L\u00fcf\u00b7te", ",", "die", "lo\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "PRELS", "VVPP", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Die durch Blumen ziehen,", "tokens": ["Die", "durch", "Blu\u00b7men", "zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Rauschet das K\u00fcssen und Kosen.", "tokens": ["Rau\u00b7schet", "das", "K\u00fcs\u00b7sen", "und", "Ko\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}}}}