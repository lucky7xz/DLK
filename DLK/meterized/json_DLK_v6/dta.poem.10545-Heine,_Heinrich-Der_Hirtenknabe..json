{"dta.poem.10545": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Hirtenknabe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "K\u00f6nig ist der Hirtenknabe,", "tokens": ["K\u00f6\u00b7nig", "ist", "der", "Hir\u00b7ten\u00b7kna\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gr\u00fcner H\u00fcgel ist sein Thron,", "tokens": ["Gr\u00fc\u00b7ner", "H\u00fc\u00b7gel", "ist", "sein", "Thron", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ueber seinem Haupt die Sonne", "tokens": ["Ue\u00b7ber", "sei\u00b7nem", "Haupt", "die", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist die gro\u00dfe, goldne Kron'.", "tokens": ["Ist", "die", "gro\u00b7\u00dfe", ",", "gold\u00b7ne", "Kron'", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ihm zu F\u00fc\u00dfen liegen Schafe,", "tokens": ["Ihm", "zu", "F\u00fc\u00b7\u00dfen", "lie\u00b7gen", "Scha\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weiche Schmeichler, rothbekreuzt;", "tokens": ["Wei\u00b7che", "Schmeich\u00b7ler", ",", "roth\u00b7be\u00b7kreuzt", ";"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kavaliere sind die K\u00e4lber,", "tokens": ["Ka\u00b7va\u00b7lie\u00b7re", "sind", "die", "K\u00e4l\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und sie wandeln stolzgespreizt.", "tokens": ["Und", "sie", "wan\u00b7deln", "stolz\u00b7ge\u00b7spreizt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Hofschauspieler sind die B\u00f6cklein,", "tokens": ["Hof\u00b7schau\u00b7spie\u00b7ler", "sind", "die", "B\u00f6c\u00b7klein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Und die V\u00f6gel ", "tokens": ["Und", "die", "V\u00f6\u00b7gel"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "--+-", "measure": "anapaest.init"}, "line.3": {"text": "Mit den Fl\u00f6ten, mit den Gl\u00f6cklein,", "tokens": ["Mit", "den", "Fl\u00f6\u00b7ten", ",", "mit", "den", "Gl\u00f6c\u00b7klein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sind die Kammermusizi.", "tokens": ["Sind", "die", "Kam\u00b7mer\u00b7mu\u00b7si\u00b7zi", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und das klingt und singt so lieblich,", "tokens": ["Und", "das", "klingt", "und", "singt", "so", "lieb\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "KON", "VVFIN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und so lieblich rauschen drein", "tokens": ["Und", "so", "lieb\u00b7lich", "rau\u00b7schen", "drein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wasserfall und Tannenb\u00e4ume,", "tokens": ["Was\u00b7ser\u00b7fall", "und", "Tan\u00b7nen\u00b7b\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und der K\u00f6nig schlummert ein.", "tokens": ["Und", "der", "K\u00f6\u00b7nig", "schlum\u00b7mert", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Unterdessen mu\u00df regieren", "tokens": ["Un\u00b7ter\u00b7des\u00b7sen", "mu\u00df", "re\u00b7gie\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der Minister, jener Hund,", "tokens": ["Der", "Mi\u00b7nis\u00b7ter", ",", "je\u00b7ner", "Hund", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PDAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dessen knurriges Gebelle", "tokens": ["Des\u00b7sen", "knur\u00b7ri\u00b7ges", "Ge\u00b7bel\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["PDAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wiederhallet in der Rund'.", "tokens": ["Wie\u00b7der\u00b7hal\u00b7let", "in", "der", "Rund'", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Schl\u00e4frig lallt der junge K\u00f6nig:", "tokens": ["Schl\u00e4f\u00b7rig", "lallt", "der", "jun\u00b7ge", "K\u00f6\u00b7nig", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u201edas Regieren ist so schwer,", "tokens": ["\u201e", "das", "Re\u00b7gie\u00b7ren", "ist", "so", "schwer", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach, ich wollt', da\u00df ich zu Hause", "tokens": ["Ach", ",", "ich", "wollt'", ",", "da\u00df", "ich", "zu", "Hau\u00b7se"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PPER", "VMFIN", "$,", "KOUS", "PPER", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schon bei meiner K\u00f6n'gin w\u00e4r'!", "tokens": ["Schon", "bei", "mei\u00b7ner", "K\u00f6n'\u00b7gin", "w\u00e4r'", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "\u201ein den Armen meiner K\u00f6n'gin", "tokens": ["\u201e", "in", "den", "Ar\u00b7men", "mei\u00b7ner", "K\u00f6n'\u00b7gin"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ruht mein K\u00f6nigshaupt so weich,", "tokens": ["Ruht", "mein", "K\u00f6\u00b7nigs\u00b7haupt", "so", "weich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und in ihren lieben Augen", "tokens": ["Und", "in", "ih\u00b7ren", "lie\u00b7ben", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Liegt mein unerme\u00dflich Reich!\u201c", "tokens": ["Liegt", "mein", "un\u00b7er\u00b7me\u00df\u00b7lich", "Reich", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJD", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}