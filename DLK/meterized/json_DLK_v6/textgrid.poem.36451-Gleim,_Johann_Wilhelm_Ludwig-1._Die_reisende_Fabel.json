{"textgrid.poem.36451": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1. Die reisende Fabel", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die arme Tochter des \u00c4sop,", "tokens": ["Die", "ar\u00b7me", "Toch\u00b7ter", "des", "\u00c4\u00b7sop", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Fabel, reiste von Athen,", "tokens": ["Die", "Fa\u00b7bel", ",", "reis\u00b7te", "von", "A\u00b7then", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "APPR", "NE", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Entfernte L\u00e4nder zu besehn.", "tokens": ["Ent\u00b7fern\u00b7te", "L\u00e4n\u00b7der", "zu", "be\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wer sie erblickte, der erhob", "tokens": ["Wer", "sie", "er\u00b7blick\u00b7te", ",", "der", "er\u00b7hob"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "PPER", "VVFIN", "$,", "PRELS", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr Wesen, ihren Gang,", "tokens": ["Ihr", "We\u00b7sen", ",", "ih\u00b7ren", "Gang", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und ihren Anzug. Nicht zu lang", "tokens": ["Und", "ih\u00b7ren", "An\u00b7zug", ".", "Nicht", "zu", "lang"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "$.", "PTKNEG", "PTKA", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und nicht zu kurz, war er bequem:", "tokens": ["Und", "nicht", "zu", "kurz", ",", "war", "er", "be\u00b7quem", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "PTKA", "ADJD", "$,", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wohin sie kam, da war sie angenehm.", "tokens": ["Wo\u00b7hin", "sie", "kam", ",", "da", "war", "sie", "an\u00b7ge\u00b7nehm", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "ADV", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Zu Rom schenkt ihr ein feinres Kleid", "tokens": ["Zu", "Rom", "schenkt", "ihr", "ein", "fein\u00b7res", "Kleid"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.2": {"text": "Ein Freigelassener", "tokens": ["Ein", "Frei\u00b7ge\u00b7las\u00b7se\u00b7ner"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.3": {"text": "Es stand ihr wohl, es war gemacht", "tokens": ["Es", "stand", "ihr", "wohl", ",", "es", "war", "ge\u00b7macht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "PPER", "VAFIN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nett, aber ohne Pracht!", "tokens": ["Nett", ",", "a\u00b7ber", "oh\u00b7ne", "Pracht", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Dann reiste sie darin, noch bl\u00f6de, nach Paris;", "tokens": ["Dann", "reis\u00b7te", "sie", "da\u00b7rin", ",", "noch", "bl\u00f6\u00b7de", ",", "nach", "Pa\u00b7ris", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PAV", "$,", "ADV", "ADJA", "$,", "APPR", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein edler Ritter", "tokens": ["Ein", "ed\u00b7ler", "Rit\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Das wohlerzogne Kind, das seine Freundin ward,", "tokens": ["Das", "woh\u00b7ler\u00b7zog\u00b7ne", "Kind", ",", "das", "sei\u00b7ne", "Freun\u00b7din", "ward", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "In Sitten und in Putz, nach seiner Landesart.", "tokens": ["In", "Sit\u00b7ten", "und", "in", "Putz", ",", "nach", "sei\u00b7ner", "Lan\u00b7des\u00b7art", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Auch nahm er einst sie mit, in einer Gallanacht,", "tokens": ["Auch", "nahm", "er", "einst", "sie", "mit", ",", "in", "ei\u00b7ner", "Gal\u00b7la\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PPER", "PTKVZ", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "An Ludwigs Hof, in Hofestracht.", "tokens": ["An", "Lud\u00b7wigs", "Hof", ",", "in", "Ho\u00b7fest\u00b7racht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "$,", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und weil der jungen Maintenon", "tokens": ["Und", "weil", "der", "jun\u00b7gen", "Main\u00b7te\u00b7non"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An Geist und Sch\u00f6nheit sie vollkommen glich,", "tokens": ["An", "Geist", "und", "Sch\u00f6n\u00b7heit", "sie", "voll\u00b7kom\u00b7men", "glich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So zog sie allsobald des K\u00f6nigs Aug' auf sich.", "tokens": ["So", "zog", "sie", "all\u00b7so\u00b7bald", "des", "K\u00f6\u00b7nigs", "Aug'", "auf", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "NN", "APPR", "PRF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Was hatte sie davon?", "tokens": ["Was", "hat\u00b7te", "sie", "da\u00b7von", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er r\u00fchmte sie den Prinzen, sie gefiel!", "tokens": ["Er", "r\u00fchm\u00b7te", "sie", "den", "Prin\u00b7zen", ",", "sie", "ge\u00b7fiel", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$,", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und einst beim Spiel,", "tokens": ["Und", "einst", "beim", "Spiel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Nannt' er, in Gnaden, sie: die ", "tokens": ["Nannt'", "er", ",", "in", "Gna\u00b7den", ",", "sie", ":", "die"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word"], "pos": ["VVFIN", "PPER", "$,", "APPR", "NN", "$,", "PPER", "$.", "ART"], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.6": {"line.1": {"text": "Ich? Ihro Majest\u00e4t! ich bin", "tokens": ["Ich", "?", "Ih\u00b7ro", "Ma\u00b7jes\u00b7t\u00e4t", "!", "ich", "bin"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "$.", "PPOSAT", "NN", "$.", "PPER", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nur eine ", "tokens": ["Nur", "ei\u00b7ne"], "token_info": ["word", "word"], "pos": ["ADV", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Mich h\u00f6ren Kinder nur so gern!", "tokens": ["Mich", "h\u00f6\u00b7ren", "Kin\u00b7der", "nur", "so", "gern", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich? Lehrerin? der Menschen? das sei fern!", "tokens": ["Ich", "?", "Leh\u00b7re\u00b7rin", "?", "der", "Men\u00b7schen", "?", "das", "sei", "fern", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$.", "NN", "$.", "ART", "NN", "$.", "PDS", "VAFIN", "ADJD", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}}}}