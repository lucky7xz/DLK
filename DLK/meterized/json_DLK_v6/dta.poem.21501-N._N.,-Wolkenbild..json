{"dta.poem.21501": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Wolkenbild.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "D\u00fcstergraue", "tokens": ["D\u00fcs\u00b7ter\u00b7grau\u00b7e"], "token_info": ["word"], "pos": ["NE"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Wolken ragen", "tokens": ["Wol\u00b7ken", "ra\u00b7gen"], "token_info": ["word", "word"], "pos": ["NN", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Trotzig auf,", "tokens": ["Trot\u00b7zig", "auf", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Felsen gleich.", "tokens": ["Fel\u00b7sen", "gleich", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADV", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Naht mit Brausen", "tokens": ["Naht", "mit", "Brau\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPR", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Sturmessausen", "tokens": ["Stur\u00b7mess\u00b7au\u00b7sen"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "F\u00e4hrt in\u2019s Wolkengebirg.", "tokens": ["F\u00e4hrt", "in's", "Wol\u00b7ken\u00b7ge\u00b7birg", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "Und die Berge zerbrechen,", "tokens": ["Und", "die", "Ber\u00b7ge", "zer\u00b7bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVINF", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.9": {"t