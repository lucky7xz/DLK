{"dta.poem.7562": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "61.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein ganzer Fr\u00fchling w\u00e4chst mit einmal aus der Erden;", "tokens": ["Ein", "gan\u00b7zer", "Fr\u00fch\u00b7ling", "w\u00e4chst", "mit", "ein\u00b7mal", "aus", "der", "Er\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was Menschen wirken, kann nur Eins ums Andre werden.", "tokens": ["Was", "Men\u00b7schen", "wir\u00b7ken", ",", "kann", "nur", "Eins", "ums", "And\u00b7re", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVINF", "$,", "VMFIN", "ADV", "NN", "APPRART", "PIS", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch wer beim Wirken fest h\u00e4lt einen Gotteshauch,", "tokens": ["Doch", "wer", "beim", "Wir\u00b7ken", "fest", "h\u00e4lt", "ei\u00b7nen", "Got\u00b7tes\u00b7hauch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPRART", "NN", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des Einzles wird zuletzt ein ganzer Fr\u00fchling auch.", "tokens": ["Des", "Einz\u00b7les", "wird", "zu\u00b7letzt", "ein", "gan\u00b7zer", "Fr\u00fch\u00b7ling", "auch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VAFIN", "ADV", "ART", "ADJA", "NN",