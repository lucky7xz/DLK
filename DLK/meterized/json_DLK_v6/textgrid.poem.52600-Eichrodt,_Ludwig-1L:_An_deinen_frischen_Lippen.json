{"textgrid.poem.52600": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: An deinen frischen Lippen", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "An deinen frischen Lippen", "tokens": ["An", "dei\u00b7nen", "fri\u00b7schen", "Lip\u00b7pen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hing ich so gl\u00fchend und so bang \u2013", "tokens": ["Hing", "ich", "so", "gl\u00fc\u00b7hend", "und", "so", "bang", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "KON", "ADV", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So manches liebe manches Mal \u2013", "tokens": ["So", "man\u00b7ches", "lie\u00b7be", "man\u00b7ches", "Mal", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "PIAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da ging mir durch die Seele tief", "tokens": ["Da", "ging", "mir", "durch", "die", "See\u00b7le", "tief"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein wunderbarer Drang.", "tokens": ["Ein", "wun\u00b7der\u00b7ba\u00b7rer", "Drang", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "In deinen sch\u00f6nen Gliedern", "tokens": ["In", "dei\u00b7nen", "sch\u00f6\u00b7nen", "Glie\u00b7dern"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Entschlummert ich mit hei\u00dfem Blut", "tokens": ["Ent\u00b7schlum\u00b7mert", "ich", "mit", "hei\u00b7\u00dfem", "Blut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So manches liebe manches Mal,", "tokens": ["So", "man\u00b7ches", "lie\u00b7be", "man\u00b7ches", "Mal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da ward mir, o so todess\u00fc\u00df,", "tokens": ["Da", "ward", "mir", ",", "o", "so", "to\u00b7des\u00b7s\u00fc\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "FM", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So \u00fcberwohl zu Muth.", "tokens": ["So", "\u00fc\u00b7ber\u00b7wohl", "zu", "Muth", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Warum in deinen Blicken", "tokens": ["Wa\u00b7rum", "in", "dei\u00b7nen", "Bli\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Kein hold Verlangen nimmermehr?", "tokens": ["Kein", "hold", "Ver\u00b7lan\u00b7gen", "nim\u00b7mer\u00b7mehr", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJD", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O wehe mir, ach, nimmermehr!", "tokens": ["O", "we\u00b7he", "mir", ",", "ach", ",", "nim\u00b7mer\u00b7mehr", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$,", "ITJ", "$,", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da zieht es auch in meine Brust", "tokens": ["Da", "zieht", "es", "auch", "in", "mei\u00b7ne", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So todt, so freudenleer.", "tokens": ["So", "todt", ",", "so", "freu\u00b7den\u00b7leer", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}