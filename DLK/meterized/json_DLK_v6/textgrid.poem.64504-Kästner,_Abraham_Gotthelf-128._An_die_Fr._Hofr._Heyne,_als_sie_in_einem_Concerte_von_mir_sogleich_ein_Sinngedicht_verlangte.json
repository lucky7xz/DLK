{"textgrid.poem.64504": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "128. An die Fr. Hofr. Heyne, als sie in einem Concerte von mir sogleich ein Sinngedicht verlangte", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du, die den Werth von Witz und Tonkunst sch\u00e4tzet,", "tokens": ["Du", ",", "die", "den", "Werth", "von", "Witz", "und", "Ton\u00b7kunst", "sch\u00e4t\u00b7zet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ART", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "F\u00fcr beyde f\u00fchlt, durch beyde selbst erg\u00f6tzet,", "tokens": ["F\u00fcr", "bey\u00b7de", "f\u00fchlt", ",", "durch", "bey\u00b7de", "selbst", "er\u00b7g\u00f6t\u00b7zet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVFIN", "$,", "APPR", "PIS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du wei\u00dft, gezwungen singt ein Virtuose nicht,", "tokens": ["Du", "wei\u00dft", ",", "ge\u00b7zwun\u00b7gen", "singt", "ein", "Vir\u00b7tu\u00b7o\u00b7se", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVPP", "VVFIN", "ART", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Warum befahlst du denn mir gleich ein Sinngedicht?", "tokens": ["Wa\u00b7rum", "be\u00b7fahlst", "du", "denn", "mir", "gleich", "ein", "Sinn\u00b7ge\u00b7dicht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}