{"textgrid.poem.46139": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Ueber den fr\u00fchen tod Fr\u00e4uleins Anna Augusta Marggr\u00e4fin zu Baden", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein leben, dessen end uns plaget,", "tokens": ["Dein", "le\u00b7ben", ",", "des\u00b7sen", "end", "uns", "pla\u00b7get", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVINF", "$,", "PRELAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "war wie ein tag, sch\u00f6n und nicht lang,", "tokens": ["war", "wie", "ein", "tag", ",", "sch\u00f6n", "und", "nicht", "lang", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "ART", "NN", "$,", "ADJD", "KON", "PTKNEG", "ADJD", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "wie ein stern vor des tags aufgang,", "tokens": ["wie", "ein", "stern", "vor", "des", "tags", "auf\u00b7gang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "die r\u00f6tin, wehrend weil es taget,", "tokens": ["die", "r\u00f6\u00b7tin", ",", "weh\u00b7rend", "weil", "es", "ta\u00b7get", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "ein seufz aus einer edlen brust,", "tokens": ["ein", "seufz", "aus", "ei\u00b7ner", "ed\u00b7len", "brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "ein klag aus lieb, nicht aus unlust,", "tokens": ["ein", "klag", "aus", "lieb", ",", "nicht", "aus", "un\u00b7lust", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJD", "$,", "PTKNEG", "APPR", "NN", "$,"], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.7": {"text": "ein nebel, den die sonn verjaget.", "tokens": ["ein", "ne\u00b7bel", ",", "den", "die", "sonn", "ver\u00b7ja\u00b7get", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "ADV", "VVFIN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Ein staub, der mit dem wind entstehet,", "tokens": ["Ein", "staub", ",", "der", "mit", "dem", "wind", "ent\u00b7ste\u00b7het", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "ein thau an einer hitz anbruch,", "tokens": ["ein", "thau", "an", "ei\u00b7ner", "hitz", "an\u00b7bruch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "ein luft mit lieblichem geruch,", "tokens": ["ein", "luft", "mit", "lieb\u00b7li\u00b7chem", "ge\u00b7ruch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "ein schnee, der fr\u00fchlingszeit abgehet,", "tokens": ["ein", "schnee", ",", "der", "fr\u00fch\u00b7lings\u00b7zeit", "ab\u00b7ge\u00b7het", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "die blum, die frisch und welk zugleich.", "tokens": ["die", "blum", ",", "die", "frisch", "und", "welk", "zu\u00b7gleich", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$,", "PRELS", "ADJD", "KON", "ADJD", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "ein regenbog von farben reich", "tokens": ["ein", "re\u00b7gen\u00b7bog", "von", "far\u00b7ben", "reich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "VVFIN", "APPR", "NN", "ADJD"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "ein zweig, den bald der wind umwehet.", "tokens": ["ein", "zweig", ",", "den", "bald", "der", "wind", "um\u00b7we\u00b7het", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "$,", "PRELS", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ein schaur in sommerszeit vergossen,", "tokens": ["Ein", "schaur", "in", "som\u00b7mers\u00b7zeit", "ver\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "ein eis am hei\u00dfen sonnenschein,", "tokens": ["ein", "eis", "am", "hei\u00b7\u00dfen", "son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "ein glas, so br\u00fcchig als es rein,", "tokens": ["ein", "glas", ",", "so", "br\u00fc\u00b7chig", "als", "es", "rein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ADJD", "KOKOM", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "ein wasser \u00fcber nacht verflossen,", "tokens": ["ein", "was\u00b7ser", "\u00fc\u00b7ber", "nacht", "ver\u00b7flos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "ein blitz zumal geschwind und hell", "tokens": ["ein", "blitz", "zu\u00b7mal", "ge\u00b7schwind", "und", "hell"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "ein stral abschie\u00dfend klar und hell", "tokens": ["ein", "stral", "ab\u00b7schie\u00b7\u00dfend", "klar", "und", "hell"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADJD", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "und ein gel\u00e4chter bald beschlossen.", "tokens": ["und", "ein", "ge\u00b7l\u00e4ch\u00b7ter", "bald", "be\u00b7schlos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ein stim, die lieblich dahin f\u00e4hret,", "tokens": ["Ein", "stim", ",", "die", "lieb\u00b7lich", "da\u00b7hin", "f\u00e4h\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PRELS", "ADJD", "PAV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "ein widerhall der stim in eil,", "tokens": ["ein", "wi\u00b7der\u00b7hall", "der", "stim", "in", "eil", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "ein zeit, vertriben mit kurzweil", "tokens": ["ein", "zeit", ",", "ver\u00b7tri\u00b7ben", "mit", "kurz\u00b7weil"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "ein traum, der mit dem schlaf aufh\u00f6ret,", "tokens": ["ein", "traum", ",", "der", "mit", "dem", "schlaf", "auf\u00b7h\u00f6\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "ein flug des vogels mit begir,", "tokens": ["ein", "flug", "des", "vo\u00b7gels", "mit", "be\u00b7gir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "ein schat, wan die sonn sticht herf\u00fcr,", "tokens": ["ein", "schat", ",", "wan", "die", "sonn", "sticht", "her\u00b7f\u00fcr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "$,", "PWAV", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.7": {"text": "ein rauch, von starkem wind zust\u00f6ret.", "tokens": ["ein", "rauch", ",", "von", "star\u00b7kem", "wind", "zu\u00b7st\u00f6\u00b7ret", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "$,", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Also dein leben, schnell verflogen,", "tokens": ["Al\u00b7so", "dein", "le\u00b7ben", ",", "schnell", "ver\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "VVINF", "$,", "ADJD", "VVPP", "$,"], "meter": "+----+-+-", "measure": "dactylic.init"}, "line.2": {"text": "hat sich nicht anderst, dan ein tag,", "tokens": ["hat", "sich", "nicht", "an\u00b7derst", ",", "dan", "ein", "tag", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "PTKNEG", "ADV", "$,", "ADV", "ART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "stern, morgenr\u00f6t, seufz, nebel, klag,", "tokens": ["stern", ",", "mor\u00b7gen\u00b7r\u00f6t", ",", "seufz", ",", "ne\u00b7bel", ",", "klag", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "FM.fr", "$,", "ADJD", "$,", "VVFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "staub, thau, luft, schnee, blum, regenbogen,", "tokens": ["staub", ",", "thau", ",", "luft", ",", "schnee", ",", "blum", ",", "re\u00b7gen\u00b7bo\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,", "NE", "$,", "ADJA", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "zweig, schaur, eis, glas, blitz, wasserfall,", "tokens": ["zweig", ",", "schaur", ",", "eis", ",", "glas", ",", "blitz", ",", "was\u00b7ser\u00b7fall", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "ADJD", "$,", "ADJD", "$,", "VVFIN", "$,", "VVIMP", "$,", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "stral, stim, gel\u00e4chter, widerhall,", "tokens": ["stral", ",", "stim", ",", "ge\u00b7l\u00e4ch\u00b7ter", ",", "wi\u00b7der\u00b7hall", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "VVFIN", "$,", "ADJA", "$,", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "zeit, traum, flug, fchat und rauch verzogen.", "tokens": ["zeit", ",", "traum", ",", "flug", ",", "fchat", "und", "rauch", "ver\u00b7zo\u00b7gen", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "VVFIN", "$,", "VVFIN", "KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}