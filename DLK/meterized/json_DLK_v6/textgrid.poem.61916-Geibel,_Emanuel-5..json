{"textgrid.poem.61916": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ins Lager nun zum Kampf geschm\u00fcckt", "tokens": ["Ins", "La\u00b7ger", "nun", "zum", "Kampf", "ge\u00b7schm\u00fcckt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "APPRART", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sind die Geschwader einger\u00fcckt,", "tokens": ["Sind", "die", "Ge\u00b7schwa\u00b7der", "ein\u00b7ge\u00b7r\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und vor dem Zelt des Kaisers weht", "tokens": ["Und", "vor", "dem", "Zelt", "des", "Kai\u00b7sers", "weht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Banner, drin der Engel steht.", "tokens": ["Das", "Ban\u00b7ner", ",", "drin", "der", "En\u00b7gel", "steht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch dr\u00fcben, wo das breite Feld", "tokens": ["Doch", "dr\u00fc\u00b7ben", ",", "wo", "das", "brei\u00b7te", "Feld"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Halbmonds Sichel tr\u00fcb erhellt,", "tokens": ["Des", "Halb\u00b7monds", "Si\u00b7chel", "tr\u00fcb", "er\u00b7hellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Liegt, zahllos wie der Sand am Meer,", "tokens": ["Liegt", ",", "zahl\u00b7los", "wie", "der", "Sand", "am", "Meer", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJD", "KOKOM", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Drachenkn\u00e4ul, das Ungarheer.", "tokens": ["Ein", "Dra\u00b7chen\u00b7kn\u00e4ul", ",", "das", "Un\u00b7gar\u00b7heer", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Da w\u00fchlt und wimmelt Hauf an Hauf,", "tokens": ["Da", "w\u00fchlt", "und", "wim\u00b7melt", "Hauf", "an", "Hauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vieltausend Feuer flackern auf,", "tokens": ["Viel\u00b7tau\u00b7send", "Feu\u00b7er", "fla\u00b7ckern", "auf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Unheimlich durch den roten Dampf", "tokens": ["Un\u00b7heim\u00b7lich", "durch", "den", "ro\u00b7ten", "Dampf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dr\u00f6hnt Erzgeklirr und Hufgestampf.", "tokens": ["Dr\u00f6hnt", "Erz\u00b7ge\u00b7klirr", "und", "Huf\u00b7ge\u00b7stampf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ro\u00dfschweife flattern wild und fremd,", "tokens": ["Ro\u00df\u00b7schwei\u00b7fe", "flat\u00b7tern", "wild", "und", "fremd", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "ADJD", "KON", "ADJD", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Der Stierhelm glei\u00dft, das Schuppenhemd,", "tokens": ["Der", "Stier\u00b7helm", "glei\u00dft", ",", "das", "Schup\u00b7pen\u00b7hemd", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In Sch\u00e4delbechern kreist der Wein,", "tokens": ["In", "Sch\u00e4\u00b7del\u00b7be\u00b7chern", "kreist", "der", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und gelle Lieder schallen drein:", "tokens": ["Und", "gel\u00b7le", "Lie\u00b7der", "schal\u00b7len", "drein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}