{"textgrid.poem.28440": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "55.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Viel M\u00f6rder pflegen uns im Wandel zu umgeben,", "tokens": ["Viel", "M\u00f6r\u00b7der", "pfle\u00b7gen", "uns", "im", "Wan\u00b7del", "zu", "um\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu rauben Gut und Blut, zu rauben beydes Leben.", "tokens": ["Zu", "rau\u00b7ben", "Gut", "und", "Blut", ",", "zu", "rau\u00b7ben", "bey\u00b7des", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ADJD", "KON", "NN", "$,", "PTKZU", "VVINF", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "De\u00df Samariters Wein und heilsam Oele macht,", "tokens": ["De\u00df", "Sa\u00b7ma\u00b7ri\u00b7ters", "Wein", "und", "heil\u00b7sam", "O\u00b7e\u00b7le", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "ADJD", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Wiewol wir sind verwund, da\u00df wir nicht sind verschmacht.", "tokens": ["Wie\u00b7wol", "wir", "sind", "ver\u00b7wund", ",", "da\u00df", "wir", "nicht", "sind", "ver\u00b7schmacht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "VVPP", "$,", "KOUS", "PPER", "PTKNEG", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+