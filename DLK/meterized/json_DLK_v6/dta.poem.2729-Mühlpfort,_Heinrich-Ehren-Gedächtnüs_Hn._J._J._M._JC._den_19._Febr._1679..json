{"dta.poem.2729": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Ehren-Ged\u00e4chtn\u00fcs/  \n Hn. J. J.   M. JC.   den 19. Febr. 1679.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Tod/ so alles zwingt/ zwingt nicht gelehrte Leute/", "tokens": ["Der", "Tod", "/", "so", "al\u00b7les", "zwingt", "/", "zwingt", "nicht", "ge\u00b7lehr\u00b7te", "Leu\u00b7te", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADV", "PIS", "VVFIN", "$(", "VVFIN", "PTKNEG", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es frist das schwartze Grab nit ihren lichten Ruhm/", "tokens": ["Es", "frist", "das", "schwart\u00b7ze", "Grab", "nit", "ih\u00b7ren", "lich\u00b7ten", "Ruhm", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "PTKNEG", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und nim\u0303t er gleich den Leib/ die Hand voll Staub zur", "tokens": ["Und", "nim\u0303t", "er", "gleich", "den", "Leib", "/", "die", "Hand", "voll", "Staub", "zur"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "NN", "$(", "ART", "NN", "ADJD", "NN", "APPRART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein ewiges Ger\u00fccht bleibt doch ihr Eigenthum.", "tokens": ["Ein", "e\u00b7wi\u00b7ges", "Ge\u00b7r\u00fccht", "bleibt", "doch", "ihr", "Ei\u00b7gen\u00b7thum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sie fallen wie die Sonn/ eh die noch gar versincket/", "tokens": ["Sie", "fal\u00b7len", "wie", "die", "Sonn", "/", "eh", "die", "noch", "gar", "ver\u00b7sin\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "ART", "NN", "$(", "KOUS", "ART", "ADV", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und in der blauen See die m\u00fcden Pferde w\u00e4scht/", "tokens": ["Und", "in", "der", "blau\u00b7en", "See", "die", "m\u00fc\u00b7den", "Pfer\u00b7de", "w\u00e4scht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So sieht man wie ihr Glantz im h\u00f6chsten Golde blincket/", "tokens": ["So", "sieht", "man", "wie", "ihr", "Glantz", "im", "h\u00f6chs\u00b7ten", "Gol\u00b7de", "blin\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "KOKOM", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und wie sie Purpur-roth/ wenn sie nunmehr verlescht;", "tokens": ["Und", "wie", "sie", "Pur\u00b7pur\u00b7roth", "/", "wenn", "sie", "nun\u00b7mehr", "ver\u00b7lescht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "NN", "$(", "KOUS", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "So auch der Sinnen Feu\u2019r/ die Fackel de\u00df Verstandes", "tokens": ["So", "auch", "der", "Sin\u00b7nen", "Feu'r", "/", "die", "Fa\u00b7ckel", "de\u00df", "Ver\u00b7stan\u00b7des"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN", "NN", "$(", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Das Licht so vielen oft zu Dienst und Nutzenschien/", "tokens": ["Das", "Licht", "so", "vie\u00b7len", "oft", "zu", "Dienst", "und", "Nut\u00b7zen\u00b7schien", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PIAT", "ADV", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Verdr\u00fccket nicht die Last de\u00df d\u00fcrren Leichen Sandes/", "tokens": ["Ver\u00b7dr\u00fc\u00b7cket", "nicht", "die", "Last", "de\u00df", "d\u00fcr\u00b7ren", "Lei\u00b7chen", "San\u00b7des", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "NN", "ART", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Man siehet seine Flamm auch bey der Nachwelt gl\u00fchn.", "tokens": ["Man", "sie\u00b7het", "sei\u00b7ne", "Flamm", "auch", "bey", "der", "Nach\u00b7welt", "gl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPOSAT", "NN", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Da weisen kluge K\u00f6pff erst ihrer Wei\u00dfheit Strahlen/", "tokens": ["Da", "wei\u00b7sen", "klu\u00b7ge", "K\u00f6pff", "erst", "ih\u00b7rer", "Wei\u00df\u00b7heit", "Strah\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJA", "NN", "ADV", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Wenn sie die Demmerung des Neides nicht mehr sticht.", "tokens": ["Wenn", "sie", "die", "Dem\u00b7me\u00b7rung", "des", "Nei\u00b7des", "nicht", "mehr", "sticht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "PTKNEG", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Wenn sie die letzte Schuld zwar der Natur bezahlen/", "tokens": ["Wenn", "sie", "die", "letz\u00b7te", "Schuld", "zwar", "der", "Na\u00b7tur", "be\u00b7zah\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "ADV", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Hingegen Ehr und Ruhm den Lorber-Krantz drauf flicht.", "tokens": ["Hin\u00b7ge\u00b7gen", "Ehr", "und", "Ruhm", "den", "Lor\u00b7ber\u00b7Krantz", "drauf", "flicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "ART", "NN", "PAV", "VVFIN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.17": {"text": "Ein solcher Unterscheid ist zwischen einem Grabe/", "tokens": ["Ein", "sol\u00b7cher", "Un\u00b7ter\u00b7scheid", "ist", "zwi\u00b7schen", "ei\u00b7nem", "Gra\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VAFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Das einen klugen Kopff und albern Tropffen deckt.", "tokens": ["Das", "ei\u00b7nen", "klu\u00b7gen", "Kopff", "und", "al\u00b7bern", "Tropf\u00b7fen", "deckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "ADJA", "NN", "KON", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Den tilgt mit seinem Thun Verwesung/ Wurm und Schabe/", "tokens": ["Den", "tilgt", "mit", "sei\u00b7nem", "Thun", "Ver\u00b7we\u00b7sung", "/", "Wurm", "und", "Scha\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "$(", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Wenn jenem wird ein Licht der Ehren auffgesteckt.", "tokens": ["Wenn", "je\u00b7nem", "wird", "ein", "Licht", "der", "Eh\u00b7ren", "auff\u00b7ge\u00b7steckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "VAFIN", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Es mag das stoltze Rom ein ewig Feuer hegen/", "tokens": ["Es", "mag", "das", "stolt\u00b7ze", "Rom", "ein", "e\u00b7wig", "Feu\u00b7er", "he\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PDS", "VVFIN", "NE", "ART", "ADJD", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Und mehr als hundert Jahr in ", "tokens": ["Und", "mehr", "als", "hun\u00b7dert", "Jahr", "in"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "KOKOM", "CARD", "NN", "APPR"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.23": {"text": "Hier will die Ewigkeit den Zunder selbsten legen/", "tokens": ["Hier", "will", "die", "E\u00b7wig\u00b7keit", "den", "Zun\u00b7der", "selbs\u00b7ten", "le\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "ART", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Die den Gelehrten pflegt ein Denckmahl auffzubaun.", "tokens": ["Die", "den", "Ge\u00b7lehr\u00b7ten", "pflegt", "ein", "Denck\u00b7mahl", "auff\u00b7zu\u00b7baun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Und billich ist es auch. Vor so viel M\u00fch und Wachen/", "tokens": ["Und", "bil\u00b7lich", "ist", "es", "auch", ".", "Vor", "so", "viel", "M\u00fch", "und", "Wa\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPER", "ADV", "$.", "APPR", "ADV", "PIAT", "NN", "KON", "NN", "$("], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.26": {"text": "Vor so viel schwere Sorg und unverdro\u00dfnen Flei\u00df/", "tokens": ["Vor", "so", "viel", "schwe\u00b7re", "Sorg", "und", "un\u00b7ver\u00b7dro\u00df\u00b7nen", "Flei\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PIAT", "ADJA", "NN", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Dadurch wir lebendig uns todten \u00e4hnlich machen/", "tokens": ["Da\u00b7durch", "wir", "le\u00b7ben\u00b7dig", "uns", "tod\u00b7ten", "\u00e4hn\u00b7lich", "ma\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "ADJD", "PPER", "ADJA", "ADJD", "VVINF", "$("], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.28": {"text": "Ertheilt der Tugend Hand zu letzt den Ehren-Prei\u00df.", "tokens": ["Er\u00b7theilt", "der", "Tu\u00b7gend", "Hand", "zu", "letzt", "den", "Eh\u00b7ren\u00b7Prei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "APPR", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Dergleichen sch\u00f6nen Lohn und pr\u00e4chtiges Geschencke/", "tokens": ["Derg\u00b7lei\u00b7chen", "sch\u00f6\u00b7nen", "Lohn", "und", "pr\u00e4ch\u00b7ti\u00b7ges", "Ge\u00b7schen\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADJA", "NN", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Mein ", "tokens": ["Mein"], "token_info": ["word"], "pos": ["PPOSAT"], "meter": "-", "measure": "single.down"}, "line.31": {"text": "Du hast mit Recht verdient/ da\u00df man dein offt gedencke/", "tokens": ["Du", "hast", "mit", "Recht", "ver\u00b7dient", "/", "da\u00df", "man", "dein", "offt", "ge\u00b7den\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "VVPP", "$(", "KOUS", "PIS", "PPOSAT", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Und da\u00df man Lorbeer-Zweig itzt auf dein Grabmal tr\u00e4gt.", "tokens": ["Und", "da\u00df", "man", "Lor\u00b7beer\u00b7Zweig", "itzt", "auf", "dein", "Grab\u00b7mal", "tr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Du Sohn der Gratien/ du Hertz der Castalinnen/", "tokens": ["Du", "Sohn", "der", "Gra\u00b7ti\u00b7en", "/", "du", "Hertz", "der", "Cas\u00b7ta\u00b7lin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NE", "$(", "PPER", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Es sah von Jugend auf/ dich Ph\u00f6bus g\u00fcnstig an/", "tokens": ["Es", "sah", "von", "Ju\u00b7gend", "auf", "/", "dich", "Ph\u00f6\u00b7bus", "g\u00fcns\u00b7tig", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "APPR", "$(", "PPER", "NE", "ADJD", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Minerva sch\u00e4rffte dir die auffgeweckten Sinnen/", "tokens": ["Mi\u00b7ner\u00b7va", "sch\u00e4rff\u00b7te", "dir", "die", "auff\u00b7ge\u00b7weck\u00b7ten", "Sin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ART", "ADJA", "NN", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.36": {"text": "Und hat das feinste Gold in dein Gehirn gethan.", "tokens": ["Und", "hat", "das", "feins\u00b7te", "Gold", "in", "dein", "Ge\u00b7hirn", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Ein Buch war deine Lust/ ein Buch hie\u00df deine Freude/", "tokens": ["Ein", "Buch", "war", "dei\u00b7ne", "Lust", "/", "ein", "Buch", "hie\u00df", "dei\u00b7ne", "Freu\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$(", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Die Lehrer wunderten den ungemeinen Flei\u00df/", "tokens": ["Die", "Leh\u00b7rer", "wun\u00b7der\u00b7ten", "den", "un\u00b7ge\u00b7mei\u00b7nen", "Flei\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Und/ was ihr Mund vorla\u00df/ blieb deiner Seelen Weyde/", "tokens": ["Und", "/", "was", "ihr", "Mund", "vor\u00b7la\u00df", "/", "blieb", "dei\u00b7ner", "See\u00b7len", "Wey\u00b7de", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "PWS", "PPOSAT", "NN", "PTKVZ", "$(", "VVFIN", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Es r\u00fchmt noch Onoltzbach der gr\u00fcnen Jahre Schwei\u00df.", "tokens": ["Es", "r\u00fchmt", "noch", "On\u00b7oltz\u00b7bach", "der", "gr\u00fc\u00b7nen", "Jah\u00b7re", "Schwei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NN", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Und als du h\u00f6her nun in freyen K\u00fcnsten kommen/", "tokens": ["Und", "als", "du", "h\u00f6\u00b7her", "nun", "in", "frey\u00b7en", "K\u00fcns\u00b7ten", "kom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "ADV", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Und Griechisch und Latein von deinen Lippen flo\u00df/", "tokens": ["Und",