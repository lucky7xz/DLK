{"textgrid.poem.36702": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Civit\u00e0 la Vigna", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ob Lavinia's Enkel dem troischen Ahn zu vergleichen,", "tokens": ["Ob", "La\u00b7vi\u00b7nia's", "En\u00b7kel", "dem", "tro\u00b7i\u00b7schen", "Ahn", "zu", "ver\u00b7glei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "NN", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+--+--+--+--+-", "measure": "amphibrach.penta.plus"}, "line.2": {"text": "Schwerlich g\u00e4b' es Virgil, wenn er sie kennte, dir zu;", "tokens": ["Schwer\u00b7lich", "g\u00e4b'", "es", "Vir\u00b7gil", ",", "wenn", "er", "sie", "kenn\u00b7te", ",", "dir", "zu", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "NE", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$,", "PPER", "PTKVZ", "$."], "meter": "+-+-+-++-+--+", "measure": "iambic.septa.chol"}, "line.3": {"text": "Aber hat sich das teukrische Blut in Reben verwandelt,", "tokens": ["A\u00b7ber", "hat", "sich", "das", "teu\u00b7kri\u00b7sche", "Blut", "in", "Re\u00b7ben", "ver\u00b7wan\u00b7delt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PRF", "ART", "ADJA", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Wahrlich, so stammen sie selbst von den Olympischen ab.", "tokens": ["Wahr\u00b7lich", ",", "so", "stam\u00b7men", "sie", "selbst", "von", "den", "O\u00b7lym\u00b7pi\u00b7schen", "ab", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+--+--+--+-+-+", "measure": "dactylic.tri.plus"}}}}}