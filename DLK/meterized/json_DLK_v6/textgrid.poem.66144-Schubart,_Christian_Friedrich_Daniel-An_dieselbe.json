{"textgrid.poem.66144": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "An dieselbe", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach, am Feste deines Lebens", "tokens": ["Ach", ",", "am", "Fes\u00b7te", "dei\u00b7nes", "Le\u00b7bens"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "APPRART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Streckt dein alter Freund", "tokens": ["Streckt", "dein", "al\u00b7ter", "Freund"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Seinen m\u00fcden Arm vergebens", "tokens": ["Sei\u00b7nen", "m\u00fc\u00b7den", "Arm", "ver\u00b7ge\u00b7bens"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nach dir aus \u2013 und weint", "tokens": ["Nach", "dir", "aus", "\u2013", "und", "weint"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPER", "PTKVZ", "$(", "KON", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Zwanzig Jahre \u2013 ha, wie tr\u00fcbe!", "tokens": ["Zwan\u00b7zig", "Jah\u00b7re", "\u2013", "ha", ",", "wie", "tr\u00fc\u00b7be", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["CARD", "NN", "$(", "ITJ", "$,", "PWAV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie von manchem H\u00f6llengram entweiht", "tokens": ["Wie", "von", "man\u00b7chem", "H\u00f6l\u00b7len\u00b7gram", "ent\u00b7weiht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "PIAT", "NN", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Flo\u00dfen diese Jahre unsrer Liebe", "tokens": ["Flo\u00b7\u00dfen", "die\u00b7se", "Jah\u00b7re", "uns\u00b7rer", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PDAT", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Hin ins Meer der Ewigkeit!!", "tokens": ["Hin", "ins", "Meer", "der", "E\u00b7wig\u00b7keit", "!!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Von den zwanzig Jahren sind kaum ", "tokens": ["Von", "den", "zwan\u00b7zig", "Jah\u00b7ren", "sind", "kaum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "CARD", "NN", "VAFIN", "ADV"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.10": {"text": "Mehr als zehen schwanden unter tausend Wehen", "tokens": ["Mehr", "als", "ze\u00b7hen", "schwan\u00b7den", "un\u00b7ter", "tau\u00b7send", "We\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "KOKOM", "CARD", "ADJA", "APPR", "CARD", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.11": {"text": "Leer \u2013 und ohne deinen Ku\u00df.", "tokens": ["Leer", "\u2013", "und", "oh\u00b7ne", "dei\u00b7nen", "Ku\u00df", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "KON", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}