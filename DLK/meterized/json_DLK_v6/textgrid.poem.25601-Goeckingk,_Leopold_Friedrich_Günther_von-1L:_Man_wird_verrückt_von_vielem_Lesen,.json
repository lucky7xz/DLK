{"textgrid.poem.25601": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Man wird verr\u00fcckt von vielem Lesen,", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Man wird verr\u00fcckt von vielem Lesen,", "tokens": ["Man", "wird", "ver\u00b7r\u00fcckt", "von", "vie\u00b7lem", "Le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ADJD", "APPR", "PIS", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sagt Mops, und bleibt dem Lesen feind.", "tokens": ["Sagt", "Mops", ",", "und", "bleibt", "dem", "Le\u00b7sen", "feind", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "KON", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch ist sein Satz je wahr gewesen,", "tokens": ["Doch", "ist", "sein", "Satz", "je", "wahr", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "ADV", "ADJD", "VAPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So folgt, was Mops wohl nicht vermeint,", "tokens": ["So", "folgt", ",", "was", "Mops", "wohl", "nicht", "ver\u00b7meint", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PWS", "NN", "ADV", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df Niemand mehr als er gelesen.", "tokens": ["Da\u00df", "Nie\u00b7mand", "mehr", "als", "er", "ge\u00b7le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "KOUS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}