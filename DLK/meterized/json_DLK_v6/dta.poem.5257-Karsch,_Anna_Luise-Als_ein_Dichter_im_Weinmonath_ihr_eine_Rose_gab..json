{"dta.poem.5257": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Als ein Dichter im Weinmonath  \n ihr eine Rose gab.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-200905197778", "language": ["de:0.99"], "booktitle": "Karsch, Anna Luise: Auserlesene Gedichte. Berlin, 1764."}, "poem": {"stanza.1": {"line.1": {"text": "Die reife Traube h\u00f6rt ich j\u00fcngst zur Rose sprechen:", "tokens": ["Die", "rei\u00b7fe", "Trau\u00b7be", "h\u00f6rt", "ich", "j\u00fcngst", "zur", "Ro\u00b7se", "spre\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo kommst du her? wo willst du hin?", "tokens": ["Wo", "kommst", "du", "her", "?", "wo", "willst", "du", "hin", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PTKVZ", "$.", "PWAV", "VMFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie sprach: Mich gab der Herbst. Ein Dichter", "tokens": ["Sie", "sprach", ":", "Mich", "gab", "der", "Herbst", ".", "Ein", "Dich\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "ART", "NN", "$.", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "soll mich brechen,", "tokens": ["soll", "mich", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "F\u00fcr eine Dichterin.", "tokens": ["F\u00fcr", "ei\u00b7ne", "Dich\u00b7te\u00b7rin", "."], "token_info": ["word", "word", "word", "pun