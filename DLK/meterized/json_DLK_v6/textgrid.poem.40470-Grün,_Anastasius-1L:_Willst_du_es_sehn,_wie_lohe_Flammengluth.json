{"textgrid.poem.40470": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Willst du es sehn, wie lohe Flammengluth", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Willst du es sehn, wie lohe Flammengluth", "tokens": ["Willst", "du", "es", "sehn", ",", "wie", "lo\u00b7he", "Flam\u00b7men\u00b7gluth"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PPER", "VVINF", "$,", "PWAV", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Beisammen friedlich wohnt mit Wasserfluth,", "tokens": ["Bei\u00b7sam\u00b7men", "fried\u00b7lich", "wohnt", "mit", "Was\u00b7ser\u00b7fluth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie beide in einander frei bestehn,", "tokens": ["Wie", "bei\u00b7de", "in", "ein\u00b7an\u00b7der", "frei", "be\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "APPR", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So mu\u00dft du ihr ins klare Auge sehn;", "tokens": ["So", "mu\u00dft", "du", "ihr", "ins", "kla\u00b7re", "Au\u00b7ge", "sehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Drin wohnt ein Feuer wie die Gluth der Sonne,", "tokens": ["Drin", "wohnt", "ein", "Feu\u00b7er", "wie", "die", "Gluth", "der", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "KOKOM", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Drans siehst du wie aus gl\u00fchem Flammenbronne", "tokens": ["Drans", "siehst", "du", "wie", "aus", "gl\u00fc\u00b7hem", "Flam\u00b7men\u00b7bron\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "KOKOM", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Oft klar den Perlenquell der Thr\u00e4nen thau'n,", "tokens": ["Oft", "klar", "den", "Per\u00b7len\u00b7quell", "der", "Thr\u00e4\u00b7nen", "thau'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Kannst Gluth in Fluth und Fluth in Gluthen schau'n.", "tokens": ["Kannst", "Gluth", "in", "Fluth", "und", "Fluth", "in", "Glu\u00b7then", "schau'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["VMFIN", "NN", "APPR", "NN", "KON", "NN", "APPR", "NN", "VVFIN", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Willst du auch sehn den Becher wunderbar,", "tokens": ["Willst", "du", "auch", "sehn", "den", "Be\u00b7cher", "wun\u00b7der\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Draus t\u00f6dtend Gift und Honig s\u00fc\u00df und klar", "tokens": ["Draus", "t\u00f6d\u00b7tend", "Gift", "und", "Ho\u00b7nig", "s\u00fc\u00df", "und", "klar"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVPP", "NN", "KON", "NN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mit einem einz'gen Zug man sangen kann:", "tokens": ["Mit", "ei\u00b7nem", "einz'\u00b7gen", "Zug", "man", "san\u00b7gen", "kann", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "O blicke ihren Rosenmund nur an!", "tokens": ["O", "bli\u00b7cke", "ih\u00b7ren", "Ro\u00b7sen\u00b7mund", "nur", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der Wunderbecher sind die Purpurlippen,", "tokens": ["Der", "Wun\u00b7der\u00b7be\u00b7cher", "sind", "die", "Pur\u00b7pur\u00b7lip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Draus S\u00fc\u00df und Herb mit Einem Zug zu nippen,", "tokens": ["Draus", "S\u00fc\u00df", "und", "Herb", "mit", "Ei\u00b7nem", "Zug", "zu", "nip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "NN", "KON", "NN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ein Honigseim, der's Herz belebt und n\u00e4hrt,", "tokens": ["Ein", "Ho\u00b7ni\u00b7gseim", ",", "der's", "Herz", "be\u00b7lebt", "und", "n\u00e4hrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELAT", "NN", "VVPP", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Ein Gift, das wild am Lebensmarke zehrt.", "tokens": ["Ein", "Gift", ",", "das", "wild", "am", "Le\u00b7bens\u00b7mar\u00b7ke", "zehrt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und kennst das goldne Wundernetz du nicht,", "tokens": ["Und", "kennst", "das", "gold\u00b7ne", "Wun\u00b7der\u00b7netz", "du", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wo sich kein Faden in den andern flicht,", "tokens": ["Wo", "sich", "kein", "Fa\u00b7den", "in", "den", "an\u00b7dern", "flicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "PIAT", "NN", "APPR", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das fest zugleich, wenn locker auch und los,", "tokens": ["Das", "fest", "zu\u00b7gleich", ",", "wenn", "lo\u00b7cker", "auch", "und", "los", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "$,", "KOUS", "NE", "ADV", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Manch bebend Herz verstrickt in seinen Schoo\u00df?", "tokens": ["Manch", "be\u00b7bend", "Herz", "ver\u00b7strickt", "in", "sei\u00b7nen", "Schoo\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJD", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.4": {"line.1": {"text": "Siehst du der Lockenhaare goldig Prangen?", "tokens": ["Siehst", "du", "der", "Lo\u00b7cken\u00b7haa\u00b7re", "gol\u00b7dig", "Pran\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das ist das Wundernetz, das mich gefangen,", "tokens": ["Das", "ist", "das", "Wun\u00b7der\u00b7netz", ",", "das", "mich", "ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das fest zugleich, wenn locker auch und los,", "tokens": ["Das", "fest", "zu\u00b7gleich", ",", "wenn", "lo\u00b7cker", "auch", "und", "los", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "$,", "KOUS", "NE", "ADV", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mein zitternd Herz verstrickt in seinen Schoo\u00df.", "tokens": ["Mein", "zit\u00b7ternd", "Herz", "ver\u00b7strickt", "in", "sei\u00b7nen", "Schoo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVPP", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Willst du es sehn, wie Aetna's Flammenbrand", "tokens": ["Willst", "du", "es", "sehn", ",", "wie", "A\u00b7et\u00b7na's", "Flam\u00b7men\u00b7brand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PPER", "VVINF", "$,", "PWAV", "NE", "NE"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Mit Thule's eis'gen Schollen sich verband,", "tokens": ["Mit", "Thu\u00b7le's", "eis'\u00b7gen", "Schol\u00b7len", "sich", "ver\u00b7band", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Eine Gottes flammender Altar,", "tokens": ["Der", "Ei\u00b7ne", "Got\u00b7tes", "flam\u00b7men\u00b7der", "Al\u00b7tar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Andern frostig, kalt und ewig starr?", "tokens": ["Die", "An\u00b7dern", "fros\u00b7tig", ",", "kalt", "und", "e\u00b7wig", "starr", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJD", "$,", "ADJD", "KON", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Das sind wir Zwei und unsre beiden Herzen,", "tokens": ["Das", "sind", "wir", "Zwei", "und", "uns\u00b7re", "bei\u00b7den", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "CARD", "KON", "PPOSAT", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ungleich an Lust, ungleicher noch an Schmerzen,", "tokens": ["Un\u00b7gleich", "an", "Lust", ",", "un\u00b7glei\u00b7cher", "noch", "an", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "$,", "ADJD", "ADV", "APPR", "NN", "$,"], "meter": "---+-+-+-+-", "measure": "unknown.measure.tetra"}, "line.7": {"text": "Das meine wie des Aetna's Brand so hei\u00df,", "tokens": ["Das", "mei\u00b7ne", "wie", "des", "A\u00b7et\u00b7na's", "Brand", "so", "hei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KOKOM", "ART", "NN", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Das ihre kalt und starr wie Nordpols Eis.", "tokens": ["Das", "ih\u00b7re", "kalt", "und", "starr", "wie", "Nord\u00b7pols", "Eis", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "ADJD", "KON", "ADJD", "KOKOM", "NE", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}