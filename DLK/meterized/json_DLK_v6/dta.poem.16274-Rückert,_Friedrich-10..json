{"dta.poem.16274": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "10.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du fragst, warum so fr\u00fch gescheite Kinder sterben,", "tokens": ["Du", "fragst", ",", "wa\u00b7rum", "so", "fr\u00fch", "ge\u00b7schei\u00b7te", "Kin\u00b7der", "ster\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ADV", "ADJD", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Indes die dummeren ein l\u00e4ngers Leben erben?", "tokens": ["In\u00b7des", "die", "dum\u00b7me\u00b7ren", "ein", "l\u00e4n\u00b7gers", "Le\u00b7ben", "er\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Antwort ist: weil man gescheitres Nichts kann thun", "tokens": ["Die", "Ant\u00b7wort", "ist", ":", "weil", "man", "ge\u00b7schei\u00b7tres", "Nichts", "kann", "thun"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "$.", "KOUS", "PIS", "ADJA", "NN", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als sterben in der Welt, die gar so dumm ist nun.", "tokens": ["Als", "ster\u00b7ben", "in", "der", "Welt", ",", "die", "gar", "so", "dumm", "ist", "nun", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "ART", "NN", "$,", "PRELS", "ADV", "ADV", "ADJD", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Drum danket alle Gott, die ihr nicht zu gescheut", "tokens": ["Drum", "dan\u00b7ket", "al\u00b7le", "Gott", ",", "die", "ihr", "nicht", "zu", "ge\u00b7scheut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PIAT", "NN", "$,", "PRELS", "PPER", "PTKNEG", "PTKA", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Geworden, sondern noch der dummen Welt euch freut.", "tokens": ["Ge\u00b7wor\u00b7den", ",", "son\u00b7dern", "noch", "der", "dum\u00b7men", "Welt", "euch", "freut", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "ADV", "ART", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}