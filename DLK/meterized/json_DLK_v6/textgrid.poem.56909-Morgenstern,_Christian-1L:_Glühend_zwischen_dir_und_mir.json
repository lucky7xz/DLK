{"textgrid.poem.56909": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gl\u00fchend zwischen dir und mir", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gl\u00fchend zwischen dir und mir", "tokens": ["Gl\u00fc\u00b7hend", "zwi\u00b7schen", "dir", "und", "mir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "PPER", "KON", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Julin\u00e4chte br\u00fcten;", "tokens": ["Ju\u00b7li\u00b7n\u00e4ch\u00b7te", "br\u00fc\u00b7ten", ";"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "gleiche Sterne dort und hier", "tokens": ["glei\u00b7che", "Ster\u00b7ne", "dort", "und", "hier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "ADV", "KON", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "unsern Schlaf beh\u00fcten.", "tokens": ["un\u00b7sern", "Schlaf", "be\u00b7h\u00fc\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "W\u00e4hl das sch\u00f6nste Sternelein,", "tokens": ["W\u00e4hl", "das", "sch\u00f6ns\u00b7te", "Ster\u00b7ne\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "will das Gleiche tuen; \u2013", "tokens": ["will", "das", "Glei\u00b7che", "tu\u00b7en", ";", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "ART", "NN", "VVINF", "$.", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "morgen droben Stelldichein", "tokens": ["mor\u00b7gen", "dro\u00b7ben", "Stell\u00b7dich\u00b7ein"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADV", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "auf geheimen Schuhen.", "tokens": ["auf", "ge\u00b7hei\u00b7men", "Schu\u00b7hen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Gibst du nur nichts anderm Raum,", "tokens": ["Gibst", "du", "nur", "nichts", "an\u00b7derm", "Raum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PIS", "PIS", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "als mich dort zu finden,", "tokens": ["als", "mich", "dort", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "wird ein gleicher s\u00fc\u00dfer Traum", "tokens": ["wird", "ein", "glei\u00b7cher", "s\u00fc\u00b7\u00dfer", "Traum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "dich und mich verbinden.", "tokens": ["dich", "und", "mich", "ver\u00b7bin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "PPER", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}