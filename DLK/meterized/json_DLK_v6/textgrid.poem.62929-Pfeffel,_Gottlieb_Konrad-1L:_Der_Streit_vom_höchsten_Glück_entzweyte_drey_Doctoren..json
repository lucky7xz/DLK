{"textgrid.poem.62929": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Streit vom h\u00f6chsten Gl\u00fcck entzweyte drey Doctoren.", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Streit vom h\u00f6chsten Gl\u00fcck entzweyte drey Doctoren.", "tokens": ["Der", "Streit", "vom", "h\u00f6chs\u00b7ten", "Gl\u00fcck", "ent\u00b7zwey\u00b7te", "drey", "Doc\u00b7to\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "VVFIN", "CARD", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In einem Fuhrmannsklub kann man nicht \u00e4rger schreyn.", "tokens": ["In", "ei\u00b7nem", "Fuhr\u00b7manns\u00b7klub", "kann", "man", "nicht", "\u00e4r\u00b7ger", "schreyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "PIS", "PTKNEG", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein vierter schwieg; er war zum Richter auserkohren", "tokens": ["Ein", "vier\u00b7ter", "schwieg", ";", "er", "war", "zum", "Rich\u00b7ter", "au\u00b7ser\u00b7koh\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "VVFIN", "$.", "PPER", "VAFIN", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und sprach: das h\u00f6chste Gl\u00fcck ist, taub zu seyn.", "tokens": ["Und", "sprach", ":", "das", "h\u00f6chs\u00b7te", "Gl\u00fcck", "ist", ",", "taub", "zu", "seyn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "V