{"dta.poem.8427": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "An einen Freund, der sich in ein  \n sch\u00f6nes Landm\u00e4dchen verliebte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1783", "urn": "urn:nbn:de:kobv:b4-200905192568", "language": ["de:0.99"], "booktitle": "H\u00f6lty, Ludwig Christoph Heinrich: Gedichte. Hamburg, 1783."}, "poem": {"stanza.1": {"line.1": {"text": "Die dir dein Genius beschert?", "tokens": ["Die", "dir", "dein", "Ge\u00b7nius", "be\u00b7schert", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sie ist es wehrt, dass du ihr K\u00fcsse giebest;", "tokens": ["Sie", "ist", "es", "wehrt", ",", "dass", "du", "ihr", "K\u00fcs\u00b7se", "gie\u00b7best", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVFIN", "$,", "KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das schlanke M\u00e4dchen ist es wehrt!", "tokens": ["Das", "schlan\u00b7ke", "M\u00e4d\u00b7chen", "ist", "es", "wehrt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sie hat kein Gold, womit das Fr\u00e4ulein pralet,", "tokens": ["Sie", "hat", "kein", "Gold", ",", "wo\u00b7mit", "das", "Fr\u00e4u\u00b7lein", "pra\u00b7let", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und keine lange Ahnenschaft;", "tokens": ["Und", "kei\u00b7ne", "lan\u00b7ge", "Ah\u00b7nen\u00b7schaft", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch ist sie sch\u00f6n, wie man die Engel malet,", "tokens": ["Doch", "ist", "sie", "sch\u00f6n", ",", "wie", "man", "die", "En\u00b7gel", "ma\u00b7let", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "$,", "PWAV", "PIS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bescheiden, edel, tugendhaft.", "tokens": ["Be\u00b7schei\u00b7den", ",", "e\u00b7del", ",", "tu\u00b7gend\u00b7haft", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$,", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Sie ist nicht stolz, wie die nach Standsgeb\u00fchren", "tokens": ["Sie", "ist", "nicht", "stolz", ",", "wie", "die", "nach", "Stands\u00b7ge\u00b7b\u00fch\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "$,", "PWAV", "ART", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Geehrten Fr\u00e4ulein oder Fraun,", "tokens": ["Ge\u00b7ehr\u00b7ten", "Fr\u00e4u\u00b7lein", "o\u00b7der", "Fraun", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die auf uns S\u00fcnder, die das Von nicht f\u00fchren,", "tokens": ["Die", "auf", "uns", "S\u00fcn\u00b7der", ",", "die", "das", "Von", "nicht", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "NN", "$,", "PRELS", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit hoher Nase niederschaun;", "tokens": ["Mit", "ho\u00b7her", "Na\u00b7se", "nie\u00b7der\u00b7schaun", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Verleumdet nicht, und spielt nicht die Kokette,", "tokens": ["Ver\u00b7leum\u00b7det", "nicht", ",", "und", "spielt", "nicht", "die", "Ko\u00b7ket\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,", "KON", "VVFIN", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wird durch kein leer Gew\u00e4sch entz\u00fcckt;", "tokens": ["Wird", "durch", "kein", "leer", "Ge\u00b7w\u00e4sch", "ent\u00b7z\u00fcckt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PIAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Schl\u00e4ft ruhig ein, und springt aus ihrem Bette,", "tokens": ["Schl\u00e4ft", "ru\u00b7hig", "ein", ",", "und", "springt", "aus", "ih\u00b7rem", "Bet\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PTKVZ", "$,", "KON", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So bald die Sonn' ins Fenster blickt.", "tokens": ["So", "bald", "die", "Sonn'", "ins", "Fens\u00b7ter", "blickt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Sie singt, beim Ramen und beim Spinnerocken,", "tokens": ["Sie", "singt", ",", "beim", "Ra\u00b7men", "und", "beim", "Spin\u00b7ne\u00b7ro\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPRART", "NN", "KON", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein weltlich oder geistlich Lied,", "tokens": ["Ein", "welt\u00b7lich", "o\u00b7der", "geist\u00b7lich", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Morgenhaub' um ihre blonden Locken,", "tokens": ["Die", "Mor\u00b7gen\u00b7haub'", "um", "ih\u00b7re", "blon\u00b7den", "Lo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bis ihre stille Traur entflieht.", "tokens": ["Bis", "ih\u00b7re", "stil\u00b7le", "Traur", "ent\u00b7flieht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Die Dame selbst w\u00fcrd' aus dem goldnen Wagen", "tokens": ["Die", "Da\u00b7me", "selbst", "w\u00fcrd'", "aus", "dem", "gold\u00b7nen", "Wa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VAFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nach deiner lieben Hanne sehn,", "tokens": ["Nach", "dei\u00b7ner", "lie\u00b7ben", "Han\u00b7ne", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und knirschend sich den platten Busen schlagen,", "tokens": ["Und", "knir\u00b7schend", "sich", "den", "plat\u00b7ten", "Bu\u00b7sen", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PRF", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und seufzen: Sie ist wahrlich sch\u00f6n!", "tokens": ["Und", "seuf\u00b7zen", ":", "Sie", "ist", "wahr\u00b7lich", "sch\u00f6n", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$.", "PPER", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Ja, sie ist sch\u00f6n! Der ganze Mai umschwebet", "tokens": ["Ja", ",", "sie", "ist", "sch\u00f6n", "!", "Der", "gan\u00b7ze", "Mai", "um\u00b7schwe\u00b7bet"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "ADJD", "$.", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihr weisses l\u00e4chelndes Gesicht;", "tokens": ["Ihr", "weis\u00b7ses", "l\u00e4\u00b7cheln\u00b7des", "Ge\u00b7sicht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr Busen bebt, wie eine Blume bebet,", "tokens": ["Ihr", "Bu\u00b7sen", "bebt", ",", "wie", "ei\u00b7ne", "Blu\u00b7me", "be\u00b7bet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die eben aus der Knospe bricht.", "tokens": ["Die", "e\u00b7ben", "aus", "der", "Knos\u00b7pe", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Die Sittsamkeit flieht goldne F\u00fcrstens\u00e4le,", "tokens": ["Die", "Sitt\u00b7sam\u00b7keit", "flieht", "gold\u00b7ne", "F\u00fcrs\u00b7ten\u00b7s\u00e4\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und liebt die niedern H\u00fctten nur.", "tokens": ["Und", "liebt", "die", "nie\u00b7dern", "H\u00fct\u00b7ten", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich selber, wenn ich mir ein M\u00e4dchen w\u00e4hle,", "tokens": ["Ich", "sel\u00b7ber", ",", "wenn", "ich", "mir", "ein", "M\u00e4d\u00b7chen", "w\u00e4h\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "KOUS", "PPER", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich such' es auf der Sch\u00e4ferflur.", "tokens": ["Ich", "such'", "es", "auf", "der", "Sch\u00e4\u00b7fer\u00b7flur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}