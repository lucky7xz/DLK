{"textgrid.poem.63558": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Unterwegs", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun brause mich, Wind, nach Hause geschwind,", "tokens": ["Nun", "brau\u00b7se", "mich", ",", "Wind", ",", "nach", "Hau\u00b7se", "ge\u00b7schwind", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "NN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dort sitzt mein Liebchen und sehnt und sinnt,", "tokens": ["Dort", "sitzt", "mein", "Lieb\u00b7chen", "und", "sehnt", "und", "sinnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "KON", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ihre einz'ge Gesellin die flackernde Kerz',", "tokens": ["Ih\u00b7re", "einz'\u00b7ge", "Ge\u00b7sel\u00b7lin", "die", "fla\u00b7ckern\u00b7de", "Ker\u00b7z'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Und sie horcht auf den Sturm und horcht auf ihr Herz.", "tokens": ["Und", "sie", "horcht", "auf", "den", "Sturm", "und", "horcht", "auf", "ihr", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "NN", "KON", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "O trage mich, Wind, durch den sausenden Hag,", "tokens": ["O", "tra\u00b7ge", "mich", ",", "Wind", ",", "durch", "den", "sau\u00b7sen\u00b7den", "Hag", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$,", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Befl\u00fcgle den Fu\u00df mir dein Fl\u00fcgelschlag,", "tokens": ["Be\u00b7fl\u00fcg\u00b7le", "den", "Fu\u00df", "mir", "dein", "Fl\u00fc\u00b7gel\u00b7schlag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Befl\u00fcgle die Zeit, und mit klirrendem Ton", "tokens": ["Be\u00b7fl\u00fcg\u00b7le", "die", "Zeit", ",", "und", "mit", "klir\u00b7ren\u00b7dem", "Ton"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "KON", "APPR", "ADJA", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Poch an ihr Fenster: wir kommen schon!", "tokens": ["Poch", "an", "ihr", "Fens\u00b7ter", ":", "wir", "kom\u00b7men", "schon", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "ADV", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Wir kommen! Und brechen wir ein in das Haus,", "tokens": ["Wir", "kom\u00b7men", "!", "Und", "bre\u00b7chen", "wir", "ein", "in", "das", "Haus", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "KON", "VVFIN", "PPER", "ART", "APPR", "ART", "NN", "$,"], "meter": "-+--+--++-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Dann st\u00fcrme dein Atem das Fl\u00e4mmchen aus,", "tokens": ["Dann", "st\u00fcr\u00b7me", "dein", "A\u00b7tem", "das", "Fl\u00e4mm\u00b7chen", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Dann saus' und brause hinaus in die Nacht,", "tokens": ["Dann", "saus'", "und", "brau\u00b7se", "hin\u00b7aus", "in", "die", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Um die H\u00fctte der Gl\u00fccklichen halte die Wacht!", "tokens": ["Um", "die", "H\u00fct\u00b7te", "der", "Gl\u00fcck\u00b7li\u00b7chen", "hal\u00b7te", "die", "Wacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}}}}}