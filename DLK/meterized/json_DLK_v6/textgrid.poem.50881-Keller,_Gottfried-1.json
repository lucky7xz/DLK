{"textgrid.poem.50881": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Unser ist das Reich der Epigonen,", "tokens": ["Un\u00b7ser", "ist", "das", "Reich", "der", "E\u00b7pi\u00b7go\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Die im gro\u00dfen Herkulanum wohnen;", "tokens": ["Die", "im", "gro\u00b7\u00dfen", "Her\u00b7ku\u00b7la\u00b7num", "woh\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Seht, wie ihr noch einen Tropfen presset", "tokens": ["Seht", ",", "wie", "ihr", "noch", "ei\u00b7nen", "Trop\u00b7fen", "pres\u00b7set"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PWAV", "PPER", "ADV", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Aus den alten Schalen der Zitronen!", "tokens": ["Aus", "den", "al\u00b7ten", "Scha\u00b7len", "der", "Zit\u00b7ro\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.5": {"text": "Geistiges ist noch genug vorhanden,", "tokens": ["Geis\u00b7ti\u00b7ges", "ist", "noch", "ge\u00b7nug", "vor\u00b7han\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Auch der Liebe Zucker wird noch lohnen.", "tokens": ["Auch", "der", "Lie\u00b7be", "Zu\u00b7cker", "wird", "noch", "loh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "VAFIN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Wasser flutet uns in weiten Meeren,", "tokens": ["Was\u00b7ser", "flu\u00b7tet", "uns", "in", "wei\u00b7ten", "Mee\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Brauchen es am wenigsten zu schonen:", "tokens": ["Brau\u00b7chen", "es", "am", "we\u00b7nigs\u00b7ten", "zu", "scho\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "PIS", "PTKZU", "VVINF", "$."], "meter": "+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.9": {"text": "Braut den Trank f\u00fcr lange Wintern\u00e4chte,", "tokens": ["Braut", "den", "Trank", "f\u00fcr", "lan\u00b7ge", "Win\u00b7ter\u00b7n\u00e4ch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Bis uns bl\u00fchen neue Lenzeskronen!", "tokens": ["Bis", "uns", "bl\u00fc\u00b7hen", "neu\u00b7e", "Len\u00b7zes\u00b7kro\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}