{"textgrid.poem.25057": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Verbrannte Briefe", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dank dir, da\u00df du den Trug mir bekannt hast!", "tokens": ["Dank", "dir", ",", "da\u00df", "du", "den", "Trug", "mir", "be\u00b7kannt", "hast", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "KOUS", "PPER", "ART", "NN", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df, die ich schrieb mit des Herzens Blut,", "tokens": ["Da\u00df", ",", "die", "ich", "schrieb", "mit", "des", "Her\u00b7zens", "Blut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PRELS", "PPER", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Du die Briefe zur\u00fcck mir gesandt hast!", "tokens": ["Du", "die", "Brie\u00b7fe", "zu\u00b7r\u00fcck", "mir", "ge\u00b7sandt", "hast", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "PTKVZ", "PPER", "VVPP", "VAFIN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Nun mit allen hinein in die Glut!", "tokens": ["Nun", "mit", "al\u00b7len", "hin\u00b7ein", "in", "die", "Glut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIAT", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Frei aufatmen werd' ich aufs neue,", "tokens": ["Frei", "auf\u00b7at\u00b7men", "werd'", "ich", "aufs", "neu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "VAFIN", "PPER", "APPRART", "ADJA", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Wenn sie verlodert sind wie mein Wahn", "tokens": ["Wenn", "sie", "ver\u00b7lo\u00b7dert", "sind", "wie", "mein", "Wahn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "KOKOM", "PPOSAT", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und die Schw\u00fcre ew'ger Treue,", "tokens": ["Und", "die", "Schw\u00fc\u00b7re", "ew'\u00b7ger", "Treu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die du im brennenden Ku\u00df mir gethan.", "tokens": ["Die", "du", "im", "bren\u00b7nen\u00b7den", "Ku\u00df", "mir", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "ADJA", "NN", "PPER", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Aber um die du, o Weib, mich betrogen,", "tokens": ["A\u00b7ber", "um", "die", "du", ",", "o", "Weib", ",", "mich", "be\u00b7tro\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPR", "PRELS", "PPER", "$,", "FM", "NN", "$,", "PPER", "VVPP", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Alle die Stunden, als ich vom Mund", "tokens": ["Al\u00b7le", "die", "Stun\u00b7den", ",", "als", "ich", "vom", "Mund"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "ART", "NN", "$,", "KOUS", "PPER", "APPRART", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Dir verzehrende Wonne gesogen,", "tokens": ["Dir", "ver\u00b7zeh\u00b7ren\u00b7de", "Won\u00b7ne", "ge\u00b7so\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "W\u00e4hrend dein Herz schon gebrochen den Bund,", "tokens": ["W\u00e4h\u00b7rend", "dein", "Herz", "schon", "ge\u00b7bro\u00b7chen", "den", "Bund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "VVPP", "ART", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.4": {"line.1": {"text": "Alle, wo ich dir am Busen gelegen,", "tokens": ["Al\u00b7le", ",", "wo", "ich", "dir", "am", "Bu\u00b7sen", "ge\u00b7le\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PWAV", "PPER", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Erd' und Himmel um dich verga\u00df", "tokens": ["Erd'", "und", "Him\u00b7mel", "um", "dich", "ver\u00b7ga\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "APPR", "PPER", "VVFIN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Und nur an deiner Pulse Schl\u00e4gen", "tokens": ["Und", "nur", "an", "dei\u00b7ner", "Pul\u00b7se", "Schl\u00e4\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Meine schwindenden Tage ma\u00df,", "tokens": ["Mei\u00b7ne", "schwin\u00b7den\u00b7den", "Ta\u00b7ge", "ma\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.5": {"line.1": {"text": "Sage! kannst du sie wieder mir geben?", "tokens": ["Sa\u00b7ge", "!", "kannst", "du", "sie", "wie\u00b7der", "mir", "ge\u00b7ben", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VMFIN", "PPER", "PPER", "ADV", "PPER", "VVINF", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Mu\u00dft du nicht zittern, wenn ich zur\u00fcck", "tokens": ["Mu\u00dft", "du", "nicht", "zit\u00b7tern", ",", "wenn", "ich", "zu\u00b7r\u00fcck"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PTKNEG", "VVINF", "$,", "KOUS", "PPER", "PTKVZ"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Heische ein halbes verschwendetes Leben,", "tokens": ["Hei\u00b7sche", "ein", "hal\u00b7bes", "ver\u00b7schwen\u00b7de\u00b7tes", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Das du um Frieden betrogen und Gl\u00fcck?", "tokens": ["Das", "du", "um", "Frie\u00b7den", "be\u00b7tro\u00b7gen", "und", "Gl\u00fcck", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "VVPP", "KON", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}}}}