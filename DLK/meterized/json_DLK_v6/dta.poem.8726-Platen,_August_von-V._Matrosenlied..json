{"dta.poem.8726": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "V.  \n  Matrosenlied.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Den das Geschick mir aufbewahrt,", "tokens": ["Den", "das", "Ge\u00b7schick", "mir", "auf\u00b7be\u00b7wahrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Der Tag des Wiedersehens bey den Meinen,", "tokens": ["Der", "Tag", "des", "Wie\u00b7der\u00b7se\u00b7hens", "bey", "den", "Mei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nach allzulanger Fahrt?", "tokens": ["Nach", "all\u00b7zu\u00b7lan\u00b7ger", "Fahrt", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "O sch\u00f6ne Flur, wo unsre m\u00fcden K\u00e4hne", "tokens": ["O", "sch\u00f6\u00b7ne", "Flur", ",", "wo", "uns\u00b7re", "m\u00fc\u00b7den", "K\u00e4h\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "$,", "PWAV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dereinst noch landen m\u00f6gen unversehrt!", "tokens": ["De\u00b7reinst", "noch", "lan\u00b7den", "m\u00f6\u00b7gen", "un\u00b7ver\u00b7sehrt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVINF", "VMFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "O M\u00e4dchen, das vielleicht mit einer Thr\u00e4ne", "tokens": ["O", "M\u00e4d\u00b7chen", ",", "das", "viel\u00b7leicht", "mit", "ei\u00b7ner", "Thr\u00e4\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "PRELS", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den armen Fl\u00fcchtling ehrt!", "tokens": ["Den", "ar\u00b7men", "Fl\u00fccht\u00b7ling", "ehrt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Denkst du der heil'gen Eide noch im Stillen,", "tokens": ["Denkst", "du", "der", "heil'\u00b7gen", "Ei\u00b7de", "noch", "im", "Stil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und hieltst du, Theure, das beschworne Wort?", "tokens": ["Und", "hieltst", "du", ",", "Theu\u00b7re", ",", "das", "be\u00b7schwor\u00b7ne", "Wort", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "ADJA", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ach, trieb nicht feindlich damals, wider Willen,", "tokens": ["Ach", ",", "trieb", "nicht", "feind\u00b7lich", "da\u00b7mals", ",", "wi\u00b7der", "Wil\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "PTKNEG", "ADJD", "ADV", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein b\u00f6s Geschick mich fort?", "tokens": ["Ein", "b\u00f6s", "Ge\u00b7schick", "mich", "fort", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Doch werden, glaub' mir, wir uns wiedersehen,", "tokens": ["Doch", "wer\u00b7den", ",", "glaub'", "mir", ",", "wir", "uns", "wie\u00b7der\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAINF", "$,", "VVFIN", "PPER", "$,", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und harrst du sehnsuchtsvoll am Strande mein,", "tokens": ["Und", "harrst", "du", "sehn\u00b7suchts\u00b7voll", "am", "Stran\u00b7de", "mein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "APPRART", "NN", "PPOSAT", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So k\u00f6nnen's, Theure, siehst du Wimpel wehen,", "tokens": ["So", "k\u00f6n\u00b7nen's", ",", "Theu\u00b7re", ",", "siehst", "du", "Wim\u00b7pel", "we\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "$,", "ADJA", "$,", "VVFIN", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nur ", "tokens": ["Nur"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}}}}}