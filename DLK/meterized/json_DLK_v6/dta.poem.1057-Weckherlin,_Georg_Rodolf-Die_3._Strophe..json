{"dta.poem.1057": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Die 3. Strophe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1618", "urn": "urn:nbn:de:kobv:b4-20628-9", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich nu das schlecht/ das ich vermag/", "tokens": ["Ich", "nu", "das", "schlecht", "/", "das", "ich", "ver\u00b7mag", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "ADJD", "$(", "PRELS", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erw\u00f6hlend euch ob andern allen/", "tokens": ["Er\u00b7w\u00f6h\u00b7lend", "euch", "ob", "an\u00b7dern", "al\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "PPER", "KOUS", "PIS", "PIAT", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mein herr/ mein hail/ Euch jetz antrag", "tokens": ["Mein", "herr", "/", "mein", "hail", "/", "Euch", "jetz", "an\u00b7trag"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$(", "PPOSAT", "NN", "$(", "PPER", "ADV", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vnd hof/ es soll Euch nicht mi\u00dffallen.", "tokens": ["Vnd", "hof", "/", "es", "soll", "Euch", "nicht", "mi\u00df\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$(", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Vil wolten mit gleichem gesang", "tokens": ["Vil", "wol\u00b7ten", "mit", "glei\u00b7chem", "ge\u00b7sang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "APPR", "ADJA", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "Jhr entlehnete kunst erzaigen/", "tokens": ["Ihr", "ent\u00b7leh\u00b7ne\u00b7te", "kunst", "er\u00b7zai\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "VVINF", "$("], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.7": {"text": "Aber jhr stoltz vnd lieder naigen/", "tokens": ["A\u00b7ber", "jhr", "stoltz", "vnd", "lie\u00b7der", "nai\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "KON", "NN", "VVFIN", "$("], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Ja sterben in jhrem aufgang;", "tokens": ["Ja", "ster\u00b7ben", "in", "jhrem", "auf\u00b7gang", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Jhre dick frembde wort ersticken", "tokens": ["Ih\u00b7re", "dick", "fremb\u00b7de", "wort", "er\u00b7sti\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJD", "ADJA", "NN", "VVINF"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.10": {"text": "Alsbald sie der erfahrnen prob", "tokens": ["Als\u00b7bald", "sie", "der", "er\u00b7fahr\u00b7nen", "prob"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "(so sie ney der hassen) erblicken/", "tokens": ["(", "so", "sie", "ney", "der", "has\u00b7sen", ")", "er\u00b7bli\u00b7cken", "/"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "ADV", "PPER", "APPR", "ART", "NN", "$(", "VVINF", "$("], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.12": {"text": "Vnd jhr finger ist vil zu grob", "tokens": ["Vnd", "jhr", "fin\u00b7ger", "ist", "vil", "zu", "grob"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "VAFIN", "ADV", "PTKA", "ADJD"], "meter": "-++--+-+", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Die Dorische harpf recht zu zwicken.", "tokens": ["Die", "Do\u00b7ri\u00b7sche", "harpf", "recht", "zu", "zwi\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}