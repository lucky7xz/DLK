{"textgrid.poem.66222": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Freiheitslied eines Kolonisten", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hinaus! hinaus ins Ehrenfeld", "tokens": ["Hin\u00b7aus", "!", "hin\u00b7aus", "ins", "Eh\u00b7ren\u00b7feld"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADV", "$.", "APZR", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit blinkendem Gewehr!", "tokens": ["Mit", "blin\u00b7ken\u00b7dem", "Ge\u00b7wehr", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Columbus, deine ganze Welt", "tokens": ["Co\u00b7lum\u00b7bus", ",", "dei\u00b7ne", "gan\u00b7ze", "Welt"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Tritt muthig daher!", "tokens": ["Tritt", "mut\u00b7hig", "da\u00b7her", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PAV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Die G\u00f6ttin ", "tokens": ["Die", "G\u00f6t\u00b7tin"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "(der Sklave sah sie nie)", "tokens": ["(", "der", "Skla\u00b7ve", "sah", "sie", "nie", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "PPER", "ADV", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Geht, Br\u00fcder, seht! sie geht voran!", "tokens": ["Geht", ",", "Br\u00fc\u00b7der", ",", "seht", "!", "sie", "geht", "vo\u00b7ran", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "VVFIN", "$.", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "O blutet f\u00fcr sie!", "tokens": ["O", "blu\u00b7tet", "f\u00fcr", "sie", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "PPER", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Ha, Vater ", "tokens": ["Ha", ",", "Va\u00b7ter"], "token_info": ["word", "punct", "word"], "pos": ["ITJ", "$,", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Und theilt mit uns Gefahr;", "tokens": ["Und", "theilt", "mit", "uns", "Ge\u00b7fahr", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Uns leuchtet, wie ein Pharusthurm,", "tokens": ["Uns", "leuch\u00b7tet", ",", "wie", "ein", "Pha\u00b7rust\u00b7hurm", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sein silbernes Haar!", "tokens": ["Sein", "sil\u00b7ber\u00b7nes", "Haar", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Du, gier'ger Britte, sprichst uns Hohn?", "tokens": ["Du", ",", "gier'\u00b7ger", "Brit\u00b7te", ",", "sprichst", "uns", "Hohn", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJA", "NN", "$,", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da nimm uns unser Gold!", "tokens": ["Da", "nimm", "uns", "un\u00b7ser", "Gold", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es k\u00e4mpft kein B\u00fcrger von Boston", "tokens": ["Es", "k\u00e4mpft", "kein", "B\u00fcr\u00b7ger", "von", "Bos\u00b7ton"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "APPR", "NE"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Um sklavischen Sold!", "tokens": ["Um", "skla\u00b7vi\u00b7schen", "Sold", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUI", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Da seht Europens Sklaven an,", "tokens": ["Da", "seht", "Eu\u00b7ro\u00b7pens", "Skla\u00b7ven", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Ketten rasseln sie!", "tokens": ["In", "Ket\u00b7ten", "ras\u00b7seln", "sie", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie braucht ein Treiber, ein Tyrann,", "tokens": ["Sie", "braucht", "ein", "Trei\u00b7ber", ",", "ein", "Ty\u00b7rann", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr w\u00fcrgbares Vieh.", "tokens": ["F\u00fcr", "w\u00fcrg\u00b7ba\u00b7res", "Vieh", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.6": {"line.1": {"text": "Ihr reicht den feigen Nacken, ihr,", "tokens": ["Ihr", "reicht", "den", "fei\u00b7gen", "Na\u00b7cken", ",", "ihr", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem Tritt der Herrschsucht dar?", "tokens": ["Dem", "Tritt", "der", "Herrschsucht", "dar", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Schwimmt her! hier wohnt die Freiheit, hier!", "tokens": ["Schwimmt", "her", "!", "hier", "wohnt", "die", "Frei\u00b7heit", ",", "hier", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$.", "ADV", "VVFIN", "ART", "NN", "$,", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hier flammt ihr Altar!", "tokens": ["Hier", "flammt", "ihr", "Al\u00b7tar", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.7": {"line.1": {"text": "Doch winkt uns Vater ", "tokens": ["Doch", "winkt", "uns", "Va\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Auf, Br\u00fcder, ins Gewehr!", "tokens": ["Auf", ",", "Br\u00fc\u00b7der", ",", "ins", "Ge\u00b7wehr", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "$,", "NN", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wer nicht f\u00fcr unsre Freiheit ficht,", "tokens": ["Wer", "nicht", "f\u00fcr", "uns\u00b7re", "Frei\u00b7heit", "ficht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den st\u00fcrzet ins Meer!", "tokens": ["Den", "st\u00fcr\u00b7zet", "ins", "Meer", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPRART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.8": {"line.1": {"text": "Herbei, Columbier, herbei!", "tokens": ["Her\u00b7bei", ",", "Co\u00b7lum\u00b7bier", ",", "her\u00b7bei", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["PTKVZ", "$,", "NN", "$,", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Im Antlitz sonnenroth!", "tokens": ["Im", "Ant\u00b7litz", "son\u00b7nen\u00b7roth", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Horch, Britte, unser Feldgeschrei", "tokens": ["Horch", ",", "Brit\u00b7te", ",", "un\u00b7ser", "Feld\u00b7ge\u00b7schrei"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NE", "$,", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist Sieg oder Tod.", "tokens": ["Ist", "Sieg", "o\u00b7der", "Tod", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}