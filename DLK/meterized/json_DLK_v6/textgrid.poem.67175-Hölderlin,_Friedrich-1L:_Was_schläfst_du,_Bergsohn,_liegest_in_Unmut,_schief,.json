{"textgrid.poem.67175": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was schl\u00e4fst du, Bergsohn, liegest in Unmut, schief,", "genre": "verse", "period": "N.A.", "pub_year": 1802, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was schl\u00e4fst du, Bergsohn, liegest in Unmut, schief,", "tokens": ["Was", "schl\u00e4fst", "du", ",", "Berg\u00b7sohn", ",", "lie\u00b7gest", "in", "Un\u00b7mut", ",", "schief", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "NN", "$,", "VVFIN", "APPR", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und frierst am kahlen Ufer, Gedultiger!", "tokens": ["Und", "frierst", "am", "kah\u00b7len", "U\u00b7fer", ",", "Ge\u00b7dul\u00b7ti\u00b7ger", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "ADJA", "NN", "$,", "NN", "$."], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Denkst nicht der Gnade du, wenns an den", "tokens": ["Denkst", "nicht", "der", "Gna\u00b7de", "du", ",", "wenns", "an", "den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PTKNEG", "ART", "NN", "PPER", "$,", "KOUS", "APPR", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Tischen die Himmlischen sonst ged\u00fcrstet?", "tokens": ["Ti\u00b7schen", "die", "Himm\u00b7li\u00b7schen", "sonst", "ge\u00b7d\u00fcrs\u00b7tet", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Kennst drunten du vom Vater die Boten nicht,", "tokens": ["Kennst", "drun\u00b7ten", "du", "vom", "Va\u00b7ter", "die", "Bo\u00b7ten", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "PTKNEG", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Nicht in der Kluft der L\u00fcfte gesch\u00e4rfter Spiel?", "tokens": ["Nicht", "in", "der", "Kluft", "der", "L\u00fcf\u00b7te", "ge\u00b7sch\u00e4rf\u00b7ter", "Spiel", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Trifft nicht das Wort dich, das voll alten", "tokens": ["Trifft", "nicht", "das", "Wort", "dich", ",", "das", "voll", "al\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "ART", "NN", "PPER", "$,", "PRELS", "ADJD", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Geists ein gewanderter Mann dir sendet?", "tokens": ["Geists", "ein", "ge\u00b7wan\u00b7der\u00b7ter", "Mann", "dir", "sen\u00b7det", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Schon t\u00f6nets aber ihm in der Brust. Tief quillts,", "tokens": ["Schon", "t\u00f6\u00b7nets", "a\u00b7ber", "ihm", "in", "der", "Brust", ".", "Tief", "quillts", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "APPR", "ART", "NN", "$.", "ADJD", "VVFIN", "$,"], "meter": "-+-+----++-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Wie damals, als hoch oben im Fels er schlief,", "tokens": ["Wie", "da\u00b7mals", ",", "als", "hoch", "o\u00b7ben", "im", "Fels", "er", "schlief", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$,", "KOUS", "ADJD", "ADV", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ihm auf. Im Zorne reinigt aber", "tokens": ["Ihm", "auf", ".", "Im", "Zor\u00b7ne", "rei\u00b7nigt", "a\u00b7ber"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "PTKVZ", "$.", "APPRART", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sich der Gefesselte nun, nun eilt er,", "tokens": ["Sich", "der", "Ge\u00b7fes\u00b7sel\u00b7te", "nun", ",", "nun", "eilt", "er", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "ADV", "$,", "ADV", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Der Linkische; der spottet der Schlacken nun,", "tokens": ["Der", "Lin\u00b7ki\u00b7sche", ";", "der", "spot\u00b7tet", "der", "Schla\u00b7cken", "nun", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "ART", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und nimmt und bricht und wirft die Zerbrochenen", "tokens": ["Und", "nimmt", "und", "bricht", "und", "wirft", "die", "Zer\u00b7bro\u00b7che\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Zorntrunken, spielend, dort und da zum", "tokens": ["Zorn\u00b7trun\u00b7ken", ",", "spie\u00b7lend", ",", "dort", "und", "da", "zum"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADJD", "$,", "ADV", "KON", "ADV", "APPRART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schauenden Ufer und bei des Fremdlings", "tokens": ["Schau\u00b7en\u00b7den", "U\u00b7fer", "und", "bei", "des", "Fremd\u00b7lings"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "APPR", "ART", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Besondrer Stimme stehen die Herden auf,", "tokens": ["Be\u00b7sond\u00b7rer", "Stim\u00b7me", "ste\u00b7hen", "die", "Her\u00b7den", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Es regen sich die W\u00e4lder, es h\u00f6rt tief Land", "tokens": ["Es", "re\u00b7gen", "sich", "die", "W\u00e4l\u00b7der", ",", "es", "h\u00f6rt", "tief", "Land"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "$,", "PPER", "VVFIN", "ADJD", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Den Stromgeist fern, und schaudernd regt im", "tokens": ["Den", "Strom\u00b7geist", "fern", ",", "und", "schau\u00b7dernd", "regt", "im"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "$,", "KON", "ADJD", "VVFIN", "APPRART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nabel der Erde der Geist sich wieder.", "tokens": ["Na\u00b7bel", "der", "Er\u00b7de", "der", "Geist", "sich", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "PRF", "ADV", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Der Fr\u00fchling k\u00f6mmt. Und jedes, in seiner Art,", "tokens": ["Der", "Fr\u00fch\u00b7ling", "k\u00f6mmt", ".", "Und", "je\u00b7des", ",", "in", "sei\u00b7ner", "Art", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "KON", "PIAT", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Bl\u00fcht. Der ist aber ferne; nicht mehr dabei.", "tokens": ["Bl\u00fcht", ".", "Der", "ist", "a\u00b7ber", "fer\u00b7ne", ";", "nicht", "mehr", "da\u00b7bei", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "PDS", "VAFIN", "ADV", "ADV", "$.", "PTKNEG", "ADV", "PAV", "$."], "meter": "---+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Irr ging er nun; denn allzugut sind", "tokens": ["Irr", "ging", "er", "nun", ";", "denn", "all\u00b7zu\u00b7gut", "sind"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "ADV", "$.", "KON", "ADJD", "VAFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Genien; himmlisch Gespr\u00e4ch ist sein nun.", "tokens": ["Ge\u00b7ni\u00b7en", ";", "himm\u00b7lisch", "Ge\u00b7spr\u00e4ch", "ist", "sein", "nun", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADJD", "NN", "VAFIN", "PPOSAT", "ADV", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}}}}