{"textgrid.poem.61696": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Sommernacht", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Willst du wieder bei mir sein,", "tokens": ["Willst", "du", "wie\u00b7der", "bei", "mir", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "APPR", "PPER", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Muse, die mich l\u00e4ngst gemieden?", "tokens": ["Mu\u00b7se", ",", "die", "mich", "l\u00e4ngst", "ge\u00b7mie\u00b7den", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach, in diesem Sternenschein", "tokens": ["Ach", ",", "in", "die\u00b7sem", "Ster\u00b7nen\u00b7schein"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "APPR", "PDAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Welche F\u00fclle, welch ein Frieden!", "tokens": ["Wel\u00b7che", "F\u00fcl\u00b7le", ",", "welch", "ein", "Frie\u00b7den", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "$,", "PWAT", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Horch! Ged\u00e4mpfter Klang erwacht", "tokens": ["Horch", "!", "Ge\u00b7d\u00e4mpf\u00b7ter", "Klang", "er\u00b7wacht"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$.", "NN", "NN", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "In den unber\u00fchrten Saiten;", "tokens": ["In", "den", "un\u00b7be\u00b7r\u00fchr\u00b7ten", "Sai\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Nimm mich hin denn, s\u00fc\u00dfe Macht!", "tokens": ["Nimm", "mich", "hin", "denn", ",", "s\u00fc\u00b7\u00dfe", "Macht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ADV", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Schon von ferne durch die Nacht", "tokens": ["Schon", "von", "fer\u00b7ne", "durch", "die", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "H\u00f6r' ich G\u00f6tter schreiten.", "tokens": ["H\u00f6r'", "ich", "G\u00f6t\u00b7ter", "schrei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}