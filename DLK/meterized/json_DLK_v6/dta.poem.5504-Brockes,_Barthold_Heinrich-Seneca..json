{"dta.poem.5504": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Seneca.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn wir einst, im Gegenthalt", "tokens": ["Wenn", "wir", "einst", ",", "im", "Ge\u00b7gen\u00b7thalt"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "$,", "APPRART", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Gegen alle Himmels-C\u00f6rper, unsrer Welt Gr\u00f6\u00df\u2019", "tokens": ["Ge\u00b7gen", "al\u00b7le", "Him\u00b7mels\u00b7C\u00f6\u00b7rper", ",", "uns\u00b7rer", "Welt", "Gr\u00f6\u00df'"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "$,", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Mit dem Seneca besehen;", "tokens": ["Mit", "dem", "Se\u00b7ne\u00b7ca", "be\u00b7se\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Werden wir mit ihm, voll Kleinmuth, ruffen und zugleich", "tokens": ["Wer\u00b7den", "wir", "mit", "ihm", ",", "voll", "Klein\u00b7muth", ",", "ruf\u00b7fen", "und", "zu\u00b7gleich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "PPER", "$,", "ADJD", "NN", "$,", "VVFIN", "KON", "ADV"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Da\u00df die Erde nur ein Punct; da\u00df es folglich th\u00f6richt w\u00e4re,", "tokens": ["Da\u00df", "die", "Er\u00b7de", "nur", "ein", "Punct", ";", "da\u00df", "es", "folg\u00b7lich", "th\u00f6\u00b7richt", "w\u00e4\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "ART", "NN", "$.", "KOUS", "PPER", "ADV", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Um den millionsten Theil solches Punctes Krieg zu f\u00fchren,", "tokens": ["Um", "den", "mil\u00b7li\u00b7ons\u00b7ten", "Theil", "sol\u00b7ches", "Punc\u00b7tes", "Krieg", "zu", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "PIAT", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.7": {"text": "Sich zu zancken, Ruhm und Ehre", "tokens": ["Sich", "zu", "zan\u00b7cken", ",", "Ruhm", "und", "Eh\u00b7re"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PRF", "PTKZU", "VVINF", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sich bestreben zu erhalten, sich bem\u00fchen zu regieren.", "tokens": ["Sich", "be\u00b7stre\u00b7ben", "zu", "er\u00b7hal\u00b7ten", ",", "sich", "be\u00b7m\u00fc\u00b7hen", "zu", "re\u00b7gie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVINF", "PTKZU", "VVINF", "$,", "PRF", "VVINF", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.2": {"line.1": {"text": "Aber h\u00f6re, Seneca, dieser dein Gedancke scheinet", "tokens": ["A\u00b7ber", "h\u00f6\u00b7re", ",", "Se\u00b7ne\u00b7ca", ",", "die\u00b7ser", "dein", "Ge\u00b7dan\u00b7cke", "schei\u00b7net"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "NE", "$,", "PDAT", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Freylich gro\u00df und wohl gedacht, wie man auch bi\u00dfher", "tokens": ["Frey\u00b7lich", "gro\u00df", "und", "wohl", "ge\u00b7dacht", ",", "wie", "man", "auch", "bi\u00df\u00b7her"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KON", "ADV", "VVPP", "$,", "PWAV", "PIS", "ADV", "ADV"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Aber dennoch irrest du. Was nicht zu vergleichen ist", "tokens": ["A\u00b7ber", "den\u00b7noch", "ir\u00b7rest", "du", ".", "Was", "nicht", "zu", "ver\u00b7glei\u00b7chen", "ist"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$.", "PWS", "PTKNEG", "PTKZU", "VVINF", "VAFIN"], "meter": "+---+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Das vergleichst du mit einander. Aller Himmel Himmel", "tokens": ["Das", "ver\u00b7gleichst", "du", "mit", "ein\u00b7an\u00b7der", ".", "Al\u00b7ler", "Him\u00b7mel", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "PRF", "$.", "ADJA", "NN", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.5": {"text": "K\u00f6nnten auf dieselbe Weise,", "tokens": ["K\u00f6nn\u00b7ten", "auf", "die\u00b7sel\u00b7be", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Eben wie der Kreis der Erden,", "tokens": ["E\u00b7ben", "wie", "der", "Kreis", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wiederum ein P\u00fcnctlein werden,", "tokens": ["Wie\u00b7de\u00b7rum", "ein", "P\u00fcnct\u00b7lein", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wenn man noch viel gr\u00f6ssre Welte", "tokens": ["Wenn", "man", "noch", "viel", "gr\u00f6ss\u00b7re", "Wel\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ADV", "PIAT", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.9": {"text": "In Vergleich mit ihnen stellte.", "tokens": ["In", "Ver\u00b7gleich", "mit", "ih\u00b7nen", "stell\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Lasset nach der Billigkeit uns viel lieber uns bem\u00fchn,", "tokens": ["Las\u00b7set", "nach", "der", "Bil\u00b7lig\u00b7keit", "uns", "viel", "lie\u00b7ber", "uns", "be\u00b7m\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PPER", "ADV", "ADV", "PPER", "VVINF", "$,"], "meter": "-+--+--+-+-+-+", "measure": "amphibrach.tri.plus"}, "line.11": {"text": "Um den grossen Kreis der Welt mit uns in Vergleich zu", "tokens": ["Um", "den", "gros\u00b7sen", "Kreis", "der", "Welt", "mit", "uns", "in", "Ver\u00b7gleich", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "ADJA", "NN", "ART", "NN", "APPR", "PPER", "APPR", "NN", "PTKZU"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.12": {"text": "So wird unser Welt-Kreis gro\u00df, und der Menschen", "tokens": ["So", "wird", "un\u00b7ser", "Welt\u00b7Kreis", "gro\u00df", ",", "und", "der", "Men\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ADJD", "$,", "KON", "ART", "NN"], "meter": "-+--++-+-+-", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Als die so schon gro\u00df genug. ", "tokens": ["Als", "die", "so", "schon", "gro\u00df", "ge\u00b7nug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADV", "ADV", "ADJD", "ADV", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Du hast hierin freylich recht, wenn du nach dem C\u00f6rper", "tokens": ["Du", "hast", "hie\u00b7rin", "frey\u00b7lich", "recht", ",", "wenn", "du", "nach", "dem", "C\u00f6r\u00b7per"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADJD", "$,", "KOUS", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Einen Menschen rechnen wilst; aber denckst du an die Seele,", "tokens": ["Ei\u00b7nen", "Men\u00b7schen", "rech\u00b7nen", "wilst", ";", "a\u00b7ber", "denckst", "du", "an", "die", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VMFIN", "$.", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Welche nicht nur ewig ist, und worin sich Kr\u00e4fte finden,", "tokens": ["Wel\u00b7che", "nicht", "nur", "e\u00b7wig", "ist", ",", "und", "wo\u00b7rin", "sich", "Kr\u00e4f\u00b7te", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "ADV", "ADJD", "VAFIN", "$,", "KON", "PWAV", "PRF", "NN", "VVINF", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Gr\u00f6ssre C\u00f6rper zu begreiffen, zu bewundern, zu ergr\u00fcnden;", "tokens": ["Gr\u00f6ss\u00b7re", "C\u00f6r\u00b7per", "zu", "be\u00b7greif\u00b7fen", ",", "zu", "be\u00b7wun\u00b7dern", ",", "zu", "er\u00b7gr\u00fcn\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Glaub ich doch, da\u00df Seneca nicht in seiner Meynung fehle:", "tokens": ["Glaub", "ich", "doch", ",", "da\u00df", "Se\u00b7ne\u00b7ca", "nicht", "in", "sei\u00b7ner", "Mey\u00b7nung", "feh\u00b7le", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "$,", "KOUS", "NE", "PTKNEG", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+---+-+-+-+-", "measure": "unknown.measure.hexa"}, "line.6": {"text": "Denn es dienet sein Gedancke, der Begierden Wuht zu", "tokens": ["Denn", "es", "die\u00b7net", "sein", "Ge\u00b7dan\u00b7cke", ",", "der", "Be\u00b7gier\u00b7den", "Wuht", "zu"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN", "$,", "ART", "NN", "NN", "PTKZU"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.7": {"text": "Und uns kr\u00e4ftig anzuhalten, n\u00e4rr\u2019scher Ehrsucht uns zu", "tokens": ["Und", "uns", "kr\u00e4f\u00b7tig", "an\u00b7zu\u00b7hal\u00b7ten", ",", "n\u00e4rr'\u00b7scher", "Ehr\u00b7sucht", "uns", "zu"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ADJD", "VVIZU", "$,", "ADJA", "NN", "PPER", "PTKZU"], "meter": "--+-+-+-+-+--+", "measure": "iambic.hexa.chol"}}}}}