{"textgrid.poem.44767": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Neue Theaterdirektion", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Thespis' altes Reich ist hin,", "tokens": ["Thes\u00b7pis'", "al\u00b7tes", "Reich", "ist", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "VAFIN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Schirm, o heilger Vater!", "tokens": ["Schirm", ",", "o", "heil\u00b7ger", "Va\u00b7ter", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Pantalon und Harlekin", "tokens": ["Pan\u00b7ta\u00b7lon", "und", "Har\u00b7le\u00b7kin"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Meistern das Theater.", "tokens": ["Meis\u00b7tern", "das", "The\u00b7a\u00b7ter", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Pierrot, ein Jammerbild,", "tokens": ["Pier\u00b7rot", ",", "ein", "Jam\u00b7mer\u00b7bild", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Hilft mit bleichen Mienen,", "tokens": ["Hilft", "mit", "blei\u00b7chen", "Mie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und was mehr als alles gilt,", "tokens": ["Und", "was", "mehr", "als", "al\u00b7les", "gilt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PIS", "KOKOM", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sind die Kolombinen.", "tokens": ["Sind", "die", "Ko\u00b7lom\u00b7bi\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}