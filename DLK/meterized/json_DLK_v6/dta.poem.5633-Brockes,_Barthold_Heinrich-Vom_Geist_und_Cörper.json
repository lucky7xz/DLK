{"dta.poem.5633": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Vom Geist und C\u00f6rper", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was f\u00fcr ein Jrrthum hat uns unsern Witz geraubet,", "tokens": ["Was", "f\u00fcr", "ein", "Jrr\u00b7thum", "hat", "uns", "un\u00b7sern", "Witz", "ge\u00b7rau\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "VAFIN", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn wir, die C\u00f6rper erst zu kennen, fest geglaubet?", "tokens": ["Wenn", "wir", ",", "die", "C\u00f6r\u00b7per", "erst", "zu", "ken\u00b7nen", ",", "fest", "ge\u00b7glau\u00b7bet", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ART", "NN", "ADV", "PTKZU", "VVINF", "$,", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Seele selbst war das, so sie zuerst verstand.", "tokens": ["Die", "See\u00b7le", "selbst", "war", "das", ",", "so", "sie", "zu\u00b7erst", "ver\u00b7stand", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "PDS", "$,", "ADV", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sie ist: es wird die\u00df Seyn von ihr erkannt,", "tokens": ["Sie", "ist", ":", "es", "wird", "die\u00df", "Seyn", "von", "ihr", "er\u00b7kannt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$.", "PPER", "VAFIN", "PDS", "PPOSAT", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "So bald als sie das Sinnliche versp\u00fcret,", "tokens": ["So", "bald", "als", "sie", "das", "Sinn\u00b7li\u00b7che", "ver\u00b7sp\u00fc\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "So sie ber\u00fchret.", "tokens": ["So", "sie", "be\u00b7r\u00fch\u00b7ret", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}