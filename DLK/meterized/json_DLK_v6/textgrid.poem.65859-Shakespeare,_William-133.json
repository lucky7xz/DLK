{"textgrid.poem.65859": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "133", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Schmach dem Herzen, das mein Herz entseelt", "tokens": ["O", "Schmach", "dem", "Her\u00b7zen", ",", "das", "mein", "Herz", "ent\u00b7seelt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit tiefer Wunde, mir, wie meinem Freund geschlagen.", "tokens": ["Mit", "tie\u00b7fer", "Wun\u00b7de", ",", "mir", ",", "wie", "mei\u00b7nem", "Freund", "ge\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PPER", "$,", "PWAV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ist's nicht genug, da\u00df man allein mich qu\u00e4lt,", "tokens": ["Ist's", "nicht", "ge\u00b7nug", ",", "da\u00df", "man", "al\u00b7lein", "mich", "qu\u00e4lt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "ADV", "$,", "KOUS", "PIS", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mu\u00df mein Geliebtester noch Sklavenfesseln tragen?", "tokens": ["Mu\u00df", "mein", "Ge\u00b7lieb\u00b7tes\u00b7ter", "noch", "Skla\u00b7ven\u00b7fes\u00b7seln", "tra\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "ADV", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mich hat mir selbst dein grausam Aug' entzogen;", "tokens": ["Mich", "hat", "mir", "selbst", "dein", "grau\u00b7sam", "Aug'", "ent\u00b7zo\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "PPOSAT", "ADJD", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Mein zweites Selbst umspannst du fester noch:", "tokens": ["Mein", "zwei\u00b7tes", "Selbst", "um\u00b7spannst", "du", "fes\u00b7ter", "noch", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "ADJD", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Um ihn, um mich, um dich bin ich betrogen,", "tokens": ["Um", "ihn", ",", "um", "mich", ",", "um", "dich", "bin", "ich", "be\u00b7tro\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "$,", "KOUI", "PPER", "$,", "KOUI", "PPER", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und dreimal dreifach dr\u00fcckt mich dieses Joch.", "tokens": ["Und", "drei\u00b7mal", "drei\u00b7fach", "dr\u00fcckt", "mich", "die\u00b7ses", "Joch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PAV", "VVFIN", "PPER", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Schmiede mein Herz in deines Busens Stein,", "tokens": ["Schmie\u00b7de", "mein", "Herz", "in", "dei\u00b7nes", "Bu\u00b7sens", "Stein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.10": {"text": "Doch dann nimm Freundes Herz f\u00fcr mein arm Herz zum Pfande:", "tokens": ["Doch", "dann", "nimm", "Freun\u00b7des", "Herz", "f\u00fcr", "mein", "arm", "Herz", "zum", "Pfan\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVIMP", "NN", "NN", "APPR", "PPOSAT", "ADJD", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wer auch mich h\u00fctet, ihm la\u00df mein Herz W\u00e4chter sein;", "tokens": ["Wer", "auch", "mich", "h\u00fc\u00b7tet", ",", "ihm", "la\u00df", "mein", "Herz", "W\u00e4ch\u00b7ter", "sein", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "NE", "VAINF", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "So mu\u00dft du lockrer kn\u00fcpfen meine Bande.", "tokens": ["So", "mu\u00dft", "du", "lock\u00b7rer", "kn\u00fcp\u00b7fen", "mei\u00b7ne", "Ban\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Und wirst's doch nicht: denn ich, verwahrt in dir,", "tokens": ["Und", "wir\u00b7st's", "doch", "nicht", ":", "denn", "ich", ",", "ver\u00b7wahrt", "in", "dir", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "PTKNEG", "$.", "KON", "PPER", "$,", "VVFIN", "APPR", "PPER", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.14": {"text": "Bin ewig dein, und alles was an mir.", "tokens": ["Bin", "e\u00b7wig", "dein", ",", "und", "al\u00b7les", "was", "an", "mir", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "PPOSAT", "$,", "KON", "PIS", "PWS", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}