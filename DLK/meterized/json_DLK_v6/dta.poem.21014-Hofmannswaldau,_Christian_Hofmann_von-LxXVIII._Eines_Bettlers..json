{"dta.poem.21014": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "LxXVIII.  \n Eines Bettlers.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Bette/ Tisch/ und Stul war dieses weite", "tokens": ["Mein", "Bet\u00b7te", "/", "Tisch", "/", "und", "Stul", "war", "die\u00b7ses", "wei\u00b7te"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$(", "NN", "$(", "KON", "NN", "VAFIN", "PDAT", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Rund/", "tokens": ["Rund", "/"], "token_info": ["word", "punct"], "pos": ["ADJD", "$("], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Zwo Sachen plagten mich/ der Magen und der", "tokens": ["Zwo", "Sa\u00b7chen", "plag\u00b7ten", "mich", "/", "der", "Ma\u00b7gen", "und", "der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["CARD", "NN", "VVFIN", "PPER", "$(", "ART", "NN", "KON", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mund.", "tokens": ["Mund", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Ich w\u00fcnschte nichts so sehr/ als bald seyn zu begrabe\u0303/", "tokens": ["Ich", "w\u00fcnschte", "nichts", "so", "sehr", "/", "als", "bald", "seyn", "zu", "be\u00b7grab\u1ebd", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "ADV", "ADV", "$(", "KOKOM", "ADV", "VAINF", "PTKZU", "VVINF", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Gleich wie ein ander Mann ein eigen Grab zu haben.", "tokens": ["Gleich", "wie", "ein", "an\u00b7der", "Mann", "ein", "ei\u00b7gen", "Grab", "zu", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "ADJD", "NN", "ART", "ADJA", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}