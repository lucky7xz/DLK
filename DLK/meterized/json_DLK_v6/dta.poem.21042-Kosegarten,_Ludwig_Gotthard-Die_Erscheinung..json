{"dta.poem.21042": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Die Erscheinung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193300", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich lag auf gr&#252;nen Matten,               ", "tokens": ["Ich", "lag", "auf", "gr", "&#252;", "nen", "Mat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "XML_entity", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NE", "$(", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "An klarer Quellen Rand.", "tokens": ["An", "kla\u00b7rer", "Quel\u00b7len", "Rand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mir k\u00fchlten Erlenschatten", "tokens": ["Mir", "k\u00fchl\u00b7ten", "Er\u00b7len\u00b7schat\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["NE", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Wangen heissen Brand.", "tokens": ["Der", "Wan\u00b7gen", "heis\u00b7sen", "Brand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich dachte diess und jenes,", "tokens": ["Ich", "dach\u00b7te", "diess", "und", "je\u00b7nes", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDS", "KON", "PDS", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und tr\u00e4umte sanftbetr\u00fcbt", "tokens": ["Und", "tr\u00e4um\u00b7te", "sanft\u00b7be\u00b7tr\u00fcbt"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Viel S\u00fcsses mir und Sch\u00f6nes,", "tokens": ["Viel", "S\u00fcs\u00b7ses", "mir", "und", "Sch\u00f6\u00b7nes", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPER", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Das diese Welt nicht giebt.", "tokens": ["Das", "die\u00b7se", "Welt", "nicht", "giebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PDAT", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und sieh, dem Hayn entschwebte", "tokens": ["Und", "sieh", ",", "dem", "Hayn", "ent\u00b7schweb\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein M\u00e4gdlein sonnenklar.", "tokens": ["Ein", "M\u00e4gd\u00b7lein", "son\u00b7nen\u00b7klar", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein weisser Schleyer webte", "tokens": ["Ein", "weis\u00b7ser", "Schle\u00b7yer", "web\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Um ihr nussbraunes Haar.", "tokens": ["Um", "ihr", "nuss\u00b7brau\u00b7nes", "Haar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Dem hellen Aug' entgl\u00e4nzte", "tokens": ["Dem", "hel\u00b7len", "Aug'", "ent\u00b7gl\u00e4nz\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Des \u00c4thers reinstes Blau.", "tokens": ["Des", "\u00c4\u00b7thers", "reins\u00b7tes", "Blau", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die frischen Wangen kr\u00e4nzte", "tokens": ["Die", "fri\u00b7schen", "Wan\u00b7gen", "kr\u00e4nz\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die sch\u00f6nste Rosenau.", "tokens": ["Die", "sch\u00f6ns\u00b7te", "Ro\u00b7se\u00b7nau", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Um ihre Lippen schwebte", "tokens": ["Um", "ih\u00b7re", "Lip\u00b7pen", "schweb\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein L\u00e4cheln hold und gut.", "tokens": ["Ein", "L\u00e4\u00b7cheln", "hold", "und", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "An ihren Wimpern bebte", "tokens": ["An", "ih\u00b7ren", "Wim\u00b7pern", "beb\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Thau der Wehemuth.", "tokens": ["Der", "Thau", "der", "We\u00b7he\u00b7muth", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ihr Auge mild' und thr\u00e4nend,", "tokens": ["Ihr", "Au\u00b7ge", "mild'", "und", "thr\u00e4\u00b7nend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NE", "KON", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "So w\u00e4hnt' ich, meinte mich \u2014", "tokens": ["So", "w\u00e4hnt'", "ich", ",", "mein\u00b7te", "mich"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wer war, wie ich, so w\u00e4hnend!", "tokens": ["Wer", "war", ",", "wie", "ich", ",", "so", "w\u00e4h\u00b7nend", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "$,", "PWAV", "PPER", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "So selig, wer, wie ich!", "tokens": ["So", "se\u00b7lig", ",", "wer", ",", "wie", "ich", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "PWS", "$,", "PWAV", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ich auf, sie zu umfassen \u2014", "tokens": ["Ich", "auf", ",", "sie", "zu", "um\u00b7fas\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$,", "PPER", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und ach! sie trat zur\u00fcck.", "tokens": ["Und", "ach", "!", "sie", "trat", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$.", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich sah sie schnell erblassen,", "tokens": ["Ich", "sah", "sie", "schnell", "er\u00b7blas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und tr\u00fcber ward ihr Blick.", "tokens": ["Und", "tr\u00fc\u00b7ber", "ward", "ihr", "Blick", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie sah mich an so innig,", "tokens": ["Sie", "sah", "mich", "an", "so", "in\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sie wies mit ihrer Hand", "tokens": ["Sie", "wies", "mit", "ih\u00b7rer", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Erhaben und tiefsinnig", "tokens": ["Er\u00b7ha\u00b7ben", "und", "tief\u00b7sin\u00b7nig"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "ADJD"], "meter": "-+--++-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Gen Himmel, und verschwand.", "tokens": ["Gen", "Him\u00b7mel", ",", "und", "ver\u00b7schwand", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Fahr wohl, fahr wohl, Erscheinung!", "tokens": ["Fahr", "wohl", ",", "fahr", "wohl", ",", "Er\u00b7schei\u00b7nung", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "$,", "VVFIN", "ADV", "$,", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Fahr wohl! Ich kenn' dich wohl!", "tokens": ["Fahr", "wohl", "!", "Ich", "kenn'", "dich", "wohl", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$.", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und deines Winkes Meinung", "tokens": ["Und", "dei\u00b7nes", "Win\u00b7kes", "Mei\u00b7nung"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Versteh' ich, wie ich soll! \u2014", "tokens": ["Ver\u00b7steh'", "ich", ",", "wie", "ich", "soll", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VVIMP", "PPER", "$,", "PWAV", "PPER", "VMFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "\u201ekein Lieben und kein Loben", "tokens": ["\u201e", "kein", "Lie\u00b7ben", "und", "kein", "Lo\u00b7ben"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PIAT", "ADJA", "KON", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "\u201everdient der Erde Tand.", "tokens": ["\u201e", "ver\u00b7dient", "der", "Er\u00b7de", "Tand", "."], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "\u201enur droben strahlt, nur droben", "tokens": ["\u201e", "nur", "dro\u00b7ben", "strahlt", ",", "nur", "dro\u00b7ben"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "ADV", "ADV", "VVFIN", "$,", "ADV", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "\u201eder Liebe Vaterland!\u201c", "tokens": ["\u201e", "der", "Lie\u00b7be", "Va\u00b7ter\u00b7land", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}