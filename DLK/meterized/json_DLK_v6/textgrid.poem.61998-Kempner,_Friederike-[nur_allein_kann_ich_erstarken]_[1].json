{"textgrid.poem.61998": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "[nur allein kann ich erstarken] [1]", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nur allein kann ich erstarken,", "tokens": ["Nur", "al\u00b7lein", "kann", "ich", "er\u00b7star\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Werden sie Dir Antwort sagen?", "tokens": ["Wer\u00b7den", "sie", "Dir", "Ant\u00b7wort", "sa\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sch\u00f6nheit freilich ist es nicht,", "tokens": ["Sch\u00f6n\u00b7heit", "frei\u00b7lich", "ist", "es", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VAFIN", "PPER", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Was nur aus dem Staube spricht.", "tokens": ["Was", "nur", "aus", "dem", "Stau\u00b7be", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Sch\u00f6n ist nur das Gro\u00dfe, Reine,", "tokens": ["Sch\u00f6n", "ist", "nur", "das", "Gro\u00b7\u00dfe", ",", "Rei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "ART", "ADJA", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meer und Feuer, Sonnenscheine,", "tokens": ["Meer", "und", "Feu\u00b7er", ",", "Son\u00b7nen\u00b7schei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sch\u00f6n ist auch Vergi\u00dfmeinnicht", "tokens": ["Sch\u00f6n", "ist", "auch", "Ver\u00b7gi\u00df\u00b7mein\u00b7nicht"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VAFIN", "ADV", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ein treues Augenlicht!", "tokens": ["Und", "ein", "treu\u00b7es", "Au\u00b7gen\u00b7licht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Alles Gute, Rechte, Biedre,", "tokens": ["Al\u00b7les", "Gu\u00b7te", ",", "Rech\u00b7te", ",", "Bied\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PIAT", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aber alles Andre, Niedre,", "tokens": ["A\u00b7ber", "al\u00b7les", "And\u00b7re", ",", "Nied\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PIS", "PIS", "$,", "PIS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00e4\u00dflich, scheu\u00dflich, ekel ist,", "tokens": ["H\u00e4\u00df\u00b7lich", ",", "scheu\u00df\u00b7lich", ",", "e\u00b7kel", "ist", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Duftig nimmer ist der Mist. \u2013", "tokens": ["Duf\u00b7tig", "nim\u00b7mer", "ist", "der", "Mist", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "ADV", "VAFIN", "ART", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}