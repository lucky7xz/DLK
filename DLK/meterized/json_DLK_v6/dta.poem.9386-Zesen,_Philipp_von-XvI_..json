{"dta.poem.9386": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "XvI .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ach! was mag Sie/ Liebste/ sprechen/", "tokens": ["Ach", "!", "was", "mag", "Sie", "/", "Liebs\u00b7te", "/", "spre\u00b7chen", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ITJ", "$.", "PWS", "VMFIN", "PPER", "$(", "NN", "$(", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "es ist jhre H\u00f6fligkeit/", "tokens": ["es", "ist", "jhre", "H\u00f6f\u00b7lig\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "die sich kann herrau\u00dfer brechen", "tokens": ["die", "sich", "kann", "herr\u00b7au\u00b7\u00dfer", "bre\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PRELS", "PRF", "VMFIN", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "und die hand der Sch\u00f6ne beit/", "tokens": ["und", "die", "hand", "der", "Sch\u00f6\u00b7ne", "beit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "wie solt ich mit blo\u00dfen schertzen", "tokens": ["wie", "solt", "ich", "mit", "blo\u00b7\u00dfen", "schert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PPER", "APPR", "ADJA", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "brechen aus dem treuen hertzen?", "tokens": ["bre\u00b7chen", "aus", "dem", "treu\u00b7en", "hert\u00b7zen", "?"