ch/ ", "tokens": ["Jhm", "noch", "/"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "ADV", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.111": {"text": "Denn wenn erreifferwegt der seltnen Tugend Arten/", "tokens": ["Denn", "wenn", "er\u00b7reif\u00b7fer\u00b7wegt", "der", "selt\u00b7nen", "Tu\u00b7gend", "Ar\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "VVFIN", "ART", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.112": {"text": "So schl\u00e4fft der Balsam noch des Traurens Unmuth ein.", "tokens": ["So", "schl\u00e4fft", "der", "Bal\u00b7sam", "noch", "des", "Trau\u00b7rens", "Un\u00b7muth", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}