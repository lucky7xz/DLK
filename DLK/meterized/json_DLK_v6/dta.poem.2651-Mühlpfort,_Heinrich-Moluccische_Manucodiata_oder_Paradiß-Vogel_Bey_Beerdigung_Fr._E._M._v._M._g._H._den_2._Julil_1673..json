{"dta.poem.2651": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Moluccische   Manucodiata   oder Paradi\u00df-Vogel/  \n Bey Beerdigung Fr. E. M. v. M. g. H. den  \n 2. Julil 1673.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Griechen Leichen-Dienst/ den theils jetzund ver\u00fcben/", "tokens": ["Der", "Grie\u00b7chen", "Lei\u00b7chen\u00b7Dienst", "/", "den", "theils", "je\u00b7tzund", "ver\u00b7\u00fc\u00b7ben", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$(", "ART", "ADV", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nachdem das T\u00fcrcksche Joch sie gantz bew\u00e4ltigt hat/", "tokens": ["Nach\u00b7dem", "das", "T\u00fcrck\u00b7sche", "Joch", "sie", "gantz", "be\u00b7w\u00e4l\u00b7tigt", "hat", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "PPER", "ADV", "VVPP", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Stellt zwar Exempel vor von ihrem treuen Lieben/", "tokens": ["Stellt", "zwar", "Ex\u00b7em\u00b7pel", "vor", "von", "ih\u00b7rem", "treu\u00b7en", "Lie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "APPR", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Doch findet Aberglaub\u2019 allda die meiste statt.", "tokens": ["Doch", "fin\u00b7det", "A\u00b7ber\u00b7glaub'", "all\u00b7da", "die", "meis\u00b7te", "statt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "ADV", "ART", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Denn/ wenn Mann oder Weib von hinnen abgefahren/", "tokens": ["Denn", "/", "wenn", "Mann", "o\u00b7der", "Weib", "von", "hin\u00b7nen", "ab\u00b7ge\u00b7fah\u00b7ren", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "KOUS", "NN", "KON", "NN", "APPR", "ADV", "VVPP", "$("], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Hat man des Grabes Ort mit einem Pfal beehrt/", "tokens": ["Hat", "man", "des", "Gra\u00b7bes", "Ort", "mit", "ei\u00b7nem", "Pfal", "be\u00b7ehrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ART", "NN", "NN", "APPR", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Dran Thiere/ Hirsch und Reh gehenckt in Bildern waren/", "tokens": ["Dran", "Thie\u00b7re", "/", "Hirsch", "und", "Reh", "ge\u00b7henckt", "in", "Bil\u00b7dern", "wa\u00b7ren", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "NN", "$(", "NN", "KON", "NN", "VVPP", "APPR", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So von des Todten Ruhm und Eigenschafft gelehrt.", "tokens": ["So", "von", "des", "Tod\u00b7ten", "Ruhm", "und", "Ei\u00b7gen\u00b7schafft", "ge\u00b7lehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Sie zeigten so die Treu und Flei\u00df in den Gesch\u00e4fften/", "tokens": ["Sie", "zeig\u00b7ten", "so", "die", "Treu", "und", "Flei\u00df", "in", "den", "Ge\u00b7sch\u00e4ff\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "KON", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ja was ein Ehgatt mehr dem andern Guts gethan/", "tokens": ["Ja", "was", "ein", "Eh\u00b7gatt", "mehr", "dem", "an\u00b7dern", "Guts", "ge\u00b7than", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PRELS", "ART", "NN", "ADV", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Sie raufften gar ihr Haar/ und liessens daran hefften", "tokens": ["Sie", "rauff\u00b7ten", "gar", "ihr", "Haar", "/", "und", "lies\u00b7sens", "da\u00b7ran", "heff\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN", "$(", "KON", "ADV", "PAV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die Tieffe ihres Leids hierdurch zu melden an.", "tokens": ["Die", "Tief\u00b7fe", "ih\u00b7res", "Leids", "hier\u00b7durch", "zu", "mel\u00b7den", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "PAV", "PTKZU", "VVINF", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Doch eitler Aberwitz! Wir/ die wir CHristum kennen", "tokens": ["Doch", "eit\u00b7ler", "A\u00b7ber\u00b7witz", "!", "Wir", "/", "die", "wir", "Chris\u00b7tum", "ken\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "$.", "PPER", "$(", "PRELS", "PPER", "NE", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und in dem Gnaden-Bund der Kinder GOttes stehn/", "tokens": ["Und", "in", "dem", "Gna\u00b7den\u00b7Bund", "der", "Kin\u00b7der", "Got\u00b7tes", "stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Die wissen unser Leid weit anders zu benennen/", "tokens": ["Die", "wis\u00b7sen", "un\u00b7ser", "Leid", "weit", "an\u00b7ders", "zu", "be\u00b7nen\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "ADJD", "ADV", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Wann \u00fcber unsern Kopff so schwartze Wolcken gehn.", "tokens": ["Wann", "\u00fc\u00b7ber", "un\u00b7sern", "Kopff", "so", "schwart\u00b7ze", "Wol\u00b7cken", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "PPOSAT", "NN", "ADV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Denn da\u00df/ ", "tokens": ["Denn", "da\u00df", "/"], "token_info": ["word", "word", "punct"], "pos": ["KON", "KOUS", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.18": {"text": "In Jammer-Fluthen schwim\u0303t ist nicht verwunderns werth/", "tokens": ["In", "Jam\u00b7mer\u00b7Flu\u00b7then", "schwim\u0303t", "ist", "nicht", "ver\u00b7wun\u00b7derns", "werth", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "VAFIN", "PTKNEG", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Weil der ergrimmte Schlag von einem solchen Schmertze/", "tokens": ["Weil", "der", "er\u00b7grimm\u00b7te", "Schlag", "von", "ei\u00b7nem", "sol\u00b7chen", "Schmert\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPR", "ART", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Recht durch den Mittel-Punct der treuen Seele f\u00e4hrt.", "tokens": ["Recht", "durch", "den", "Mit\u00b7tel\u00b7Punct", "der", "treu\u00b7en", "See\u00b7le", "f\u00e4hrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Es wird sein bester Schatz/ der Au\u00dfzug aller Freuden", "tokens": ["Es", "wird", "sein", "bes\u00b7ter", "Schatz", "/", "der", "Au\u00df\u00b7zug", "al\u00b7ler", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "ADJA", "NN", "$(", "ART", "NN", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Des Hertzens halber Theil in ihren Sarch gelegt.", "tokens": ["Des", "Hert\u00b7zens", "hal\u00b7ber", "Theil", "in", "ih\u00b7ren", "Sarch", "ge\u00b7legt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPO", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Er siht jetzt Lieb und Treu von seiner Seiten scheiden/", "tokens": ["Er", "siht", "jetzt", "Lieb", "und", "Treu", "von", "sei\u00b7ner", "Sei\u00b7ten", "schei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NN", "KON", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und wie die Asche man davon zu Grabe tr\u00e4gt.", "tokens": ["Und", "wie", "die", "A\u00b7sche", "man", "da\u00b7von", "zu", "Gra\u00b7be", "tr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "PIS", "PAV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Er misst die Tugenden/ so ihn vorhin ergetzet/", "tokens": ["Er", "misst", "die", "Tu\u00b7gen\u00b7den", "/", "so", "ihn", "vor\u00b7hin", "er\u00b7get\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$(", "ADV", "PPER", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Des Hauses Wachsamkeit hat sich nunmehr verwacht:", "tokens": ["Des", "Hau\u00b7ses", "Wach\u00b7sam\u00b7keit", "hat", "sich", "nun\u00b7mehr", "ver\u00b7wacht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "PRF", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Die Mutter so begl\u00fcckt an S\u00f6hnen ward gesch\u00e4tzet/", "tokens": ["Die", "Mut\u00b7ter", "so", "be\u00b7gl\u00fcckt", "an", "S\u00f6h\u00b7nen", "ward", "ge\u00b7sch\u00e4t\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "APPR", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Die Kummer-Wenderin geht in die lange Nacht.", "tokens": ["Die", "Kum\u00b7mer\u00b7Wen\u00b7de\u00b7rin", "geht", "in", "die", "lan\u00b7ge", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Er darff nicht erst ihr Grab mit einem Bild bekr\u00e4ntzen/", "tokens": ["Er", "darff", "nicht", "erst", "ihr", "Grab", "mit", "ei\u00b7nem", "Bild", "be\u00b7kr\u00b7\u00e4nt\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADV", "PPOSAT", "NN", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.30": {"text": "So lange sie gelebt war sie der Tugend Bild:", "tokens": ["So", "lan\u00b7ge", "sie", "ge\u00b7lebt", "war", "sie", "der", "Tu\u00b7gend", "Bild", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "VVPP", "VAFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Sie wiech nach Schnecken-Art nicht aus des Hauses Gr\u00e4ntzen/", "tokens": ["Sie", "wiech", "nach", "Schne\u00b7cken\u00b7Art", "nicht", "aus", "des", "Hau\u00b7ses", "Gr\u00e4nt\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "PTKNEG", "APPR", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Die stille Fr\u00f6mmigkeit blieb stets ihr Ehren-Schild,", "tokens": ["Die", "stil\u00b7le", "Fr\u00f6m\u00b7mig\u00b7keit", "blieb", "stets", "ihr", "Eh\u00b7ren\u00b7Schild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Doch weil es Christlich ist/ und es auch r\u00fchmlich heisset/", "tokens": ["Doch", "weil", "es", "Christ\u00b7lich", "ist", "/", "und", "es", "auch", "r\u00fchm\u00b7lich", "heis\u00b7set", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "VAFIN", "$(", "KON", "PPER", "ADV", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Da\u00df man der Todten Ruhm der Nach-Welt einverleibt/", "tokens": ["Da\u00df", "man", "der", "Tod\u00b7ten", "Ruhm", "der", "Nach\u00b7Welt", "ein\u00b7ver\u00b7leibt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Und ihr so reines Grab von eigner Zierath gleisset/", "tokens": ["Und", "ihr", "so", "rei\u00b7nes", "Grab", "von", "eig\u00b7ner", "Zie\u00b7rath", "gleis\u00b7set", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Die in Gem\u00fcthern bl\u00fcht und stets best\u00e4ndig bleibt/", "tokens": ["Die", "in", "Ge\u00b7m\u00fc\u00b7thern", "bl\u00fcht", "und", "stets", "be\u00b7st\u00e4n\u00b7dig", "bleibt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "VVFIN", "KON", "ADV", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "So soll ein Vogel jetzt ihr Angedencken ehren/", "tokens": ["So", "soll", "ein", "Vo\u00b7gel", "jetzt", "ihr", "An\u00b7ge\u00b7den\u00b7cken", "eh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "ADV", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Der von dem Paradi\u00df den sch\u00f6nen Nahmen f\u00fchrt/", "tokens": ["Der", "von", "dem", "Pa\u00b7ra\u00b7di\u00df", "den", "sch\u00f6\u00b7nen", "Nah\u00b7men", "f\u00fchrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Der die Unsterbligkeit kan durch sein Vorbild lehren", "tokens": ["Der", "die", "U\u00b7nster\u00b7blig\u00b7keit", "kan", "durch", "sein", "Vor\u00b7bild", "leh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.40": {"text": "Und der ", "tokens": ["Und", "der"], "token_info": ["word", "word"], "pos": ["KON", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.41": {"text": "Molucco sendet ihn aus seinen reichen Inseln/", "tokens": ["Mo\u00b7luc\u00b7co", "sen\u00b7det", "ihn", "aus", "sei\u00b7nen", "rei\u00b7chen", "In\u00b7seln", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Wo Nelcken/ Mastix/ Zimm\u2019t in reichster Fruchtbarkeit/", "tokens": ["Wo", "Nel\u00b7cken", "/", "Mas\u00b7tix", "/", "Zim\u00b7m't", "in", "reichs\u00b7ter", "Frucht\u00b7bar\u00b7keit", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$(", "NE", "$(", "VVFIN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+--+-+-+", "measure": 