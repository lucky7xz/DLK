{"dta.poem.9150": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "IiI.  \n Der furchtsame liebhaber.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Was soll ich l\u00e4nger schweigen?", "tokens": ["Was", "soll", "ich", "l\u00e4n\u00b7ger", "schwei\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Es ist doch nun geschehn/", "tokens": ["Es", "ist", "doch", "nun", "ge\u00b7schehn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nachdem die klaren zeugen", "tokens": ["Nach\u00b7dem", "die", "kla\u00b7ren", "zeu\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mir aus den augen sehn:", "tokens": ["Mir", "aus", "den", "au\u00b7gen", "sehn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "VVINF", "$."], "meter": "++-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich habe durch den s\u00fcssen trieb", "tokens": ["Ich", "ha\u00b7be", "durch", "den", "s\u00fcs\u00b7sen", "trieb"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ART", "ADJA", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der sch\u00f6nheit auch ein m\u00e4dgen lieb.", "tokens": ["Der", "sch\u00f6n\u00b7heit", "auch", "ein", "m\u00e4d\u00b7gen", "lieb", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ART", "ADJA", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "2. Und will es jemand wissen/", "tokens": ["Und", "will", "es", "je\u00b7mand", "wis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PIS", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was denn die liebe sey/", "tokens": ["Was", "denn", "die", "lie\u00b7be", "sey", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "ADJA", "VAFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Darauff ich mich beflissen/", "tokens": ["Dar\u00b7auff", "ich", "mich", "be\u00b7flis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "PRF", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So sag ichs ohne scheu/", "tokens": ["So", "sag", "ichs", "oh\u00b7ne", "scheu", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und spreche die beschaffenheit", "tokens": ["Und", "spre\u00b7che", "die", "be\u00b7schaf\u00b7fen\u00b7heit"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der leib ist furcht und bl\u00f6digkeit.", "tokens": ["Der", "leib", "ist", "furcht", "und", "bl\u00f6\u00b7dig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "3. Sonst pfleg ich zwar zu schertzen", "tokens": ["Sonst", "pfleg", "ich", "zwar", "zu", "schert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In allem was ich thu;", "tokens": ["In", "al\u00b7lem", "was", "ich", "thu", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch meine furcht im hertzen", "tokens": ["Doch", "mei\u00b7ne", "furcht", "im", "hert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "L\u00e4sst ihr gar wenig zu:", "tokens": ["L\u00e4sst", "ihr", "gar", "we\u00b7nig", "zu", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PIS", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Denn dieser scrupel f\u00e4llt mir ein/", "tokens": ["Denn", "die\u00b7ser", "scru\u00b7pel", "f\u00e4llt", "mir", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VVFIN", "PPER", "ART", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das m\u00e4dgen m\u00f6chte b\u00f6se seyn.", "tokens": ["Das", "m\u00e4d\u00b7gen", "m\u00f6ch\u00b7te", "b\u00f6\u00b7se", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVINF", "VMFIN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}