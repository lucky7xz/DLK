{"textgrid.poem.66443": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Spruch", "genre": "verse", "period": "N.A.", "pub_year": 1789, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So ging's den Edlen aller Zeit.", "tokens": ["So", "ging's", "den", "Ed\u00b7len", "al\u00b7ler", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was war ihr Lohn? Undankbarkeit.", "tokens": ["Was", "war", "ihr", "Lohn", "?", "Un\u00b7dank\u00b7bar\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "VAFIN", "PPOSAT", "NN", "$.", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der h\u00f6chste Herrscher aller Thronen", "tokens": ["Der", "h\u00f6chs\u00b7te", "Herr\u00b7scher", "al\u00b7ler", "Thro\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Behielt sich's vor, sie selber zu belohnen.", "tokens": ["Be\u00b7hielt", "sich's", "vor", ",", "sie", "sel\u00b7ber", "zu", "be\u00b7loh\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PTKVZ", "$,", "PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}