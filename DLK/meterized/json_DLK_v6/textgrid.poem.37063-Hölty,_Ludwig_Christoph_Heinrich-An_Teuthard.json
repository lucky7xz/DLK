{"textgrid.poem.37063": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "An Teuthard", "genre": "verse", "period": "N.A.", "pub_year": 1772, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Trotz jedem Ausland, st\u00fcrmet Begeisterung", "tokens": ["Trotz", "je\u00b7dem", "Aus\u00b7land", ",", "st\u00fcr\u00b7met", "Be\u00b7geis\u00b7te\u00b7rung"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PIAT", "NN", "$,", "VVFIN", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "In deutschen Seelen. Barden, ihr zeuget es,", "tokens": ["In", "deut\u00b7schen", "See\u00b7len", ".", "Bar\u00b7den", ",", "ihr", "zeu\u00b7get", "es", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$.", "NN", "$,", "PPER", "VVFIN", "PPER", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die ihr von Sarons Palmen, und von", "tokens": ["Die", "ihr", "von", "Sa\u00b7rons", "Pal\u00b7men", ",", "und", "von"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "PPER", "APPR", "NE", "NN", "$,", "KON", "APPR"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Heimischen Eichen euch Kr\u00e4nze wandet.", "tokens": ["Hei\u00b7mi\u00b7schen", "Ei\u00b7chen", "euch", "Kr\u00e4n\u00b7ze", "wan\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Mit schnellern Fl\u00fcgen, als der Hesperier", "tokens": ["Mit", "schnel\u00b7lern", "Fl\u00fc\u00b7gen", ",", "als", "der", "Hes\u00b7pe\u00b7rier"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und Britte flogt ihr, Barden des Vaterlands,", "tokens": ["Und", "Brit\u00b7te", "flogt", "ihr", ",", "Bar\u00b7den", "des", "Va\u00b7ter\u00b7lands", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "$,", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Auf Bragas Gipfel. Noch war D\u00e4mmrung,", "tokens": ["Auf", "Bra\u00b7gas", "Gip\u00b7fel", ".", "Noch", "war", "D\u00e4mm\u00b7rung", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "$.", "ADV", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "D\u00e4mmrung zerflog, und die Mittagssonne", "tokens": ["D\u00e4mm\u00b7rung", "zer\u00b7flog", ",", "und", "die", "Mit\u00b7tags\u00b7son\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "$,", "KON", "ART", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Stand hoch am Himmel. \u2013 \u2013 Muse Teutoniens,", "tokens": ["Stand", "hoch", "am", "Him\u00b7mel", ".", "\u2013", "\u2013", "Mu\u00b7se", "Teu\u00b7to\u00b7ni\u00b7ens", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPRART", "NN", "$.", "$(", "$(", "NN", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du bietest deiner Schwester, der Brittinn Trotz,", "tokens": ["Du", "bie\u00b7test", "dei\u00b7ner", "Schwes\u00b7ter", ",", "der", "Brit\u00b7tinn", "Trotz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und \u00fcberfleugst sie bald; du l\u00e4chelst,", "tokens": ["Und", "\u00fc\u00b7berf\u00b7leugst", "sie", "bald", ";", "du", "l\u00e4\u00b7chelst", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$.", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Muse, der gaukelnden Afterschwester,", "tokens": ["Mu\u00b7se", ",", "der", "gau\u00b7keln\u00b7den", "Af\u00b7ter\u00b7schwes\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Des bunten M\u00e4dchens, das an der Seine Strand", "tokens": ["Des", "bun\u00b7ten", "M\u00e4d\u00b7chens", ",", "das", "an", "der", "Sei\u00b7ne", "Strand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "APPR", "ART", "PPOSAT", "NN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ein Liedgen klimpert. Schande dem Sohne Teuts,", "tokens": ["Ein", "Lied\u00b7gen", "klim\u00b7pert", ".", "Schan\u00b7de", "dem", "Soh\u00b7ne", "Teuts", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "NN", "ART", "NN", "NE", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der's durstig trinket, weil es Wollust", "tokens": ["Der's", "durs\u00b7tig", "trin\u00b7ket", ",", "weil", "es", "Wol\u00b7lust"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "ADJD", "VVFIN", "$,", "KOUS", "PPER", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Durch die entloderten Adern str\u00f6met!", "tokens": ["Durch", "die", "ent\u00b7lo\u00b7der\u00b7ten", "A\u00b7dern", "str\u00f6\u00b7met", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Kein deutscher J\u00fcngling w\u00e4hle das M\u00e4dchen sich,", "tokens": ["Kein", "deut\u00b7scher", "J\u00fcng\u00b7ling", "w\u00e4h\u00b7le", "das", "M\u00e4d\u00b7chen", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "ART", "NN", "PRF", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Das deutsche Lieder ha\u00dfet, und Buhlersang", "tokens": ["Das", "deut\u00b7sche", "Lie\u00b7der", "ha\u00b7\u00dfet", ",", "und", "Buh\u00b7ler\u00b7sang"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "KON", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Des Galliers in ihrer Laute", "tokens": ["Des", "Gal\u00b7liers", "in", "ih\u00b7rer", "Lau\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "T\u00e4ndelnde Silberaccorde t\u00f6net!", "tokens": ["T\u00e4n\u00b7deln\u00b7de", "Sil\u00b7be\u00b7ra\u00b7ccor\u00b7de", "t\u00f6\u00b7net", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Schwing deine Gei\u00dfel, S\u00e4nger der Tugend, schwing", "tokens": ["Schwing", "dei\u00b7ne", "Gei\u00b7\u00dfel", ",", "S\u00e4n\u00b7ger", "der", "Tu\u00b7gend", ",", "schwing"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "NN", "ART", "NN", "$,", "VVFIN"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Die Feuergei\u00dfel, welche die Braga gab,", "tokens": ["Die", "Feu\u00b7er\u00b7gei\u00b7\u00dfel", ",", "wel\u00b7che", "die", "Bra\u00b7ga", "gab", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Den Natternschwarm, der unsre deutsche", "tokens": ["Den", "Nat\u00b7tern\u00b7schwarm", ",", "der", "uns\u00b7re", "deut\u00b7sche"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Redligkeit, Keuschheit und Treue t\u00f6dtet,", "tokens": ["Red\u00b7lig\u00b7keit", ",", "Keuschheit", "und", "Treu\u00b7e", "t\u00f6d\u00b7tet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.7": {"line.1": {"text": "Zur\u00fcckzust\u00e4upen. Ich will, o Freund, inde\u00df,", "tokens": ["Zu\u00b7r\u00fcck\u00b7zu\u00b7st\u00e4u\u00b7pen", ".", "Ich", "will", ",", "o", "Freund", ",", "in\u00b7de\u00df", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$.", "PPER", "VMFIN", "$,", "FM", "NN", "$,", "ADV", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wenn deine Gei\u00dfel brauset, der tollen Brut,", "tokens": ["Wenn", "dei\u00b7ne", "Gei\u00b7\u00dfel", "brau\u00b7set", ",", "der", "tol\u00b7len", "Brut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Am Busen eines deutschen M\u00e4dchens,", "tokens": ["Am", "Bu\u00b7sen", "ei\u00b7nes", "deut\u00b7schen", "M\u00e4d\u00b7chens", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Unter den Blumen des Fr\u00fchlings, lachen.", "tokens": ["Un\u00b7ter", "den", "Blu\u00b7men", "des", "Fr\u00fch\u00b7lings", ",", "la\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}}}}