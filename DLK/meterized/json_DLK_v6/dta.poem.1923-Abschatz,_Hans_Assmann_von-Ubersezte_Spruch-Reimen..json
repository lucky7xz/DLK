{"dta.poem.1923": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Ubersezte Spruch-Reimen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Grosse Kieffern m\u00fcssen offt f\u00fcr der Winde Macht erzit-", "tokens": ["Gros\u00b7se", "Kief\u00b7fern", "m\u00fcs\u00b7sen", "offt", "f\u00fcr", "der", "Win\u00b7de", "Macht", "er\u00b7zit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VMFIN", "ADV", "APPR", "ART", "NN", "NN", "TRUNC"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "tern/", "tokens": ["tern", "/"], "token_info": ["word", "punct"], "pos": ["VVINF", "$("], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Und mit desto schwererm Fall hohe Th\u00fcrme sich ersch\u00fct-", "tokens": ["Und", "mit", "des\u00b7to", "schwe\u00b7rerm", "Fall", "ho\u00b7he", "Th\u00fcr\u00b7me", "sich", "er\u00b7sch\u00fct"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ADJA", "NN", "PRF", "TRUNC"], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "tern/", "tokens": ["tern", "/"], "token_info": ["word", "punct"], "pos": ["VVINF", "$("], "meter": "-", "measure": "single.down"}, "line.5": {"text": "Ja der Felsen Riesen-Haubt von dem Donner-Keil zersplit-", "tokens": ["Ja", "der", "Fel\u00b7sen", "Rie\u00b7sen\u00b7Haubt", "von", "dem", "Don\u00b7ner\u00b7Keil", "zer\u00b7split"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "ART", "NN", "NE", "APPR", "ART", "NN", "TRUNC"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "tern/", "tokens": ["tern", "/"], "token_info": ["word", "punct"], "pos": ["VVINF", "$("], "meter": "-", "measure": "single.down"}, "line.7": {"text": "Wenn das sichre Thal nichts wei\u00df von dergleichen Ungewit-", "tokens": ["Wenn", "das", "sich\u00b7re", "Thal", "nichts", "wei\u00df", "von", "derg\u00b7lei\u00b7chen", "Un\u00b7ge\u00b7wit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "PIS", "VVFIN", "APPR", "PIS", "TRUNC"], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "tern.", "tokens": ["tern", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "-", "measure": "single.down"}}}}}