{"textgrid.poem.52055": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "12.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "H\u00f6rt doch, was Sylvius zu mir vorgestern sprach,", "tokens": ["H\u00f6rt", "doch", ",", "was", "Syl\u00b7vius", "zu", "mir", "vor\u00b7ges\u00b7tern", "sprach", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "PRELS", "NE", "APPR", "PPER", "ADV", "VVFIN", "$,"], "meter": "--+-+-+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Nichts \u00fcblers kan uns Gott, als durch ein Weib begaben:", "tokens": ["Nichts", "\u00fcb\u00b7lers", "kan", "uns", "Gott", ",", "als", "durch", "ein", "Weib", "be\u00b7ga\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "VMFIN", "PPER", "NN", "$,", "KOUS", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So sprach er; als er gleich das Sechste lie\u00df begraben,", "tokens": ["So", "sprach", "er", ";", "als", "er", "gleich", "das", "Sechs\u00b7te", "lie\u00df", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "KOUS", "PPER", "ADV", "ART", "NN", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und holt ihm unverwandt das Siebende hernach.", "tokens": ["Und", "holt", "ihm", "un\u00b7ver\u00b7wandt", "das", "Sie\u00b7ben\u00b7de", "her\u00b7nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}