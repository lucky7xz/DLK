{"dta.poem.19816": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Aussicht in die Ewigkeit .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519168", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O wie gehts im Himmel zu               ", "tokens": ["O", "wie", "gehts", "im", "Him\u00b7mel", "zu"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "PWAV", "VVFIN", "APPRART", "NN", "PTKZU"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und im ewigen Leben,", "tokens": ["Und", "im", "e\u00b7wi\u00b7gen", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "ADJA", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Alles kann man haben gnug,", "tokens": ["Al\u00b7les", "kann", "man", "ha\u00b7ben", "gnug", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PIS", "VAFIN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Darf kein Geld ausgeben,", "tokens": ["Darf", "kein", "Geld", "aus\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Alles darf man borgen,", "tokens": ["Al\u00b7les", "darf", "man", "bor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PIS", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Nicht f\u00fcrs Zahlen sorgen;", "tokens": ["Nicht", "f\u00fcrs", "Zah\u00b7len", "sor\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPRART", "NN", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Wenn ich einmal drinnen w\u00e4r,", "tokens": ["Wenn", "ich", "ein\u00b7mal", "drin\u00b7nen", "w\u00e4r", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Wollt nicht mehr heraus begehr.", "tokens": ["Wollt", "nicht", "mehr", "he\u00b7raus", "be\u00b7gehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "ADV", "APZR", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "F\u00e4llt im Himmel Fasttag ein,", "tokens": ["F\u00e4llt", "im", "Him\u00b7mel", "Fast\u00b7tag", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Speisen wir Forellen,", "tokens": ["Spei\u00b7sen", "wir", "Fo\u00b7rel\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Peter geht in Keller nein,", "tokens": ["Pe\u00b7ter", "geht", "in", "Kel\u00b7ler", "nein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "NN", "PTKANT", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Thut den Wein bestellen;", "tokens": ["Thut", "den", "Wein", "be\u00b7stel\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "David spielt die Harpfen,", "tokens": ["Da\u00b7vid", "spielt", "die", "Harp\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Ulrich bratet Karpfen,", "tokens": ["Ul\u00b7rich", "bra\u00b7tet", "Karp\u00b7fen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Margareth backt K\u00fcchlein gnug,", "tokens": ["Mar\u00b7ga\u00b7reth", "backt", "K\u00fcch\u00b7lein", "gnug", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Paulus schenkt den Wein in Krug.", "tokens": ["Pau\u00b7lus", "schenkt", "den", "Wein", "in", "Krug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Lorenz hinter der K\u00fcchenth\u00fcr,", "tokens": ["Lo\u00b7renz", "hin\u00b7ter", "der", "K\u00fc\u00b7chent\u00b7h\u00fcr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Thut sich auch bewegen,", "tokens": ["Thut", "sich", "auch", "be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Tritt mit seinem Rost herf\u00fcr,", "tokens": ["Tritt", "mit", "sei\u00b7nem", "Rost", "her\u00b7f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Thut Leberw\u00fcrst drauf legen,", "tokens": ["Thut", "Le\u00b7ber\u00b7w\u00fcrst", "drauf", "le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PAV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dorthe und Sabina,", "tokens": ["Dor\u00b7the", "und", "Sa\u00b7bi\u00b7na", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "NE", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "Liesbeth und Cathrina", "tokens": ["Lies\u00b7be\u00b7th", "und", "Cat\u00b7hri\u00b7na"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Alle um den Heerd rum stehn,", "tokens": ["Al\u00b7le", "um", "den", "Heerd", "rum", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "APPR", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Nach den Speisen sie auch sehn.", "tokens": ["Nach", "den", "Spei\u00b7sen", "sie", "auch", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Jezt wolln wir zu Tische gehn,", "tokens": ["Jezt", "wolln", "wir", "zu", "Ti\u00b7sche", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die beste Speis zu essen,", "tokens": ["Die", "bes\u00b7te", "Speis", "zu", "es\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Engel um den Tisch rum stehn,", "tokens": ["Die", "En\u00b7gel", "um", "den", "Tisch", "rum", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schenken Wein in'd Gl\u00e4ser.", "tokens": ["Schen\u00b7ken", "Wein", "in'd", "Gl\u00e4\u00b7ser", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Sie thun uns invitiren,", "tokens": ["Sie", "thun", "uns", "in\u00b7vi\u00b7ti\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.6": {"text": "Der Barthel mu\u00df transchieren,", "tokens": ["Der", "Bar\u00b7thel", "mu\u00df", "tran\u00b7schie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Joseph legt das Essen vor,", "tokens": ["Jo\u00b7se\u00b7ph", "legt", "das", "Es\u00b7sen", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.8": {"text": "C\u00e4cilia b'stellt ein Musikchor.", "tokens": ["C\u00e4\u00b7ci\u00b7lia", "stell", "ein", "Mu\u00b7sik\u00b7chor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Martin auf dem Schimmel reit,", "tokens": ["Mar\u00b7tin", "auf", "dem", "Schim\u00b7mel", "reit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Thut fein gallopiren,", "tokens": ["Thut", "fein", "gal\u00b7lo\u00b7pi\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJD", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Blasi h\u00e4lt die Schmier bereit,", "tokens": ["Bla\u00b7si", "h\u00e4lt", "die", "Schmier", "be\u00b7reit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Thut die Kutschen schmieren,", "tokens": ["Thut", "die", "Kut\u00b7schen", "schmie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "W\u00e4ren wir ja Narren,", "tokens": ["W\u00e4\u00b7ren", "wir", "ja", "Nar\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Wenn wir nicht th\u00e4ten fahren,", "tokens": ["Wenn", "wir", "nicht", "th\u00e4\u00b7ten", "fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und th\u00e4ten alle weil zu Fu\u00dfe gehn,", "tokens": ["Und", "th\u00e4\u00b7ten", "al\u00b7le", "weil", "zu", "Fu\u00b7\u00dfe", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "KOUS", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und lie\u00dfen Ro\u00df und Kutsche stehn.", "tokens": ["Und", "lie\u00b7\u00dfen", "Ro\u00df", "und", "Kut\u00b7sche", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Nun adje du falsche Welt,", "tokens": ["Nun", "ad\u00b7je", "du", "fal\u00b7sche", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Du thust mich verdrie\u00dfen,", "tokens": ["Du", "thust", "mich", "ver\u00b7drie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Im Himmel mir es besser g'f\u00e4llt,", "tokens": ["Im", "Him\u00b7mel", "mir", "es", "bes\u00b7ser", "g'\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Wo alle Freuden fliessen.", "tokens": ["Wo", "al\u00b7le", "Freu\u00b7den", "flies\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Alles ist verf\u00e4nglich,", "tokens": ["Al\u00b7les", "ist", "ver\u00b7f\u00e4ng\u00b7lich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Und alles ist verg\u00e4nglich,", "tokens": ["Und", "al\u00b7les", "ist", "ver\u00b7g\u00e4ng\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wenn ich einmal den Himmel hab,", "tokens": ["Wenn", "ich", "ein\u00b7mal", "den", "Him\u00b7mel", "hab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Hust' ich auf die Welt herab.", "tokens": ["Hust'", "ich", "auf", "die", "Welt", "her\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}