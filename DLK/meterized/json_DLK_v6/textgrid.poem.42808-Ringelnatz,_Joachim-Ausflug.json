{"textgrid.poem.42808": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Ausflug", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es wehten Sommerkleider. Enten schnabelten.", "tokens": ["Es", "weh\u00b7ten", "Som\u00b7mer\u00b7klei\u00b7der", ".", "En\u00b7ten", "schna\u00b7bel\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$.", "NN", "VVFIN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Es knirschten kleine Steine,", "tokens": ["Es", "knirschten", "klei\u00b7ne", "Stei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Und meine Blicke wippten \u00fcber Beine", "tokens": ["Und", "mei\u00b7ne", "Bli\u00b7cke", "wipp\u00b7ten", "\u00fc\u00b7ber", "Bei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von M\u00e4dchen, die Mist gabelten.", "tokens": ["Von", "M\u00e4d\u00b7chen", ",", "die", "Mist", "ga\u00b7bel\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Ein weidgerechter J\u00e4ger kam daher,", "tokens": ["Ein", "weid\u00b7ge\u00b7rech\u00b7ter", "J\u00e4\u00b7ger", "kam", "da\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PAV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der sein Gewehr", "tokens": ["Der", "sein", "Ge\u00b7wehr"], "token_info": ["word", "word", "word"], "pos": ["ART", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "An einem Fels zerschlug", "tokens": ["An", "ei\u00b7nem", "Fels", "zer\u00b7schlug"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und sprach: \u00bbGenug!\u00ab", "tokens": ["Und", "sprach", ":", "\u00bb", "Ge\u00b7nug", "!", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "$.", "$(", "ADV", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Scheu dumme \u2013 hei\u00dft nach unsrer Weltanschauung \u2013", "tokens": ["Scheu", "dum\u00b7me", "\u2013", "hei\u00dft", "nach", "uns\u00b7rer", "Welt\u00b7an\u00b7schau\u00b7ung", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "$(", "VVFIN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Scheu dumme H\u00fchner fl\u00fcchteten nerv\u00f6s,", "tokens": ["Scheu", "dum\u00b7me", "H\u00fch\u00b7ner", "fl\u00fcch\u00b7te\u00b7ten", "ner\u00b7v\u00f6s", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "VVFIN", "NE", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Und eine himmlische Erbauung", "tokens": ["Und", "ei\u00b7ne", "himm\u00b7li\u00b7sche", "Er\u00b7bau\u00b7ung"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Kam \u00fcber mich. Ich war niemandem b\u00f6s.", "tokens": ["Kam", "\u00fc\u00b7ber", "mich", ".", "Ich", "war", "nie\u00b7man\u00b7dem", "b\u00f6s", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPER", "$.", "PPER", "VAFIN", "PIS", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Im Achtzigkilometertempo prickelten", "tokens": ["Im", "Acht\u00b7zig\u00b7ki\u00b7lo\u00b7me\u00b7ter\u00b7tem\u00b7po", "pri\u00b7ckel\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN"], "meter": "-++--+-+-+--", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Uns Phantasien \u00fcber Tod und Gl\u00fcck,", "tokens": ["Uns", "Phan\u00b7ta\u00b7si\u00b7en", "\u00fc\u00b7ber", "Tod", "und", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-----+-+", "measure": "dactylic.init"}, "line.3": {"text": "Und in dem Staub, den wir dabei entwickelten,", "tokens": ["Und", "in", "dem", "Staub", ",", "den", "wir", "da\u00b7bei", "ent\u00b7wi\u00b7ckel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "PAV", "VVFIN", "$,"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.4": {"text": "Blieb rein Geschautes j\u00e4mmerlich zur\u00fcck.", "tokens": ["Blieb", "rein", "Ge\u00b7schau\u00b7tes", "j\u00e4m\u00b7mer\u00b7lich", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Wie ich mich fremd in viel Intimes dachte,", "tokens": ["Wie", "ich", "mich", "fremd", "in", "viel", "In\u00b7ti\u00b7mes", "dach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "ADJD", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "So schnell vorbei, war's keine S\u00fcnd.", "tokens": ["So", "schnell", "vor\u00b7bei", ",", "wa\u00b7r's", "kei\u00b7ne", "S\u00fcnd", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKVZ", "$,", "VAFIN", "PIAT", "NN", "$."], "meter": "-+----+-+", "measure": "dactylic.init"}, "line.3": {"text": "Zerzaust, begl\u00fcckt, weil mir die Landschaft lachte", "tokens": ["Zer\u00b7zaust", ",", "be\u00b7gl\u00fcckt", ",", "weil", "mir", "die", "Land\u00b7schaft", "lach\u00b7te"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "VVPP", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zur Autofahrt Stuttgart nach Schw\u00e4bisch-Gm\u00fcnd.", "tokens": ["Zur", "Au\u00b7to\u00b7fahrt", "Stutt\u00b7gart", "nach", "Schw\u00e4\u00b7bisch\u00b7G\u00b7m\u00fcnd", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NE", "APPR", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}}}}