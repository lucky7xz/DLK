{"textgrid.poem.54789": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "3. Uber den t\u00f6dlichen Hintritt Wolffgangs Freyherren von Gera/ als er in den Blattern verschiden", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein abgeseelter Leib war nichts als eine Wunden.", "tokens": ["Dein", "ab\u00b7ge\u00b7seel\u00b7ter", "Leib", "war", "nichts", "als", "ei\u00b7ne", "Wun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "PIS", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den wie der wehrte Geist/ dem Heiland gleiche war:", "tokens": ["Den", "wie", "der", "wehr\u00b7te", "Geist", "/", "dem", "Hei\u00b7land", "glei\u00b7che", "war", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOKOM", "ART", "ADJA", "NN", "$(", "ART", "NN", "ADJA", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So ist das m\u00fcde Fleisch das hinfill auf die Baar/", "tokens": ["So", "ist", "das", "m\u00fc\u00b7de", "Fleisch", "das", "hin\u00b7fill", "auf", "die", "Baar", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "ART", "ADJD", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Des Herren Jesu Leich auch von uns gleich gefunden.", "tokens": ["Des", "Her\u00b7ren", "Je\u00b7su", "Leich", "auch", "von", "uns", "gleich", "ge\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART",