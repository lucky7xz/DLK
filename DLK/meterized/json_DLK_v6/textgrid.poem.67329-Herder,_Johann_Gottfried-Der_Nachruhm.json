{"textgrid.poem.67329": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Der Nachruhm", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mich reizet nicht des Ruhmes Schall,", "tokens": ["Mich", "rei\u00b7zet", "nicht", "des", "Ruh\u00b7mes", "Schall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der aus Posaunen t\u00f6nt,", "tokens": ["Der", "aus", "Po\u00b7sau\u00b7nen", "t\u00f6nt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den jeder leise Widerhall", "tokens": ["Den", "je\u00b7der", "lei\u00b7se", "Wi\u00b7der\u00b7hall"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im stillen Thal verh\u00f6hnt.", "tokens": ["Im", "stil\u00b7len", "Thal", "ver\u00b7h\u00f6hnt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ein Ruhm, der wie der Sturmwind braust,", "tokens": ["Ein", "Ruhm", ",", "der", "wie", "der", "Sturm\u00b7wind", "braust", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "KOKOM", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ist selbst ein Sturm, der bald versaust.", "tokens": ["Ist", "selbst", "ein", "Sturm", ",", "der", "bald", "ver\u00b7saust", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Mich reizet mehr der Silberton,", "tokens": ["Mich", "rei\u00b7zet", "mehr", "der", "Sil\u00b7ber\u00b7ton", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der unbelauschet klingt", "tokens": ["Der", "un\u00b7be\u00b7lau\u00b7schet", "klingt"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und meiner Muse sch\u00f6nsten Lohn,", "tokens": ["Und", "mei\u00b7ner", "Mu\u00b7se", "sch\u00f6ns\u00b7ten", "Lohn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Dank des Herzens, singt,", "tokens": ["Den", "Dank", "des", "Her\u00b7zens", ",", "singt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Thr\u00e4ne, die dem Aug' entflie\u00dft", "tokens": ["Die", "Thr\u00e4\u00b7ne", ",", "die", "dem", "Aug'", "ent\u00b7flie\u00dft"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und mich mit Bruderliebe gr\u00fc\u00dft.", "tokens": ["Und", "mich", "mit", "Bru\u00b7der\u00b7lie\u00b7be", "gr\u00fc\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Nicht Allen g\u00f6nnte die Natur", "tokens": ["Nicht", "Al\u00b7len", "g\u00f6nn\u00b7te", "die", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "NE", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das allgepriesne Gl\u00fcck,", "tokens": ["Das", "all\u00b7ge\u00b7pri\u00b7es\u00b7ne", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Zu bilden auf des Sch\u00f6pfers Spur", "tokens": ["Zu", "bil\u00b7den", "auf", "des", "Sch\u00f6p\u00b7fers", "Spur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein ew'ges Meisterst\u00fcck,", "tokens": ["Ein", "ew'\u00b7ges", "Meis\u00b7ter\u00b7st\u00fcck", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Das, ein Vollkommnes seiner Art,", "tokens": ["Das", ",", "ein", "Voll\u00b7komm\u00b7nes", "sei\u00b7ner", "Art", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Nachwelt stetes Muster ward,", "tokens": ["Der", "Nach\u00b7welt", "ste\u00b7tes", "Mus\u00b7ter", "ward", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "An dem, im Anblick noch entz\u00fcckt,", "tokens": ["An", "dem", ",", "im", "An\u00b7blick", "noch", "ent\u00b7z\u00fcckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "APPRART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der sp\u00e4te Sch\u00fcler steht", "tokens": ["Der", "sp\u00e4\u00b7te", "Sch\u00fc\u00b7ler", "steht"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und in des Meisters Seele blickt", "tokens": ["Und", "in", "des", "Meis\u00b7ters", "See\u00b7le", "blickt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und stumm von dannen geht,", "tokens": ["Und", "stumm", "von", "dan\u00b7nen", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Inde\u00df sein Herz den seltnen Geist", "tokens": ["In\u00b7de\u00df", "sein", "Herz", "den", "selt\u00b7nen", "Geist"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit lautem Puls gl\u00fcckselig preist.", "tokens": ["Mit", "lau\u00b7tem", "Puls", "gl\u00fcck\u00b7se\u00b7lig", "preist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wir schwimmen in dem Strom der Zeit", "tokens": ["Wir", "schwim\u00b7men", "in", "dem", "Strom", "der", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf Welle Welle fort;", "tokens": ["Auf", "Wel\u00b7le", "Wel\u00b7le", "fort", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Meer der Allvergessenheit", "tokens": ["Das", "Meer", "der", "All\u00b7ver\u00b7ges\u00b7sen\u00b7heit"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist unser letzter Ort.", "tokens": ["Ist", "un\u00b7ser", "letz\u00b7ter", "Ort", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Genug, wenn Welle Welle trieb", "tokens": ["Ge\u00b7nug", ",", "wenn", "Wel\u00b7le", "Wel\u00b7le", "trieb"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und ohne Namen Wirkung blieb;", "tokens": ["Und", "oh\u00b7ne", "Na\u00b7men", "Wir\u00b7kung", "blieb", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Wenn dann auch in der Zeiten Bau", "tokens": ["Wenn", "dann", "auch", "in", "der", "Zei\u00b7ten", "Bau"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ADV", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mich bald ihr Schutt begr\u00e4bt,", "tokens": ["Mich", "bald", "ihr", "Schutt", "be\u00b7gr\u00e4bt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und meine Kraft auf Gottes Au", "tokens": ["Und", "mei\u00b7ne", "Kraft", "auf", "Got\u00b7tes", "Au"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "APPR", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In andern Blumen lebt,", "tokens": ["In", "an\u00b7dern", "Blu\u00b7men", "lebt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und mein Gedanke mit zum Geist", "tokens": ["Und", "mein", "Ge\u00b7dan\u00b7ke", "mit", "zum", "Geist"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "APPR", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vollendender Gedanken fleu\u00dft.", "tokens": ["Voll\u00b7en\u00b7den\u00b7der", "Ge\u00b7dan\u00b7ken", "fleu\u00dft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Sch\u00f6n ist's, von Allen anerkannt,", "tokens": ["Sch\u00f6n", "ist's", ",", "von", "Al\u00b7len", "an\u00b7er\u00b7kannt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "APPR", "NE", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich allgelobt zu sehn;", "tokens": ["Sich", "all\u00b7ge\u00b7lobt", "zu", "sehn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch sch\u00f6ner noch, auch ungenannt,", "tokens": ["Doch", "sch\u00f6\u00b7ner", "noch", ",", "auch", "un\u00b7ge\u00b7nannt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wohlth\u00e4tig fest zu stehn.", "tokens": ["Wohlt\u00b7h\u00e4\u00b7tig", "fest", "zu", "stehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Verdienst ist meines Stolzes Neid", "tokens": ["Ver\u00b7dienst", "ist", "mei\u00b7nes", "Stol\u00b7zes", "Neid"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und bei Verdienst Unsichtbarkeit.", "tokens": ["Und", "bei", "Ver\u00b7dienst", "Un\u00b7sicht\u00b7bar\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "So nennet Gottes Creatur", "tokens": ["So", "nen\u00b7net", "Got\u00b7tes", "Crea\u00b7tur"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Nur schweigend seinen Ruhm;", "tokens": ["Nur", "schwei\u00b7gend", "sei\u00b7nen", "Ruhm", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie bl\u00fcht in wirkender Natur,", "tokens": ["Sie", "bl\u00fcht", "in", "wir\u00b7ken\u00b7der", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr selbst ein Eigenthum.", "tokens": ["Ihr", "selbst", "ein", "Ei\u00b7gen\u00b7thum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der Sch\u00f6pfer zeigt sich nicht, und k\u00fchn", "tokens": ["Der", "Sch\u00f6p\u00b7fer", "zeigt", "sich", "nicht", ",", "und", "k\u00fchn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "PTKNEG", "$,", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Verkennt der Thor und leugnet ihn.", "tokens": ["Ver\u00b7kennt", "der", "Thor", "und", "leug\u00b7net", "ihn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}