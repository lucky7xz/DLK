{"textgrid.poem.29142": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "57.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Pinca darff gar n\u00f6thig Heller,", "tokens": ["Pin\u00b7ca", "darff", "gar", "n\u00f6\u00b7thig", "Hel\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "ADV", "ADJD", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wil verpf\u00e4nden ihren Keller,", "tokens": ["Wil", "ver\u00b7pf\u00e4n\u00b7den", "ih\u00b7ren", "Kel\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Den zu weisen endlich ein,", "tokens": ["Den", "zu", "wei\u00b7sen", "end\u00b7lich", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKZU", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dem sie m\u00f6chte s\u00e4umig seyn;", "tokens": ["Dem", "sie", "m\u00f6ch\u00b7te", "s\u00e4u\u00b7mig", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VMFIN", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wil dazu, Geld eh zu heben,", "tokens": ["Wil", "da\u00b7zu", ",", "Geld", "eh", "zu", "he\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PAV", "$,", "NN", "KOUS", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Auch den Apffelgarten geben.", "tokens": ["Auch", "den", "Apf\u00b7fel\u00b7gar\u00b7ten", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}