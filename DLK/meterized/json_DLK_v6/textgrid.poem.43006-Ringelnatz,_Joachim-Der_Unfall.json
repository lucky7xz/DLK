{"textgrid.poem.43006": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Der Unfall", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es sprach das Gehirn ersch\u00fcttert", "tokens": ["Es", "sprach", "das", "Ge\u00b7hirn", "er\u00b7sch\u00fct\u00b7tert"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVPP"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Zur Nase: \u00bbDu blutest stark!\u00ab", "tokens": ["Zur", "Na\u00b7se", ":", "\u00bb", "Du", "blu\u00b7test", "stark", "!", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "$.", "$(", "PPER", "VVFIN", "ADJD", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Es sagte der Hut verbittert:", "tokens": ["Es", "sag\u00b7te", "der", "Hut", "ver\u00b7bit\u00b7tert", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "\u00bbich bin total zerknittert", "tokens": ["\u00bb", "ich", "bin", "to\u00b7tal", "zer\u00b7knit\u00b7tert"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VAFIN", "ADJD", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und war auf Seide gef\u00fcttert", "tokens": ["Und", "war", "auf", "Sei\u00b7de", "ge\u00b7f\u00fct\u00b7tert"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPR", "NE", "VVPP"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Und kostete drei\u00dfig Mark!\u00ab", "tokens": ["Und", "kos\u00b7te\u00b7te", "drei\u00b7\u00dfig", "Mark", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "CARD", "NN", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Es sagte das Auge verschwommen:", "tokens": ["Es", "sag\u00b7te", "das", "Au\u00b7ge", "ver\u00b7schwom\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "\u00bbich f\u00fchle mich wieder frei.", "tokens": ["\u00bb", "ich", "f\u00fch\u00b7le", "mich", "wie\u00b7der", "frei", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Das Ganze wird uns gut bekommen;", "tokens": ["Das", "Gan\u00b7ze", "wird", "uns", "gut", "be\u00b7kom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das Herz ist nicht entzwei!\u00ab", "tokens": ["Das", "Herz", "ist", "nicht", "ent\u00b7zwei", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "PTKVZ", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Das Herz sagte: \u00bbSowas kommt vor.", "tokens": ["Das", "Herz", "sag\u00b7te", ":", "\u00bb", "So\u00b7was", "kommt", "vor", "."], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "PIS", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vor allem aber lebt unser Humor,", "tokens": ["Vor", "al\u00b7lem", "a\u00b7ber", "lebt", "un\u00b7ser", "Hu\u00b7mor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und deshalb werde ich nun", "tokens": ["Und", "des\u00b7halb", "wer\u00b7de", "ich", "nun"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PAV", "VAFIN", "PPER", "ADV"], "meter": "---+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "In eurem Namen Gott innig danken,", "tokens": ["In", "eu\u00b7rem", "Na\u00b7men", "Gott", "in\u00b7nig", "dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Wie das die Erschreckten und Kranken", "tokens": ["Wie", "das", "die", "Er\u00b7schreck\u00b7ten", "und", "Kran\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PDS", "ART", "NN", "KON", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Leider fast nur die \u2013 tun!\u00ab", "tokens": ["Lei\u00b7der", "fast", "nur", "die", "\u2013", "tun", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["ADV", "ADV", "ADV", "ART", "$(", "VVINF", "$.", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}