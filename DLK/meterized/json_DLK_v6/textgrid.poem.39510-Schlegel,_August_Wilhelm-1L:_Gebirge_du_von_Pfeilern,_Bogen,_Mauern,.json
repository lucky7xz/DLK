{"textgrid.poem.39510": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gebirge du von Pfeilern, Bogen, Mauern,", "genre": "verse", "period": "N.A.", "pub_year": 1805, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gebirge du von Pfeilern, Bogen, Mauern,", "tokens": ["Ge\u00b7bir\u00b7ge", "du", "von", "Pfei\u00b7lern", ",", "Bo\u00b7gen", ",", "Mau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "PPER", "APPR", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit deutscher Kunst des welschen Himmels Prangen!", "tokens": ["Mit", "deut\u00b7scher", "Kunst", "des", "wel\u00b7schen", "Him\u00b7mels", "Pran\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "An deinem hochgeth\u00fcrmten Umri\u00df hangen", "tokens": ["An", "dei\u00b7nem", "hoch\u00b7get\u00b7h\u00fcrm\u00b7ten", "Um\u00b7ri\u00df", "han\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Blicke staunend halb und halb mit Trauern.", "tokens": ["Die", "Bli\u00b7cke", "stau\u00b7nend", "halb", "und", "halb", "mit", "Trau\u00b7ern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADJD", "KON", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ein steinern Heer von V\u00e4tern und Erbauern", "tokens": ["Ein", "stei\u00b7nern", "Heer", "von", "V\u00e4\u00b7tern", "und", "Er\u00b7bau\u00b7ern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Kirche h\u00e4lt dich, selbst ihr Bild, umfangen,", "tokens": ["Der", "Kir\u00b7che", "h\u00e4lt", "dich", ",", "selbst", "ihr", "Bild", ",", "um\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "ADV", "PPOSAT", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und lehrt, wie wandelbar die Zeit empfangen", "tokens": ["Und", "lehrt", ",", "wie", "wan\u00b7del\u00b7bar", "die", "Zeit", "emp\u00b7fan\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "PWAV", "ADJD", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wahrheit, so alle Zeit soll \u00fcberdauern.", "tokens": ["Wahr\u00b7heit", ",", "so", "al\u00b7le", "Zeit", "soll", "\u00fc\u00b7berd\u00b7au\u00b7ern", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "PIAT", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Der Chor vertieft sich ernst in farb'gem Lichte,", "tokens": ["Der", "Chor", "ver\u00b7tieft", "sich", "ernst", "in", "fa\u00b7rb'\u00b7gem", "Lich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Doch Eitelkeit der kl\u00fcgelnden Geschlechter", "tokens": ["Doch", "Ei\u00b7tel\u00b7keit", "der", "kl\u00fc\u00b7geln\u00b7den", "Ge\u00b7schlech\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hat das Portal der alten Form entwendet.", "tokens": ["Hat", "das", "Por\u00b7tal", "der", "al\u00b7ten", "Form", "ent\u00b7wen\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Nun la\u00dfen sie, des Heiligen Ver\u00e4chter,", "tokens": ["Nun", "la\u00b7\u00dfen", "sie", ",", "des", "Hei\u00b7li\u00b7gen", "Ver\u00b7\u00e4ch\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In nacktem Wust den Tempel unvollendet,", "tokens": ["In", "nack\u00b7tem", "Wust", "den", "Tem\u00b7pel", "un\u00b7voll\u00b7en\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und so verstummt die marmorne Geschichte.", "tokens": ["Und", "so", "ver\u00b7stummt", "die", "mar\u00b7mor\u00b7ne", "Ge\u00b7schich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}}}}