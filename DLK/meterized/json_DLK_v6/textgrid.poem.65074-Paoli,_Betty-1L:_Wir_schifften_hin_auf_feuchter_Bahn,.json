{"textgrid.poem.65074": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wir schifften hin auf feuchter Bahn,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir schifften hin auf feuchter Bahn,", "tokens": ["Wir", "schiff\u00b7ten", "hin", "auf", "feuch\u00b7ter", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Meer lag schwarz umnachtet,", "tokens": ["Das", "Meer", "lag", "schwarz", "um\u00b7nach\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Wellen brausten um den Kahn", "tokens": ["Die", "Wel\u00b7len", "braus\u00b7ten", "um", "den", "Kahn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit uns'rem Schmerz befrachtet.", "tokens": ["Mit", "un\u00b7s'\u00b7rem", "Schmerz", "be\u00b7frach\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Es sang sein Lied voll wilder Lust", "tokens": ["Es", "sang", "sein", "Lied", "voll", "wil\u00b7der", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Sturm, beim Schlag der Ruder,", "tokens": ["Der", "Sturm", ",", "beim", "Schlag", "der", "Ru\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Als ahne er in uns'rer Brust", "tokens": ["Als", "ah\u00b7ne", "er", "in", "un\u00b7s'\u00b7rer", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Den nimmerstillen Bruder.", "tokens": ["Den", "nim\u00b7mer\u00b7stil\u00b7len", "Bru\u00b7der", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die Finsterni\u00df ward nur erhellt", "tokens": ["Die", "Fins\u00b7ter\u00b7ni\u00df", "ward", "nur", "er\u00b7hellt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von bleichen Phosphorfunken,", "tokens": ["Von", "blei\u00b7chen", "Phos\u00b7phor\u00b7fun\u00b7ken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es schien die ganze weite Welt", "tokens": ["Es", "schien", "die", "gan\u00b7ze", "wei\u00b7te", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In Zauberschlaf versunken.", "tokens": ["In", "Zau\u00b7ber\u00b7schlaf", "ver\u00b7sun\u00b7ken", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Doch unser'm Herzen war's, als sei", "tokens": ["Doch", "un\u00b7ser'm", "Her\u00b7zen", "wa\u00b7r's", ",", "als", "sei"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "$,", "KOKOM", "VAFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die sturmdurchfurchte Welle", "tokens": ["Die", "sturm\u00b7durch\u00b7furch\u00b7te", "Wel\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ihm zu der Heimath, stark und frei.", "tokens": ["Ihm", "zu", "der", "Hei\u00b7math", ",", "stark", "und", "frei", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die bang gesuchte Schwelle.", "tokens": ["Die", "bang", "ge\u00b7such\u00b7te", "Schwel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Hoch \u00fcber uns der Himmel hehr,", "tokens": ["Hoch", "\u00fc\u00b7ber", "uns", "der", "Him\u00b7mel", "hehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durchwallt von Sternenreigen,", "tokens": ["Durch\u00b7wallt", "von", "Ster\u00b7nen\u00b7rei\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und unter uns, das dunkle Meer,", "tokens": ["Und", "un\u00b7ter", "uns", ",", "das", "dunk\u00b7le", "Meer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Grab voll Ruh' und Schweigen.", "tokens": ["Ein", "Grab", "voll", "Ruh'", "und", "Schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und still verschwamm des Innern Streit", "tokens": ["Und", "still", "ver\u00b7schwamm", "des", "In\u00b7nern", "Streit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu wundersamem Frieden,", "tokens": ["Zu", "wun\u00b7der\u00b7sa\u00b7mem", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da wir von Grab und Seligkeit", "tokens": ["Da", "wir", "von", "Grab", "und", "Se\u00b7lig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nur durch ein Brett geschieden.", "tokens": ["Nur", "durch", "ein", "Brett", "ge\u00b7schie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}