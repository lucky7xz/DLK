{"textgrid.poem.36207": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Fantasie", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Alte T\u00f6ne t\u00f6nen wieder,", "tokens": ["Al\u00b7te", "T\u00f6\u00b7ne", "t\u00f6\u00b7nen", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Rasch entflieht das wilde Leben;", "tokens": ["Rasch", "ent\u00b7flieht", "das", "wil\u00b7de", "Le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Jetzt der Sehnsucht hingegeben,", "tokens": ["Jetzt", "der", "Sehn\u00b7sucht", "hin\u00b7ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn der Knabe einsam weint;", "tokens": ["Wenn", "der", "Kna\u00b7be", "ein\u00b7sam", "weint", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dann zu hoher Lust vereint,", "tokens": ["Dann", "zu", "ho\u00b7her", "Lust", "ver\u00b7eint", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wenn der Freuden Ziel gefunden;", "tokens": ["Wenn", "der", "Freu\u00b7den", "Ziel", "ge\u00b7fun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Bald von leichtem Scherz umwunden,", "tokens": ["Bald", "von", "leich\u00b7tem", "Scherz", "um\u00b7wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "In des \u00dcbermutes F\u00fclle;", "tokens": ["In", "des", "\u00dc\u00b7ber\u00b7mu\u00b7tes", "F\u00fcl\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Zwischendrein die alte Stille,", "tokens": ["Zwi\u00b7schen\u00b7drein", "die", "al\u00b7te", "Stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Frisch lebendig was vergangen,", "tokens": ["Frisch", "le\u00b7ben\u00b7dig", "was", "ver\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "PWS", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Alter Liebe angehangen,", "tokens": ["Al\u00b7ter", "Lie\u00b7be", "an\u00b7ge\u00b7han\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Wie vergangen schon das Neue;", "tokens": ["Wie", "ver\u00b7gan\u00b7gen", "schon", "das", "Neu\u00b7e", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "ADV", "ART", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Schmerzen, die ich nimmer scheue,", "tokens": ["Schmer\u00b7zen", ",", "die", "ich", "nim\u00b7mer", "scheu\u00b7e", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Weil sie tiefre Lust erzeugen,", "tokens": ["Weil", "sie", "tief\u00b7re", "Lust", "er\u00b7zeu\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Kalte Fesseln, die mich beugen,", "tokens": ["Kal\u00b7te", "Fes\u00b7seln", ",", "die", "mich", "beu\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "An der Jugendbl\u00fcte nagen;", "tokens": ["An", "der", "Ju\u00b7gend\u00b7bl\u00fc\u00b7te", "na\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "La\u00dft, o la\u00dft mich alles sagen.", "tokens": ["La\u00dft", ",", "o", "la\u00dft", "mich", "al\u00b7les", "sa\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "FM", "VVIMP", "PPER", "PIS", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Weh, ach weh! ihr \u00f6den Mauern,", "tokens": ["Weh", ",", "ach", "weh", "!", "ihr", "\u00f6\u00b7den", "Mau\u00b7ern", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "PTKVZ", "$.", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo die Blume ward gefunden,", "tokens": ["Wo", "die", "Blu\u00b7me", "ward", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die mit Freuden mich umwunden;", "tokens": ["Die", "mit", "Freu\u00b7den", "mich", "um\u00b7wun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df sie alle gleich verschwunden,", "tokens": ["Da\u00df", "sie", "al\u00b7le", "gleich", "ver\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mu\u00df ich trauern.", "tokens": ["Mu\u00df", "ich", "trau\u00b7ern", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Fr\u00fchen Leiden hingegeben,", "tokens": ["Fr\u00fc\u00b7hen", "Lei\u00b7den", "hin\u00b7ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Mu\u00dfte Sch\u00f6nheit so verderben,", "tokens": ["Mu\u00df\u00b7te", "Sch\u00f6n\u00b7heit", "so", "ver\u00b7der\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "S\u00fc\u00dfe Anmut welkend sterben;", "tokens": ["S\u00fc\u00b7\u00dfe", "An\u00b7mut", "wel\u00b7kend", "ster\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Bl\u00fchend noch mu\u00df Tod erwerben", "tokens": ["Bl\u00fc\u00b7hend", "noch", "mu\u00df", "Tod", "er\u00b7wer\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "ADV", "VMFIN", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "All mein Leben.", "tokens": ["All", "mein", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Kam die Liebe zum Knaben gegangen.", "tokens": ["Kam", "die", "Lie\u00b7be", "zum", "Kna\u00b7ben", "ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Da die lang Ersehnte nun ihm nahte,", "tokens": ["Da", "die", "lang", "Er\u00b7sehn\u00b7te", "nun", "ihm", "nah\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJD", "VVFIN", "ADV", "PPER", "VVFIN", "$,"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Wei\u00df er kaum sein neues Gl\u00fcck zu wagen.", "tokens": ["Wei\u00df", "er", "kaum", "sein", "neu\u00b7es", "Gl\u00fcck", "zu", "wa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Freude, klare Freude gibt ihm alles;", "tokens": ["Freu\u00b7de", ",", "kla\u00b7re", "Freu\u00b7de", "gibt", "ihm", "al\u00b7les", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "VVFIN", "PPER", "PIS", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "In der Freude aber neu Verlangen,", "tokens": ["In", "der", "Freu\u00b7de", "a\u00b7ber", "neu", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "ADJD", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Das die Freude oft zu Leide machte.", "tokens": ["Das", "die", "Freu\u00b7de", "oft", "zu", "Lei\u00b7de", "mach\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "O dies Verlangen", "tokens": ["O", "dies", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["NE", "PDS", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Zu k\u00fchlen, an den Lippen festzuhangen,", "tokens": ["Zu", "k\u00fch\u00b7len", ",", "an", "den", "Lip\u00b7pen", "fest\u00b7zu\u00b7han\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Bis da\u00df in s\u00fc\u00dfer Lust der Sinn vergangen!", "tokens": ["Bis", "da\u00df", "in", "s\u00fc\u00b7\u00dfer", "Lust", "der", "Sinn", "ver\u00b7gan\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "APPR", "ADJA", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Und fa\u00dft dich einmal dieses tiefe Sehnen,", "tokens": ["Und", "fa\u00dft", "dich", "ein\u00b7mal", "die\u00b7ses", "tie\u00b7fe", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "So darfst du nimmer w\u00e4hnen, es zu f\u00fcllen,", "tokens": ["So", "darfst", "du", "nim\u00b7mer", "w\u00e4h\u00b7nen", ",", "es", "zu", "f\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VVINF", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Und wollte dich umh\u00fcllen ganz die Liebe", "tokens": ["Und", "woll\u00b7te", "dich", "um\u00b7h\u00fcl\u00b7len", "ganz", "die", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Zu ihren sch\u00f6nen Freuden.", "tokens": ["Zu", "ih\u00b7ren", "sch\u00f6\u00b7nen", "Freu\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "La\u00df uns fr\u00f6hlich t\u00e4ndeln,", "tokens": ["La\u00df", "uns", "fr\u00f6h\u00b7lich", "t\u00e4n\u00b7deln", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "La\u00df uns Scherz ersinnen,", "tokens": ["La\u00df", "uns", "Scherz", "er\u00b7sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Mit blitzenden Augen,", "tokens": ["Mit", "blit\u00b7zen\u00b7den", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Mit lieblichen Lippen.", "tokens": ["Mit", "lieb\u00b7li\u00b7chen", "Lip\u00b7pen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "O wie s\u00fc\u00df ist die Freude,", "tokens": ["O", "wie", "s\u00fc\u00df", "ist", "die", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Mit der Liebe zu spielen,", "tokens": ["Mit", "der", "Lie\u00b7be", "zu", "spie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.7": {"text": "Und eins mit dem andern", "tokens": ["Und", "eins", "mit", "dem", "an\u00b7dern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "APPR", "ART", "ADJA"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Zu t\u00e4ndeln wie Kinder!", "tokens": ["Zu", "t\u00e4n\u00b7deln", "wie", "Kin\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "KOKOM", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "Nur dich Hohe schm\u00fcckt die Krone.", "tokens": ["Nur", "dich", "Ho\u00b7he", "schm\u00fcckt", "die", "Kro\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "NE", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lichtglanz mu\u00df dich golden zieren,", "tokens": ["Licht\u00b7glanz", "mu\u00df", "dich", "gol\u00b7den", "zie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PRF", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Rosenstrahlend triumphieren,", "tokens": ["Ro\u00b7sen\u00b7strah\u00b7lend", "tri\u00b7um\u00b7phie\u00b7ren", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Herrin, auf des Herzens Throne!", "tokens": ["Her\u00b7rin", ",", "auf", "des", "Her\u00b7zens", "Thro\u00b7ne", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Alles gab ich dir zum Lohne,", "tokens": ["Al\u00b7les", "gab", "ich", "dir", "zum", "Loh\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Alles f\u00fcr die heil'ge Freude,", "tokens": ["Al\u00b7les", "f\u00fcr", "die", "heil'\u00b7ge", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Bis wir freudeflammend beide,", "tokens": ["Bis", "wir", "freu\u00b7de\u00b7flam\u00b7mend", "bei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "PIS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Beide sagten: Nun verschone!", "tokens": ["Bei\u00b7de", "sag\u00b7ten", ":", "Nun", "ver\u00b7scho\u00b7ne", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$.", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Wenn ich unverstanden bliebe", "tokens": ["Wenn", "ich", "un\u00b7ver\u00b7stan\u00b7den", "blie\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ohne Gegenstand mein Streben,", "tokens": ["Oh\u00b7ne", "Ge\u00b7gen\u00b7stand"