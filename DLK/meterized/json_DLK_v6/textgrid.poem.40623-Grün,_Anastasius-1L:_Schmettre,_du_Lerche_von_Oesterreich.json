{"textgrid.poem.40623": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Schmettre, du Lerche von Oesterreich", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schmettre, du Lerche von Oesterreich", "tokens": ["Schmett\u00b7re", ",", "du", "Ler\u00b7che", "von", "O\u00b7es\u00b7ter\u00b7reich"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "PPER", "NN", "APPR", "NE"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Hell von der Donau zum Rhein!", "tokens": ["Hell", "von", "der", "Do\u00b7nau", "zum", "Rhein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NE", "APPRART", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Juble! Du kommest aus Morgenroth,", "tokens": ["Jub\u00b7le", "!", "Du", "kom\u00b7mest", "aus", "Mor\u00b7gen\u00b7roth", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Ziehest in Morgenroth ein.", "tokens": ["Zie\u00b7hest", "in", "Mor\u00b7gen\u00b7roth", "ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "PTKVZ", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Schwinge dich, Adler von Oesterreich,", "tokens": ["Schwin\u00b7ge", "dich", ",", "Ad\u00b7ler", "von", "O\u00b7es\u00b7ter\u00b7reich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NN", "APPR", "NE", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Ledig von Fessel und Band,", "tokens": ["Le\u00b7dig", "von", "Fes\u00b7sel", "und", "Band", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Bringe die Gr\u00fc\u00dfe vom Donaubord", "tokens": ["Brin\u00b7ge", "die", "Gr\u00fc\u00b7\u00dfe", "vom", "Do\u00b7nau\u00b7bord"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Allem germanischen Land!", "tokens": ["Al\u00b7lem", "ger\u00b7ma\u00b7ni\u00b7schen", "Land", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$."], "meter": "----+-+", "measure": "unknown.measure.di"}}, "stanza.3": {"line.1": {"text": "Jauchze, du Herze von Oesterreich,", "tokens": ["Jauch\u00b7ze", ",", "du", "Her\u00b7ze", "von", "O\u00b7es\u00b7ter\u00b7reich", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "APPR", "NE", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Jauchze mit jubelndem Schrei:", "tokens": ["Jauch\u00b7ze", "mit", "ju\u00b7beln\u00b7dem", "Schrei", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Heil dir, mein deutsches Vaterland,", "tokens": ["Heil", "dir", ",", "mein", "deut\u00b7sches", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Einig und m\u00e4chtig und frei!", "tokens": ["Ei\u00b7nig", "und", "m\u00e4ch\u00b7tig", "und", "frei", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "KON", "ADJD", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.4": {"line.1": {"text": "Br\u00fcder, wir Boten aus Oesterreich", "tokens": ["Br\u00fc\u00b7der", ",", "wir", "Bo\u00b7ten", "aus", "O\u00b7es\u00b7ter\u00b7reich"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PPER", "NN", "APPR", "NE"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Gr\u00fc\u00dfen euch traulich mit Sang;", "tokens": ["Gr\u00fc\u00b7\u00dfen", "euch", "trau\u00b7lich", "mit", "Sang", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "APPR", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Schlagt ihr mit freudigem Handschlag ein,", "tokens": ["Schlagt", "ihr", "mit", "freu\u00b7di\u00b7gem", "Hand\u00b7schlag", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Hat es den rechten Klang.", "tokens": ["Hat", "es", "den", "rech\u00b7ten", "Klang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}