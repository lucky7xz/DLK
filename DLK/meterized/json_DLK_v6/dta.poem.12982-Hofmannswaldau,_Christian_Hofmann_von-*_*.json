{"dta.poem.12982": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "* *", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wei\u00df man die danckbarkeit nicht \u00fcberall zu loben?", "tokens": ["Wei\u00df", "man", "die", "dan\u00b7ck\u00b7bar\u00b7keit", "nicht", "\u00fc\u00b7be\u00b7rall", "zu", "lo\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "NN", "PTKNEG", "ADV", "PTKZU", "VVINF", "$."], "meter": "+--+--+-+-+-+-", "measure": "elegiambus"}, "line.2": {"text": "Indessen l\u00e4st sie sich an wenig orten sehn.", "tokens": ["In\u00b7des\u00b7sen", "l\u00e4st", "sie", "sich", "an", "we\u00b7nig", "or\u00b7ten", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So lange Damons wind\u2019 in deine segel wehn,", "tokens": ["So", "lan\u00b7ge", "Da\u00b7mons", "wind'", "in", "dei\u00b7ne", "se\u00b7gel", "wehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NE", "VVFIN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und du noch hoffnung hast, wird jener stets erhoben.", "tokens": ["Und", "du", "noch", "hoff\u00b7nung", "hast", ",", "wird", "je\u00b7ner", "stets", "er\u00b7ho\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "NN", "VAFIN", "$,", "VAFIN", "PDS", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Hast du die wohlthat weg, so mag er ewig ruhn:", "tokens": ["Hast", "du", "die", "wohlt\u00b7hat", "weg", ",", "so", "mag", "er", "e\u00b7wig", "ruhn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "PTKVZ", "$,", "ADV", "VMFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So will kein Criton mehr sich, wie vorhin beqvemen.", "tokens": ["So", "will", "kein", "Cri\u00b7ton", "mehr", "sich", ",", "wie", "vor\u00b7hin", "be\u00b7qve\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIAT", "NN", "ADV", "PRF", "$,", "PWAV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Was du dich nicht gesch\u00e4mt vom Damon anzunehmen,", "tokens": ["Was", "du", "dich", "nicht", "ge\u00b7sch\u00e4mt", "vom", "Da\u00b7mon", "an\u00b7zu\u00b7neh\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PRF", "PTKNEG", "VVPP", "APPRART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Das sch\u00e4mst du dich hernach ihm wieder anzuthun.", "tokens": ["Das", "sch\u00e4mst", "du", "dich", "her\u00b7nach", "ihm", "wie\u00b7der", "an\u00b7zu\u00b7thun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PRF", "ADV", "PPER", "ADV", "VVIZU", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}