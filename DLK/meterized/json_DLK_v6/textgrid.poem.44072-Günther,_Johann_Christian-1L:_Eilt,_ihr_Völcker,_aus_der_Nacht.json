{"textgrid.poem.44072": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Eilt, ihr V\u00f6lcker, aus der Nacht", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eilt, ihr V\u00f6lcker, aus der Nacht", "tokens": ["Eilt", ",", "ihr", "V\u00f6l\u00b7cker", ",", "aus", "der", "Nacht"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Zu dem neugebohrnen Lichte,", "tokens": ["Zu", "dem", "neu\u00b7ge\u00b7bohr\u00b7nen", "Lich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Das den Stall zum Himmel macht,", "tokens": ["Das", "den", "Stall", "zum", "Him\u00b7mel", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Eilt und zeigt Vergn\u00fcgungsfr\u00fcchte", "tokens": ["Eilt", "und", "zeigt", "Ver\u00b7gn\u00fc\u00b7gungs\u00b7fr\u00fcch\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "KON", "VVFIN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Dort, wo sich der Herr der Welt", "tokens": ["Dort", ",", "wo", "sich", "der", "Herr", "der", "Welt"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "PRF", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Uns zum Reichthum arm gestellt.", "tokens": ["Uns", "zum", "Reicht\u00b7hum", "arm", "ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Diese Windeln, dieses Stroh", "tokens": ["Die\u00b7se", "Win\u00b7deln", ",", "die\u00b7ses", "Stroh"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PDAT", "NN", "$,", "PDAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Werden unser Freyheitszeichen;", "tokens": ["Wer\u00b7den", "un\u00b7ser", "Frey\u00b7he\u00b7its\u00b7zei\u00b7chen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$."], "meter": "----+--+-", "measure": "iambic.di.relaxed"}, "line.3": {"text": "Es gef\u00e4llt der Vorsicht so,", "tokens": ["Es", "ge\u00b7f\u00e4llt", "der", "Vor\u00b7sicht", "so", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Satans Arglist zu beschleichen,", "tokens": ["Sa\u00b7tans", "Arg\u00b7list", "zu", "be\u00b7schlei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Dem der Schein der D\u00fcrftigkeit", "tokens": ["Dem", "der", "Schein", "der", "D\u00fcrf\u00b7tig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Schimpf und Gram und Fall verleiht.", "tokens": ["Schimpf", "und", "Gram", "und", "Fall", "ver\u00b7leiht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Jezo werden Wolf und Schaf,", "tokens": ["Je\u00b7zo", "wer\u00b7den", "Wolf", "und", "Schaf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NE", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Kind und L\u00f6wen sicher spielen,", "tokens": ["Kind", "und", "L\u00f6\u00b7wen", "si\u00b7cher", "spie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Zorn, der Adam traf,", "tokens": ["Und", "der", "Zorn", ",", "der", "A\u00b7dam", "traf", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mu\u00df sich in den Thr\u00e4nen k\u00fchlen,", "tokens": ["Mu\u00df", "sich", "in", "den", "Thr\u00e4\u00b7nen", "k\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Deren Strom voll Lieb und Geist", "tokens": ["De\u00b7ren", "Strom", "voll", "Lieb", "und", "Geist"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "NN", "ADJD", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Von des Heilands Wangen fleust.", "tokens": ["Von", "des", "Hei\u00b7lands", "Wan\u00b7gen", "fleust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}