{"dta.poem.19452": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Lenzfahrt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Am Himmel w\u00e4chst der Sonne Glut,", "tokens": ["Am", "Him\u00b7mel", "w\u00e4chst", "der", "Son\u00b7ne", "Glut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aufquillt der See, das Eis zersprang,", "tokens": ["Auf\u00b7quillt", "der", "See", ",", "das", "Eis", "zer\u00b7sprang", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das erste Segel theilt die Flut,", "tokens": ["Das", "ers\u00b7te", "Se\u00b7gel", "theilt", "die", "Flut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mir schwillt das Herz wie Segeldrang.", "tokens": ["Mir", "schwillt", "das", "Herz", "wie", "Se\u00b7gel\u00b7drang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KOKOM", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Zu wandern ist das Herz verdammt,", "tokens": ["Zu", "wan\u00b7dern", "ist", "das", "Herz", "ver\u00b7dammt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das seinen Jugendtag vers\u00e4umt,", "tokens": ["Das", "sei\u00b7nen", "Ju\u00b7gend\u00b7tag", "ver\u00b7s\u00e4umt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sobald die Lenzessonne flammt,", "tokens": ["So\u00b7bald", "die", "Len\u00b7zes\u00b7son\u00b7ne", "flammt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sobald die Welle wieder sch\u00e4umt.", "tokens": ["So\u00b7bald", "die", "Wel\u00b7le", "wie\u00b7der", "sch\u00e4umt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Verscherzte Jugend ist ein Schmerz", "tokens": ["Ver\u00b7scherz\u00b7te", "Ju\u00b7gend", "ist", "ein", "Schmerz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und einer ew'gen Sehnsucht Hort,", "tokens": ["Und", "ei\u00b7ner", "ew'\u00b7gen", "Sehn\u00b7sucht", "Hort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nach seinem Lenze sucht das Herz", "tokens": ["Nach", "sei\u00b7nem", "Len\u00b7ze", "sucht", "das", "Herz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In einem fort, in einem fort!", "tokens": ["In", "ei\u00b7nem", "fort", ",", "in", "ei\u00b7nem", "fort", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PTKVZ", "$,", "APPR", "ART", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und ob die Locke dir ergraut", "tokens": ["Und", "ob", "die", "Lo\u00b7cke", "dir", "er\u00b7graut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und bald das Herz wird stille stehn,", "tokens": ["Und", "bald", "das", "Herz", "wird", "stil\u00b7le", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VAFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Noch mu\u00df es, wann die Welle blaut,", "tokens": ["Noch", "mu\u00df", "es", ",", "wann", "die", "Wel\u00b7le", "blaut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nach seinem Lenze wandern gehn.", "tokens": ["Nach", "sei\u00b7nem", "Len\u00b7ze", "wan\u00b7dern", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}