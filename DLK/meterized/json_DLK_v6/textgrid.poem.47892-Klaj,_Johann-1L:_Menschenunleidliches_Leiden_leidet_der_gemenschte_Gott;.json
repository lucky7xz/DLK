{"textgrid.poem.47892": {"metadata": {"author": {"name": "Klaj, Johann", "birth": "N.A.", "death": "N.A."}, "title": "1L: Menschenunleidliches Leiden leidet der gemenschte Gott;", "genre": "verse", "period": "N.A.", "pub_year": 1636, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Menschenunleidliches Leiden leidet der gemenschte Gott;", "tokens": ["Men\u00b7schen\u00b7un\u00b7leid\u00b7li\u00b7ches", "Lei\u00b7den", "lei\u00b7det", "der", "ge\u00b7menschte", "Gott", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Gott/ der Mensch und Menschenheiland/ dultet undultliche Noht:", "tokens": ["Gott", "/", "der", "Mensch", "und", "Men\u00b7schen\u00b7hei\u00b7land", "/", "dul\u00b7tet", "un\u00b7dult\u00b7li\u00b7che", "Noht", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$(", "ART", "NN", "KON", "NN", "$(", "VVFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-++--+", "measure": "iambic.octa.plus.octa.plus.chol"}, "line.3": {"text": "Er schwitzet Blut und Krafft; Ihn fassen Fessel-Ketten/", "tokens": ["Er", "schwit\u00b7zet", "Blut", "und", "Krafft", ";", "Ihn", "fas\u00b7sen", "Fes\u00b7sel\u00b7Ket\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$.", "PPER", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vns von Blutverschuldter Schuld und des Falles Fall zu retten", "tokens": ["Vns", "von", "Blut\u00b7ver\u00b7schuld\u00b7ter", "Schuld", "und", "des", "Fal\u00b7les", "Fall", "zu", "ret\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "NN", "NN", "KON", "ART", "ADJA", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "Da\u00df wir m\u00f6chten seyn befreiet von der H\u00f6llen Jammerhafft/", "tokens": ["Da\u00df", "wir", "m\u00f6ch\u00b7ten", "seyn", "be\u00b7frei\u00b7et", "von", "der", "H\u00f6l\u00b7len", "Jam\u00b7mer\u00b7hafft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "VAINF", "VVFIN", "APPR", "ART", "NN", "NN", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Wird das L\u00e4mlein angeklaget; Es leidet Hon und Schmach/", "tokens": ["Wird", "das", "L\u00e4m\u00b7lein", "an\u00b7ge\u00b7kla\u00b7get", ";", "Es", "lei\u00b7det", "Hon", "und", "Schmach", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVPP", "$.", "PPER", "VVFIN", "NE", "KON", "NN", "$("], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.7": {"text": "Da\u00df des Teufels S\u00fcndenschulden wird ein ausges\u00fcnte Sach,", "tokens": ["Da\u00df", "des", "Teu\u00b7fels", "S\u00fcn\u00b7den\u00b7schul\u00b7den", "wird", "ein", "aus\u00b7ge\u00b7s\u00fcn\u00b7te", "Sach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Vns zu kauffen solche Zier/ die den Engeln jetzt behaget.", "tokens": ["Vns", "zu", "kauf\u00b7fen", "sol\u00b7che", "Zier", "/", "die", "den", "En\u00b7geln", "jetzt", "be\u00b7ha\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "PIAT", "NN", "$(", "ART", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.9": {"text": "Sein Haubt von Dornen schw\u00fcllt/ der Leib von Geisselstriemen:", "tokens": ["Sein", "Haubt", "von", "Dor\u00b7nen", "schw\u00fcllt", "/", "der", "Leib", "von", "Geis\u00b7sel\u00b7strie\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "VVFIN", "$(", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Vnsern Lohn nach langem Leid dort mit Kronen zu verbl\u00fcmen;", "tokens": ["Vn\u00b7sern", "Lohn", "nach", "lan\u00b7gem", "Leid", "dort", "mit", "Kro\u00b7nen", "zu", "ver\u00b7bl\u00fc\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ADJA", "NN", "ADV", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.11": {"text": "Da\u00df sich S\u00fcndenwunden schl\u00f6ssen/ deren Thun di\u00df Thun bef\u00fcllt.", "tokens": ["Da\u00df", "sich", "S\u00fcn\u00b7den\u00b7wun\u00b7den", "schl\u00f6s\u00b7sen", "/", "de\u00b7ren", "Thun", "di\u00df", "Thun", "be\u00b7f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "NN", "VVINF", "$(", "PRELAT", "NN", "PDS", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.12": {"text": "Er mu\u00df auf Vrtheil sterben/ schwebt zwischen Lufft und Erd:", "tokens": ["Er", "mu\u00df", "auf", "Vrtheil", "ster\u00b7ben", "/", "schwebt", "zwi\u00b7schen", "Lufft", "und", "Erd", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "VVINF", "$(", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "+-----+--+-+", "measure": "trochaic.tetra.relaxed"}, "line.13": {"text": "Da\u00df der Mensch auch lo\u00df gesprochen von des Fluches Ausspruch werd/", "tokens": ["Da\u00df", "der", "Mensch", "auch", "lo\u00df", "ge\u00b7spro\u00b7chen", "von", "des", "Flu\u00b7ches", "Aus\u00b7spruch", "werd", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "ADV", "VVPP", "APPR", "ART", "ADJA", "NN", "VAFIN", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.14": {"text": "Vns den h\u00f6chsterw\u00fcnschten Fried/ mit dem H\u00f6chsten/ zu erwerben/", "tokens": ["Vns", "den", "h\u00f6chs\u00b7ter\u00b7w\u00fcnschten", "Fried", "/", "mit", "dem", "H\u00f6chs\u00b7ten", "/", "zu", "er\u00b7wer\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "$(", "APPR", "ART", "NN", "$(", "PTKZU", "VVINF", "$("], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.15": {"text": "Er gibt das Leben hin/ und gehet auch zu Grabe;", "tokens": ["Er", "gibt", "das", "Le\u00b7ben", "hin", "/", "und", "ge\u00b7het", "auch", "zu", "Gra\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$(", "KON", "VVFIN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Da\u00df an uns des Todesstal f\u00fcrter keine Macht nicht habe", "tokens": ["Da\u00df", "an", "uns", "des", "To\u00b7des\u00b7stal", "f\u00fcr\u00b7ter", "kei\u00b7ne", "Macht", "nicht", "ha\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPER", "ART", "NN", "APPR", "PIAT", "NN", "PTKNEG", "VAFIN"], "meter": "-+--+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.17": {"text": "Vnd wir nach dem Sterben erben dort des Lebens Lustgewinn.", "tokens": ["Vnd", "wir", "nach", "dem", "Ster\u00b7ben", "er\u00b7ben", "dort", "des", "Le\u00b7bens", "Lust\u00b7ge\u00b7winn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "ART", "NN", "VVFIN", "ADV", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.18": {"text": "Also stirbet an dem Holtz selbst\u00e4ndiger Gott und Leben/", "tokens": ["Al\u00b7so", "stir\u00b7bet", "an", "dem", "Holtz", "selb\u00b7st\u00e4n\u00b7di\u00b7ger", "Gott", "und", "Le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "ADJA", "NN", "KON", "NN", "$("], "meter": "+-+-+-+-+--+-+-", "measure": "trochaic.septa.relaxed"}, "line.19": {"text": "Weil das Leben Lebenlo\u00df/ wird es uns das Leben geben.", "tokens": ["Weil", "das", "Le\u00b7ben", "Le\u00b7ben\u00b7lo\u00df", "/", "wird", "es", "uns", "das", "Le\u00b7ben", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "$(", "VAFIN", "PPER", "PRF", "ART", "NN", "VVINF", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}}, "stanza.2": {"line.1": {"text": "Hier Herr Klajens bunter Reim will uns dessen Ausdrukk geben/", "tokens": ["Hier", "Herr", "Kla\u00b7jens", "bun\u00b7ter", "Reim", "will", "uns", "des\u00b7sen", "Aus\u00b7drukk", "ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "NE", "ADJA", "NN", "VMFIN", "PPER", "PDS", "NN", "VVINF", "$("], "meter": "--+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Macht die Wundertraur geschicht in den klugen Schl\u00fcssen leben;", "tokens": ["Macht", "die", "Wun\u00b7der\u00b7traur", "ge\u00b7schicht", "in", "den", "klu\u00b7gen", "Schl\u00fcs\u00b7sen", "le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVPP", "APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "J\u00fcngst vermeldt er unsren Ohren diesen teuren Heilgewinn/", "tokens": ["J\u00fcngst", "ver\u00b7meldt", "er", "un\u00b7sren", "Oh\u00b7ren", "die\u00b7sen", "teu\u00b7ren", "Heil\u00b7ge\u00b7winn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "PDAT", "ADJA", "NN", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Nun gibt diesen auch zu lesen Klajens ausge\u00fcbter Sinn:", "tokens": ["Nun", "gibt", "die\u00b7sen", "auch", "zu", "le\u00b7sen", "Kla\u00b7jens", "aus\u00b7ge\u00b7\u00fcb\u00b7ter", "Sinn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PDAT", "ADV", "PTKZU", "VVINF", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Freilich hat er ihm gesch\u00f6pft hier der K\u00fcnstler Sinnen Habe", "tokens": ["Frei\u00b7lich", "hat", "er", "ihm", "ge\u00b7sch\u00f6pft", "hier", "der", "K\u00fcnst\u00b7ler", "Sin\u00b7nen", "Ha\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "VVPP", "ADV", "ART", "NN", "NN", "NN"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Von den Zeiten/ wie man grub unsers S\u00fcndentilgers Grabe/", "tokens": ["Von", "den", "Zei\u00b7ten", "/", "wie", "man", "grub", "un\u00b7sers", "S\u00fcn\u00b7den\u00b7til\u00b7gers", "Gra\u00b7be", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$(", "PWAV", "PIS", "VVFIN", "PPOSAT", "NN", "NN", "$("], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.7": {"text": "Kan er also redsam singen/ da\u00df ich fast beredet werd/", "tokens": ["Kan", "er", "al\u00b7so", "red\u00b7sam", "sin\u00b7gen", "/", "da\u00df", "ich", "fast", "be\u00b7re\u00b7det", "werd", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADJD", "VVINF", "$(", "KOUS", "PPER", "ADV", "VVPP", "VAFIN", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Vnser Klaj hab der Zeiten auch gelebet auf der Erd.", "tokens": ["Vn\u00b7ser", "Klaj", "hab", "der", "Zei\u00b7ten", "auch", "ge\u00b7le\u00b7bet", "auf", "der", "Erd", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "ART", "NN", "ADV", "VVPP", "APPR", "ART", "NN", "$."], "meter": "+-+--+-+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.9": {"text": "Was Nutz aber bringt es denn/ ihm der Musen Gunst erwerben?", "tokens": ["Was", "Nutz", "a\u00b7ber", "bringt", "es", "denn", "/", "ihm", "der", "Mu\u00b7sen", "Gunst", "er\u00b7wer\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "ADV", "VVFIN", "PPER", "ADV", "$(", "PPER", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+--+--+-+-+-+-", "measure": "amphibrach.tri.plus"}, "line.10": {"text": "Da\u00df er unverlebet lebt und im Sterben nicht kan sterben/", "tokens": ["Da\u00df", "er", "un\u00b7ver\u00b7le\u00b7bet", "lebt", "und", "im", "Ster\u00b7ben", "nicht", "kan", "ster\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VVFIN", "KON", "APPRART", "NN", "PTKNEG", "VMFIN", "VVINF", "$("], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.11": {"text": "Da\u00df er mit des Ruhmes Boten Welt und Wolken \u00fcberf\u00fcllt/", "tokens": ["Da\u00df", "er", "mit", "des", "Ruh\u00b7mes", "Bo\u00b7ten", "Welt", "und", "Wol\u00b7ken", "\u00fc\u00b7berf\u00b7\u00fcllt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "ADJA", "NN", "NN", "KON", "NN", "VVFIN", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.12": {"text": "Vnd der Schrifftberuffte Nam hin bis zu den Sternen schw\u00fcllt.", "tokens": ["Vnd", "der", "Schrifft\u00b7be\u00b7ruff\u00b7te", "Nam", "hin", "bis", "zu", "den", "Ster\u00b7nen", "schw\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NE", "ADV", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "--+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "Doch er lehret nicht allein; Anmut mu\u00df die Red verbl\u00fcmen/", "tokens": ["Doch", "er", "leh\u00b7ret", "nicht", "al\u00b7lein", ";", "An\u00b7mut", "mu\u00df", "die", "Red", "ver\u00b7bl\u00fc\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKNEG", "ADV", "$.", "NN", "VMFIN", "ART", "NN", "VVINF", "$("], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.14": {"text": "Wie die Matten frischbegr\u00fcnt zieren ihres Malwerks Striemen:", "tokens": ["Wie", "die", "Mat\u00b7ten", "frischbe\u00b7gr\u00fcnt", "zie\u00b7ren", "ih\u00b7res", "Mal\u00b7werks", "Strie\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJD", "VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.15": {"text": "Von den blossen Sachen reden bl\u00f6\u00dflich/ ist gemeine Sach;", "tokens": ["Von", "den", "blos\u00b7sen", "Sa\u00b7chen", "re\u00b7den", "bl\u00f6\u00df\u00b7lich", "/", "ist", "ge\u00b7mei\u00b7ne", "Sach", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "ADJD", "$(", "VAFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.16": {"text": "Kunst und Stikkwerk unterstreuen/ di\u00df erwekket Neider Schmach.", "tokens": ["Kunst", "und", "Stikk\u00b7werk", "un\u00b7ter\u00b7streu\u00b7en", "/", "di\u00df", "er\u00b7wek\u00b7ket", "Nei\u00b7der", "Schmach", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVINF", "$(", "PDS", "VVFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.17": {"text": "Pegnitz/ ehre deinen Schwan/ der vor seiner Elb behaget;", "tokens": ["Peg\u00b7nitz", "/", "eh\u00b7re", "dei\u00b7nen", "Schwan", "/", "der", "vor", "sei\u00b7ner", "Elb", "be\u00b7ha\u00b7get", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "VVFIN", "PPOSAT", "NN", "$(", "ART", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.18": {"text": "Da\u00df er ihr von dir entwand/ ist/ wor\u00fcm sie vielmals klaget:", "tokens": ["Da\u00df", "er", "ihr", "von", "dir", "ent\u00b7wand", "/", "ist", "/", "wo\u00b7r\u00fcm", "sie", "viel\u00b7mals", "kla\u00b7get", ":"], "token_info": ["