{"textgrid.poem.43957": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich nehm in Brust und Armen", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich nehm in Brust und Armen", "tokens": ["Ich", "nehm", "in", "Brust", "und", "Ar\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den schweren Abschiedsku\u00df.", "tokens": ["Den", "schwe\u00b7ren", "Ab\u00b7schieds\u00b7ku\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Himmel hat Erbarmen,", "tokens": ["Der", "Him\u00b7mel", "hat", "Er\u00b7bar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Indem er trennen mu\u00df.", "tokens": ["In\u00b7dem", "er", "tren\u00b7nen", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich k\u00fc\u00df, ich wein und liebe,", "tokens": ["Ich", "k\u00fc\u00df", ",", "ich", "wein", "und", "lie\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "PTKVZ", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mein treues Lorchen spricht,", "tokens": ["Mein", "treu\u00b7es", "Lor\u00b7chen", "spricht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Sie habe gleiche Triebe;", "tokens": ["Sie", "ha\u00b7be", "glei\u00b7che", "Trie\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wie aber, weint sie nicht?", "tokens": ["Wie", "a\u00b7ber", ",", "weint", "sie", "nicht", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$,", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Du suchest ja dein Gl\u00fccke,", "tokens": ["Du", "su\u00b7chest", "ja", "dein", "Gl\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das hier wohl nicht mehr bl\u00fcht.", "tokens": ["Das", "hier", "wohl", "nicht", "mehr", "bl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADV", "PTKNEG", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich ha\u00dfe das Geschicke,", "tokens": ["Ich", "ha\u00b7\u00dfe", "das", "Ge\u00b7schi\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das uns vonsammen zieht.", "tokens": ["Das", "uns", "von\u00b7sam\u00b7men", "zieht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ach, s\u00e4hstu meine Schmerzen \u2013", "tokens": ["Ach", ",", "s\u00e4h\u00b7stu", "mei\u00b7ne", "Schmer\u00b7zen", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ich schweige, werthes Licht;", "tokens": ["Ich", "schwei\u00b7ge", ",", "wert\u00b7hes", "Licht", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ich liebe dich von Herzen,", "tokens": ["Ich", "lie\u00b7be", "dich", "von", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und darum wein ich nicht.", "tokens": ["Und", "da\u00b7rum", "wein", "ich", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}