{"textgrid.poem.43011": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Wer hat gewonnen?", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weil du ber\u00fchmt bist und mir Wahrheit sagst,", "tokens": ["Weil", "du", "be\u00b7r\u00fchmt", "bist", "und", "mir", "Wahr\u00b7heit", "sagst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN", "KON", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Zerschlag ich dir ein St\u00fcck von deinen Z\u00e4hnen.", "tokens": ["Zer\u00b7schlag", "ich", "dir", "ein", "St\u00fcck", "von", "dei\u00b7nen", "Z\u00e4h\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nun du mich meidest und sogar verklagst,", "tokens": ["Nun", "du", "mich", "mei\u00b7dest", "und", "so\u00b7gar", "ver\u00b7klagst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PRF", "VVFIN", "KON", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Bitt ich dich um Verzeihung unter Tr\u00e4nen.", "tokens": ["Bitt", "ich", "dich", "um", "Ver\u00b7zei\u00b7hung", "un\u00b7ter", "Tr\u00e4\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Du l\u00e4chelst siegreich, l\u00e4\u00dft mich also leiden. \u2013 \u2013", "tokens": ["Du", "l\u00e4\u00b7chelst", "sieg\u00b7reich", ",", "l\u00e4\u00dft", "mich", "al\u00b7so", "lei\u00b7den", ".", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "ADV", "VVINF", "$.", "$(", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich werde reich und gut. Du wirst senil.", "tokens": ["Ich", "wer\u00b7de", "reich", "und", "gut", ".", "Du", "wirst", "se\u00b7nil", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "KON", "ADJD", "$.", "PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich frech. \u2013 Und wir vers\u00f6hnen uns und meiden", "tokens": ["Ich", "frech", ".", "\u2013", "Und", "wir", "ver\u00b7s\u00f6h\u00b7nen", "uns", "und", "mei\u00b7den"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADJD", "$.", "$(", "KON", "PPER", "VVFIN", "PPER", "KON", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und plagen uns im wechselschroffen Spiel.", "tokens": ["Und", "pla\u00b7gen", "uns", "im", "wech\u00b7sel\u00b7schrof\u00b7fen", "Spiel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch immer ruhiger und mehr besonnen", "tokens": ["Doch", "im\u00b7mer", "ru\u00b7hi\u00b7ger", "und", "mehr", "be\u00b7son\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "KON", "ADV", "VVINF"], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Legt sich der Kampf. Die Wahrheit steht. Es fragt", "tokens": ["Legt", "sich", "der", "Kampf", ".", "Die", "Wahr\u00b7heit", "steht", ".", "Es", "fragt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PRF", "ART", "NN", "$.", "ART", "NN", "VVFIN", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Jeder von uns und jeder neu verzagt:", "tokens": ["Je\u00b7der", "von", "uns", "und", "je\u00b7der", "neu", "ver\u00b7zagt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "PPER", "KON", "PIS", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "\u00bbwer hat gewonnen?!\u00ab", "tokens": ["\u00bb", "wer", "hat", "ge\u00b7won\u00b7nen", "?!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWS", "VAFIN", "VVPP", "$.", "$("], "meter": "-+-+-", "measure": "iambic.di"}}}}}