{"dta.poem.1219": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Keine veracht/ Nach einer getracht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Climen ist hurtig und geschickt/", "tokens": ["Cli\u00b7men", "ist", "hur\u00b7tig", "und", "ge\u00b7schickt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJD", "KON", "VVPP", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Mit Gold und Schnee kan Iris prangen/", "tokens": ["Mit", "Gold", "und", "Schnee", "kan", "I\u00b7ris", "pran\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VMFIN", "NE", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Belisens Rede macht entz\u00fcckt/", "tokens": ["Be\u00b7li\u00b7sens", "Re\u00b7de", "macht", "ent\u00b7z\u00fcckt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "VVPP", "$("], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.4": {"text": "Amenens Zier h\u00e4lt viel gefangen/", "tokens": ["A\u00b7me\u00b7nens", "Zier", "h\u00e4lt", "viel", "ge\u00b7fan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "ADV", "ADJD", "$("], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Bey wem kan sich mit Blick und Lachen", "tokens": ["Bey", "wem", "kan", "sich", "mit", "Blick", "und", "La\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PWS", "VMFIN", "PRF", "APPR", "NN", "KON", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Nicht angenehm die Fillis machen?", "tokens": ["Nicht", "an\u00b7ge\u00b7nehm", "die", "Fil\u00b7lis", "ma\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "ART", "NE", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich stehe zu/ da\u00df solcher Schein", "tokens": ["Ich", "ste\u00b7he", "zu", "/", "da\u00df", "sol\u00b7cher", "Schein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKZU", "$(", "KOUS", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mir \u00f6ffters in die Augen stralet;", "tokens": ["Mir", "\u00f6ff\u00b7ters", "in", "die", "Au\u00b7gen", "stra\u00b7let", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Doch bleibt mein treues Hertze rein/", "tokens": ["Doch", "bleibt", "mein", "treu\u00b7es", "Hert\u00b7ze", "rein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "VVFIN", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Darein ein ander Bild gemahlet/", "tokens": ["Da\u00b7rein", "ein", "an\u00b7der", "Bild", "ge\u00b7mah\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJD", "NN", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und dannenher kan ich erkennen/", "tokens": ["Und", "dan\u00b7nen\u00b7her", "kan", "ich", "er\u00b7ken\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VMFIN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Was mich f\u00fcr edle Flammen brennen.", "tokens": ["Was", "mich", "f\u00fcr", "ed\u00b7le", "Flam\u00b7men", "bren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Nicht hofft/ o Wunder unsrer Zeit!", "tokens": ["Nicht", "hofft", "/", "o", "Wun\u00b7der", "uns\u00b7rer", "Zeit", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "$(", "FM", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Hertze wider zu erheben/", "tokens": ["Mein", "Hert\u00b7ze", "wi\u00b7der", "zu", "er\u00b7he\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "PTKVZ", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ein ander/ welcher noch befreyt/", "tokens": ["Ein", "an\u00b7der", "/", "wel\u00b7cher", "noch", "be\u00b7freyt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "$(", "PRELS", "ADV", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wird euch das Seine willig geben.", "tokens": ["Wird", "euch", "das", "Sei\u00b7ne", "wil\u00b7lig", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "PPOSAT", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ihr werd\u2019t aus meiner Treu erkennen/", "tokens": ["Ihr", "werd't", "aus", "mei\u00b7ner", "Treu", "er\u00b7ken\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Was mich vor edle Flammen brennen.", "tokens": ["Was", "mich", "vor", "ed\u00b7le", "Flam\u00b7men", "bren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}