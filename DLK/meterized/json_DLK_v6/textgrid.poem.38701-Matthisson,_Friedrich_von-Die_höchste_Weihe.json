{"textgrid.poem.38701": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Die h\u00f6chste Weihe", "genre": "verse", "period": "N.A.", "pub_year": 1793, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer, als Melpomen' ihn weihte,", "tokens": ["Wer", ",", "als", "Mel\u00b7po\u00b7men'", "ihn", "weih\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "$,", "KOUS", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Heilig ihr Veredlung schwur,", "tokens": ["Hei\u00b7lig", "ihr", "Ver\u00b7ed\u00b7lung", "schwur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Selbstgef\u00fchl der G\u00f6tter leite", "tokens": ["Selbst\u00b7ge\u00b7f\u00fchl", "der", "G\u00f6t\u00b7ter", "lei\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Den durch W\u00fcst' und Blumenflur.", "tokens": ["Den", "durch", "W\u00fcst'", "und", "Blu\u00b7men\u00b7flur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Mild und segnend, gleich Auroren,", "tokens": ["Mild", "und", "seg\u00b7nend", ",", "gleich", "Au\u00b7ro\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "$,", "ADV", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wann der Lenz der Erde naht,", "tokens": ["Wann", "der", "Lenz", "der", "Er\u00b7de", "naht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wallt die freundlichste der Horen", "tokens": ["Wallt", "die", "freund\u00b7lichs\u00b7te", "der", "Ho\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Treu mit ihm des Daseyns Pfad.", "tokens": ["Treu", "mit", "ihm", "des", "Da\u00b7seyns", "Pfad", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wo Vernunft und Hochsinn wohnen", "tokens": ["Wo", "Ver\u00b7nunft", "und", "Hoch\u00b7sinn", "woh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "KON", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gl\u00fcht sein Herz von Sympathie;", "tokens": ["Gl\u00fcht", "sein", "Herz", "von", "Sym\u00b7pa\u00b7thie", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Rein erklingt in allen Zonen", "tokens": ["Rein", "er\u00b7klingt", "in", "al\u00b7len", "Zo\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihm des Weltalls Harmonie.", "tokens": ["Ihm", "des", "Welt\u00b7alls", "Har\u00b7mo\u00b7nie", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ihn entz\u00fcckt der Meere Spiegel", "tokens": ["Ihn", "ent\u00b7z\u00fcckt", "der", "Mee\u00b7re", "Spie\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die Silberperl' am Kraut,", "tokens": ["Und", "die", "Sil\u00b7ber\u00b7perl'", "am", "Kraut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPRART", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Die Viol' am Todtenh\u00fcgel", "tokens": ["Die", "Vi\u00b7ol'", "am", "Tod\u00b7ten\u00b7h\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und die Ros' im Kranz der Braut.", "tokens": ["Und", "die", "Ros'", "im", "Kranz", "der", "Braut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPRART", "NN", "ART", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.5": {"line.1": {"text": "Ihm erhebt der Katarakten", "tokens": ["Ihm", "er\u00b7hebt", "der", "Ka\u00b7ta\u00b7rak\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Donnersturz den trunknen Geist;", "tokens": ["Don\u00b7ner\u00b7sturz", "den", "trunk\u00b7nen", "Geist", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihm das B\u00e4chlein so vom nackten", "tokens": ["Ihm", "das", "B\u00e4ch\u00b7lein", "so", "vom", "nack\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ART", "NN", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Klippenabhang niederfleu\u00dft.", "tokens": ["Klip\u00b7pen\u00b7ab\u00b7hang", "nie\u00b7der\u00b7fleu\u00dft", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Er vernimmt der Hofnung Wehen", "tokens": ["Er", "ver\u00b7nimmt", "der", "Hof\u00b7nung", "We\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hoch vom lichten Sternenraum,", "tokens": ["Hoch", "vom", "lich\u00b7ten", "Ster\u00b7nen\u00b7raum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hebt, wo Blumen auferstehen,", "tokens": ["Hebt", ",", "wo", "Blu\u00b7men", "auf\u00b7er\u00b7ste\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "NN", "VVIZU", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihres Schleiers goldnen Saum.", "tokens": ["Ih\u00b7res", "Schlei\u00b7ers", "gold\u00b7nen", "Saum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Trinkt auf hoher Alpenweide", "tokens": ["Trinkt", "auf", "ho\u00b7her", "Al\u00b7pen\u00b7wei\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit dem Adler Himmelsglanz,", "tokens": ["Mit", "dem", "Ad\u00b7ler", "Him\u00b7mels\u00b7glanz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Windet auf beschneiter Haide", "tokens": ["Win\u00b7det", "auf", "be\u00b7schnei\u00b7ter", "Hai\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dunkles Immergr\u00fcn zum Kranz;", "tokens": ["Dunk\u00b7les", "Im\u00b7mer\u00b7gr\u00fcn", "zum", "Kranz", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Sieht um Platons Kelch die Rosen", "tokens": ["Sieht", "um", "Pla\u00b7tons", "Kelch", "die", "Ro\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "NE", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Heitrer Weisheit wieder gl\u00fchn,", "tokens": ["Hei\u00b7trer", "Weis\u00b7heit", "wie\u00b7der", "gl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Roms Ruinen sich entmoosen", "tokens": ["Roms", "Ru\u00b7i\u00b7nen", "sich", "ent\u00b7moo\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "NN", "PRF", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und Athens Gefilde bl\u00fchn.", "tokens": ["Und", "A\u00b7thens", "Ge\u00b7fil\u00b7de", "bl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NN", "VVINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.9": {"line.1": {"text": "Be\u00dfrer Zukunft Bilder schweben", "tokens": ["Be\u00df\u00b7rer", "Zu\u00b7kunft", "Bil\u00b7der", "schwe\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo Gew\u00f6lk ihn tr\u00fcb' umzieht,", "tokens": ["Wo", "Ge\u00b7w\u00f6lk", "ihn", "tr\u00fcb'", "um\u00b7zieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PPER", "VVFIN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und, harmonisch wie sein Leben,", "tokens": ["Und", ",", "har\u00b7mo\u00b7nisch", "wie", "sein", "Le\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADJD", "KOKOM", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "T\u00f6nt im Volk sein hehres Lied.", "tokens": ["T\u00f6nt", "im", "Volk", "sein", "heh\u00b7res", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "St\u00e4t, wie Vestas Flamme lodert,", "tokens": ["St\u00e4t", ",", "wie", "Ves\u00b7tas", "Flam\u00b7me", "lo\u00b7dert", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "NE", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Troz der Erdenst\u00fcrme Wuth,", "tokens": ["Troz", "der", "Er\u00b7den\u00b7st\u00fcr\u00b7me", "Wuth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bis die schwarze Bark' ihn fodert,", "tokens": ["Bis", "die", "schwar\u00b7ze", "Bark'", "ihn", "fo\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seines Geistes reine Gluth.", "tokens": ["Sei\u00b7nes", "Geis\u00b7tes", "rei\u00b7ne", "Gluth", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}