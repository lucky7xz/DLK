{"textgrid.poem.42228": {"metadata": {"author": {"name": "Wedekind, Frank", "birth": "N.A.", "death": "N.A."}, "title": "An das Leben", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn mir dereinst von dieser Seuche", "tokens": ["Wenn", "mir", "de\u00b7reinst", "von", "die\u00b7ser", "Seu\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Genesung wird im k\u00fchlen Grab,", "tokens": ["Ge\u00b7ne\u00b7sung", "wird", "im", "k\u00fch\u00b7len", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dann sei, da\u00df jung und alt entfleuche,", "tokens": ["Dann", "sei", ",", "da\u00df", "jung", "und", "alt", "ent\u00b7fleu\u00b7che", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "KOUS", "ADJD", "KON", "ADJD", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Denkmal eine Vogelscheuche:", "tokens": ["Mein", "Denk\u00b7mal", "ei\u00b7ne", "Vo\u00b7gel\u00b7scheu\u00b7che", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Mein Hut auf meinem Wanderstab.", "tokens": ["Mein", "Hut", "auf", "mei\u00b7nem", "Wan\u00b7der\u00b7stab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Hut war schwarz und breitger\u00e4ndert,", "tokens": ["Der", "Hut", "war", "schwarz", "und", "breit\u00b7ge\u00b7r\u00e4n\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Im Herbst von dunklem Gr\u00fcn umlaubt.", "tokens": ["Im", "Herbst", "von", "dunk\u00b7lem", "Gr\u00fcn", "um\u00b7laubt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie hat der Winter ihn ver\u00e4ndert!", "tokens": ["Wie", "hat", "der", "Win\u00b7ter", "ihn", "ver\u00b7\u00e4n\u00b7dert", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Jetzt deckt er schmutzig, schlapp, entb\u00e4ndert", "tokens": ["Jetzt", "deckt", "er", "schmut\u00b7zig", ",", "schlapp", ",", "ent\u00b7b\u00e4n\u00b7dert"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$,", "ADJD", "$,", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Mein m\u00fcdes fr\u00fchgebeugtes Haupt.", "tokens": ["Mein", "m\u00fc\u00b7des", "fr\u00fch\u00b7ge\u00b7beug\u00b7tes", "Haupt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Den Stecken hielt ich friedlich nieder,", "tokens": ["Den", "Ste\u00b7cken", "hielt", "ich", "fried\u00b7lich", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bis ich der Unschuld heil'gen Schlaf", "tokens": ["Bis", "ich", "der", "Un\u00b7schuld", "heil'\u00b7gen", "Schlaf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gef\u00e4hrdet sah von gift'ger Hyder.", "tokens": ["Ge\u00b7f\u00e4hr\u00b7det", "sah", "von", "gift'\u00b7ger", "Hy\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich schlug, da\u00df ich die eignen Glieder", "tokens": ["Ich", "schlug", ",", "da\u00df", "ich", "die", "eig\u00b7nen", "Glie\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Mit grauenvollstem Fluche traf.", "tokens": ["Mit", "grau\u00b7en\u00b7volls\u00b7tem", "Flu\u00b7che", "traf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Zur Seuche, dran ich elend sieche", "tokens": ["Zur", "Seu\u00b7che", ",", "dran", "ich", "e\u00b7lend", "sie\u00b7che"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "PAV", "PPER", "ADJD", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ward mir des Ungeheuers Gift:", "tokens": ["Ward", "mir", "des", "Un\u00b7ge\u00b7heu\u00b7ers", "Gift", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der gr\u00e4\u00dflichste der Erdenfl\u00fcche.", "tokens": ["Der", "gr\u00e4\u00df\u00b7lichs\u00b7te", "der", "Er\u00b7den\u00b7fl\u00fc\u00b7che", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich taumle hin, ich wanke, krieche,", "tokens": ["Ich", "taum\u00b7le", "hin", ",", "ich", "wan\u00b7ke", ",", "krie\u00b7che", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "PPER", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Bis mich im Tod Erl\u00f6sung trifft.", "tokens": ["Bis", "mich", "im", "Tod", "Er\u00b7l\u00f6\u00b7sung", "trifft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPRART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}