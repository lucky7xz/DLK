{"dta.poem.3163": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "14.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Weil ich kein Weltkind bin, nicht habe Weltverstand,", "tokens": ["Weil", "ich", "kein", "Welt\u00b7kind", "bin", ",", "nicht", "ha\u00b7be", "Welt\u00b7ver\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "VAFIN", "$,", "PTKNEG", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der rechte Sinn mir fehlt f\u00fcr Weltbetrieb und Tand;", "tokens": ["Der", "rech\u00b7te", "Sinn", "mir", "fehlt", "f\u00fcr", "Welt\u00b7be\u00b7trieb", "und", "Tand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Scheint jeder auf der Welt berufen mit Behagen", "tokens": ["Scheint", "je\u00b7der", "auf", "der", "Welt", "be\u00b7ru\u00b7fen", "mit", "Be\u00b7ha\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "APPR", "ART", "NN", "VVPP", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von Weltgut l\u00e4stigem mir etwas abzujagen.", "tokens": ["Von", "Welt\u00b7gut", "l\u00e4s\u00b7ti\u00b7gem", "mir", "et\u00b7was", "ab\u00b7zu\u00b7ja\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "PPER", "PIS", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch f\u00fchlt kein Freund sich aufgelegt, von unbequemen", "tokens": ["Doch", "f\u00fchlt", "kein", "Freund", "sich", "auf\u00b7ge\u00b7legt", ",", "von", "un\u00b7be\u00b7que\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "PIAT", "NN", "PRF", "VVPP", "$,", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gesch\u00e4ften weltlichen mir auch was abzunehmen.", "tokens": ["Ge\u00b7sch\u00e4f\u00b7ten", "welt\u00b7li\u00b7chen", "mir", "auch", "was", "ab\u00b7zu\u00b7neh\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "PIS", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}