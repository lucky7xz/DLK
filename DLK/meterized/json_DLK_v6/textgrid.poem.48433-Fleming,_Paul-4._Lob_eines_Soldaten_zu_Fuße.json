{"textgrid.poem.48433": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "4. Lob eines Soldaten zu Fu\u00dfe", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin ein Man ins Feld: mein k\u00fchner Mut ist gro\u00df,", "tokens": ["Ich", "bin", "ein", "Man", "ins", "Feld", ":", "mein", "k\u00fch\u00b7ner", "Mut", "ist", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "PIS", "APPRART", "NN", "$.", "PPOSAT", "ADJA", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "ist gr\u00f6\u00dfer als ich selbst. Ich f\u00fcrchte keinen Sto\u00df,", "tokens": ["ist", "gr\u00f6\u00b7\u00dfer", "als", "ich", "selbst", ".", "Ich", "f\u00fcrch\u00b7te", "kei\u00b7nen", "Sto\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KOKOM", "PPER", "ADV", "$.", "PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "ich scheue keinen Schu\u00df. Die Liebe so zu leben", "tokens": ["ich", "scheu\u00b7e", "kei\u00b7nen", "Schu\u00df", ".", "Die", "Lie\u00b7be", "so", "zu", "le\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$.", "ART", "NN", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "hat mir in meinen Sinn von Jugend an gegeben,", "tokens": ["hat", "mir", "in", "mei\u00b7nen", "Sinn", "von", "Ju\u00b7gend", "an", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPOSAT", "NN", "APPR", "NN", "APZR", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "da\u00df ich mich was versuch'. Es wird nicht Iederman", "tokens": ["da\u00df", "ich", "mich", "was", "ver\u00b7such'", ".", "Es", "wird", "nicht", "Ie\u00b7der\u00b7man"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "PWS", "VVFIN", "$.", "PPER", "VAFIN", "PTKNEG", "NE"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "zum Kriege auserkorn. Wer Pulver riechen kan,", "tokens": ["zum", "Krie\u00b7ge", "au\u00b7ser\u00b7korn", ".", "Wer", "Pul\u00b7ver", "rie\u00b7chen", "kan", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVIZU", "$.", "PWS", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "auf Balg und Sto\u00df besteht, nicht die Kartaunen scheuet,", "tokens": ["auf", "Balg", "und", "Sto\u00df", "be\u00b7steht", ",", "nicht", "die", "Kar\u00b7tau\u00b7nen", "scheu\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$,", "PTKNEG", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "der ist ein Man wie ich. Ist Einer, der sich freuet,", "tokens": ["der", "ist", "ein", "Man", "wie", "ich", ".", "Ist", "Ei\u00b7ner", ",", "der", "sich", "freu\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "VAFIN", "ART", "PIS", "KOKOM", "PPER", "$.", "VAFIN", "PIS", "$,", "PRELS", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "wenn itzt der laute L\u00e4rm mit vollen Spielen geht", "tokens": ["wenn", "itzt", "der", "lau\u00b7te", "L\u00e4rm", "mit", "vol\u00b7len", "Spie\u00b7len", "geht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "und uns der k\u00fchne Feind im blanken Felde steht,", "tokens": ["und", "uns", "der", "k\u00fch\u00b7ne", "Feind", "im", "blan\u00b7ken", "Fel\u00b7de", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "so bin auch warlich ichs. Wir treuen Kameraden", "tokens": ["so", "bin", "auch", "war\u00b7lich", "ichs", ".", "Wir", "treu\u00b7en", "Ka\u00b7me\u00b7ra\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "ADV", "PIS", "$.", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "stehn als f\u00fcr einen Man. Die schie\u00dfen, jene laden,", "tokens": ["stehn", "als", "f\u00fcr", "ei\u00b7nen", "Man", ".", "Die", "schie\u00b7\u00dfen", ",", "je\u00b7ne", "la\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "APPR", "ART", "PIS", "$.", "ART", "ADJA", "$,", "PDS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "wir wechseln emsig um. Wir schrein einander zu,", "tokens": ["wir", "wech\u00b7seln", "em\u00b7sig", "um", ".", "Wir", "schrein", "ein\u00b7an\u00b7der", "zu", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PTKVZ", "$.", "PPER", "VVFIN", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "da\u00df keiner etwan nicht, was ihm verf\u00e4nglich, tu'.", "tokens": ["da\u00df", "kei\u00b7ner", "et\u00b7wan", "nicht", ",", "was", "ihm", "ver\u00b7f\u00e4ng\u00b7lich", ",", "tu'", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "PTKNEG", "$,", "PWS", "PPER", "ADJD", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Wir fechten br\u00fcderlich. Der Feind wird abgehalten,", "tokens": ["Wir", "fech\u00b7ten", "br\u00fc\u00b7der\u00b7lich", ".", "Der", "Feind", "wird", "ab\u00b7ge\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$.", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "da\u00df er, wie stark er k\u00f6mmt, die Ordnung nicht kan spalten.", "tokens": ["da\u00df", "er", ",", "wie", "stark", "er", "k\u00f6mmt", ",", "die", "Ord\u00b7nung", "nicht", "kan", "spal\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PWAV", "ADJD", "PPER", "VVFIN", "$,", "ART", "NN", "PTKNEG", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Wir weichen keinen Schritt. Gleich wie ein Ieder steht,", "tokens": ["Wir", "wei\u00b7chen", "kei\u00b7nen", "Schritt", ".", "Gleich", "wie", "ein", "Ie\u00b7der", "steht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$.", "ADV", "KOKOM", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "da f\u00e4llt, da stirbt er auch. Was ist uns mehr zur\u00fccke", "tokens": ["da", "f\u00e4llt", ",", "da", "stirbt", "er", "auch", ".", "Was", "ist", "uns", "mehr", "zu\u00b7r\u00fc\u00b7cke"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "ADV", "$.", "PWS", "VAFIN", "PPER", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "um alles unser Tun, am Gl\u00fcck und Ungel\u00fccke,", "tokens": ["um", "al\u00b7les", "un\u00b7ser", "Tun", ",", "am", "Gl\u00fcck", "und", "Un\u00b7ge\u00b7l\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "PPOSAT", "NN", "$,", "APPRART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "als wenn man stirbet wol? Wol aber sterben hei\u00dft,", "tokens": ["als", "wenn", "man", "stir\u00b7bet", "wol", "?", "Wol", "a\u00b7ber", "ster\u00b7ben", "hei\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "PIS", "VVFIN", "ADV", "$.", "ADV", "ADV", "VVINF", "VVFIN", "$,"], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}, "line.21": {"text": "wenn man mit Lust, ohn' Schmerz und bald sein Blut vergeust,", "tokens": ["wenn", "man", "mit", "Lust", ",", "ohn'", "Schmerz", "und", "bald", "sein", "Blut", "ver\u00b7geust", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "NN", "$,", "APPR", "NN", "KON", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "gleich wie es hier geschicht. Die ritterlichen Wunden", "tokens": ["gleich", "wie", "es", "hier", "ge\u00b7schicht", ".", "Die", "rit\u00b7ter\u00b7li\u00b7chen", "Wun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "KOKOM", "PPER", "ADV", "VVPP", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "empf\u00e4ngt man ins Gesicht'. An mir wird keine funden,", "tokens": ["emp\u00b7f\u00e4ngt", "man", "ins", "Ge\u00b7sicht'", ".", "An", "mir", "wird", "kei\u00b7ne", "fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPRART", "NN", "$.", "APPR", "PPER", "VAFIN", "PIAT", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "die auf dem R\u00fccken sei. Das St\u00fcrmen und die Schlacht,", "tokens": ["die", "auf", "dem", "R\u00fc\u00b7cken", "sei", ".", "Das", "St\u00fcr\u00b7men", "und", "die", "Schlacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VAFIN", "$.", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "die gestern noch geschah, sind unser' Zier und Pracht;", "tokens": ["die", "ge\u00b7stern", "noch", "ge\u00b7schah", ",", "sind", "un\u00b7ser'", "Zier", "und", "Pracht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "VVFIN", "$,", "VAFIN", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.26": {"text": "die Tugend sieht so aus. Wenn dann nach vielen Streichen,", "tokens": ["die", "Tu\u00b7gend", "sieht", "so", "aus", ".", "Wenn", "dann", "nach", "vie\u00b7len", "Strei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PTKVZ", "$.", "KOUS", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "nach langer Fechtenszeit die m\u00fcden Feinde weichen,", "tokens": ["nach", "lan\u00b7ger", "Fech\u00b7tens\u00b7zeit", "die", "m\u00fc\u00b7den", "Fein\u00b7de", "wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "da sind wir Herren erst, da w\u00e4chset uns der Mut,", "tokens": ["da", "sind", "wir", "Her\u00b7ren", "erst", ",", "da", "w\u00e4ch\u00b7set", "uns", "der", "Mut", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "ADV", "$,", "ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "der zwar nie kleine war. Da kriegt man Geld und Gut", "tokens": ["der", "zwar", "nie", "klei\u00b7ne", "war", ".", "Da", "kriegt", "man", "Geld", "und", "Gut"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADV", "ADJA", "VAFIN", "$.", "ADV", "VVFIN", "PIS", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "mehr als man haben will. Wir machen frische Beute.", "tokens": ["mehr", "als", "man", "ha\u00b7ben", "will", ".", "Wir", "ma\u00b7chen", "fri\u00b7sche", "Beu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PIS", "VAINF", "VMFIN", "$.", "PPER", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Das Vaterland wird froh. Wir retten Land und Leute", "tokens": ["Das", "Va\u00b7ter\u00b7land", "wird", "froh", ".", "Wir", "ret\u00b7ten", "Land", "und", "Leu\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$.", "PPER", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "und machen sie auch arm, nach dem das Gl\u00fccke f\u00e4llt.", "tokens": ["und", "ma\u00b7chen", "sie", "auch", "arm", ",", "nach", "dem", "das", "Gl\u00fc\u00b7cke", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADJD", "$,", "APPR", "ART", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Die Herren sind uns gleich: wir stehen feil um Geld.", "tokens": ["Die", "Her\u00b7ren", "sind", "uns", "gleich", ":", "wir", "ste\u00b7hen", "feil", "um", "Geld", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "$.", "PPER", "VVFIN", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}