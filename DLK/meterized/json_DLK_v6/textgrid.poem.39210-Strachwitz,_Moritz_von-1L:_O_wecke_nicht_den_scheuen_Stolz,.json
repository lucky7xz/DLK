{"textgrid.poem.39210": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: O wecke nicht den scheuen Stolz,", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O wecke nicht den scheuen Stolz,", "tokens": ["O", "we\u00b7cke", "nicht", "den", "scheu\u00b7en", "Stolz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihn weckt ein leicht Ger\u00e4usch,", "tokens": ["Ihn", "weckt", "ein", "leicht", "Ge\u00b7r\u00e4usch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er bricht den Liebespfeil im Holz,", "tokens": ["Er", "bricht", "den", "Lie\u00b7be\u00b7spfeil", "im", "Holz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Spitze bleibt im Fleisch;", "tokens": ["Die", "Spit\u00b7ze", "bleibt", "im", "Fleisch", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er geht urpl\u00f6tzlich wie ein Sturm", "tokens": ["Er", "geht", "ur\u00b7pl\u00f6tz\u00b7lich", "wie", "ein", "Sturm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Durch den allersch\u00f6nsten Mai,", "tokens": ["Durch", "den", "al\u00b7ler\u00b7sch\u00f6ns\u00b7ten", "Mai", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Die Liebe kr\u00fcmmt sich wie ein Wurm,", "tokens": ["Die", "Lie\u00b7be", "kr\u00fcmmt", "sich", "wie", "ein", "Wurm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der Fr\u00fchling ist vorbei!", "tokens": ["Der", "Fr\u00fch\u00b7ling", "ist", "vor\u00b7bei", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich habe Dich so sehr geliebt,", "tokens": ["Ich", "ha\u00b7be", "Dich", "so", "sehr", "ge\u00b7liebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So sehr ein Mann gekonnt,", "tokens": ["So", "sehr", "ein", "Mann", "ge\u00b7konnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dein Aug' hat meine Stirn getr\u00fcbt,", "tokens": ["Dein", "Aug'", "hat", "mei\u00b7ne", "Stirn", "ge\u00b7tr\u00fcbt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dein Auge sie besonnt.", "tokens": ["Dein", "Au\u00b7ge", "sie", "be\u00b7sonnt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Vielleicht hast Du mich auch geminnt!", "tokens": ["Viel\u00b7leicht", "hast", "Du", "mich", "auch", "ge\u00b7minnt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vielleicht \u2013 es ist zu sp\u00e4t!", "tokens": ["Viel\u00b7leicht", "\u2013", "es", "ist", "zu", "sp\u00e4t", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "PPER", "VAFIN", "PTKA", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die Stunde rinnt, die Wunde rinnt,", "tokens": ["Die", "Stun\u00b7de", "rinnt", ",", "die", "Wun\u00b7de", "rinnt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die Saat ist abgem\u00e4ht! \u2013", "tokens": ["Die", "Saat", "ist", "ab\u00b7ge\u00b7m\u00e4ht", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wie bist Du sch\u00f6n! Im Herzen w\u00fchlt", "tokens": ["Wie", "bist", "Du", "sch\u00f6n", "!", "Im", "Her\u00b7zen", "w\u00fchlt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "ADJD", "$.", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der abgebrochne Schaft,", "tokens": ["Der", "ab\u00b7ge\u00b7broch\u00b7ne", "Schaft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du hast zu gerne Ball gespielt", "tokens": ["Du", "hast", "zu", "ger\u00b7ne", "Ball", "ge\u00b7spielt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit mir und meiner Kraft.", "tokens": ["Mit", "mir", "und", "mei\u00b7ner", "Kraft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich bog mein trotzig Knie um Dich,", "tokens": ["Ich", "bog", "mein", "trot\u00b7zig", "Knie", "um", "Dich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJD", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Du hast Dich abgekehrt;", "tokens": ["Du", "hast", "Dich", "ab\u00b7ge\u00b7kehrt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Da lacht' ich wild und st\u00fcrzte mich", "tokens": ["Da", "lacht'", "ich", "wild", "und", "st\u00fcrz\u00b7te", "mich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "KON", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "In meines Stolzes Schwert.", "tokens": ["In", "mei\u00b7nes", "Stol\u00b7zes", "Schwert", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Fahr' wohl, ich kann nicht zweimal knien", "tokens": ["Fahr'", "wohl", ",", "ich", "kann", "nicht", "zwei\u00b7mal", "kni\u00b7en"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "$,", "PPER", "VMFIN", "PTKNEG", "ADV", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Um alles Heil der Welt!", "tokens": ["Um", "al\u00b7les", "Heil", "der", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PIAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dir aber w\u00fcnsch' ich Maiengr\u00fcn,", "tokens": ["Dir", "a\u00b7ber", "w\u00fcn\u00b7sch'", "ich", "Mai\u00b7en\u00b7gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wohin Dein Auge f\u00e4llt.", "tokens": ["Wo\u00b7hin", "Dein", "Au\u00b7ge", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und wenn es einst in Liebe schmolz", "tokens": ["Und", "wenn", "es", "einst", "in", "Lie\u00b7be", "schmolz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADV", "APPR", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "F\u00fcr einen bessern Mann,", "tokens": ["F\u00fcr", "ei\u00b7nen", "bes\u00b7sern", "Mann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Du kennst den Stolz, den scheuen Stolz,", "tokens": ["Du", "kennst", "den", "Stolz", ",", "den", "scheu\u00b7en", "Stolz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "O r\u00fchre nie daran!", "tokens": ["O", "r\u00fch\u00b7re", "nie", "da\u00b7ran", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}