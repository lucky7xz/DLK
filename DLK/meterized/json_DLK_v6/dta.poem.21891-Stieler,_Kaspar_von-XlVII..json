{"dta.poem.21891": {"metadata": {"author": {"name": "Stieler, Kaspar von", "birth": "N.A.", "death": "N.A."}, "title": "XlVII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1660", "urn": "urn:nbn:de:kobv:b4-20586-2", "language": ["de:0.99"], "booktitle": "Filidor der Dorfferer [i. e. Stieler, Kaspar von]: Die Geharnschte Venus. Hamburg, 1660."}, "poem": {"stanza.1": {"line.1": {"text": "Ein wahres Wort/ wer nicht wil k\u00fchne seyn", "tokens": ["Ein", "wah\u00b7res", "Wort", "/", "wer", "nicht", "wil", "k\u00fch\u00b7ne", "seyn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$(", "PWS", "PTKNEG", "VMFIN", "VVFIN", "PPOSAT"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "der stelle nur den Liebes-handel ein.", "tokens": ["der", "stel\u00b7le", "nur", "den", "Lie\u00b7bes\u00b7han\u00b7del", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Man tr\u00e4get mir die vollen Wahren an", "tokens": ["Man", "tr\u00e4\u00b7get", "mir", "die", "vol\u00b7len", "Wah\u00b7ren", "an"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "ART", "ADJA", "NN", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "und ich kan mich nicht in die Nahrung finden.", "tokens": ["und", "ich", "kan", "mich", "nicht", "in", "die", "Nah\u00b7rung", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPER", "PTKNEG", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-++--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Wers auff Verlust nicht wagen wil noch", "tokens": ["Wers", "auff", "Ver\u00b7lust", "nicht", "wa\u00b7gen", "wil", "noch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "PTKNEG", "VVINF", "VMFIN", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "was Wunder? da\u00df er allzeit bleibt dahin-", "tokens": ["was", "Wun\u00b7der", "?", "da\u00df", "er", "all\u00b7zeit", "bleibt", "da\u00b7hin"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "NN", "$.", "KOUS", "PPER", "ADV", "VVFIN", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}