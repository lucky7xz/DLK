{"textgrid.poem.40460": {"metadata": {"author": {"name": "Scheffel, Joseph Viktor von", "birth": "N.A.", "death": "N.A."}, "title": "Der Jubilar im Neckartal", "genre": "verse", "period": "N.A.", "pub_year": 1856, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das war ein schmucker Pfarrvikar,", "tokens": ["Das", "war", "ein", "schmu\u00b7cker", "Pfarr\u00b7vi\u00b7kar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Alt Wertheims Sohn und Zierde,", "tokens": ["Alt", "Wert\u00b7heims", "Sohn", "und", "Zier\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NE", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Als man ihn heut vor f\u00fcnfzig Jahr'", "tokens": ["Als", "man", "ihn", "heut", "vor", "f\u00fcnf\u00b7zig", "Jahr'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "PPER", "ADV", "APPR", "CARD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zum Kirchdienst ordinierte.", "tokens": ["Zum", "Kirch\u00b7dienst", "or\u00b7di\u00b7nier\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Er diente ihr, wie Gott ihn schuf,", "tokens": ["Er", "dien\u00b7te", "ihr", ",", "wie", "Gott", "ihn", "schuf", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PWAV", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit reichen Geistesgaben", "tokens": ["Mit", "rei\u00b7chen", "Geis\u00b7tes\u00b7ga\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und hat, getreu dem Lehrberuf,", "tokens": ["Und", "hat", ",", "ge\u00b7treu", "dem", "Lehr\u00b7be\u00b7ruf", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "$,", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sein Pfund nicht leer vergraben.", "tokens": ["Sein", "Pfund", "nicht", "leer", "ver\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKNEG", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wie er sich hielt zu seinem Amt", "tokens": ["Wie", "er", "sich", "hielt", "zu", "sei\u00b7nem", "Amt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PRF", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bezeugt ihm die Gemeinde,", "tokens": ["Be\u00b7zeugt", "ihm", "die", "Ge\u00b7mein\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er hat geflucht nicht, noch verdammt,", "tokens": ["Er", "hat", "ge\u00b7flucht", "nicht", ",", "noch", "ver\u00b7dammt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "PTKNEG", "$,", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und keinen Mann zum Feinde.", "tokens": ["Und", "kei\u00b7nen", "Mann", "zum", "Fein\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ihn hie\u00df sein klarer Forschergeist", "tokens": ["Ihn", "hie\u00df", "sein", "kla\u00b7rer", "For\u00b7scher\u00b7geist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Des Sch\u00f6pfers Allmacht ehren", "tokens": ["Des", "Sch\u00f6p\u00b7fers", "All\u00b7macht", "eh\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und was die Sch\u00f6pfung Wunder weist,", "tokens": ["Und", "was", "die", "Sch\u00f6p\u00b7fung", "Wun\u00b7der", "weist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Erkennen und erkl\u00e4ren.", "tokens": ["Er\u00b7ken\u00b7nen", "und", "er\u00b7kl\u00e4\u00b7ren", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Begann des langen Winters Macht", "tokens": ["Be\u00b7gann", "des", "lan\u00b7gen", "Win\u00b7ters", "Macht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durchs Neckartal zu dunkeln,", "tokens": ["Durchs", "Ne\u00b7ckar\u00b7tal", "zu", "dun\u00b7keln", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sah oft der Schiffer durch die Nacht", "tokens": ["Sah", "oft", "der", "Schif\u00b7fer", "durch", "die", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Pfarrhofs L\u00e4mplein funkeln:", "tokens": ["Des", "Pfarr\u00b7hofs", "L\u00e4m\u00b7plein", "fun\u00b7keln", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Er war's, der einsam \u00fcbersa\u00df,", "tokens": ["Er", "wa\u00b7r's", ",", "der", "ein\u00b7sam", "\u00fc\u00b7ber\u00b7sa\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Den Kosmos zu erlernen,", "tokens": ["Den", "Kos\u00b7mos", "zu", "er\u00b7ler\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und was er nicht in B\u00fcchern las,", "tokens": ["Und", "was", "er", "nicht", "in", "B\u00fc\u00b7chern", "las", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "PTKNEG", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das las er in den Sternen!", "tokens": ["Das", "las", "er", "in", "den", "Ster\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Doch fr\u00fchlings, wenn das Maikraut bl\u00fcht,", "tokens": ["Doch", "fr\u00fch\u00b7lings", ",", "wenn", "das", "Mai\u00b7kraut", "bl\u00fcht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da ging er zu den S\u00e4ngern", "tokens": ["Da", "ging", "er", "zu", "den", "S\u00e4n\u00b7gern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und sang manch lustig pf\u00e4lzisch Lied", "tokens": ["Und", "sang", "manch", "lus\u00b7tig", "pf\u00e4l\u00b7zisch", "Lied"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PIAT", "ADJD", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu Heidelberg im Engern.", "tokens": ["Zu", "Hei\u00b7del\u00b7berg", "im", "En\u00b7gern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Zum Krittler, dem's zu lustig war,", "tokens": ["Zum", "Kritt\u00b7ler", ",", "dem's", "zu", "lus\u00b7tig", "war", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "PTKA", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sprach er: \u00bbWas kritisieren?", "tokens": ["Sprach", "er", ":", "\u00bb", "Was", "kri\u00b7ti\u00b7sie\u00b7ren", "?"], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "$(", "PWS", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ich werd' ja doch einst Jubilar,", "tokens": ["Ich", "werd'", "ja", "doch", "einst", "Ju\u00b7bi\u00b7lar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Drum la\u00dft mich jubilieren!\u00ab", "tokens": ["Drum", "la\u00dft", "mich", "ju\u00b7bi\u00b7lie\u00b7ren", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PAV", "VVFIN", "PPER", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und richtig jubeln frohgesinnt", "tokens": ["Und", "rich\u00b7tig", "ju\u00b7beln", "froh\u00b7ge\u00b7sinnt"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVINF", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Heut mit dem alten Freunde", "tokens": ["Heut", "mit", "dem", "al\u00b7ten", "Freun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Hochfestlich Kind und Kindeskind,", "tokens": ["Hoch\u00b7fest\u00b7lich", "Kind", "und", "Kin\u00b7des\u00b7kind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Amtsbr\u00fcder und Gemeinde.", "tokens": ["Amts\u00b7br\u00fc\u00b7der", "und", "Ge\u00b7mein\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Noch str\u00f6mt der Neckar frei von Eis,", "tokens": ["Noch", "str\u00f6mt", "der", "Ne\u00b7ckar", "frei", "von", "Eis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Wellen singen und brausen:", "tokens": ["Die", "Wel\u00b7len", "sin\u00b7gen", "und", "brau\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KON", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "\u00bbhoch! dreimal hoch der Jubelgreis,", "tokens": ["\u00bb", "hoch", "!", "drei\u00b7mal", "hoch", "der", "Ju\u00b7bel\u00b7greis", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$.", "ADV", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der Pfarrer von Ziegelhausen!\u00ab", "tokens": ["Der", "Pfar\u00b7rer", "von", "Zie\u00b7gel\u00b7hau\u00b7sen", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$.", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}