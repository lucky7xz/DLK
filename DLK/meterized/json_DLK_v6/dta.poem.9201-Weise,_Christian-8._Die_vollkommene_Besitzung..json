{"dta.poem.9201": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "8.  Die vollkommene Besitzung.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Mein gem\u00fcthe sey verschwiegen/", "tokens": ["Mein", "ge\u00b7m\u00fc\u00b7the", "sey", "ver\u00b7schwie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VAFIN", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kanst du dich gleichwohl vergn\u00fcgen", "tokens": ["Kanst", "du", "dich", "gleich\u00b7wohl", "ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PRF", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "An der milden freundlichkeit:", "tokens": ["An", "der", "mil\u00b7den", "freund\u00b7lich\u00b7keit", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Zwey personen m\u00fcssen spielen/", "tokens": ["Zwey", "per\u00b7so\u00b7nen", "m\u00fcs\u00b7sen", "spie\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "VVINF", "VMFIN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und hingegen unter vielen/", "tokens": ["Und", "hin\u00b7ge\u00b7gen", "un\u00b7ter", "vie\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PIAT", "$("], "meter": "-++-+-+-", "measure": "unknown.measure.tetra"}, "line.6": {"text": "Wird die freude leicht zerstreut.", "tokens": ["Wird", "die", "freu\u00b7de", "leicht", "zer\u00b7streut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "La\u00df dich streichlen/ la\u00df dich k\u00fcssen/", "tokens": ["La\u00df", "dich", "streich\u00b7len", "/", "la\u00df", "dich", "k\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVFIN", "$(", "VVIMP", "PPER", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Darff es doch kein ander wissen/", "tokens": ["Darff", "es", "doch", "kein", "an\u00b7der", "wis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PIAT", "ADJD", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wann du nur versichert bist/", "tokens": ["Wann", "du", "nur", "ver\u00b7si\u00b7chert", "bist", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVPP", "VAFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df dein liebgen unter allen/", "tokens": ["Da\u00df", "dein", "lieb\u00b7gen", "un\u00b7ter", "al\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "APPR", "PIAT", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Dir zu lieb und wohl gefallen/", "tokens": ["Dir", "zu", "lieb", "und", "wohl", "ge\u00b7fal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKA", "ADJD", "KON", "ADV", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Stiller gunst verbunden ist.", "tokens": ["Stil\u00b7ler", "gunst", "ver\u00b7bun\u00b7den", "ist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "La\u00df dir sanfft die h\u00e4nde tr\u00fccken/", "tokens": ["La\u00df", "dir", "sanfft", "die", "h\u00e4n\u00b7de", "tr\u00fc\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVFIN", "ART", "ADJA", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Spiele mit entz\u00fcckten blicken/", "tokens": ["Spie\u00b7le", "mit", "ent\u00b7z\u00fcck\u00b7ten", "bli\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Jtzund hast du noch die wahl/", "tokens": ["Jt\u00b7zund", "hast", "du", "noch", "die", "wahl", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Drum so last die jugend schertzen/", "tokens": ["Drum", "so", "last", "die", "ju\u00b7gend", "schert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "VVFIN", "ART", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und versencke deine schmertzen/", "tokens": ["Und", "ver\u00b7sen\u00b7cke", "dei\u00b7ne", "schmert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "In den s\u00fcssen rosenthal.", "tokens": ["In", "den", "s\u00fcs\u00b7sen", "ro\u00b7sen\u00b7thal", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}