{"textgrid.poem.45202": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[betrachtet euch das Ausland sp\u00f6ttisch]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Betrachtet euch das Ausland sp\u00f6ttisch", "tokens": ["Be\u00b7trach\u00b7tet", "euch", "das", "Aus\u00b7land", "sp\u00f6t\u00b7tisch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und nennt euch kopflos, leer an Sinn,", "tokens": ["Und", "nennt", "euch", "kopf\u00b7los", ",", "leer", "an", "Sinn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "$,", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mein ich, ihr seid doch nur \u00e4sthetisch:", "tokens": ["Mein", "ich", ",", "ihr", "seid", "doch", "nur", "\u00e4s\u00b7the\u00b7tisch", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "PPER", "$,", "PPER", "VAFIN", "ADV", "ADV", "ADJD", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Der erste Eindruck rei\u00dft euch hin.", "tokens": ["Der", "ers\u00b7te", "Ein\u00b7druck", "rei\u00dft", "euch", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}