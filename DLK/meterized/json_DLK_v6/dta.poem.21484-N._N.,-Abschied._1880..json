 {"text": "Als ein treuer Troubadour.", "tokens": ["Als", "ein", "treu\u00b7er", "Trou\u00b7ba\u00b7dour", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.19": {"line.1": {"text": "M\u00fcde, stumm und ganz verlassen", "tokens": ["M\u00fc\u00b7de", ",", "stumm", "und", "ganz", "ver\u00b7las\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADJD", "KON", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lieg\u2019 ich nun bei fahlem Lichte, \u2026", "tokens": ["Lieg'", "ich", "nun", "bei", "fah\u00b7lem", "Lich\u00b7te", ",", "\u2026"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVIMP", "PPER", "ADV", "APPR", "ADJA", "NN", "$,", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Drau\u00dfen t\u00f6nt es durch die nassen", "tokens": ["Drau\u00b7\u00dfen", "t\u00f6nt", "es", "durch", "die", "nas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Regen\u00fcberstr\u00f6mten Gassen", "tokens": ["Re\u00b7gen\u00b7\u00fc\u00b7ber\u00b7str\u00f6m\u00b7ten", "Gas\u00b7sen"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie ein fernes Liebeslied.", "tokens": ["Wie", "ein", "fer\u00b7nes", "Lie\u00b7bes\u00b7lied", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.20": {"line.1": {"text": "Hast mein Herze schn\u00f6d verrathen,", "tokens": ["Hast", "mein", "Her\u00b7ze", "schn\u00f6d", "ver\u00b7ra\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "VVFIN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Trinkst die Lieb\u2019 aus andrem Kelche, \u2014 \u2014", "tokens": ["Trinkst", "die", "Lieb'", "aus", "an\u00b7drem", "Kel\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,", "$(", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hagelwetter meiner Saaten,", "tokens": ["Ha\u00b7gel\u00b7wet\u00b7ter", "mei\u00b7ner", "Saa\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ich verachte deine Thaten,", "tokens": ["Ich", "ver\u00b7ach\u00b7te", "dei\u00b7ne", "Tha\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Neuer Lenz gl\u00fcht mir im Blut.", "tokens": ["Neu\u00b7er", "Lenz", "gl\u00fcht", "mir", "im", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.21": {"line.1": {"text": "Greife nach dem Helm, dem blanken,", "tokens": ["Grei\u00b7fe", "nach", "dem", "Helm", ",", "dem", "blan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "PRELS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nach dem Schwert und hartem Schilde, \u2014", "tokens": ["Nach", "dem", "Schwert", "und", "har\u00b7tem", "Schil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "KON", "ADJA", "NN", "$,", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf dem Schlachtfeld der Gedanken", "tokens": ["Auf", "dem", "Schlacht\u00b7feld", "der", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Reit\u2019 ich trotzig in die Schranken,", "tokens": ["Reit'", "ich", "trot\u00b7zig", "in", "die", "Schran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Todesdurstig \u2014 liebesbleich!", "tokens": ["To\u00b7des\u00b7durs\u00b7tig", "lie\u00b7bes\u00b7bleich", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADJD", "$(", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.22": {"line.1": {"text": "Menschheit, du unwandelbare", "tokens": ["Menschheit", ",", "du", "un\u00b7wan\u00b7del\u00b7ba\u00b7re"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "PPER", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sch\u00f6nste, ewigjunge Bl\u00fcthe,", "tokens": ["Sch\u00f6ns\u00b7te", ",", "e\u00b7wig\u00b7jun\u00b7ge", "Bl\u00fc\u00b7the", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dunkles R\u00e4thsel \u2014 einzigwahre", "tokens": ["Dunk\u00b7les", "R\u00e4th\u00b7sel", "ein\u00b7zig\u00b7wah\u00b7re"], "token_info": ["word", "word", "punct", "word"], "pos": ["ADJA", "NN", "$(", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Gottheit Du! \u2014 welch\u2019 wunderklare,", "tokens": ["Got\u00b7theit", "Du", "!", "welch'", "wun\u00b7der\u00b7kla\u00b7re", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "$(", "PWAT", "ADJA", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Liebe f\u00fcllt f\u00fcr dich mein Herz.", "tokens": ["Lie\u00b7be", "f\u00fcllt", "f\u00fcr", "dich", "mein", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.23": {"line.1": {"text": "La\u00df der Brust mein Blut entwallen,", "tokens": ["La\u00df", "der", "Brust", "mein", "Blut", "ent\u00b7wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "La\u00df f\u00fcr dich mich jubelnd sterben,", "tokens": ["La\u00df", "f\u00fcr", "dich", "mich", "ju\u00b7belnd", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "PPER", "PRF", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ja, f\u00fcr deine G\u00f6tterhallen", "tokens": ["Ja", ",", "f\u00fcr", "dei\u00b7ne", "G\u00f6t\u00b7ter\u00b7hal\u00b7len"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Will ich k\u00e4mpfen, will ich fallen", "tokens": ["Will", "ich", "k\u00e4mp\u00b7fen", ",", "will", "ich", "fal\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "PPER", "VVINF", "$,", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Allgeliebt-Allliebende!", "tokens": ["All\u00b7ge\u00b7lieb\u00b7tAll\u00b7lie\u00b7ben\u00b7de", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+-+--", "measure": "unknown.measure.tri"}}, "stanza.24": {"line.1": {"text": "Doch im letzten Todesbeben,", "tokens": ["Doch", "im", "letz\u00b7ten", "To\u00b7des\u00b7be\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn sich neigt die blasse Stirne,", "tokens": ["Wenn", "sich", "neigt", "die", "blas\u00b7se", "Stir\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wird mich noch ein Duft von Reben", "tokens": ["Wird", "mich", "noch", "ein", "Duft", "von", "Re\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und von Rosen lind umweben,", "tokens": ["Und", "von", "Ro\u00b7sen", "lind", "um\u00b7we\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Meiner Jugend Liebestraum!", "tokens": ["Mei\u00b7ner", "Ju\u00b7gend", "Lie\u00b7best\u00b7raum", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}