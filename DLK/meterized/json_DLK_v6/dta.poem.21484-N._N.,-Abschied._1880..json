{"dta.poem.21484": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Abschied.  \n 1880.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "S\u00fc\u00dfe und geliebte Dame,", "tokens": ["S\u00fc\u00b7\u00dfe", "und", "ge\u00b7lieb\u00b7te", "Da\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meiner Seele sch\u00f6ne F\u00fcrstin, \u2014", "tokens": ["Mei\u00b7ner", "See\u00b7le", "sch\u00f6\u00b7ne", "F\u00fcrs\u00b7tin", ","], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Stets gepriesen sei dein Name! \u2014", "tokens": ["Stets", "ge\u00b7prie\u00b7sen", "sei", "dein", "Na\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVPP", "VAFIN", "PPOSAT", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wundenkrank und bla\u00df vom Grame", "tokens": ["Wun\u00b7den\u00b7krank", "und", "bla\u00df", "vom", "Gra\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "ADJD", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Biet\u2019 ich dir den letzten Gru\u00df.", "tokens": ["Biet'", "ich", "dir", "den", "letz\u00b7ten", "Gru\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Bei der Lampe fahlem Scheine,", "tokens": ["Bei", "der", "Lam\u00b7pe", "fah\u00b7lem", "Schei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In dem d\u00fcstren Wirthshaus tr\u00e4um\u2019 ich", "tokens": ["In", "dem", "d\u00fcst\u00b7ren", "Wirths\u00b7haus", "tr\u00e4um'", "ich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Einsam nun und ganz alleine", "tokens": ["Ein\u00b7sam", "nun", "und", "ganz", "al\u00b7lei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "ADV", "KON", "ADV", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hinter schwerem Spanierweine,", "tokens": ["Hin\u00b7ter", "schwe\u00b7rem", "Spa\u00b7ni\u00b7er\u00b7wei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Trinke seinen hei\u00dfen Duft.", "tokens": ["Trin\u00b7ke", "sei\u00b7nen", "hei\u00b7\u00dfen", "Duft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ha \u2026 wie str\u00f6mt\u2019s da auf mich nieder,", "tokens": ["Ha", "\u2026", "wie", "str\u00f6mt's", "da", "auf", "mich", "nie\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "PWAV", "VVFIN", "ADV", "APPR", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schwinden nicht die dunklen Bogen?", "tokens": ["Schwin\u00b7den", "nicht", "die", "dunk\u00b7len", "Bo\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Jasminduft \u2026 wei\u00df bl\u00fcht der Flieder,", "tokens": ["Jas\u00b7min\u00b7duft", "\u2026", "wei\u00df", "bl\u00fcht", "der", "Flie\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "VVFIN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sommernacht umf\u00e4ngt mich wieder,", "tokens": ["Som\u00b7mer\u00b7nacht", "um\u00b7f\u00e4ngt", "mich", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Silbern blitzt die feuchte Luft.", "tokens": ["Sil\u00b7bern", "blitzt", "die", "feuch\u00b7te", "Luft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Mondlicht \u2026 Bl\u00fcthenduft \u2026 und dr\u00fcben", "tokens": ["Mond\u00b7licht", "\u2026", "Bl\u00fc\u00b7then\u00b7duft", "\u2026", "und", "dr\u00fc\u00b7ben"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "$(", "NN", "$(", "KON", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schlag der Nachtigall im Laubwerk \u2026", "tokens": ["Schlag", "der", "Nach\u00b7ti\u00b7gall", "im", "Laub\u00b7werk", "\u2026"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPRART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sanfte Citherkl\u00e4nge h\u00fcben,", "tokens": ["Sanf\u00b7te", "Cit\u00b7her\u00b7kl\u00e4n\u00b7ge", "h\u00fc\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und aus meiner Seele tr\u00fcben", "tokens": ["Und", "aus", "mei\u00b7ner", "See\u00b7le", "tr\u00fc\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Kammern wichen Leid und Angst.", "tokens": ["Kam\u00b7mern", "wi\u00b7chen", "Leid", "und", "Angst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Ei, was war mir alles Hassen,", "tokens": ["Ei", ",", "was", "war", "mir", "al\u00b7les", "Has\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "VAFIN", "PPER", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dachte nur an Deine Sch\u00f6nheit,", "tokens": ["Dach\u00b7te", "nur", "an", "Dei\u00b7ne", "Sch\u00f6n\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Als Du hinschrittst durch die Gassen", "tokens": ["Als", "Du", "hin\u00b7schrittst", "durch", "die", "Gas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Einstmals, stand ich ganz verlassen", "tokens": ["Einst\u00b7mals", ",", "stand", "ich", "ganz", "ver\u00b7las\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "VVFIN", "PPER", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "An der Kirche dunklem Thor.", "tokens": ["An", "der", "Kir\u00b7che", "dunk\u00b7lem", "Thor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Stand und sah dich! \u2014 Wie durchflossen", "tokens": ["Stand", "und", "sah", "dich", "!", "Wie", "durch\u00b7flos\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["NN", "KON", "VVFIN", "PPER", "$.", "$(", "PWAV", "VVPP"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Pl\u00f6tzlich Licht und Gluth mein Dasein,", "tokens": ["Pl\u00f6tz\u00b7lich", "Licht", "und", "Gluth", "mein", "Da\u00b7sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sonnen mir im Herzen sprossen,", "tokens": ["Son\u00b7nen", "mir", "im", "Her\u00b7zen", "spros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Welten sah ich aufgeschlossen,", "tokens": ["Wel\u00b7ten", "sah", "ich", "auf\u00b7ge\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und ich f\u00fchlte Gottes Ku\u00df.", "tokens": ["Und", "ich", "f\u00fchl\u00b7te", "Got\u00b7tes", "Ku\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Wie die Nacht dem goldnen Tage,", "tokens": ["Wie", "die", "Nacht", "dem", "gold\u00b7nen", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Liebestrunken folg\u2019 ich zitternd", "tokens": ["Lie\u00b7be\u00b7strun\u00b7ken", "fol\u00b7g'", "ich", "zit\u00b7ternd"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "VVPP"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Dir seitdem, da\u00df ich dir sage,", "tokens": ["Dir", "seit\u00b7dem", ",", "da\u00df", "ich", "dir", "sa\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PAV", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was ich leide und ertrage,", "tokens": ["Was", "ich", "lei\u00b7de", "und", "er\u00b7tra\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df mein Ich in Dir erstarb.", "tokens": ["Da\u00df", "mein", "Ich", "in", "Dir", "er\u00b7starb", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "PPER", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Nun, da n\u00e4cht\u2019ge Zauber fluthen", "tokens": ["Nun", ",", "da", "n\u00e4cht'\u00b7ge", "Zau\u00b7ber", "flut\u00b7hen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch die L\u00fcfte, auf den Erdball,", "tokens": ["Durch", "die", "L\u00fcf\u00b7te", ",", "auf", "den", "Erd\u00b7ball", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hei\u00dfer alle Sinne bluten,", "tokens": ["Hei\u00b7\u00dfer", "al\u00b7le", "Sin\u00b7ne", "blu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hei\u00dfer alle Herzen gluthen,", "tokens": ["Hei\u00b7\u00dfer", "al\u00b7le", "Her\u00b7zen", "glu\u00b7then", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wandle ich vor deiner Th\u00fcr.", "tokens": ["Wand\u00b7le", "ich", "vor", "dei\u00b7ner", "Th\u00fcr."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.9": {"line.1": {"text": "R\u00f6thlich gl\u00e4nzt der s\u00fc\u00dfe Flimmer", "tokens": ["R\u00f6th\u00b7lich", "gl\u00e4nzt", "der", "s\u00fc\u00b7\u00dfe", "Flim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lichts in deinem hohen Saale, \u2014", "tokens": ["Lichts", "in", "dei\u00b7nem", "ho\u00b7hen", "Saa\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "APPR", "PPOSAT", "ADJA", "NN", "$,", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "O Madonna, soll ich nimmer", "tokens": ["O", "Ma\u00b7don\u00b7na", ",", "soll", "ich", "nim\u00b7mer"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NE", "$,", "VMFIN", "PPER", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deines Kleides seidnen Schimmer", "tokens": ["Dei\u00b7nes", "Klei\u00b7des", "seid\u00b7nen", "Schim\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Heut\u2019 am Fenster noch ersp\u00e4h\u2019n?", "tokens": ["Heut'", "am", "Fens\u00b7ter", "noch", "er\u00b7sp\u00e4h'n", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Einmal nur auf dem Balkone", "tokens": ["Ein\u00b7mal", "nur", "auf", "dem", "Bal\u00b7ko\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ART", "NN"], "meter": "+-+--++-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Zeige dich, mein Seelentraumbild,", "tokens": ["Zei\u00b7ge", "dich", ",", "mein", "See\u00b7len\u00b7traum\u00b7bild", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie die Mutter mit dem Sohne", "tokens": ["Wie", "die", "Mut\u00b7ter", "mit", "dem", "Soh\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hoch auf g\u00fcldnem Himmelsthrone", "tokens": ["Hoch", "auf", "g\u00fcld\u00b7nem", "Him\u00b7melst\u00b7hro\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Zwingst du mich, im Staub zu knien \u2026", "tokens": ["Zwingst", "du", "mich", ",", "im", "Staub", "zu", "kni\u00b7en", "\u2026"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "$,", "APPRART", "NN", "PTKZU", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Sommern\u00e4chte, \u2014 trunkne Stunden,", "tokens": ["Som\u00b7mer\u00b7n\u00e4ch\u00b7te", ",", "trunk\u00b7ne", "Stun\u00b7den", ","], "token_info": ["word", "punct", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "$(", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Da ich so vor ihrem Fenster,", "tokens": ["Da", "ich", "so", "vor", "ih\u00b7rem", "Fens\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Blutend aus viels\u00fc\u00dfen Wunden,", "tokens": ["Blu\u00b7tend", "aus", "viel\u00b7s\u00fc\u00b7\u00dfen", "Wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lauten und mit leisen Munden", "tokens": ["Lau\u00b7ten", "und", "mit", "lei\u00b7sen", "Mun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sang, ein blasser Troubadour.", "tokens": ["Sang", ",", "ein", "blas\u00b7ser", "Trou\u00b7ba\u00b7dour", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Da ich sp\u00e4hend alle Wege", "tokens": ["Da", "ich", "sp\u00e4\u00b7hend", "al\u00b7le", "We\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Niedersah, ob nicht ein Bursche", "tokens": ["Nie\u00b7der\u00b7sah", ",", "ob", "nicht", "ein", "Bur\u00b7sche"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "KOUS", "PTKNEG", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Girrend k\u00e4m\u2019 mir ins Gehege, \u2014", "tokens": ["Gir\u00b7rend", "k\u00e4m'", "mir", "ins", "Ge\u00b7he\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "VVFIN", "PPER", "APPRART", "NN", "$,", "$("], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.4": {"text": "Hei, wie h\u00e4tten meine Schl\u00e4ge", "tokens": ["Hei", ",", "wie", "h\u00e4t\u00b7ten", "mei\u00b7ne", "Schl\u00e4\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "PWAV", "VAFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Liebeswunden ihm versetzt.", "tokens": ["Lie\u00b7bes\u00b7wun\u00b7den", "ihm", "ver\u00b7setzt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Da mit Veilchen und mit Rosen", "tokens": ["Da", "mit", "Veil\u00b7chen", "und", "mit", "Ro\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "KON", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ich des Nachts ihr Fenster kr\u00e4nzte,", "tokens": ["Ich", "des", "Nachts", "ihr", "Fens\u00b7ter", "kr\u00e4nz\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und mit kecken Studiosen", "tokens": ["Und", "mit", "ke\u00b7cken", "Stu\u00b7di\u00b7o\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"tex