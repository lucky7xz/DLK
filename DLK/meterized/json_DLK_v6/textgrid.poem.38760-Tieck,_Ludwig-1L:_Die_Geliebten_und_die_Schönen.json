{"textgrid.poem.38760": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Geliebten und die Sch\u00f6nen", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Geliebten und die Sch\u00f6nen", "tokens": ["Die", "Ge\u00b7lieb\u00b7ten", "und", "die", "Sch\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weinen,", "tokens": ["Wei\u00b7nen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Da\u00df der Fr\u00fchling mit den Kr\u00e4nzen,", "tokens": ["Da\u00df", "der", "Fr\u00fch\u00b7ling", "mit", "den", "Kr\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit der Blumen s\u00fcssem Gl\u00e4nzen,", "tokens": ["Mit", "der", "Blu\u00b7men", "s\u00fcs\u00b7sem", "Gl\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mit den Nachtigallen T\u00f6nen,", "tokens": ["Mit", "den", "Nach\u00b7ti\u00b7gal\u00b7len", "T\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Im Erscheinen", "tokens": ["Im", "Er\u00b7schei\u00b7nen"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Nur so kurze Zeit mag weilen,", "tokens": ["Nur", "so", "kur\u00b7ze", "Zeit", "mag", "wei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJA", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Da\u00df er mit den V\u00f6geln, D\u00fcften, Farben mu\u00df so schnelle eilen.", "tokens": ["Da\u00df", "er", "mit", "den", "V\u00f6\u00b7geln", ",", "D\u00fcf\u00b7ten", ",", "Far\u00b7ben", "mu\u00df", "so", "schnel\u00b7le", "ei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "$,", "NN", "$,", "NN", "VMFIN", "ADV", "ADJA", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.2": {"line.1": {"text": "Freilich ist es nur ein Tr\u00e4umen,", "tokens": ["Frei\u00b7lich", "ist", "es", "nur", "ein", "Tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Spielend", "tokens": ["Spie\u00b7lend"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Kommt das Kind in unsre Auen,", "tokens": ["Kommt", "das", "Kind", "in", "uns\u00b7re", "Au\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie wir in die Bl\u00e4ue schauen", "tokens": ["Wie", "wir", "in", "die", "Bl\u00e4u\u00b7e", "schau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "ART", "NN", "VVINF"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.5": {"text": "Seines Blicks, in Himmels-R\u00e4umen,", "tokens": ["Sei\u00b7nes", "Blicks", ",", "in", "Him\u00b7mels\u00b7R\u00e4u\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Lieblich f\u00fchlend", "tokens": ["Lieb\u00b7lich", "f\u00fch\u00b7lend"], "token_info": ["word", "word"], "pos": ["ADJD", "VVPP"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Wie ein Fest uns soll beginnen,", "tokens": ["Wie", "ein", "Fest", "uns", "soll", "be\u00b7gin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPER", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Flog der Lose boshaft, wenn wir r\u00fcckw\u00e4rts schauen, schon von hinnen.", "tokens": ["Flog", "der", "Lo\u00b7se", "bos\u00b7haft", ",", "wenn", "wir", "r\u00fcck\u00b7w\u00e4rts", "schau\u00b7en", ",", "schon", "von", "hin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "$,", "KOUS", "PPER", "ADV", "VVINF", "$,", "ADV", "APPR", "ADV", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.3": {"line.1": {"text": "Alle Bl\u00fcthen sind verstreuet,", "tokens": ["Al\u00b7le", "Bl\u00fc\u00b7then", "sind", "ver\u00b7streu\u00b7et", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gr\u00fcnen", "tokens": ["Gr\u00fc\u00b7nen"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "M\u00f6chte Laub und Gras so treulich,", "tokens": ["M\u00f6ch\u00b7te", "Laub", "und", "Gras", "so", "treu\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Blumen m\u00f6chten seyn erfreulich,", "tokens": ["Blu\u00b7men", "m\u00f6ch\u00b7ten", "seyn", "er\u00b7freu\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPOSAT", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch das Jahr ist schon entzweiet;", "tokens": ["Doch", "das", "Jahr", "ist", "schon", "ent\u00b7zwei\u00b7et", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und erschienen", "tokens": ["Und", "er\u00b7schie\u00b7nen"], "token_info": ["word", "word"], "pos": ["KON", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Ist der Sommer mit den Fr\u00fcchten,", "tokens": ["Ist", "der", "Som\u00b7mer", "mit", "den", "Fr\u00fcch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Nachtigall sieht alles reifen mu\u00df in andre Fr\u00fchling' fl\u00fcchten.", "tokens": ["Nach\u00b7ti\u00b7gall", "sieht", "al\u00b7les", "rei\u00b7fen", "mu\u00df", "in", "and\u00b7re", "Fr\u00fch\u00b7ling'", "fl\u00fcch\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIS", "VVINF", "VMFIN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.4": {"line.1": {"text": "Holde Liebe, s\u00fc\u00dfe, treue,", "tokens": ["Hol\u00b7de", "Lie\u00b7be", ",", "s\u00fc\u00b7\u00dfe", ",", "treu\u00b7e", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Klagen", "tokens": ["Kla\u00b7gen"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Mu\u00df ich, da\u00df wie Wolkenschauer", "tokens": ["Mu\u00df", "ich", ",", "da\u00df", "wie", "Wol\u00b7ken\u00b7schau\u00b7er"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "PPER", "$,", "KOUS", "KOKOM", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Pl\u00f6tzlich kommt die Angst und Trauer;", "tokens": ["Pl\u00f6tz\u00b7lich", "kommt", "die", "Angst", "und", "Trau\u00b7er", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Machst du erst das Jahr so neue?", "tokens": ["Machst", "du", "erst", "das", "Jahr", "so", "neu\u00b7e", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "ADV", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mu\u00df ich fragen,", "tokens": ["Mu\u00df", "ich", "fra\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Ach wie grausam, da\u00df so balde", "tokens": ["Ach", "wie", "grau\u00b7sam", ",", "da\u00df", "so", "bal\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "KOKOM", "ADJD", "$,", "KOUS", "ADV", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ich allein gelassen von Gesang und Licht und Duft im Walde?", "tokens": ["Ich", "al\u00b7lein", "ge\u00b7las\u00b7sen", "von", "Ge\u00b7sang", "und", "Licht", "und", "Duft", "im", "Wal\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "APPR", "NN", "KON", "NN", "KON", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.5": {"line.1": {"text": "Ja der Fr\u00fchling mu\u00df entweichen,", "tokens": ["Ja", "der", "Fr\u00fch\u00b7ling", "mu\u00df", "ent\u00b7wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Freuden,", "tokens": ["Freu\u00b7den", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Alle frohen goldnen Stunden", "tokens": ["Al\u00b7le", "fro\u00b7hen", "gold\u00b7nen", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Haben bald den Tod gefunden,", "tokens": ["Ha\u00b7ben", "bald", "den", "Tod", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Blumen, Lichter, Farben bleichen,", "tokens": ["Blu\u00b7men", ",", "Lich\u00b7ter", ",", "Far\u00b7ben", "blei\u00b7chen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "W\u00e4lder kleiden", "tokens": ["W\u00e4l\u00b7der", "klei\u00b7den"], "token_info": ["word", "word"], "pos": ["NN", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Sich in gr\u00fcnen Schmuck und prangen,", "tokens": ["Sich", "in", "gr\u00fc\u00b7nen", "Schmuck", "und", "pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ADJA", "NN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ziehn das Kleid aus, trauern stille, wann die Hochzeit ist vergangen.", "tokens": ["Ziehn", "das", "Kleid", "aus", ",", "trau\u00b7ern", "stil\u00b7le", ",", "wann", "die", "Hoch\u00b7zeit", "ist", "ver\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "$,", "VVINF", "VVFIN", "$,", "PWAV", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.6": {"line.1": {"text": "Darum pfl\u00fcckt die Garten-Sterne", "tokens": ["Da\u00b7rum", "pfl\u00fcckt", "die", "Gar\u00b7ten\u00b7S\u00b7ter\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ART", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Sinnend", "tokens": ["Sin\u00b7nend"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Gern das liebesschwangre Herze,", "tokens": ["Gern", "das", "lie\u00b7bes\u00b7schwang\u00b7re", "Her\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tr\u00e4gt sie wie die glimmende Kerze", "tokens": ["Tr\u00e4gt", "sie", "wie", "die", "glim\u00b7men\u00b7de", "Ker\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "KOKOM", "ART", "ADJA", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Still behutsam nur so ferne,", "tokens": ["Still", "be\u00b7hut\u00b7sam", "nur", "so", "fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "ADV", "ADV", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df sie brennend", "tokens": ["Da\u00df", "sie", "bren\u00b7nend"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "VVPP"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Des Geliebten Hand mag fassen,", "tokens": ["Des", "Ge\u00b7lieb\u00b7ten", "Hand", "mag", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und der l\u00e4chelt in die Flamme, die am Abend mu\u00df verblassen.", "tokens": ["Und", "der", "l\u00e4\u00b7chelt", "in", "die", "Flam\u00b7me", ",", "die", "am", "A\u00b7bend", "mu\u00df", "ver\u00b7blas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "VVFIN", "APPR", "ART", "NN", "$,", "PRELS", "APPRART", "NN", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.7": {"line.1": {"text": "Doch er lie\u00dft den Liebesmuth", "tokens": ["Doch", "er", "lie\u00dft", "den", "Lie\u00b7bes\u00b7muth"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Freudig", "tokens": ["Freu\u00b7dig"], "token_info": ["word"], "pos": ["ADJD"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "In dem zarten bunten Kinde,", "tokens": ["In", "dem", "zar\u00b7ten", "bun\u00b7ten", "Kin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Druckt das Zeichen dann gelinde", "tokens": ["Druckt", "das", "Zei\u00b7chen", "dann", "ge\u00b7lin\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ADV", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "An die rothe Lippengluth,", "tokens": ["An", "die", "ro\u00b7the", "Lip\u00b7pen\u00b7gluth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Seufzt: was leid' ich!", "tokens": ["Seufzt", ":", "was", "leid'", "ich", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PWS", "VVFIN", "PPER", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Warum sollten doch wohl pflegen", "tokens": ["Wa\u00b7rum", "soll\u00b7ten", "doch", "wohl", "pfle\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "ADV", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "G\u00e4rtner helle Rosen, k\u00f6nnten Liebesherzen sie nicht hegen?", "tokens": ["G\u00e4rt\u00b7ner", "hel\u00b7le", "Ro\u00b7sen", ",", "k\u00f6nn\u00b7ten", "Lie\u00b7bes\u00b7her\u00b7zen", "sie", "nicht", "he\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,", "VMFIN", "NN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.8": {"line.1": {"text": "Also mu\u00df ein liebes Singen", "tokens": ["Al\u00b7so", "mu\u00df", "ein", "lie\u00b7bes", "Sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Innig,", "tokens": ["In\u00b7nig", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Wie es fl\u00fcchtig geistig schwebet,", "tokens": ["Wie", "es", "fl\u00fcch\u00b7tig", "geis\u00b7tig", "schwe\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kaum bewu\u00dft sich, da\u00df es lebet,", "tokens": ["Kaum", "be\u00b7wu\u00dft", "sich", ",", "da\u00df", "es", "le\u00b7bet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PRF", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Das geliebte Herz durchdringen:", "tokens": ["Das", "ge\u00b7lieb\u00b7te", "Herz", "durch\u00b7drin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ach, das bin ich!", "tokens": ["Ach", ",", "das", "bin", "ich", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PDS", "VAFIN", "PPER", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Klagt die Seele in die T\u00f6ne,", "tokens": ["Klagt", "die", "See\u00b7le", "in", "die", "T\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Um so k\u00fcrzer euer Leben, um so mehr nur hold und sch\u00f6ne.", "tokens": ["Um", "so", "k\u00fcr\u00b7zer", "eu\u00b7er", "Le\u00b7ben", ",", "um", "so", "mehr", "nur", "hold", "und", "sch\u00f6\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "ADJD", "PPOSAT", "NN", "$,", "KOUI", "ADV", "ADV", "ADV", "ADJD", "KON", "ADJA", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.9": {"line.1": {"text": "Was soll Liebe doch wohl lieben,", "tokens": ["Was", "soll", "Lie\u00b7be", "doch", "wohl", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "NN", "ADV", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Liebe,", "tokens": ["Lie\u00b7be", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Als das sch\u00f6ne arm Verg\u00e4ngliche?", "tokens": ["Als", "das", "sch\u00f6\u00b7ne", "arm", "Ver\u00b7g\u00e4ng\u00b7li\u00b7che", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "ADJD", "NN", "$."], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Pflegen mu\u00df sie zart die kr\u00e4nkliche", "tokens": ["Pfle\u00b7gen", "mu\u00df", "sie", "zart", "die", "kr\u00e4nk\u00b7li\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "PPER", "ADJD", "ART", "ADJA"], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.5": {"text": "Freude, und sich daran \u00fcben,", "tokens": ["Freu\u00b7de", ",", "und", "sich", "da\u00b7ran", "\u00fc\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "PRF", "PAV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Denn sie bliebe", "tokens": ["Denn", "sie", "blie\u00b7be"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPER", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Nicht die Liebe, wenn das eine,", "tokens": ["Nicht", "die", "Lie\u00b7be", ",", "wenn", "das", "ei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "$,", "KOUS", "PDS", "ART", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Was da ist und bleibt, ihr Wunsch wie Freude sollte seyn alleine.", "tokens": ["Was", "da", "ist", "und", "bleibt", ",", "ihr", "Wunsch", "wie", "Freu\u00b7de", "soll\u00b7te", "seyn", "al\u00b7lei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VAFIN", "KON", "VVFIN", "$,", "PPOSAT", "NN", "KOKOM", "NN", "VMFIN", "PPOSAT", "ADV", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.10": {"line.1": {"text": "Was noch zarter ist als T\u00f6ne,", "tokens": ["Was", "noch", "zar\u00b7ter", "ist", "als", "T\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADJD", "VAFIN", "KOKOM", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Scherzend", "tokens": ["Scher\u00b7zend"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Mehr als Melodie und D\u00fcfte,", "tokens": ["Mehr", "als", "Me\u00b7lo\u00b7die", "und", "D\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Selber nicht ber\u00fchrt die L\u00fcfte,", "tokens": ["Sel\u00b7ber", "nicht", "be\u00b7r\u00fchrt", "die", "L\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Lebend in der eignen Sch\u00f6ne", "tokens": ["Le\u00b7bend", "in", "der", "eig\u00b7nen", "Sch\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Lieblich scherzend? \u2013", "tokens": ["Lieb\u00b7lich", "scher\u00b7zend", "?", "\u2013"], "token_info": ["word", "word", "punct", "punct"], "pos": ["ADJD", "VVPP", "$.", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Ach es sind die Liebsgedanken", "tokens": ["Ach", "es", "sind", "die", "Liebs\u00b7ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ITJ", "PPER", "VAFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Die in Wehmuth, Sehnsucht, Andacht, wie in Blumenkelchen schwanken,", "tokens": ["Die", "in", "Weh\u00b7muth", ",", "Sehn\u00b7sucht", ",", "An\u00b7dacht", ",", "wie", "in", "Blu\u00b7men\u00b7kel\u00b7chen", "schwan\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "$,", "NN", "$,", "NN", "$,", "PWAV", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.11": {"line.1": {"text": "Wem die Lippen sind verschlossen", "tokens": ["Wem", "die", "Lip\u00b7pen", "sind", "ver\u00b7schlos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "ART", "NN", "VAFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kl\u00e4ngen,", "tokens": ["Kl\u00e4n\u00b7gen", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Wem nicht Blumen Winter giebet", "tokens": ["Wem", "nicht", "Blu\u00b7men", "Win\u00b7ter", "gie\u00b7bet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PTKNEG", "NN", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und er treu und sehnlich liebet", "tokens": ["Und", "er", "treu", "und", "sehn\u00b7lich", "lie\u00b7bet"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ADJD", "KON", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ganz von Ahndungen umflossen,", "tokens": ["Ganz", "von", "Ahn\u00b7dun\u00b7gen", "um\u00b7flos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVINF", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "In Gef\u00e4ngen", "tokens": ["In", "Ge\u00b7f\u00e4n\u00b7gen"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Mu\u00df sein Herz heimlich zerrinnen,", "tokens": ["Mu\u00df", "sein", "Herz", "heim\u00b7lich", "zer\u00b7rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "ADJD", "VVINF", "$,"], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Wunsch, Andenken ewiges, sind die Blumen, die er kann gewinnen.", "tokens": ["Wunsch", ",", "An\u00b7den\u00b7ken", "e\u00b7wi\u00b7ges", ",", "sind", "die", "Blu\u00b7men", ",", "die", "er", "kann", "ge\u00b7win\u00b7nen", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "ADJA", "$,", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+--+-+-+-+-", "measure": "trochaic.octa.plus.relaxed"}}}}}