{"dta.poem.18270": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "An meine Freunde.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "An dem die Dummen sich leimen;", "tokens": ["An", "dem", "die", "Dum\u00b7men", "sich", "lei\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "PRF", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die Dichter reimen und reimen", "tokens": ["Die", "Dich\u00b7ter", "rei\u00b7men", "und", "rei\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "KON", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und noch immer erscheint das \u201eDichterheim\u201c!", "tokens": ["Und", "noch", "im\u00b7mer", "er\u00b7scheint", "das", "\u201e", "Dich\u00b7ter\u00b7heim", "\u201c", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["KON", "ADV", "ADV", "VVFIN", "ART", "$(", "NN", "$(", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Drum schaart euch zusammen nun Mann an Mann", "tokens": ["Drum", "schaart", "euch", "zu\u00b7sam\u00b7men", "nun", "Mann", "an", "Mann"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "ADV", "NN", "APPR", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Und wetzt eure Schwerter und sagt mir an:", "tokens": ["Und", "wetzt", "eu\u00b7re", "Schwer\u00b7ter", "und", "sagt", "mir", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "KON", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Wann werden wir endlich zu Boden treten", "tokens": ["Wann", "wer\u00b7den", "wir", "end\u00b7lich", "zu", "Bo\u00b7den", "tre\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "APPR", "NN", "VVINF"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Das lyrische Kruppzeug der Afterpoeten?", "tokens": ["Das", "ly\u00b7ri\u00b7sche", "Krupp\u00b7zeug", "der", "Af\u00b7ter\u00b7po\u00b7e\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}}}}