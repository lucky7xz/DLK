{"textgrid.poem.47682": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[zephyros, so leise spielend]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zephyros, so leise spielend,", "tokens": ["Ze\u00b7phy\u00b7ros", ",", "so", "lei\u00b7se", "spie\u00b7lend", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mein geheimes Wehe f\u00fchlend", "tokens": ["Mein", "ge\u00b7hei\u00b7mes", "We\u00b7he", "f\u00fch\u00b7lend"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In ger\u00fchrter Sympathie \u2013", "tokens": ["In", "ge\u00b7r\u00fchr\u00b7ter", "Sym\u00b7pa\u00b7thie", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ein getreuer Bote zieh',", "tokens": ["Ein", "ge\u00b7treu\u00b7er", "Bo\u00b7te", "zieh'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Hin zu meiner Wonne flieh',", "tokens": ["Hin", "zu", "mei\u00b7ner", "Won\u00b7ne", "flieh'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Aber nicht gelinde k\u00fchlend,", "tokens": ["A\u00b7ber", "nicht", "ge\u00b7lin\u00b7de", "k\u00fch\u00b7lend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADJA", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Flammenhauchend f\u00e4chle sie!", "tokens": ["Flam\u00b7men\u00b7hau\u00b7chend", "f\u00e4ch\u00b7le", "sie", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Dann, so wie die hei\u00dfen Plagen", "tokens": ["Dann", ",", "so", "wie", "die", "hei\u00b7\u00dfen", "Pla\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "ADV", "KOKOM", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihre zarte Wange schlagen,", "tokens": ["Ih\u00b7re", "zar\u00b7te", "Wan\u00b7ge", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Welche du von ihrem Zagen,", "tokens": ["Wel\u00b7che", "du", "von", "ih\u00b7rem", "Za\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wieder rasch erl\u00f6sen mu\u00dft \u2013", "tokens": ["Wie\u00b7der", "rasch", "er\u00b7l\u00f6\u00b7sen", "mu\u00dft", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVINF", "VMFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "\u00bbzephyros,\u00ab so wird sie fragen,", "tokens": ["\u00bb", "ze\u00b7phy\u00b7ros", ",", "\u00ab", "so", "wird", "sie", "fra\u00b7gen", ","], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$,", "$(", "ADV", "VAFIN", "PPER", "VVINF", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "\u00bbwelche Gluthen herzutragen,", "tokens": ["\u00bb", "wel\u00b7che", "Glu\u00b7then", "her\u00b7zu\u00b7tra\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "PWAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Fiel dich an die seltne Lust?\u00ab \u2013", "tokens": ["Fiel", "dich", "an", "die", "selt\u00b7ne", "Lust", "?", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "$.", "$(", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "\u00bbseine Seufzer,\u00ab sollst du sagen,", "tokens": ["\u00bb", "sei\u00b7ne", "Seuf\u00b7zer", ",", "\u00ab", "sollst", "du", "sa\u00b7gen", ","], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "$,", "$(", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "\u00bbsendet dir die treuste Brust.\u00ab", "tokens": ["\u00bb", "sen\u00b7det", "dir", "die", "treus\u00b7te", "Brust", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "PPER", "ART", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}