{"textgrid.poem.60452": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Beethovens B\u00fcste", "genre": "verse", "period": "N.A.", "pub_year": 1840, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Traurig kehrt ich eines Abends", "tokens": ["Trau\u00b7rig", "kehrt", "ich", "ei\u00b7nes", "A\u00b7bends"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In mein einsam d\u00fcstres Zimmer,", "tokens": ["In", "mein", "ein\u00b7sam", "d\u00fcst\u00b7res", "Zim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00dcberraschend drin entgegen", "tokens": ["\u00dc\u00b7berr\u00b7a\u00b7schend", "drin", "ent\u00b7ge\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADV", "PTKVZ"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Blinkte mir ein Freudenschimmer.", "tokens": ["Blink\u00b7te", "mir", "ein", "Freu\u00b7den\u00b7schim\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Mit dem sichern Blick der Liebe", "tokens": ["Mit", "dem", "si\u00b7chern", "Blick", "der", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hatt ein Freund den Spalt getroffen,", "tokens": ["Hatt", "ein", "Freund", "den", "Spalt", "ge\u00b7trof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo des Unmuts d\u00fcstre Zelle", "tokens": ["Wo", "des", "Un\u00b7muts", "d\u00fcst\u00b7re", "Zel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VVFIN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Blieb dem Strahl der Freude offen.", "tokens": ["Blieb", "dem", "Strahl", "der", "Freu\u00b7de", "of\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ha! ich fand des Mannes B\u00fcste,", "tokens": ["Ha", "!", "ich", "fand", "des", "Man\u00b7nes", "B\u00fcs\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Den ich h\u00f6chst als Meister ehre", "tokens": ["Den", "ich", "h\u00f6chst", "als", "Meis\u00b7ter", "eh\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "PPER", "ADV", "KOUS", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nebst dem schroffen Urgebirge", "tokens": ["Nebst", "dem", "schrof\u00b7fen", "Ur\u00b7ge\u00b7bir\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und dem grenzenlosen Meere.", "tokens": ["Und", "dem", "gren\u00b7zen\u00b7lo\u00b7sen", "Mee\u00b7re", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ein Gewitter in den Alpen,", "tokens": ["Ein", "Ge\u00b7wit\u00b7ter", "in", "den", "Al\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "St\u00fcrme auf dem Ozeane", "tokens": ["St\u00fcr\u00b7me", "auf", "dem", "O\u00b7zea\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Und das gro\u00dfe Herz Beethovens,", "tokens": ["Und", "das", "gro\u00b7\u00dfe", "Herz", "Beet\u00b7ho\u00b7vens", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Laut im heiligen Orkane,", "tokens": ["Laut", "im", "hei\u00b7li\u00b7gen", "Or\u00b7ka\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Sind die Wecker mir des Mutes,", "tokens": ["Sind", "die", "We\u00b7cker", "mir", "des", "Mu\u00b7tes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der das Schicksal wagt zu fodern,", "tokens": ["Der", "das", "Schick\u00b7sal", "wagt", "zu", "fo\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Der den letzten Baum des Edens", "tokens": ["Der", "den", "letz\u00b7ten", "Baum", "des", "E\u00b7dens"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "L\u00e4chelnd sieht zu Asche lodern.", "tokens": ["L\u00e4\u00b7chelnd", "sieht", "zu", "A\u00b7sche", "lo\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "K\u00e4mpfen lern ich ohne Hassen,", "tokens": ["K\u00e4mp\u00b7fen", "lern", "ich", "oh\u00b7ne", "Has\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gl\u00fchend lieben und entsagen,", "tokens": ["Gl\u00fc\u00b7hend", "lie\u00b7ben", "und", "ent\u00b7sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und des Todes Wonneschauer,", "tokens": ["Und", "des", "To\u00b7des", "Won\u00b7ne\u00b7schau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn Beethovens Lieder klagen;", "tokens": ["Wenn", "Beet\u00b7ho\u00b7vens", "Lie\u00b7der", "kla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Wenn sie jubeln, Leben schmetternd,", "tokens": ["Wenn", "sie", "ju\u00b7beln", ",", "Le\u00b7ben", "schmet\u00b7ternd", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "$,", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df die tiefsten Gr\u00e4ber kl\u00fcften", "tokens": ["Da\u00df", "die", "tiefs\u00b7ten", "Gr\u00e4\u00b7ber", "kl\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ein dionysisch Taumeln", "tokens": ["Und", "ein", "di\u00b7o\u00b7ny\u00b7sisch", "Tau\u00b7meln"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJD", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Rauschet \u00fcber allen Gr\u00fcften.", "tokens": ["Rau\u00b7schet", "\u00fc\u00b7ber", "al\u00b7len", "Gr\u00fcf\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Wenn sie z\u00fcrnen, h\u00f6r ich rasseln", "tokens": ["Wenn", "sie", "z\u00fcr\u00b7nen", ",", "h\u00f6r", "ich", "ras\u00b7seln"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVINF", "$,", "VVFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Menschenwillens heilge Speere,", "tokens": ["Men\u00b7schen\u00b7wil\u00b7lens", "heil\u00b7ge", "Spee\u00b7re", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und besiegt zum Abgrund, heulend,", "tokens": ["Und", "be\u00b7siegt", "zum", "Ab\u00b7grund", ",", "heu\u00b7lend", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$,", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fl\u00fcchten die D\u00e4monenheere. \u2013", "tokens": ["Fl\u00fcch\u00b7ten", "die", "D\u00e4\u00b7mo\u00b7nen\u00b7hee\u00b7re", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["NN", "ART", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Sanftes Wogen, holdes Rieseln;", "tokens": ["Sanf\u00b7tes", "Wo\u00b7gen", ",", "hol\u00b7des", "Rie\u00b7seln", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sind des Weltmeers k\u00fchle Wellen", "tokens": ["Sind", "des", "Welt\u00b7meers", "k\u00fch\u00b7le", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "S\u00fc\u00df beseelt zu Liebesstimmen?", "tokens": ["S\u00fc\u00df", "be\u00b7seelt", "zu", "Lie\u00b7bess\u00b7tim\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie sie steigen, sinken, schwellen!", "tokens": ["Wie", "sie", "stei\u00b7gen", ",", "sin\u00b7ken", ",", "schwel\u00b7len", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWAV", "PPER", "VVINF", "$,", "VVFIN", "$,", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Auf der glatten Muscheldiele", "tokens": ["Auf", "der", "glat\u00b7ten", "Mu\u00b7schel\u00b7die\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Halten Nixen ihren Reigen,", "tokens": ["Hal\u00b7ten", "Ni\u00b7xen", "ih\u00b7ren", "Rei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Keime k\u00fcnftger Nachtigallen", "tokens": ["Kei\u00b7me", "k\u00fcnft\u00b7ger", "Nach\u00b7ti\u00b7gal\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tr\u00e4umen auf Korallenzweigen.", "tokens": ["Tr\u00e4u\u00b7men", "auf", "Ko\u00b7ral\u00b7len\u00b7zwei\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Horch! noch leiser! dem Naturgeist", "tokens": ["Horch", "!", "noch", "lei\u00b7ser", "!", "dem", "Na\u00b7tur\u00b7geist"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NE", "$.", "ADV", "ADJD", "$.", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Abgelauschte Lieder sind es,", "tokens": ["Ab\u00b7ge\u00b7lauschte", "Lie\u00b7der", "sind", "es", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "PPER", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Die er fl\u00fcstert in das erste", "tokens": ["Die", "er", "fl\u00fcs\u00b7tert", "in", "das", "ers\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "VVFIN", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tr\u00e4umen eines sch\u00f6nen Kindes;", "tokens": ["Tr\u00e4u\u00b7men", "ei\u00b7nes", "sch\u00f6\u00b7nen", "Kin\u00b7des", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Die er spielt auf Mondstrahlsaiten,", "tokens": ["Die", "er", "spielt", "auf", "Mond\u00b7strahl\u00b7sai\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ob dem Abgrund ausgespannten,", "tokens": ["Ob", "dem", "Ab\u00b7grund", "aus\u00b7ge\u00b7spann\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Deren Rhythmen in der Erdnacht", "tokens": ["De\u00b7ren", "Rhyth\u00b7men", "in", "der", "Erd\u00b7nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Starren zu Kristallenkanten;", "tokens": ["Star\u00b7ren", "zu", "Kris\u00b7tal\u00b7len\u00b7kan\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$."], "meter": "+--++-+-", "measure": "dactylic.init"}}, "stanza.13": {"line.1": {"text": "Und nach deren Zaubertakten", "tokens": ["Und", "nach", "de\u00b7ren", "Zau\u00b7ber\u00b7tak\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PRELAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Rose l\u00e4\u00dft die Knospe springen,", "tokens": ["Ro\u00b7se", "l\u00e4\u00dft", "die", "Knos\u00b7pe", "sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Kranich aus des Herbstes Wehmut", "tokens": ["Kra\u00b7nich", "aus", "des", "Herbs\u00b7tes", "Weh\u00b7mut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "L\u00fcftet seine Wanderschwingen. \u2013", "tokens": ["L\u00fcf\u00b7tet", "sei\u00b7ne", "Wan\u00b7der\u00b7schwin\u00b7gen", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Ach, Coriolan! vor\u00fcber", "tokens": ["Ach", ",", "Co\u00b7ri\u00b7o\u00b7lan", "!", "vor\u00b7\u00fc\u00b7ber"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["ITJ", "$,", "NE", "$.", "ADV"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ist das Ringen, wilde Pochen,", "tokens": ["Ist", "das", "Rin\u00b7gen", ",", "wil\u00b7de", "Po\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Pl\u00f6tzlich sinds die letzten T\u00f6ne,", "tokens": ["Pl\u00f6tz\u00b7lich", "sinds", "die", "letz\u00b7ten", "T\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dumpf verhallend und gebrochen.", "tokens": ["Dumpf", "ver\u00b7hal\u00b7lend", "und", "ge\u00b7bro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "KON", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Wie der Held im sch\u00f6nen Frevel", "tokens": ["Wie", "der", "Held", "im", "sch\u00f6\u00b7nen", "Fre\u00b7vel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00dcberst\u00fcrmte alle Schranken,", "tokens": ["\u00dc\u00b7bers\u00b7t\u00fcrm\u00b7te", "al\u00b7le", "Schran\u00b7ken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dann \u2013 der tragisch \u00dcberwundne", "tokens": ["Dann", "\u2013", "der", "tra\u00b7gisch", "\u00dc\u00b7berw\u00b7und\u00b7ne"], "token_info": ["word"