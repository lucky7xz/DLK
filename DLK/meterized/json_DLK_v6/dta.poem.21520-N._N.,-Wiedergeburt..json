{"dta.poem.21520": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Wiedergeburt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Fall\u2019 ab von mir, du gottverfluchte S\u00fcnde,", "tokens": ["Fall'", "ab", "von", "mir", ",", "du", "gott\u00b7ver\u00b7fluch\u00b7te", "S\u00fcn\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "APPR", "PPER", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Fall\u2019 ab von mir wie m\u00fcrber Bl\u00e4tter Spreu,", "tokens": ["Fall'", "ab", "von", "mir", "wie", "m\u00fcr\u00b7ber", "Bl\u00e4t\u00b7ter", "Spreu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "APPR", "PPER", "KOKOM", "ADJA", "NN", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Auf da\u00df die Welt ich endlich \u00fcberwinde \u2014", "tokens": ["Auf", "da\u00df", "die", "Welt", "ich", "end\u00b7lich", "\u00fc\u00b7berw\u00b7in\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "ART", "NN", "PPER", "ADV", "ADJA", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auf da\u00df ich endlich \u2014 endlich Frieden finde!", "tokens": ["Auf", "da\u00df", "ich", "end\u00b7lich", "end\u00b7lich", "Frie\u00b7den", "fin\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "ADV", "$(", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Erhebe dich, du trotzig starker Leu", "tokens": ["Er\u00b7he\u00b7be", "dich", ",", "du", "trot\u00b7zig", "star\u00b7ker", "Leu"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "PPER", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Der Weltentsagung \u2014 recke dich empor,", "tokens": ["Der", "Wel\u00b7ten\u00b7tsa\u00b7gung", "re\u00b7cke", "dich", "em\u00b7por", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Zerbrich die Schranke, die dich h\u00e4lt, in Splitter!", "tokens": ["Zer\u00b7brich", "die", "Schran\u00b7ke", ",", "die", "dich", "h\u00e4lt", ",", "in", "Split\u00b7ter", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ihr Osterwinde rauscht, ein Feierchor!", "tokens": ["Ihr", "Os\u00b7ter\u00b7win\u00b7de", "rauscht", ",", "ein", "Fei\u00b7er\u00b7chor", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Aufsprang mir der Erkenntni\u00df Freiheitsthor:", "tokens": ["Auf\u00b7sprang", "mir", "der", "Er\u00b7kennt\u00b7ni\u00df", "Frei\u00b7heits\u00b7thor", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Entsagt hab' ich jedwedem Tand und Flitter!             ", "tokens": ["Ent\u00b7sagt", "hab'", "ich", "jed\u00b7we\u00b7dem", "Tand", "und", "Flit\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "PIAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}