{"dta.poem.7108": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Entwurff einiger Betrachtungen,  \n \u00fcber den  \n  Zustand der Seeligen.  \n Beym Anfang des 1727sten Jahres.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Indessen, da der Wechsel unsrer Zeit", "tokens": ["In\u00b7des\u00b7sen", ",", "da", "der", "Wech\u00b7sel", "uns\u00b7rer", "Zeit"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Uns nachzudencken Anla\u00df giebt,", "tokens": ["Uns", "nach\u00b7zu\u00b7den\u00b7cken", "An\u00b7la\u00df", "giebt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVIZU", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit welcher Huld, mit welcher Z\u00e4rtlichkeit", "tokens": ["Mit", "wel\u00b7cher", "Huld", ",", "mit", "wel\u00b7cher", "Z\u00e4rt\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PWAT", "NN", "$,", "APPR", "PWAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Uns GOTT im vor\u2019gen Jahr geliebt;", "tokens": ["Uns", "GoTT", "im", "vor'\u00b7gen", "Jahr", "ge\u00b7liebt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Auf! lasst uns denn nun dran gedencken,", "tokens": ["Auf", "!", "lasst", "uns", "denn", "nun", "dran", "ge\u00b7den\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "VVFIN", "PPER", "ADV", "ADV", "PAV", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und Jhm so offt ein Lied zum Neu-Jahr schencken,", "tokens": ["Und", "Jhm", "so", "offt", "ein", "Lied", "zum", "Neu\u00b7Jahr", "schen\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "ADV", "ART", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Des H\u00f6chsten Ruhm und unsre Lust verneuern,", "tokens": ["Des", "H\u00f6chs\u00b7ten", "Ruhm", "und", "uns\u00b7re", "Lust", "ver\u00b7neu\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Bis wir dereinsten dort ein ewig Neu-Jahr feyern.", "tokens": ["Bis", "wir", "de\u00b7reins\u00b7ten", "dort", "ein", "e\u00b7wig", "Neu\u00b7Jahr", "fey\u00b7ern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ART", "ADJD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}