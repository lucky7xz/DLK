{"textgrid.poem.46843": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[in diesem Wetter, in diesem Braus]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In diesem Wetter, in diesem Braus,", "tokens": ["In", "die\u00b7sem", "Wet\u00b7ter", ",", "in", "die\u00b7sem", "Braus", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Nie h\u00e4tt' ich gesendet die Kinder hinaus;", "tokens": ["Nie", "h\u00e4tt'", "ich", "ge\u00b7sen\u00b7det", "die", "Kin\u00b7der", "hin\u00b7aus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVPP", "ART", "NN", "PTKVZ", "$."], "meter": "-+-----+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Man hat sie hinaus getragen,", "tokens": ["Man", "hat", "sie", "hin\u00b7aus", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "APZR", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ich durfte dazu nichts sagen.", "tokens": ["Ich", "durf\u00b7te", "da\u00b7zu", "nichts", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PAV", "PIS", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "In diesem Wetter, in diesem Saus,", "tokens": ["In", "die\u00b7sem", "Wet\u00b7ter", ",", "in", "die\u00b7sem", "Saus", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Nie h\u00e4tt' ich gelassen die Kinder hinaus,", "tokens": ["Nie", "h\u00e4tt'", "ich", "ge\u00b7las\u00b7sen", "die", "Kin\u00b7der", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVPP", "ART", "NN", "APZR", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Ich f\u00fcrchtete, sie erkranken,", "tokens": ["Ich", "f\u00fcrch\u00b7te\u00b7te", ",", "sie", "er\u00b7kran\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Das sind nun eitle Gedanken.", "tokens": ["Das", "sind", "nun", "eit\u00b7le", "Ge\u00b7dan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "In diesem Wetter, in diesem Graus,", "tokens": ["In", "die\u00b7sem", "Wet\u00b7ter", ",", "in", "die\u00b7sem", "Graus", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "H\u00e4tt' ich gelassen die Kinder hinaus,", "tokens": ["H\u00e4tt'", "ich", "ge\u00b7las\u00b7sen", "die", "Kin\u00b7der", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVPP", "ART", "NN", "APZR", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Ich sorgte, sie st\u00fcrben morgen,", "tokens": ["Ich", "sorg\u00b7te", ",", "sie", "st\u00fcr\u00b7ben", "mor\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Das ist nun nicht zu besorgen.", "tokens": ["Das", "ist", "nun", "nicht", "zu", "be\u00b7sor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "In diesem Wetter, in diesem Braus,", "tokens": ["In", "die\u00b7sem", "Wet\u00b7ter", ",", "in", "die\u00b7sem", "Braus", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sie ruhn als wie in der Mutter Haus,", "tokens": ["Sie", "ruhn", "als", "wie", "in", "der", "Mut\u00b7ter", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOUS", "KOKOM", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Von keinem Sturme erschrecket,", "tokens": ["Von", "kei\u00b7nem", "Stur\u00b7me", "er\u00b7schre\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Von Gottes Hand bedecket.", "tokens": ["Von", "Got\u00b7tes", "Hand", "be\u00b7de\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}