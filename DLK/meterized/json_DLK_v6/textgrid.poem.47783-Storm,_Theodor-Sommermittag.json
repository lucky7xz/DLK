{"textgrid.poem.47783": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Sommermittag", "genre": "verse", "period": "N.A.", "pub_year": 1852, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun ist es still um Hof und Scheuer,", "tokens": ["Nun", "ist", "es", "still", "um", "Hof", "und", "Scheu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und in der M\u00fchle ruht der Stein;", "tokens": ["Und", "in", "der", "M\u00fch\u00b7le", "ruht", "der", "Stein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Birnenbaum mit blanken Bl\u00e4ttern", "tokens": ["Der", "Bir\u00b7nen\u00b7baum", "mit", "blan\u00b7ken", "Bl\u00e4t\u00b7tern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Steht regungslos im Sonnenschein.", "tokens": ["Steht", "re\u00b7gungs\u00b7los", "im", "Son\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPRART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Die Bienen summen so verschlafen;", "tokens": ["Die", "Bie\u00b7nen", "sum\u00b7men", "so", "ver\u00b7schla\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und in der offnen Bodenluk',", "tokens": ["Und", "in", "der", "off\u00b7nen", "Bo\u00b7den\u00b7luk'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Benebelt von dem Duft des Heues,", "tokens": ["Be\u00b7ne\u00b7belt", "von", "dem", "Duft", "des", "Heu\u00b7es", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Im grauen R\u00f6cklein nickt der Puk.", "tokens": ["Im", "grau\u00b7en", "R\u00f6ck\u00b7lein", "nickt", "der", "Puk", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der M\u00fcller schnarcht und das Gesinde,", "tokens": ["Der", "M\u00fcl\u00b7ler", "schnarcht", "und", "das", "Ge\u00b7sin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und nur die Tochter wacht im Haus;", "tokens": ["Und", "nur", "die", "Toch\u00b7ter", "wacht", "im", "Haus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die lachet still und zieht sich heimlich", "tokens": ["Die", "la\u00b7chet", "still", "und", "zieht", "sich", "heim\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADJD", "KON", "VVFIN", "PRF", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcrsichtig die Pantoffeln aus.", "tokens": ["F\u00fcr\u00b7sich\u00b7tig", "die", "Pan\u00b7tof\u00b7feln", "aus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Sie geht und weckt den M\u00fcllerburschen,", "tokens": ["Sie", "geht", "und", "weckt", "den", "M\u00fcl\u00b7ler\u00b7bur\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der kaum den schweren Augen traut:", "tokens": ["Der", "kaum", "den", "schwe\u00b7ren", "Au\u00b7gen", "traut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbnun k\u00fcsse mich, verliebter Junge;", "tokens": ["\u00bb", "nun", "k\u00fcs\u00b7se", "mich", ",", "ver\u00b7lieb\u00b7ter", "Jun\u00b7ge", ";"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch sauber, sauber! nicht zu laut.\u00ab", "tokens": ["Doch", "sau\u00b7ber", ",", "sau\u00b7ber", "!", "nicht", "zu", "laut", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADJD", "$,", "ADJD", "$.", "PTKNEG", "PTKA", "ADJD", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}