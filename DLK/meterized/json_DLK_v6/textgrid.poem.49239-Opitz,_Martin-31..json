{"textgrid.poem.49239": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "31.", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was ist dein sch\u00f6ner Leib, du schn\u00f6de blinde Jugend,", "tokens": ["Was", "ist", "dein", "sch\u00f6\u00b7ner", "Leib", ",", "du", "schn\u00f6\u00b7de", "blin\u00b7de", "Ju\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPOSAT", "ADJA", "NN", "$,", "PPER", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gebricht es ihm am Zier der guten Zucht und Tugend?", "tokens": ["Ge\u00b7bricht", "es", "ihm", "am", "Zier", "der", "gu\u00b7ten", "Zucht", "und", "Tu\u00b7gend", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "APPRART", "NN", "ART", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die hellen Augen sind ein Fenster b\u00f6ser Lust,", "tokens": ["Die", "hel\u00b7len", "Au\u00b7gen", "sind", "ein", "Fens\u00b7ter", "b\u00f6\u00b7ser", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Leib ist eine Kist', erf\u00fcllt durch Koth und Wust,", "tokens": ["Der", "Leib", "ist", "ei\u00b7ne", "Kist'", ",", "er\u00b7f\u00fcllt", "durch", "Koth", "und", "Wust", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der Mund ein Thor, daraus sich Schand' und Laster finden,", "tokens": ["Der", "Mund", "ein", "Thor", ",", "da\u00b7raus", "sich", "Schand'", "und", "Las\u00b7ter", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "PAV", "PRF", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der zarten Br\u00fcste Quell ein Brunnen aller S\u00fcnden,", "tokens": ["Der", "zar\u00b7ten", "Br\u00fcs\u00b7te", "Quell", "ein", "Brun\u00b7nen", "al\u00b7ler", "S\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "ART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der Freuden Port, die Scho\u00df, ein Grab der Ueppigkeit", "tokens": ["Der", "Freu\u00b7den", "Port", ",", "die", "Scho\u00df", ",", "ein", "Grab", "der", "Uep\u00b7pig\u00b7keit"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "NE", "$,", "ART", "NN", "$,", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und Walstatt unsrer Bl\u00fct' und besten Lebenszeit.", "tokens": ["Und", "Wals\u00b7tatt", "uns\u00b7rer", "Bl\u00fct'", "und", "bes\u00b7ten", "Le\u00b7bens\u00b7zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPOSAT", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wo aber Zier und Scham, zwo edle, theure Kronen,", "tokens": ["Wo", "a\u00b7ber", "Zier", "und", "Scham", ",", "zwo", "ed\u00b7le", ",", "theu\u00b7re", "Kro\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADV", "NN", "KON", "NN", "$,", "CARD", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Das selten funden wird, in einem Leibe wohnen,", "tokens": ["Das", "sel\u00b7ten", "fun\u00b7den", "wird", ",", "in", "ei\u00b7nem", "Lei\u00b7be", "woh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$,", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Da stehet alles wol, da sieht es lustig aus,", "tokens": ["Da", "ste\u00b7het", "al\u00b7les", "wol", ",", "da", "sieht", "es", "lus\u00b7tig", "aus", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "$,", "ADV", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Da ist ein sch\u00f6ner Wirth und auch ein sch\u00f6nes Haus.", "tokens": ["Da", "ist", "ein", "sch\u00f6\u00b7ner", "Wirth", "und", "auch", "ein", "sch\u00f6\u00b7nes", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "KON", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}