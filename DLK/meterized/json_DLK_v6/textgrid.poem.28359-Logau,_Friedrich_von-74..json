{"textgrid.poem.28359": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "74.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kirchen-gehen, Predigt-h\u00f6ren,", "tokens": ["Kir\u00b7chen\u00b7ge\u00b7hen", ",", "Pre\u00b7dig\u00b7th\u00f6\u00b7ren", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Singen, beten, andre lehren,", "tokens": ["Sin\u00b7gen", ",", "be\u00b7ten", ",", "and\u00b7re", "leh\u00b7ren", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Seuffzen und gen Himmel schauen,", "tokens": ["Seuff\u00b7zen", "und", "gen", "Him\u00b7mel", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nichts als nur vom Gott-vertrauen", "tokens": ["Nichts", "als", "nur", "vom", "Got\u00b7tver\u00b7trau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "KOKOM", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und vom glauben und vom lieben", "tokens": ["Und", "vom", "glau\u00b7ben", "und", "vom", "lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "VVINF", "KON", "APPRART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und von andrem Guts-ver\u00fcben", "tokens": ["Und", "von", "an\u00b7drem", "Guts\u00b7ver\u00b7\u00fcben"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Reden f\u00fchren: ich wil meinen,", "tokens": ["Re\u00b7den", "f\u00fch\u00b7ren", ":", "ich", "wil", "mei\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$.", "PPER", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Die es thun, Gott sind die deinen.", "tokens": ["Die", "es", "thun", ",", "Gott", "sind", "die", "dei\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVINF", "$,", "NN", "VAFIN", "ART", "PPOSAT", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "O, noch lange nicht! im R\u00fccken", "tokens": ["O", ",", "noch", "lan\u00b7ge", "nicht", "!", "im", "R\u00fc\u00b7cken"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "$,", "ADV", "ADV", "PTKNEG", "$.", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Schm\u00fctzen und von fornen schm\u00fccken,", "tokens": ["Schm\u00fct\u00b7zen", "und", "von", "for\u00b7nen", "schm\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Seinen Nechsten hassen, neiden,", "tokens": ["Sei\u00b7nen", "Nechs\u00b7ten", "has\u00b7sen", ",", "nei\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Dessen bestes stets vermeiden,", "tokens": ["Des\u00b7sen", "bes\u00b7tes", "stets", "ver\u00b7mei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Dessen Nachtheil emsig stifften,", "tokens": ["Des\u00b7sen", "Nacht\u00b7heil", "em\u00b7sig", "stiff\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Zungen-Honig, Hertzens-Gifften,", "tokens": ["Zun\u00b7gen\u00b7Ho\u00b7nig", ",", "Hert\u00b7zens\u00b7Giff\u00b7ten", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Jenes aussen, dieses innen", "tokens": ["Je\u00b7nes", "aus\u00b7sen", ",", "die\u00b7ses", "in\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PDS", "VVINF", "$,", "PDAT", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Lieblich, t\u00fcckisch f\u00fchren k\u00fcnnen:", "tokens": ["Lieb\u00b7lich", ",", "t\u00fc\u00b7ckisch", "f\u00fch\u00b7ren", "k\u00fcn\u00b7nen", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "VVINF", "VMINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Meinstu, da\u00df dem Christen-Leben", "tokens": ["Meins\u00b7tu", ",", "da\u00df", "dem", "Chris\u00b7ten\u00b7Le\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "KOUS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Beydes \u00e4hnlich sey und eben?", "tokens": ["Bey\u00b7des", "\u00e4hn\u00b7lich", "sey", "und", "e\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADJD", "VAFIN", "KON", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Gott hat neben sich gesetzet", "tokens": ["Gott", "hat", "ne\u00b7ben", "sich", "ge\u00b7set\u00b7zet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "APPR", "PRF", "VVPP"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.20": {"text": "Auch den Nechsten; wird verletzet", "tokens": ["Auch", "den", "Nechs\u00b7ten", ";", "wird", "ver\u00b7let\u00b7zet"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ART", "NN", "$.", "VAFIN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Durch den Dienst, der ihn gleich liebet", "tokens": ["Durch", "den", "Dienst", ",", "der", "ihn", "gleich", "lie\u00b7bet"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Und den Nechsten \u00fcbergibet;", "tokens": ["Und", "den", "Nechs\u00b7ten", "\u00fc\u00b7ber\u00b7gi\u00b7bet", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Halbe Christen sind zu nennen,", "tokens": ["Hal\u00b7be", "Chris\u00b7ten", "sind", "zu", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Die da Gott und Nechsten trennen.", "tokens": ["Die", "da", "Gott", "und", "Nechs\u00b7ten", "tren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}