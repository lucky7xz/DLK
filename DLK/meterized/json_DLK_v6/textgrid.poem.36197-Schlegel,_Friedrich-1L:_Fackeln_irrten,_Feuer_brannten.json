{"textgrid.poem.36197": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Fackeln irrten, Feuer brannten", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fackeln irrten, Feuer brannten", "tokens": ["Fa\u00b7ckeln", "irr\u00b7ten", ",", "Feu\u00b7er", "brann\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "$,", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In dem Walde um den Toten,", "tokens": ["In", "dem", "Wal\u00b7de", "um", "den", "To\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wei\u00dfe Zelte in dem Gr\u00fcnen", "tokens": ["Wei\u00b7\u00dfe", "Zel\u00b7te", "in", "dem", "Gr\u00fc\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "All der Leid- und Kriegsgenossen.", "tokens": ["All", "der", "Lei\u00b7d", "und", "Kriegs\u00b7ge\u00b7nos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "TRUNC", "KON", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Balsam, Aloe und Myrrhen", "tokens": ["Bal\u00b7sam", ",", "A\u00b7loe", "und", "Myr\u00b7rhen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "NN", "KON", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Mu\u00df die heil'gen Dienste zollen,", "tokens": ["Mu\u00df", "die", "heil'\u00b7gen", "Diens\u00b7te", "zol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Um den Leichnam zu erhalten,", "tokens": ["Um", "den", "Leich\u00b7nam", "zu", "er\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Bis er zu der Heimat komme.", "tokens": ["Bis", "er", "zu", "der", "Hei\u00b7mat", "kom\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Klagges\u00e4nge und Gebete", "tokens": ["Klag\u00b7ge\u00b7s\u00e4n\u00b7ge", "und", "Ge\u00b7be\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Steigen, feierlichen Tones,", "tokens": ["Stei\u00b7gen", ",", "fei\u00b7er\u00b7li\u00b7chen", "To\u00b7nes", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Durch die Nacht in dunkler Stunde,", "tokens": ["Durch", "die", "Nacht", "in", "dunk\u00b7ler", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Bis zu Gottes hohem Throne.", "tokens": ["Bis", "zu", "Got\u00b7tes", "ho\u00b7hem", "Thro\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Und nun ward ein Suchen, Tragen,", "tokens": ["Und", "nun", "ward", "ein", "Su\u00b7chen", ",", "Tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ART", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Als der Gottesdienst vollzogen,", "tokens": ["Als", "der", "Got\u00b7tes\u00b7dienst", "voll\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Bei des Fr\u00fchrots Morgenscheine,", "tokens": ["Bei", "des", "Fr\u00fch\u00b7rots", "Mor\u00b7gen\u00b7schei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Jeder f\u00fcr die Seinen sorgend;", "tokens": ["Je\u00b7der", "f\u00fcr", "die", "Sei\u00b7nen", "sor\u00b7gend", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Ein'ge f\u00fchren sie auf Bahren,", "tokens": ["Ein'\u00b7ge", "f\u00fch\u00b7ren", "sie", "auf", "Bah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Aus des Waldes Gr\u00fcn geflochten,", "tokens": ["Aus", "des", "Wal\u00b7des", "Gr\u00fcn", "ge\u00b7floch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Tragend andre auf den Schultern,", "tokens": ["Tra\u00b7gend", "and\u00b7re", "auf", "den", "Schul\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PIS", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Sorgsam andre auf den Rossen,", "tokens": ["Sorg\u00b7sam", "and\u00b7re", "auf", "den", "Ros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PIS", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Hier den Leichnam balsamierend,", "tokens": ["Hier", "den", "Leich\u00b7nam", "bal\u00b7sa\u00b7mie\u00b7rend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Dort in neue Klag' ergossen,", "tokens": ["Dort", "in", "neu\u00b7e", "Klag'", "er\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Andre lebend noch Verwund'te,", "tokens": ["And\u00b7re", "le\u00b7bend", "noch", "Ver\u00b7wun\u00b7d'\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ADJD", "ADV", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.24": {"text": "Tragen sie mit Sorge schonend.", "tokens": ["Tra\u00b7gen", "sie", "mit", "Sor\u00b7ge", "scho\u00b7nend", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Doch des sel'gen Rolands Leiche,", "tokens": ["Doch", "des", "sel'\u00b7gen", "Ro\u00b7lands", "Lei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.26": {"text": "Tr\u00e4gt auf Teppichen von Golde,", "tokens": ["Tr\u00e4gt", "auf", "Tep\u00b7pi\u00b7chen", "von", "Gol\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "APPR", "NE", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.27": {"text": "Eingeh\u00fcllt im F\u00fcrstenmantel,", "tokens": ["Ein\u00b7ge\u00b7h\u00fcllt", "im", "F\u00fcrs\u00b7ten\u00b7man\u00b7tel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Dort ein Maultierpaar erhoben,", "tokens": ["Dort", "ein", "Maul\u00b7tier\u00b7paar", "er\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "Schmuckvoll in des Zuges Mitte.", "tokens": ["Schmuck\u00b7voll", "in", "des", "Zu\u00b7ges", "Mit\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.30": {"text": "Bis nach Blavas hohem Schlosse", "tokens": ["Bis", "nach", "Bla\u00b7vas", "ho\u00b7hem", "Schlos\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "NE", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.31": {"text": "Hie\u00df ihn Kaiser Karol tragen,", "tokens": ["Hie\u00df", "ihn", "Kai\u00b7ser", "Ka\u00b7rol", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "NE", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Dort zu Sankt Romanus Dome,", "tokens": ["Dort", "zu", "Sankt", "Ro\u00b7ma\u00b7nus", "Do\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVFIN", "NE", "NE", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.33": {"text": "Den er selber hat gestiftet", "tokens": ["Den", "er", "sel\u00b7ber", "hat", "ge\u00b7stif\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "VAFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Und den stolzen Bau erhoben.", "tokens": ["Und", "den", "stol\u00b7zen", "Bau", "er\u00b7ho\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.35": {"text": "Da ward ehrenvoll die Leiche,", "tokens": ["Da", "ward", "eh\u00b7ren\u00b7voll", "die", "Lei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "Mit dem elfenbeinern Horne", "tokens": ["Mit", "dem", "el\u00b7fen\u00b7bei\u00b7nern", "Hor\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.37": {"text": "Zu den F\u00fc\u00dfen, und dem Schwerte", "tokens": ["Zu", "den", "F\u00fc\u00b7\u00dfen", ",", "und", "dem", "Schwer\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "KON", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.38": {"text": "Ruhend an dem Haupt des Toten,", "tokens": ["Ru\u00b7hend", "an", "dem", "Haupt", "des", "To\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.39": {"text": "In die tiefe Gruft gesenket,", "tokens": ["In", "die", "tie\u00b7fe", "Gruft", "ge\u00b7sen\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.40": {"text": "Bei dem Klang der Trauerglocken.", "tokens": ["Bei", "dem", "Klang", "der", "Trau\u00b7er\u00b7glo\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.41": {"text": "Selig wohl sind Blavas Mauern,", "tokens": ["Se\u00b7lig", "wohl", "sind", "Bla\u00b7vas", "Mau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VAFIN", "NE", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.42": {"text": "Welche Stadt in ihrem Scho\u00dfe", "tokens": ["Wel\u00b7che", "Stadt", "in", "ih\u00b7rem", "Scho\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAT", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.43": {"text": "Hat so hohen Gast empfangen,", "tokens": ["Hat", "so", "ho\u00b7hen", "Gast", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.44": {"text": "Trost dadurch und Schutz gewonnen.", "tokens": ["Trost", "da\u00b7durch", "und", "Schutz", "ge\u00b7won\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PAV", "KON", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.45": {"text": "Da um Roland nun die Klage,", "tokens": ["Da", "um", "Ro\u00b7land", "nun", "die", "Kla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "ADV", "ART", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.46": {"text": "Nach vollbrachtem Seelenopfer,", "tokens": ["Nach", "voll\u00b7brach\u00b7tem", "See\u00b7len\u00b7op\u00b7fer", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.47": {"text": "Nach vollbrachtem Totendienste,", "tokens": ["Nach", "voll\u00b7brach\u00b7tem", "To\u00b7ten\u00b7diens\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.48": {"text": "Wieder sich erheben wollte,", "tokens": ["Wie\u00b7der", "sich", "er\u00b7he\u00b7ben", "woll\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.49": {"text": "Sprach der gottgeweihte Bischof", "tokens": ["Sprach", "der", "gott\u00b7ge\u00b7weih\u00b7te", "Bi\u00b7schof"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN"], "meter": "+-+-+---", "measure": "unknown.measure.tri"}, "line.50": {"text": "Laut die trostesreichen Worte:", "tokens": ["Laut", "die", "tros\u00b7tes\u00b7rei\u00b7chen", "Wor\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.51": {"text": "\u00bbo wie sollte, Klag' anstimmend,", "tokens": ["\u00bb", "o", "wie", "soll\u00b7te", ",", "Klag'", "an\u00b7stim\u00b7mend", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "FM", "PWAV", "VMFIN", "$,", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.52": {"text": "Uns um den zu weinen ziemen,", "tokens": ["Uns", "um", "den", "zu", "wei\u00b7nen", "zie\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.53": {"text": "Welcher selig im Bezirke", "tokens": ["Wel\u00b7cher", "se\u00b7lig", "im", "Be\u00b7zir\u00b7ke"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAT", "ADJD", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.54": {"text": "Wohnet schon des Paradieses?", "tokens": ["Woh\u00b7net", "schon", "des", "Pa\u00b7ra\u00b7die\u00b7ses", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.55": {"text": "Gl\u00e4nzend wohl und ruhmgezieret", "tokens": ["Gl\u00e4n\u00b7zend", "wohl", "und", "ruhm\u00b7ge\u00b7zie\u00b7ret"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "ADV", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.56": {"text": "War er, als er wallt' hienieden,", "tokens": ["War", "er", ",", "als", "er", "wallt'", "hien\u00b7ie\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.57": {"text": "Doch noch heller jetzo schimmert", "tokens": ["Doch", "noch", "hel\u00b7ler", "jet\u00b7zo", "schim\u00b7mert"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.58": {"text": "Hoch er \u00fcber den Gestirnen.", "tokens": ["Hoch", "er", "\u00fc\u00b7ber", "den", "Ge\u00b7stir\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.59": {"text": "Denn in seines Herzens Tiefe", "tokens": ["Denn", "in", "sei\u00b7nes", "Her\u00b7zens", "Tie\u00b7fe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.60": {"text": "War ja Gottes Wort geschrieben;", "tokens": ["War", "ja", "Got\u00b7tes", "Wort", "ge\u00b7schrie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NN", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.61": {"text": "Heiter war er, fromm und bieder,", "tokens": ["Hei\u00b7ter", "war", "er", ",", "fromm", "und", "bie\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.62": {"text": "Allen er ein Vater schiene,", "tokens": ["Al\u00b7len", "er", "ein", "Va\u00b7ter", "schie\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.63": {"text": "War der Ehre Licht und Gipfel", "tokens": ["War", "der", "Eh\u00b7re", "Licht", "und", "Gip\u00b7fel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.64": {"text": "Und des Rittertumes Zierde.", "tokens": ["Und", "des", "Rit\u00b7ter\u00b7tu\u00b7mes", "Zier\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.65": {"text": "Drum so wendet nicht die Blicke", "tokens": ["Drum", "so", "wen\u00b7det", "nicht", "die", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "ADV", "VVFIN", "PTKNEG", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.66": {"text": "Zu dem Sarge, wo mit nichten", "tokens": ["Zu", "dem", "Sar\u00b7ge", ",", "wo", "mit", "nich\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PWAV", "APPR", "PIS"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.67": {"text": "Ihr noch k\u00f6nnt den Edlen finden,", "tokens": ["Ihr", "noch", "k\u00f6nnt", "den", "Ed\u00b7len", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.68": {"text": "Der jetzt schon hinaufgestiegen", "tokens": ["Der", "jetzt", "schon", "hin\u00b7auf\u00b7ge\u00b7stie\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADV", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.69": {"text": "Ist zu jener Burg des Himmels.\u00ab \u2013", "tokens": ["Ist", "zu", "je\u00b7ner", "Burg", "des", "Him\u00b7mels", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VAFIN", "APPR", "PDAT", "NN", "ART", "NN", "$.", "$(", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.70": {"text": "Also lauten jenes frommen", "tokens": ["Al\u00b7so", "lau\u00b7ten", "je\u00b7nes", "from\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PDAT", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.71": {"text": "Bischofs Worte voll des Trostes.", "tokens": ["Bi\u00b7schofs", "Wor\u00b7te", "voll", "des", "Tros\u00b7tes", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.72": {"text": "Manche Helden sie begruben,", "tokens": ["Man\u00b7che", "Hel\u00b7den", "sie", "be\u00b7gru\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.73": {"text": "Da Roland bestattet worden,", "tokens": ["Da", "Ro\u00b7land", "be\u00b7stat\u00b7tet", "wor\u00b7den", ","], "token_info":