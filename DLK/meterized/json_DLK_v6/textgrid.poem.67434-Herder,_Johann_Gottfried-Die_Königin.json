{"textgrid.poem.67434": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Die K\u00f6nigin", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Soll ich, o K\u00f6nigin, Dich um das, was Jedem gef\u00e4llet,", "tokens": ["Soll", "ich", ",", "o", "K\u00f6\u00b7ni\u00b7gin", ",", "Dich", "um", "das", ",", "was", "Je\u00b7dem", "ge\u00b7f\u00e4l\u00b7let", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "FM", "NN", "$,", "PRF", "APPR", "PDS", "$,", "PRELS", "PIS", "VVPP", "$,"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Preisen? Ich preis' in Dir, was nur den G\u00f6ttern gef\u00e4llt.", "tokens": ["Prei\u00b7sen", "?", "Ich", "preis'", "in", "Dir", ",", "was", "nur", "den", "G\u00f6t\u00b7tern", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VVFIN", "APPR", "PPER", "$,", "PRELS", "ADV", "ART", "NN", "VVPP", "$."], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Deine Sch\u00f6nheit opferst Du gern der edleren Tugend,", "tokens": ["Dei\u00b7ne", "Sch\u00f6n\u00b7heit", "op\u00b7ferst", "Du", "gern", "der", "ed\u00b7le\u00b7ren", "Tu\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Deine H\u00f6he der Huld sittlicher Grazien auf.", "tokens": ["Dei\u00b7ne", "H\u00f6\u00b7he", "der", "Huld", "sitt\u00b7li\u00b7cher", "Gra\u00b7zi\u00b7en", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+---+--+--+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Kronen bietet man Dir; Du kennst der Kronen nur", "tokens": ["Kro\u00b7nen", "bie\u00b7tet", "man", "Dir", ";", "Du", "kennst", "der", "Kro\u00b7nen", "nur"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PIS", "PPER", "$.", "PPER", "VVFIN", "ART", "NN", "ADV"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "Dich; sie gl\u00e4nzet aus Dir rings in der Liebe des Volks.", "tokens": ["Dich", ";", "sie", "gl\u00e4n\u00b7zet", "aus", "Dir", "rings", "in", "der", "Lie\u00b7be", "des", "Volks", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$.", "PPER", "VVFIN", "APPR", "PPER", "ADV", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}}}}}