{"dta.poem.2899": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "100.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Und s\u00e4hest du auch Tod und Weh im Leben nie,", "tokens": ["Und", "s\u00e4\u00b7hest", "du", "auch", "Tod", "und", "Weh", "im", "Le\u00b7ben", "nie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "NN", "KON", "NN", "APPRART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es ist in deiner Sprach', in deiner Fantasie.", "tokens": ["Es", "ist", "in", "dei\u00b7ner", "Sprach'", ",", "in", "dei\u00b7ner", "Fan\u00b7ta\u00b7sie", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Du siehst es innerlich, und h\u00f6rst es geistig immer;", "tokens": ["Du", "siehst", "es", "in\u00b7ner\u00b7lich", ",", "und", "h\u00f6rst", "es", "geis\u00b7tig", "im\u00b7mer", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$,", "KON", "VVFIN", "PPER", "ADJD", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Schatten \u00fcbert\u00fcncht kein Luft- und Lebensschimmer.", "tokens": ["Den", "Schat\u00b7ten", "\u00fc\u00b7ber\u00b7t\u00fcncht", "kein", "Luft", "und", "Le\u00b7bens\u00b7schim\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIAT", "TRUNC", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Gewohnheit dumpfe nur macht dich vom Schreckbild frei,", "tokens": ["Ge\u00b7wohn\u00b7heit", "dump\u00b7fe", "nur", "macht", "dich", "vom", "Schreck\u00b7bild", "frei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du h\u00f6rest es und siehst, und denkest nichts dabei.", "tokens": ["Du", "h\u00f6\u00b7rest", "es", "und", "siehst", ",", "und", "den\u00b7kest", "nichts", "da\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KON", "VVFIN", "$,", "KON", "VVFIN", "PIS", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}