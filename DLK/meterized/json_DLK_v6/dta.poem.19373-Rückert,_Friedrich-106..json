{"dta.poem.19373": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "106.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nachahmung ist was sich zuerst im Kinde regt,", "tokens": ["Nac\u00b7hah\u00b7mung", "ist", "was", "sich", "zu\u00b7erst", "im", "Kin\u00b7de", "regt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PRELS", "PRF", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was ihm die schwache Hand, den zarten Mund bewegt.", "tokens": ["Was", "ihm", "die", "schwa\u00b7che", "Hand", ",", "den", "zar\u00b7ten", "Mund", "be\u00b7wegt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Es tr\u00e4gt die Puppe, wie es selbst die Mutter trug,", "tokens": ["Es", "tr\u00e4gt", "die", "Pup\u00b7pe", ",", "wie", "es", "selbst", "die", "Mut\u00b7ter", "trug", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PWAV", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und schl\u00e4gt auf das Klavier, weil es der Bruder schlug.", "tokens": ["Und", "schl\u00e4gt", "auf", "das", "Kla\u00b7vier", ",", "weil", "es", "der", "Bru\u00b7der", "schlug", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-++-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Es nimmt das Buch, woraus der Vater betend las;", "tokens": ["Es", "nimmt", "das", "Buch", ",", "wo\u00b7raus", "der", "Va\u00b7ter", "be\u00b7tend", "las", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PWAV", "ART", "NN", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was es handfaltend summt, auch ein Gebet ist das.", "tokens": ["Was", "es", "hand\u00b7fal\u00b7tend", "summt", ",", "auch", "ein", "Ge\u00b7bet", "ist", "das", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADJD", "VVFIN", "$,", "ADV", "ART", "NN", "VAFIN", "PDS", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}}, "stanza.4": {"line.1": {"text": "Du kanst nicht besser streun in ihm des Guten Samen,", "tokens": ["Du", "kanst", "nicht", "bes\u00b7ser", "streun", "in", "ihm", "des", "Gu\u00b7ten", "Sa\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADJD", "VVFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als wenn du Gutes stets ihm vorthust, nachzuahmen.", "tokens": ["Als", "wenn", "du", "Gu\u00b7tes", "stets", "ihm", "vor\u00b7thust", ",", "nach\u00b7zu\u00b7ah\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "NN", "VVFIN", "PPER", "VVFIN", "$,", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}