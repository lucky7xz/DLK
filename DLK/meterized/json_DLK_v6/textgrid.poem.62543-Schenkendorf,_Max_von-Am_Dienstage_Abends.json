{"textgrid.poem.62543": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "Am Dienstage Abends", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach Herr, wann wird die Stunde schlagen,", "tokens": ["Ach", "Herr", ",", "wann", "wird", "die", "Stun\u00b7de", "schla\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$,", "PWAV", "VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die meinen hei\u00dfen Wunsch erf\u00fcllt?", "tokens": ["Die", "mei\u00b7nen", "hei\u00b7\u00dfen", "Wunsch", "er\u00b7f\u00fcllt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wann alle, alle Plagen", "tokens": ["Wann", "al\u00b7le", ",", "al\u00b7le", "Pla\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PWAV", "PIS", "$,", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Versinken, wie ein n\u00e4chtlich Bild?", "tokens": ["Ver\u00b7sin\u00b7ken", ",", "wie", "ein", "n\u00e4cht\u00b7lich", "Bild", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wann schlie\u00dfest du", "tokens": ["Wann", "schlie\u00b7\u00dfest", "du"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Zur ew'gen Ruh'", "tokens": ["Zur", "ew'\u00b7gen", "Ruh'"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Mein thr\u00e4nenschweres Auge zu?", "tokens": ["Mein", "thr\u00e4\u00b7nen\u00b7schwe\u00b7res", "Au\u00b7ge", "zu", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}