{"textgrid.poem.40170": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Symbol", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Menschheit Seele nahm Gestalt im Weibe,", "tokens": ["Der", "Menschheit", "See\u00b7le", "nahm", "Ge\u00b7stalt", "im", "Wei\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "im Traum vom Weibe deuten sich die Zeiten:", "tokens": ["im", "Traum", "vom", "Wei\u00b7be", "deu\u00b7ten", "sich", "die", "Zei\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "NN", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "des Griechen ", "tokens": ["des", "Grie\u00b7chen"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "umfa\u00dft in ", "tokens": ["um\u00b7fa\u00dft", "in"], "token_info": ["word", "word"], "pos": ["VVFIN", "APPR"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.2": {"line.1": {"text": "des Christen Sehnsucht nach der ", "tokens": ["des", "Chris\u00b7ten", "Sehn\u00b7sucht", "nach", "der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "des Weibes reinste Liebe zu verkl\u00e4ren,", "tokens": ["des", "Wei\u00b7bes", "reins\u00b7te", "Lie\u00b7be", "zu", "ver\u00b7kl\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "wallt zu ", "tokens": ["wallt", "zu"], "token_info": ["word", "word"], "pos": ["VVFIN", "APPR"], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "die keusch des Sohns des keuscheren genesen.", "tokens": ["die", "keusch", "des", "Sohns", "des", "keu\u00b7sche\u00b7ren", "ge\u00b7ne\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ART", "NN", "ART", "ADJA", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wann kommt die Zeit, da aus der Menschheit Gr\u00fcnden", "tokens": ["Wann", "kommt", "die", "Zeit", ",", "da", "aus", "der", "Menschheit", "Gr\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ART", "NN", "$,", "KOUS", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "ein ", "tokens": ["ein"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "der Reinheit endlich wird die Sch\u00f6nheit ", "tokens": ["der", "Rein\u00b7heit", "end\u00b7lich", "wird", "die", "Sch\u00f6n\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "und Allen, Allen die Erl\u00f6sung k\u00fcnden,", "tokens": ["und", "Al\u00b7len", ",", "Al\u00b7len", "die", "Er\u00b7l\u00f6\u00b7sung", "k\u00fcn\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "$,", "NE", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "die Ich, Geliebte, einst in ", "tokens": ["die", "Ich", ",", "Ge\u00b7lieb\u00b7te", ",", "einst", "in"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ART", "PPER", "$,", "NN", "$,", "ADV", "APPR"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "du ", "tokens": ["du"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}}}}}