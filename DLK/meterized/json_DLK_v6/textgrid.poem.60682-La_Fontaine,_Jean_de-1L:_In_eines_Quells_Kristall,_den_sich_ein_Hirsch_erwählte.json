{"textgrid.poem.60682": {"metadata": {"author": {"name": "La Fontaine, Jean de", "birth": "N.A.", "death": "N.A."}, "title": "1L: In eines Quells Kristall, den sich ein Hirsch erw\u00e4hlte", "genre": "verse", "period": "N.A.", "pub_year": 1658, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In eines Quells Kristall, den sich ein Hirsch erw\u00e4hlte", "tokens": ["In", "ei\u00b7nes", "Quells", "Kris\u00b7tall", ",", "den", "sich", "ein", "Hirsch", "er\u00b7w\u00e4hl\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "$,", "PRELS", "PRF", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als Spiegelglas, sah er und lobte voller Preis", "tokens": ["Als", "Spie\u00b7gel\u00b7glas", ",", "sah", "er", "und", "lob\u00b7te", "vol\u00b7ler", "Preis"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "$,", "VVFIN", "PPER", "KON", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Sch\u00f6nheit des Geweihs.", "tokens": ["Die", "Sch\u00f6n\u00b7heit", "des", "Ge\u00b7weihs", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Was um so mehr ihn qu\u00e4lte,", "tokens": ["Was", "um", "so", "mehr", "ihn", "qu\u00e4l\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ADV", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Das war die D\u00fcnnheit seiner Beine,", "tokens": ["Das", "war", "die", "D\u00fcnn\u00b7heit", "sei\u00b7ner", "Bei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die man im Wasser fast verschwinden sah.", "tokens": ["Die", "man", "im", "Was\u00b7ser", "fast", "ver\u00b7schwin\u00b7den", "sah", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "APPRART", "NN", "ADV", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Er stand in eitler Selbstbetrachtung da,", "tokens": ["Er", "stand", "in", "eit\u00b7ler", "Selbst\u00b7be\u00b7trach\u00b7tung", "da", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Zufrieden bis auf dieses eine,", "tokens": ["Zu\u00b7frie\u00b7den", "bis", "auf", "die\u00b7ses", "ei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "APPR", "PDAT", "ART", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Da\u00df ihm sein Beinwerk keine Ehre mache.", "tokens": ["Da\u00df", "ihm", "sein", "Bein\u00b7werk", "kei\u00b7ne", "Eh\u00b7re", "ma\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "\u00bbwelch dumme Sache,\u00ab", "tokens": ["\u00bb", "welch", "dum\u00b7me", "Sa\u00b7che", ",", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWAT", "ADJA", "NN", "$,", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "So sprach er, \u00bbda\u00df solch stolzes Haupt,", "tokens": ["So", "sprach", "er", ",", "\u00bb", "da\u00df", "solch", "stol\u00b7zes", "Haupt", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "$(", "KOUS", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Das Astwerk tr\u00e4gt, als h\u00e4tt ich einen Baum beraubt,", "tokens": ["Das", "Ast\u00b7werk", "tr\u00e4gt", ",", "als", "h\u00e4tt", "ich", "ei\u00b7nen", "Baum", "be\u00b7raubt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KOKOM", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Auf spindeld\u00fcrren Beinen leben mu\u00df!\u00ab", "tokens": ["Auf", "spin\u00b7del\u00b7d\u00fcr\u00b7ren", "Bei\u00b7nen", "le\u00b7ben", "mu\u00df", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "VMFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Indem er also klagte voll Verdru\u00df,", "tokens": ["In\u00b7dem", "er", "al\u00b7so", "klag\u00b7te", "voll", "Ver\u00b7dru\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Kommt wild ein Jagdhund angeschnaubt.", "tokens": ["Kommt", "wild", "ein", "Jagd\u00b7hund", "an\u00b7ge\u00b7schnaubt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Er will sich retten, fl\u00fcchtet in das Waldrevier.", "tokens": ["Er", "will", "sich", "ret\u00b7ten", ",", "fl\u00fcch\u00b7tet", "in", "das", "Wald\u00b7re\u00b7vier", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "VVINF", "$,", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Doch sein Geweih \u2013 verh\u00e4ngnisvolle Zier \u2013", "tokens": ["Doch", "sein", "Ge\u00b7weih", "\u2013", "ver\u00b7h\u00e4ng\u00b7nis\u00b7vol\u00b7le", "Zier", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$(", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "H\u00e4lt jeden Augenblick ihn auf,", "tokens": ["H\u00e4lt", "je\u00b7den", "Au\u00b7gen\u00b7blick", "ihn", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Den Dienst verhindernd, den die Beine leisten wollen,", "tokens": ["Den", "Dienst", "ver\u00b7hin\u00b7dernd", ",", "den", "die", "Bei\u00b7ne", "leis\u00b7ten", "wol\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "PRELS", "ART", "NN", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Die ihm durch schnellen Lauf", "tokens": ["Die", "ihm", "durch", "schnel\u00b7len", "Lauf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.21": {"text": "Das Leben retten sollen.", "tokens": ["Das", "Le\u00b7ben", "ret\u00b7ten", "sol\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.22": {"text": "Da widerrief er und verw\u00fcnschte laut die Gaben,", "tokens": ["Da", "wi\u00b7der\u00b7rief", "er", "und", "ver\u00b7w\u00fcnschte", "laut", "die", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.23": {"text": "Die ihm der Himmel j\u00e4hrlich neu gebracht.", "tokens": ["Die", "ihm", "der", "Him\u00b7mel", "j\u00e4hr\u00b7lich", "neu", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "ADJD", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Sch\u00f6nheit besticht; was n\u00fctzt, das l\u00e4\u00dft man au\u00dfer acht.", "tokens": ["Sch\u00f6n\u00b7heit", "be\u00b7sticht", ";", "was", "n\u00fctzt", ",", "das", "l\u00e4\u00dft", "man", "au\u00b7\u00dfer", "acht", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "$.", "PWS", "VVFIN", "$,", "PDS", "VVFIN", "PIS", "APPR", "CARD", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.3": {"line.1": {"text": "Und doch wird Sch\u00f6nes oft Verderben f\u00fcr uns haben.", "tokens": ["Und", "doch", "wird", "Sch\u00f6\u00b7nes", "oft", "Ver\u00b7der\u00b7ben", "f\u00fcr", "uns", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "NN", "ADV", "NN", "APPR", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Dem Hirsch mi\u00dff\u00e4llt sein Bein, das ihn behende macht,", "tokens": ["Dem", "Hirsch", "mi\u00df\u00b7f\u00e4llt", "sein", "Bein", ",", "das", "ihn", "be\u00b7hen\u00b7de", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,", "PRELS", "PPER", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Ein Hemmnis, sein Geweih, preist er voll Unbedacht.", "tokens": ["Ein", "Hemm\u00b7nis", ",", "sein", "Ge\u00b7weih", ",", "preist", "er", "voll", "Un\u00b7be\u00b7dacht", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}