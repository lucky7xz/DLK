{"textgrid.poem.37236": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "[mich wurmt es, wenn ich nur dran denke]", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mich wurmt es, wenn ich nur dran denke. \u2013", "tokens": ["Mich", "wurmt", "es", ",", "wenn", "ich", "nur", "dran", "den\u00b7ke", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADV", "PAV", "VVFIN", "$.", "$("], "meter": "-+--+-+--", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Es sa\u00df zu M\u00fcnchen in der Schenke", "tokens": ["Es", "sa\u00df", "zu", "M\u00fcn\u00b7chen", "in", "der", "Schen\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NE", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Protz mit dunkelroter Nase", "tokens": ["Ein", "Protz", "mit", "dun\u00b7kel\u00b7ro\u00b7ter", "Na\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Beim elften oder zw\u00f6lften Glase.", "tokens": ["Beim", "elf\u00b7ten", "o\u00b7der", "zw\u00f6lf\u00b7ten", "Gla\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Da schlich sich k\u00fcmmerlich heran", "tokens": ["Da", "schlich", "sich", "k\u00fcm\u00b7mer\u00b7lich", "he\u00b7ran"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ADJD", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein armer, alter Bettelmann,", "tokens": ["Ein", "ar\u00b7mer", ",", "al\u00b7ter", "Bet\u00b7tel\u00b7mann", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zog vor dem Protzen seinen Hut", "tokens": ["Zog", "vor", "dem", "Prot\u00b7zen", "sei\u00b7nen", "Hut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und fleht: Gn\u00e4 Herr, ach sein S' so gut!", "tokens": ["Und", "fleht", ":", "Gn\u00e4", "Herr", ",", "ach", "sein", "S'", "so", "gut", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "NE", "NN", "$,", "XY", "PPOSAT", "NN", "ADV", "ADJD", "$."], "meter": "-++-+-+-+", "measure": "unknown.measure.penta"}}, "stanza.3": {"line.1": {"text": "Der Protz jedoch, fuchsteufelswild,", "tokens": ["Der", "Protz", "je\u00b7doch", ",", "fuchs\u00b7teu\u00b7fels\u00b7wild", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Statt was zu geben, flucht und schilt:", "tokens": ["Statt", "was", "zu", "ge\u00b7ben", ",", "flucht", "und", "schilt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PIS", "PTKZU", "VVINF", "$,", "VVFIN", "KON", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Gehst raus, du alter Lump, du schlechter!", "tokens": ["Gehst", "raus", ",", "du", "al\u00b7ter", "Lump", ",", "du", "schlech\u00b7ter", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,", "PPER", "ADJA", "NN", "$,", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nix m\u00f6cht' er, als grad saufen m\u00f6cht' er!", "tokens": ["Nix", "m\u00f6cht'", "er", ",", "als", "grad", "sau\u00b7fen", "m\u00f6cht'", "er", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "$,", "KOUS", "ADV", "VVINF", "VMFIN", "PPER", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}