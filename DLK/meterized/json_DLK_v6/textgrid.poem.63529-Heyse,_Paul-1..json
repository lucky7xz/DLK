{"textgrid.poem.63529": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf die Nacht in den Spinnstuben", "tokens": ["Auf", "die", "Nacht", "in", "den", "Spinn\u00b7stu\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+--++-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Da singen die M\u00e4dchen,", "tokens": ["Da", "sin\u00b7gen", "die", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Da lachen die Dorfbuben,", "tokens": ["Da", "la\u00b7chen", "die", "Dorf\u00b7bu\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+--++-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wie flink gehn die R\u00e4dchen!", "tokens": ["Wie", "flink", "gehn", "die", "R\u00e4d\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Spinnt jedes am Brautschatz,", "tokens": ["Spinnt", "je\u00b7des", "am", "Braut\u00b7schatz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "APPRART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Da\u00df der Liebste sich freut.", "tokens": ["Da\u00df", "der", "Liebs\u00b7te", "sich", "freut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Nicht lange, so gibt es", "tokens": ["Nicht", "lan\u00b7ge", ",", "so", "gibt", "es"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "$,", "ADV", "VVFIN", "PPER"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Ein Hochzeitsgel\u00e4ut.", "tokens": ["Ein", "Hoch\u00b7zeits\u00b7ge\u00b7l\u00e4ut", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Kein Mensch, der mir gut ist,", "tokens": ["Kein", "Mensch", ",", "der", "mir", "gut", "ist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "PPER", "ADJD", "VAFIN", "$,"], "meter": "-+-+++", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Will nach mir fragen.", "tokens": ["Will", "nach", "mir", "fra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Wie bang mir zumut ist,", "tokens": ["Wie", "bang", "mir", "zu\u00b7mut", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wem soll ich's klagen?", "tokens": ["Wem", "soll", "ich's", "kla\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PIS", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Die Tr\u00e4nen rinnen", "tokens": ["Die", "Tr\u00e4\u00b7nen", "rin\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Mir \u00fcbers Gesicht \u2013", "tokens": ["Mir", "\u00fc\u00b7bers", "Ge\u00b7sicht", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Wof\u00fcr ich soll spinnen,", "tokens": ["Wo\u00b7f\u00fcr", "ich", "soll", "spin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Ich wei\u00df es nicht!", "tokens": ["Ich", "wei\u00df", "es", "nicht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}