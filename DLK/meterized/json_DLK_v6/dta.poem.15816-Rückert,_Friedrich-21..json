{"dta.poem.15816": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "21.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du klagest, junger Freund, unfreundlich sei dein Haus,", "tokens": ["Du", "kla\u00b7gest", ",", "jun\u00b7ger", "Freund", ",", "un\u00b7freund\u00b7lich", "sei", "dein", "Haus", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADJA", "NN", "$,", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und denkst dir mancherlei, dem abzuhelfen, aus.", "tokens": ["Und", "denkst", "dir", "man\u00b7cher\u00b7lei", ",", "dem", "ab\u00b7zu\u00b7hel\u00b7fen", ",", "aus", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIS", "$,", "PRELS", "VVIZU", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ich rathe dir, hinein ein freundlich Weib zu f\u00fchren,", "tokens": ["Ich", "ra\u00b7the", "dir", ",", "hin\u00b7ein", "ein", "freund\u00b7lich", "Weib", "zu", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "ADV", "ART", "ADJD", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So wirst du Freundlichkeit in allen Ecken sp\u00fcren.", "tokens": ["So", "wirst", "du", "Freund\u00b7lich\u00b7keit", "in", "al\u00b7len", "E\u00b7cken", "sp\u00fc\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAF