{"textgrid.poem.63975": {"metadata": {"author": {"name": "Hille, Peter", "birth": "N.A.", "death": "N.A."}, "title": "Serpentinreiterin", "genre": "verse", "period": "N.A.", "pub_year": 1879, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein sehniger Adel die junge Gestalt,", "tokens": ["Ein", "seh\u00b7ni\u00b7ger", "A\u00b7del", "die", "jun\u00b7ge", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Den wippenden Zelter in leiser Gewalt,", "tokens": ["Den", "wip\u00b7pen\u00b7den", "Zel\u00b7ter", "in", "lei\u00b7ser", "Ge\u00b7walt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Nun reitet sie rund in wendendem Kreis", "tokens": ["Nun", "rei\u00b7tet", "sie", "rund", "in", "wen\u00b7den\u00b7dem", "Kreis"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "PIAT", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wie der steinerne Gast unirdisch wei\u00df.", "tokens": ["Wie", "der", "stei\u00b7ner\u00b7ne", "Gast", "un\u00b7ir\u00b7disch", "wei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "ADJD", "VVFIN", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "M\u00e4nnerseelen ger\u00e4nnen zu Eis,", "tokens": ["M\u00e4n\u00b7ner\u00b7see\u00b7len", "ge\u00b7r\u00e4n\u00b7nen", "zu", "Eis", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "NN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Ein Don Juanblut treibt k\u00fchner und hei\u00df.", "tokens": ["Ein", "Don", "Ju\u00b7an\u00b7blut", "treibt", "k\u00fch\u00b7ner", "und", "hei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Nun das da? Was f\u00fcr ein sonderes Ding?", "tokens": ["Nun", "das", "da", "?", "Was", "f\u00fcr", "ein", "son\u00b7de\u00b7res", "Ding", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADV", "$.", "PWS", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-----+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Ein berittener Schmetterling!", "tokens": ["Ein", "be\u00b7rit\u00b7te\u00b7ner", "Schmet\u00b7ter\u00b7ling", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.5": {"line.1": {"text": "Nun kommt von Farren ein Fl\u00f6ten und Flirren", "tokens": ["Nun", "kommt", "von", "Far\u00b7ren", "ein", "Fl\u00f6\u00b7ten", "und", "Flir\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN", "ART", "NN", "KON", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Auftrachtendes Schlagen, farbiges Girren,", "tokens": ["Auf\u00b7trach\u00b7ten\u00b7des", "Schla\u00b7gen", ",", "far\u00b7bi\u00b7ges", "Gir\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Ergie\u00dfen und Flattern, ein br\u00fcnstiges Bl\u00fchen", "tokens": ["Er\u00b7gie\u00b7\u00dfen", "und", "Flat\u00b7tern", ",", "ein", "br\u00fcns\u00b7ti\u00b7ges", "Bl\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "KON", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Breithinschmausende T\u00f6ne ergl\u00fchen.", "tokens": ["Brei\u00b7thinsc\u00b7hmau\u00b7sen\u00b7de", "T\u00f6\u00b7ne", "er\u00b7gl\u00fc\u00b7hen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Gr\u00fcnleuchtendes Winden, purpurnes Schweifen", "tokens": ["Gr\u00fcn\u00b7leuch\u00b7ten\u00b7des", "Win\u00b7den", ",", "pur\u00b7pur\u00b7nes", "Schwei\u00b7fen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN"], "meter": "++--+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Kelche, die nach Blumen greifen.", "tokens": ["Kel\u00b7che", ",", "die", "nach", "Blu\u00b7men", "grei\u00b7fen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Alle Leidenschaft angefacht.", "tokens": ["Al\u00b7le", "Lei\u00b7den\u00b7schaft", "an\u00b7ge\u00b7facht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVPP", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.8": {"text": "Gro\u00dfgestirnte tiefblaue Nacht.", "tokens": ["Gro\u00df\u00b7ges\u00b7tirn\u00b7te", "tief\u00b7blau\u00b7e", "Nacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.6": {"line.1": {"text": "Der Pegasus, das Musenro\u00df,", "tokens": ["Der", "Pe\u00b7ga\u00b7sus", ",", "das", "Mu\u00b7sen\u00b7ro\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NE", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie's aufrecht in den Himmel scho\u00df!", "tokens": ["Wie's", "auf\u00b7recht", "in", "den", "Him\u00b7mel", "scho\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "In tauschendem Rausch das mutige Leben,", "tokens": ["In", "tau\u00b7schen\u00b7dem", "Rausch", "das", "mu\u00b7ti\u00b7ge", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das Starke allein sich wissen zu geben.", "tokens": ["Das", "Star\u00b7ke", "al\u00b7lein", "sich", "wis\u00b7sen", "zu", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PRF", "VVINF", "PTKZU", "VVINF", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Ein sehniger Adel die junge Gestalt,", "tokens": ["Ein", "seh\u00b7ni\u00b7ger", "A\u00b7del", "die", "jun\u00b7ge", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Den wippenden Zelter in leiser Gewalt.", "tokens": ["Den", "wip\u00b7pen\u00b7den", "Zel\u00b7ter", "in", "lei\u00b7ser", "Ge\u00b7walt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}}}}