{"dta.poem.20044": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "An die ** .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wann mit frevelndem Mund ihr Heiliges wagt zu verk\u00fcnden,", "tokens": ["Wann", "mit", "fre\u00b7veln\u00b7dem", "Mund", "ihr", "Hei\u00b7li\u00b7ges", "wagt", "zu", "ver\u00b7k\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ADJA", "NN", "PPOSAT", "NN", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "--+--+-+-++--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Und vor dem Altar steht, hinter dem R\u00fccken das Kreuz.", "tokens": ["Und", "vor", "dem", "Al\u00b7tar", "steht", ",", "hin\u00b7ter", "dem", "R\u00fc\u00b7cken", "das", "Kreuz", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "$,", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Fa\u00dft ein Schauer mich oft, und ich steh' in banger Er-", "tokens": ["Fa\u00dft", "ein", "Schau\u00b7er", "mich", "oft", ",", "und", "ich", "steh'", "in", "ban\u00b7ger", "Er"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "PPER", "ADV", "$,", "KON", "PPER", "VVFIN", "APPR", "ADJA", "TRUNC"], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "wartung,", "tokens": ["war\u00b7tung", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Ob vor Anmuth nicht springe vom Kreuze das Bild.", "tokens": ["Ob", "vor", "An\u00b7muth", "