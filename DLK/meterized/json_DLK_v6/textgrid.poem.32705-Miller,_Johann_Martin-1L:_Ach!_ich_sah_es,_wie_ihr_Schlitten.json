{"textgrid.poem.32705": {"metadata": {"author": {"name": "Miller, Johann Martin", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ach! ich sah es, wie ihr Schlitten", "genre": "verse", "period": "N.A.", "pub_year": 1772, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach! ich sah es, wie ihr Schlitten", "tokens": ["Ach", "!", "ich", "sah", "es", ",", "wie", "ihr", "Schlit\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "$.", "PPER", "VVFIN", "PPER", "$,", "PWAV", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Windeschnell vorbeigeglitten.", "tokens": ["Win\u00b7de\u00b7schnell", "vor\u00b7bei\u00b7ge\u00b7glit\u00b7ten", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In dem ganzen, langen Reihn", "tokens": ["In", "dem", "gan\u00b7zen", ",", "lan\u00b7gen", "Reihn"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sah ich ihren nur allein!", "tokens": ["Sah", "ich", "ih\u00b7ren", "nur", "al\u00b7lein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Von den Silbergl\u00f6ckchen allen", "tokens": ["Von", "den", "Sil\u00b7ber\u00b7gl\u00f6ck\u00b7chen", "al\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00f6rt' ich ihre nur erschallen;", "tokens": ["H\u00f6rt'", "ich", "ih\u00b7re", "nur", "er\u00b7schal\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber trauervoll und bang", "tokens": ["A\u00b7ber", "trau\u00b7er\u00b7voll", "und", "bang"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "T\u00f6nte mir ihr Silberklang.", "tokens": ["T\u00f6n\u00b7te", "mir", "ihr", "Sil\u00b7ber\u00b7klang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Dieses Herz, das ich besessen,", "tokens": ["Die\u00b7ses", "Herz", ",", "das", "ich", "be\u00b7ses\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wird es meiner nicht vergessen?", "tokens": ["Wird", "es", "mei\u00b7ner", "nicht", "ver\u00b7ges\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "PTKNEG", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Lachte sie dem J\u00fcngling nicht", "tokens": ["Lach\u00b7te", "sie", "dem", "J\u00fcng\u00b7ling", "nicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKNEG"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit vergn\u00fcgtem Angesicht?", "tokens": ["Mit", "ver\u00b7gn\u00fcg\u00b7tem", "An\u00b7ge\u00b7sicht", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "O ihr schneebedeckten H\u00f6hen,", "tokens": ["O", "ihr", "schnee\u00b7be\u00b7deck\u00b7ten", "H\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo ich sie zuerst gesehen,", "tokens": ["Wo", "ich", "sie", "zu\u00b7erst", "ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Stimmt in meinen Klageton!", "tokens": ["Stimmt", "in", "mei\u00b7nen", "Kla\u00b7ge\u00b7ton", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sie verga\u00df Amyntens schon!", "tokens": ["Sie", "ver\u00b7ga\u00df", "A\u00b7myn\u00b7tens", "schon", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "In dem Schnee, auf diesen Auen,", "tokens": ["In", "dem", "Schnee", ",", "auf", "die\u00b7sen", "Au\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Soll man ihren Namen schauen!", "tokens": ["Soll", "man", "ih\u00b7ren", "Na\u00b7men", "schau\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Unter tiefer Herzenspein,", "tokens": ["Un\u00b7ter", "tie\u00b7fer", "Her\u00b7zen\u00b7spein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Grab' ich hundertmal ihn ein.", "tokens": ["Grab'", "ich", "hun\u00b7dert\u00b7mal", "ihn", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Aber bald wird er vergehen!", "tokens": ["A\u00b7ber", "bald", "wird", "er", "ver\u00b7ge\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Winde werden ihn verwehen!", "tokens": ["Win\u00b7de", "wer\u00b7den", "ihn", "ver\u00b7we\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach, so schwand aus ihrem Sinn", "tokens": ["Ach", ",", "so", "schwand", "aus", "ih\u00b7rem", "Sinn"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Auch mein Angedenken hin!", "tokens": ["Auch", "mein", "An\u00b7ge\u00b7den\u00b7ken", "hin", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}