{"dta.poem.4054": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "An ihren Genius .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905197104", "language": ["de:0.99"], "booktitle": "H\u00f6lderlin, Friedrich: Gedichte. Stuttgart u. a., 1826."}, "poem": {"stanza.1": {"line.1": {"text": "Send' ihr Blumen und Fr\u00fcchte aus nie versiegender", "tokens": ["Send'", "ihr", "Blu\u00b7men", "und", "Fr\u00fcch\u00b7te", "aus", "nie", "ver\u00b7sie\u00b7gen\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "PPOSAT", "NN", "KON", "NN", "APPR", "ADV", "ADJA"], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "F\u00fclle,", "tokens": ["F\u00fcl\u00b7le", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Send' ihr, freundlicher Geist, ewige Jugend", "tokens": ["Send'", "ihr", ",", "freund\u00b7li\u00b7cher", "Geist", ",", "e\u00b7wi\u00b7ge", "Ju\u00b7gend"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "ADJA", "NN", "$,", "ADJA", "NN"], "meter": "+-+--++--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "herab!", "tokens": ["her\u00b7ab", "!"], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "H\u00fcll' in deine Wonnen sie ein und la\u00df sie die", "tokens": ["H\u00fcll'", "in", "dei\u00b7ne", "Won\u00b7nen", "sie", "ein", "und", "la\u00df", "sie", "die"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "NN", "PPER", "PTKVZ", "KON", "VVIMP", "PPER", "ART"], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "Zeit nicht", "tokens": ["Zeit", "nicht"], "token_info": ["word", "word"], "pos": ["NN", "PTKNEG"], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Sehn, wo einsam und fremd sie, die Athenerin,", "tokens": ["Sehn", ",", "wo", "ein\u00b7sam", "und", "fremd", "sie", ",", "die", "A\u00b7the\u00b7ne\u00b7rin", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "ADJD", "KON", "ADJD", "PPER", "$,", "ART", "NN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.8": {"text": "lebt,", "tokens": ["lebt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.9": {"text": "Bis sie im Lande der Seligen einst die f\u00fcrstlichen", "tokens": ["Bis", "sie", "im", "Lan\u00b7de", "der", "Se\u00b7li\u00b7gen", "einst", "die", "f\u00fcrst\u00b7li\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN", "ART", "NN", "ADV", "ART", "ADJA"], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Schwestern,", "tokens": ["Schwes\u00b7tern", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.11": {"text": "Die zu Phidias Zeit herrschten und liebten, um-", "tokens": ["Die", "zu", "Phi\u00b7di\u00b7as", "Zeit", "herrschten", "und", "lieb\u00b7ten", ",", "um"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "APPR", "NE", "NN", "VVFIN", "KON", "VVFIN", "$,", "TRUNC"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.12": {"text": "f\u00e4ngt.", "tokens": ["f\u00e4ngt", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}}}}}