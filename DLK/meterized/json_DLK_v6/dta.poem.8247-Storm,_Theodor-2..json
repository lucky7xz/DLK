{"dta.poem.8247": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1852", "urn": "urn:nbn:de:kobv:b4-200905196098", "language": ["de:0.99"], "booktitle": "Storm, Theodor: Gedichte. Kiel, 1852."}, "poem": {"stanza.1": {"line.1": {"text": "Und die f\u00fcr immer scheiden gehn,", "tokens": ["Und", "die", "f\u00fcr", "im\u00b7mer", "schei\u00b7den", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPR", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die m\u00fcssen, w\u00e4r's auch zum Verderben,", "tokens": ["Die", "m\u00fcs\u00b7sen", ",", "w\u00e4r's", "auch", "zum", "Ver\u00b7der\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "$,", "VAFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Wahrheit ohne Hehl gestehn.", "tokens": ["Die", "Wahr\u00b7heit", "oh\u00b7ne", "Hehl", "ge\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "So leg' ich's denn in deine H\u00e4nde,", "tokens": ["So", "leg'", "ich's", "denn", "in", "dei\u00b7ne", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was immer mir das Herz bewegt;", "tokens": ["Was", "im\u00b7mer", "mir", "das", "Herz", "be\u00b7wegt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es ist die letzte Blumenspende,", "tokens": ["Es", "ist", "die", "letz\u00b7te", "Blu\u00b7men\u00b7spen\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auf ein geliebtes Grab gelegt.", "tokens": ["Auf", "ein", "ge\u00b7lieb\u00b7tes", "Grab", "ge\u00b7legt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}