{"textgrid.poem.26279": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Ach, daheim der Mondstrahl \u00fcberm Flieder", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gerne m\u00f6chte ich die H\u00e4nde falten", "tokens": ["Ger\u00b7ne", "m\u00f6ch\u00b7te", "ich", "die", "H\u00e4n\u00b7de", "fal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ART", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und die Wege gehen, die erinnerungsalten,", "tokens": ["Und", "die", "We\u00b7ge", "ge\u00b7hen", ",", "die", "e\u00b7rin\u00b7ne\u00b7rungs\u00b7al\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVINF", "$,", "PRELS", "VVFIN", "$,"], "meter": "--+-+--++--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "M\u00f6chte meine Heimatn\u00e4chte wiedersehen.", "tokens": ["M\u00f6ch\u00b7te", "mei\u00b7ne", "Hei\u00b7mat\u00b7n\u00e4ch\u00b7te", "wie\u00b7der\u00b7se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}, "stanza.2": {"line.1": {"text": "Ach, nun singt die Amsel bei der R\u00f6te", "tokens": ["Ach", ",", "nun", "singt", "die", "Am\u00b7sel", "bei", "der", "R\u00f6\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "ADV", "VVFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Schmaler Abendwolken im Holunder,", "tokens": ["Schma\u00b7ler", "Ab\u00b7end\u00b7wol\u00b7ken", "im", "Ho\u00b7lun\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Hier im Reisfeld gurgelt die Posaunenkr\u00f6te,", "tokens": ["Hier", "im", "Reis\u00b7feld", "gur\u00b7gelt", "die", "Po\u00b7sau\u00b7nen\u00b7kr\u00f6\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Und zum Himmel spreizt sich Palmenplunder.", "tokens": ["Und", "zum", "Him\u00b7mel", "spreizt", "sich", "Pal\u00b7men\u00b7plun\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "PRF", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Ach, daheim der Mondstrahl \u00fcberm Flieder!", "tokens": ["Ach", ",", "da\u00b7heim", "der", "Mond\u00b7strahl", "\u00fc\u00b7berm", "Flie\u00b7der", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "\u2013 H\u00f6rst du nicht der Gartent\u00fcre Klinke? \u2013", "tokens": ["\u2013", "H\u00f6rst", "du", "nicht", "der", "Gar\u00b7ten\u00b7t\u00fc\u00b7re", "Klin\u00b7ke", "?", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "NN", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Liebste, steig die hellen Stufen nieder,", "tokens": ["Liebs\u00b7te", ",", "steig", "die", "hel\u00b7len", "Stu\u00b7fen", "nie\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und ich steh' im Hohlweg unten, winke.", "tokens": ["Und", "ich", "steh'", "im", "Hohl\u00b7weg", "un\u00b7ten", ",", "win\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "NN", "ADV", "$,", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Und wir wandern um das kleine Haus,", "tokens": ["Und", "wir", "wan\u00b7dern", "um", "das", "klei\u00b7ne", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Sitzen unterm alten Apfelbaume.", "tokens": ["Sit\u00b7zen", "un\u00b7term", "al\u00b7ten", "Ap\u00b7fel\u00b7bau\u00b7me", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und der Nachtigall geht die Lust nicht aus,", "tokens": ["Und", "der", "Nach\u00b7ti\u00b7gall", "geht", "die", "Lust", "nicht", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN", "PTKNEG", "PTKVZ", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Und der Mond, er kr\u00f6nt uns in dem ewigen Raume.", "tokens": ["Und", "der", "Mond", ",", "er", "kr\u00f6nt", "uns", "in", "dem", "e\u00b7wi\u00b7gen", "Rau\u00b7me", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PPER", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "--+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}}}}