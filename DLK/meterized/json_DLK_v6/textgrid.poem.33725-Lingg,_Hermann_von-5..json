{"textgrid.poem.33725": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du r\u00fchmst den Schlaf, weil jeder Schmerz versiege,", "tokens": ["Du", "r\u00fchmst", "den", "Schlaf", ",", "weil", "je\u00b7der", "Schmerz", "ver\u00b7sie\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "KOUS", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von seinem Hauch in s\u00fc\u00dfen Traum gesungen,", "tokens": ["Von", "sei\u00b7nem", "Hauch", "in", "s\u00fc\u00b7\u00dfen", "Traum", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Weil ausgel\u00f6scht in seinen D\u00e4mmerungen", "tokens": ["Weil", "aus\u00b7ge\u00b7l\u00f6scht", "in", "sei\u00b7nen", "D\u00e4m\u00b7me\u00b7run\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des Tages Qual wie Glut im Duft verfliege?", "tokens": ["Des", "Ta\u00b7ges", "Qual", "wie", "Glut", "im", "Duft", "ver\u00b7flie\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KOKOM", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und bangst du nicht, auf jener dunklen Stiege", "tokens": ["Und", "bangst", "du", "nicht", ",", "auf", "je\u00b7ner", "dunk\u00b7len", "Stie\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "$,", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hinabzugehn ins L\u00fcgenreich, bezwungen", "tokens": ["Hin\u00b7ab\u00b7zu\u00b7gehn", "ins", "L\u00fc\u00b7gen\u00b7reich", ",", "be\u00b7zwun\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ADV", "APPRART", "NN", "$,", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und wehrlos hinzusinken, wahnumschlungen,", "tokens": ["Und", "wehr\u00b7los", "hin\u00b7zu\u00b7sin\u00b7ken", ",", "wa\u00b7hnum\u00b7schlun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADJD", "VVINF", "$,", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Beraubt um deiner Freiheit k\u00fchnste Siege?", "tokens": ["Be\u00b7raubt", "um", "dei\u00b7ner", "Frei\u00b7heit", "k\u00fchns\u00b7te", "Sie\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "O la\u00df im Schlaf sein Weh den Feigen t\u00f6ten,", "tokens": ["O", "la\u00df", "im", "Schlaf", "sein", "Weh", "den", "Fei\u00b7gen", "t\u00f6\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPRART", "NN", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "La\u00df Blumen selig tr\u00e4umen, la\u00df der Kr\u00f6ten", "tokens": ["La\u00df", "Blu\u00b7men", "se\u00b7lig", "tr\u00e4u\u00b7men", ",", "la\u00df", "der", "Kr\u00f6\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "NN", "ADJD", "VVINF", "$,", "VVIMP", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Geschlecht den Winterschlaf im Felsen r\u00fchmen!", "tokens": ["Ge\u00b7schlecht", "den", "Win\u00b7ter\u00b7schlaf", "im", "Fel\u00b7sen", "r\u00fch\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch uns soll nichts des Lebens Schmerz verbl\u00fcmen,", "tokens": ["Doch", "uns", "soll", "nichts", "des", "Le\u00b7bens", "Schmerz", "ver\u00b7bl\u00fc\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PIS", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nur ihm sei Dank mit jeder Morgenr\u00f6te,", "tokens": ["Nur", "ihm", "sei", "Dank", "mit", "je\u00b7der", "Mor\u00b7gen\u00b7r\u00f6\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der uns vom Staub zum Menschengeist erh\u00f6hte.", "tokens": ["Der", "uns", "vom", "Staub", "zum", "Men\u00b7schen\u00b7geist", "er\u00b7h\u00f6h\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}