{"textgrid.poem.42801": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Hilflose Tiere", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn ein Hund kotzt, soll man keinen Augenblick", "tokens": ["Wenn", "ein", "Hund", "kotzt", ",", "soll", "man", "kei\u00b7nen", "Au\u00b7gen\u00b7blick"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$,", "VMFIN", "PIS", "PIAT", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Ihn dann st\u00f6ren,", "tokens": ["Ihn", "dann", "st\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Soll man auf ihn h\u00f6ren.", "tokens": ["Soll", "man", "auf", "ihn", "h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "APPR", "PPER", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "T\u00f6ne sind Bruchst\u00fccke von Musik.", "tokens": ["T\u00f6\u00b7ne", "sind", "Bruchs\u00b7t\u00fc\u00b7cke", "von", "Mu\u00b7sik", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Ob ger\u00e4uschvoll oder leise,", "tokens": ["Ob", "ge\u00b7r\u00e4uschvoll", "o\u00b7der", "lei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "KON", "ADJD", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Massig oder klein bei klein \u2013", "tokens": ["Mas\u00b7sig", "o\u00b7der", "klein", "bei", "klein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "APPR", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kann es doch die sch\u00f6nste Speise,", "tokens": ["Kann", "es", "doch", "die", "sch\u00f6ns\u00b7te", "Spei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kann es beispielsweise", "tokens": ["Kann", "es", "bei\u00b7spiels\u00b7wei\u00b7se"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Hammelkeule in Madeira sein.", "tokens": ["Ham\u00b7mel\u00b7keu\u00b7le", "in", "Ma\u00b7dei\u00b7ra", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "VAINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Auch das Dichten ist ein Vonsichgeben.", "tokens": ["Auch", "das", "Dich\u00b7ten", "ist", "ein", "Von\u00b7sich\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Eisen bricht. Und alles geht vorbei,", "tokens": ["Ei\u00b7sen", "bricht", ".", "Und", "al\u00b7les", "geht", "vor\u00b7bei", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$.", "KON", "PIS", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Auch die Wolke und das Leben.", "tokens": ["Auch", "die", "Wol\u00b7ke", "und", "das", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ein einz'ger Koch verdirbt den ganzen Brei.", "tokens": ["Und", "ein", "einz'\u00b7ger", "Koch", "ver\u00b7dirbt", "den", "gan\u00b7zen", "Brei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "--+-+-+-+-+", "measure": "anapaest.init"}}, "stanza.4": {"line.1": {"text": "Mag sich also keiner \u00fcberheben,", "tokens": ["Mag", "sich", "al\u00b7so", "kei\u00b7ner", "\u00fc\u00b7ber\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "ADV", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Der auf Menschtum und Gesundheit protzt.", "tokens": ["Der", "auf", "Menschtum", "und", "Ge\u00b7sund\u00b7heit", "protzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NE", "KON", "NN", "VVPP", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.5": {"line.1": {"text": "Wenn ein H\u00fcndchen kotzt \u2013", "tokens": ["Wenn", "ein", "H\u00fcnd\u00b7chen", "kotzt", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "\u00d6ffentlich genau so wie zu Hause \u2013", "tokens": ["\u00d6f\u00b7fent\u00b7lich", "ge\u00b7nau", "so", "wie", "zu", "Hau\u00b7se", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "KOKOM", "APPR", "NN", "$("], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Sollst du mit ihm leiden,", "tokens": ["Sollst", "du", "mit", "ihm", "lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Maulkorb ihm durchschneiden;", "tokens": ["Maul\u00b7korb", "ihm", "durch\u00b7schnei\u00b7den", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVINF", "$."], "meter": "++--+-", "measure": "trochaic.tri.relaxed"}, "line.5": {"text": "Denn sonst wirkt der Korb wie eine Brause.", "tokens": ["Denn", "sonst", "wirkt", "der", "Korb", "wie", "ei\u00b7ne", "Brau\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "KOKOM", "ART", "NN", "$."], "meter": "-++-+-+-+-", "measure": "unknown.measure.penta"}}, "stanza.6": {"line.1": {"text": "Will das R\u00fchrende dir h\u00e4\u00dflich scheinen,", "tokens": ["Will", "das", "R\u00fch\u00b7ren\u00b7de", "dir", "h\u00e4\u00df\u00b7lich", "schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Denke: Gro\u00dfes spiegelt sich im Kleinen.", "tokens": ["Den\u00b7ke", ":", "Gro\u00b7\u00dfes", "spie\u00b7gelt", "sich", "im", "Klei\u00b7nen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$.", "NN", "VVFIN", "PRF", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Wirst dich doch der eignen \u00dcbelkeit", "tokens": ["Wirst", "dich", "doch", "der", "eig\u00b7nen", "\u00dc\u00b7bel\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Niemals sch\u00e4men.", "tokens": ["Nie\u00b7mals", "sch\u00e4\u00b7men", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "G\u00f6nne Tieren wenigstens die Zeit,", "tokens": ["G\u00f6n\u00b7ne", "Tie\u00b7ren", "we\u00b7nigs\u00b7tens", "die", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Widerw\u00e4rtiges zur\u00fcckzunehmen.", "tokens": ["Wi\u00b7der\u00b7w\u00e4r\u00b7ti\u00b7ges", "zu\u00b7r\u00fcck\u00b7zu\u00b7neh\u00b7men", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Oder la\u00df das ruhig liegen. Weil", "tokens": ["O\u00b7der", "la\u00df", "das", "ru\u00b7hig", "lie\u00b7gen", ".", "Weil"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "VVIMP", "ART", "ADJD", "VVINF", "$.", "KOUS"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Roheit niemals Gl\u00fcck bringt oder Segen.", "tokens": ["Ro\u00b7heit", "nie\u00b7mals", "Gl\u00fcck", "bringt", "o\u00b7der", "Se\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "NN", "VVFIN", "KON", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Jeder soll vor seiner T\u00fcre fegen.", "tokens": ["Je\u00b7der", "soll", "vor", "sei\u00b7ner", "T\u00fc\u00b7re", "fe\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und die Stiefelsohle ist kein K\u00f6rperteil.", "tokens": ["Und", "die", "Stie\u00b7fel\u00b7soh\u00b7le", "ist", "kein", "K\u00f6r\u00b7per\u00b7teil", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}}}}