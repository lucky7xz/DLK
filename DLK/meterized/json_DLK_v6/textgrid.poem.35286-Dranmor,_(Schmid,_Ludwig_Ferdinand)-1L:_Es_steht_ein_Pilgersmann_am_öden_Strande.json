{"textgrid.poem.35286": {"metadata": {"author": {"name": "Dranmor, (Schmid, Ludwig Ferdinand)", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es steht ein Pilgersmann am \u00f6den Strande", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es steht ein Pilgersmann am \u00f6den Strande", "tokens": ["Es", "steht", "ein", "Pil\u00b7gers\u00b7mann", "am", "\u00f6\u00b7den", "Stran\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und blickt sehns\u00fcchtig \u00fcbers weite Meer;", "tokens": ["Und", "blickt", "sehn\u00b7s\u00fcch\u00b7tig", "\u00fc\u00b7bers", "wei\u00b7te", "Meer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Tr\u00e4umt auch sein Herz vom fernen Vaterlande,", "tokens": ["Tr\u00e4umt", "auch", "sein", "Herz", "vom", "fer\u00b7nen", "Va\u00b7ter\u00b7lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sein Herz ist hoffnungsleer.", "tokens": ["Sein", "Herz", "ist", "hoff\u00b7nungs\u00b7leer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er hat geliebt \u2013 wie konnt' es anders sein?", "tokens": ["Er", "hat", "ge\u00b7liebt", "\u2013", "wie", "konnt'", "es", "an\u00b7ders", "sein", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$(", "PWAV", "VMFIN", "PPER", "ADV", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Er hat geglaubt \u2013 will einer ihn verdammen?", "tokens": ["Er", "hat", "ge\u00b7glaubt", "\u2013", "will", "ei\u00b7ner", "ihn", "ver\u00b7dam\u00b7men", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$(", "VMFIN", "PIS", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Er hat verzagt \u2013 der Himmel stand in Flammen,", "tokens": ["Er", "hat", "ver\u00b7zagt", "\u2013", "der", "Him\u00b7mel", "stand", "in", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$(", "ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Er ist entflohn \u2013 er lebt und stirbt allein.", "tokens": ["Er", "ist", "ent\u00b7flohn", "\u2013", "er", "lebt", "und", "stirbt", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$(", "PPER", "VVFIN", "KON", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Helena! Wie die Wolken dort zerflie\u00dfen,", "tokens": ["He\u00b7le\u00b7na", "!", "Wie", "die", "Wol\u00b7ken", "dort", "zer\u00b7flie\u00b7\u00dfen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PWAV", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "So starb der Hoffnung letzter Wahn dahin;", "tokens": ["So", "starb", "der", "Hoff\u00b7nung", "letz\u00b7ter", "Wahn", "da\u00b7hin", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJA", "NN", "PAV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Willst du mich noch in deine Arme schlie\u00dfen,", "tokens": ["Willst", "du", "mich", "noch", "in", "dei\u00b7ne", "Ar\u00b7me", "schlie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gealtert wie ich bin?", "tokens": ["Ge\u00b7al\u00b7tert", "wie", "ich", "bin", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "KOKOM", "PPER", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "O, f\u00fcr die Qual, die ich geduldig trug,", "tokens": ["O", ",", "f\u00fcr", "die", "Qual", ",", "die", "ich", "ge\u00b7dul\u00b7dig", "trug", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Soll ich dir jetzt ein bl\u00f6des L\u00e4cheln zeigen?", "tokens": ["Soll", "ich", "dir", "jetzt", "ein", "bl\u00f6\u00b7des", "L\u00e4\u00b7cheln", "zei\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ein einz'ges Wort nach jahrelangem Schweigen,", "tokens": ["Ein", "einz'\u00b7ges", "Wort", "nach", "jah\u00b7re\u00b7lan\u00b7gem", "Schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ein einz'ger Gru\u00df \u2013 es ist genug, genug.", "tokens": ["Ein", "einz'\u00b7ger", "Gru\u00df", "\u2013", "es", "ist", "ge\u00b7nug", ",", "ge\u00b7nug", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "PPER", "VAFIN", "ADV", "$,", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich frage nicht, ob du mir treu geblieben;", "tokens": ["Ich", "fra\u00b7ge", "nicht", ",", "ob", "du", "mir", "treu", "ge\u00b7blie\u00b7ben", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich kann wohl zweifeln, doch ich z\u00fcrne nicht;", "tokens": ["Ich", "kann", "wohl", "zwei\u00b7feln", ",", "doch", "ich", "z\u00fcr\u00b7ne", "nicht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "VVINF", "$,", "KON", "PPER", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Denn bist du elend, werd' ich ewig lieben", "tokens": ["Denn", "bist", "du", "e\u00b7lend", ",", "werd'", "ich", "e\u00b7wig", "lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "$,", "VAFIN", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dein trauernd Angesicht;", "tokens": ["Dein", "trau\u00b7ernd", "An\u00b7ge\u00b7sicht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und bist du gl\u00fccklich \u2013 darf ich freudig nur", "tokens": ["Und", "bist", "du", "gl\u00fcck\u00b7lich", "\u2013", "darf", "ich", "freu\u00b7dig", "nur"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "$(", "VMFIN", "PPER", "ADJD", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "In diese W\u00e4lder mein Geheimnis bannen.", "tokens": ["In", "die\u00b7se", "W\u00e4l\u00b7der", "mein", "Ge\u00b7heim\u00b7nis", "ban\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Du aber schlafe unter gr\u00fcnen Tannen,", "tokens": ["Du", "a\u00b7ber", "schla\u00b7fe", "un\u00b7ter", "gr\u00fc\u00b7nen", "Tan\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Huldvoll verzeihend den gebrochnen Schwur.", "tokens": ["Huld\u00b7voll", "ver\u00b7zei\u00b7hend", "den", "ge\u00b7broch\u00b7nen", "Schwur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.4": {"line.1": {"text": "Tochter der Sterne! Holde, totenbleiche,", "tokens": ["Toch\u00b7ter", "der", "Ster\u00b7ne", "!", "Hol\u00b7de", ",", "to\u00b7ten\u00b7blei\u00b7che", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "ART", "NN", "$.", "NN", "$,", "ADJA", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Verg\u00f6nne mir ein einz'ges, letztes Wort:", "tokens": ["Ver\u00b7g\u00f6n\u00b7ne", "mir", "ein", "einz'\u00b7ges", ",", "letz\u00b7tes", "Wort", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "F\u00fcr unser kurzes Gl\u00fcck, das schmerzenreiche,", "tokens": ["F\u00fcr", "un\u00b7ser", "kur\u00b7zes", "Gl\u00fcck", ",", "das", "schmer\u00b7zen\u00b7rei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gedenke meiner dort!", "tokens": ["Ge\u00b7den\u00b7ke", "mei\u00b7ner", "dort", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wer wei\u00df, ob wir uns jemals wiedersehn?", "tokens": ["Wer", "wei\u00df", ",", "ob", "wir", "uns", "je\u00b7mals", "wie\u00b7der\u00b7sehn", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "KOUS", "PPER", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ich will mich nicht an Engelsthr\u00e4nen laben;", "tokens": ["Ich", "will", "mich", "nicht", "an", "En\u00b7gelst\u00b7hr\u00e4\u00b7nen", "la\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "In diesen Bl\u00e4ttern ist mein Herz begraben;", "tokens": ["In", "die\u00b7sen", "Bl\u00e4t\u00b7tern", "ist", "mein", "Herz", "be\u00b7gra\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Helena! Du allein wirst mich verstehn.", "tokens": ["He\u00b7le\u00b7na", "!", "Du", "al\u00b7lein", "wirst", "mich", "ver\u00b7stehn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPER", "ADV", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}