{"textgrid.poem.63413": {"metadata": {"author": {"name": "Brinckman, John", "birth": "N.A.", "death": "N.A."}, "title": "Snider", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nee, Jung, du wagst din L\u00e4ben recht,", "tokens": ["Nee", ",", "Jung", ",", "du", "wagst", "din", "L\u00e4\u00b7ben", "recht", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "dat up sonn Been du geihst!", "tokens": ["dat", "up", "sonn", "Be\u00b7en", "du", "geihst", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.nl", "FM.nl", "FM.nl", "VVFIN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "De s\u00fcnd jo as'n W\u00e4werknecht", "tokens": ["De", "s\u00fcnd", "jo", "as'n", "W\u00e4\u00b7wer\u00b7knecht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NE", "NE", "NE", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "sin, dat's dat allermeist.", "tokens": ["sin", ",", "dat's", "dat", "al\u00b7ler\u00b7meist", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "FM.la", "FM.la", "FM.la", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Un, Jung, wenn du de Hand mi giffst,", "tokens": ["Un", ",", "Jung", ",", "wenn", "du", "de", "Hand", "mi", "giffst", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "KOUS", "PPER", "NE", "NN", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "denn heww ick grote Not,", "tokens": ["denn", "heww", "ick", "gro\u00b7te", "Not", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "wat du dor mal an hacken bliffst,", "tokens": ["wat", "du", "dor", "mal", "an", "ha\u00b7cken", "bliffst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "as Metten an min'n Hot.", "tokens": ["as", "Met\u00b7ten", "an", "min'n", "Hot", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Nee, Jung, du m\u00f6st inne Sniderlihr,", "tokens": ["Nee", ",", "Jung", ",", "du", "m\u00f6st", "in\u00b7ne", "Sni\u00b7der\u00b7lihr", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "PPER", "VMFIN", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "du b\u00fcst as'n Lappen Pl\u00fcnn;", "tokens": ["du", "b\u00fcst", "as'n", "Lap\u00b7pen", "Pl\u00fcnn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "NE", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "du kannst to Nod d\u00f6rch't Nadel\u00fchr", "tokens": ["du", "kannst", "to", "Nod", "d\u00f6rch't", "Na\u00b7de\u00b7l\u00fchr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "FM", "FM", "FM", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "s\u00fclst d\u00f6rch, du b\u00fcst so d\u00fcnn.", "tokens": ["s\u00fclst", "d\u00f6rch", ",", "du", "b\u00fcst", "so", "d\u00fcnn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "$,", "PPER", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Un wenn du'n goden Snider warst,", "tokens": ["Un", "wenn", "du'n", "go\u00b7den", "Sni\u00b7der", "warst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "ART", "ADJA", "NN", "VAFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "m\u00f6st w\u00e4gen s\u0153ben Pund.", "tokens": ["m\u00f6st", "w\u00e4\u00b7gen", "s\u0153\u00b7ben", "Pund", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Un wenn du dat nich w\u00e4gen deist \u2013", "tokens": ["Un", "wenn", "du", "dat", "nich", "w\u00e4\u00b7gen", "de\u00b7ist", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "PPER", "FM.nl", "FM.nl", "FM.nl", "FM.nl", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "denn b\u00fcst nich mihr gesund!", "tokens": ["denn", "b\u00fcst", "nich", "mihr", "ge\u00b7sund", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}