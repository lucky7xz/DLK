{"textgrid.poem.49783": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1. August 1914", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es wurde still.", "tokens": ["Es", "wur\u00b7de", "still", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Ein ganzes Volk, es hielt mit einem", "tokens": ["Ein", "gan\u00b7zes", "Volk", ",", "es", "hielt", "mit", "ei\u00b7nem"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PPER", "VVFIN", "APPR", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Den Atem an. Doch stockte keinem", "tokens": ["Den", "A\u00b7tem", "an", ".", "Doch", "stock\u00b7te", "kei\u00b7nem"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "$.", "KON", "VVFIN", "PIAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Darum des Herzens Schlag.", "tokens": ["Da\u00b7rum", "des", "Her\u00b7zens", "Schlag", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "So ging der Tag.", "tokens": ["So", "ging", "der", "Tag", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Dann senkt sich feierlich und milde", "tokens": ["Dann", "senkt", "sich", "fei\u00b7er\u00b7lich", "und", "mil\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ADJD", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Der Abend \u00fcber die Gefilde,", "tokens": ["Der", "A\u00b7bend", "\u00fc\u00b7ber", "die", "Ge\u00b7fil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und heiter blinkt und fern", "tokens": ["Und", "hei\u00b7ter", "blinkt", "und", "fern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "KON", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Ein heller Stern,", "tokens": ["Ein", "hel\u00b7ler", "Stern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Als wenn er's heut wie immer f\u00e4nde:", "tokens": ["Als", "wenn", "er's", "heut", "wie", "im\u00b7mer", "f\u00e4n\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PIS", "ADV", "KOKOM", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "In allen H\u00fctten m\u00fcde H\u00e4nde", "tokens": ["In", "al\u00b7len", "H\u00fct\u00b7ten", "m\u00fc\u00b7de", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Und gute Rast", "tokens": ["Und", "gu\u00b7te", "Rast"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.13": {"text": "Nach hei\u00dfer Arbeit Last.", "tokens": ["Nach", "hei\u00b7\u00dfer", "Ar\u00b7beit", "Last", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Horcht!", "tokens": ["Horcht", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.15": {"text": "War's nicht, als h\u00e4tt' ein Ruf geklungen,", "tokens": ["Wa\u00b7r's", "nicht", ",", "als", "h\u00e4tt'", "ein", "Ruf", "ge\u00b7klun\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "$,", "KOKOM", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.16": {"text": "Ein Ton, als wie aus Erz gedrungen?", "tokens": ["Ein", "Ton", ",", "als", "wie", "aus", "Erz", "ge\u00b7drun\u00b7gen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "KOKOM", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Da, \u2013 wieder! Auf!", "tokens": ["Da", ",", "\u2013", "wie\u00b7der", "!", "Auf", "!"], "token_info": ["word", "punct", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "$,", "$(", "ADV", "$.", "APPR", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.18": {"text": "Auf zu den Waffen! Auf!", "tokens": ["Auf", "zu", "den", "Waf\u00b7fen", "!", "Auf", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "$.", "APPR", "$."], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.19": {"text": "Nun geht es brausend durch die W\u00e4lder,", "tokens": ["Nun", "geht", "es", "brau\u00b7send", "durch", "die", "W\u00e4l\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Nun dr\u00f6hnt es \u00fcber stille Felder:", "tokens": ["Nun", "dr\u00f6hnt", "es", "\u00fc\u00b7ber", "stil\u00b7le", "Fel\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Die Wehr zur Hand!", "tokens": ["Die", "Wehr", "zur", "Hand", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.22": {"text": "Und sch\u00fctzt das Vaterland!", "tokens": ["Und", "sch\u00fctzt", "das", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.23": {"text": "Auf springt das Volk, es reckt die Glieder,", "tokens": ["Auf", "springt", "das", "Volk", ",", "es", "reckt", "die", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Und keine Sorge dr\u00fcckt uns nieder.", "tokens": ["Und", "kei\u00b7ne", "Sor\u00b7ge", "dr\u00fcckt", "uns", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Komm, was es sei!", "tokens": ["Komm", ",", "was", "es", "sei", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWS", "PPER", "VAFIN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.26": {"text": "Von Ungewi\u00dfheit frei", "tokens": ["Von", "Un\u00b7ge\u00b7wi\u00df\u00b7heit", "frei"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.27": {"text": "Wir wollen es gemeinsam tragen", "tokens": ["Wir", "wol\u00b7len", "es", "ge\u00b7mein\u00b7sam", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Und heute schon als Bestes sagen,", "tokens": ["Und", "heu\u00b7te", "schon", "als", "Bes\u00b7tes", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "KOUS", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Da\u00df man uns Hand in Hand", "tokens": ["Da\u00df", "man", "uns", "Hand", "in", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "PPER", "NN", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.30": {"text": "Als Br\u00fcder fand.", "tokens": ["Als", "Br\u00fc\u00b7der", "fand", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.31": {"text": "Dem Kaiser, der dies Wort gegeben,", "tokens": ["Dem", "Kai\u00b7ser", ",", "der", "dies", "Wort", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PDS", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Wird Dank in jedem Herzen leben.", "tokens": ["Wird", "Dank", "in", "je\u00b7dem", "Her\u00b7zen", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Und jetzt, \u2013 hurra!", "tokens": ["Und", "jetzt", ",", "\u2013", "hur\u00b7ra", "!"], "token_info": ["word", "word", "punct", "punct", "word", "punct"], "pos": ["KON", "ADV", "$,", "$(", "ITJ", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.34": {"text": "Du Mutter uns, \u2013 Germania!", "tokens": ["Du", "Mut\u00b7ter", "uns", ",", "\u2013", "Ger\u00b7ma\u00b7nia", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["PPER", "NN", "PPER", "$,", "$(", "NE", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}