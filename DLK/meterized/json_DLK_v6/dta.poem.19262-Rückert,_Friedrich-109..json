{"dta.poem.19262": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "109.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Licht ist auch ohne W\u00e4rm', und W\u00e4rm' auch ohne Licht,", "tokens": ["Licht", "ist", "auch", "oh\u00b7ne", "W\u00e4rm'", ",", "und", "W\u00e4rm'", "auch", "oh\u00b7ne", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "APPR", "NN", "$,", "KON", "NN", "ADV", "APPR", "NN", "$,"], "meter": "++-+-+-+-+-+", "measure": "unknown.measure.septa"}, "line.2": {"text": "Doch ohne Licht zugleich und W\u00e4rm' ist Feuer nicht;", "tokens": ["Doch", "oh\u00b7ne", "Licht", "zu\u00b7gleich", "und", "W\u00e4rm'", "ist", "Feu\u00b7er", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ADV", "KON", "NN", "VAFIN", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "(gemaltes abgez\u00e4hlt) drum wird das Feuer seyn,", "tokens": ["(", "ge\u00b7mal\u00b7tes", "ab\u00b7ge\u00b7z\u00e4hlt", ")", "drum", "wird", "das", "Feu\u00b7er", "seyn", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJA", "VVPP", "$(", "PAV", "VAFIN", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was Licht und W\u00e4rm' auch sei, von beiden der Verein:", "tokens": ["Was", "Licht", "und", "W\u00e4rm'", "auch", "sei", ",", "von", "bei\u00b7den", "der", "Ver\u00b7ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "KON", "NN", "ADV", "VAFIN", "$,", "APPR", "PIAT", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ein Geist, in dem verm\u00e4hlt Verstand ist und Gef\u00fchl,", "tokens": ["Ein", "Geist", ",", "in", "dem", "ver\u00b7m\u00e4hlt", "Ver\u00b7stand", "ist", "und", "Ge\u00b7f\u00fchl", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ART", "VVFIN", "NN", "VAFIN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des Innigkeit nicht dumpf, und Klarheit nicht ist k\u00fchl.", "tokens": ["Des", "In\u00b7nig\u00b7keit", "nicht", "dumpf", ",", "und", "Klar\u00b7heit", "nicht", "ist", "k\u00fchl", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "ADJD", "$,", "KON", "NN", "PTKNEG", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}