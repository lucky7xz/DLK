{"textgrid.poem.66977": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Einem Aszeten", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "R\u00fchme nur dich der Entsagung! Darf ich ganz vertraulich lachen?", "tokens": ["R\u00fch\u00b7me", "nur", "dich", "der", "Ent\u00b7sa\u00b7gung", "!", "Darf", "ich", "ganz", "ver\u00b7trau\u00b7lich", "la\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPER", "ART", "NN", "$.", "VMFIN", "PPER", "ADV", "ADJD", "VVINF", "$."], "meter": "+--+--+-+-+-+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Ist es doch so toren-menschlich, aus der Not die Tugend machen ...", "tokens": ["Ist", "es", "doch", "so", "to\u00b7ren\u00b7menschlich", ",", "aus", "der", "Not", "die", "Tu\u00b7gend", "ma\u00b7chen", "..."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ADJD", "$,", "APPR", "ART", "NN", "ART", "NN", "VVINF", "$("], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Mach sie wenigstens zum Engel, aber, bitte, nicht zum Drachen!", "tokens": ["Mach", "sie", "we\u00b7nigs\u00b7tens", "zum", "En\u00b7gel", ",", "a\u00b7ber", ",", "bit\u00b7te", ",", "nicht", "zum", "Dra\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "NN", "$,", "ADV", "$,", "PTKANT", "$,", "PTKNEG", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}}}}