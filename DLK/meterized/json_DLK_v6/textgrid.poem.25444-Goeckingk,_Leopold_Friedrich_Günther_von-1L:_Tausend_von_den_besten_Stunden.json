{"textgrid.poem.25444": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Tausend von den besten Stunden", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tausend von den besten Stunden", "tokens": ["Tau\u00b7send", "von", "den", "bes\u00b7ten", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["CARD", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dieses Jahres, dank' ich dir!", "tokens": ["Die\u00b7ses", "Jah\u00b7res", ",", "dank'", "ich", "dir", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "VVFIN", "PPER", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Um mein Leben hast du mir", "tokens": ["Um", "mein", "Le\u00b7ben", "hast", "du", "mir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "PPOSAT", "NN", "VAFIN", "PPER", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Einen Myrtenkranz gewunden,", "tokens": ["Ei\u00b7nen", "Myr\u00b7ten\u00b7kranz", "ge\u00b7wun\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VAPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Seit in dieser W\u00fcste hier,", "tokens": ["Seit", "in", "die\u00b7ser", "W\u00fcs\u00b7te", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "PDAT", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ich das h\u00f6chste Gl\u00fcck, in dir", "tokens": ["Ich", "das", "h\u00f6chs\u00b7te", "Gl\u00fcck", ",", "in", "dir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ART", "ADJA", "NN", "$,", "APPR", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Einen weisen Freund gefunden;", "tokens": ["Ei\u00b7nen", "wei\u00b7sen", "Freund", "ge\u00b7fun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Seit nicht mehr die kranke Ruhmbegier", "tokens": ["Seit", "nicht", "mehr", "die", "kran\u00b7ke", "Ruhm\u00b7be\u00b7gier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PTKNEG", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "Ueber Staub und Motten wacht,", "tokens": ["Ue\u00b7ber", "Staub", "und", "Mot\u00b7ten", "wacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Seit ich kl\u00fcger, manche halbe Nacht,", "tokens": ["Seit", "ich", "kl\u00fc\u00b7ger", ",", "man\u00b7che", "hal\u00b7be", "Nacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "$,", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.11": {"text": "Mit Sophien und mit dir,", "tokens": ["Mit", "So\u00b7phi\u00b7en", "und", "mit", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Weggescherzt und weggelacht.", "tokens": ["Weg\u00b7ge\u00b7scherzt", "und", "weg\u00b7ge\u00b7lacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Niemals sah ich in den letzten Stunden", "tokens": ["Nie\u00b7mals", "sah", "ich", "in", "den", "letz\u00b7ten", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.14": {"text": "Eines Jahres, mit so heiterm Blick'", "tokens": ["Ei\u00b7nes", "Jah\u00b7res", ",", "mit", "so", "hei\u00b7term", "Blick'"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "APPR", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.15": {"text": "In ein Jahr, das bald verschwunden,", "tokens": ["In", "ein", "Jahr", ",", "das", "bald", "ver\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Aber nicht verloren ist, zur\u00fcck.", "tokens": ["A\u00b7ber", "nicht", "ver\u00b7lo\u00b7ren", "ist", ",", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PTKNEG", "VVPP", "VAFIN", "$,", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.17": {"text": "Zwar verweilet sich am Grabe", "tokens": ["Zwar", "ver\u00b7wei\u00b7let", "sich", "am", "Gra\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Meines ", "tokens": ["Mei\u00b7nes"], "token_info": ["word"], "pos": ["PPOSAT"], "meter": "+-", "measure": "trochaic.single"}, "line.19": {"text": "Weh'! da\u00df ich ihn nicht mehr habe!", "tokens": ["Weh'", "!", "da\u00df", "ich", "ihn", "nicht", "mehr", "ha\u00b7be", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "KOUS", "PPER", "PPER", "PTKNEG", "ADV", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Wohl! da\u00df ich ihn wieder finden kann!", "tokens": ["Wohl", "!", "da\u00df", "ich", "ihn", "wie\u00b7der", "fin\u00b7den", "kann", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "KOUS", "PPER", "PPER", "ADV", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.21": {"text": "Und ich werd' ihn wieder finden,", "tokens": ["Und", "ich", "werd'", "ihn", "wie\u00b7der", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Wenn ich meine Spanne Raum", "tokens": ["Wenn", "ich", "mei\u00b7ne", "Span\u00b7ne", "Raum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.23": {"text": "Durchgekrochen bin, und dieser Traum", "tokens": ["Durch\u00b7ge\u00b7kro\u00b7chen", "bin", ",", "und", "die\u00b7ser", "Traum"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VAFIN", "$,", "KON", "PDAT", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.24": {"text": "In dem Arm' der Engel wird verschwinden.", "tokens": ["In", "dem", "Arm'", "der", "En\u00b7gel", "wird", "ver\u00b7schwin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VAFIN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.25": {"text": "Noch, da ich im Traume bin,", "tokens": ["Noch", ",", "da", "ich", "im", "Trau\u00b7me", "bin", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "APPRART", "NN", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.26": {"text": "Freut mich's, wenn er mich entz\u00fcckt;", "tokens": ["Freut", "mich's", ",", "wenn", "er", "mich", "ent\u00b7z\u00fcckt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "KOUS", "PPER", "PRF", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.27": {"text": "Weislich seh' ich selbst dahin,", "tokens": ["Weis\u00b7lich", "seh'", "ich", "selbst", "da\u00b7hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ADV", "PAV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.28": {"text": "Da\u00df der schwere Alp der Sorgen", "tokens": ["Da\u00df", "der", "schwe\u00b7re", "Alp", "der", "Sor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "Mein zufriednes Herz nicht dr\u00fcckt,", "tokens": ["Mein", "zu\u00b7fried\u00b7nes", "Herz", "nicht", "dr\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.30": {"text": "Und mein Auge, jeden Morgen,", "tokens": ["Und", "mein", "Au\u00b7ge", ",", "je\u00b7den", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.31": {"text": "Heiter nach dem Himmel blickt.", "tokens": ["Hei\u00b7ter", "nach", "dem", "Him\u00b7mel", "blickt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.32": {"text": "Ob die Gro\u00dfen in ", "tokens": ["Ob", "die", "Gro\u00b7\u00dfen", "in"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.33": {"text": "Von mir h\u00f6ren? an mich denken?", "tokens": ["Von", "mir", "h\u00f6\u00b7ren", "?", "an", "mich", "den\u00b7ken", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVINF", "$.", "APPR", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Sollte das mich heimlich kr\u00e4nken,", "tokens": ["Soll\u00b7te", "das", "mich", "heim\u00b7lich", "kr\u00e4n\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PDS", "PRF", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.35": {"text": "Und die Stirn in Falten ziehn?", "tokens": ["Und", "die", "Stirn", "in", "Fal\u00b7ten", "ziehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "NN", "VVINF", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.36": {"text": "Zu versorgen hab' ich keinen,", "tokens": ["Zu", "ver\u00b7sor\u00b7gen", "hab'", "ich", "kei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VAFIN", "PPER", "PIAT", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.37": {"text": "Als nur mich, nur mich allein;", "tokens": ["Als", "nur", "mich", ",", "nur", "mich", "al\u00b7lein", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "$,", "ADV", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.38": {"text": "Und du weist ja, diesem Einen", "tokens": ["Und", "du", "weist", "ja", ",", "die\u00b7sem", "Ei\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV", "$,", "PDAT", "ART"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.39": {"text": "Ist sein H\u00e4uschen nie zu klein,", "tokens": ["Ist", "sein", "H\u00e4usc\u00b7hen", "nie", "zu", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "PTKA", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.40": {"text": "Nie sein Tuch zu grob gewebet,", "tokens": ["Nie", "sein", "Tuch", "zu", "grob", "ge\u00b7we\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "PTKA", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.41": {"text": "Nie zu leicht sein Frankenwein;", "tokens": ["Nie", "zu", "leicht", "sein", "Fran\u00b7ken\u00b7wein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKA", "ADJD", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.42": {"text": "Gut genug mag alles seyn,", "tokens": ["Gut", "ge\u00b7nug", "mag", "al\u00b7les", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "VMFIN", "PIS", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.43": {"text": "Wenn man ohne Sorgen lebet.", "tokens": ["Wenn", "man", "oh\u00b7ne", "Sor\u00b7gen", "le\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.44": {"text": "Auszuf\u00fcllen meine kleine Sph\u00e4re,", "tokens": ["Aus\u00b7zu\u00b7f\u00fcl\u00b7len", "mei\u00b7ne", "klei\u00b7ne", "Sph\u00e4\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.45": {"text": "Das sey meine Sorge; damit gut;", "tokens": ["Das", "sey", "mei\u00b7ne", "Sor\u00b7ge", ";", "da\u00b7mit", "gut", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "$.", "PAV", "ADJD", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.46": {"text": "Und ein Unversch\u00e4mter st\u00f6re", "tokens": ["Und", "ein", "Un\u00b7ver\u00b7sch\u00e4m\u00b7ter", "st\u00f6\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.47": {"text": "Meinethalb mit Bettelei um Ehre,", "tokens": ["Mei\u00b7net\u00b7halb", "mit", "Bet\u00b7te\u00b7lei", "um", "Eh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.48": {"text": "Oder Gold, den Mann, der niemals ruht!", "tokens": ["O\u00b7der", "Gold", ",", "den", "Mann", ",", "der", "nie\u00b7mals", "ruht", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.49": {"text": "Mag man Andrer Renten mehren,", "tokens": ["Mag", "man", "A\u00b7ndrer", "Ren\u00b7ten", "meh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.50": {"text": "Schr\u00e4nke selbst die Meinen ein:", "tokens": ["Schr\u00e4n\u00b7ke", "selbst", "die", "Mei\u00b7nen", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.51": {"text": "Meinen Schlaf soll das nicht st\u00f6ren,", "tokens": ["Mei\u00b7nen", "Schlaf", "soll", "das", "nicht", "st\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PDS", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.52": {"text": "Nicht verg\u00e4llen meinen Wein.", "tokens": ["Nicht", "ver\u00b7g\u00e4l\u00b7len", "mei\u00b7nen", "Wein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.53": {"text": "Und was soll mir Geld? So Viele", "tokens": ["Und", "was", "soll", "mir", "Geld", "?", "So", "Vie\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PWS", "VMFIN", "PPER", "NN", "$.", "ADV", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.54": {"text": "Haben dreimal mehr, als ich;", "tokens": ["Ha\u00b7ben", "drei\u00b7mal", "mehr", ",", "als", "ich", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "$,", "KOUS", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.55": {"text": "Rennen nach der Freude Ziele", "tokens": ["Ren\u00b7nen", "nach", "der", "Freu\u00b7de", "Zie\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.56": {"text": "T\u00e4glich au\u00dfer Athem sich;", "tokens": ["T\u00e4g\u00b7lich", "au\u00b7\u00dfer", "A\u00b7them", "sich", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "PRF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.57": {"text": "Ich geh' auch den Weg im Spiele,", "tokens": ["Ich", "geh'", "auch", "den", "Weg", "im", "Spie\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.58": {"text": "Und der Erst' am Ziel' \u2013 bin ich!", "tokens": ["Und", "der", "Er\u00b7st'", "am", "Ziel'", "\u2013", "bin", "ich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPRART", "NN", "$(", "VAFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.59": {"text": "Ob der reichsten Sch\u00f6ne Hand", "tokens": ["Ob", "der", "reichs\u00b7ten", "Sch\u00f6\u00b7ne", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.60": {"text": "Des Verschwenders List sich weihet,", "tokens": ["Des", "Ver\u00b7schwen\u00b7ders", "List", "sich", "wei\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.61": {"text": "Ob ein M\u00e4dchen voll Verstand", "tokens": ["Ob", "ein", "M\u00e4d\u00b7chen", "voll", "Ver\u00b7stand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJD", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.62": {"text": "Einem Narren Weihrauch streuet,", "tokens": ["Ei\u00b7nem", "Nar\u00b7ren", "Weih\u00b7rauch", "streu\u00b7et", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.63": {"text": "Und ein dummes, Weis' entzweiet,", "tokens": ["Und", "ein", "dum\u00b7mes", ",", "Weis'", "ent\u00b7zwei\u00b7et", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "$,", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.64": {"text": "Und ein sch\u00f6nes, unbedacht,", "tokens": ["Und", "ein", "sch\u0