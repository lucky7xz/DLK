okens": ["Ist", "ein", "gar", "zu", "lan\u00b7ger", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.72": {"text": "Liebe zu Horazens Sch\u00f6nen,", "tokens": ["Lie\u00b7be", "zu", "Ho\u00b7ra\u00b7zens", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.73": {"text": "Ist ein gar zu kurzer Scherz.", "tokens": ["Ist", "ein", "gar", "zu", "kur\u00b7zer", "Scherz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.74": {"text": "La\u00df die Thoren, die zu Wagen,", "tokens": ["La\u00df", "die", "Tho\u00b7ren", ",", "die", "zu", "Wa\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "$,", "PRELS", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.75": {"text": "So wie die zu Ro\u00df und Fu\u00df,", "tokens": ["So", "wie", "die", "zu", "Ro\u00df", "und", "Fu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.76": {"text": "B\u00f6ses oder Gutes sagen;", "tokens": ["B\u00f6\u00b7ses", "o\u00b7der", "Gu\u00b7tes", "sa\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.77": {"text": "Kann ihr sch\u00f6nstes Lob behagen?", "tokens": ["Kann", "ihr", "sch\u00f6ns\u00b7tes", "Lob", "be\u00b7ha\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.78": {"text": "Es ist einer Metze Gru\u00df,", "tokens": ["Es", "ist", "ei\u00b7ner", "Met\u00b7ze", "Gru\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.79": {"text": "Und ihr Tadel zu ertragen.", "tokens": ["Und", "ihr", "Ta\u00b7del", "zu", "er\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.80": {"text": "Wahrlich, hab' ich auch nicht Zeit,", "tokens": ["Wahr\u00b7lich", ",", "hab'", "ich", "auch", "nicht", "Zeit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VAFIN", "PPER", "ADV", "PTKNEG", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.81": {"text": "Mich mit ihnen abzugeben;", "tokens": ["Mich", "mit", "ih\u00b7nen", "ab\u00b7zu\u00b7ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPER", "VVIZU", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.82": {"text": "Der verschwendet nur das Leben,", "tokens": ["Der", "ver\u00b7schwen\u00b7det", "nur", "das", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.83": {"text": "Wer der Weisheit alten Streit", "tokens": ["Wer", "der", "Weis\u00b7heit", "al\u00b7ten", "Streit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.84": {"text": "Mit der Unvernunft erneut.", "tokens": ["Mit", "der", "Un\u00b7ver\u00b7nunft", "er\u00b7neut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.85": {"text": "Freund! dir ist der Rest der Zeit,", "tokens": ["Freund", "!", "dir", "ist", "der", "Rest", "der", "Zeit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.86": {"text": "Die ich hab', im neuen Jahre,", "tokens": ["Die", "ich", "hab'", ",", "im", "neu\u00b7en", "Jah\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VAFIN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.87": {"text": "Wenn du sonst ihn willst, geweiht?", "tokens": ["Wenn", "du", "sonst", "ihn", "willst", ",", "ge\u00b7weiht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPER", "VMFIN", "$,", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.88": {"text": "Ach! erspare denn, erspare", "tokens": ["Ach", "!", "er\u00b7spa\u00b7re", "denn", ",", "er\u00b7spa\u00b7re"], "token_info": ["word", "punct", "word", "word", "punct", "word"], "pos": ["ITJ", "$.", "VVFIN", "ADV", "$,", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.89": {"text": "Was du kannst, von deiner Zeit!", "tokens": ["Was", "du", "kannst", ",", "von", "dei\u00b7ner", "Zeit", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VMFIN", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.90": {"text": "Tausche, wie in diesem Jahre,", "tokens": ["Tau\u00b7sche", ",", "wie", "in", "die\u00b7sem", "Jah\u00b7re", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.91": {"text": "Mit mir um; hast freilich nicht", "tokens": ["Mit", "mir", "um", ";", "hast", "frei\u00b7lich", "nicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPER", "PTKVZ", "$.", "VAFIN", "ADV", "PTKNEG"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.92": {"text": "Dir das beste Loos gezogen,", "tokens": ["Dir", "das", "bes\u00b7te", "Loos", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.93": {"text": "Doch beim Tausch' wird, wie man spricht,", "tokens": ["Doch", "beim", "Tausch'", "wird", ",", "wie", "man", "spricht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VAFIN", "$,", "PWAV", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.94": {"text": "Einer allemal betrogen.", "tokens": ["Ei\u00b7ner", "al\u00b7le\u00b7mal", "be\u00b7tro\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.95": {"text": "Hast du doch aus deinen Sch\u00e4tzen", "tokens": ["Hast", "du", "doch", "aus", "dei\u00b7nen", "Sch\u00e4t\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.96": {"text": "Lange, lange zuzusetzen;", "tokens": ["Lan\u00b7ge", ",", "lan\u00b7ge", "zu\u00b7zu\u00b7set\u00b7zen", ";"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VVIZU", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.97": {"text": "Nimm es so genau denn nicht.", "tokens": ["Nimm", "es", "so", "ge\u00b7nau", "denn", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "ADJD", "ADV", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}