{"textgrid.poem.39756": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1. Ausmarsch", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein Kessel, brodemvolle Stadt,", "tokens": ["Dein", "Kes\u00b7sel", ",", "bro\u00b7dem\u00b7vol\u00b7le", "Stadt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Liegt dampfend unter mir,", "tokens": ["Liegt", "damp\u00b7fend", "un\u00b7ter", "mir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Frisch, wie mich Gott geschaffen hat,", "tokens": ["Frisch", ",", "wie", "mich", "Gott", "ge\u00b7schaf\u00b7fen", "hat", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "PPER", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So wandr' ich singend hier.", "tokens": ["So", "wandr'", "ich", "sin\u00b7gend", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Mir ist, wie dem Versunkenen,", "tokens": ["Mir", "ist", ",", "wie", "dem", "Ver\u00b7sun\u00b7ke\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PWAV", "ART", "NN", "$,"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.2": {"text": "Der aufstieg aus der Gruft,", "tokens": ["Der", "auf\u00b7stieg", "aus", "der", "Gruft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mir, wie dem halb Ertrunkenen", "tokens": ["Mir", ",", "wie", "dem", "halb", "Er\u00b7trun\u00b7ke\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "PWAV", "ART", "ADJD", "NN"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.4": {"text": "Beim ersten Athem Luft.", "tokens": ["Beim", "ers\u00b7ten", "A\u00b7them", "Luft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich blicke hinter mich; der Dampf", "tokens": ["Ich", "bli\u00b7cke", "hin\u00b7ter", "mich", ";", "der", "Dampf"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$.", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ballt zu Gestalten sich,", "tokens": ["Ballt", "zu", "Ge\u00b7stal\u00b7ten", "sich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "PRF", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Und werdender Gespenster Kampf", "tokens": ["Und", "wer\u00b7den\u00b7der", "Ge\u00b7spens\u00b7ter", "Kampf"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Entspinnt sich schauerlich.", "tokens": ["Ent\u00b7spinnt", "sich", "schau\u00b7er\u00b7lich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ein Kohlenaug', ein Beingesicht,", "tokens": ["Ein", "Koh\u00b7le\u00b7naug'", ",", "ein", "Bein\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Ries', ein Zwerg, in Streit;", "tokens": ["Ein", "Ries'", ",", "ein", "Zwerg", ",", "in", "Streit", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So tauchen aus dem D\u00e4mmerlicht", "tokens": ["So", "tau\u00b7chen", "aus", "dem", "D\u00e4m\u00b7mer\u00b7licht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Geiz, Ehrgeiz, Hochmut, Neid.", "tokens": ["Geiz", ",", "Ehr\u00b7geiz", ",", "Hoch\u00b7mut", ",", "Neid", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Sie b\u00e4umen sich, sie ringen wild,", "tokens": ["Sie", "b\u00e4u\u00b7men", "sich", ",", "sie", "rin\u00b7gen", "wild", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "$,", "PPER", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie schwanken auf und ab,", "tokens": ["Sie", "schwan\u00b7ken", "auf", "und", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Im Dunst erzeugt sich das Gebild,", "tokens": ["Im", "Dunst", "er\u00b7zeugt", "sich", "das", "Ge\u00b7bild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Dunst sinkt es zu Grab.", "tokens": ["Im", "Dunst", "sinkt", "es", "zu", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ich sehe nichts von H\u00e4usern mehr,", "tokens": ["Ich", "se\u00b7he", "nichts", "von", "H\u00e4u\u00b7sern", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "APPR", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich seh' nur dies Gew\u00fchl:", "tokens": ["Ich", "seh'", "nur", "dies", "Ge\u00b7w\u00fchl", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PDS", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Jetzt merk' ich, warum mir so schwer", "tokens": ["Jetzt", "merk'", "ich", ",", "wa\u00b7rum", "mir", "so", "schwer"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "PPER", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da drunten ist, so schw\u00fcl.", "tokens": ["Da", "drun\u00b7ten", "ist", ",", "so", "schw\u00fcl", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Wer wei\u00df, welch schlimmer Geist an mir", "tokens": ["Wer", "wei\u00df", ",", "welch", "schlim\u00b7mer", "Geist", "an", "mir"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "$,", "PWAT", "ADJA", "NN", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu b\u00f6ser Stunde zerrt,", "tokens": ["Zu", "b\u00f6\u00b7ser", "Stun\u00b7de", "zerrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und richtigen Gedanken schier", "tokens": ["Und", "rich\u00b7ti\u00b7gen", "Ge\u00b7dan\u00b7ken", "schier"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Weg ins Herz versperrt?", "tokens": ["Den", "Weg", "ins", "Herz", "ver\u00b7sperrt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Durchstr\u00f6me mich, o Gottes Luft,", "tokens": ["Durch\u00b7str\u00f6\u00b7me", "mich", ",", "o", "Got\u00b7tes", "Luft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "FM", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und st\u00e4rke meinen Sinn;", "tokens": ["Und", "st\u00e4r\u00b7ke", "mei\u00b7nen", "Sinn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Durchathme mich, o Bl\u00fctenduft,", "tokens": ["Durch\u00b7ath\u00b7me", "mich", ",", "o", "Bl\u00fc\u00b7ten\u00b7duft", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bis ich gel\u00e4utert bin!", "tokens": ["Bis", "ich", "ge\u00b7l\u00e4u\u00b7tert", "bin", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}