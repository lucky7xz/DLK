{"textgrid.poem.35021": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es neigt die Sonne sich zur R\u00fcste,", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es neigt die Sonne sich zur R\u00fcste,", "tokens": ["Es", "neigt", "die", "Son\u00b7ne", "sich", "zur", "R\u00fcs\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Himmel flammt in gold'ner Gluth.", "tokens": ["Der", "Him\u00b7mel", "flammt", "in", "gold'\u00b7ner", "Gluth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr Strahl, der mir die Wange k\u00fc\u00dfte,", "tokens": ["Ihr", "Strahl", ",", "der", "mir", "die", "Wan\u00b7ge", "k\u00fc\u00df\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zuckt purpurn durch des Aethers Fluth.", "tokens": ["Zuckt", "pur\u00b7purn", "durch", "des", "A\u00b7e\u00b7thers", "Fluth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Mir bleibt die Herrlichkeit verborgen,", "tokens": ["Mir", "bleibt", "die", "Herr\u00b7lich\u00b7keit", "ver\u00b7bor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die sie im Scheiden angefacht;", "tokens": ["Die", "sie", "im", "Schei\u00b7den", "an\u00b7ge\u00b7facht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mir wird's nicht Abend und nicht Morgen,", "tokens": ["Mir", "wird's", "nicht", "A\u00b7bend", "und", "nicht", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "NN", "KON", "PTKNEG", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Nur Nachts giebt's f\u00fcr mich, finst're Nacht.", "tokens": ["Nur", "Nachts", "giebt's", "f\u00fcr", "mich", ",", "finst'\u00b7re", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "APPR", "PPER", "$,", "VVFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Doch, will die Thr\u00e4ne mir befeuchten", "tokens": ["Doch", ",", "will", "die", "Thr\u00e4\u00b7ne", "mir", "be\u00b7feuch\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "VMFIN", "ART", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Das gramerbla\u00dfte Angesicht,", "tokens": ["Das", "gra\u00b7mer\u00b7bla\u00df\u00b7te", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "So darf ", "tokens": ["So", "darf"], "token_info": ["word", "word"], "pos": ["ADV", "VMFIN"], "meter": "+-", "measure": "trochaic.single"}, "line.12": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}}, "stanza.2": {"line.1": {"text": "Es neigt der Sommer sich zur R\u00fcste;", "tokens": ["Es", "neigt", "der", "Som\u00b7mer", "sich", "zur", "R\u00fcs\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PRF", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Es flieht der V\u00f6gel munt're Schaar,", "tokens": ["Es", "flieht", "der", "V\u00f6\u00b7gel", "munt'\u00b7re", "Schaar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als ob der Wald nun sterben m\u00fc\u00dfte", "tokens": ["Als", "ob", "der", "Wald", "nun", "ster\u00b7ben", "m\u00fc\u00df\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "ART", "NN", "ADV", "VVINF", "VMFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und Feld und Flur auf immerdar.", "tokens": ["Und", "Feld", "und", "Flur", "auf", "im\u00b7mer\u00b7dar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "APPR", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es welkt der Liebe duft'ges Zeichen,", "tokens": ["Es", "welkt", "der", "Lie\u00b7be", "duft'\u00b7ges", "Zei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Rose, die so sch\u00f6n gebl\u00fcht,", "tokens": ["Die", "Ro\u00b7se", ",", "die", "so", "sch\u00f6n", "ge\u00b7bl\u00fcht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und herbstlich Trauern will sich schleichen", "tokens": ["Und", "herbst\u00b7lich", "Trau\u00b7ern", "will", "sich", "schlei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "NN", "VMFIN", "PRF", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Mir in's vereinsamte Gem\u00fcth.", "tokens": ["Mir", "in's", "ver\u00b7ein\u00b7sam\u00b7te", "Ge\u00b7m\u00fcth", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.9": {"text": "Doch, will kein Reis mehr Bl\u00fcthen treiben,", "tokens": ["Doch", ",", "will", "kein", "Reis", "mehr", "Bl\u00fc\u00b7then", "trei\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VMFIN", "PIAT", "NN", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Des Herzens Blumen welken nicht.", "tokens": ["Des", "Her\u00b7zens", "Blu\u00b7men", "wel\u00b7ken", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Treu mu\u00df der inn're Fr\u00fchling bleiben:", "tokens": ["Treu", "mu\u00df", "der", "inn'\u00b7re", "Fr\u00fch\u00b7ling", "blei\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}}, "stanza.3": {"line.1": {"text": "Es neigt das Leben sich zur R\u00fcste;", "tokens": ["Es", "neigt", "das", "Le\u00b7ben", "sich", "zur", "R\u00fcs\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PRF", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wei\u00df f\u00e4llt um's Haupt des Alters Schnee.", "tokens": ["Wei\u00df", "f\u00e4llt", "um's", "Haupt", "des", "Al\u00b7ters", "Schnee", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "NE", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nun schweigt das irdische Gel\u00fcste,", "tokens": ["Nun", "schweigt", "das", "ir\u00b7di\u00b7sche", "Ge\u00b7l\u00fcs\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und es verstummt manch' tiefes Weh.", "tokens": ["Und", "es", "ver\u00b7stummt", "man\u00b7ch'", "tie\u00b7fes", "Weh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Ist nach des Lebens kurzen Tagen", "tokens": ["Ist", "nach", "des", "Le\u00b7bens", "kur\u00b7zen", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Des Leibes schwache Kraft entfloh'n,", "tokens": ["Des", "Lei\u00b7bes", "schwa\u00b7che", "Kraft", "ent\u00b7floh'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So darf der Staub nicht l\u00e4nger tragen", "tokens": ["So", "darf", "der", "Staub", "nicht", "l\u00e4n\u00b7ger", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "NN", "PTKNEG", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Den freigeword'nen Himmelssohn.", "tokens": ["Den", "frei\u00b7ge\u00b7word'\u00b7nen", "Him\u00b7mels\u00b7sohn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und mu\u00df es balde nun geschehen,", "tokens": ["Und", "mu\u00df", "es", "bal\u00b7de", "nun", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Da\u00df man mich tr\u00e4gt zur letzten Schicht,", "tokens": ["Da\u00df", "man", "mich", "tr\u00e4gt", "zur", "letz\u00b7ten", "Schicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "So wird mein Aug' den Helfer sehen:", "tokens": ["So", "wird", "mein", "Aug'", "den", "Hel\u00b7fer", "se\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}}}}}