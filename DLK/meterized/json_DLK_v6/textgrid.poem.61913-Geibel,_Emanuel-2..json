{"textgrid.poem.61913": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbes brach der Erzverw\u00fcster,", "tokens": ["\u00bb", "es", "brach", "der", "Erz\u00b7ver\u00b7w\u00fcs\u00b7ter", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Heide brach ins Land,", "tokens": ["Der", "Hei\u00b7de", "brach", "ins", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Von seinen Pfaden d\u00fcster", "tokens": ["Von", "sei\u00b7nen", "Pfa\u00b7den", "d\u00fcs\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zum Himmel raucht der Brand.", "tokens": ["Zum", "Him\u00b7mel", "raucht", "der", "Brand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Durch H\u00fcttenschutt und Saaten", "tokens": ["Durch", "H\u00fct\u00b7ten\u00b7schutt", "und", "Saa\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "St\u00fcrmt heulend seine Wut,", "tokens": ["St\u00fcrmt", "heu\u00b7lend", "sei\u00b7ne", "Wut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und seine Rosse waten", "tokens": ["Und", "sei\u00b7ne", "Ros\u00b7se", "wa\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Bis an den Zaum im Blut.", "tokens": ["Bis", "an", "den", "Zaum", "im", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Dem Greuel wie ein Rabe", "tokens": ["Dem", "Greu\u00b7el", "wie", "ein", "Ra\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KOKOM", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Fliegt das Ger\u00fccht voraus,", "tokens": ["Fliegt", "das", "Ge\u00b7r\u00fccht", "vo\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Da greift entsetzt zum Stabe", "tokens": ["Da", "greift", "ent\u00b7setzt", "zum", "Sta\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das Volk und wandert aus.", "tokens": ["Das", "Volk", "und", "wan\u00b7dert", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie schweifen ohne St\u00e4tte", "tokens": ["Sie", "schwei\u00b7fen", "oh\u00b7ne", "St\u00e4t\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Dem scheuen Wilde gleich.", "tokens": ["Dem", "scheu\u00b7en", "Wil\u00b7de", "gleich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "O Kaiser, hilf und rette", "tokens": ["O", "Kai\u00b7ser", ",", "hilf", "und", "ret\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$,", "VVIMP", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Vom Untergang das Reich!\u00ab", "tokens": ["Vom", "Un\u00b7ter\u00b7gang", "das", "Reich", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}