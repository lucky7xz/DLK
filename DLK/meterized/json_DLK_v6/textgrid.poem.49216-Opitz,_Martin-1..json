{"textgrid.poem.49216": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zum Beten setze dich, wie jener Grieche lehret,", "tokens": ["Zum", "Be\u00b7ten", "set\u00b7ze", "dich", ",", "wie", "je\u00b7ner", "Grie\u00b7che", "leh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "$,", "PWAV", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn Gott wil auf der Flucht nicht angerufen sein;", "tokens": ["Denn", "Gott", "wil", "auf", "der", "Flucht", "nicht", "an\u00b7ge\u00b7ru\u00b7fen", "sein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VMFIN", "APPR", "ART", "NN", "PTKNEG", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Er hei\u00dfet und begehrt ein starkes Herz allein;", "tokens": ["Er", "hei\u00b7\u00dfet", "und", "be\u00b7gehrt", "ein", "star\u00b7kes", "Herz", "al\u00b7lein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das hat man aber nicht, wann er es nicht verehret.", "tokens": ["Das", "hat", "man", "a\u00b7ber", "nicht", ",", "wann", "er", "es", "nicht", "ver\u00b7eh\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PIS", "ADV", "PTKNEG", "$,", "PWAV", "PPER", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}