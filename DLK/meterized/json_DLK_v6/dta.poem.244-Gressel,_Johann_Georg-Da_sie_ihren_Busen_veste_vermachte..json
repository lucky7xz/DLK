{"dta.poem.244": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Da sie ihren Busen veste vermachte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Kind/ sey doch so bl\u00f6de nicht/", "tokens": ["Mein", "Kind", "/", "sey", "doch", "so", "bl\u00f6\u00b7de", "nicht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "VAFIN", "ADV", "ADV", "ADJA", "PTKNEG", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "La\u00df deinen Busen offen/", "tokens": ["La\u00df", "dei\u00b7nen", "Bu\u00b7sen", "of\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "PTKVZ", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So sieht man/ da\u00df dir nichts gebricht/", "tokens": ["So", "sieht", "man", "/", "da\u00df", "dir", "nichts", "ge\u00b7bricht", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "$(", "KOUS", "PPER", "PIS", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df alles eingetroffen:", "tokens": ["Da\u00df", "al\u00b7les", "ein\u00b7ge\u00b7trof\u00b7fen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sonst dencket man gewi\u00df von dir/", "tokens": ["Sonst", "den\u00b7cket", "man", "ge\u00b7wi\u00df", "von", "dir", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "APPR", "PPER", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Du h\u00e4ttest nicht der Br\u00fcste Zier.", "tokens": ["Du", "h\u00e4t\u00b7test", "nicht", "der", "Br\u00fcs\u00b7te", "Zier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ein Griff entweyht nicht deine Brust/", "tokens": ["Ein", "Griff", "ent\u00b7weyht", "nicht", "dei\u00b7ne", "Brust", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und macht ihr keine Flecken/", "tokens": ["Und", "macht", "ihr", "kei\u00b7ne", "Fle\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIAT", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Was n\u00fctzt ein Schatz der unbewust", "tokens": ["Was", "n\u00fctzt", "ein", "Schatz", "der", "un\u00b7be\u00b7wust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Sand und Steine decken?", "tokens": ["Den", "Sand", "und", "Stei\u00b7ne", "de\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Perl/ so stets verborgen liegt/", "tokens": ["Die", "Perl", "/", "so", "stets", "ver\u00b7bor\u00b7gen", "liegt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADV", "ADV", "VVPP", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit ihrem Glantze nicht vergn\u00fcgt.", "tokens": ["Mit", "ih\u00b7rem", "Glant\u00b7ze", "nicht", "ver\u00b7gn\u00fcgt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Was die Natur uns Menschen giebt/", "tokens": ["Was", "die", "Na\u00b7tur", "uns", "Men\u00b7schen", "giebt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "PPER", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das darff man allen zeigen/", "tokens": ["Das", "darff", "man", "al\u00b7len", "zei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PIS", "PIS", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Am meisten diesem/ der uns liebt/", "tokens": ["Am", "meis\u00b7ten", "die\u00b7sem", "/", "der", "uns", "liebt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "PIS", "PDAT", "$(", "PRELS", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem wir die Sinnen beugen.", "tokens": ["Dem", "wir", "die", "Sin\u00b7nen", "beu\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Was ist es/ das zum Sclaven macht?", "tokens": ["Was", "ist", "es", "/", "das", "zum", "Scla\u00b7ven", "macht", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$(", "ART", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wol anderst/ denn der Br\u00fcste Pracht.", "tokens": ["Wol", "an\u00b7derst", "/", "denn", "der", "Br\u00fcs\u00b7te", "Pracht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$(", "KON", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.4": {"line.1": {"text": "Was nun die Liebe heilig heist/", "tokens": ["Was", "nun", "die", "Lie\u00b7be", "hei\u00b7lig", "heist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "ADJD", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das lasse auch verehren/", "tokens": ["Das", "las\u00b7se", "auch", "ver\u00b7eh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und wenn denn seine Pflicht erweist/", "tokens": ["Und", "wenn", "denn", "sei\u00b7ne", "Pflicht", "er\u00b7weist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So must du den nicht st\u00f6hren/", "tokens": ["So", "must", "du", "den", "nicht", "st\u00f6h\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ART", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dem deine Brust das Altar ist/", "tokens": ["Dem", "dei\u00b7ne", "Brust", "das", "Al\u00b7tar", "ist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ART", "NN", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Auf dem er deine Gottheit k\u00fc\u00dft.", "tokens": ["Auf", "dem", "er", "dei\u00b7ne", "Got\u00b7theit", "k\u00fc\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}