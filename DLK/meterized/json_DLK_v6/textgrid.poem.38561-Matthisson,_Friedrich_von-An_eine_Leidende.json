{"textgrid.poem.38561": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "An eine Leidende", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Arme Verla\u00dfne! dein harren die H\u00fctten des ewigen Friedens!", "tokens": ["Ar\u00b7me", "Ver\u00b7la\u00df\u00b7ne", "!", "dein", "har\u00b7ren", "die", "H\u00fct\u00b7ten", "des", "e\u00b7wi\u00b7gen", "Frie\u00b7dens", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "PPOSAT", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Schon schwebt n\u00e4her der Ruh' l\u00e4chelnder Genius dir.", "tokens": ["Schon", "schwebt", "n\u00e4\u00b7her", "der", "Ruh'", "l\u00e4\u00b7cheln\u00b7der", "Ge\u00b7nius", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "ART", "NN", "ADJA", "NN", "PPER", "$."], "meter": "-+-+-++-+-+-", "measure": "unknown.measure.hexa"}, "line.3": {"text": "Weine, du Liebende, nicht! bald t\u00f6nt der Vollendung Triumphlied,", "tokens": ["Wei\u00b7ne", ",", "du", "Lie\u00b7ben\u00b7de", ",", "nicht", "!", "bald", "t\u00f6nt", "der", "Vol\u00b7len\u00b7dung", "Tri\u00b7ump\u00b7hlied", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "NN", "$,", "PTKNEG", "$.", "ADV", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+--+--+-+-+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Bald der Engelgesang, der dich, Schwester, begr\u00fc\u00dft.", "tokens": ["Bald", "der", "En\u00b7gel\u00b7ge\u00b7sang", ",", "der", "dich", ",", "Schwes\u00b7ter", ",", "be\u00b7gr\u00fc\u00dft", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PRELS", "PPER", "$,", "NN", "$,", "VVPP", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.5": {"text": "Ueber den Sternen, Geliebte, am Urquell der Kraft und des Lebens,", "tokens": ["Ue\u00b7ber", "den", "Ster\u00b7nen", ",", "Ge\u00b7lieb\u00b7te", ",", "am", "Ur\u00b7quell", "der", "Kraft", "und", "des", "Le\u00b7bens", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "NN", "$,", "APPRART", "NN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.6": {"text": "Gr\u00fcnt die Palme, die dann dich unter Himmlischen kr\u00e4nzt!", "tokens": ["Gr\u00fcnt", "die", "Pal\u00b7me", ",", "die", "dann", "dich", "un\u00b7ter", "Himm\u00b7li\u00b7schen", "kr\u00e4nzt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PRELS", "ADV", "PRF", "APPR", "NN", "VVFIN", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}}}}}