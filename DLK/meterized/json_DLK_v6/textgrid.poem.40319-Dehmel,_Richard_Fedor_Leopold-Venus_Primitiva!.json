{"textgrid.poem.40319": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Venus Primitiva!", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O da\u00df der Ku\u00df doch ewig dauern m\u00f6chte", "tokens": ["O", "da\u00df", "der", "Ku\u00df", "doch", "e\u00b7wig", "dau\u00b7ern", "m\u00f6ch\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOUS", "ART", "NN", "ADV", "ADJD", "VVINF", "VMFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "\u2013 starr stand, wie Binsen starr, der Schwarm der G\u00e4ste \u2013", "tokens": ["\u2013", "starr", "stand", ",", "wie", "Bin\u00b7sen", "starr", ",", "der", "Schwarm", "der", "G\u00e4s\u00b7te", "\u2013"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "VVFIN", "$,", "PWAV", "NN", "ADJD", "$,", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "der Ku\u00df doch ewig, den ich auf die Rechte,", "tokens": ["der", "Ku\u00df", "doch", "e\u00b7wig", ",", "den", "ich", "auf", "die", "Rech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "$,", "PRELS", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "tanztaumelnd dir auf Hals und Br\u00fcste pre\u00dfte!", "tokens": ["tanz\u00b7tau\u00b7melnd", "dir", "auf", "Hals", "und", "Br\u00fcs\u00b7te", "pre\u00df\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nein, l\u00e4nger duld'ich nicht dies bl\u00f6de Sehnen,", "tokens": ["Nein", ",", "l\u00e4n\u00b7ger", "duld'\u00b7ich", "nicht", "dies", "bl\u00f6\u00b7de", "Seh\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADJD", "ADJD", "PTKNEG", "PDS", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "ich ", "tokens": ["ich"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "die liebekranken Glieder N\u00e4chtens dehnen;", "tokens": ["die", "lie\u00b7be\u00b7kran\u00b7ken", "Glie\u00b7der", "N\u00e4ch\u00b7tens", "deh\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "o komm, du Weib! \u2013 ", "tokens": ["o", "komm", ",", "du", "Weib", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["FM", "VVFIN", "$,", "PPER", "NN", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "O komm! noch f\u00fchlt dich zitternd jeder Sinn,", "tokens": ["O", "komm", "!", "noch", "f\u00fchlt", "dich", "zit\u00b7ternd", "je\u00b7der", "Sinn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$.", "ADV", "VVFIN", "PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "vom hei\u00dfen Duft berauscht aus deinem Kleide;", "tokens": ["vom", "hei\u00b7\u00dfen", "Duft", "be\u00b7rauscht", "aus", "dei\u00b7nem", "Klei\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "noch wogt um mich, du Flammenk\u00f6nigin,", "tokens": ["noch", "wogt", "um", "mich", ",", "du", "Flam\u00b7men\u00b7k\u00f6\u00b7ni\u00b7gin", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "$,", "PPER", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "und gl\u00fcht im Aschenflor die Kupferseide.", "tokens": ["und", "gl\u00fcht", "im", "A\u00b7schen\u00b7flor", "die", "Kup\u00b7fer\u00b7sei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Gie\u00df aus in mich die Schale deiner Glut!", "tokens": ["Gie\u00df", "aus", "in", "mich", "die", "Scha\u00b7le", "dei\u00b7ner", "Glut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "APPR", "PRF", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Befrei mich von der S\u00fcnde: von dem Grauen", "tokens": ["Be\u00b7frei", "mich", "von", "der", "S\u00fcn\u00b7de", ":", "von", "dem", "Grau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PRF", "APPR", "ART", "NN", "$.", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "vor dieses Feuerregens wilder Brut,", "tokens": ["vor", "die\u00b7ses", "Feu\u00b7er\u00b7re\u00b7gens", "wil\u00b7der", "Brut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "von diesen Wehn, die w\u00fchlend in mir brauen!", "tokens": ["von", "die\u00b7sen", "Wehn", ",", "die", "w\u00fch\u00b7lend", "in", "mir", "brau\u00b7en", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "PRELS", "ADJD", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Es schie\u00dft die Saat aus ihrem dunklen Schoo\u00df,", "tokens": ["Es", "schie\u00dft", "die", "Saat", "aus", "ih\u00b7rem", "dunk\u00b7len", "Schoo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "die lange schmachtend lag in spr\u00f6der H\u00fclle;", "tokens": ["die", "lan\u00b7ge", "schmach\u00b7tend", "lag", "in", "spr\u00f6\u00b7der", "H\u00fcl\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "ich will mich lauter bl\u00fchn, lauter und los", "tokens": ["ich", "will", "mich", "lau\u00b7ter", "bl\u00fchn", ",", "lau\u00b7ter", "und", "los"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PRF", "ADJD", "VVINF", "$,", "ADJD", "KON", "PTKVZ"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "aus dieser Br\u00fcnstigkeit zu Frucht und F\u00fclle!", "tokens": ["aus", "die\u00b7ser", "Br\u00fcns\u00b7tig\u00b7keit", "zu", "Frucht", "und", "F\u00fcl\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Oh komm! satt bin ich meiner Knabenlust.", "tokens": ["Oh", "komm", "!", "satt", "bin", "ich", "mei\u00b7ner", "Kna\u00b7ben\u00b7lust", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "$.", "ADJD", "VAFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Komm, komm, du Weib! Nimm auf in Deine Schale", "tokens": ["Komm", ",", "komm", ",", "du", "Weib", "!", "Nimm", "auf", "in", "Dei\u00b7ne", "Scha\u00b7le"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "PPER", "NN", "$.", "NE", "APPR", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "die Furcht, die Sehnsucht dieser jungen Brust!", "tokens": ["die", "Furcht", ",", "die", "Sehn\u00b7sucht", "die\u00b7ser", "jun\u00b7gen", "Brust", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Noch trank ich nie den Rausch eurer Pokale.", "tokens": ["Noch", "trank", "ich", "nie", "den", "Rausch", "eu\u00b7rer", "Po\u00b7ka\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Auf Nelkend\u00fcften kommt die Nacht gezogen;", "tokens": ["Auf", "Nel\u00b7ken\u00b7d\u00fcf\u00b7ten", "kommt", "die", "Nacht", "ge\u00b7zo\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "o k\u00e4mst auch Du so s\u00fc\u00df und so verstohlen,", "tokens": ["o", "k\u00e4mst", "auch", "Du", "so", "s\u00fc\u00df", "und", "so", "ver\u00b7stoh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "VVFIN", "ADV", "PPER", "ADV", "ADJD", "KON", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "so mondeswei\u00df! O sieh: auf Sammetwogen,", "tokens": ["so", "mon\u00b7des\u00b7wei\u00df", "!", "O", "sieh", ":", "auf", "Sam\u00b7met\u00b7wo\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$.", "NE", "PTKVZ", "$.", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "auf Purpurflaum, auf schw\u00e4rzeste Violen", "tokens": ["auf", "Pur\u00b7pur\u00b7flaum", ",", "auf", "schw\u00e4r\u00b7zes\u00b7te", "Vi\u00b7o\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+---+-+-+-", "measure": "dactylic.init"}}, "stanza.8": {"line.1": {"text": "will ich dich betten \u2013 oh \u2013 dich an mich betten,", "tokens": ["will", "ich", "dich", "bet\u00b7ten", "\u2013", "oh", "\u2013", "dich", "an", "mich", "bet\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "VVINF", "$(", "FM", "$(", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "da\u00df alle meine M\u00e4chte an des Weibes", "tokens": ["da\u00df", "al\u00b7le", "mei\u00b7ne", "M\u00e4ch\u00b7te", "an", "des", "Wei\u00b7bes"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "PPOSAT", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "blendenden G\u00f6ttlichkeiten sich entketten,", "tokens": ["blen\u00b7den\u00b7den", "G\u00f6tt\u00b7lich\u00b7kei\u00b7ten", "sich", "ent\u00b7ket\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "hinschwellend in den Teppich deines Leibes.", "tokens": ["hin\u00b7schwel\u00b7lend", "in", "den", "Tep\u00b7pich", "dei\u00b7nes", "Lei\u00b7bes", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": ". . . . . . . . . . . . . . . . . . . . . . . . . . . . . . .", "tokens": [".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", "."], "token_info": ["punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct"], "pos": ["$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$."]}}, "stanza.10": {"line.1": {"text": "Wunderlich, wie dies Erinnern", "tokens": ["Wun\u00b7der\u00b7lich", ",", "wie", "dies", "E\u00b7rin\u00b7nern"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADJD", "$,", "PWAV", "PDS", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "pl\u00f6tzlich mein Erschauern k\u00fchlt.", "tokens": ["pl\u00f6tz\u00b7lich", "mein", "Er\u00b7schau\u00b7ern", "k\u00fchlt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach! der Glutpokal war zinnern", "tokens": ["Ach", "!", "der", "Glut\u00b7po\u00b7kal", "war", "zin\u00b7nern"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "ART", "NN", "VAFIN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "und zerschmolz mir, kaum gef\u00fcllt.", "tokens": ["und", "zer\u00b7schmolz", "mir", ",", "kaum", "ge\u00b7f\u00fcllt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "D\u00e4mpfe, die den Himmel sch\u00e4nden,", "tokens": ["D\u00e4mp\u00b7fe", ",", "die", "den", "Him\u00b7mel", "sch\u00e4n\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "seh ich aus den Schlacken kriechen,", "tokens": ["seh", "ich", "aus", "den", "Schla\u00b7cken", "krie\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "widerlich wie diese W\u00e4nde,", "tokens": ["wi\u00b7der\u00b7lich", "wie", "die\u00b7se", "W\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "PDAT", "NN", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.4": {"text": "die nach Pech und Moder riechen.", "tokens": ["die", "nach", "Pech", "und", "Mo\u00b7der", "rie\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NE", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Aus den hohen H\u00e4usern dr\u00fcben dringen", "tokens": ["Aus", "den", "ho\u00b7hen", "H\u00e4u\u00b7sern", "dr\u00fc\u00b7ben", "drin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ADV", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "durch die Schattenmassen Gespenster,", "tokens": ["durch", "die", "Schat\u00b7ten\u00b7mas\u00b7sen", "Ge\u00b7spens\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "die den Glanz der Nacht verschlingen;", "tokens": ["die", "den", "Glanz", "der", "Nacht", "ver\u00b7schlin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "schon verdunkelt sich ein Fenster.", "tokens": ["schon", "ver\u00b7dun\u00b7kelt", "sich", "ein", "Fens\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Kommt! ich will die Stirn euch bieten,", "tokens": ["Kommt", "!", "ich", "will", "die", "Stirn", "euch", "bie\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "PPER", "VMFIN", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schatten meiner verpra\u00dften Stunden,", "tokens": ["Schat\u00b7ten", "mei\u00b7ner", "ver\u00b7pra\u00df\u00b7ten", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "der ich Tausenden gleich an dir gelitten,", "tokens": ["der", "ich", "Tau\u00b7sen\u00b7den", "gleich", "an", "dir", "ge\u0