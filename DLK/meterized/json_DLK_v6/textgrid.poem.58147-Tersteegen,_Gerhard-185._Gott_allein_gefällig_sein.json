{"textgrid.poem.58147": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "185. Gott allein gef\u00e4llig sein", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer etwas will vor Menschen scheinen,", "tokens": ["Wer", "et\u00b7was", "will", "vor", "Men\u00b7schen", "schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "VMFIN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der ist von Gott schon ausgekehrt;", "tokens": ["Der", "ist", "von", "Gott", "schon", "aus\u00b7ge\u00b7kehrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf Gott nur sehen und Gott meinen,", "tokens": ["Auf", "Gott", "nur", "se\u00b7hen", "und", "Gott", "mei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVINF", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+--++-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Macht dich in Gottes Augen wert.", "tokens": ["Macht", "dich", "in", "Got\u00b7tes", "Au\u00b7gen", "wert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "NN", "NN", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}}}}