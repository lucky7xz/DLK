{"textgrid.poem.37403": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Auf Wiedersehn", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich schn\u00fcrte meinen Ranzen", "tokens": ["Ich", "schn\u00fcr\u00b7te", "mei\u00b7nen", "Ran\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und kam zu einer Stadt,", "tokens": ["Und", "kam", "zu", "ei\u00b7ner", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Allwo es mir im ganzen", "tokens": ["All\u00b7wo", "es", "mir", "im", "gan\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PPER", "APPRART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Recht gut gefallen hat.", "tokens": ["Recht", "gut", "ge\u00b7fal\u00b7len", "hat", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Nur eines macht beklommen,", "tokens": ["Nur", "ei\u00b7nes", "macht", "be\u00b7klom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So freundlich sonst der Ort:", "tokens": ["So", "freund\u00b7lich", "sonst", "der", "Ort", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wer heute angekommen,", "tokens": ["Wer", "heu\u00b7te", "an\u00b7ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Geht morgen wieder fort.", "tokens": ["Geht", "mor\u00b7gen", "wie\u00b7der", "fort", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Bekr\u00e4nzt mit Trauerweiden", "tokens": ["Be\u00b7kr\u00e4nzt", "mit", "Trau\u00b7er\u00b7wei\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["VVPP", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vor\u00fcber zieht der Flu\u00df,", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "zieht", "der", "Flu\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den jeder beim Verscheiden", "tokens": ["Den", "je\u00b7der", "beim", "Ver\u00b7schei\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PIS", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zuletzt passieren mu\u00df.", "tokens": ["Zu\u00b7letzt", "pas\u00b7sie\u00b7ren", "mu\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wohl dem, der ohne Grauen,", "tokens": ["Wohl", "dem", ",", "der", "oh\u00b7ne", "Grau\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "$,", "PRELS", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In Liebe treu bew\u00e4hrt,", "tokens": ["In", "Lie\u00b7be", "treu", "be\u00b7w\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zu jenen dunklen Auen", "tokens": ["Zu", "je\u00b7nen", "dunk\u00b7len", "Au\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Getrost hin\u00fcberf\u00e4hrt.", "tokens": ["Ge\u00b7trost", "hin\u00b7\u00fc\u00b7berf\u00b7\u00e4hrt", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Zwei Blinde, m\u00fcd vom Wandern,", "tokens": ["Zwei", "Blin\u00b7de", ",", "m\u00fcd", "vom", "Wan\u00b7dern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "$,", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sah ich am Ufer stehn;", "tokens": ["Sah", "ich", "am", "U\u00b7fer", "stehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der eine sprach zum andern:", "tokens": ["Der", "ei\u00b7ne", "sprach", "zum", "an\u00b7dern", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "APPRART", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Leb wohl, auf Wiedersehn.", "tokens": ["Leb", "wohl", ",", "auf", "Wie\u00b7der\u00b7sehn", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}