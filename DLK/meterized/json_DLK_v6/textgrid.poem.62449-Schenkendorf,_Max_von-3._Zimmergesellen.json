{"textgrid.poem.62449": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "3. Zimmergesellen", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zimmergesell, Zimmergesell,", "tokens": ["Zim\u00b7mer\u00b7ge\u00b7sell", ",", "Zim\u00b7mer\u00b7ge\u00b7sell", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "+---+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Wirf es hin, das braune Fell,", "tokens": ["Wirf", "es", "hin", ",", "das", "brau\u00b7ne", "Fell", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PTKVZ", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Richtscheid hin und Winkelma\u00df,", "tokens": ["Richt\u00b7scheid", "hin", "und", "Win\u00b7kel\u00b7ma\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Weil der Feind das Recht verga\u00df.", "tokens": ["Weil", "der", "Feind", "das", "Recht", "ver\u00b7ga\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nimm die Waffen schnell", "tokens": ["Nimm", "die", "Waf\u00b7fen", "schnell"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ART", "NN", "ADJD"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Starker Zimmergesell.", "tokens": ["Star\u00b7ker", "Zim\u00b7mer\u00b7ge\u00b7sell", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Aber die Axt, aber das Beil", "tokens": ["A\u00b7ber", "die", "Axt", ",", "a\u00b7ber", "das", "Beil"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "ADV", "ART", "NN"], "meter": "+--+---+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Wirf sie nimmer fort in Eil',", "tokens": ["Wirf", "sie", "nim\u00b7mer", "fort", "in", "Eil'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "PTKVZ", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Deines starken Armes Macht", "tokens": ["Dei\u00b7nes", "star\u00b7ken", "Ar\u00b7mes", "Macht"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Braucht sie wol in offner Schlacht,", "tokens": ["Braucht", "sie", "wol", "in", "off\u00b7ner", "Schlacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie den leichten Pfeil,", "tokens": ["Wie", "den", "leich\u00b7ten", "Pfeil", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Starker, schwingst du dein Beil.", "tokens": ["Star\u00b7ker", ",", "schwingst", "du", "dein", "Beil", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Und zum Ma\u00dfe den schlanken Stab", "tokens": ["Und", "zum", "Ma\u00b7\u00dfe", "den", "schlan\u00b7ken", "Stab"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Brich im n\u00e4chsten Eichwald ab;", "tokens": ["Brich", "im", "n\u00e4chs\u00b7ten", "Eich\u00b7wald", "ab", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil der Feind das Ma\u00df verga\u00df,", "tokens": ["Weil", "der", "Feind", "das", "Ma\u00df", "ver\u00b7ga\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Halte du am rechten Ma\u00df;", "tokens": ["Hal\u00b7te", "du", "am", "rech\u00b7ten", "Ma\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nach dem rhein'schen Schuh", "tokens": ["Nach", "dem", "rhein'\u00b7schen", "Schuh"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Mi\u00df die Zahlung ihm zu.", "tokens": ["Mi\u00df", "die", "Zah\u00b7lung", "ihm", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Gottes sch\u00f6nster Bau, er zerf\u00e4llt,", "tokens": ["Got\u00b7tes", "sch\u00f6ns\u00b7ter", "Bau", ",", "er", "zer\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,", "PPER", "VVFIN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Und in Fesseln klagt die Welt,", "tokens": ["Und", "in", "Fes\u00b7seln", "klagt", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Ist auch wer, der S\u00e4umni\u00df kennt,", "tokens": ["Ist", "auch", "wer", ",", "der", "S\u00e4u\u00b7mni\u00df", "kennt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PWS", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn es in den Sparren brennt?", "tokens": ["Wenn", "es", "in", "den", "Spar\u00b7ren", "brennt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Frisch ins Waffenfeld", "tokens": ["Frisch", "ins", "Waf\u00b7fen\u00b7feld"], "token_info": ["word", "word", "word"], "pos": ["NE", "APPRART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Starker B\u00fcrger und Held.", "tokens": ["Star\u00b7ker", "B\u00fcr\u00b7ger", "und", "Held", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "Unsern Hauptmann w\u00e4hlen wir nun", "tokens": ["Un\u00b7sern", "Haupt\u00b7mann", "w\u00e4h\u00b7len", "wir", "nun"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADV"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Zu dem freien k\u00fchnen Thun,", "tokens": ["Zu", "dem", "frei\u00b7en", "k\u00fch\u00b7nen", "Thun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Stimmet, wer im Felde f\u00fchr' \u2013", "tokens": ["Stim\u00b7met", ",", "wer", "im", "Fel\u00b7de", "f\u00fchr'", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWS", "APPRART", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Du, o staatlicher Polir!", "tokens": ["Du", ",", "o", "staat\u00b7li\u00b7cher", "Po\u00b7lir", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "FM", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Kluger Zimmermann", "tokens": ["Klu\u00b7ger", "Zim\u00b7mer\u00b7mann"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Zeuch dem Haufen voran.", "tokens": ["Zeuch", "dem", "Hau\u00b7fen", "vo\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "In den W\u00e4ldern, zu dem Verhau", "tokens": ["In", "den", "W\u00e4l\u00b7dern", ",", "zu", "dem", "Ver\u00b7hau"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Und zum leichten Br\u00fcckenbau", "tokens": ["Und", "zum", "leich\u00b7ten", "Br\u00fc\u00b7cken\u00b7bau"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schickt sich wol der Zimmermann;", "tokens": ["Schickt", "sich", "wol", "der", "Zim\u00b7mer\u00b7mann", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Aber wohler wird's ihm dann,", "tokens": ["A\u00b7ber", "woh\u00b7ler", "wird's", "ihm", "dann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn es blitzt und kracht", "tokens": ["Wenn", "es", "blitzt", "und", "kracht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "KON", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "In der freudigen Schlacht.", "tokens": ["In", "der", "freu\u00b7di\u00b7gen", "Schlacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.7": {"line.1": {"text": "In dem Teutoburger Wald", "tokens": ["In", "dem", "Teu\u00b7to\u00b7bur\u00b7ger", "Wald"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Stehn die B\u00e4ume stark und alt,", "tokens": ["Stehn", "die", "B\u00e4u\u00b7me", "stark", "und", "alt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "G\u00e4ben wol ein sch\u00f6nes Haus;", "tokens": ["G\u00e4\u00b7ben", "wol", "ein", "sch\u00f6\u00b7nes", "Haus", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch uns \u00fcberl\u00e4uft ein Graus \u2013", "tokens": ["Doch", "uns", "\u00fc\u00b7berl\u00b7\u00e4uft", "ein", "Graus", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$("], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.5": {"text": "Der von Hermann spricht,", "tokens": ["Der", "von", "Her\u00b7mann", "spricht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NE", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Baum, wir f\u00e4llen dich nicht.", "tokens": ["Baum", ",", "wir", "f\u00e4l\u00b7len", "dich", "nicht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.8": {"line.1": {"text": "Steh' noch lange, gr\u00fcnes Gezelt,", "tokens": ["Steh'", "noch", "lan\u00b7ge", ",", "gr\u00fc\u00b7nes", "Ge\u00b7zelt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Freiheitszeichen aller Welt.", "tokens": ["Frei\u00b7heits\u00b7zei\u00b7chen", "al\u00b7ler", "Welt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Deutschland hei\u00dfet unser Haus,", "tokens": ["Deutschland", "hei\u00b7\u00dfet", "un\u00b7ser", "Haus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Von dem Giebel weht ein Strau\u00df,", "tokens": ["Von", "dem", "Gie\u00b7bel", "weht", "ein", "Strau\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn der Bau gelang,", "tokens": ["Wenn", "der", "Bau", "ge\u00b7lang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Tapfern Preu\u00dfen zum Dank.", "tokens": ["Tap\u00b7fern", "Preu\u00b7\u00dfen", "zum", "Dank", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "APPRART", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}}}}