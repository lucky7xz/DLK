{"textgrid.poem.63464": {"metadata": {"author": {"name": "Brinckman, John", "birth": "N.A.", "death": "N.A."}, "title": "Wat hett sonn Junker mihr", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wat scher'ck mi \u00fcm sonn Eddelmann", "tokens": ["Wat", "scher'ck", "mi", "\u00fcm", "sonn", "Ed\u00b7del\u00b7mann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "un all sin'n blanken Staat,", "tokens": ["un", "all", "sin'n", "blan\u00b7ken", "Staat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM", "PIAT", "ADJA", "ADJA", "NN", "$,"], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.3": {"text": "sin Kumms\u00e4l un sin Schimmelspann,", "tokens": ["sin", "Kumm\u00b7s\u00e4l", "un", "sin", "Schim\u00b7mel\u00b7spann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "sin Kutschwag, sin Hanswust Johann", "tokens": ["sin", "Kut\u00b7schwag", ",", "sin", "Hans\u00b7wust", "Jo\u00b7hann"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "mit Tressen uppe Naht!", "tokens": ["mit", "Tres\u00b7sen", "up\u00b7pe", "Naht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Frag' nicks nah all sonn Himphamp nah \u2013", "tokens": ["Frag'", "nicks", "nah", "all", "sonn", "Him\u00b7phamp", "nah", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "ADJD", "PIAT", "ADJA", "NN", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "mi d\u00fcst min eegen Sweet!", "tokens": ["mi", "d\u00fcst", "min", "e\u00b7e\u00b7gen", "Sweet", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "NE", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Un wu ick gah un wu ick stah,", "tokens": ["Un", "wu", "ick", "gah", "un", "wu", "ick", "stah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "dor gah un stah, dor stah un gah", "tokens": ["dor", "gah", "un", "stah", ",", "dor", "stah", "un", "gah"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["FM", "FM", "FM", "VVFIN", "$,", "FM", "FM", "FM", "FM"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "ick up min eegen F\u00f6t.", "tokens": ["ick", "up", "min", "e\u00b7e\u00b7gen", "F\u00f6t", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ick d\u00f6sch hier up min eegen D\u00e4l", "tokens": ["Ick", "d\u00f6sch", "hier", "up", "min", "e\u00b7e\u00b7gen", "D\u00e4l"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "min Rogg- un Weitengarw;", "tokens": ["min", "Rog\u00b7g", "un", "Wei\u00b7ten\u00b7garw", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "TRUNC", "KON", "NE", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "min Fust is rug un swer un g\u00e4l", "tokens": ["min", "Fust", "is", "rug", "un", "swer", "un", "g\u00e4l"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "FM", "FM", "FM", "FM", "FM", "FM"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "un stif grad as min D\u00f6scherst\u00e4l", "tokens": ["un", "stif", "grad", "as", "min", "D\u00f6\u00b7scher\u00b7st\u00e4l"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["FM", "FM", "FM", "FM", "PPOSAT", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "un groff un unbedarwt.", "tokens": ["un", "groff", "un", "un\u00b7be\u00b7darwt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "ADJD", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Dat Kurn is min un min dat Brot,", "tokens": ["Dat", "Kurn", "is", "min", "un", "min", "dat", "Brot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "FM", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "is Dr\u00f6mt dat, is dat Last \u2013", "tokens": ["is", "Dr\u00f6mt", "dat", ",", "is", "dat", "Last", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["FM.nl", "FM.nl", "FM.nl", "$,", "FM", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "dat's all glik v\u00e4l un all glik got,", "tokens": ["dat's", "all", "glik", "v\u00e4l", "un", "all", "glik", "got", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PIAT", "NN", "FM", "FM", "PIAT", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "nog hett, wen kihrt sin eegen Not", "tokens": ["nog", "hett", ",", "wen", "kihrt", "sin", "e\u00b7e\u00b7gen", "Not"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "VAFIN", "$,", "PWS", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "un nimmt all as dat pa\u00dft.", "tokens": ["un", "nimmt", "all", "as", "dat", "pa\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "VVFIN", "PIAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.5": {"line.1": {"text": "Min Sch\u00f6ttel vull un warm min Jack", "tokens": ["Min", "Sch\u00f6t\u00b7tel", "vull", "un", "warm", "min", "Jack"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "FM", "FM", "ADJD", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "un rein min heeden B\u00fcr,", "tokens": ["un", "rein", "min", "hee\u00b7den", "B\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM", "ADJD", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "ahn Lunk min Dack, ahn Swamm min Fack,", "tokens": ["ahn", "Lunk", "min", "Dack", ",", "ahn", "Swamm", "min", "Fack", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PPOSAT", "NN", "$,", "VVFIN", "NE", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "min Hus ahn Schimp un Schann un Snack \u2013", "tokens": ["min", "Hus", "ahn", "Schimp", "un", "Schann", "un", "Snack", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$("], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "wat hett sonn Junker mihr!", "tokens": ["wat", "hett", "sonn", "Jun\u00b7ker", "mihr", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "NE", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}