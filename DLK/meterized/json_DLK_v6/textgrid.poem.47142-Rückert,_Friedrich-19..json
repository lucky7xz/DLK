{"textgrid.poem.47142": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "19.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Geliebte, wenn du fremde Kl\u00e4nge", "tokens": ["Ge\u00b7lieb\u00b7te", ",", "wenn", "du", "frem\u00b7de", "Kl\u00e4n\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hast hier in deinem Lied entdeckt;", "tokens": ["Hast", "hier", "in", "dei\u00b7nem", "Lied", "ent\u00b7deckt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie sollen schildern das Gedr\u00e4nge,", "tokens": ["Sie", "sol\u00b7len", "schil\u00b7dern", "das", "Ge\u00b7dr\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das mir im Busen war geweckt.", "tokens": ["Das", "mir", "im", "Bu\u00b7sen", "war", "ge\u00b7weckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "APPRART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Gedr\u00e4nge g\u00e4render Gef\u00fchle,", "tokens": ["Ge\u00b7dr\u00e4n\u00b7ge", "g\u00e4\u00b7ren\u00b7der", "Ge\u00b7f\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Geweckt von deinem Liebesblick,", "tokens": ["Ge\u00b7weckt", "von", "dei\u00b7nem", "Lie\u00b7bes\u00b7blick", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie ahnende Gewitterschw\u00fcle", "tokens": ["Wie", "ah\u00b7nen\u00b7de", "Ge\u00b7wit\u00b7ter\u00b7schw\u00fc\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vor h\u00f6chstem, nahendem Geschick.", "tokens": ["Vor", "h\u00f6chs\u00b7tem", ",", "na\u00b7hen\u00b7dem", "Ge\u00b7schick", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "In dunkle Ferne griff die Ahnung", "tokens": ["In", "dunk\u00b7le", "Fer\u00b7ne", "griff", "die", "Ah\u00b7nung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nach tief ersehntem Herzbedarf,", "tokens": ["Nach", "tief", "er\u00b7sehn\u00b7tem", "Herz\u00b7be\u00b7darf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und sah nicht, wie mit sichrer Bahnung", "tokens": ["Und", "sah", "nicht", ",", "wie", "mit", "sich\u00b7rer", "Bah\u00b7nung"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKNEG", "$,", "PWAV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das Gl\u00fcck dazu den Weg entwarf.", "tokens": ["Das", "Gl\u00fcck", "da\u00b7zu", "den", "Weg", "ent\u00b7warf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Noch einmal sollte sich die Dichtung", "tokens": ["Noch", "ein\u00b7mal", "soll\u00b7te", "sich", "die", "Dich\u00b7tung"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VMFIN", "PRF", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In alles Dichtens \u00dcberschwang", "tokens": ["In", "al\u00b7les", "Dich\u00b7tens", "\u00dc\u00b7bersc\u00b7hwang"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ersch\u00f6pfen, bis durch Selbstvernichtung", "tokens": ["Er\u00b7sch\u00f6p\u00b7fen", ",", "bis", "durch", "Selbst\u00b7ver\u00b7nich\u00b7tung"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVPP", "$,", "KOUS", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Aus ihr die Wirklichkeit entsprang.", "tokens": ["Aus", "ihr", "die", "Wirk\u00b7lich\u00b7keit", "ent\u00b7sprang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Nach Sonnen langt' ich und nach Sternen,", "tokens": ["Nach", "Son\u00b7nen", "langt'", "ich", "und", "nach", "Ster\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die ich erschuf in meinem Traum;", "tokens": ["Die", "ich", "er\u00b7schuf", "in", "mei\u00b7nem", "Traum", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und was ich sucht' in Himmelsfernen,", "tokens": ["Und", "was", "ich", "sucht'", "in", "Him\u00b7mels\u00b7fer\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Stand l\u00e4chelnd nah' im Erdenraum.", "tokens": ["Stand", "l\u00e4\u00b7chelnd", "nah'", "im", "Er\u00b7den\u00b7raum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Du hattest tiefer nicht empfunden,", "tokens": ["Du", "hat\u00b7test", "tie\u00b7fer", "nicht", "emp\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Doch klarer, was auch ich empfand,", "tokens": ["Doch", "kla\u00b7rer", ",", "was", "auch", "ich", "emp\u00b7fand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "PRELS", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und l\u00e4cheltest, bis mir geschwunden", "tokens": ["Und", "l\u00e4\u00b7chel\u00b7test", ",", "bis", "mir", "ge\u00b7schwun\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "VVPP"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Die T\u00e4uschung, die dich nie umwand.", "tokens": ["Die", "T\u00e4u\u00b7schung", ",", "die", "dich", "nie", "um\u00b7wand", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Da sanken alle Nebel nieder,", "tokens": ["Da", "san\u00b7ken", "al\u00b7le", "Ne\u00b7bel", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und deutlich tratest du hervor;", "tokens": ["Und", "deut\u00b7lich", "tra\u00b7test", "du", "her\u00b7vor", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und nun h\u00f6r', o Geliebte, wieder", "tokens": ["Und", "nun", "h\u00f6r'", ",", "o", "Ge\u00b7lieb\u00b7te", ",", "wie\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KON", "ADV", "VVFIN", "$,", "FM", "NN", "$,", "ADV"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Ganz deiner eignen Lieder Chor.", "tokens": ["Ganz", "dei\u00b7ner", "eig\u00b7nen", "Lie\u00b7der", "Chor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "La\u00df auch das erste mich erneuen,", "tokens": ["La\u00df", "auch", "das", "ers\u00b7te", "mich", "er\u00b7neu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "ADJA", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das dort im Garten mir entsprang,", "tokens": ["Das", "dort", "im", "Gar\u00b7ten", "mir", "ent\u00b7sprang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als fr\u00fche Werbung nur den scheuen", "tokens": ["Als", "fr\u00fc\u00b7he", "Wer\u00b7bung", "nur", "den", "scheu\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "NN", "ADV", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Flug noch um deinen Schleier schwang.", "tokens": ["Flug", "noch", "um", "dei\u00b7nen", "Schlei\u00b7er", "schwang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Derselbe Schleier ist's, der gr\u00fcne,", "tokens": ["Der\u00b7sel\u00b7be", "Schlei\u00b7er", "ist's", ",", "der", "gr\u00fc\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDAT", "NN", "VAFIN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der, l\u00e4ngst entwandt dem Angesicht,", "tokens": ["Der", ",", "l\u00e4ngst", "ent\u00b7wandt", "dem", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als Vorhang einer andern B\u00fchne", "tokens": ["Als", "Vor\u00b7hang", "ei\u00b7ner", "an\u00b7dern", "B\u00fch\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mir noch gef\u00e4llt, und minder nicht.", "tokens": ["Mir", "noch", "ge\u00b7f\u00e4llt", ",", "und", "min\u00b7der", "nicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "$,", "KON", "ADV", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Er flattert dort nun um die Wiege,", "tokens": ["Er", "flat\u00b7tert", "dort", "nun", "um", "die", "Wie\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dem neugebornen Rosenblatt", "tokens": ["Dem", "neu\u00b7ge\u00b7bor\u00b7nen", "Ro\u00b7sen\u00b7blatt"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zu wehren ab die Stubenfliege,", "tokens": ["Zu", "weh\u00b7ren", "ab", "die", "Stu\u00b7ben\u00b7flie\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und wehrt sie nicht, weil Riss' er hat.", "tokens": ["Und", "wehrt", "sie", "nicht", ",", "weil", "Riss'", "er", "hat", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "$,", "KOUS", "NE", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}