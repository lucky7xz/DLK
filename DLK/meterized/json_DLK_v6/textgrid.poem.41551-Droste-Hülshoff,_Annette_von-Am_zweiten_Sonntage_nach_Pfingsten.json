{"textgrid.poem.41551": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Am zweiten Sonntage nach Pfingsten", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Haus hab' ich gekauft, ein Weib hab' ich genommen,", "tokens": ["Ein", "Haus", "hab'", "ich", "ge\u00b7kauft", ",", "ein", "Weib", "hab'", "ich", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP", "$,", "ART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Drum Herr kann ich nicht kommen.", "tokens": ["Drum", "Herr", "kann", "ich", "nicht", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "NN", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das Haus mein Erdenleib,", "tokens": ["Das", "Haus", "mein", "Er\u00b7den\u00b7leib", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Des ich in Ruh' mu\u00df pflegen,", "tokens": ["Des", "ich", "in", "Ruh'", "mu\u00df", "pfle\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Poesie das Weib,", "tokens": ["Die", "Poe\u00b7sie", "das", "Weib", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Dem ich zu F\u00fc\u00dfen legen", "tokens": ["Dem", "ich", "zu", "F\u00fc\u00b7\u00dfen", "le\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Will meiner Mu\u00dfe Frommen", "tokens": ["Will", "mei\u00b7ner", "Mu\u00b7\u00dfe", "From\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Zu s\u00fc\u00dfem Zeitvertreib.", "tokens": ["Zu", "s\u00fc\u00b7\u00dfem", "Zeit\u00b7ver\u00b7treib", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Gebrechlich ist mein Haus, bedarf gar sehr der St\u00fctzen,", "tokens": ["Ge\u00b7brech\u00b7lich", "ist", "mein", "Haus", ",", "be\u00b7darf", "gar", "sehr", "der", "St\u00fct\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPOSAT", "NN", "$,", "VVFIN", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Soll es mir ferner n\u00fctzen.", "tokens": ["Soll", "es", "mir", "fer\u00b7ner", "n\u00fct\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ADV", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "So lieblich ist die Frau,", "tokens": ["So", "lieb\u00b7lich", "ist", "die", "Frau", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Sie zieht mich ohne Ma\u00dfen", "tokens": ["Sie", "zieht", "mich", "oh\u00b7ne", "Ma\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Zu ihrer Sch\u00f6nheit Schau.", "tokens": ["Zu", "ih\u00b7rer", "Sch\u00f6n\u00b7heit", "Schau", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Ach, ihr mag ich wohl lassen", "tokens": ["Ach", ",", "ihr", "mag", "ich", "wohl", "las\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PPER", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Der lichten Stunden Blitzen,", "tokens": ["Der", "lich\u00b7ten", "Stun\u00b7den", "Blit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der Tr\u00e4ume D\u00e4mmertau.", "tokens": ["Der", "Tr\u00e4u\u00b7me", "D\u00e4m\u00b7mer\u00b7tau", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Was f\u00fchl' ich denn so hei\u00df in meinem Busen quellen,", "tokens": ["Was", "f\u00fchl'", "ich", "denn", "so", "hei\u00df", "in", "mei\u00b7nem", "Bu\u00b7sen", "quel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "ADV", "ADJD", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als wollt' es ihn zerschellen?", "tokens": ["Als", "wollt'", "es", "ihn", "zer\u00b7schel\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Was fl\u00fcstert an mein Ohr?", "tokens": ["Was", "fl\u00fcs\u00b7tert", "an", "mein", "Ohr", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Mich d\u00fcnkt es, eine Stimme", "tokens": ["Mich", "d\u00fcnkt", "es", ",", "ei\u00b7ne", "Stim\u00b7me"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dring' aus dem Bau hervor", "tokens": ["Dring'", "aus", "dem", "Bau", "her\u00b7vor"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "PTKVZ"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "Wie in verhaltnem Grimme,", "tokens": ["Wie", "in", "ver\u00b7halt\u00b7nem", "Grim\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wie fernen Meeres Wellen,", "tokens": ["Wie", "fer\u00b7nen", "Mee\u00b7res", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und spricht: o Tor! du Tor!", "tokens": ["Und", "spricht", ":", "o", "Tor", "!", "du", "Tor", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "FM", "NN", "$.", "PPER", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Kein Haus hast du gekauft, es ward dir nur verpf\u00e4ndet", "tokens": ["Kein", "Haus", "hast", "du", "ge\u00b7kauft", ",", "es", "ward", "dir", "nur", "ver\u00b7pf\u00e4n\u00b7det"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "VVPP", "$,", "PPER", "VAFIN", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bis jener Faden endet,", "tokens": ["Bis", "je\u00b7ner", "Fa\u00b7den", "en\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Des Dauer keiner kennt,", "tokens": ["Des", "Dau\u00b7er", "kei\u00b7ner", "kennt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIS", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und keiner mag verl\u00e4ngen,", "tokens": ["Und", "kei\u00b7ner", "mag", "ver\u00b7l\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Spindel rollt und rennt.", "tokens": ["Die", "Spin\u00b7del", "rollt", "und", "rennt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Ach! jener Stunde Dr\u00e4ngen", "tokens": ["Ach", "!", "je\u00b7ner", "Stun\u00b7de", "Dr\u00e4n\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ITJ", "$.", "PDAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Hat keiner noch gewendet,", "tokens": ["Hat", "kei\u00b7ner", "noch", "ge\u00b7wen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "So tief die Angst ihn brennt!", "tokens": ["So", "tief", "die", "Angst", "ihn", "brennt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Nicht lieblich ist die Frau, 's ist eine strenge Norne,", "tokens": ["Nicht", "lieb\u00b7lich", "ist", "die", "Frau", ",", "'s", "ist", "ei\u00b7ne", "stren\u00b7ge", "Nor\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VAFIN", "ART", "NN", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Erzittre ihrem Zorne;", "tokens": ["Er\u00b7zitt\u00b7re", "ih\u00b7rem", "Zor\u00b7ne", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie schl\u00fcrft dein Leben auf.", "tokens": ["Sie", "schl\u00fcrft", "dein", "Le\u00b7ben", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und mu\u00df es dann entrinnen,", "tokens": ["Und", "mu\u00df", "es", "dann", "ent\u00b7rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So tu den besten Kauf:", "tokens": ["So", "tu", "den", "bes\u00b7ten", "Kauf", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Wohl magst du dir gewinnen,", "tokens": ["Wohl", "magst", "du", "dir", "ge\u00b7win\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Was aller Leiden Dorne", "tokens": ["Was", "al\u00b7ler", "Lei\u00b7den", "Dor\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "PIAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wiegt \u00fcberschwenglich auf.", "tokens": ["Wiegt", "\u00fc\u00b7bersc\u00b7hweng\u00b7lich", "auf", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Drum sorge ferner nicht um deines Hauses W\u00e4nde,", "tokens": ["Drum", "sor\u00b7ge", "fer\u00b7ner", "nicht", "um", "dei\u00b7nes", "Hau\u00b7ses", "W\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADV", "PTKNEG", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des Eigent\u00fcmers H\u00e4nde", "tokens": ["Des", "Ei\u00b7gen\u00b7t\u00fc\u00b7mers", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sind sch\u00fctzend drauf gelegt.", "tokens": ["Sind", "sch\u00fct\u00b7zend", "drauf", "ge\u00b7legt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "PAV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und wie ein Wuchrer handle,", "tokens": ["Und", "wie", "ein", "Wuch\u00b7rer", "hand\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Um was dein Herz bewegt;", "tokens": ["Um", "was", "dein", "Herz", "be\u00b7wegt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Mit jener Frau verwandle", "tokens": ["Mit", "je\u00b7ner", "Frau", "ver\u00b7wand\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "In Himmelshauch die Spende,", "tokens": ["In", "Him\u00b7mels\u00b7hauch", "die", "Spen\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der \u00fcbern Abgrund tr\u00e4gt!", "tokens": ["Der", "\u00fc\u00b7bern", "Ab\u00b7grund", "tr\u00e4gt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}