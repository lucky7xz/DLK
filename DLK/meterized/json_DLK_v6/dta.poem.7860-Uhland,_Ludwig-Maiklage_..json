{"dta.poem.7860": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Maiklage .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Leuchtet schon die Fr\u00fchlingssonne", "tokens": ["Leuch\u00b7tet", "schon", "die", "Fr\u00fch\u00b7lings\u00b7son\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ueber See und Aue hin?", "tokens": ["Ue\u00b7ber", "See", "und", "Au\u00b7e", "hin", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hat zur St\u00e4tte stiller Wonne", "tokens": ["Hat", "zur", "St\u00e4t\u00b7te", "stil\u00b7ler", "Won\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPRART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich gew\u00f6lbt der Zweige Gr\u00fcn?", "tokens": ["Sich", "ge\u00b7w\u00f6lbt", "der", "Zwei\u00b7ge", "Gr\u00fcn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ach! die Gute, die ich meine,", "tokens": ["Ach", "!", "die", "Gu\u00b7te", ",", "die", "ich", "mei\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schenkt mir keinen Maienstral,", "tokens": ["Schenkt", "mir", "kei\u00b7nen", "Mai\u00b7en\u00b7stral", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wandelt nicht im Bl\u00fcthenhaine,", "tokens": ["Wan\u00b7delt", "nicht", "im", "Bl\u00fc\u00b7then\u00b7hai\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ruhet nicht im Quellenthal.", "tokens": ["Ru\u00b7het", "nicht", "im", "Quel\u00b7len\u00b7thal", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ja! es waren sch\u00f6nre Zeiten,", "tokens": ["Ja", "!", "es", "wa\u00b7ren", "sch\u00f6n\u00b7re", "Zei\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PPER", "VAFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Als in buntbekr\u00e4nzten Reihn", "tokens": ["Als", "in", "bunt\u00b7be\u00b7kr\u00e4nz\u00b7ten", "Reihn"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hirten mit den s\u00fc\u00dfen Br\u00e4uten", "tokens": ["Hir\u00b7ten", "mit", "den", "s\u00fc\u00b7\u00dfen", "Br\u00e4u\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Walleten zum Opferhain;", "tokens": ["Wal\u00b7le\u00b7ten", "zum", "Op\u00b7fer\u00b7hain", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Als die Jungfrau, Kr\u00fcge tragend,", "tokens": ["Als", "die", "Jung\u00b7frau", ",", "Kr\u00fc\u00b7ge", "tra\u00b7gend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Oft zum k\u00fchlen Brunnen trat,", "tokens": ["Oft", "zum", "k\u00fch\u00b7len", "Brun\u00b7nen", "trat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und der Wandrer, sehnlich fragend,", "tokens": ["Und", "der", "Wand\u00b7rer", ",", "sehn\u00b7lich", "fra\u00b7gend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sie um Trunk und Liebe bat.", "tokens": ["Sie", "um", "Trunk", "und", "Lie\u00b7be", "bat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ach! das Toben roher St\u00fcrme", "tokens": ["Ach", "!", "das", "To\u00b7ben", "ro\u00b7her", "St\u00fcr\u00b7me"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ri\u00df den goldnen Fr\u00fchling fort.", "tokens": ["Ri\u00df", "den", "gold\u00b7nen", "Fr\u00fch\u00b7ling", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schl\u00f6sser stiegen auf und Th\u00fcrme,", "tokens": ["Schl\u00f6s\u00b7ser", "stie\u00b7gen", "auf", "und", "Th\u00fcr\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKVZ", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Traurig sa\u00df die Jungfrau dort;", "tokens": ["Trau\u00b7rig", "sa\u00df", "die", "Jung\u00b7frau", "dort", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Lauschte n\u00e4chtlichem Gesange,", "tokens": ["Lauschte", "n\u00e4cht\u00b7li\u00b7chem", "Ge\u00b7san\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sah hinab in\u2019s Schlachtgew\u00fchl,", "tokens": ["Sah", "hin\u00b7ab", "in's", "Schlacht\u00b7ge\u00b7w\u00fchl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Sah es, wie im Waffendrange", "tokens": ["Sah", "es", ",", "wie", "im", "Waf\u00b7fen\u00b7dran\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ihr getreuer Streiter fiel.", "tokens": ["Ihr", "ge\u00b7treu\u00b7er", "Strei\u00b7ter", "fiel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und ein Alter, dumpf und tr\u00fcbe,", "tokens": ["Und", "ein", "Al\u00b7ter", ",", "dumpf", "und", "tr\u00fc\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ADJD", "KON", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lagerte sich auf die Welt,", "tokens": ["La\u00b7ger\u00b7te", "sich", "auf", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Das die sch\u00f6ne Jugendliebe", "tokens": ["Das", "die", "sch\u00f6\u00b7ne", "Ju\u00b7gend\u00b7lie\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie ein Traum befangen h\u00e4lt.", "tokens": ["Wie", "ein", "Traum", "be\u00b7fan\u00b7gen", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVPP", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Im Vor\u00fcbereilen gr\u00fc\u00dfen", "tokens": ["Im", "Vor\u00b7\u00fc\u00b7be\u00b7rei\u00b7len", "gr\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sich mit Blicken, voll von Schmerz,", "tokens": ["Sich", "mit", "Bli\u00b7cken", ",", "voll", "von", "Schmerz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "NN", "$,", "ADJD", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Die sich fest und ewig schlie\u00dfen", "tokens": ["Die", "sich", "fest", "und", "e\u00b7wig", "schlie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PRF", "PTKVZ", "KON", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "M\u00f6chten an das treue Herz.", "tokens": ["M\u00f6ch\u00b7ten", "an", "das", "treu\u00b7e", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Welkt, ihr Blumen und ihr B\u00e4ume,", "tokens": ["Welkt", ",", "ihr", "Blu\u00b7men", "und", "ihr", "B\u00e4u\u00b7me", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00f6hnet nicht der Liebe Schmerz!", "tokens": ["H\u00f6h\u00b7net", "nicht", "der", "Lie\u00b7be", "Schmerz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sterbet auch, ihr Jugendkeime!", "tokens": ["Ster\u00b7bet", "auch", ",", "ihr", "Ju\u00b7gend\u00b7kei\u00b7me", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schmachte hin, du volles Herz!", "tokens": ["Schmach\u00b7te", "hin", ",", "du", "vol\u00b7les", "Herz", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "In die \u00f6de Nacht der Gr\u00fcfte", "tokens": ["In", "die", "\u00f6\u00b7de", "Nacht", "der", "Gr\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sinkt, ihr J\u00fcnglinge, hinab!", "tokens": ["Sinkt", ",", "ihr", "J\u00fcng\u00b7lin\u00b7ge", ",", "hin\u00b7ab", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "$,", "PTKVZ", "$."], "meter": "+-++--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Flieder wallen in die L\u00fcfte,", "tokens": ["Flie\u00b7der", "wal\u00b7len", "in", "die", "L\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Rosen bl\u00fchn um euer Grab.", "tokens": ["Ro\u00b7sen", "bl\u00fchn", "um", "eu\u00b7er", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}