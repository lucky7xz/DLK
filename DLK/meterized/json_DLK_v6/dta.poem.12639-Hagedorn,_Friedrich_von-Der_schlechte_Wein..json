{"dta.poem.12639": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Der schlechte Wein.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1744", "urn": "urn:nbn:de:kobv:b4-200905199128", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wein, den die Bosheit ausged\u00e4cht,", "tokens": ["Wein", ",", "den", "die", "Bos\u00b7heit", "aus\u00b7ge\u00b7d\u00e4cht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Des Wassers Ruhm empor zu bringen,", "tokens": ["Des", "Was\u00b7sers", "Ruhm", "em\u00b7por", "zu", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der aus Verzweiflung trunken macht,", "tokens": ["Der", "aus", "Ver\u00b7zwei\u00b7flung", "trun\u00b7ken", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In dem wir Gift und Tod verschlingen:", "tokens": ["In", "dem", "wir", "Gift", "und", "Tod", "ver\u00b7schlin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "In dem des Hefens Aufruhr tobt,", "tokens": ["In", "dem", "des", "He\u00b7fens", "Auf\u00b7ruhr", "tobt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den niemand als der Wirth belobt,", "tokens": ["Den", "nie\u00b7mand", "als", "der", "Wirth", "be\u00b7lobt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "KOKOM", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Den Wirth und Wirthin spart! von dir will ich ietzt singen.", "tokens": ["Den", "Wirth", "und", "Wirt\u00b7hin", "spart", "!", "von", "dir", "will", "ich", "ietzt", "sin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVFIN", "$.", "APPR", "PPER", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ein harter Fluch beschwert das Land,", "tokens": ["Ein", "har\u00b7ter", "Fluch", "be\u00b7schwert", "das", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo dieser Weinstock aufgeschossen;", "tokens": ["Wo", "die\u00b7ser", "Wein\u00b7stock", "auf\u00b7ge\u00b7schos\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Es hat in dem bestraften Sand", "tokens": ["Es", "hat", "in", "dem", "be\u00b7straf\u00b7ten", "Sand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Sohn des Vaters Blut vergossen,", "tokens": ["Ein", "Sohn", "des", "Va\u00b7ters", "Blut", "ver\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und, falls mich kein Gedicht ber\u00fcckt,", "tokens": ["Und", ",", "falls", "mich", "kein", "Ge\u00b7dicht", "be\u00b7r\u00fcckt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So ist der Winzer gleich erstickt,", "tokens": ["So", "ist", "der", "Win\u00b7zer", "gleich", "er\u00b7stickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der seiner Beeren Kost zum erstenmahl genossen.", "tokens": ["Der", "sei\u00b7ner", "Bee\u00b7ren", "Kost", "zum", "ers\u00b7ten\u00b7mahl", "ge\u00b7nos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "NN", "APPRART", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Auf, auf, ihr Keile! zeigt euch bald!", "tokens": ["Auf", ",", "auf", ",", "ihr", "Kei\u00b7le", "!", "zeigt", "euch", "bald", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "PTKVZ", "$,", "PPOSAT", "NN", "$.", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf, auf, entz\u00fcndet euch, ihr Blitze!", "tokens": ["Auf", ",", "auf", ",", "ent\u00b7z\u00fcn\u00b7det", "euch", ",", "ihr", "Blit\u00b7ze", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "$,", "PTKVZ", "$,", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Vereint die r\u00e4chende Gewalt!", "tokens": ["Ver\u00b7eint", "die", "r\u00e4\u00b7chen\u00b7de", "Ge\u00b7walt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch trefft nur dieses Weinbergs Spitze,", "tokens": ["Doch", "trefft", "nur", "die\u00b7ses", "Wein\u00b7bergs", "Spit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PDAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und macht, da\u00df dieser Theil der Welt,", "tokens": ["Und", "macht", ",", "da\u00df", "die\u00b7ser", "Theil", "der", "Welt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PDAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den diese Pflanze recht verstellt,", "tokens": ["Den", "die\u00b7se", "Pflan\u00b7ze", "recht", "ver\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PDAT", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Nicht ferner H\u00e4rlinge so schlimmer Art besitze!", "tokens": ["Nicht", "fer\u00b7ner", "H\u00e4r\u00b7lin\u00b7ge", "so", "schlim\u00b7mer", "Art", "be\u00b7sit\u00b7ze", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "NN", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-++--+-+-+-", "measure": "iambic.hexa.relaxed"}}}}}