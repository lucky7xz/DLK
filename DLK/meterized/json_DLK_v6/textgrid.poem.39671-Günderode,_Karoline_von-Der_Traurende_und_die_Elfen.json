{"textgrid.poem.39671": {"metadata": {"author": {"name": "G\u00fcnderode, Karoline von", "birth": "N.A.", "death": "N.A."}, "title": "Der Traurende und die Elfen", "genre": "verse", "period": "N.A.", "pub_year": 1793, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zum Grab der Trauten schleicht der Knabe,", "tokens": ["Zum", "Grab", "der", "Trau\u00b7ten", "schleicht", "der", "Kna\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ihm ist das Herz so bang und schwer;", "tokens": ["Ihm", "ist", "das", "Herz", "so", "bang", "und", "schwer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da sinkt die dunkle Nacht hernieder", "tokens": ["Da", "sinkt", "die", "dunk\u00b7le", "Nacht", "her\u00b7nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PTKVZ"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und bleiche Geister geh'n umher;", "tokens": ["Und", "blei\u00b7che", "Geis\u00b7ter", "geh'n", "um\u00b7her", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Des Abends feuchte Nebel thauen,", "tokens": ["Des", "A\u00b7bends", "feuch\u00b7te", "Ne\u00b7bel", "thau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Nachtwind w\u00fchlt in seinem Haar,", "tokens": ["Der", "Nacht\u00b7wind", "w\u00fchlt", "in", "sei\u00b7nem", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Das Alles wird er nicht gewahr.", "tokens": ["Das", "Al\u00b7les", "wird", "er", "nicht", "ge\u00b7wahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VAFIN", "PPER", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "In Tr\u00e4umen ist er ganz verlohren,", "tokens": ["In", "Tr\u00e4u\u00b7men", "ist", "er", "ganz", "ver\u00b7loh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Er merket nicht der Stunden Gang;", "tokens": ["Er", "mer\u00b7ket", "nicht", "der", "Stun\u00b7den", "Gang", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da wekt ihn aus dem dumpfen Schlummer", "tokens": ["Da", "wekt", "ihn", "aus", "dem", "dum\u00b7pfen", "Schlum\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Musik und froher Chorgesang,", "tokens": ["Mu\u00b7sik", "und", "fro\u00b7her", "Chor\u00b7ge\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er blicket auf: und schaut den Reigen", "tokens": ["Er", "bli\u00b7cket", "auf", ":", "und", "schaut", "den", "Rei\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$.", "KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Elfen, deren munt'rer Tanz", "tokens": ["Der", "El\u00b7fen", ",", "de\u00b7ren", "munt'\u00b7rer", "Tanz"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sich schlingt um frischer Gr\u00e4ber Kranz.", "tokens": ["Sich", "schlingt", "um", "fri\u00b7scher", "Gr\u00e4\u00b7ber", "Kranz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und sieh! ihm naht der Elfen Sch\u00f6nste,", "tokens": ["Und", "sieh", "!", "ihm", "naht", "der", "El\u00b7fen", "Sch\u00f6ns\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "$.", "PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und spricht: \u00bbwas trauerst du so sehr?", "tokens": ["Und", "spricht", ":", "\u00bb", "was", "trau\u00b7erst", "du", "so", "sehr", "?"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "$(", "PWS", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Komm! ist dein M\u00e4dchen dir gestorben?", "tokens": ["Komm", "!", "ist", "dein", "M\u00e4d\u00b7chen", "dir", "ge\u00b7stor\u00b7ben", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "VAFIN", "PPOSAT", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vergi\u00df sie! komm zum Tanze her.", "tokens": ["Ver\u00b7gi\u00df", "sie", "!", "komm", "zum", "Tan\u00b7ze", "her", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$.", "VVFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Frei sind wir Elfen, ohne Sorgen,", "tokens": ["Frei", "sind", "wir", "El\u00b7fen", ",", "oh\u00b7ne", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "NN", "$,", "KOUI", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Leicht wie der Sinn ist unser Fu\u00df,", "tokens": ["Leicht", "wie", "der", "Sinn", "ist", "un\u00b7ser", "Fu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und froh und leicht sind Lieb und Ku\u00df.", "tokens": ["Und", "froh", "und", "leicht", "sind", "Lieb", "und", "Ku\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "ADJD", "VAFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "O z\u00f6gre nicht! nur wenig Stunden", "tokens": ["O", "z\u00f6g\u00b7re", "nicht", "!", "nur", "we\u00b7nig", "Stun\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "PTKNEG", "$.", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So moderst du, nur kurze Zeit", "tokens": ["So", "mo\u00b7derst", "du", ",", "nur", "kur\u00b7ze", "Zeit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "$,", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So welket Alles, was jetzt bl\u00fchet,", "tokens": ["So", "wel\u00b7ket", "Al\u00b7les", ",", "was", "jetzt", "bl\u00fc\u00b7het", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Drum komm! entsag dem schweren Leid'. \u2013", "tokens": ["Drum", "komm", "!", "ent\u00b7sag", "dem", "schwe\u00b7ren", "Leid'", ".", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PAV", "VVFIN", "$.", "VVFIN", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wild springt er auf zum raschen Tanze", "tokens": ["Wild", "springt", "er", "auf", "zum", "ra\u00b7schen", "Tan\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "APPR", "APPRART", "ADJA", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Und \u00fcber seiner Braut Gebein", "tokens": ["Und", "\u00fc\u00b7ber", "sei\u00b7ner", "Braut", "Ge\u00b7bein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Schlingt sich der lust'ge Elfenreihn.", "tokens": ["Schlingt", "sich", "der", "lust'\u00b7ge", "El\u00b7fen\u00b7reihn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Er tanzt, vergisset die Geliebte,", "tokens": ["Er", "tanzt", ",", "ver\u00b7gis\u00b7set", "die", "Ge\u00b7lieb\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Leicht, wie der Elfen, wird sein Sinn", "tokens": ["Leicht", ",", "wie", "der", "El\u00b7fen", ",", "wird", "sein", "Sinn"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "$,", "PWAV", "ART", "NN", "$,", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Entbunden aller Erdensorgen", "tokens": ["Ent\u00b7bun\u00b7den", "al\u00b7ler", "Er\u00b7den\u00b7sor\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["NN", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schwingt er sich \u00fcber Wolken hin.", "tokens": ["Schwingt", "er", "sich", "\u00fc\u00b7ber", "Wol\u00b7ken", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er sieht Geschlechter kommen, sterben,", "tokens": ["Er", "sieht", "Ge\u00b7schlech\u00b7ter", "kom\u00b7men", ",", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "VVINF", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Kann Alles froh und lustig sehn", "tokens": ["Kann", "Al\u00b7les", "froh", "und", "lus\u00b7tig", "sehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PIS", "ADJD", "KON", "ADJD", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der Dinge Bl\u00fchen und Vergehn.", "tokens": ["Der", "Din\u00b7ge", "Bl\u00fc\u00b7hen", "und", "Ver\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}