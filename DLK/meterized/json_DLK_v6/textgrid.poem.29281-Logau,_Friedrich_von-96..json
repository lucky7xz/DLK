{"textgrid.poem.29281": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "96.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "B\u00fccher haben auch ihr Gl\u00fccke: Wann sie nicht gesaltzen seyn,", "tokens": ["B\u00fc\u00b7cher", "ha\u00b7ben", "auch", "ihr", "Gl\u00fc\u00b7cke", ":", "Wann", "sie", "nicht", "ge\u00b7salt\u00b7zen", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "PPOSAT", "NN", "$.", "PWAV", "PPER", "PTKNEG", "VVPP", "VAINF", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Fasst man dennoch gute W\u00fcrtze, Pfeffer oder Saffran drein;", "tokens": ["Fasst", "man", "den\u00b7noch", "gu\u00b7te", "W\u00fcrt\u00b7ze", ",", "Pfef\u00b7fer", "o\u00b7der", "Saf\u00b7fran", "drein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "ADJA", "NN", "$,", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "K\u00fcmmt es dir, ich bin zu friede, liebes Buch, nur auch so gut,", "tokens": ["K\u00fcmmt", "es", "dir", ",", "ich", "bin", "zu", "frie\u00b7de", ",", "lie\u00b7bes", "Buch", ",", "nur", "auch", "so", "gut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$,", "PPER", "VAFIN", "APPR", "NN", "$,", "ADJA", "NN", "$,", "ADV", "ADV", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Wann mit dir nur in geheime niemand was versch\u00e4mtes thut.", "tokens": ["Wann", "mit", "dir", "nur", "in", "ge\u00b7hei\u00b7me", "nie\u00b7mand", "was", "ver\u00b7sch\u00e4m\u00b7tes", "thut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "PPER", "ADV", "APPR", "ADJA", "PIS", "PWS", "ADJA", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}