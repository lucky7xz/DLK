{"textgrid.poem.24548": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Als einer Doctor ward/ nach Anleitung seiner Disputation: de Jure & erroribus circa divortia", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Scheidung/ welche sonst Verbundnen Ungl\u00fcck macht/", "tokens": ["Die", "Schei\u00b7dung", "/", "wel\u00b7che", "sonst", "Ver\u00b7bund\u00b7nen", "Un\u00b7gl\u00fcck", "macht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PRELS", "ADV", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hat Ihm/ Geehrtester/ viel Ehr und Gl\u00fcck gebracht/", "tokens": ["Hat", "Ihm", "/", "Ge\u00b7ehr\u00b7tes\u00b7ter", "/", "viel", "Ehr", "und", "Gl\u00fcck", "ge\u00b7bracht", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$(", "NN", "$(", "PIAT", "NN", "KON", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Durch den gelehrten Streit bekommt er ein Ergetzen/", "tokens": ["Durch", "den", "ge\u00b7lehr\u00b7ten", "Streit", "be\u00b7kommt", "er", "ein", "Er\u00b7get\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "PPER", "ART", "NN", "$("], "meter": "-+-+-+-+--++-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Ein Kind/ dadurch er sich kan v\u00f6llig gl\u00fccklich sch\u00e4tzen.", "tokens": ["Ein", "Kind", "/", "da\u00b7durch", "er", "sich", "kan", "v\u00f6l\u00b7lig", "gl\u00fcck\u00b7lich", "sch\u00e4t\u00b7zen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PAV", "PPER", "PRF", "VMFIN", "ADJD", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der Himmel und sein Geist giebt Ihm die sch\u00f6ne Braut/", "tokens": ["Der", "Him\u00b7mel", "und", "sein", "Geist", "giebt", "Ihm", "die", "sch\u00f6\u00b7ne", "Braut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PPOSAT", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "An welcher alle Welt die besten Sitten schaut/", "tokens": ["An", "wel\u00b7cher", "al\u00b7le", "Welt", "die", "bes\u00b7ten", "Sit\u00b7ten", "schaut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PIAT", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Sie wird Zufriedenheit in seine Seele f\u00fchren/", "tokens": ["Sie", "wird", "Zu\u00b7frie\u00b7den\u00b7heit", "in", "sei\u00b7ne", "See\u00b7le", "f\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ihn mit dem Ehren-Hut/ doch keiner Feder zieren.", "tokens": ["Ihn", "mit", "dem", "Eh\u00b7ren\u00b7Hut", "/", "doch", "kei\u00b7ner", "Fe\u00b7der", "zie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "$(", "ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Sie leistet vielen zwar/ wie Ihm der Ehe Pflicht/", "tokens": ["Sie", "leis\u00b7tet", "vie\u00b7len", "zwar", "/", "wie", "Ihm", "der", "E\u00b7he", "Pflicht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "ADV", "$(", "PWAV", "PPER", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Doch liebt sie Ihn getreu und bricht die Ehe nicht.", "tokens": ["Doch", "liebt", "sie", "Ihn", "ge\u00b7treu", "und", "bricht", "die", "E\u00b7he", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "ADJD", "KON", "VVFIN", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Sie scheint die Ketzerey nicht g\u00e4ntzlich zu vermeiden/", "tokens": ["Sie", "scheint", "die", "Ket\u00b7ze\u00b7rey", "nicht", "g\u00e4ntz\u00b7lich", "zu", "ver\u00b7mei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKNEG", "ADJD", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Allein kein Kluger wird sich darum von ihr scheiden.", "tokens": ["Al\u00b7lein", "kein", "Klu\u00b7ger", "wird", "sich", "da\u00b7rum", "von", "ihr", "schei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VAFIN", "PRF", "PAV", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Und weil Er t\u00fcchtig gnung/ gef\u00e4llt Er Ihr so sehr/", "tokens": ["Und", "weil", "Er", "t\u00fcch\u00b7tig", "gnung", "/", "ge\u00b7f\u00e4llt", "Er", "Ihr", "so", "sehr", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "ADV", "$(", "VVFIN", "PPER", "PPER", "ADV", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "De\u00dfwegen folget auch die Trennung nimmermehr.", "tokens": ["De\u00df\u00b7we\u00b7gen", "fol\u00b7get", "auch", "die", "Tren\u00b7nung", "nim\u00b7mer\u00b7mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADV", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ein anderer mag Sie in Geist und Armen fassen/", "tokens": ["Ein", "an\u00b7de\u00b7rer", "mag", "Sie", "in", "Geist", "und", "Ar\u00b7men", "fas\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VMFIN", "PPER", "APPR", "NN", "KON", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "So wird Sie Ihn dennoch nicht freventlich verlassen.", "tokens": ["So", "wird", "Sie", "Ihn", "den\u00b7noch", "nicht", "fre\u00b7vent\u00b7lich", "ver\u00b7las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "ADV", "PTKNEG", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Sie stellet Ihm nicht nach/ Sie ist sein Zeit-Vertreib/", "tokens": ["Sie", "stel\u00b7let", "Ihm", "nicht", "nach", "/", "Sie", "ist", "sein", "Zeit\u00b7Ver\u00b7treib", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "APPR", "$(", "PPER", "VAFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und in kein Kloster l\u00e4uft di\u00df allzu edle Weib.", "tokens": ["Und", "in", "kein", "Klos\u00b7ter", "l\u00e4uft", "di\u00df", "all\u00b7zu", "ed\u00b7le", "Weib", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "NN", "VVFIN", "PDS", "PTKA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Im Kriege kan kein Held die starcke Heldin fangen", "tokens": ["Im", "Krie\u00b7ge", "kan", "kein", "Held", "die", "star\u00b7cke", "Hel\u00b7din", "fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VMFIN", "PIAT", "NN", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Nur/ der sich Ihr ergiebt/ mag Sie gew\u00fcnscht erlangen.", "tokens": ["Nur", "/", "der", "sich", "Ihr", "er\u00b7giebt", "/", "mag", "Sie", "ge\u00b7w\u00fcnscht", "er\u00b7lan\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "PRELS", "PRF", "PPER", "VVFIN", "$(", "VMFIN", "PPER", "VVPP", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "In keinen Eckel wird sein edler Geist gesetzt/", "tokens": ["In", "kei\u00b7nen", "E\u00b7ckel", "wird", "sein", "ed\u00b7ler", "Geist", "ge\u00b7setzt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VAFIN", "PPOSAT", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Wenn Ihn ihr Nectar-Thau so Tag als Nacht benetzt.", "tokens": ["Wenn", "Ihn", "ihr", "Nec\u00b7ta\u00b7r\u00b7T\u00b7hau", "so", "Tag", "als", "Nacht", "be\u00b7netzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "ADV", "NN", "KOUS", "NN", "VVPP", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.23": {"text": "Der Todt/ der alles bricht/ wird dieses Band nicht reissen/", "tokens": ["Der", "Todt", "/", "der", "al\u00b7les", "bricht", "/", "wird", "die\u00b7ses", "Band", "nicht", "reis\u00b7sen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "PIS", "VVFIN", "$(", "VAFIN", "PDAT", "NN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Weil die Gelehrten auch im Grabe lebend heissen.", "tokens": ["Weil", "die", "Ge\u00b7lehr\u00b7ten", "auch", "im", "Gra\u00b7be", "le\u00b7bend", "heis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "APPRART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Nun dieser Edle Schatz ist ", "tokens": ["Nun", "die\u00b7ser", "Ed\u00b7le", "Schatz", "ist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PDAT", "ADJA", "NN", "VAFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.26": {"text": "Ich w\u00fcnsche Gl\u00fcck darzu. Und wird mir wissend seyn/", "tokens": ["Ich", "w\u00fcn\u00b7sche", "Gl\u00fcck", "dar\u00b7zu", ".", "Und", "wird", "mir", "wis\u00b7send", "seyn", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "PAV", "$.", "KON", "VAFIN", "PPER", "VVPP", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Wenn Er die andre Braut nacht Hause denckt zu f\u00fchren/", "tokens": ["Wenn", "Er", "die", "and\u00b7re", "Braut", "nacht", "Hau\u00b7se", "denckt", "zu", "f\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "NN", "NN", "VVFIN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "So l\u00e4\u00dft dar\u00fcber ihm auch seine Freude sp\u00fcren.", "tokens": ["So", "l\u00e4\u00dft", "da\u00b7r\u00fc\u00b7ber", "ihm", "auch", "sei\u00b7ne", "Freu\u00b7de", "sp\u00fc\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PAV", "PPER", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}