{"textgrid.poem.42391": {"metadata": {"author": {"name": "Lachmann, Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie jene Frauen von einf\u00e4ltgem Sinn,", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie jene Frauen von einf\u00e4ltgem Sinn,", "tokens": ["Wie", "je\u00b7ne", "Frau\u00b7en", "von", "ein\u00b7f\u00e4lt\u00b7gem", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die so voll Demut ihren Gatten eigen", "tokens": ["Die", "so", "voll", "De\u00b7mut", "ih\u00b7ren", "Gat\u00b7ten", "ei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADJD", "NN", "PPOSAT", "NN", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie M\u00e4gde sind, und die sich von Beginn", "tokens": ["Wie", "M\u00e4g\u00b7de", "sind", ",", "und", "die", "sich", "von", "Be\u00b7ginn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "VAFIN", "$,", "KON", "PRELS", "PRF", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Bis an ihr Ende seinem Willen neigen,", "tokens": ["Bis", "an", "ihr", "En\u00b7de", "sei\u00b7nem", "Wil\u00b7len", "nei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "PPOSAT", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die, wie in Dumpfheit, ihre Liebe, drin", "tokens": ["Die", ",", "wie", "in", "Dum\u00b7pf\u00b7heit", ",", "ih\u00b7re", "Lie\u00b7be", ",", "drin"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ART", "$,", "PWAV", "APPR", "NN", "$,", "PPOSAT", "NN", "$,", "ADV"], "meter": "-+-++-+-+-+", "measure": "unknown.measure.hexa"}, "line.2": {"text": "Ihr Dasein eingeschlossen liegt, mit Schweigen,", "tokens": ["Ihr", "Da\u00b7sein", "ein\u00b7ge\u00b7schlos\u00b7sen", "liegt", ",", "mit", "Schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "VVFIN", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie bei dem Opferdienst die Priesterin,", "tokens": ["Wie", "bei", "dem", "Op\u00b7fer\u00b7dienst", "die", "Pries\u00b7te\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In Sorgfalt nur und treuem Tun bezeigen:", "tokens": ["In", "Sorg\u00b7falt", "nur", "und", "treu\u00b7em", "Tun", "be\u00b7zei\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "KON", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So ist mein ganzes Sein um dich gefaltet", "tokens": ["So", "ist", "mein", "gan\u00b7zes", "Sein", "um", "dich", "ge\u00b7fal\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "APPR", "PPER", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und hat nach keinem \u00c4ussren mehr Bestreben,", "tokens": ["Und", "hat", "nach", "kei\u00b7nem", "\u00c4uss\u00b7ren", "mehr", "Be\u00b7stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "PIAT", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Als seien Ziel und Fernsicht ihm entschwunden", "tokens": ["Als", "sei\u00b7en", "Ziel", "und", "Fern\u00b7sicht", "ihm", "ent\u00b7schwun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "KON", "NN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und jede Sehnsucht fast in mir erkaltet:", "tokens": ["Und", "je\u00b7de", "Sehn\u00b7sucht", "fast", "in", "mir", "er\u00b7kal\u00b7tet", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und ist mir doch, ich sei, dir hingegeben,", "tokens": ["Und", "ist", "mir", "doch", ",", "ich", "sei", ",", "dir", "hin\u00b7ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "$,", "PPER", "VAFIN", "$,", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Welt und allem Menschenlos verbunden.", "tokens": ["Der", "Welt", "und", "al\u00b7lem", "Men\u00b7schen\u00b7los", "ver\u00b7bun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PIS", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}