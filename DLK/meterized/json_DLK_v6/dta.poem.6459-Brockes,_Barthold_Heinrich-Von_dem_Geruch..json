{"dta.poem.6459": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Geruch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Von allen riechenden Gesch\u00f6pffen, dran wir hie", "tokens": ["Von", "al\u00b7len", "rie\u00b7chen\u00b7den", "Ge\u00b7sch\u00f6pf\u00b7fen", ",", "dran", "wir", "hie"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$,", "PAV", "PPER", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Uns laben, freuen und ergetzen,", "tokens": ["Uns", "la\u00b7ben", ",", "freu\u00b7en", "und", "er\u00b7get\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$,", "ADJA", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sind Amber, sammt dem Museus, die,", "tokens": ["Sind", "Am\u00b7ber", ",", "sammt", "dem", "Mu\u00b7seus", ",", "die", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "APPR", "ART", "NN", "$,", "PRELS", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "So wir am allerh\u00f6chsten sch\u00e4tzen.", "tokens": ["So", "wir", "am", "al\u00b7ler\u00b7h\u00f6chs\u00b7ten", "sch\u00e4t\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "APPRART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der rare Ambra labt noch mehr als einen Sinn,", "tokens": ["Der", "ra\u00b7re", "A\u00b7mbra", "labt", "noch", "mehr", "als", "ei\u00b7nen", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "PIAT", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Vergn\u00fcget den Geschmack, nebst dem Geruch zugleich,", "tokens": ["Ver\u00b7gn\u00fc\u00b7get", "den", "Ge\u00b7schmack", ",", "nebst", "dem", "Ge\u00b7ruch", "zu\u00b7gleich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "APPR", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Da er Arabiens Geschenck noch \u00fcbertrifft.", "tokens": ["Da", "er", "A\u00b7ra\u00b7biens", "Ge\u00b7schenck", "noch", "\u00fc\u00b7bert\u00b7rifft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NE", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.8": {"text": "Der Schatz, nach welchen man so ferne", "tokens": ["Der", "Schatz", ",", "nach", "wel\u00b7chen", "man", "so", "fer\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "APPR", "PWAT", "PIS", "ADV", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Und auf entlegnen Meeren schifft,", "tokens": ["Und", "auf", "ent\u00b7leg\u00b7nen", "Mee\u00b7ren", "schifft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Geworffen an dem Strand, durch Thetis wildes Reich,", "tokens": ["Ge\u00b7worf\u00b7fen", "an", "dem", "Strand", ",", "durch", "The\u00b7tis", "wil\u00b7des", "Reich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "APPR", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}