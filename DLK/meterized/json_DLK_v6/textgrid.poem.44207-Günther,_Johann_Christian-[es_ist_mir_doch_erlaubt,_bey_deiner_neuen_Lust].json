{"textgrid.poem.44207": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "[es ist mir doch erlaubt, bey deiner neuen Lust]", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist mir doch erlaubt, bey deiner neuen Lust,", "tokens": ["Es", "ist", "mir", "doch", "er\u00b7laubt", ",", "bey", "dei\u00b7ner", "neu\u00b7en", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vergn\u00fcgt- und edles Paar, die Neigung meiner Brust", "tokens": ["Ver\u00b7gn\u00fcg\u00b7t", "und", "ed\u00b7les", "Paar", ",", "die", "Nei\u00b7gung", "mei\u00b7ner", "Brust"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["TRUNC", "KON", "ADJA", "NN", "$,", "ART", "NN", "PPOSAT", "NN"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Durch Zeilen auf der Flucht mit W\u00fcntschen einzusenden,", "tokens": ["Durch", "Zei\u00b7len", "auf", "der", "Flucht", "mit", "W\u00fcnt\u00b7schen", "ein\u00b7zu\u00b7sen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ob gleich der schwere Kiel den angesezten H\u00e4nden", "tokens": ["Ob", "gleich", "der", "schwe\u00b7re", "Kiel", "den", "an\u00b7ge\u00b7sez\u00b7ten", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Nicht recht gehorchen will, aus Furcht, da\u00df Ha\u00df und Neid,", "tokens": ["Nicht", "recht", "ge\u00b7hor\u00b7chen", "will", ",", "aus", "Furcht", ",", "da\u00df", "Ha\u00df", "und", "Neid", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VVINF", "VMFIN", "$,", "APPR", "NN", "$,", "KOUS", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der mich zum Ziele macht, die schlechte Schuldigkeit", "tokens": ["Der", "mich", "zum", "Zie\u00b7le", "macht", ",", "die", "schlech\u00b7te", "Schul\u00b7dig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PPER", "APPRART", "NN", "VVFIN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So wie schon meinen Ruhm durch Peitsch und Ruthen jage.", "tokens": ["So", "wie", "schon", "mei\u00b7nen", "Ruhm", "durch", "Peitsch", "und", "Ru\u00b7then", "ja\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ADV", "PPOSAT", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-++", "measure": "unknown.measure.septa"}, "line.8": {"text": "Doch, da\u00df ich eisern bin und nun nach nichts mehr frage,", "tokens": ["Doch", ",", "da\u00df", "ich", "ei\u00b7sern", "bin", "und", "nun", "nach", "nichts", "mehr", "fra\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "KON", "ADV", "APPR", "PIS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Was Gift und Tadelsucht vor Wafen auf mich dr\u00fcckt,", "tokens": ["Was", "Gift", "und", "Ta\u00b7del\u00b7sucht", "vor", "Wa\u00b7fen", "auf", "mich", "dr\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "KON", "NN", "APPR", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Das lehrt mich die Gedult; und was mich jezt entz\u00fcckt,", "tokens": ["Das", "lehrt", "mich", "die", "Ge\u00b7dult", ";", "und", "was", "mich", "jezt", "ent\u00b7z\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ART", "NN", "$.", "KON", "PWS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Das ist die schnelle Post von deinem Wechselk\u00fc\u00dfen,", "tokens": ["Das", "ist", "die", "schnel\u00b7le", "Post", "von", "dei\u00b7nem", "Wech\u00b7sel\u00b7k\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die auch durch kalte Luft mich warm besuchen m\u00fc\u00dfen.", "tokens": ["Die", "auch", "durch", "kal\u00b7te", "Luft", "mich", "warm", "be\u00b7su\u00b7chen", "m\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ADJA", "NN", "PPER", "ADJD", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ich zweifle nicht daran, es werd auch dies dein Band", "tokens": ["Ich", "zweif\u00b7le", "nicht", "da\u00b7ran", ",", "es", "werd", "auch", "dies", "dein", "Band"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "PAV", "$,", "PPER", "VAFIN", "ADV", "PDS", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Der Leute M\u00e4hrlein seyn, denn wenn sich unser Stand", "tokens": ["Der", "Leu\u00b7te", "M\u00e4hr\u00b7lein", "seyn", ",", "denn", "wenn", "sich", "un\u00b7ser", "Stand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VAINF", "$,", "KON", "KOUS", "PRF", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Mit Myrthenkr\u00e4nzen flicht, so hat man nur zu leiden,", "tokens": ["Mit", "Myr\u00b7then\u00b7kr\u00e4n\u00b7zen", "flicht", ",", "so", "hat", "man", "nur", "zu", "lei\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,", "ADV", "VAFIN", "PIS", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Da\u00df Thoren unsrer Eh viel Folterb\u00e4ncke schneiden.", "tokens": ["Da\u00df", "Tho\u00b7ren", "uns\u00b7rer", "Eh", "viel", "Fol\u00b7ter\u00b7b\u00e4n\u00b7cke", "schnei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPOSAT", "NN", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Man bleibt nicht unberedt, wenn gleich nur unser Ku\u00df", "tokens": ["Man", "bleibt", "nicht", "un\u00b7be\u00b7redt", ",", "wenn", "gleich", "nur", "un\u00b7ser", "Ku\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PTKNEG", "ADJD", "$,", "KOUS", "ADV", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Aus Mod und H\u00f6fligkeit mit M\u00e4gdgen scherzen mu\u00df;", "tokens": ["Aus", "Mod", "und", "H\u00f6f\u00b7lig\u00b7keit", "mit", "M\u00e4gd\u00b7gen", "scher\u00b7zen", "mu\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "APPR", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Und wenn man ihre Hand nur unversehns ergrifen,", "tokens": ["Und", "wenn", "man", "ih\u00b7re", "Hand", "nur", "un\u00b7ver\u00b7sehns", "er\u00b7gri\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "PPOSAT", "NN", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "So ist das Zungenschwerd der Unschuld schon geschlifen.", "tokens": ["So", "ist", "das", "Zun\u00b7gen\u00b7schwerd", "der", "Un\u00b7schuld", "schon", "ge\u00b7schli\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Da heist man Br\u00e4utigam und weis es selber nicht,", "tokens": ["Da", "heist", "man", "Br\u00e4u\u00b7ti\u00b7gam", "und", "weis", "es", "sel\u00b7ber", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "NE", "KON", "PTKVZ", "PPER", "ADV", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Da wird ein Urtheltisch von Weibern aufgericht,", "tokens": ["Da", "wird", "ein", "Ur\u00b7thel\u00b7tisch", "von", "Wei\u00b7bern", "auf\u00b7ge\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Die, weil sie ihre Brunst nicht mehr vergn\u00fcgen k\u00f6nnen,", "tokens": ["Die", ",", "weil", "sie", "ih\u00b7re", "Brunst", "nicht", "mehr", "ver\u00b7gn\u00fc\u00b7gen", "k\u00f6n\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "PPER", "PPOSAT", "NN", "PTKNEG", "ADV", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Den J\u00fcngern keinen Scherz, geschweig ein M\u00e4ulchen g\u00f6nnen.", "tokens": ["Den", "J\u00fcn\u00b7gern", "kei\u00b7nen", "Scherz", ",", "ge\u00b7schweig", "ein", "M\u00e4ul\u00b7chen", "g\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "$,", "ADJD", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Die ganze Stadt wird voll, man lauft von Haus zu Haus,", "tokens": ["Die", "gan\u00b7ze", "Stadt", "wird", "voll", ",", "man", "lauft", "von", "Haus", "zu", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "$,", "PIS", "VVFIN", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Man breitets durch den Marckt und in dem Tempel aus,", "tokens": ["Man", "brei\u00b7tets", "durch", "den", "Marckt", "und", "in", "dem", "Tem\u00b7pel", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "ART", "NN", "KON", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Und kommt es allererst gar vor das Wochenbette,", "tokens": ["Und", "kommt", "es", "al\u00b7le\u00b7rerst", "gar", "vor", "das", "Wo\u00b7chen\u00b7bet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "So bei\u00df- und schlagen sich die Zungen um die Wette.", "tokens": ["So", "bei\u00df", "und", "schla\u00b7gen", "sich", "die", "Zun\u00b7gen", "um", "die", "Wet\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "TRUNC", "KON", "VVFIN", "PRF", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Wer weis, wie lang es w\u00e4hrt, so f\u00e4llt der Heuchler bey", "tokens": ["Wer", "weis", ",", "wie", "lang", "es", "w\u00e4hrt", ",", "so", "f\u00e4llt", "der", "Heuch\u00b7ler", "bey"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "PTKVZ", "$,", "PWAV", "ADJD", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "ART", "NN", "APPR"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Und macht von unsrer Lust ein solches Mordgeschrey,", "tokens": ["Und", "macht", "von", "uns\u00b7rer", "Lust", "ein", "sol\u00b7ches", "Mord\u00b7ge\u00b7schrey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Als wenn die Seeligkeit an einem Grife hinge", "tokens": ["Als", "wenn", "die", "See\u00b7lig\u00b7keit", "an", "ei\u00b7nem", "Gri\u00b7fe", "hin\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "ART", "NN", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Und unsre Faust bereits den Weg zur H\u00f6llen gienge.", "tokens": ["Und", "uns\u00b7re", "Faust", "be\u00b7reits", "den", "Weg", "zur", "H\u00f6l\u00b7len", "gien\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Exempel sind nicht rar, und mein verwandtes Blut", "tokens": ["Ex\u00b7em\u00b7pel", "sind", "nicht", "rar", ",", "und", "mein", "ver\u00b7wand\u00b7tes", "Blut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PTKNEG", "ADJD", "$,", "KON", "PPOSAT", "ADJA", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.34": {"text": "Erf\u00e4hrt die L\u00e4sterung der unbedachten Wut,", "tokens": ["Er\u00b7f\u00e4hrt", "die", "L\u00e4s\u00b7te\u00b7rung", "der", "un\u00b7be\u00b7dach\u00b7ten", "Wut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Die Seil aus Haaren macht und stets vor Eifer springet,", "tokens": ["Die", "Seil", "aus", "Haa\u00b7ren", "macht", "und", "stets", "vor", "Ei\u00b7fer", "sprin\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "KON", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Wenn ein getreues Herz dem andern Opfer bringet.", "tokens": ["Wenn", "ein", "ge\u00b7treu\u00b7es", "Herz", "dem", "an\u00b7dern", "Op\u00b7fer", "brin\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Du wirst, mein Br\u00e4utigam, aus allen Worten sehn,", "tokens": ["Du", "wirst", ",", "mein", "Br\u00e4u\u00b7ti\u00b7gam", ",", "aus", "al\u00b7len", "Wor\u00b7ten", "sehn", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PPOSAT", "NE", "$,", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Da\u00df meine Musen sich von keiner Galle blehn,", "tokens": ["Da\u00df", "mei\u00b7ne", "Mu\u00b7sen", "sich", "von", "kei\u00b7ner", "Gal\u00b7le", "blehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PRF", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Und kanst dir leicht dabey die sichre Rechnung machen,", "tokens": ["Und", "kanst", "dir", "leicht", "da\u00b7bey", "die", "sich\u00b7re", "Rech\u00b7nung", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADJD", "PAV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Da\u00df Groll und Unvernunft bey dir nicht be\u00dfer lachen.", "tokens": ["Da\u00df", "Groll", "und", "Un\u00b7ver\u00b7nunft", "bey", "dir", "nicht", "be\u00b7\u00dfer", "la\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word",