{"textgrid.poem.43892": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Getreue Magdalis, du forderst zwar den Zoll,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Getreue Magdalis, du forderst zwar den Zoll,", "tokens": ["Ge\u00b7treu\u00b7e", "Mag\u00b7da\u00b7lis", ",", "du", "for\u00b7derst", "zwar", "den", "Zoll", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der j\u00e4hrlich wiederkommt, zum gr\u00fcnen Donnerstage;", "tokens": ["Der", "j\u00e4hr\u00b7lich", "wie\u00b7der\u00b7kommt", ",", "zum", "gr\u00fc\u00b7nen", "Don\u00b7ners\u00b7ta\u00b7ge", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "$,", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch meine Hand weis nichts, was sie dir geben soll,", "tokens": ["Doch", "mei\u00b7ne", "Hand", "weis", "nichts", ",", "was", "sie", "dir", "ge\u00b7ben", "soll", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PTKVZ", "PIS", "$,", "PRELS", "PPER", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil ich in selbter nichts als Luft und Mangel trage.", "tokens": ["Weil", "ich", "in", "selb\u00b7ter", "nichts", "als", "Luft", "und", "Man\u00b7gel", "tra\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "PIS", "KOKOM", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Kein guter Marcipan, kein Mantel von Damast", "tokens": ["Kein", "gu\u00b7ter", "Mar\u00b7ci\u00b7pan", ",", "kein", "Man\u00b7tel", "von", "Da\u00b7mast"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "$,", "PIAT", "NN", "APPR", "NE"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "L\u00e4st meiner Armuth zu, dich reichlich zu bedencken,", "tokens": ["L\u00e4st", "mei\u00b7ner", "Ar\u00b7muth", "zu", ",", "dich", "reich\u00b7lich", "zu", "be\u00b7den\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PTKVZ", "$,", "PRF", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und weil du gestern schon mein Herz gestohlen hast,", "tokens": ["Und", "weil", "du", "ge\u00b7stern", "schon", "mein", "Herz", "ge\u00b7stoh\u00b7len", "hast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ADV", "PPOSAT", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "So steht es nicht bey mir, es heute dir zu schencken.", "tokens": ["So", "steht", "es", "nicht", "bey", "mir", ",", "es", "heu\u00b7te", "dir", "zu", "schen\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "APPR", "PPER", "$,", "PPER", "ADV", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Doch alles m\u00f6chte seyn, wenn mich die Poesie", "tokens": ["Doch", "al\u00b7les", "m\u00f6ch\u00b7te", "seyn", ",", "wenn", "mich", "die", "Poe\u00b7sie"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VMFIN", "VAINF", "$,", "KOUS", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Kummers, deinen Wuntsch zu stillen, \u00fcberh\u00fcbe.", "tokens": ["Des", "Kum\u00b7mers", ",", "dei\u00b7nen", "Wunt\u00b7sch", "zu", "stil\u00b7len", ",", "\u00fc\u00b7berh\u00b7\u00fc\u00b7be", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$,", "VVFIN", "$."], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Allein sie wegert sich; drum nimm, wo nicht zu fr\u00fch,", "tokens": ["Al\u00b7lein", "sie", "we\u00b7gert", "sich", ";", "drum", "nimm", ",", "wo", "nicht", "zu", "fr\u00fch", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PRF", "$.", "PAV", "PTKVZ", "$,", "PWAV", "PTKNEG", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Hier die Best\u00e4ndigkeit von meiner reinen Liebe.", "tokens": ["Hier", "die", "Be\u00b7st\u00e4n\u00b7dig\u00b7keit", "von", "mei\u00b7ner", "rei\u00b7nen", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}