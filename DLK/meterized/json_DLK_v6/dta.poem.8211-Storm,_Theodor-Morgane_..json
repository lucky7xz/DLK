{"dta.poem.8211": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Morgane .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1852", "urn": "urn:nbn:de:kobv:b4-200905196098", "language": ["de:0.99"], "booktitle": "Storm, Theodor: Gedichte. Kiel, 1852."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn Luft und Fluth zusammenragen", "tokens": ["Wenn", "Luft", "und", "Fluth", "zu\u00b7sam\u00b7men\u00b7ra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "KON", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und ohne Regung schl\u00e4ft die See,", "tokens": ["Und", "oh\u00b7ne", "Re\u00b7gung", "schl\u00e4ft", "die", "See", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dann steht an unserm grauen Strande", "tokens": ["Dann", "steht", "an", "un\u00b7serm", "grau\u00b7en", "Stran\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das Wunder aus dem Morgenlande,", "tokens": ["Das", "Wun\u00b7der", "aus", "dem", "Mor\u00b7gen\u00b7lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Morgane, die berufne Fee.", "tokens": ["Mor\u00b7ga\u00b7ne", ",", "die", "be\u00b7ruf\u00b7ne", "Fee", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Arglistig halb und halb von Sinne,", "tokens": ["Arg\u00b7lis\u00b7tig", "halb", "und", "halb", "von", "Sin\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "KON", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Verschmachtend nach dem Kelch der Minne,", "tokens": ["Ver\u00b7schmach\u00b7tend", "nach", "dem", "Kelch", "der", "Min\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der stets an ihrem Mund versiegt,", "tokens": ["Der", "stets", "an", "ih\u00b7rem", "Mund", "ver\u00b7siegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Umgaukelt sie des Wandrers Pfade,", "tokens": ["Um\u00b7gau\u00b7kelt", "sie", "des", "Wand\u00b7rers", "Pfa\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und lockt ihn an ein Scheingestade,", "tokens": ["Und", "lockt", "ihn", "an", "ein", "Schein\u00b7ge\u00b7sta\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das in des Todes Reichen liegt.", "tokens": ["Das", "in", "des", "To\u00b7des", "Rei\u00b7chen", "liegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Von ihrem Zauberspiel geblendet", "tokens": ["Von", "ih\u00b7rem", "Zau\u00b7ber\u00b7spiel", "ge\u00b7blen\u00b7det"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ruht manches Haupt in Nacht gewendet", "tokens": ["Ruht", "man\u00b7ches", "Haupt", "in", "Nacht", "ge\u00b7wen\u00b7det"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIAT", "NN", "APPR", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Begraben in der W\u00fcste Schlucht ;", "tokens": ["Be\u00b7gra\u00b7ben", "in", "der", "W\u00fcs\u00b7te", "Schlucht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Denn ihre Liebe ist Verderben,", "tokens": ["Denn", "ih\u00b7re", "Lie\u00b7be", "ist", "Ver\u00b7der\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ihr Hauch ist Gift, ihr Ku\u00df ist Sterben,", "tokens": ["Ihr", "Hauch", "ist", "Gift", ",", "ihr", "Ku\u00df", "ist", "Ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "$,", "PPOSAT", "NN", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die sch\u00f6nen Augen sind verflucht.", "tokens": ["Die", "sch\u00f6\u00b7nen", "Au\u00b7gen", "sind", "ver\u00b7flucht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "So steht sie jetzt im hohen Norden", "tokens": ["So", "steht", "sie", "jetzt", "im", "ho\u00b7hen", "Nor\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "An unsres Meeres dunkeln Borden,", "tokens": ["An", "uns\u00b7res", "Mee\u00b7res", "dun\u00b7keln", "Bor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So schreibt sie fingernd in den Dunst;", "tokens": ["So", "schreibt", "sie", "fin\u00b7gernd", "in", "den", "Dunst", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und quellend aus den luftgen Spuren", "tokens": ["Und", "quel\u00b7lend", "aus", "den", "luft\u00b7gen", "Spu\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Erstehn in d\u00e4mmernden Conturen", "tokens": ["Er\u00b7stehn", "in", "d\u00e4m\u00b7mern\u00b7den", "Con\u00b7tu\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Bilder ihrer argen Kunst.", "tokens": ["Die", "Bil\u00b7der", "ih\u00b7rer", "ar\u00b7gen", "Kunst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Doch hebt sich nicht wie dort im S\u00fcden", "tokens": ["Doch", "hebt", "sich", "nicht", "wie", "dort", "im", "S\u00fc\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "PTKNEG", "KOKOM", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auf rosigen Karyatiden", "tokens": ["Auf", "ro\u00b7si\u00b7gen", "Ka\u00b7ry\u00b7a\u00b7ti\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Wunderm\u00e4rchenschlo\u00df in's Blau;", "tokens": ["Ein", "Wun\u00b7der\u00b7m\u00e4r\u00b7chen\u00b7schlo\u00df", "in's", "Blau", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nur eines Haubergs graues Bildni\u00df", "tokens": ["Nur", "ei\u00b7nes", "Hau\u00b7bergs", "grau\u00b7es", "Bild\u00b7ni\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Schwimmt einsam in der Nebelwildni\u00df,", "tokens": ["Schwimmt", "ein\u00b7sam", "in", "der", "Ne\u00b7bel\u00b7wild\u00b7ni\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und keinen lockt der Hexenbau.", "tokens": ["Und", "kei\u00b7nen", "lockt", "der", "He\u00b7xen\u00b7bau", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Bald wechselt sie die dunkle K\u00fcste", "tokens": ["Bald", "wech\u00b7selt", "sie", "die", "dunk\u00b7le", "K\u00fcs\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Libyens sonnengelber W\u00fcste", "tokens": ["Mit", "Li\u00b7by\u00b7ens", "son\u00b7nen\u00b7gel\u00b7ber", "W\u00fcs\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "KON", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und mit der Tropenw\u00e4lder Duft;", "tokens": ["Und", "mit", "der", "Tro\u00b7pen\u00b7w\u00e4l\u00b7der", "Duft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dann bl\u00e4s't sie lachend durch die H\u00e4nde,", "tokens": ["Dann", "bl\u00e4s't", "sie", "la\u00b7chend", "durch", "die", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Dann schwankt das Haus, und Fach und W\u00e4nde", "tokens": ["Dann", "schwankt", "das", "Haus", ",", "und", "Fach", "und", "W\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Verrinnen quirlend in die Luft.", "tokens": ["Ver\u00b7rin\u00b7nen", "quir\u00b7lend", "in", "die", "Luft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}