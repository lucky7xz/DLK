{"textgrid.poem.41625": {"metadata": {"author": {"name": "Baudelaire, Charles", "birth": "N.A.", "death": "N.A."}, "title": "I.", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer tilgt den alten Fluch der Schuld, der an uns zehrt,", "tokens": ["Wer", "tilgt", "den", "al\u00b7ten", "Fluch", "der", "Schuld", ",", "der", "an", "uns", "zehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der sich windet und nimmer will sterben,", "tokens": ["Der", "sich", "win\u00b7det", "und", "nim\u00b7mer", "will", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "VVFIN", "KON", "ADV", "VMFIN", "VVINF", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Von unsrem Blut sich wie der Wurm von Leichen n\u00e4hrt,", "tokens": ["Von", "uns\u00b7rem", "Blut", "sich", "wie", "der", "Wurm", "von", "Lei\u00b7chen", "n\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PRF", "KOKOM", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Gleichwie Raupen, die B\u00e4ume verderben?", "tokens": ["Gleich\u00b7wie", "Rau\u00b7pen", ",", "die", "B\u00e4u\u00b7me", "ver\u00b7der\u00b7ben", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Wer tilgt den alten Fluch der Schuld, der an uns zehrt?", "tokens": ["Wer", "tilgt", "den", "al\u00b7ten", "Fluch", "der", "Schuld", ",", "der", "an", "uns", "zehrt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Durch welchen Wein, durch welch Gebr\u00e4u, durch welche Tr\u00e4nke", "tokens": ["Durch", "wel\u00b7chen", "Wein", ",", "durch", "welch", "Ge\u00b7br\u00e4u", ",", "durch", "wel\u00b7che", "Tr\u00e4n\u00b7ke"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PWAT", "NN", "$,", "APPR", "PWAT", "NN", "$,", "APPR", "PWAT", "NN"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Wird der Peiniger eingelullt,", "tokens": ["Wird", "der", "Pei\u00b7ni\u00b7ger", "ein\u00b7ge\u00b7lullt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Der Kurtisane gleich voll Gier und finstrer R\u00e4nke,", "tokens": ["Der", "Kur\u00b7ti\u00b7sa\u00b7ne", "gleich", "voll", "Gier", "und", "finst\u00b7rer", "R\u00e4n\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Ameise gleich an Geduld?", "tokens": ["Der", "A\u00b7mei\u00b7se", "gleich", "an", "Ge\u00b7duld", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Durch welchen Wein, durch welch Gebr\u00e4u, durch welche Tr\u00e4nke?", "tokens": ["Durch", "wel\u00b7chen", "Wein", ",", "durch", "welch", "Ge\u00b7br\u00e4u", ",", "durch", "wel\u00b7che", "Tr\u00e4n\u00b7ke", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "$,", "APPR", "PWAT", "NN", "$,", "APPR", "PWAT", "NN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}, "stanza.3": {"line.1": {"text": "Sags, sch\u00f6ne Zauberin, o sag es, wenn dirs kund,", "tokens": ["Sags", ",", "sch\u00f6\u00b7ne", "Zau\u00b7be\u00b7rin", ",", "o", "sag", "es", ",", "wenn", "dirs", "kund", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,", "FM", "VVFIN", "PPER", "$,", "KOUS", "PIS", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Diesem Geist, den die \u00c4ngste umkrampfen,", "tokens": ["Die\u00b7sem", "Geist", ",", "den", "die", "\u00c4ngs\u00b7te", "um\u00b7kramp\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PRELS", "ART", "NN", "VVINF", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Dem Sterbenden, bedeckt von Leichen, todeswund,", "tokens": ["Dem", "Ster\u00b7ben\u00b7den", ",", "be\u00b7deckt", "von", "Lei\u00b7chen", ",", "to\u00b7des\u00b7wund", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "VVPP", "APPR", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den der Pferde Hufe zerstampfen,", "tokens": ["Den", "der", "Pfer\u00b7de", "Hu\u00b7fe", "zer\u00b7stamp\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "NN", "VVPP", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Sags, sch\u00f6ne Zauberin, o sag es, wenn dirs kund,", "tokens": ["Sags", ",", "sch\u00f6\u00b7ne", "Zau\u00b7be\u00b7rin", ",", "o", "sag", "es", ",", "wenn", "dirs", "kund", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,", "FM", "VVFIN", "PPER", "$,", "KOUS", "PIS", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Sag es dem R\u00f6chelnden, den W\u00f6lfe schon umlauern,", "tokens": ["Sag", "es", "dem", "R\u00f6\u00b7cheln\u00b7den", ",", "den", "W\u00f6l\u00b7fe", "schon", "um\u00b7lau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "$,", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den kr\u00e4chzend der Rabe umschwirrt,", "tokens": ["Den", "kr\u00e4ch\u00b7zend", "der", "Ra\u00b7be", "um\u00b7schwirrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Sags dem Zertr\u00fcmmerten, da\u00df er in Todesschauern", "tokens": ["Sags", "dem", "Zer\u00b7tr\u00fcm\u00b7mer\u00b7ten", ",", "da\u00df", "er", "in", "To\u00b7des\u00b7schau\u00b7ern"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "$,", "KOUS", "PPER", "APPR", "NN"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Verzagt, da\u00df ein Grabmal ihm wird;", "tokens": ["Ver\u00b7zagt", ",", "da\u00df", "ein", "Grab\u00b7mal", "ihm", "wird", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KOUS", "ART", "NN", "PPER", "VAFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Dem armen R\u00f6chelnden, den W\u00f6lfe schon umlauern!", "tokens": ["Dem", "ar\u00b7men", "R\u00f6\u00b7cheln\u00b7den", ",", "den", "W\u00f6l\u00b7fe", "schon", "um\u00b7lau\u00b7ern", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Wird je ein Himmel bl\u00fchn, der schwarz wie Schlamm und tot?", "tokens": ["Wird", "je", "ein", "Him\u00b7mel", "bl\u00fchn", ",", "der", "schwarz", "wie", "Schlamm", "und", "tot", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "VVINF", "$,", "PRELS", "ADJD", "KOKOM", "NN", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und kannst du zerrei\u00dfen das Dunkel,", "tokens": ["Und", "kannst", "du", "zer\u00b7rei\u00b7\u00dfen", "das", "Dun\u00b7kel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Das z\u00e4h wie Pech, und wo kein Fr\u00fch- noch Abendrot,", "tokens": ["Das", "z\u00e4h", "wie", "Pech", ",", "und", "wo", "kein", "Fr\u00fch", "noch", "A\u00b7ben\u00b7drot", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KOKOM", "NE", "$,", "KON", "PWAV", "PIAT", "TRUNC", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nicht Blitze noch Sternengefunkel?", "tokens": ["Nicht", "Blit\u00b7ze", "noch", "Ster\u00b7nen\u00b7ge\u00b7fun\u00b7kel", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "ADV", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Wird je ein Himmel bl\u00fchn, der schwarz wie Schlamm und tot?", "tokens": ["Wird", "je", "ein", "Him\u00b7mel", "bl\u00fchn", ",", "der", "schwarz", "wie", "Schlamm", "und", "tot", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "VVINF", "$,", "PRELS", "ADJD", "KOKOM", "NN", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Der Hoffnung Licht, das aus der Herberg aufgeglommen,", "tokens": ["Der", "Hoff\u00b7nung", "Licht", ",", "das", "aus", "der", "Her\u00b7berg", "auf\u00b7ge\u00b7glom\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verlosch, da kaum wirs gewahrt!", "tokens": ["Ver\u00b7losch", ",", "da", "kaum", "wirs", "ge\u00b7wahrt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "KOUS", "ADV", "PIS", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Wie sollen ohne Mond noch Strahl zur Pforte kommen", "tokens": ["Wie", "sol\u00b7len", "oh\u00b7ne", "Mond", "noch", "Strahl", "zur", "Pfor\u00b7te", "kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "APPR", "NN", "ADV", "NN", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Dulder der b\u00f6sen Fahrt?", "tokens": ["Die", "Dul\u00b7der", "der", "b\u00f6\u00b7sen", "Fahrt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Der Satan losch das Licht, das hell uns aufgeglommen!", "tokens": ["Der", "Sa\u00b7tan", "losch", "das", "Licht", ",", "das", "hell", "uns", "auf\u00b7ge\u00b7glom\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ART", "NN", "$,", "PRELS", "ADJD", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Liebst, holde Zauberin, du der Verdammten Qual,", "tokens": ["Liebst", ",", "hol\u00b7de", "Zau\u00b7be\u00b7rin", ",", "du", "der", "Ver\u00b7damm\u00b7ten", "Qual", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kennst du des Unheilbaren Schmerzen?", "tokens": ["Kennst", "du", "des", "Un\u00b7heil\u00b7ba\u00b7ren", "Schmer\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Den Fluch der alten Schuld mit seinem giftgen Stahl,", "tokens": ["Den", "Fluch", "der", "al\u00b7ten", "Schuld", "mit", "sei\u00b7nem", "gift\u00b7gen", "Stahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den er st\u00f6\u00dft in unsere Herzen?", "tokens": ["Den", "er", "st\u00f6\u00dft", "in", "un\u00b7se\u00b7re", "Her\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Liebst, holde Zauberin, du der Verdammten Qual?", "tokens": ["Liebst", ",", "hol\u00b7de", "Zau\u00b7be\u00b7rin", ",", "du", "der", "Ver\u00b7damm\u00b7ten", "Qual", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Unwiederbringliches nagt mit verruchtem Bisse", "tokens": ["Un\u00b7wie\u00b7der\u00b7bring\u00b7li\u00b7ches", "nagt", "mit", "ver\u00b7ruch\u00b7tem", "Bis\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "VVFIN", "APPR", "ADJA", "NN"], "meter": "---+--+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Unsres Geistes zerbrechliches Haus,", "tokens": ["Uns\u00b7res", "Geis\u00b7tes", "zer\u00b7brech\u00b7li\u00b7ches", "Haus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Und den Termiten gleich fri\u00dft es geheime Risse", "tokens": ["Und", "den", "Ter\u00b7mi\u00b7ten", "gleich", "fri\u00dft", "es", "ge\u00b7hei\u00b7me", "Ris\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADV", "VVFIN", "PPER", "ADJA", "NN"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "In die Fundamente des Baus.", "tokens": ["In", "die", "Fun\u00b7da\u00b7men\u00b7te", "des", "Baus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Unwiederbringliches nagt mit verruchtem Bisse!", "tokens": ["Un\u00b7wie\u00b7der\u00b7bring\u00b7li\u00b7ches", "nagt", "mit", "ver\u00b7ruch\u00b7tem", "Bis\u00b7se", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "---+--+--+-+-", "measure": "iambic.tetra.relaxed"}}}}}