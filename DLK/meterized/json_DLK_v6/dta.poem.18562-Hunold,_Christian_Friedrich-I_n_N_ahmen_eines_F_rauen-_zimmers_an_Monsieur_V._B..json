{"dta.poem.18562": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "I n  N ahmen eines  F rauen-  \n zimmers an  Monsieur V. B.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1702", "urn": "urn:nbn:de:kobv:b4-200905197766", "language": ["de:0.99"], "booktitle": "Hunold, Christian Friedrich: Die Edle Bem\u00fchung m\u00fcssiger Stunden. Hamburg, 1702."}, "poem": {"stanza.1": {"line.1": {"text": "So soll ich mich in rauher Einsamkeit", "tokens": ["So", "soll", "ich", "mich", "in", "rau\u00b7her", "Ein\u00b7sam\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mein Wehrtester/ nun gantz verlassen sehen?", "tokens": ["Mein", "Wehr\u00b7tes\u00b7ter", "/", "nun", "gantz", "ver\u00b7las\u00b7sen", "se\u00b7hen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "ADV", "ADV", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So wechselt sich die Lust mit Angst und Leid?", "tokens": ["So", "wech\u00b7selt", "sich", "die", "Lust", "mit", "Angst", "und", "Leid", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So mu\u00df die Brust in tieffster Trauer gehen?", "tokens": ["So", "mu\u00df", "die", "Brust", "in", "tieffs\u00b7ter", "Trau\u00b7er", "ge\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ach dencke nur dadurch zerfliest in Schmertz", "tokens": ["Ach", "den\u00b7cke", "nur", "da\u00b7durch", "zer\u00b7fliest", "in", "Schmertz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "VVFIN", "ADV", "PAV", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ein treues Hertz.", "tokens": ["Ein", "treu\u00b7es", "Hertz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}