{"dta.poem.736": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "XxII.  An  Engenien.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Was wundert jhr euch noch/ Jhr Rose der Jungfrawen ", "tokens": ["Was", "wun\u00b7dert", "jhr", "euch", "noch", "/", "Ihr", "Ro\u00b7se", "der", "Jung\u00b7fra\u00b7wen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "PPER", "ADV", "$(", "PPOSAT", "NN", "ART", "NN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Das dieses Spiel der zeit/ die Ros/ in ewrer ", "tokens": ["Das", "die\u00b7ses", "Spiel", "der", "zeit", "/", "die", "Ros", "/", "in", "ew\u00b7rer"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PDS", "PDAT", "NN", "ART", "NN", "$(", "ART", "NN", "$(", "APPR", "PPOSAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die alle Rosen trotzt/ so vnversehens verschwandt? ", "tokens": ["Die", "al\u00b7le", "Ro\u00b7sen", "trotzt", "/", "so", "vn\u00b7ver\u00b7se\u00b7hens", "ver\u00b7schwandt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$(", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.4": {"text": "Eugenie so gehts/ so schwindet was wir schawen.", "tokens": ["Eu\u00b7ge\u00b7nie", "so", "gehts", "/", "so", "schwin\u00b7det", "was", "wir", "scha\u00b7wen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "$(", "ADV", "VVFIN", "PWS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "So bald de\u00df Todes Sen\u00df wird diesen Leib abhawen:", "tokens": ["So", "bald", "de\u00df", "To\u00b7des", "Sen\u00df", "wird", "die\u00b7sen", "Leib", "ab\u00b7ha\u00b7wen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "NE", "VAFIN", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-++-+", "measure": "unknown.measure.septa"}, "line.2": {"text": "Mu\u00df dieser ", "tokens": ["Mu\u00df", "die\u00b7ser"], "token_info": ["word", "word"], "pos": ["VMFIN", "PDAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Der liebe diese Brust/ der Mund/ der Arm in sand/", "tokens": ["Der", "lie\u00b7be", "die\u00b7se", "Brust", "/", "der", "Mund", "/", "der", "Arm", "in", "sand", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PDAT", "NN", "$(", "ART", "NN", "$(", "ART", "NN", "APPR", "NE", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vnd dem/ der euch mit lieb jtzt ehrt/ wird f\u00fcr euch grawen!", "tokens": ["Vnd", "dem", "/", "der", "euch", "mit", "lieb", "jtzt", "ehrt", "/", "wird", "f\u00fcr", "euch", "gra\u00b7wen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "$(", "PRELS", "PPER", "APPR", "ADJD", "ADV", "VVFIN", "$(", "VAFIN", "APPR", "PPER", "VVINF", "$."], "meter": "-+---+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Der seufftzer ist vmbsonst/ nichts ist das auff der welt/", "tokens": ["Der", "seufft\u00b7zer", "ist", "vmbsonst", "/", "nichts", "ist", "das", "auff", "der", "welt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "ADV", "$(", "PIS", "VAFIN", "ART", "APPR", "ART", "NN", "$("], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wie sch\u00f6n es jm\u0303mer sey bestand vnd farbe h\u00e4lt/ ", "tokens": ["Wie", "sch\u00f6n", "es", "jm\u0303\u00b7mer", "sey", "be\u00b7stand", "vnd", "far\u00b7be", "h\u00e4lt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "ADV", "VAFIN", "VVFIN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wir sind von Mutterleib zum vntergang erkohren.", "tokens": ["Wir", "sind", "von", "Mut\u00b7ter\u00b7leib", "zum", "vn\u00b7ter\u00b7gang", "er\u00b7koh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Mag auch an Sch\u00f6nheit was der Rosen gleiche seyn?", "tokens": ["Mag", "auch", "an", "Sch\u00f6n\u00b7heit", "was", "der", "Ro\u00b7sen", "glei\u00b7che", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPR", "NN", "PWS", "ART", "NN", "ADJA", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch ehe sie recht bl\u00fcht verwelckt vnd f\u00e4lt sie eyn!", "tokens": ["Doch", "e\u00b7he", "sie", "recht", "bl\u00fcht", "ver\u00b7welckt", "vnd", "f\u00e4lt", "sie", "eyn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "VVFIN", "VVPP", "KON", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nicht anders gehn wir fort/ so bald wir sind gebohren.", "tokens": ["Nicht", "an\u00b7ders", "gehn", "wir", "fort", "/", "so", "bald", "wir", "sind", "ge\u00b7boh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVFIN", "PPER", "PTKVZ", "$(", "ADV", "ADV", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}