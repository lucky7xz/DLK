{"textgrid.poem.39408": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Qu\u00e4lend ungestilltes Sehnen", "genre": "verse", "period": "N.A.", "pub_year": 1791, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Qu\u00e4lend ungestilltes Sehnen", "tokens": ["Qu\u00e4\u00b7lend", "un\u00b7ge\u00b7still\u00b7tes", "Seh\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Pocht mir in emp\u00f6rter Brust.", "tokens": ["Pocht", "mir", "in", "em\u00b7p\u00f6r\u00b7ter", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Liebe, die mir Seel' und Sinnen", "tokens": ["Lie\u00b7be", ",", "die", "mir", "Seel'", "und", "Sin\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "PPER", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schmeichelnd wu\u00dfte zu gewinnen,", "tokens": ["Schmei\u00b7chelnd", "wu\u00df\u00b7te", "zu", "ge\u00b7win\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wiegt dein zauberisches W\u00e4hnen", "tokens": ["Wiegt", "dein", "zau\u00b7be\u00b7ri\u00b7sches", "W\u00e4h\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nur in Tr\u00e4ume kurzer Lust,", "tokens": ["Nur", "in", "Tr\u00e4u\u00b7me", "kur\u00b7zer", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und erweckt zu Thr\u00e4nen?", "tokens": ["Und", "er\u00b7weckt", "zu", "Thr\u00e4\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "S\u00fc\u00df berauscht in Thr\u00e4nen", "tokens": ["S\u00fc\u00df", "be\u00b7rauscht", "in", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "An des Lieben Brust mich lehnen,", "tokens": ["An", "des", "Lie\u00b7ben", "Brust", "mich", "leh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Arm um Arm gestrickt,", "tokens": ["Arm", "um", "Arm", "ge\u00b7strickt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.11": {"text": "Mund auf Mund gedr\u00fcckt,", "tokens": ["Mund", "auf", "Mund", "ge\u00b7dr\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.12": {"text": "Das nur stillt mein Sehnen!", "tokens": ["Das", "nur", "stillt", "mein", "Seh\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Ach, ich gab ihm keine Kunde,", "tokens": ["Ach", ",", "ich", "gab", "ihm", "kei\u00b7ne", "Kun\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wu\u00dft' es selber nicht zuvor;", "tokens": ["Wu\u00dft'", "es", "sel\u00b7ber", "nicht", "zu\u00b7vor", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKNEG", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und nun beb' ich so beklommen:", "tokens": ["Und", "nun", "beb'", "ich", "so", "be\u00b7klom\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird der Traute, wird er kommen?", "tokens": ["Wird", "der", "Trau\u00b7te", ",", "wird", "er", "kom\u00b7men", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "VAFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Still und g\u00fcnstig ist die Stunde,", "tokens": ["Still", "und", "g\u00fcns\u00b7tig", "ist", "die", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nirgends droht ein horchend Ohr", "tokens": ["Nir\u00b7gends", "droht", "ein", "hor\u00b7chend", "Ohr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJD", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Dem geheimen Bunde.", "tokens": ["Dem", "ge\u00b7hei\u00b7men", "Bun\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Treu im sel'gen Bunde", "tokens": ["Treu", "im", "sel'\u00b7gen", "Bun\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "An des Lieben Brust mich lehnen,", "tokens": ["An", "des", "Lie\u00b7ben", "Brust", "mich", "leh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Arm um Arm gestrickt,", "tokens": ["Arm", "um", "Arm", "ge\u00b7strickt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.11": {"text": "Mund auf Mund gedr\u00fcckt,", "tokens": ["Mund", "auf", "Mund", "ge\u00b7dr\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.12": {"text": "Das nur stillt mein Sehnen.", "tokens": ["Das", "nur", "stillt", "mein", "Seh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "H\u00f6r' ich leise Tritte rauschen,", "tokens": ["H\u00f6r'", "ich", "lei\u00b7se", "Trit\u00b7te", "rau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Denk' ich: ah, da ist er schon!", "tokens": ["Denk'", "ich", ":", "ah", ",", "da", "ist", "er", "schon", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$.", "VVFIN", "$,", "ADV", "VAFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ahndung hat ihm wohl verk\u00fcndet,", "tokens": ["Ahn\u00b7dung", "hat", "ihm", "wohl", "ver\u00b7k\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df die sch\u00f6ne Zeit sich findet,", "tokens": ["Da\u00df", "die", "sch\u00f6\u00b7ne", "Zeit", "sich", "fin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wonn' um Wonne frei zu tauschen. \u2013", "tokens": ["Wonn'", "um", "Won\u00b7ne", "frei", "zu", "tau\u00b7schen", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "APPR", "NN", "ADJD", "PTKZU", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Doch sie ist schon halb entflohn", "tokens": ["Doch", "sie", "ist", "schon", "halb", "ent\u00b7flohn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "ADV", "ADJD", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Bei vergebnem Lauschen.", "tokens": ["Bei", "ver\u00b7geb\u00b7nem", "Lau\u00b7schen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Mit entz\u00fccktem Lauschen", "tokens": ["Mit", "ent\u00b7z\u00fcck\u00b7tem", "Lau\u00b7schen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "An des Lieben Brust mich lehnen,", "tokens": ["An", "des", "Lie\u00b7ben", "Brust", "mich", "leh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Arm um Arm gestrickt,", "tokens": ["Arm", "um", "Arm", "ge\u00b7strickt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.11": {"text": "Mund auf Mund gedr\u00fcckt,", "tokens": ["Mund", "auf", "Mund", "ge\u00b7dr\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.12": {"text": "Das nur stillt mein Sehnen.", "tokens": ["Das", "nur", "stillt", "mein", "Seh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "T\u00e4uschen wird vielleicht mein Sehnen,", "tokens": ["T\u00e4u\u00b7schen", "wird", "viel\u00b7leicht", "mein", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hofft' ich, des Gesanges Lust.", "tokens": ["Hofft'", "ich", ",", "des", "Ge\u00b7san\u00b7ges", "Lust", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ART", "ADJA", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Ungest\u00fcmer W\u00fcnsche Gl\u00fchen", "tokens": ["Un\u00b7ge\u00b7st\u00fc\u00b7mer", "W\u00fcn\u00b7sche", "Gl\u00fc\u00b7hen"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lindern sanfte Melodieen. \u2013", "tokens": ["Lin\u00b7dern", "sanf\u00b7te", "Me\u00b7lo\u00b7di\u00b7e\u00b7en", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["NN", "ADJA", "NN", "$.", "$("], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Doch das Lied enthob mit St\u00f6hnen", "tokens": ["Doch", "das", "Lied", "ent\u00b7hob", "mit", "St\u00f6h\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Tief erathmend sich der Brust,", "tokens": ["Tief", "e\u00b7rath\u00b7mend", "sich", "der", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "PRF", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und erstarb in Thr\u00e4nen.", "tokens": ["Und", "er\u00b7starb", "in", "Thr\u00e4\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "S\u00fc\u00df berauscht in Thr\u00e4nen", "tokens": ["S\u00fc\u00df", "be\u00b7rauscht", "in", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "An des Lieben Brust mich lehnen,", "tokens": ["An", "des", "Lie\u00b7ben", "Brust", "mich", "leh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Arm um Arm gestrickt,", "tokens": ["Arm", "um", "Arm", "ge\u00b7strickt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.11": {"text": "Mund auf Mund gedr\u00fcckt,", "tokens": ["Mund", "auf", "Mund", "ge\u00b7dr\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.12": {"text": "Das nur stillt mein Sehnen.", "tokens": ["Das", "nur", "stillt", "mein", "Seh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}