{"dta.poem.9514": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Heyraths-Gedancken.  \n C. H. v. H.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sol denn die traurigkeit den gantzen geist beschl\u00fcssen", "tokens": ["Sol", "denn", "die", "trau\u00b7rig\u00b7keit", "den", "gant\u00b7zen", "geist", "be\u00b7schl\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "ART", "NN", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und die gew\u00fcnschte zeit samt aller lust verfl\u00fcssen?", "tokens": ["Und", "die", "ge\u00b7w\u00fcnschte", "zeit", "samt", "al\u00b7ler", "lust", "ver\u00b7fl\u00fcs\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "--+-+-+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Sol denn die einsamkeit/ o meisterin der pein!", "tokens": ["Sol", "denn", "die", "ein\u00b7sam\u00b7keit", "/", "o", "meis\u00b7te\u00b7rin", "der", "pein", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "$(", "FM", "FM", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Des hanses bester schatz/ des bettes zierrath seyn?", "tokens": ["Des", "han\u00b7ses", "bes\u00b7ter", "schatz", "/", "des", "bet\u00b7tes", "zier\u00b7rath", "seyn", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$(", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Nein/ nein/ es m\u00fcssen nicht die sehnen m\u00fc\u00dfig liegen/", "tokens": ["Nein", "/", "nein", "/", "es", "m\u00fcs\u00b7sen", "nicht", "die", "seh\u00b7nen", "m\u00fc\u00b7\u00dfig", "lie\u00b7gen", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$(", "PTKANT", "$(", "PPER", "VMFIN", "PTKNEG", "ART", "VVINF", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Es soll das alter hier nicht ohne zeugen siegen.", "tokens": ["Es", "soll", "das", "al\u00b7ter", "hier", "nicht", "oh\u00b7ne", "zeu\u00b7gen", "sie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "ADV", "PTKNEG", "APPR", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Es ist gut einsam seyn/ wenn schnee das haupt bedeckt/", "tokens": ["Es", "ist", "gut", "ein\u00b7sam", "seyn", "/", "wenn", "schnee", "das", "haupt", "be\u00b7deckt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "ADJD", "VAINF", "$(", "KOUS", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ein winter-kaltes ey\u00df uns in den lenden steckt/", "tokens": ["Ein", "win\u00b7ter\u00b7kal\u00b7tes", "ey\u00df", "uns", "in", "den", "len\u00b7den", "steckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+---+", "measure": "unknown.measure.penta"}, "line.9": {"text": "Und uns der zeiten zahn die besten adern r\u00fchret.", "tokens": ["Und", "uns", "der", "zei\u00b7ten", "zahn", "die", "bes\u00b7ten", "a\u00b7dern", "r\u00fch\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wer sonnen-heisse glut noch in den sehnen f\u00fchret/", "tokens": ["Wer", "son\u00b7nen\u00b7heis\u00b7se", "glut", "noch", "in", "den", "seh\u00b7nen", "f\u00fch\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "NN", "ADV", "APPR", "ART", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Der tret in zuversicht ein gleiches wesen an/", "tokens": ["Der", "tret", "in", "zu\u00b7ver\u00b7sicht", "ein", "glei\u00b7ches", "we\u00b7sen", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "So glut zu halten wei\u00df/ und glut erregen kan.", "tokens": ["So", "glut", "zu", "hal\u00b7ten", "wei\u00df", "/", "und", "glut", "er\u00b7re\u00b7gen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKZU", "VVINF", "VVFIN", "$(", "KON", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ist doch in dieser stadt noch wohl ein bild zu finden/", "tokens": ["Ist", "doch", "in", "die\u00b7ser", "stadt", "noch", "wohl", "ein", "bild", "zu", "fin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PDAT", "NN", "ADV", "ADV", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "So die aus Cyperu kan mit sch\u00f6nheit \u00fcberwinden/", "tokens": ["So", "die", "aus", "Cy\u00b7pe\u00b7ru", "kan", "mit", "sch\u00f6n\u00b7heit", "\u00fc\u00b7berw\u00b7in\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "APPR", "NE", "VMFIN", "APPR", "NN", "VVINF", "$("], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.15": {"text": "Bey welcher der corall den s\u00fcssen mund bedeckt/", "tokens": ["Bey", "wel\u00b7cher", "der", "co\u00b7rall", "den", "s\u00fcs\u00b7sen", "mund", "be\u00b7deckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "NN", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Ja selbst der sonnen krafft in beyden augen steckt/", "tokens": ["Ja", "selbst", "der", "son\u00b7nen", "krafft", "in", "bey\u00b7den", "au\u00b7gen", "steckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "ART", "ADJA", "NN", "APPR", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Von welcher lippen nichts als ancker-tropffen fallen/", "tokens": ["Von", "wel\u00b7cher", "lip\u00b7pen", "nichts", "als", "an\u00b7cker\u00b7tropf\u00b7fen", "fal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "PIS", "KOKOM", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "So schnee auff ihrer brust vermischet mit corallen.", "tokens": ["So", "schnee", "auff", "ih\u00b7rer", "brust", "ver\u00b7mi\u00b7schet", "mit", "co\u00b7ral\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "VVFIN", "APPR", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Und welcher mit verdienst das hohe lob geb\u00fchrt/", "tokens": ["Und", "wel\u00b7cher", "mit", "ver\u00b7dienst", "das", "ho\u00b7he", "lob", "ge\u00b7b\u00fchrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAT", "APPR", "VVFIN", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Da\u00df sie den schwanen selbst die farbe hat entf\u00fchrt.", "tokens": ["Da\u00df", "sie", "den", "schwa\u00b7nen", "selbst", "die", "far\u00b7be", "hat", "ent\u00b7f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "ADV", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Das ist ein zeitvertreib/ so die erwehlen sollen/", "tokens": ["Das", "ist", "ein", "zeit\u00b7ver\u00b7treib", "/", "so", "die", "er\u00b7weh\u00b7len", "sol\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$(", "ADV", "ART", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Die in der sterbligkeit mit recht verfahren wollen.", "tokens": ["Die", "in", "der", "ster\u00b7blig\u00b7keit", "mit", "recht", "ver\u00b7fah\u00b7ren", "wol\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "APPR", "ADJD", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Must alles fleisch gepaart in Noens kasten gehn/", "tokens": ["Must", "al\u00b7les", "fleisch", "ge\u00b7paart", "in", "No\u00b7ens", "kas\u00b7ten", "gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ADJD", "VVPP", "APPR", "NE", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Wie will man ungepaart in dieser welt bestehn?", "tokens": ["Wie", "will", "man", "un\u00b7ge\u00b7paart", "in", "die\u00b7ser", "welt", "be\u00b7stehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PIS", "ADJD", "APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}