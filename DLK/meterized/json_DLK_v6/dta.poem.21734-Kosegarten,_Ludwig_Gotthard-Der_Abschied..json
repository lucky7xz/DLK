{"dta.poem.21734": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Der Abschied.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1802", "urn": "urn:nbn:de:kobv:b4-200905193310", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ihr gottgeweihten Mauern,", "tokens": ["Ihr", "gott\u00b7ge\u00b7weih\u00b7ten", "Mau\u00b7ern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihr, deren ernster Ring,", "tokens": ["Ihr", ",", "de\u00b7ren", "erns\u00b7ter", "Ring", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Durchweht von heil'gen Schauern,", "tokens": ["Durch\u00b7weht", "von", "heil'\u00b7gen", "Schau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Eilf Sommer mich umfing;", "tokens": ["Eilf", "Som\u00b7mer", "mich", "um\u00b7fing", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ihr kl\u00f6sterlichen Klausen,", "tokens": ["Ihr", "kl\u00f6s\u00b7ter\u00b7li\u00b7chen", "Klau\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wo Ruh und Andacht hausen,", "tokens": ["Wo", "Ruh", "und", "An\u00b7dacht", "hau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Die ich jetzt lassen soll \u2014", "tokens": ["Die", "ich", "jetzt", "las\u00b7sen", "soll"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVINF", "VMFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Gehabt euch wohl!", "tokens": ["Ge\u00b7habt", "euch", "wohl", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Ihr immer gr\u00fcnen B\u00e4ume,", "tokens": ["Ihr", "im\u00b7mer", "gr\u00fc\u00b7nen", "B\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die ihr mir freundlich lauscht", "tokens": ["Die", "ihr", "mir", "freund\u00b7lich", "lauscht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "PPER", "ADJD", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und oft in s\u00fcsse Tr\u00e4ume", "tokens": ["Und", "oft", "in", "s\u00fcs\u00b7se", "Tr\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Schw\u00e4rmende gerauscht;", "tokens": ["Die", "Schw\u00e4r\u00b7men\u00b7de", "ge\u00b7rauscht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ihr duftenden Geb\u00fcsche", "tokens": ["Ihr", "duf\u00b7ten\u00b7den", "Ge\u00b7b\u00fc\u00b7sche"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Voll K\u00fchlung und voll Frische,", "tokens": ["Voll", "K\u00fch\u00b7lung", "und", "voll", "Fri\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "ADJD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Voll Lispeln und Gesang,", "tokens": ["Voll", "Lis\u00b7peln", "und", "Ge\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Habt Dank! Habt Dank!", "tokens": ["Habt", "Dank", "!", "Habt", "Dank", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$.", "VAFIN", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Ihr Blumen und ihr Quellen,", "tokens": ["Ihr", "Blu\u00b7men", "und", "ihr", "Quel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihr Rasen, seideweich,", "tokens": ["Ihr", "Ra\u00b7sen", ",", "sei\u00b7de\u00b7weich", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr kleinen Murmelwellen,", "tokens": ["Ihr", "klei\u00b7nen", "Mur\u00b7mel\u00b7wel\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nie, nie vergess ich euch.", "tokens": ["Nie", ",", "nie", "ver\u00b7gess", "ich", "euch", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wohl aus dem Weltgedr\u00e4nge,", "tokens": ["Wohl", "aus", "dem", "Welt\u00b7ge\u00b7dr\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Dem l\u00e4stigen Gepr\u00e4nge", "tokens": ["Dem", "l\u00e4s\u00b7ti\u00b7gen", "Ge\u00b7pr\u00e4n\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Schaut sehnend einst der Blick", "tokens": ["Schaut", "seh\u00b7nend", "einst", "der", "Blick"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Nach euch zur\u00fcck.", "tokens": ["Nach", "euch", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKVZ", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Ihr heiligen Jungfrauen,", "tokens": ["Ihr", "hei\u00b7li\u00b7gen", "Jung\u00b7frau\u00b7en", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die selig Tag und Nacht", "tokens": ["Die", "se\u00b7lig", "Tag", "und", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Antlitz Gottes schauen,", "tokens": ["Das", "Ant\u00b7litz", "Got\u00b7tes", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Habt mich in guter Acht!", "tokens": ["Habt", "mich", "in", "gu\u00b7ter", "Acht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Urbild der Huld und G\u00fcte,", "tokens": ["Ur\u00b7bild", "der", "Huld", "und", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "KON", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Madonna, ach beh\u00fcte", "tokens": ["Ma\u00b7don\u00b7na", ",", "ach", "be\u00b7h\u00fc\u00b7te"], "token_info": ["word", "punct", "word", "word"], "pos": ["NE", "$,", "ITJ", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Vor eitelm Wahn den Sinn", "tokens": ["Vor", "ei\u00b7telm", "Wahn", "den", "Sinn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Der S\u00fcnderinn!", "tokens": ["Der", "S\u00fcn\u00b7de\u00b7rinn", "!"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}