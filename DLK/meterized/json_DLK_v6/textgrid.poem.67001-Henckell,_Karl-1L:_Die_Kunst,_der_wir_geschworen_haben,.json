{"textgrid.poem.67001": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Kunst, der wir geschworen haben,", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Kunst, der wir geschworen haben,", "tokens": ["Die", "Kunst", ",", "der", "wir", "ge\u00b7schwo\u00b7ren", "ha\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie liegt von je im \u00bbSch\u00fctzengraben\u00ab,", "tokens": ["Sie", "liegt", "von", "je", "im", "\u00bb", "Sch\u00fct\u00b7zen\u00b7gra\u00b7ben", "\u00ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADV", "APPRART", "$(", "NN", "$(", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der uns im Blute steckt, der Geist,", "tokens": ["Der", "uns", "im", "Blu\u00b7te", "steckt", ",", "der", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VVFIN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist schon im Frieden \u00bbeingekreist\u00ab", "tokens": ["Ist", "schon", "im", "Frie\u00b7den", "\u00bb", "ein\u00b7ge\u00b7kreist", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "ADV", "APPRART", "NN", "$(", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und wird, wenn er nicht schmeichelnd lungert,", "tokens": ["Und", "wird", ",", "wenn", "er", "nicht", "schmei\u00b7chelnd", "lun\u00b7gert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "$,", "KOUS", "PPER", "PTKNEG", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Nach M\u00f6glichkeit auch \u00bbausgehungert\u00ab.", "tokens": ["Nach", "M\u00f6g\u00b7lich\u00b7keit", "auch", "\u00bb", "aus\u00b7ge\u00b7hun\u00b7gert", "\u00ab", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["APPR", "NN", "ADV", "$(", "VVPP", "$(", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "\u00bbdurchhalten\u00ab ist des Dichters Wort,", "tokens": ["\u00bb", "durch\u00b7hal\u00b7ten", "\u00ab", "ist", "des", "Dich\u00b7ters", "Wort", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$(", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der nicht um falschen Beifall schnorrt,", "tokens": ["Der", "nicht", "um", "fal\u00b7schen", "Bei\u00b7fall", "schnorrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Er hat im Kampfe mit der Welt", "tokens": ["Er", "hat", "im", "Kamp\u00b7fe", "mit", "der", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Sein Sach auf sich und Gott gestellt.", "tokens": ["Sein", "Sach", "auf", "sich", "und", "Gott", "ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PRF", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Sein Schaffen, Glauben, Leiden, Leben", "tokens": ["Sein", "Schaf\u00b7fen", ",", "Glau\u00b7ben", ",", "Lei\u00b7den", ",", "Le\u00b7ben"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["PPOSAT", "NN", "$,", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Ist Festung. Losung: Nicht ergeben!", "tokens": ["Ist", "Fes\u00b7tung", ".", "Lo\u00b7sung", ":", "Nicht", "er\u00b7ge\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$.", "NN", "$.", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}