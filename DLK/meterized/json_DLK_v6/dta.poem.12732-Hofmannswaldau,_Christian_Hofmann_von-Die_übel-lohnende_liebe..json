{"dta.poem.12732": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Die \u00fcbel-lohnende liebe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich wei\u00df schon, da\u00df die brunst der liebe grausam lohnt;", "tokens": ["Ich", "wei\u00df", "schon", ",", "da\u00df", "die", "brunst", "der", "lie\u00b7be", "grau\u00b7sam", "lohnt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOUS", "ART", "NN", "ART", "VVFIN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch schmiedes-kinder sind der funcken schon gewohnt.", "tokens": ["Doch", "schmie\u00b7des\u00b7kin\u00b7der", "sind", "der", "fun\u00b7cken", "schon", "ge\u00b7wohnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}