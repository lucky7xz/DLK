{"dta.poem.19997": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Abschied .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Geh ich einsam durch die schwarzen Gassen,", "tokens": ["Geh", "ich", "ein\u00b7sam", "durch", "die", "schwar\u00b7zen", "Gas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Schweigt die Stadt als w\u00e4r' sie unbewohnt,", "tokens": ["Schweigt", "die", "Stadt", "als", "w\u00e4r'", "sie", "un\u00b7be\u00b7wohnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KOKOM", "VAFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Aus der Ferne rauschen nur die Wasser", "tokens": ["Aus", "der", "Fer\u00b7ne", "rau\u00b7schen", "nur", "die", "Was\u00b7ser"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "ADV", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und am Himmel geht der bleiche Mond.", "tokens": ["Und", "am", "Him\u00b7mel", "geht", "der", "blei\u00b7che", "Mond", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Bleib' ich lang vor jenem Hause stehen,", "tokens": ["Bleib'", "ich", "lang", "vor", "je\u00b7nem", "Hau\u00b7se", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "PDAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Drinn das liebe liebe Liebchen wohnt,", "tokens": ["Drinn", "das", "lie\u00b7be", "lie\u00b7be", "Lieb\u00b7chen", "wohnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wei\u00df nicht, da\u00df sein Treuer ferne ziehet,", "tokens": ["Wei\u00df", "nicht", ",", "da\u00df", "sein", "Treu\u00b7er", "fer\u00b7ne", "zie\u00b7het", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,", "KOUS", "PPOSAT", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Stumm und harmvoll, wie der bleiche Mond.", "tokens": ["Stumm", "und", "harm\u00b7voll", ",", "wie", "der", "blei\u00b7che", "Mond", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$,", "PWAV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Breit' ich lange sehnend meine Arme", "tokens": ["Breit'", "ich", "lan\u00b7ge", "seh\u00b7nend", "mei\u00b7ne", "Ar\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Nach dem lieben lieben Liebchen aus,", "tokens": ["Nach", "dem", "lie\u00b7ben", "lie\u00b7ben", "Lieb\u00b7chen", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und nun sprech' ich: lebet wohl, ihr Gassen!", "tokens": ["Und", "nun", "sprech'", "ich", ":", "le\u00b7bet", "wohl", ",", "ihr", "Gas\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$.", "VVFIN", "ADV", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Lebe wohl, du stilles, stilles Haus!", "tokens": ["Le\u00b7be", "wohl", ",", "du", "stil\u00b7les", ",", "stil\u00b7les", "Haus", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "PPER", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Und du K\u00e4mmerlein im Haus dort oben,", "tokens": ["Und", "du", "K\u00e4m\u00b7mer\u00b7lein", "im", "Haus", "dort", "o\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "NN", "APPRART", "NN", "ADV", "ADV", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Nach dem oft das warme Herze schwoll,", "tokens": ["Nach", "dem", "oft", "das", "war\u00b7me", "Her\u00b7ze", "schwoll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "ART", "ADJA", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und du Fensterlein, draus Liebchen schaute,", "tokens": ["Und", "du", "Fens\u00b7ter\u00b7lein", ",", "draus", "Lieb\u00b7chen", "schau\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "NN", "$,", "PAV", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und du Th\u00fcre, draus sie gieng, leb wohl!", "tokens": ["Und", "du", "Th\u00fc\u00b7re", ",", "draus", "sie", "gieng", ",", "leb", "wohl", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "NN", "$,", "PAV", "PPER", "VVFIN", "$,", "VVFIN", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Geh' ich bang nun nach den alten Mauern,", "tokens": ["Geh'", "ich", "bang", "nun", "nach", "den", "al\u00b7ten", "Mau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADJD", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Schauend r\u00fcckw\u00e4rts oft mit nassem Blick,", "tokens": ["Schau\u00b7end", "r\u00fcck\u00b7w\u00e4rts", "oft", "mit", "nas\u00b7sem", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Schlie\u00dft der W\u00e4chter hinter mir die Thore,", "tokens": ["Schlie\u00dft", "der", "W\u00e4ch\u00b7ter", "hin\u00b7ter", "mir", "die", "Tho\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NE", "APPR", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wei\u00df nicht, da\u00df mein Herze noch zur\u00fcck.", "tokens": ["Wei\u00df", "nicht", ",", "da\u00df", "mein", "Her\u00b7ze", "noch", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,", "KOUS", "PPOSAT", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}