{"dta.poem.21754": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Das Andenken.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1802", "urn": "urn:nbn:de:kobv:b4-200905193310", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Freund, in welchen fernen Regionen,", "tokens": ["Freund", ",", "in", "wel\u00b7chen", "fer\u00b7nen", "Re\u00b7gi\u00b7o\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "PWAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Welchen sterngestickten Himmelszonen,", "tokens": ["Wel\u00b7chen", "stern\u00b7ge\u00b7stick\u00b7ten", "Him\u00b7mels\u00b7zo\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Schwebst du itzt auf unersp\u00e4hter Bahn?", "tokens": ["Schwebst", "du", "itzt", "auf", "un\u00b7er\u00b7sp\u00e4h\u00b7ter", "Bahn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Schaust im ungeheuren Weltenraume", "tokens": ["Schaust", "im", "un\u00b7ge\u00b7heu\u00b7ren", "Wel\u00b7ten\u00b7rau\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Ebentheuer, welche selbst im Traume", "tokens": ["E\u00b7bent\u00b7heu\u00b7er", ",", "wel\u00b7che", "selbst", "im", "Trau\u00b7me"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PRELS", "ADV", "APPRART", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Kepler nicht, noch Galil\u00e4i sahn.", "tokens": ["Ke\u00b7pler", "nicht", ",", "noch", "Ga\u00b7li\u00b7l\u00e4i", "sahn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "$,", "ADV", "NE", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Schw\u00e4rmst du etwa mit des Strahles Schwinge", "tokens": ["Schw\u00e4rmst", "du", "et\u00b7wa", "mit", "des", "Strah\u00b7les", "Schwin\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Hie und dorthin in dem Schlangenringe,", "tokens": ["Hie", "und", "dor\u00b7thin", "in", "dem", "Schlan\u00b7gen\u00b7rin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Den des Ew'gen Finger tr\u00e4gt und h\u00e4lt?", "tokens": ["Den", "des", "Ew'\u00b7gen", "Fin\u00b7ger", "tr\u00e4gt", "und", "h\u00e4lt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Forschest l\u00fcstern nach dem Quell der Schwere,", "tokens": ["For\u00b7schest", "l\u00fcs\u00b7tern", "nach", "dem", "Quell", "der", "Schwe\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Schiffst auf ", "tokens": ["Schiffst", "auf"], "token_info": ["word", "word"], "pos": ["NN", "APPR"], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "Untersuchest ", "tokens": ["Un\u00b7ter\u00b7su\u00b7chest"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Landest itzt am Ufer der ", "tokens": ["Lan\u00b7dest", "itzt", "am", "U\u00b7fer", "der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPRART", "NN", "ART"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.2": {"text": "Itzt am Archipelag der ", "tokens": ["Itzt", "am", "Ar\u00b7chi\u00b7pe\u00b7lag", "der"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "ART"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Am Gestad' itzt des ", "tokens": ["Am", "Ge\u00b7stad'", "itzt", "des"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "ART"], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.4": {"text": "St\u00fcrzest jetzt dich in des ", "tokens": ["St\u00fcr\u00b7zest", "jetzt", "dich", "in", "des"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PRF", "APPR", "ART"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Schwimmst hinunter dann des Milchstroms Fluthen,", "tokens": ["Schwimmst", "hin\u00b7un\u00b7ter", "dann", "des", "Milch\u00b7stroms", "Flut\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Bis zum glorievollen ", "tokens": ["Bis", "zum", "glo\u00b7rie\u00b7vol\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["APPR", "APPRART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Oder fl\u00fcchtetest du wallfahrtm\u00fcde", "tokens": ["O\u00b7der", "fl\u00fcch\u00b7te\u00b7test", "du", "wall\u00b7fahrt\u00b7m\u00fc\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Zu des ", "tokens": ["Zu", "des"], "token_info": ["word", "word"], "pos": ["APPR", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Pflegst auf seinem S\u00f6ller stolzer Ruh?", "tokens": ["Pflegst", "auf", "sei\u00b7nem", "S\u00f6l\u00b7ler", "stol\u00b7zer", "Ruh", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Siehst der Welten Labyrinthent\u00e4nzen", "tokens": ["Siehst", "der", "Wel\u00b7ten", "La\u00b7by\u00b7rin\u00b7then\u00b7t\u00e4n\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Sonder Stillstand, sonder Ziel und Gr\u00e4nzen", "tokens": ["Son\u00b7der", "Still\u00b7stand", ",", "son\u00b7der", "Ziel", "und", "Gr\u00e4n\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "NN", "$,", "KON", "NN", "KON", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "In erhabener Bewundrung zu?", "tokens": ["In", "er\u00b7ha\u00b7be\u00b7ner", "Be\u00b7wund\u00b7rung", "zu", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+---+-+", "measure": "unknown.measure.tetra"}}, "stanza.5": {"line.1": {"text": "Schwebe, wo du schwebst, in welchen Fernen,", "tokens": ["Schwe\u00b7be", ",", "wo", "du", "schwebst", ",", "in", "wel\u00b7chen", "Fer\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PPER", "VVFIN", "$,", "APPR", "PWAT", "ADJA", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Walle, wo du wallst, auf welchen Sternen \u2014", "tokens": ["Wal\u00b7le", ",", "wo", "du", "wallst", ",", "auf", "wel\u00b7chen", "Ster\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PPER", "VVFIN", "$,", "APPR", "PWAT", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Weiss ich doch, dein wonnetrunkner Blick", "tokens": ["Weiss", "ich", "doch", ",", "dein", "won\u00b7ne\u00b7trunk\u00b7ner", "Blick"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "$,", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Schauet oft aus jenen Glanzgefilden", "tokens": ["Schau\u00b7et", "oft", "aus", "je\u00b7nen", "Glanz\u00b7ge\u00b7fil\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Wehmuthd\u00e4mmernd nach dem blassen milden", "tokens": ["Weh\u00b7muth\u00b7d\u00e4m\u00b7mernd", "nach", "dem", "blas\u00b7sen", "mil\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Mutterstern, der dich gebar, zur\u00fcck,", "tokens": ["Mut\u00b7ters\u00b7tern", ",", "der", "dich", "ge\u00b7bar", ",", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "ADJD", "$,", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Wo du viermal sieben Sommer s\u00e4umtest,", "tokens": ["Wo", "du", "vier\u00b7mal", "sie\u00b7ben", "Som\u00b7mer", "s\u00e4um\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "CARD", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Deiner Kindheit holde Tr\u00e4ume tr\u00e4umtest,", "tokens": ["Dei\u00b7ner", "Kind\u00b7heit", "hol\u00b7de", "Tr\u00e4u\u00b7me", "tr\u00e4um\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Deiner Jugend Auen froh durchflogst.", "tokens": ["Dei\u00b7ner", "Ju\u00b7gend", "Au\u00b7en", "froh", "durch\u00b7flogst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Wo du l\u00fcstern aus dem Nektarbusen", "tokens": ["Wo", "du", "l\u00fcs\u00b7tern", "aus", "dem", "Nekt\u00b7ar\u00b7bu\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Der Natur, dem Honigmund' der Musen", "tokens": ["Der", "Na\u00b7tur", ",", "dem", "Ho\u00b7nig\u00b7mund'", "der", "Mu\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Freude, Freyheit und Begeistrung sogst;", "tokens": ["Freu\u00b7de", ",", "Frey\u00b7heit", "und", "Be\u00b7geis\u00b7trung", "sogst", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Wo des Wissens Kelch dich itzt erquickte,", "tokens": ["Wo", "des", "Wis\u00b7sens", "Kelch", "dich", "itzt", "er\u00b7quick\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Itzt der Dichtung Zauber dich entz\u00fcckte,", "tokens": ["Itzt", "der", "Dich\u00b7tung", "Zau\u00b7ber", "dich", "ent\u00b7z\u00fcck\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Itzt der Ahnung Schauder dich durchdrang;", "tokens": ["Itzt", "der", "Ah\u00b7nung", "Schau\u00b7der", "dich", "durch\u00b7drang", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Manch befreundet Herz sich an dich schmiegte,", "tokens": ["Manch", "be\u00b7freun\u00b7det", "Herz", "sich", "an", "dich", "schmieg\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "NN", "PRF", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Manch verwandter Geist sich zu dir f\u00fcgte,", "tokens": ["Manch", "ver\u00b7wand\u00b7ter", "Geist", "sich", "zu", "dir", "f\u00fcg\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "PRF", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Mancher Arm vertraulich dich umschlang.", "tokens": ["Man\u00b7cher", "Arm", "ver\u00b7trau\u00b7lich", "dich", "um\u00b7schlang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJD", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Ja, ich weiss, du schaust mit sanftem Sehnen", "tokens": ["Ja", ",", "ich", "weiss", ",", "du", "schaust", "mit", "sanf\u00b7tem", "Seh\u00b7nen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Oft hinunter nach dem Stern von Thr\u00e4nen,", "tokens": ["Oft", "hin\u00b7un\u00b7ter", "nach", "dem", "Stern", "von", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Aus des Empyr\u00e4ums heilger Nacht.", "tokens": ["Aus", "des", "Em\u00b7py\u00b7r\u00e4ums", "heil\u00b7ger", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Reifte doch dein Geist in seinen Strahlen,", "tokens": ["Reif\u00b7te", "doch", "dein", "Geist", "in", "sei\u00b7nen", "Strah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Wird doch dein in seinen stillen Thalen", "tokens": ["Wird", "doch", "dein", "in", "sei\u00b7nen", "stil\u00b7len", "Tha\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PPOSAT", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Lange noch mit Lieb' und Leid gedacht!", "tokens": ["Lan\u00b7ge", "noch", "mit", "Lieb'", "und", "Leid", "ge\u00b7dacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}