{"dta.poem.9579": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Er bindet sie an.  \n C. E.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Komm Venns/ kn\u00fcpffe mir ein band von myrthen-zweigen/", "tokens": ["Komm", "Venns", "/", "kn\u00fcpf\u00b7fe", "mir", "ein", "band", "von", "myr\u00b7then\u00b7zwei\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "$(", "VVFIN", "PPER", "ART", "NN", "APPR", "NN", "$("], "meter": "--+-+-+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Und mische deinen schmuck den frischen bl\u00e4ttern ein;", "tokens": ["Und", "mi\u00b7sche", "dei\u00b7nen", "schmuck", "den", "fri\u00b7schen", "bl\u00e4t\u00b7tern", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Streu Flora/ wo du kanst/ je\u00dfmin und nelcken drein.", "tokens": ["Streu", "Flo\u00b7ra", "/", "wo", "du", "kanst", "/", "je\u00df\u00b7min", "und", "nel\u00b7cken", "drein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$(", "PWAV", "PPER", "VMFIN", "$(", "NE", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Auff Thetis! wirstu nicht aus deinen tieffen steigen?", "tokens": ["Auff", "The\u00b7tis", "!", "wirs\u00b7tu", "nicht", "aus", "dei\u00b7nen", "tief\u00b7fen", "stei\u00b7gen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$.", "VAFIN", "PTKNEG", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Hervor! auch du must uns die nassen sch\u00e4tze zeigen/", "tokens": ["Her\u00b7vor", "!", "auch", "du", "must", "uns", "die", "nas\u00b7sen", "sch\u00e4t\u00b7ze", "zei\u00b7gen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKVZ", "$.", "ADV", "PPER", "VMFIN", "PPER", "ART", "ADJA", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Bring\u2019 runte perlen her/ die nur die besten seyn:", "tokens": ["Bring'", "run\u00b7te", "per\u00b7len", "her", "/", "die", "nur", "die", "bes\u00b7ten", "seyn", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "PTKVZ", "$(", "PRELS", "ADV", "ART", "ADJA", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Jhr berge/ zinset gold/ ihr felsen/ edelstein/", "tokens": ["Ihr", "ber\u00b7ge", "/", "zin\u00b7set", "gold", "/", "ihr", "fel\u00b7sen", "/", "e\u00b7del\u00b7stein", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "VVFIN", "NN", "$(", "PPOSAT", "NN", "$(", "ADJD", "$("], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.8": {"text": "Wir wollen alles hier in eins zusammen reigen.", "tokens": ["Wir", "wol\u00b7len", "al\u00b7les", "hier", "in", "eins", "zu\u00b7sam\u00b7men", "rei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIS", "ADV", "APPR", "PIS", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Cupido/ nimm du drauff di\u00df angenehme band/", "tokens": ["Cu\u00b7pi\u00b7do", "/", "nimm", "du", "drauff", "di\u00df", "an\u00b7ge\u00b7neh\u00b7me", "band", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "VVIMP", "PPER", "PAV", "PDS", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und flicht es Solimen um ihre liljen-hand;", "tokens": ["Und", "flicht", "es", "So\u00b7li\u00b7men", "um", "ih\u00b7re", "lil\u00b7jen\u00b7hand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Doch sag\u2019 ihr auch zugleich von deiner mutter wegen/", "tokens": ["Doch", "sag'", "ihr", "auch", "zu\u00b7gleich", "von", "dei\u00b7ner", "mut\u00b7ter", "we\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "APPR", "PPOSAT", "NN", "APPR", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Da\u00df sie dir folgen wird in k\u00f6niglicher pracht/", "tokens": ["Da\u00df", "sie", "dir", "fol\u00b7gen", "wird", "in", "k\u00f6\u00b7nig\u00b7li\u00b7cher", "pracht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVINF", "VAFIN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Um ihr den sch\u00f6nheits-prei\u00df/ den Eris hat erdacht/", "tokens": ["Um", "ihr", "den", "sch\u00f6n\u00b7heits\u00b7prei\u00df", "/", "den", "E\u00b7ris", "hat", "er\u00b7dacht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "ART", "NN", "$(", "ART", "NE", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Den g\u00f6ldnen apffel selbst zun f\u00fcssen hin zu legen.", "tokens": ["Den", "g\u00f6ld\u00b7nen", "apf\u00b7fel", "selbst", "zun", "f\u00fcs\u00b7sen", "hin", "zu", "le\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVINF", "VVFIN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}