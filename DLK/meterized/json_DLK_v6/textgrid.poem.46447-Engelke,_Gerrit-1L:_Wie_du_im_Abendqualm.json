{"textgrid.poem.46447": {"metadata": {"author": {"name": "Engelke, Gerrit", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie du im Abendqualm", "genre": "verse", "period": "N.A.", "pub_year": 1913, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie du im Abendqualm", "tokens": ["Wie", "du", "im", "A\u00b7bend\u00b7qualm"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "So einf\u00e4ltig an mir vor\u00fcbergehst,", "tokens": ["So", "ein\u00b7f\u00e4l\u00b7tig", "an", "mir", "vor\u00b7\u00fc\u00b7ber\u00b7gehst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Tauchst du in meinen Gleichmutblick den deinen \u2013", "tokens": ["Tauchst", "du", "in", "mei\u00b7nen", "Gleich\u00b7mut\u00b7blick", "den", "dei\u00b7nen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "ART", "PPOSAT", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den deinen", "tokens": ["Den", "dei\u00b7nen"], "token_info": ["word", "word"], "pos": ["ART", "PPOSAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Der in dem mageren Gesicht wie eine Frage,", "tokens": ["Der", "in", "dem", "ma\u00b7ge\u00b7ren", "Ge\u00b7sicht", "wie", "ei\u00b7ne", "Fra\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wie feuchter grauer Schimmer schwimmt \u2013", "tokens": ["Wie", "feuch\u00b7ter", "grau\u00b7er", "Schim\u00b7mer", "schwimmt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "O unbewu\u00dfte M\u00e4dchenklage \u2013", "tokens": ["O", "un\u00b7be\u00b7wu\u00df\u00b7te", "M\u00e4d\u00b7chen\u00b7kla\u00b7ge", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Dein Auge fr\u00e4gt \u2013 dein Auge glimmt \u2013", "tokens": ["Dein", "Au\u00b7ge", "fr\u00e4gt", "\u2013", "dein", "Au\u00b7ge", "glimmt", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$(", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du hast so sehnsuchtmagere Glieder,", "tokens": ["Du", "hast", "so", "sehn\u00b7sucht\u00b7ma\u00b7ge\u00b7re", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Du tr\u00e4gst noch zartgeflochtene falbe Kinderhaare,", "tokens": ["Du", "tr\u00e4gst", "noch", "zart\u00b7ge\u00b7floch\u00b7te\u00b7ne", "fal\u00b7be", "Kin\u00b7der\u00b7haa\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Du hast so aufgeschossene Glieder,", "tokens": ["Du", "hast", "so", "auf\u00b7ge\u00b7schos\u00b7se\u00b7ne", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Du bist wohl dreizehn Jahre alt \u2013 schon dreizehn Jahre \u2013", "tokens": ["Du", "bist", "wohl", "drei\u00b7zehn", "Jah\u00b7re", "alt", "\u2013", "schon", "drei\u00b7zehn", "Jah\u00b7re", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "CARD", "NN", "ADJD", "$(", "ADV", "CARD", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Du tr\u00e4gst das blaugepunkte kurze Kleid", "tokens": ["Du", "tr\u00e4gst", "das", "blau\u00b7ge\u00b7punk\u00b7te", "kur\u00b7ze", "Kleid"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Aus Waschkattun,", "tokens": ["Aus", "Waschkat\u00b7tun", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Du gehst in l\u00e4cherlichen Kinderschuh'n \u2013", "tokens": ["Du", "gehst", "in", "l\u00e4\u00b7cher\u00b7li\u00b7chen", "Kin\u00b7der\u00b7schuh'n", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Du steckst noch ganz in Kindlichkeit,", "tokens": ["Du", "steckst", "noch", "ganz", "in", "Kind\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Doch dein Auge \u2013 dein Auge allein....", "tokens": ["Doch", "dein", "Au\u00b7ge", "\u2013", "dein", "Au\u00b7ge", "al\u00b7lein", "...."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$(", "PPOSAT", "NN", "ADV", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}}, "stanza.3": {"line.1": {"text": "Doch gehst du wie in tr\u00fcbem Bangen,", "tokens": ["Doch", "gehst", "du", "wie", "in", "tr\u00fc\u00b7bem", "Ban\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "KOKOM", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Doch gehst du so befangen \u2013", "tokens": ["Doch", "gehst", "du", "so", "be\u00b7fan\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich wei\u00df, es weht der Fr\u00fchjahrswind \u2013", "tokens": ["Ich", "wei\u00df", ",", "es", "weht", "der", "Fr\u00fch\u00b7jahrs\u00b7wind", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Luft ist dunstigblau, bl\u00fctenlind \u2013", "tokens": ["Die", "Luft", "ist", "duns\u00b7tig\u00b7blau", ",", "bl\u00fc\u00b7ten\u00b7lind", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "VVFIN", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Du m\u00f6chtest gern dich selbst erl\u00f6sen \u2013", "tokens": ["Du", "m\u00f6ch\u00b7test", "gern", "dich", "selbst", "er\u00b7l\u00f6\u00b7sen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PPER", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Geh weiter \u2013 weiter, kleines blasses Kind \u2013", "tokens": ["Geh", "wei\u00b7ter", "\u2013", "wei\u00b7ter", ",", "klei\u00b7nes", "blas\u00b7ses", "Kind", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$(", "PTKVZ", "$,", "ADJA", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}