{"textgrid.poem.45750": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[\u00f6streich ist wie Polen]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00d6streich ist wie Polen,", "tokens": ["\u00d6s\u00b7treich", "ist", "wie", "Po\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "KOKOM", "NE", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Wer was braucht, gehts von da zu holen.", "tokens": ["Wer", "was", "braucht", ",", "gehts", "von", "da", "zu", "ho\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PWS", "VVFIN", "$,", "VVFIN", "APPR", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.3": {"text": "\u00d6streich ist wie die T\u00fcrkei,", "tokens": ["\u00d6s\u00b7treich", "ist", "wie", "die", "T\u00fcr\u00b7kei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "KOKOM", "ART", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Bei allem, was es tut, ist der Mufti dabei.", "tokens": ["Bei", "al\u00b7lem", ",", "was", "es", "tut", ",", "ist", "der", "Muf\u00b7ti", "da\u00b7bei", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "PWS", "PPER", "VVFIN", "$,", "VAFIN", "ART", "NN", "PAV", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}}}}}