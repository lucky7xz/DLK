{"textgrid.poem.54122": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Sauflied, ganz allein", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Manchmal denke ich an dich,", "tokens": ["Manch\u00b7mal", "den\u00b7ke", "ich", "an", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "das bekommt mich aber nich,", "tokens": ["das", "be\u00b7kommt", "mich", "a\u00b7ber", "nich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "denn am n\u00e4chsten Tag bin ich so m\u00fcde.", "tokens": ["denn", "am", "n\u00e4chs\u00b7ten", "Tag", "bin", "ich", "so", "m\u00fc\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Du mein holdes Glasgespinst!", "tokens": ["Du", "mein", "hol\u00b7des", "Glas\u00b7ge\u00b7spinst", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ob du dich auf mich besinnst?", "tokens": ["Ob", "du", "dich", "auf", "mich", "be\u00b7sinnst", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Morgens warst du immer etwas pr\u00fcde.", "tokens": ["Mor\u00b7gens", "warst", "du", "im\u00b7mer", "et\u00b7was", "pr\u00fc\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Darum trink ich auf dein Wohl", "tokens": ["Da\u00b7rum", "trink", "ich", "auf", "dein", "Wohl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "dieses Gl\u00e4schen Alkohol!", "tokens": ["die\u00b7ses", "Gl\u00e4s\u00b7chen", "Al\u00b7ko\u00b7hol", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Braun und blond \u2013 rot und schwarz \u2013", "tokens": ["Braun", "und", "blond", "\u2013", "rot", "und", "schwarz", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "$(", "ADJD", "KON", "ADJD", "$("], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.10": {"text": "Ihr sollt leben!", "tokens": ["Ihr", "sollt", "le\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Deine Augen sind so blau", "tokens": ["Dei\u00b7ne", "Au\u00b7gen", "sind", "so", "blau"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "ganz genau wie bei der Frau", "tokens": ["ganz", "ge\u00b7nau", "wie", "bei", "der", "Frau"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KOKOM", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Erna Margot Glyn-Kaliski.", "tokens": ["Er\u00b7na", "Mar\u00b7got", "Glyn\u00b7Ka\u00b7li\u00b7ski", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Rheinwein ist nicht stark genug,", "tokens": ["Rhein\u00b7wein", "ist", "nicht", "stark", "ge\u00b7nug", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PTKNEG", "ADJD", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "darum nehm ich einen Schluck", "tokens": ["da\u00b7rum", "nehm", "ich", "ei\u00b7nen", "Schluck"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "von dem guten, gelben Whisky.", "tokens": ["von", "dem", "gu\u00b7ten", ",", "gel\u00b7ben", "Whis\u00b7ky", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Und ich trinke auf dein Wohl", "tokens": ["Und", "ich", "trin\u00b7ke", "auf", "dein", "Wohl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "dieses Fl\u00e4schchen Alikol \u2013", "tokens": ["die\u00b7ses", "Fl\u00e4\u00b7schchen", "A\u00b7li\u00b7kol", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "NN", "NE", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Braun und Blond \u2013 Black and White . . .", "tokens": ["Braun", "und", "Blond", "\u2013", "Black", "and", "Whi\u00b7te", ".", ".", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NE", "KON", "NN", "$(", "FM.la", "FM.la", "FM.la", "$.", "$.", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.10": {"text": "Ihr sollt leben!", "tokens": ["Ihr", "sollt", "le\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Tinte, Rotwein und Odol", "tokens": ["Tin\u00b7te", ",", "Rot\u00b7wein", "und", "O\u00b7dol"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "NN", "KON", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "sind drei Fl\u00fcssigkeiten wohl \u2013", "tokens": ["sind", "drei", "Fl\u00fcs\u00b7sig\u00b7kei\u00b7ten", "wohl", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "CARD", "NN", "ADV", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "davon kann der Mensch schon leben.", "tokens": ["da\u00b7von", "kann", "der", "Mensch", "schon", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "So sch\u00f6n kannst du gar nicht sein,", "tokens": ["So", "sch\u00f6n", "kannst", "du", "gar", "nicht", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VMFIN", "PPER", "ADV", "PTKNEG", "VAINF", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "wie in meinen Tr\u00e4umerein \u2013", "tokens": ["wie", "in", "mei\u00b7nen", "Tr\u00e4u\u00b7me\u00b7rein", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "PPOSAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "so viel kannst du gar nicht geben.", "tokens": ["so", "viel", "kannst", "du", "gar", "nicht", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.7": {"text": "Allersch\u00f6nste Frauenzier,", "tokens": ["Al\u00b7ler\u00b7sch\u00f6ns\u00b7te", "Frau\u00b7en\u00b7zier", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "ach, wie gut, da\u00df du nicht hier!", "tokens": ["ach", ",", "wie", "gut", ",", "da\u00df", "du", "nicht", "hier", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["XY", "$,", "PWAV", "ADJD", "$,", "KOUS", "PPER", "PTKNEG", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Oh, wie gerne man doch k\u00fc\u00dft,", "tokens": ["Oh", ",", "wie", "ger\u00b7ne", "man", "doch", "k\u00fc\u00dft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "ADV", "PIS", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "wenn die Frau wo anders ist . . . !", "tokens": ["wenn", "die", "Frau", "wo", "an\u00b7ders", "ist", ".", ".", ".", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["KOUS", "ART", "NN", "PWAV", "ADV", "VAFIN", "$.", "$.", "$.", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Und darum trink ich auf dein Wohl!", "tokens": ["Und", "da\u00b7rum", "trink", "ich", "auf", "dein", "Wohl", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Nun ade, mein Land Tirol!", "tokens": ["Nun", "a\u00b7de", ",", "mein", "Land", "Ti\u00b7rol", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "PPOSAT", "NN", "NE", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.13": {"text": "Lebe wohl! Nur in den kleinen R\u00e4uschen", "tokens": ["Le\u00b7be", "wohl", "!", "Nur", "in", "den", "klei\u00b7nen", "R\u00e4u\u00b7schen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "$.", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.14": {"text": "lebe wohl, kann die Frau uns nicht entt\u00e4uschen!", "tokens": ["le\u00b7be", "wohl", ",", "kann", "die", "Frau", "uns", "nicht", "ent\u00b7t\u00e4u\u00b7schen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "VMFIN", "ART", "NN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.15": {"text": "Lebe wohl! Lebe wohl!", "tokens": ["Le\u00b7be", "wohl", "!", "Le\u00b7be", "wohl", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$.", "VVIMP", "ADV", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.16": {"text": "Lebe wohl, mein Land Tirol \u2013!", "tokens": ["Le\u00b7be", "wohl", ",", "mein", "Land", "Ti\u00b7rol", "\u2013", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VVIMP", "ADV", "$,", "PPOSAT", "NN", "NE", "$(", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}