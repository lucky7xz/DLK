{"textgrid.poem.67052": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Teck", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ah! so hab ich noch die Traubenh\u00fcgel erstiegen,", "tokens": ["Ah", "!", "so", "hab", "ich", "noch", "die", "Trau\u00b7ben\u00b7h\u00fc\u00b7gel", "er\u00b7stie\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Ehe der leuchtende Strahl an der g\u00fcldenen Ferne hinabsinkt.", "tokens": ["E\u00b7he", "der", "leuch\u00b7ten\u00b7de", "Strahl", "an", "der", "g\u00fcl\u00b7de\u00b7nen", "Fer\u00b7ne", "hin\u00b7ab\u00b7sinkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.3": {"text": "Und wie wohl ist mir! Ich streck im stolzen Gef\u00fchle \u2013", "tokens": ["Und", "wie", "wohl", "ist", "mir", "!", "Ich", "streck", "im", "stol\u00b7zen", "Ge\u00b7f\u00fch\u00b7le", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADV", "VAFIN", "PPER", "$.", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "$("], "meter": "---+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Als umschl\u00e4nge mein Arm das Unendliche \u2013 auf zu den Wolken", "tokens": ["Als", "um\u00b7schl\u00e4n\u00b7ge", "mein", "Arm", "das", "Un\u00b7end\u00b7li\u00b7che", "\u2013", "auf", "zu", "den", "Wol\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "VVFIN", "PPOSAT", "NN", "ART", "NN", "$(", "APPR", "APPR", "ART", "NN"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.5": {"text": "Meine gefaltete H\u00e4nde, zu danken im edlen Gef\u00fchle,", "tokens": ["Mei\u00b7ne", "ge\u00b7fal\u00b7te\u00b7te", "H\u00e4n\u00b7de", ",", "zu", "dan\u00b7ken", "im", "ed\u00b7len", "Ge\u00b7f\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PTKZU", "VVINF", "APPRART", "ADJA", "NN", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.6": {"text": "Da\u00df er ein Herz mir gab, dem Schaffer der edlen Gef\u00fchle.", "tokens": ["Da\u00df", "er", "ein", "Herz", "mir", "gab", ",", "dem", "Schaf\u00b7fer", "der", "ed\u00b7len", "Ge\u00b7f\u00fch\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PPER", "VVFIN", "$,", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Mich mit den Frohen zu freuen, zu schauen den herbstlichen Jubel,", "tokens": ["Mich", "mit", "den", "Fro\u00b7hen", "zu", "freu\u00b7en", ",", "zu", "schau\u00b7en", "den", "herbst\u00b7li\u00b7chen", "Ju\u00b7bel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.8": {"text": "Wie sie die k\u00f6stliche Traube mit heiterstaunendem Blicke", "tokens": ["Wie", "sie", "die", "k\u00f6st\u00b7li\u00b7che", "Trau\u00b7be", "mit", "hei\u00b7ter\u00b7stau\u00b7nen\u00b7dem", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+--+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "\u00dcber sich halten, und lange noch zaudern, die gl\u00e4nzende Beere", "tokens": ["\u00dc\u00b7ber", "sich", "hal\u00b7ten", ",", "und", "lan\u00b7ge", "noch", "zau\u00b7dern", ",", "die", "gl\u00e4n\u00b7zen\u00b7de", "Bee\u00b7re"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PRF", "VVINF", "$,", "KON", "ADV", "ADV", "VVINF", "$,", "ART", "ADJA", "NN"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.10": {"text": "In des Kelterers H\u00e4nde zu geben \u2013 wie der ger\u00fchrte", "tokens": ["In", "des", "Kel\u00b7te\u00b7rers", "H\u00e4n\u00b7de", "zu", "ge\u00b7ben", "\u2013", "wie", "der", "ge\u00b7r\u00fchr\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "PTKZU", "VVINF", "$(", "KOKOM", "ART", "ADJA"], "meter": "--+--+--+-++-+-", "measure": "anapaest.tri.plus"}, "line.11": {"text": "Silberlockigte Greis an der abgeernteten Rebe", "tokens": ["Sil\u00b7berl\u00b7o\u00b7ckig\u00b7te", "Greis", "an", "der", "ab\u00b7geern\u00b7te\u00b7ten", "Re\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "+--+-+--+-+-+-", "measure": "iambic.hexa.invert"}, "line.12": {"text": "K\u00f6niglich froh zum herbstlichen Mahle sich setzt mit den Kleinen,", "tokens": ["K\u00f6\u00b7nig\u00b7lich", "froh", "zum", "herbst\u00b7li\u00b7chen", "Mah\u00b7le", "sich", "setzt", "mit", "den", "Klei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "APPRART", "ADJA", "NN", "PRF", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.13": {"text": "O! und zu ihnen spricht aus der F\u00fclle des dankenden Herzens:", "tokens": ["O", "!", "und", "zu", "ih\u00b7nen", "spricht", "aus", "der", "F\u00fcl\u00b7le", "des", "dan\u00b7ken\u00b7den", "Her\u00b7zens", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "KON", "APPR", "PPER", "VVFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Kinder! am Segen des Herrn ist alles, alles gelegen \u2013 \u2013", "tokens": ["Kin\u00b7der", "!", "am", "Se\u00b7gen", "des", "Herrn", "ist", "al\u00b7les", ",", "al\u00b7les", "ge\u00b7le\u00b7gen", "\u2013", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["NN", "$.", "APPRART", "NN", "ART", "NN", "VAFIN", "PIS", "$,", "PIS", "VVPP", "$(", "$("], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.15": {"text": "Mich mit den Frohen zu freuen, zu schauen den herbstlichen Jubel,", "tokens": ["Mich", "mit", "den", "Fro\u00b7hen", "zu", "freu\u00b7en", ",", "zu", "schau\u00b7en", "den", "herbst\u00b7li\u00b7chen", "Ju\u00b7bel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.16": {"text": "War ich herauf von den H\u00fctten der gastlichen Freundschaft gegangen.", "tokens": ["War", "ich", "her\u00b7auf", "von", "den", "H\u00fct\u00b7ten", "der", "gast\u00b7li\u00b7chen", "Freund\u00b7schaft", "ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+--+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.17": {"text": "Aber siehe! allm\u00e4chtig rei\u00dfen mich hin in ernste Bewundrung", "tokens": ["A\u00b7ber", "sie\u00b7he", "!", "all\u00b7m\u00e4ch\u00b7tig", "rei\u00b7\u00dfen", "mich", "hin", "in", "erns\u00b7te", "Be\u00b7wund\u00b7rung"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "$.", "ADJD", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+--+-+--+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.18": {"text": "Gegen\u00fcber die waldigte Riesengebirge. \u2013 La\u00df mich vergessen,", "tokens": ["Ge\u00b7gen\u00b7\u00fc\u00b7ber", "die", "wal\u00b7dig\u00b7te", "Rie\u00b7sen\u00b7ge\u00b7bir\u00b7ge", ".", "\u2013", "La\u00df", "mich", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$.", "$(", "VVIMP", "PPER", "VVPP", "$,"], "meter": "--+--+--+--+-+--+-", "measure": "anapaest.tetra.plus"}, "line.19": {"text": "La\u00df mich deine Lust, du falbigte Rebe, vergessen,", "tokens": ["La\u00df", "mich", "dei\u00b7ne", "Lust", ",", "du", "fal\u00b7big\u00b7te", "Re\u00b7be", ",", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "$,", "PPER", "ADJA", "NN", "$,", "VVPP", "$,"], "meter": "+-+-+-+--+--+-", "measure": "hexameter"}, "line.20": {"text": "Da\u00df ich mit voller Seele sie schaue, die Riesengebirge!", "tokens": ["Da\u00df", "ich", "mit", "vol\u00b7ler", "See\u00b7le", "sie", "schau\u00b7e", ",", "die", "Rie\u00b7sen\u00b7ge\u00b7bir\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "PPER", "VVFIN", "$,", "ART", "NN", "$."], "meter": "-+-+-+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.21": {"text": "Ha! wie jenes so k\u00f6niglich \u00fcber die Br\u00fcder emporragt!", "tokens": ["Ha", "!", "wie", "je\u00b7nes", "so", "k\u00f6\u00b7nig\u00b7lich", "\u00fc\u00b7ber", "die", "Br\u00fc\u00b7der", "em\u00b7por\u00b7ragt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWAV", "PDS", "ADV", "ADJD", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+--+--+--+---+", "measure": "trochaic.hexa.relaxed"}, "line.22": {"text": "Teck ist sein Name. Da klangen einst Harnische, Schwerder ert\u00f6nten", "tokens": ["Teck", "ist", "sein", "Na\u00b7me", ".", "Da", "klan\u00b7gen", "einst", "Har\u00b7ni\u00b7sche", ",", "Schwer\u00b7der", "er\u00b7t\u00f6n\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "VAFIN", "PPOSAT", "NN", "$.", "ADV", "VVFIN", "ADV", "NN", "$,", "NN", "VVINF"], "meter": "+-+-+-+--+--+--+-", "measure": "trochaic.septa.relaxed"}, "line.23": {"text": "Zwischen den moosigten Mauren der F\u00fcrsten und blinkende Helme.", "tokens": ["Zwi\u00b7schen", "den", "moo\u00b7sig\u00b7ten", "Mau\u00b7ren", "der", "F\u00fcrs\u00b7ten", "und", "blin\u00b7ken\u00b7de", "Hel\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "KON", "ADJA", "NN", "$."], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.24": {"text": "Eisern waren und gro\u00df und bieder seine Bewohner.", "tokens": ["Ei\u00b7sern", "wa\u00b7ren", "und", "gro\u00df", "und", "bie\u00b7der", "sei\u00b7ne", "Be\u00b7woh\u00b7ner", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "KON", "ADJD", "KON", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.25": {"text": "Mit dem kommenden Tag stand \u00fcber den moosigten Mauren", "tokens": ["Mit", "dem", "kom\u00b7men\u00b7den", "Tag", "stand", "\u00fc\u00b7ber", "den", "moo\u00b7sig\u00b7ten", "Mau\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.26": {"text": "In der ehernen R\u00fcstung der F\u00fcrst, sein Gebirge zu schauen.", "tokens": ["In", "der", "e\u00b7her\u00b7nen", "R\u00fcs\u00b7tung", "der", "F\u00fcrst", ",", "sein", "Ge\u00b7bir\u00b7ge", "zu", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.27": {"text": "Mein dies Riesengebirge \u2013 so stolz \u2013 so k\u00f6niglich herrlich \u2013 ?", "tokens": ["Mein", "dies", "Rie\u00b7sen\u00b7ge\u00b7bir\u00b7ge", "\u2013", "so", "stolz", "\u2013", "so", "k\u00f6\u00b7nig\u00b7lich", "herr\u00b7lich", "\u2013", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "PDS", "NN", "$(", "ADV", "ADJD", "$(", "ADV", "ADJD", "ADJD", "$(", "$."], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.28": {"text": "Sprach er mit ernsterer Stirne, mit hohem, denkendem Auge \u2013", "tokens": ["Sprach", "er", "mit", "erns\u00b7te\u00b7rer", "Stir\u00b7ne", ",", "mit", "ho\u00b7hem", ",", "den\u00b7ken\u00b7dem", "Au\u00b7ge", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "$,", "ART", "NN", "$("], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.29": {"text": "Mein die trotzende Felsen? die tausendj\u00e4hrige Eichen?", "tokens": ["Mein", "die", "trot\u00b7zen\u00b7de", "Fel\u00b7sen", "?", "die", "tau\u00b7send\u00b7j\u00e4h\u00b7ri\u00b7ge", "Ei\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ART", "ADJA", "NN", "$.", "ART", "ADJA", "NN", "$."], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.30": {"text": "Ha! und ich? \u2013 und ich? \u2013 bald w\u00e4re mein Harnisch gerostet,", "tokens": ["Ha", "!", "und", "ich", "?", "\u2013", "und", "ich", "?", "\u2013", "bald", "w\u00e4\u00b7re", "mein", "Har\u00b7nisch", "ge\u00b7ros\u00b7tet", ","], "token_info": ["word", "punct", "word", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "KON", "PPER", "$.", "$(", "KON", "PPER", "$.", "$(", "ADV", "VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+--+--+-", "measure": "hexameter"}, "line.31": {"text": "O! der Schande! mein Harnisch gerostet in diesem Gebirge.", "tokens": ["O", "!", "der", "Schan\u00b7de", "!", "mein", "Har\u00b7nisch", "ge\u00b7ros\u00b7tet", "in", "die\u00b7sem", "Ge\u00b7bir\u00b7ge", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ART", "NN", "$.", "PPOSAT", "NN", "VVPP", "APPR", "PDAT", "NN", "$."], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.32": {"text": "Aber ich schw\u00f6r \u2013 ich schw\u00f6r, ich meide mein Riesengebirge,", "tokens": ["A\u00b7ber", "ich", "schw\u00f6r", "\u2013", "ich", "schw\u00f6r", ",", "ich", "mei\u00b7de", "mein", "Rie\u00b7sen\u00b7ge\u00b7bir\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFI