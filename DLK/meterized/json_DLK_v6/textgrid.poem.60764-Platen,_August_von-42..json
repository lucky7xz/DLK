{"textgrid.poem.60764": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "42.", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einmal will ich, das versprech ich, ohne Liebgekose leben,", "tokens": ["Ein\u00b7mal", "will", "ich", ",", "das", "ver\u00b7sprech", "ich", ",", "oh\u00b7ne", "Lieb\u00b7ge\u00b7ko\u00b7se", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "$,", "PDS", "VVFIN", "PPER", "$,", "KOUI", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Wann die Blumen hier im Garten nach den Tafeln Mose leben;", "tokens": ["Wann", "die", "Blu\u00b7men", "hier", "im", "Gar\u00b7ten", "nach", "den", "Ta\u00b7feln", "Mo\u00b7se", "le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "APPRART", "NN", "APPR", "ART", "NN", "NE", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "H\u00f6r ich abends auf den Stra\u00dfen einen Vogel, eine Fl\u00f6te,", "tokens": ["H\u00f6r", "ich", "a\u00b7bends", "auf", "den", "Stra\u00b7\u00dfen", "ei\u00b7nen", "Vo\u00b7gel", ",", "ei\u00b7ne", "Fl\u00f6\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "APPR", "ART", "NN", "ART", "NE", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Sag ich bei mir selbst: Es m\u00f6ge dieser Virtuose leben!", "tokens": ["Sag", "ich", "bei", "mir", "selbst", ":", "Es", "m\u00f6\u00b7ge", "die\u00b7ser", "Vir\u00b7tu\u00b7o\u00b7se", "le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "PPER", "ADV", "$.", "PPER", "VMFIN", "PDAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Freund! es ist der Lenz gekommen, unsre Wege sind verschieden:", "tokens": ["Freund", "!", "es", "ist", "der", "Lenz", "ge\u00b7kom\u00b7men", ",", "uns\u00b7re", "We\u00b7ge", "sind", "ver\u00b7schie\u00b7den", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VAFIN", "ART", "NN", "VVPP", "$,", "PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Lebe wie die keusche Lilje, la\u00df mich wie die Rose leben!", "tokens": ["Le\u00b7be", "wie", "die", "keu\u00b7sche", "Lil\u00b7je", ",", "la\u00df", "mich", "wie", "die", "Ro\u00b7se", "le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "KOKOM", "ART", "ADJA", "NN", "$,", "VVIMP", "PPER", "KOKOM", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.7": {"text": "La\u00dft mich euren Rat vernehmen, was das Beste sei von Zweien:", "tokens": ["La\u00dft", "mich", "eu\u00b7ren", "Rat", "ver\u00b7neh\u00b7men", ",", "was", "das", "Bes\u00b7te", "sei", "von", "Zwei\u00b7en", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "VVINF", "$,", "PRELS", "ART", "NN", "VAFIN", "APPR", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Weise leben, lose reden? Weise reden, lose leben?", "tokens": ["Wei\u00b7se", "le\u00b7ben", ",", "lo\u00b7se", "re\u00b7den", "?", "Wei\u00b7se", "re\u00b7den", ",", "lo\u00b7se", "le\u00b7ben", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "ADJA", "VVINF", "$.", "NN", "VVINF", "$,", "ADJA", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.9": {"text": "Wollt ihr mich durchaus verkennen, tut es immerhin, denn immer", "tokens": ["Wollt", "ihr", "mich", "durc\u00b7haus", "ver\u00b7ken\u00b7nen", ",", "tut", "es", "im\u00b7mer\u00b7hin", ",", "denn", "im\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["VMFIN", "PPER", "PRF", "ADV", "VVINF", "$,", "VVFIN", "PPER", "ADV", "$,", "KON", "ADV"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.10": {"text": "Werd ich, ob ich l\u00e4chle dr\u00fcber oder mich erbose, leben!", "tokens": ["Werd", "ich", ",", "ob", "ich", "l\u00e4ch\u00b7le", "dr\u00fc\u00b7ber", "o\u00b7der", "mich", "er\u00b7bo\u00b7se", ",", "le\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "PAV", "KON", "PPER", "VVFIN", "$,", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}}}}