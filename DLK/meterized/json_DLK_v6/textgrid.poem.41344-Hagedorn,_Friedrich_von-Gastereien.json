{"textgrid.poem.41344": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Gastereien", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Wissenschaft, ein Gastmahl anzustellen,", "tokens": ["Die", "Wis\u00b7sen\u00b7schaft", ",", "ein", "Gast\u00b7mahl", "an\u00b7zu\u00b7stel\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo zwanzig sich, als wie durchs Loos, gesellen,", "tokens": ["Wo", "zwan\u00b7zig", "sich", ",", "als", "wie", "durchs", "Loos", ",", "ge\u00b7sel\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "CARD", "PRF", "$,", "KOUS", "KOKOM", "APPRART", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Geliebte Stadt! die war dir l\u00e4ngst bekannt;", "tokens": ["Ge\u00b7lieb\u00b7te", "Stadt", "!", "die", "war", "dir", "l\u00e4ngst", "be\u00b7kannt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$.", "ART", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Allein, die Kunst, drei, die von gleichen Sitten", "tokens": ["Al\u00b7lein", ",", "die", "Kunst", ",", "drei", ",", "die", "von", "glei\u00b7chen", "Sit\u00b7ten"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "ART", "NN", "$,", "CARD", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "---+-+-+-+-", "measure": "unknown.measure.tetra"}, "line.5": {"text": "Und Herzen sind, auf Ein Gericht zu bitten,", "tokens": ["Und", "Her\u00b7zen", "sind", ",", "auf", "Ein", "Ge\u00b7richt", "zu", "bit\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "$,", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die fremde Kunst wird Reichen nie genannt.", "tokens": ["Die", "frem\u00b7de", "Kunst", "wird", "Rei\u00b7chen", "nie", "ge\u00b7nannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Der einen kann es nicht an Schmeichlern fehlen;", "tokens": ["Der", "ei\u00b7nen", "kann", "es", "nicht", "an", "Schmeich\u00b7lern", "feh\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VMFIN", "PPER", "PTKNEG", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die andre wird mit Sorgfalt Freunde w\u00e4hlen.", "tokens": ["Die", "and\u00b7re", "wird", "mit", "Sorg\u00b7falt", "Freun\u00b7de", "w\u00e4h\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VAFIN", "APPR", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "O stolzes Geld, ach h\u00e4ttest du Verstand!", "tokens": ["O", "stol\u00b7zes", "Geld", ",", "ach", "h\u00e4t\u00b7test", "du", "Ver\u00b7stand", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "XY", "VAFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}