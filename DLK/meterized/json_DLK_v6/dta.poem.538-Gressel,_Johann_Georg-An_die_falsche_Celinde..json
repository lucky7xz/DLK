{"dta.poem.538": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "An die falsche  Celinde.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Celinde fahr nur hin/ ich mag dich nicht mehr lieben/", "tokens": ["Ce\u00b7lin\u00b7de", "fahr", "nur", "hin", "/", "ich", "mag", "dich", "nicht", "mehr", "lie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "PTKVZ", "$(", "PPER", "VMFIN", "PRF", "PTKNEG", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mich soll dein Wanckelmuth hinf\u00fchro nicht betr\u00fcben/", "tokens": ["Mich", "soll", "dein", "Wan\u00b7ckel\u00b7muth", "hin\u00b7f\u00fch\u00b7ro", "nicht", "be\u00b7tr\u00fc\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "ADV", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du ziehst mich nicht mehr auf/ ich kenne dich nun wol.", "tokens": ["Du", "ziehst", "mich", "nicht", "mehr", "auf", "/", "ich", "ken\u00b7ne", "dich", "nun", "wol", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "ADV", "APPR", "$(", "PPER", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und wei\u00df wie ich den Reitz der Augen fliehen soll.", "tokens": ["Und", "wei\u00df", "wie", "ich", "den", "Reitz", "der", "Au\u00b7gen", "flie\u00b7hen", "soll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOKOM", "PPER", "ART", "NN", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich spei\u00dfte mich vor dem mit lieblichen Gedancken/", "tokens": ["Ich", "spei\u00df\u00b7te", "mich", "vor", "dem", "mit", "lieb\u00b7li\u00b7chen", "Ge\u00b7dan\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "APPR", "ADJA", "NN", "$("], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Und glaubte deine Treu die w\u00fcrde niemahls wancken.", "tokens": ["Und", "glaub\u00b7te", "dei\u00b7ne", "Treu", "die", "w\u00fcr\u00b7de", "nie\u00b7mahls", "wan\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "ART", "VAFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Nun aber kenn\u2019 ich dich/ du l\u00e4st auch andre ein/", "tokens": ["Nun", "a\u00b7ber", "kenn'", "ich", "dich", "/", "du", "l\u00e4st", "auch", "and\u00b7re", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PRF", "$(", "PPER", "VVFIN", "ADV", "PIS", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Du f\u00fchrst mich hinters Licht/ und gibst mir falschen Schein.", "tokens": ["Du", "f\u00fchrst", "mich", "hin\u00b7ters", "Licht", "/", "und", "gibst", "mir", "fal\u00b7schen", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "$(", "KON", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ich sage nun ", "tokens": ["Ich", "sa\u00b7ge", "nun"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Ich frage nichts darnach/ allein ich will dir fehlen/", "tokens": ["Ich", "fra\u00b7ge", "nichts", "dar\u00b7nach", "/", "al\u00b7lein", "ich", "will", "dir", "feh\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "PAV", "$(", "ADV", "PPER", "VMFIN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Der M\u00e4dgens sind genug/ sie wachsen alle Jahr/", "tokens": ["Der", "M\u00e4d\u00b7gens", "sind", "ge\u00b7nug", "/", "sie", "wach\u00b7sen", "al\u00b7le", "Jahr", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$(", "PPER", "VVFIN", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und biehten sich wol selbst an einem Freyer dar.", "tokens": ["Und", "bieh\u00b7ten", "sich", "wol", "selbst", "an", "ei\u00b7nem", "Frey\u00b7er", "dar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADV", "ADV", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wir leben nun getrennt/ und sind also geschieden", "tokens": ["Wir", "le\u00b7ben", "nun", "ge\u00b7trennt", "/", "und", "sind", "al\u00b7so", "ge\u00b7schie\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "VVPP", "$(", "KON", "VAFIN", "ADV", "VVPP"], "meter": "-+-+-+-++--+-", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Ich lasse dich/ und du mich auch hinfort/ zu frieden.", "tokens": ["Ich", "las\u00b7se", "dich", "/", "und", "du", "mich", "auch", "hin\u00b7fort", "/", "zu", "frie\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$(", "KON", "PPER", "PRF", "ADV", "ADV", "$(", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Das M\u00e4dgen/ so mich will bey sich im Bette sehn/", "tokens": ["Das", "M\u00e4d\u00b7gen", "/", "so", "mich", "will", "bey", "sich", "im", "Bet\u00b7te", "sehn", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADV", "PPER", "VMFIN", "APPR", "PRF", "APPRART", "NN", "VVINF", "$("], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "Das mu\u00df mit jedermann nicht in Bekanntschafft stehn.", "tokens": ["Das", "mu\u00df", "mit", "je\u00b7der\u00b7mann", "nicht", "in", "Be\u00b7kannt\u00b7schafft", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "APPR", "PIS", "PTKNEG", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}