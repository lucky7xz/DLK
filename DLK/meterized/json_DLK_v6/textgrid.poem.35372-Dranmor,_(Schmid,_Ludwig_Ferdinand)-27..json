{"textgrid.poem.35372": {"metadata": {"author": {"name": "Dranmor, (Schmid, Ludwig Ferdinand)", "birth": "N.A.", "death": "N.A."}, "title": "27.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tod, der du meine innersten Gedanken", "tokens": ["Tod", ",", "der", "du", "mei\u00b7ne", "in\u00b7ners\u00b7ten", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Beherrschest, unbezwingbar, unaufhaltsam,", "tokens": ["Be\u00b7herr\u00b7schest", ",", "un\u00b7be\u00b7zwing\u00b7bar", ",", "un\u00b7auf\u00b7halt\u00b7sam", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "ADJD", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der du mein ganzes Sein durchdringst, gewaltsam", "tokens": ["Der", "du", "mein", "gan\u00b7zes", "Sein", "durch\u00b7dringst", ",", "ge\u00b7walt\u00b7sam"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "PPER", "PPOSAT", "ADJA", "NN", "VVFIN", "$,", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ersch\u00fctternd meines Wissens enge Schranken,", "tokens": ["Er\u00b7sch\u00fct\u00b7ternd", "mei\u00b7nes", "Wis\u00b7sens", "en\u00b7ge", "Schran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ich ringe furchtlos mich zu dir empor,", "tokens": ["Ich", "rin\u00b7ge", "furcht\u00b7los", "mich", "zu", "dir", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PPER", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Tod, den ich unsern Friedensspender nenne.", "tokens": ["Tod", ",", "den", "ich", "un\u00b7sern", "Frie\u00b7dens\u00b7spen\u00b7der", "nen\u00b7ne", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.7": {"text": "Doch, da\u00df die ganze Menschheit dich erkenne,", "tokens": ["Doch", ",", "da\u00df", "die", "gan\u00b7ze", "Menschheit", "dich", "er\u00b7ken\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Tritt endlich aus der D\u00e4mmerung hervor!", "tokens": ["Tritt", "end\u00b7lich", "aus", "der", "D\u00e4m\u00b7me\u00b7rung", "her\u00b7vor", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Es werde Tag! Vertrieben sei der Spuk,", "tokens": ["Es", "wer\u00b7de", "Tag", "!", "Ver\u00b7trie\u00b7ben", "sei", "der", "Spuk", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "$.", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Verflucht des Aberglaubens freches Spiel!", "tokens": ["Ver\u00b7flucht", "des", "A\u00b7berg\u00b7lau\u00b7bens", "fre\u00b7ches", "Spiel", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Verwelken mag der Gr\u00e4ber Blumenschmuck;", "tokens": ["Ver\u00b7wel\u00b7ken", "mag", "der", "Gr\u00e4\u00b7ber", "Blu\u00b7men\u00b7schmuck", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Zu Asche brenne, was dem Nichts verfiel!", "tokens": ["Zu", "A\u00b7sche", "bren\u00b7ne", ",", "was", "dem", "Nichts", "ver\u00b7fiel", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Was frommt der Kirche Segen einer Leiche,", "tokens": ["Was", "frommt", "der", "Kir\u00b7che", "Se\u00b7gen", "ei\u00b7ner", "Lei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Die tote Sprache, mit Vergeltung prahlend?", "tokens": ["Die", "to\u00b7te", "Spra\u00b7che", ",", "mit", "Ver\u00b7gel\u00b7tung", "prah\u00b7lend", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Lebendig ist das Wort, das sonnengleiche,", "tokens": ["Le\u00b7ben\u00b7dig", "ist", "das", "Wort", ",", "das", "son\u00b7nen\u00b7glei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.16": {"text": "In Millionen Herzen widerstrahlend,", "tokens": ["In", "Mil\u00b7lion\u00b7en", "Her\u00b7zen", "wi\u00b7der\u00b7strah\u00b7lend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.17": {"text": "Das Wort, das tausendj\u00e4hr'ge Siegel sprengt,", "tokens": ["Das", "Wort", ",", "das", "tau\u00b7send\u00b7j\u00e4hr'\u00b7ge", "Sie\u00b7gel", "sprengt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Der echte Glaube, der die Form zertr\u00fcmmert;", "tokens": ["Der", "ech\u00b7te", "Glau\u00b7be", ",", "der", "die", "Form", "zer\u00b7tr\u00fcm\u00b7mert", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Lebendig ist der Tod, der uns bek\u00fcmmert,", "tokens": ["Le\u00b7ben\u00b7dig", "ist", "der", "Tod", ",", "der", "uns", "be\u00b7k\u00fcm\u00b7mert", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.20": {"text": "Solang das Jenseits unsere Brust beengt.", "tokens": ["So\u00b7lang", "das", "Jen\u00b7seits", "un\u00b7se\u00b7re", "Brust", "be\u00b7engt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.21": {"text": "Was unser ist, was liebend wir umfassen,", "tokens": ["Was", "un\u00b7ser", "ist", ",", "was", "lie\u00b7bend", "wir", "um\u00b7fas\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VAFIN", "$,", "PRELS", "ADJD", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Verschmelze wieder sich mit der Natur,", "tokens": ["Ver\u00b7schmel\u00b7ze", "wie\u00b7der", "sich", "mit", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.23": {"text": "Und jene Sehnsucht, eine Feuerspur,", "tokens": ["Und", "je\u00b7ne", "Sehn\u00b7sucht", ",", "ei\u00b7ne", "Feu\u00b7er\u00b7spur", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Ein ausgepr\u00e4gtes Bild zur\u00fcckzulassen,", "tokens": ["Ein", "aus\u00b7ge\u00b7pr\u00e4g\u00b7tes", "Bild", "zu\u00b7r\u00fcck\u00b7zu\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Versinke in dem gro\u00dfen Weltgetriebe.", "tokens": ["Ver\u00b7sin\u00b7ke", "in", "dem", "gro\u00b7\u00dfen", "Welt\u00b7ge\u00b7trie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Der Drang des Schaffens, der sich selbst gen\u00fcgt,", "tokens": ["Der", "Drang", "des", "Schaf\u00b7fens", ",", "der", "sich", "selbst", "ge\u00b7n\u00fcgt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "PRELS", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Die Selbstverl\u00e4ugnung, die uns selten tr\u00fcgt,", "tokens": ["Die", "Selbst\u00b7ver\u00b7l\u00e4ug\u00b7nung", ",", "die", "uns", "sel\u00b7ten", "tr\u00fcgt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Das sind die Zeichen wahrer Gottesliebe.", "tokens": ["Das", "sind", "die", "Zei\u00b7chen", "wah\u00b7rer", "Got\u00b7tes\u00b7lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}