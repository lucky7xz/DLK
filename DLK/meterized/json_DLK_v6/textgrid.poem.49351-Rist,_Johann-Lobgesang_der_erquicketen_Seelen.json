{"textgrid.poem.49351": {"metadata": {"author": {"name": "Rist, Johann", "birth": "N.A.", "death": "N.A."}, "title": "Lobgesang der erquicketen Seelen", "genre": "verse", "period": "N.A.", "pub_year": 1637, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie sol ich gnug dich preisen,", "tokens": ["Wie", "sol", "ich", "gnug", "dich", "prei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie sol ich Dank erweisen", "tokens": ["Wie", "sol", "ich", "Dank", "er\u00b7wei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PPER", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dir, Jesu, s\u00fc\u00dfes Leben,", "tokens": ["Dir", ",", "Je\u00b7su", ",", "s\u00fc\u00b7\u00dfes", "Le\u00b7ben", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "NE", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df du mir Trost gegeben?", "tokens": ["Da\u00df", "du", "mir", "Trost", "ge\u00b7ge\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Nun kan ichs recht erkennen,", "tokens": ["Nun", "kan", "ichs", "recht", "er\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Da\u00df ich dein Kind zu nennen,", "tokens": ["Da\u00df", "ich", "dein", "Kind", "zu", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Dieweil du durch dein Sterben", "tokens": ["Die\u00b7weil", "du", "durch", "dein", "Ster\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Verhindert mein Verderben.", "tokens": ["Ver\u00b7hin\u00b7dert", "mein", "Ver\u00b7der\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Ich schwebt' in tausend N\u00f6ten,", "tokens": ["Ich", "schwebt'", "in", "tau\u00b7send", "N\u00f6\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Bald lie\u00dfest du dich t\u00f6ten,", "tokens": ["Bald", "lie\u00b7\u00dfest", "du", "dich", "t\u00f6\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Da\u00df ja der S\u00fcnden B\u00fcrde.", "tokens": ["Da\u00df", "ja", "der", "S\u00fcn\u00b7den", "B\u00fcr\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Dadurch erhoben w\u00fcrde.", "tokens": ["Da\u00b7durch", "er\u00b7ho\u00b7ben", "w\u00fcr\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Lob sei dir, Herr, gesungen,", "tokens": ["Lob", "sei", "dir", ",", "Herr", ",", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "$,", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Da\u00df du f\u00fcr mich gerungen", "tokens": ["Da\u00df", "du", "f\u00fcr", "mich", "ge\u00b7run\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Am Oelberg' und erhitzet", "tokens": ["Am", "O\u00b7el\u00b7ber\u00b7g'", "und", "er\u00b7hit\u00b7zet"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "KON", "VVFIN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.16": {"text": "Hast h\u00e4ufig Blut geschwitzet.", "tokens": ["Hast", "h\u00e4u\u00b7fig", "Blut", "ge\u00b7schwit\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "Lob sei dir, Herr, gesaget,", "tokens": ["Lob", "sei", "dir", ",", "Herr", ",", "ge\u00b7sa\u00b7get", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "$,", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "Da\u00df du den Kampf gewaget", "tokens": ["Da\u00df", "du", "den", "Kampf", "ge\u00b7wa\u00b7get"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Und, als der W\u00fcrger kommen,", "tokens": ["Und", ",", "als", "der", "W\u00fcr\u00b7ger", "kom\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.20": {"text": "Ihm hast die Macht genommen.", "tokens": ["Ihm", "hast", "die", "Macht", "ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.21": {"text": "Ich preise dich von Herzen,", "tokens": ["Ich", "prei\u00b7se", "dich", "von", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.22": {"text": "Da\u00df du so bittre Schmerzen", "tokens": ["Da\u00df", "du", "so", "bitt\u00b7re", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.23": {"text": "In Ketten und in Banden", "tokens": ["In", "Ket\u00b7ten", "und", "in", "Ban\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.24": {"text": "F\u00fcr mich hast ausgestanden.", "tokens": ["F\u00fcr", "mich", "hast", "aus\u00b7ge\u00b7stan\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.25": {"text": "Ich lebt' im Lasterorden,", "tokens": ["Ich", "lebt'", "im", "Las\u00b7ter\u00b7or\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.26": {"text": "Du bist verstricket worden;", "tokens": ["Du", "bist", "ver\u00b7stri\u00b7cket", "wor\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "VAPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.27": {"text": "Die S\u00fcnd' hab' ich begangen,", "tokens": ["Die", "S\u00fcnd'", "hab'", "ich", "be\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.28": {"text": "Daf\u00fcr bist du gefangen.", "tokens": ["Da\u00b7f\u00fcr", "bist", "du", "ge\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.29": {"text": "Man solte mich verklagen,", "tokens": ["Man", "sol\u00b7te", "mich", "ver\u00b7kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.30": {"text": "Drauf haben dich geschlagen", "tokens": ["Drauf", "ha\u00b7ben", "dich", "ge\u00b7schla\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.31": {"text": "Die Buben in die Wette,", "tokens": ["Die", "Bu\u00b7ben", "in", "die", "Wet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.32": {"text": "Nur da\u00df ich Frieden h\u00e4tte.", "tokens": ["Nur", "da\u00df", "ich", "Frie\u00b7den", "h\u00e4t\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "NN", "VAFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.33": {"text": "Wie kan ich dich gnug loben,", "tokens": ["Wie", "kan", "ich", "dich", "gnug", "lo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.34": {"text": "Da\u00df du der Feinde Toben,", "tokens": ["Da\u00df", "du", "der", "Fein\u00b7de", "To\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.35": {"text": "Ihr Schm\u00e4hen, Schelten, Neiden", "tokens": ["Ihr", "Schm\u00e4\u00b7hen", ",", "Schel\u00b7ten", ",", "Nei\u00b7den"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["PPOSAT", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.36": {"text": "F\u00fcr mich hast wollen leiden?", "tokens": ["F\u00fcr", "mich", "hast", "wol\u00b7len", "lei\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.37": {"text": "Wie kan ichs gnug erheben,", "tokens": ["Wie", "kan", "ichs", "gnug", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PIS", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.38": {"text": "Da\u00df du dein Haubt gegeben", "tokens": ["Da\u00df", "du", "dein", "Haubt", "ge\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.39": {"text": "Zum Schauspiel und die Spitzen", "tokens": ["Zum", "Schau\u00b7spiel", "und", "die", "Spit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.40": {"text": "Des Dorns es lassen ritzen?", "tokens": ["Des", "Dorns", "es", "las\u00b7sen", "rit\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVINF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.41": {"text": "Dein Leib, der ganz zuschlagen,", "tokens": ["Dein", "Leib", ",", "der", "ganz", "zu\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.42": {"text": "Must auch erb\u00e4rmlich tragen", "tokens": ["Must", "auch", "er\u00b7b\u00e4rm\u00b7lich", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADV", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.43": {"text": "Das Kreuz um meinetwillen,", "tokens": ["Das", "Kreuz", "um", "mei\u00b7net\u00b7wil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.44": {"text": "Des Vaters Zorn zu stillen.", "tokens": ["Des", "Va\u00b7ters", "Zorn", "zu", "stil\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.45": {"text": "Du bist ja zugesellet", "tokens": ["Du", "bist", "ja", "zu\u00b7ge\u00b7sel\u00b7let"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.46": {"text": "Den M\u00f6rdern und gestellet", "tokens": ["Den", "M\u00f6r\u00b7dern", "und", "ge\u00b7stel\u00b7let"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.47": {"text": "Zum Scheusal allen Heiden;", "tokens": ["Zum", "Scheu\u00b7sal", "al\u00b7len", "Hei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.48": {"text": "O welch' ein schrecklichs Leiden!", "tokens": ["O", "welch'", "ein", "schreck\u00b7lichs", "Lei\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PWAT", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.49": {"text": "Doch alle diese Schmerzen", "tokens": ["Doch", "al\u00b7le", "die\u00b7se", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIS", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.50": {"text": "Erlittest du von Herzen,", "tokens": ["Er\u00b7lit\u00b7test", "du", "von", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.51": {"text": "Dein Blut must' h\u00e4ufig flie\u00dfen,", "tokens": ["Dein", "Blut", "must'", "h\u00e4u\u00b7fig", "flie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.52": {"text": "Nur meinen Fall zu b\u00fc\u00dfen.", "tokens": ["Nur", "mei\u00b7nen", "Fall", "zu", "b\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.53": {"text": "Ei, solt' ich mich mit Thr\u00e4nen", "tokens": ["Ei", ",", "solt'", "ich", "mich", "mit", "Thr\u00e4\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "VMFIN", "PPER", "PRF", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.54": {"text": "Nun auch nach dir nicht sehnen,", "tokens": ["Nun", "auch", "nach", "dir", "nicht", "seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.55": {"text": "Der du mirs hast erworben,", "tokens": ["Der", "du", "mirs", "hast", "er\u00b7wor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "NE", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.56": {"text": "Da\u00df ich nicht gar verdorben?", "tokens": ["Da\u00df", "ich", "nicht", "gar", "ver\u00b7dor\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.57": {"text": "Wolan, es bleibt versenket", "tokens": ["Wo\u00b7lan", ",", "es", "bleibt", "ver\u00b7sen\u00b7ket"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "PPER", "VVFIN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.58": {"text": "Die Schuld, so mich gekr\u00e4nket,", "tokens": ["Die", "Schuld", ",", "so", "mich", "ge\u00b7kr\u00e4n\u00b7ket", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.59": {"text": "Drauf preis' ich deinen Namen,", "tokens": ["Drauf", "preis'", "ich", "dei\u00b7nen", "Na\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.60": {"text": "O Jesu, Helfer, Amen.", "tokens": ["O", "Je\u00b7su", ",", "Hel\u00b7fer", ",", "A\u00b7men", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "NE", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}