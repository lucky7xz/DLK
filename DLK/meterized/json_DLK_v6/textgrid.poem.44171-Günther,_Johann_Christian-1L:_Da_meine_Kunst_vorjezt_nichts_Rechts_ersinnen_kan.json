{"textgrid.poem.44171": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Da meine Kunst vorjezt nichts Rechts ersinnen kan", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da meine Kunst vorjezt nichts Rechts ersinnen kan", "tokens": ["Da", "mei\u00b7ne", "Kunst", "vor\u00b7jezt", "nichts", "Rechts", "er\u00b7sin\u00b7nen", "kan"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "PIS", "ADV", "VVINF", "VMFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und doch verbunden ist, ein Denckmahl herzuschreiben,", "tokens": ["Und", "doch", "ver\u00b7bun\u00b7den", "ist", ",", "ein", "Denck\u00b7mahl", "her\u00b7zu\u00b7schrei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "VAFIN", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So las mich, werther Freund, stets deinen Freund verbleiben,", "tokens": ["So", "las", "mich", ",", "wert\u00b7her", "Freund", ",", "stets", "dei\u00b7nen", "Freund", "ver\u00b7blei\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und dencke so an mich wie an den Hercomann!", "tokens": ["Und", "den\u00b7cke", "so", "an", "mich", "wie", "an", "den", "Her\u00b7co\u00b7mann", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "PPER", "KOKOM", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}