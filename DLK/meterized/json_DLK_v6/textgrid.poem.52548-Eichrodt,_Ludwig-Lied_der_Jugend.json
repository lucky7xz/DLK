{"textgrid.poem.52548": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Lied der Jugend", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich vierzig Jahre bin,", "tokens": ["Wenn", "ich", "vier\u00b7zig", "Jah\u00b7re", "bin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "CARD", "NN", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Will ich weise werden,", "tokens": ["Will", "ich", "wei\u00b7se", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "VAINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Oder einen andern Sinn", "tokens": ["O\u00b7der", "ei\u00b7nen", "an\u00b7dern", "Sinn"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Gebt mir und Geberden!", "tokens": ["Gebt", "mir", "und", "Ge\u00b7ber\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "KON", "NN", "$."], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.5": {"text": "Ja, ich trotze der Gefahr", "tokens": ["Ja", ",", "ich", "trot\u00b7ze", "der", "Ge\u00b7fahr"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "In des Lebens St\u00fcrmen,", "tokens": ["In", "des", "Le\u00b7bens", "St\u00fcr\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Mag sie um mich her sogar", "tokens": ["Mag", "sie", "um", "mich", "her", "so\u00b7gar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "PRF", "APZR", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Wellenhoch sich th\u00fcrmen.", "tokens": ["Wel\u00b7len\u00b7hoch", "sich", "th\u00fcr\u00b7men", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PRF", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Billig ist und leicht genug,", "tokens": ["Bil\u00b7lig", "ist", "und", "leicht", "ge\u00b7nug", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "KON", "ADJD", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Was ihr lehrt zu meiden,", "tokens": ["Was", "ihr", "lehrt", "zu", "mei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Auszuweichen feig und klug", "tokens": ["Aus\u00b7zu\u00b7wei\u00b7chen", "feig", "und", "klug"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Menschenw\u00fcrdgen Leiden;", "tokens": ["Men\u00b7schen\u00b7w\u00fcrd\u00b7gen", "Lei\u00b7den", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Und zufrieden freut ihr euch", "tokens": ["Und", "zu\u00b7frie\u00b7den", "freut", "ihr", "euch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Eures d\u00fcrftgen Looses,", "tokens": ["Eu\u00b7res", "d\u00fcrft\u00b7gen", "Loo\u00b7ses", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Und verbannt aus eurem Reich", "tokens": ["Und", "ver\u00b7bannt", "aus", "eu\u00b7rem", "Reich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "K\u00fchnes ist und Gro\u00dfes.", "tokens": ["K\u00fch\u00b7nes", "ist", "und", "Gro\u00b7\u00dfes", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "KON", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Wie nach Ungewittern nur", "tokens": ["Wie", "nach", "Un\u00b7ge\u00b7wit\u00b7tern", "nur"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Unser Athem schmachtet,", "tokens": ["Un\u00b7ser", "A\u00b7them", "schmach\u00b7tet", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wenn es \u00fcber schw\u00fcler Flur", "tokens": ["Wenn", "es", "\u00fc\u00b7ber", "schw\u00fc\u00b7ler", "Flur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ohne K\u00fchlung nachtet,", "tokens": ["Oh\u00b7ne", "K\u00fch\u00b7lung", "nach\u00b7tet", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "So verlanget allezeit", "tokens": ["So", "ver\u00b7lan\u00b7get", "al\u00b7le\u00b7zeit"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Uns nach Leidenschaften,", "tokens": ["Uns", "nach", "Lei\u00b7den\u00b7schaf\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Weil wir in Allt\u00e4glichkeit", "tokens": ["Weil", "wir", "in", "All\u00b7t\u00e4g\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Welkten und erschlafften.", "tokens": ["Welk\u00b7ten", "und", "er\u00b7schlaff\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Die wir jung und muthig sind,", "tokens": ["Die", "wir", "jung", "und", "mut\u00b7hig", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "KON", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Lassen Segel schwellen", "tokens": ["Las\u00b7sen", "Se\u00b7gel", "schwel\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["NE", "NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Mit der Leidenschaften Wind,", "tokens": ["Mit", "der", "Lei\u00b7den\u00b7schaf\u00b7ten", "Wind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf emp\u00f6rten Wellen,", "tokens": ["Auf", "em\u00b7p\u00f6r\u00b7ten", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "W\u00e4hrend, die da weise sind,", "tokens": ["W\u00e4h\u00b7rend", ",", "die", "da", "wei\u00b7se", "sind", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PRELS", "ADV", "ADV", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Rudern fern der Scylle", "tokens": ["Ru\u00b7dern", "fern", "der", "Scyl\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADJD", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Und Charybdis ohne Wind", "tokens": ["Und", "Cha\u00b7ryb\u00b7dis", "oh\u00b7ne", "Wind"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NE", "APPR", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.8": {"text": "Durch die Wasserstille.", "tokens": ["Durch", "die", "Was\u00b7ser\u00b7stil\u00b7le", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}