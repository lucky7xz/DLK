{"dta.poem.10476": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "XxXIII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Sie liebten sich beide, doch keiner", "tokens": ["Sie", "lieb\u00b7ten", "sich", "bei\u00b7de", ",", "doch", "kei\u00b7ner"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "PIS", "$,", "ADV", "PIS"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Wollt' es dem andern gestehn;", "tokens": ["Wollt'", "es", "dem", "an\u00b7dern", "ge\u00b7stehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Sie sahen sich an so feindlich,", "tokens": ["Sie", "sa\u00b7hen", "sich", "an", "so", "feind\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ADV", "ADJD", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und wollten vor Liebe vergehn.", "tokens": ["Und", "woll\u00b7ten", "vor", "Lie\u00b7be", "ver\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "NN", "VVINF", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Sie trennten sich endlich und sah'n sich", "tokens": ["Sie", "trenn\u00b7ten", "sich", "end\u00b7lich", "und", "sah'n", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "KON", "VVFIN", "PRF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Nur noch zuweilen im Traum;", "tokens": ["Nur", "noch", "zu\u00b7wei\u00b7len", "im", "Traum", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "APPRART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Sie waren l\u00e4ngst gestorben,", "tokens": ["Sie", "wa\u00b7ren", "l\u00e4ngst", "ge\u00b7stor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und wu\u00dften es selber kaum.", "tokens": ["Und", "wu\u00df\u00b7ten", "es", "sel\u00b7ber", "kaum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}