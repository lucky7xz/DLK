{"dta.poem.10539": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Ii.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.85", "af:0.14"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Die Mutter Gottes zu Kevlaar", "tokens": ["Die", "Mut\u00b7ter", "Got\u00b7tes", "zu", "Kev\u00b7laar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "APPR", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Tr\u00e4gt heut' ihr bestes Kleid;", "tokens": ["Tr\u00e4gt", "heut'", "ihr", "bes\u00b7tes", "Kleid", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Heut' hat sie viel zu schaffen,", "tokens": ["Heut'", "hat", "sie", "viel", "zu", "schaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Es kommen viel' kranke Leut'.", "tokens": ["Es", "kom\u00b7men", "viel'", "kran\u00b7ke", "Leut'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Die kranken Leute bringen", "tokens": ["Die", "kran\u00b7ken", "Leu\u00b7te", "brin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihr dar, als Opferspend',", "tokens": ["Ihr", "dar", ",", "als", "Op\u00b7fer\u00b7spend'", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$,", "KOUS", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Aus Wachs gebildete Glieder,", "tokens": ["Aus", "Wachs", "ge\u00b7bil\u00b7de\u00b7te", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Viel w\u00e4chserne F\u00fc\u00df' und H\u00e4nd'.", "tokens": ["Viel", "w\u00e4ch\u00b7ser\u00b7ne", "F\u00fc\u00df'", "und", "H\u00e4nd'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Und wer eine Wachshand opfert,", "tokens": ["Und", "wer", "ei\u00b7ne", "Wachs\u00b7hand", "op\u00b7fert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Dem heilt an der Hand die Wund';", "tokens": ["Dem", "heilt", "an", "der", "Hand", "die", "Wund'", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und wer einen Wachsfu\u00df opfert,", "tokens": ["Und", "wer", "ei\u00b7nen", "Wachs\u00b7fu\u00df", "op\u00b7fert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "VVFIN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Dem wird der Fu\u00df gesund.", "tokens": ["Dem", "wird", "der", "Fu\u00df", "ge\u00b7sund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Nach Kevlaar ging Mancher auf Kr\u00fccken,", "tokens": ["Nach", "Kev\u00b7laar", "ging", "Man\u00b7cher", "auf", "Kr\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PIS", "APPR", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Der jetzo tanzt auf dem Seil',", "tokens": ["Der", "jet\u00b7zo", "tanzt", "auf", "dem", "Seil'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Gar Mancher spielt jetzt die Bratsche,", "tokens": ["Gar", "Man\u00b7cher", "spielt", "jetzt", "die", "Brat\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Dem dort kein Finger war heil.", "tokens": ["Dem", "dort", "kein", "Fin\u00b7ger", "war", "heil", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "PIAT", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "Die Mutter nahm ein Wachslicht,", "tokens": ["Die", "Mut\u00b7ter", "nahm", "ein", "Wachs\u00b7licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und bildete d'raus ein Herz.", "tokens": ["Und", "bil\u00b7de\u00b7te", "d'raus", "ein", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "\u201ebring das der Mutter Gottes,", "tokens": ["\u201e", "bring", "das", "der", "Mut\u00b7ter", "Got\u00b7tes", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PDS", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dann heilt sie deinen Schmerz.\u201c", "tokens": ["Dann", "heilt", "sie", "dei\u00b7nen", "Schmerz", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Der Sohn nahm seufzend das Wachsherz", "tokens": ["Der", "Sohn", "nahm", "seuf\u00b7zend", "das", "Wachs\u00b7herz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ging seufzend zum Heiligenbild;", "tokens": ["Ging", "seuf\u00b7zend", "zum", "Hei\u00b7li\u00b7gen\u00b7bild", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPRART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Die Thr\u00e4ne quillt aus dem Auge,", "tokens": ["Die", "Thr\u00e4\u00b7ne", "quillt", "aus", "dem", "Au\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Das Wort aus dem Herzen quillt:", "tokens": ["Das", "Wort", "aus", "dem", "Her\u00b7zen", "quillt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "\u201edu Hochgebenedeite,", "tokens": ["\u201e", "du", "Hoch\u00b7ge\u00b7be\u00b7ne\u00b7dei\u00b7te", ","], "token_info": ["punct", "word", "word", "punct"], "pos": ["$(", "PPER", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du reine Gottesmagd,", "tokens": ["Du", "rei\u00b7ne", "Got\u00b7tes\u00b7magd", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du K\u00f6nigin des Himmels,", "tokens": ["Du", "K\u00f6\u00b7ni\u00b7gin", "des", "Him\u00b7mels", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dir sey mein Leid geklagt!", "tokens": ["Dir", "sey", "mein", "Leid", "ge\u00b7klagt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "\u201eich wohnte mit meiner Mutter", "tokens": ["\u201e", "ich", "wohn\u00b7te", "mit", "mei\u00b7ner", "Mut\u00b7ter"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Zu C\u00f6llen in der Stadt,", "tokens": ["Zu", "C\u00f6l\u00b7len", "in", "der", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Stadt, die viele hundert", "tokens": ["Der", "Stadt", ",", "die", "vie\u00b7le", "hun\u00b7dert"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PIAT", "CARD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Kapellen und Kirchen hat.", "tokens": ["Ka\u00b7pel\u00b7len", "und", "Kir\u00b7chen", "hat", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.9": {"line.1": {"text": "\u201eund neben uns wohnte Gretchen,", "tokens": ["\u201e", "und", "ne\u00b7ben", "uns", "wohn\u00b7te", "Gret\u00b7chen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "APPR", "PPER", "VVFIN", "NE", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Doch die ist todt jetzund \u2014", "tokens": ["Doch", "die", "ist", "todt", "je\u00b7tzund"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "ADJD", "ADV", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Marie, dir bring' ich ein Wachsherz,", "tokens": ["Ma\u00b7rie", ",", "dir", "bring'", "ich", "ein", "Wachs\u00b7herz", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Heil' du meine Herzenswund'.", "tokens": ["Heil'", "du", "mei\u00b7ne", "Her\u00b7zens\u00b7wund'", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "\u201eheil' Du mein krankes Herze,", "tokens": ["\u201e", "heil'", "Du", "mein", "kran\u00b7kes", "Her\u00b7ze", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "PPOSAT", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich will auch sp\u00e4t und fr\u00fch'", "tokens": ["Ich", "will", "auch", "sp\u00e4t", "und", "fr\u00fch'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "ADJD", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Inbr\u00fcnstiglich beten und singen:", "tokens": ["In\u00b7br\u00fcns\u00b7tig\u00b7lich", "be\u00b7ten", "und", "sin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "KON", "VVFIN", "$."], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.4": {"text": "Gelobt seyst du, Marie!\u201c", "tokens": ["Ge\u00b7lobt", "seyst", "du", ",", "Ma\u00b7rie", "!", "\u201c"], "token_info": ["word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "$,", "NE", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}