{"dta.poem.9703": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Madrigal  \n Auf die vielen kr\u00e4hen in  \u2012 \u2012  \n E. N.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.85", "it:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Man fragte mich in einer compagnie:", "tokens": ["Man", "frag\u00b7te", "mich", "in", "ei\u00b7ner", "com\u00b7pag\u00b7nie", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "APPR", "ART", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Warum ist doch so viel geschmei\u00dfe hie", "tokens": ["Wa\u00b7rum", "ist", "doch", "so", "viel", "ge\u00b7schmei\u00b7\u00dfe", "hie"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "ADV", "ADV", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Von kr\u00e4hen und von raben?", "tokens": ["Von", "kr\u00e4\u00b7hen", "und", "von", "ra\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "KON", "APPR", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich sagte: weil sie gute nahrung haben/", "tokens": ["Ich", "sag\u00b7te", ":", "weil", "sie", "gu\u00b7te", "nah\u00b7rung", "ha\u00b7ben", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "KOUS", "PPER", "ADJA", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "So lassen sie sich in der menge sehen;", "tokens": ["So", "las\u00b7sen", "sie", "sich", "in", "der", "men\u00b7ge", "se\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Denn wo viel schinder seyn/", "tokens": ["Denn", "wo", "viel", "schin\u00b7der", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADV", "ADJD", "VAINF", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Da gibt es auch viel kr\u00e4hen.", "tokens": ["Da", "gibt", "es", "auch", "viel", "kr\u00e4\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}