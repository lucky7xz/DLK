{"textgrid.poem.53057": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: War ists, ewer Jugendt Zier,", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "War ists, ewer Jugendt Zier,", "tokens": ["War", "ists", ",", "e\u00b7wer", "Ju\u00b7gendt", "Zier", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "VAFIN", "$,", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sch\u00f6ne Jungfraw, kompt mir f\u00fcr,", "tokens": ["Sch\u00f6\u00b7ne", "Jung\u00b7fraw", ",", "kompt", "mir", "f\u00fcr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PPER", "APPR", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie die Burg, so mit dem Wall", "tokens": ["Wie", "die", "Burg", ",", "so", "mit", "dem", "Wall"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "$,", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist versehen \u00fcberall.", "tokens": ["Ist", "ver\u00b7se\u00b7hen", "\u00fc\u00b7be\u00b7rall", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch weil nichts so starck vnd fest", "tokens": ["Doch", "weil", "nichts", "so", "starck", "vnd", "fest"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PIS", "ADV", "ADJD", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ist zu keiner Zeit gewest,", "tokens": ["Ist", "zu", "kei\u00b7ner", "Zeit", "ge\u00b7west", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PIAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Da\u00df es nicht durch grosse Macht", "tokens": ["Da\u00df", "es", "nicht", "durch", "gros\u00b7se", "Macht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Zur Verw\u00fcstung wer gebracht,", "tokens": ["Zur", "Ver\u00b7w\u00fcs\u00b7tung", "wer", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PWS", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Wird auch, vnd zwar ohne Blitz,", "tokens": ["Wird", "auch", ",", "vnd", "zwar", "oh\u00b7ne", "Blitz", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "$,", "KON", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Ohne donnern der Gesch\u00fctz'", "tokens": ["Oh\u00b7ne", "don\u00b7nern", "der", "Ge\u00b7sch\u00fctz'"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Morgen vor dem SonnenSchein", "tokens": ["Mor\u00b7gen", "vor", "dem", "Son\u00b7nen", "Schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Diese Burg erstiegen seyn.", "tokens": ["Die\u00b7se", "Burg", "er\u00b7stie\u00b7gen", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVPP", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}