{"textgrid.poem.54401": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "10. Gedichte", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie munter sahst du dich nach Thirsis Heerden um?", "tokens": ["Wie", "mun\u00b7ter", "sahst", "du", "dich", "nach", "Thir\u00b7sis", "Heer\u00b7den", "um", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "PRF", "APPR", "NE", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dein Auge war entbrannt; wie schlug dir nicht das Herz!", "tokens": ["Dein", "Au\u00b7ge", "war", "ent\u00b7brannt", ";", "wie", "schlug", "dir", "nicht", "das", "Herz", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$.", "PWAV", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das Feuer in der Brust brach auf durch Stirn und Wangen;", "tokens": ["Das", "Feu\u00b7er", "in", "der", "Brust", "brach", "auf", "durch", "Stirn", "und", "Wan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "APPR", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Kurz, Liebe Furcht und Pein verdoppelten die Flammen.", "tokens": ["Kurz", ",", "Lie\u00b7be", "Furcht", "und", "Pein", "ver\u00b7dop\u00b7pel\u00b7ten", "die", "Flam\u00b7men", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJA", "NN", "KON", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}, "line.5": {"text": "Ich sah dir zwar mit Lust, doch auch mit Schmerzen zu,", "tokens": ["Ich", "sah", "dir", "zwar", "mit", "Lust", ",", "doch", "auch", "mit", "Schmer\u00b7zen", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "NN", "$,", "ADV", "ADV", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und dachte bey mir selbst: wo bleibt da Schwur und Treu?", "tokens": ["Und", "dach\u00b7te", "bey", "mir", "selbst", ":", "wo", "bleibt", "da", "Schwur", "und", "Treu", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "ADV", "$.", "PWAV", "VVFIN", "ADV", "VVFIN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Du wurdest bla\u00df und roth, das muste dich verrahten,", "tokens": ["Du", "wur\u00b7dest", "bla\u00df", "und", "roth", ",", "das", "mus\u00b7te", "dich", "ver\u00b7rah\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "KON", "ADJD", "$,", "PDS", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Als dich Endimion um deine Heerden fragte.", "tokens": ["Als", "dich", "En\u00b7di\u00b7mi\u00b7on", "um", "dei\u00b7ne", "Heer\u00b7den", "frag\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.9": {"text": "Was sollt ich Armer thun? ich eilte von dem Platz,", "tokens": ["Was", "sollt", "ich", "Ar\u00b7mer", "thun", "?", "ich", "eil\u00b7te", "von", "dem", "Platz", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "NN", "VVINF", "$.", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Da ich sonst so vergn\u00fcgt an deiner Seite sa\u00df;", "tokens": ["Da", "ich", "sonst", "so", "ver\u00b7gn\u00fcgt", "an", "dei\u00b7ner", "Sei\u00b7te", "sa\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+++-+-+-+-+", "measure": "unknown.measure.septa"}, "line.11": {"text": "Und stehe noch bet\u00e4ubt, von dem was ich geh\u00f6ret.", "tokens": ["Und", "ste\u00b7he", "noch", "be\u00b7t\u00e4ubt", ",", "von", "dem", "was", "ich", "ge\u00b7h\u00f6\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "VVPP", "$,", "APPR", "ART", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "So sprach Endimion voll Unmuth und Gedanken,", "tokens": ["So", "sprach", "En\u00b7di\u00b7mi\u00b7on", "voll", "Un\u00b7muth", "und", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Doch Chloris kam dazu und bot ihm einen Gru\u00df;", "tokens": ["Doch", "Chlo\u00b7ris", "kam", "da\u00b7zu", "und", "bot", "ihm", "ei\u00b7nen", "Gru\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "PAV", "KON", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Sie neigte ihren Stab, und sprach: Endimion,", "tokens": ["Sie", "neig\u00b7te", "ih\u00b7ren", "Stab", ",", "und", "sprach", ":", "En\u00b7di\u00b7mi\u00b7on", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "KON", "VVFIN", "$.", "NN", "$,"], "meter": "-+-+-+-++--+", "measure": "iambic.hexa.chol"}, "line.15": {"text": "Wie konnst du zornig seyn, da ich doch nichts verbrochen?", "tokens": ["Wie", "konnst", "du", "zor\u00b7nig", "seyn", ",", "da", "ich", "doch", "nichts", "ver\u00b7bro\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADJD", "VAINF", "$,", "KOUS", "PPER", "ADV", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Was Thirsis mir gesagt, da\u00df kann ich auch erzehlen:", "tokens": ["Was", "Thir\u00b7sis", "mir", "ge\u00b7sagt", ",", "da\u00df", "kann", "ich", "auch", "er\u00b7zeh\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "PPER", "VVPP", "$,", "KOUS", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Er fragte nach dem Lamm, das gestern sich verirrt,", "tokens": ["Er", "frag\u00b7te", "nach", "dem", "Lamm", ",", "das", "ge\u00b7stern", "sich", "ver\u00b7irrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,", "PRELS", "ADV", "PRF", "VVPP", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "Er fragte selbst nach dir, nach deiner Heerd und Trift;", "tokens": ["Er", "frag\u00b7te", "selbst", "nach", "dir", ",", "nach", "dei\u00b7ner", "Heerd", "und", "Trift", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPER", "$,", "APPR", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Er fragte, ob ich dich auch \u00f6fters hier gesehen?", "tokens": ["Er", "frag\u00b7te", ",", "ob", "ich", "dich", "auch", "\u00f6f\u00b7ters", "hier", "ge\u00b7se\u00b7hen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "PRF", "ADV", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Er fragte wo die Fluhr der Doris anzutreffen.", "tokens": ["Er", "frag\u00b7te", "wo", "die", "Fluhr", "der", "Do\u00b7ris", "an\u00b7zu\u00b7tref\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PWAV", "ART", "NN", "ART", "NE", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Mich d\u00fcnkt; dies alles kann dir nicht zuwider seyn.", "tokens": ["Mich", "d\u00fcnkt", ";", "dies", "al\u00b7les", "kann", "dir", "nicht", "zu\u00b7wi\u00b7der", "seyn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PDS", "PIS", "VMFIN", "PPER", "PTKNEG", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Gesetzt, ich h\u00e4tte mich dabey auch oft entf\u00e4rbt;", "tokens": ["Ge\u00b7setzt", ",", "ich", "h\u00e4t\u00b7te", "mich", "da\u00b7bey", "auch", "oft", "ent\u00b7f\u00e4rbt", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PPER", "VAFIN", "PRF", "PAV", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Ich sah das erste mal den Thirsis auf der Heyde,", "tokens": ["Ich", "sah", "das", "ers\u00b7te", "mal", "den", "Thir\u00b7sis", "auf", "der", "Hey\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "ADV", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Du kennst mein sch\u00fcchternes, mein allzubl\u00f6des Wesen,", "tokens": ["Du", "kennst", "mein", "sch\u00fcch\u00b7ter\u00b7nes", ",", "mein", "all\u00b7zu\u00b7bl\u00f6\u00b7des", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Das \u00fcberzeuget dich, von meiner Redlichkeit.", "tokens": ["Das", "\u00fc\u00b7berz\u00b7eu\u00b7get", "dich", ",", "von", "mei\u00b7ner", "Red\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Ja Chloris, sagt er drauf: dein Sch\u00e4fer der vergiebt", "tokens": ["Ja", "Chlo\u00b7ris", ",", "sagt", "er", "drauf", ":", "dein", "Sch\u00e4\u00b7fer", "der", "ver\u00b7giebt"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "NE", "$,", "VVFIN", "PPER", "PTKVZ", "$.", "PPOSAT", "NN", "ART", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Dir alles was geschehn, nur bleibe mir ergeben.", "tokens": ["Dir", "al\u00b7les", "was", "ge\u00b7schehn", ",", "nur", "blei\u00b7be", "mir", "er\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "PWS", "VVPP", "$,", "ADV", "VVFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Mich schl\u00e4fert; setze dich, und h\u00fcte nur die Schaafe.", "tokens": ["Mich", "schl\u00e4\u00b7fert", ";", "set\u00b7ze", "dich", ",", "und", "h\u00fc\u00b7te", "nur", "die", "Schaa\u00b7fe", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "VVFIN", "PPER", "$,", "KON", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Kaum als er dies gesagt, so schlief er ruhig ein.", "tokens": ["Kaum", "als", "er", "dies", "ge\u00b7sagt", ",", "so", "schlief", "er", "ru\u00b7hig", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "PDS", "VVPP", "$,", "ADV", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Sie gieng und suchte sich den allersch\u00f6nsten Ast,", "tokens": ["Sie", "gieng", "und", "such\u00b7te", "sich", "den", "al\u00b7ler\u00b7sch\u00f6ns\u00b7ten", "Ast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Und schnitt ein nettes Rohr, den Sch\u00e4fer zu beschenken.", "tokens": ["Und", "schnitt", "ein", "net\u00b7tes", "Rohr", ",", "den", "Sch\u00e4\u00b7fer", "zu", "be\u00b7schen\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Ihr Vater, welcher sich an diesen Baum gelehnet,", "tokens": ["Ihr", "Va\u00b7ter", ",", "wel\u00b7cher", "sich", "an", "die\u00b7sen", "Baum", "ge\u00b7leh\u00b7net", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PRF", "APPR", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Der sprach: mein liebstes Kind, komm schele mir den Stamm,", "tokens": ["Der", "sprach", ":", "mein", "liebs\u00b7tes", "Kind", ",", "komm", "sche\u00b7le", "mir", "den", "Stamm", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$.", "PPOSAT", "ADJA", "NN", "$,", "VVFIN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Und reiche mir den Bast, und kn\u00fcpf mir Schu und Strumpf.", "tokens": ["Und", "rei\u00b7che", "mir", "den", "Bast", ",", "und", "kn\u00fcpf", "mir", "Schu", "und", "Strumpf", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "$,", "KON", "VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "O h\u00e4tt ich einen Trunk von diesen klaren B\u00e4chen!", "tokens": ["O", "h\u00e4tt", "ich", "ei\u00b7nen", "Trunk", "von", "die\u00b7sen", "kla\u00b7ren", "B\u00e4\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ART", "NN", "APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Ich bin so alt und krank, kann mich an nichts erholen.", "tokens": ["Ich", "bin", "so", "alt", "und", "krank", ",", "kann", "mich", "an", "nichts", "er\u00b7ho\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$,", "VMFIN", "PRF", "APPR", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Mich abgelebten Mann tr\u00e4gt nicht mein Hirtenstab,", "tokens": ["Mich", "ab\u00b7ge\u00b7leb\u00b7ten", "Mann", "tr\u00e4gt", "nicht", "mein", "Hir\u00b7ten\u00b7stab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "VVFIN", "PTKNEG", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Geh, hohle mir den Stock, der an dem Zaune steht.", "tokens": ["Geh", ",", "hoh\u00b7le", "mir", "den", "Stock", ",", "der", "an", "dem", "Zau\u00b7ne", "steht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "PPER", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Indem sie noch bem\u00fcht den alten Vater pflegte,", "tokens": ["In\u00b7dem", "sie", "noch", "be\u00b7m\u00fcht", "den", "al\u00b7ten", "Va\u00b7ter", "pfleg\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Wie? rief Endimion, voll Zorn und Eifersucht;", "tokens": ["Wie", "?", "rief", "En\u00b7di\u00b7mi\u00b7on", ",", "voll", "Zorn", "und", "Ei\u00b7fer\u00b7sucht", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "VVFIN", "NN", "$,", "ADJD", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Ist das die Redlichkeit, die mir dein Mund versprach?", "tokens": ["Ist", "das", "die", "Red\u00b7lich\u00b7keit", ",", "die", "mir", "dein", "Mund", "ver\u00b7sprach", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ART", "NN", "$,", "PRELS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Wenn dich ein Sch\u00e4fer lockt, so stehest du schon da;", "tokens": ["Wenn", "dich", "ein", "Sch\u00e4\u00b7fer", "lockt", ",", "so", "ste\u00b7hest", "du", "schon", "da", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Vergist Endimion, der dich nicht kann vergessen.", "tokens": ["Ver\u00b7gist", "En\u00b7di\u00b7mi\u00b7on", ",", "der", "dich", "nicht", "kann", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PRELS", "PPER", "PTKNEG", "VMFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Du hast von meiner Gunst so viel und reiche Gaben:", "tokens": ["Du", "hast", "von", "mei\u00b7ner", "Gunst", "so", "viel", "und", "rei\u00b7che", "Ga\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "ADV", "ADV", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "So bald mein Schaaf gebiehrt, kriegst du das erste Lamm,", "tokens": ["So", "bald", "mein", "Schaaf", "ge\u00b7biehrt", ",", "kriegst", "du", "das", "ers\u00b7te", "Lamm", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "VVPP", "$,", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Kaum da\u00df der Bienenheer Gefach und Korb verl\u00e4\u00dft,", "tokens": ["Kaum", "da\u00df", "der", "Bie\u00b7nen\u00b7heer", "Ge\u00b7fach", "und", "Korb", "ver\u00b7l\u00e4\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Nehm ich den Honig aus. Du must ihn mit geniessen.", "tokens": ["Nehm", "ich", "den", "Ho\u00b7nig", "aus", ".", "Du", "must", "ihn", "mit", "ge\u00b7nies\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$.", "PPER", "VMFIN", "PPER", "APPR", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Mein Gra\u00df, mein Futter w\u00e4chst auch mit vor deine Heerden,", "tokens": ["Mein", "Gra\u00df", ",", "mein", "Fut\u00b7ter", "w\u00e4chst", "auch", "mit", "vor", "dei\u00b7ne", "Heer\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "VVFIN", "ADV", "APPR", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Wenn dich die Sonne sticht, besch\u00fctzet dich mein Laub,", "tokens": ["Wenn", "dich", "die", "Son\u00b7ne", "sticht", ",", "be\u00b7sch\u00fct\u00b7zet", "dich", "mein", "Laub", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "So gar dein Sch\u00e4ferschurz kommt auch von meiner Hand.", "tokens": ["So", "gar", "dein", "Sch\u00e4\u00b7fer\u00b7schurz", "kommt", "auch", "von", "mei\u00b7ner", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Von Myrrthen hab ich dir oft einen Kranz gewunden.", "tokens": ["Von", "Myrrt\u00b7hen", "hab", "ich", "dir", "oft", "ei\u00b7nen", "Kranz", "ge\u00b7wun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "PPER", "ADV", "ART", "NN", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "Ich theile K\u00e4se, Milch, Rohm, Butter, Brodt, und Eyer,", "tokens": ["Ich", "thei\u00b7le", "K\u00e4\u00b7se", ",", "Milch", ",", "Rohm", ",", "But\u00b7ter", ",", "Brodt", ",", "und", "Ey\u00b7er", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "NN", "$,", "NE", "$,", "NN", "$,", "NN", "$,", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "Und schl\u00e4fst du dann und wann bey deinen Heerden ein,", "tokens": ["Und", "schl\u00e4fst", "du", "dann", "und", "wann", "bey", "dei\u00b7nen", "Heer\u00b7den", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "KON", "PWAV", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "So pfl\u00fcck ich Blumen ab, bedecke deinen Leib,", "tokens": ["So", "pfl\u00fcck", "ich", "Blu\u00b7men", "ab", ",", "be\u00b7de\u00b7cke", "dei\u00b7nen", "Leib", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "NN", "PTKVZ", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.55": {"text": "Da\u00df dich kein Unfall trift von Schlangen und Geschmeisse.", "tokens": ["Da\u00df", "dich", "kein", "Un\u00b7fall", "trift", "von", "Schlan\u00b7gen", "und", "Ge\u00b7schmeis\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.56": {"text": "Mein Hector mu\u00df dich auch mit mir zugleich bewachen;", "tokens": ["Mein", "Hec\u00b7tor", "mu\u00df", "dich", "auch", "mit", "mir", "zu\u00b7gleich", "be\u00b7wa\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "ADV", "APPR", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.57": {"text": "Ich webe dir dabey den sch\u00f6nsten Sch\u00e4ferhuth.", "tokens": ["Ich", "we\u00b7be", "dir", "da\u00b7bey", "den", "sch\u00f6ns\u00b7ten", "Sch\u00e4\u00b7fer\u00b7huth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PAV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.58": {"text": "Doch alles ist umsonst, was ich vor dich gethan.", "tokens": ["Doch", "al\u00b7les", "ist", "um\u00b7sonst", ",", "was", "ich", "vor", "dich", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "ADV", "$,", "PWS", "PPER", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.59": {"text": "Sie wust indessen nichts von Vorwurf und von Klagen;", "tokens": ["Sie", "wust", "in\u00b7des\u00b7sen", "nichts", "von", "Vor\u00b7wurf", "und", "von", "Kla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIS", "APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.60": {"text": "Sie nahm den Vater mit, und sprach mit vollem Lachen:", "tokens": ["Sie", "nahm", "den", "Va\u00b7ter", "mit", ",", "und", "sprach", "mit", "vol\u00b7lem", "La\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,", "KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.61": {"text": "Nimm hin, Endimion, dies schmal und schlanke Rohr,", "tokens": ["Nimm", "hin", ",", "En\u00b7di\u00b7mi\u00b7on", ",", "dies", "schmal", "und", "schlan\u00b7ke", "Rohr", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "NN", "$,", "PDS", "ADJD", "KON", "ADJA", "NN", "$,"], "meter": "-----+-+-+-+", "measure": "unknown.measure.tetra"}, "line.62": {"text": "Das ich mit eigner Hand, mein Sch\u00e4fer ausgeschnitzt,", "tokens": ["Das", "ich", "mit", "eig\u00b7ner", "Hand", ",", "mein", "Sch\u00e4\u00b7fer", "aus\u00b7ge\u00b7schnitzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "APPR", "ADJA", "NN", "$,", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.63": {"text": "Mein Vater kam dazu, und brachte mir die Tauben;", "tokens": ["Mein", "Va\u00b7ter", "kam", "da\u00b7zu", ",", "und", "brach\u00b7te", "mir", "die", "Tau\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PAV", "$,", "KON", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.64": {"text": "Die sollen Zeuge seyn, da\u00df ich die Deine bleibe.", "tokens": ["Die", "sol\u00b7len", "Zeu\u00b7ge", "seyn", ",", "da\u00df", "ich", "die", "Dei\u00b7ne", "blei\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VAINF", "$,", "KOUS", "PPER", "ART", "PPOSAT", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.65": {"text": "Da ward Endimion ganz ausser sich gebracht.", "tokens": ["Da", "ward", "En\u00b7di\u00b7mi\u00b7on", "ganz", "aus\u00b7ser", "sich", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "ADV", "APPR", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.66": {"text": "Verfluchte Eifersucht, du Henker meiner Ruh,", "tokens": ["Ver\u00b7fluch\u00b7te", "Ei\u00b7fer\u00b7sucht", ",", "du", "Hen\u00b7ker", "mei\u00b7ner", "Ruh", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.67": {"text": "Die sich in meiner Brust zu Chloris Schmach entsponnen,", "tokens": ["Die", "sich", "in", "mei\u00b7ner", "Brust", "zu", "Chlo\u00b7ris", "Schmach", "ent\u00b7spon\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "PPOSAT", "NN", "APPR", "NE", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.68": {"text": "Gedacht er bey sich selbst; ich mag dich nicht mehr kennen.", "tokens": ["Ge\u00b7dacht", "er", "bey", "sich", "selbst", ";", "ich", "mag", "dich", "nicht", "mehr", "ken\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "PRF", "ADV", "$.", "PPER", "VMFIN", "PRF", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.69": {"text": "Er nahm sie bey der Hand, er nahm sie auf den Schoo\u00df,", "tokens": ["Er", "nahm", "sie", "bey", "der", "Hand", ",", "er", "nahm", "sie", "auf", "den", "Schoo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$,", "PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.70": {"text": "Und k\u00fc\u00dfte ganz entbrannt ihr Stirne, Mund und Brust.", "tokens": ["Und", "k\u00fc\u00df\u00b7te", "ganz", "ent\u00b7brannt", "ihr", "Stir\u00b7ne", ",", "Mund", "und", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "PPOSAT", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.71": {"text": "Kurz; der Zufriedenheit, war weiter nichts zu gleichen.", "tokens": ["Kurz", ";", "der", "Zu\u00b7frie\u00b7den\u00b7heit", ",", "war", "wei\u00b7ter", "nichts", "zu", "glei\u00b7chen", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$.", "ART", "NN", "$,", "VAFIN", "ADV", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.72": {"text": "Ein tr\u00e4nenreicher Bach ergo\u00df sich auf den Wangen,", "tokens": ["Ein", "tr\u00e4\u00b7nen\u00b7rei\u00b7cher", "Bach", "er\u00b7go\u00df", "sich", "auf", "den", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.73": {"text": "Und mischte sich zugleich mit in den k\u00fchlen Thau.", "tokens": ["Und", "mischte", "sich", "zu\u00b7gleich", "mit", "in", "den", "k\u00fch\u00b7len", "Thau", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADV", "APPR", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.74": {"text": "Die holde Sch\u00e4ferinn entri\u00df sich seinem Arm;", "tokens": ["Die", "hol\u00b7de", "Sch\u00e4\u00b7fe\u00b7rinn", "ent\u00b7ri\u00df", "sich", "sei\u00b7nem", "Arm", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.75": {"text": "Und eilte nach dem Vieh,