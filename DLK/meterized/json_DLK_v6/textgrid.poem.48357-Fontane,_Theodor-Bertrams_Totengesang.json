{"textgrid.poem.48357": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Bertrams Totengesang", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie schossen ihn tot um Mitternacht,", "tokens": ["Sie", "schos\u00b7sen", "ihn", "tot", "um", "Mit\u00b7ter\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "APPR", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wo das Steinkreuz ragt empor,", "tokens": ["Wo", "das", "Stein\u00b7kreuz", "ragt", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und sie lie\u00dfen ihn liegen in seinem Blut", "tokens": ["Und", "sie", "lie\u00b7\u00dfen", "ihn", "lie\u00b7gen", "in", "sei\u00b7nem", "Blut"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Auf dem einsamen Heidemoor.", "tokens": ["Auf", "dem", "ein\u00b7sa\u00b7men", "Hei\u00b7de\u00b7moor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sie ritten zu ihres Vaters Haus", "tokens": ["Sie", "rit\u00b7ten", "zu", "ih\u00b7res", "Va\u00b7ters", "Haus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und sprachen: \u00bbEs ist geschehn:", "tokens": ["Und", "spra\u00b7chen", ":", "\u00bb", "Es", "ist", "ge\u00b7schehn", ":"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "$(", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Unsre Schwester, die zu oft ihn sah,", "tokens": ["Uns\u00b7re", "Schwes\u00b7ter", ",", "die", "zu", "oft", "ihn", "sah", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "APPR", "ADV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Soll ihn nicht wieder sehn.\u00ab", "tokens": ["Soll", "ihn", "nicht", "wie\u00b7der", "sehn", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "ADV", "VVINF", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Am andern Morgen aber zur\u00fcck", "tokens": ["Am", "an\u00b7dern", "Mor\u00b7gen", "a\u00b7ber", "zu\u00b7r\u00fcck"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ADV", "PTKVZ"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Ritten sie zu der Stell',", "tokens": ["Rit\u00b7ten", "sie", "zu", "der", "Stell'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Und sie machten von Zweigen die Totenbahr'", "tokens": ["Und", "sie", "mach\u00b7ten", "von", "Zwei\u00b7gen", "die", "To\u00b7ten\u00b7bahr'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "NN", "ART", "NN"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Und trugen ihn in die Kapell'.", "tokens": ["Und", "tru\u00b7gen", "ihn", "in", "die", "Ka\u00b7pell'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+---+-+", "measure": "dactylic.init"}}, "stanza.4": {"line.1": {"text": "Ihre Schwester harrte des Zuges schon,", "tokens": ["Ih\u00b7re", "Schwes\u00b7ter", "harr\u00b7te", "des", "Zu\u00b7ges", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Sie zerri\u00df ihr langes Kleid,", "tokens": ["Sie", "zer\u00b7ri\u00df", "ihr", "lan\u00b7ges", "Kleid", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihre gelben Locken l\u00f6ste sie auf", "tokens": ["Ih\u00b7re", "gel\u00b7ben", "Lo\u00b7cken", "l\u00f6s\u00b7te", "sie", "auf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "APPR"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Und kniete an Bertrams Seit'.", "tokens": ["Und", "knie\u00b7te", "an", "Bert\u00b7rams", "Seit'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NE", "NE", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Sie holte geweihtes Wasser herbei", "tokens": ["Sie", "hol\u00b7te", "ge\u00b7weih\u00b7tes", "Was\u00b7ser", "her\u00b7bei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "PTKVZ"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und wusch ihm die Wunden rein,", "tokens": ["Und", "wusch", "ihm", "die", "Wun\u00b7den", "rein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "ADJD", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Einen Kranz um die Brust, einen Kranz ins Haar \u2013", "tokens": ["Ei\u00b7nen", "Kranz", "um", "die", "Brust", ",", "ei\u00b7nen", "Kranz", "ins", "Haar", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,", "ART", "NN", "APPRART", "NN", "$("], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "\u00bbnun\u00ab, sprach sie, \u00bbmag es sein!\u00ab", "tokens": ["\u00bb", "nun", "\u00ab", ",", "sprach", "sie", ",", "\u00bb", "mag", "es", "sein", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "$(", "$,", "VVFIN", "PPER", "$,", "$(", "VMFIN", "PPER", "VAINF", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Sie h\u00fcllten ihn ein in schneewei\u00df Lein", "tokens": ["Sie", "h\u00fcll\u00b7ten", "ihn", "ein", "in", "schnee\u00b7wei\u00df", "Lein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ART", "APPR", "ADJD", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und trugen ihn dann zur Ruh',", "tokens": ["Und", "tru\u00b7gen", "ihn", "dann", "zur", "Ruh'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die M\u00f6nche sangen die Totenmess'", "tokens": ["Die", "M\u00f6n\u00b7che", "san\u00b7gen", "die", "To\u00b7ten\u00b7mess'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und Litaneien dazu.", "tokens": ["Und", "Li\u00b7ta\u00b7nei\u00b7en", "da\u00b7zu", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.7": {"line.1": {"text": "Sie trugen ihn fort an den alten Ort,", "tokens": ["Sie", "tru\u00b7gen", "ihn", "fort", "an", "den", "al\u00b7ten", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Nacht war still und bang;", "tokens": ["Die", "Nacht", "war", "still", "und", "bang", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es fiel der Tau, der Nebel zog", "tokens": ["Es", "fiel", "der", "Tau", ",", "der", "Ne\u00b7bel", "zog"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Heidemoor entlang.", "tokens": ["Das", "Hei\u00b7de\u00b7moor", "ent\u00b7lang", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Sie gruben sein Grab zwei Fu\u00df tief nur,", "tokens": ["Sie", "gru\u00b7ben", "sein", "Grab", "zwei", "Fu\u00df", "tief", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "CARD", "NN", "ADJD", "ADV", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wo das Kreuz gen Osten schaut,", "tokens": ["Wo", "das", "Kreuz", "gen", "Os\u00b7ten", "schaut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und sie deckten ihn zu mit Ginstergestr\u00fcpp", "tokens": ["Und", "sie", "deck\u00b7ten", "ihn", "zu", "mit", "Gins\u00b7ter\u00b7ge\u00b7str\u00fcpp"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "APPR", "APPR", "NN"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Und mit Moos und mit Farrenkraut.", "tokens": ["Und", "mit", "Moos", "und", "mit", "Far\u00b7ren\u00b7kraut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.9": {"line.1": {"text": "Der M\u00f6nche einer stand am Grab", "tokens": ["Der", "M\u00f6n\u00b7che", "ei\u00b7ner", "stand", "am", "Grab"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und betete, bis es getagt;", "tokens": ["Und", "be\u00b7te\u00b7te", ",", "bis", "es", "ge\u00b7tagt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Und in der Kapelle singen sie,", "tokens": ["Und", "in", "der", "Ka\u00b7pel\u00b7le", "sin\u00b7gen", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Solange das Steinkreuz ragt.", "tokens": ["So\u00b7lan\u00b7ge", "das", "Stein\u00b7kreuz", "ragt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}