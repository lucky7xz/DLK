{"textgrid.poem.61938": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "9.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Spanisches bringt mir die Post? Was seh' ich! Die eigenen Lieder", "tokens": ["Spa\u00b7ni\u00b7sches", "bringt", "mir", "die", "Post", "?", "Was", "seh'", "ich", "!", "Die", "ei\u00b7ge\u00b7nen", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "ART", "NN", "$.", "PWS", "VVFIN", "PPER", "$.", "ART", "ADJA", "NN"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Sind's; im kastilischen Vers staunend erkenn' ich mich selbst.", "tokens": ["Sin\u00b7d's", ";", "im", "kas\u00b7ti\u00b7li\u00b7schen", "Vers", "stau\u00b7nend", "er\u00b7kenn'", "ich", "mich", "selbst", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "APPRART", "ADJA", "NN", "ADJD", "VVFIN", "PPER", "PRF", "ADV", "$."], "meter": "+--+-+-++--+--+", "measure": "iambic.septa.invert"}, "line.3": {"text": "Was ich als J\u00fcngling sang, wie vertraulich zugleich und wie fremd doch", "tokens": ["Was", "ich", "als", "J\u00fcng\u00b7ling", "sang", ",", "wie", "ver\u00b7trau\u00b7lich", "zu\u00b7gleich", "und", "wie", "fremd", "doch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "KOUS", "NN", "VVFIN", "$,", "PWAV", "ADJD", "ADV", "KON", "PWAV", "ADJD", "ADV"], "meter": "-+-+-+--+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Gr\u00fc\u00dft es mich hier und erscheint frischer und zierlicher fast,", "tokens": ["Gr\u00fc\u00dft", "es", "mich", "hier", "und", "er\u00b7scheint", "fri\u00b7scher", "und", "zier\u00b7li\u00b7cher", "fast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "KON", "VVFIN", "ADJD", "KON", "ADV", "ADV", "$,"], "meter": "+--++-++--+--+", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "Wie mein T\u00f6chterchen j\u00fcngst, zum Faschingsballe ger\u00fcstet", "tokens": ["Wie", "mein", "T\u00f6ch\u00b7ter\u00b7chen", "j\u00fcngst", ",", "zum", "Fa\u00b7schings\u00b7bal\u00b7le", "ge\u00b7r\u00fcs\u00b7tet"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "ADV", "$,", "APPRART", "NN", "VVPP"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "In des Zigeunergewands Flittern mir doppelt gefiel.", "tokens": ["In", "des", "Zi\u00b7geu\u00b7ner\u00b7ge\u00b7wands", "Flit\u00b7tern", "mir", "dop\u00b7pelt", "ge\u00b7fiel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Harmlos warf ich euch hin, ihr Ges\u00e4nge der Jugend, und immer", "tokens": ["Harm\u00b7los", "warf", "ich", "euch", "hin", ",", "ihr", "Ge\u00b7s\u00e4n\u00b7ge", "der", "Ju\u00b7gend", ",", "und", "im\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "PPER", "PTKVZ", "$,", "PPOSAT", "NN", "ART", "NN", "$,", "KON", "ADV"], "meter": "+-+--++-+--+--+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Blieb mir ein R\u00e4tsel die Gunst, die man so reich euch gew\u00e4hrt;", "tokens": ["Blieb", "mir", "ein", "R\u00e4t\u00b7sel", "die", "Gunst", ",", "die", "man", "so", "reich", "euch", "ge\u00b7w\u00e4hrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "NN", "$,", "PRELS", "PIS", "ADV", "ADJD", "PPER", "VVPP", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.3": {"text": "Denn leichtwiegend erscheint ihr zumeist dem gereifteren Urteil;", "tokens": ["Denn", "leicht\u00b7wie\u00b7gend", "er\u00b7scheint", "ihr", "zu\u00b7meist", "dem", "ge\u00b7reif\u00b7te\u00b7ren", "Ur\u00b7teil", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Nur im melodischen Hauch schwebt ihr gef\u00e4llig dahin.", "tokens": ["Nur", "im", "me\u00b7lo\u00b7di\u00b7schen", "Hauch", "schwebt", "ihr", "ge\u00b7f\u00e4l\u00b7lig", "da\u00b7hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "VVFIN", "PPER", "ADJD", "PAV", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Aber ich darf mich r\u00fchmen, da\u00df nie der Erfolg mich verblendet,", "tokens": ["A\u00b7ber", "ich", "darf", "mich", "r\u00fch\u00b7men", ",", "da\u00df", "nie", "der", "Er\u00b7folg", "mich", "ver\u00b7blen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPER", "VVINF", "$,", "KOUS", "ADV", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.6": {"text": "Da\u00df ich des Kranzes ", "tokens": ["Da\u00df", "ich", "des", "Kran\u00b7zes"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "In die Tiefen der Brust und des Weltlaufs sucht' ich zu dringen,", "tokens": ["In", "die", "Tie\u00b7fen", "der", "Brust", "und", "des", "Welt\u00b7laufs", "sucht'", "ich", "zu", "drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "KON", "ART", "NN", "VVFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.8": {"text": "Und mit heiligem Ernst rang ich zum Gipfel der Kunst.", "tokens": ["Und", "mit", "hei\u00b7li\u00b7gem", "Ernst", "rang", "ich", "zum", "Gip\u00b7fel", "der", "Kunst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "$."], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.9": {"text": "Viel zwar blieb mir versagt, doch reift auch manches im stillen,", "tokens": ["Viel", "zwar", "blieb", "mir", "ver\u00b7sagt", ",", "doch", "reift", "auch", "man\u00b7ches", "im", "stil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "VVPP", "$,", "ADV", "VVFIN", "ADV", "PIS", "APPRART", "ADJA", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Dran sich ein deutsches Gem\u00fct wohl zu erfreuen vermag,", "tokens": ["Dran", "sich", "ein", "deut\u00b7sches", "Ge\u00b7m\u00fct", "wohl", "zu", "er\u00b7freu\u00b7en", "ver\u00b7mag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PRF", "ART", "ADJA", "NN", "ADV", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.11": {"text": "Wenn ich die R\u00e4tsel der Zeit und des Herzens im Liede zu deuten", "tokens": ["Wenn", "ich", "die", "R\u00e4t\u00b7sel", "der", "Zeit", "und", "des", "Her\u00b7zens", "im", "Lie\u00b7de", "zu", "deu\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "KON", "ART", "NN", "APPRART", "NN", "PTKZU", "VVINF"], "meter": "-+-+--+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Oder im ernsten Kothurn festlich zu schreiten gewagt.", "tokens": ["O\u00b7der", "im", "erns\u00b7ten", "Ko\u00b7thurn", "fest\u00b7lich", "zu", "schrei\u00b7ten", "ge\u00b7wagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "ADJA", "NN", "ADJD", "PTKZU", "VVINF", "VVPP", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.13": {"text": "Und so bitt' ich: Verzeiht, was wild und jugendlich aufscho\u00df,", "tokens": ["Und", "so", "bitt'", "ich", ":", "Ver\u00b7zeiht", ",", "was", "wild", "und", "ju\u00b7gend\u00b7lich", "auf\u00b7scho\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$.", "VVFIN", "$,", "PRELS", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.14": {"text": "Und im wuchernden Laub la\u00dft euch gefallen die Frucht!", "tokens": ["Und", "im", "wu\u00b7chern\u00b7den", "Laub", "la\u00dft", "euch", "ge\u00b7fal\u00b7len", "die", "Frucht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "ADJA", "NN", "VVFIN", "PPER", "VVPP", "ART", "NN", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Durchs Helldunkel der Nacht hinschreit' ich am Hafen; die feine", "tokens": ["Durchs", "Hell\u00b7dun\u00b7kel", "der", "Nacht", "hin\u00b7schreit'", "ich", "am", "Ha\u00b7fen", ";", "die", "fei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "PPER", "APPRART", "NN", "$.", "ART", "ADJA"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Sichel des Halbmonds schwebt \u00fcber den Giebeln der Burg.", "tokens": ["Si\u00b7chel", "des", "Halb\u00b7monds", "schwebt", "\u00fc\u00b7ber", "den", "Gie\u00b7beln", "der", "Burg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.3": {"text": "Rings in der Stadt kein Laut! Nur fern in den L\u00fcften ein Brausen", "tokens": ["Rings", "in", "der", "Stadt", "kein", "Laut", "!", "Nur", "fern", "in", "den", "L\u00fcf\u00b7ten", "ein", "Brau\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "PIAT", "NN", "$.", "ADV", "ADJD", "APPR", "ART", "NN", "ART", "NN"], "meter": "+--+-+-+--+--+-", "measure": "hexameter"}, "line.4": {"text": "H\u00f6r' ich, und unter dem Eis schluchzen die Wasser des Stroms,", "tokens": ["H\u00f6r'", "ich", ",", "und", "un\u00b7ter", "dem", "Eis", "schluch\u00b7zen", "die", "Was\u00b7ser", "des", "Stroms", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KON", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Und im gelinderen Hauch, der pl\u00f6tzlich Wangen und Stirn mir", "tokens": ["Und", "im", "ge\u00b7lin\u00b7de\u00b7ren", "Hauch", ",", "der", "pl\u00f6tz\u00b7lich", "Wan\u00b7gen", "und", "Stirn", "mir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN", "$,", "PRELS", "ADJD", "NN", "KON", "NN", "PPER"], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Anr\u00fchrt, flattert ein Gru\u00df, nahender Fr\u00fchling, von dir.", "tokens": ["An\u00b7r\u00fchrt", ",", "flat\u00b7tert", "ein", "Gru\u00df", ",", "na\u00b7hen\u00b7der", "Fr\u00fch\u00b7ling", ",", "von", "dir", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVFIN", "ART", "NN", "$,", "ADJA", "NN", "$,", "APPR", "PPER", "$."], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Aus dem erwachenden Forst heimkehrend bringt mir ein holdes", "tokens": ["Aus", "dem", "er\u00b7wa\u00b7chen\u00b7den", "Forst", "heim\u00b7keh\u00b7rend", "bringt", "mir", "ein", "hol\u00b7des"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJD", "VVFIN", "PPER", "ART", "ADJA"], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Kind Schneegl\u00f6ckchen zum Fest, frisch an der Halde gepfl\u00fcckt.", "tokens": ["Kind", "Schnee\u00b7gl\u00f6ck\u00b7chen", "zum", "Fest", ",", "frisch", "an", "der", "Hal\u00b7de", "ge\u00b7pfl\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPRART", "NN", "$,", "ADJD", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+---+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "O willkommen im Strau\u00df, ihr Erstlingskinder der Sonne!", "tokens": ["O", "will\u00b7kom\u00b7men", "im", "Strau\u00df", ",", "ihr", "Erst\u00b7lings\u00b7kin\u00b7der", "der", "Son\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "APPRART", "NE", "$,", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Euer gew\u00fcrziger Hauch duftet wie Jugend mich an,", "tokens": ["Eu\u00b7er", "ge\u00b7w\u00fcr\u00b7zi\u00b7ger", "Hauch", "duf\u00b7tet", "wie", "Ju\u00b7gend", "mich", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "KOKOM", "NN", "PPER", "PTKVZ", "$,"], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Und, den gemessenen Ernst abstreifend der Wintergedanken,", "tokens": ["Und", ",", "den", "ge\u00b7mes\u00b7se\u00b7nen", "Ernst", "ab\u00b7strei\u00b7fend", "der", "Win\u00b7ter\u00b7ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ART", "ADJA", "NN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+--+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Sehnt sich nach freierem Spiel, vollerem Klange das Herz.", "tokens": ["Sehnt", "sich", "nach", "frei\u00b7e\u00b7rem", "Spiel", ",", "vol\u00b7le\u00b7rem", "Klan\u00b7ge", "das", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ADJA", "NN", "$,", "ADJA", "NN", "ART", "NN", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.7": {"text": "Liegt, ihr Gl\u00f6ckchen, denn hier bei dem letzten der Distichen! Morgen", "tokens": ["Liegt", ",", "ihr", "Gl\u00f6ck\u00b7chen", ",", "denn", "hier", "bei", "dem", "letz\u00b7ten", "der", "Dis\u00b7ti\u00b7chen", "!", "Mor\u00b7gen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "$,", "KON", "ADV", "APPR", "ART", "ADJA", "ART", "NN", "$.", "NN"], "meter": "+-+-+-+-+--+--+-", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "Spann' ich zu Lenzmelodien andere Saiten mir auf.", "tokens": ["Spann'", "ich", "zu", "Lenz\u00b7me\u00b7lo\u00b7di\u00b7en", "an\u00b7de\u00b7re", "Sai\u00b7ten", "mir", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "NE", "ADJA", "NN", "PPER", "PTKVZ", "$."], "meter": "+--+--+-+--+--+", "measure": "dactylic.di.plus"}}}}}