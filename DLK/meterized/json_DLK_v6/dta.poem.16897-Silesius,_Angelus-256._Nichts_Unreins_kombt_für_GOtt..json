{"dta.poem.16897": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "256.  Nichts Unreins kombt f\u00fcr GOtt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Ach Mensch werd\u2019 \u00fcberformt: f\u00fcrwahr du must so fein", "tokens": ["Ach", "Mensch", "werd'", "\u00fc\u00b7ber\u00b7formt", ":", "f\u00fcr\u00b7wahr", "du", "must", "so", "fein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "NN", "VAFIN", "VVPP", "$.", "VVFIN", "PPER", "VMFIN", "ADV", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "F\u00fcr GOttes Angesicht/ als Christi Seele seyn.", "tokens": ["F\u00fcr", "Got\u00b7tes", "An\u00b7ge\u00b7sicht", "/", "als", "Chris\u00b7ti", "See\u00b7le", "seyn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$(", "KOUS", "NE", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}