{"textgrid.poem.63417": {"metadata": {"author": {"name": "Brinckman, John", "birth": "N.A.", "death": "N.A."}, "title": "In'n Dik", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schoh af un Str\u00fcmp af, de B\u00fccks \u0153wer't Knee,", "tokens": ["Schoh", "af", "un", "Str\u00fcmp", "af", ",", "de", "B\u00fccks", "\u0153wer't", "Knee", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,", "NE", "NE", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "platschen de Jungs dor in'n Dik;", "tokens": ["plat\u00b7schen", "de", "Jungs", "dor", "in'n", "Dik", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Gos un Gant un all dat leew Veeh,", "tokens": ["Gos", "un", "Gant", "un", "all", "dat", "leew", "Veeh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "W\u00e4tick un Aanten toglik.", "tokens": ["W\u00e4\u00b7tick", "un", "Aan\u00b7ten", "tog\u00b7lik", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "V\u00f6rn inne Mad dor w\u00f6ltern de Swin,", "tokens": ["V\u00f6rn", "in\u00b7ne", "Mad", "dor", "w\u00f6l\u00b7tern", "de", "Swin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Borg, S\u0153g, Fasel un P\u00f6lk;", "tokens": ["Borg", ",", "S\u0153g", ",", "Fa\u00b7sel", "un", "P\u00f6lk", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "NN", "FM", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "un de l\u00fctt Farken de quieken un schri'n,", "tokens": ["un", "de", "l\u00fctt", "Far\u00b7ken", "de", "qui\u00b7e\u00b7ken", "un", "schri'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "un de twee Kalwer de b\u00f6lk'.", "tokens": ["un", "de", "twee", "Kal\u00b7wer", "de", "b\u00f6l\u00b7k'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "FM", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.3": {"line.1": {"text": "Wasser de jault, den Swanz manke Been,", "tokens": ["Was\u00b7ser", "de", "jault", ",", "den", "Swanz", "man\u00b7ke", "Be\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "$,", "ART", "NN", "VVFIN", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "r\u00fcckt in de Str\u00fcmp un de Schoh;", "tokens": ["r\u00fcckt", "in", "de", "Str\u00fcmp", "un", "de", "Schoh", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NE", "NE", "FM", "FM", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "man de l\u00fctt Dirn de sitt up'n Steen,", "tokens": ["man", "de", "l\u00fctt", "Dirn", "de", "sitt", "up'n", "Steen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "FM.da", "FM.da", "FM.da", "FM.da", "FM.da", "FM.da", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "klappt inne H\u00e4nn' un kickt to.", "tokens": ["klappt", "in\u00b7ne", "H\u00e4nn'", "un", "kickt", "to", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+--++-+", "measure": "dactylic.init"}}, "stanza.4": {"line.1": {"text": "Moder de steiht \u00fcnner't Dur dor un schellt:", "tokens": ["Mo\u00b7der", "de", "steiht", "\u00fcn\u00b7ner't", "Dur", "dor", "un", "schellt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "FM", "FM", "VVFIN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "\u00bbg\u00f6ren, ick kam mit'n Schacht!\u00ab", "tokens": ["\u00bb", "g\u00f6\u00b7ren", ",", "ick", "kam", "mit'n", "Schacht", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVINF", "$,", "PPER", "VVFIN", "APPR", "NN", "$.", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Vader de k\u00fcmmt grad to Middag ut't Feld,", "tokens": ["Va\u00b7der", "de", "k\u00fcmmt", "grad", "to", "Mid\u00b7dag", "ut't", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "ADV", "FM", "FM", "FM", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Vader de s\u00fcht dat un lacht.", "tokens": ["Va\u00b7der", "de", "s\u00fcht", "dat", "un", "lacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "ART", "FM", "VVFIN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.5": {"line.1": {"text": "\u00bblat ehr man, Moder! De G\u00f6rn heww' D\u00e4g", "tokens": ["\u00bb", "lat", "ehr", "man", ",", "Mo\u00b7der", "!", "De", "G\u00f6rn", "hew\u00b7w'", "D\u00e4g"], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "VVFIN", "KOUS", "PIS", "$,", "NN", "$.", "NE", "NE", "NE", "NE"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "man inne P\u00fctt un de Mad,", "tokens": ["man", "in\u00b7ne", "P\u00fctt", "un", "de", "Mad", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "FM", "FM", "FM", "FM", "FM", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "grad as de Farken, de P\u00f6lk un de S\u0153g", "tokens": ["grad", "as", "de", "Far\u00b7ken", ",", "de", "P\u00f6lk", "un", "de", "S\u0153g"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "FM", "FM", "NN", "$,", "FM", "FM", "FM", "FM", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "un as de Faselswin grad.\u00ab", "tokens": ["un", "as", "de", "Fa\u00b7sels\u00b7win", "grad", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "$.", "$("], "meter": "++-+-+-", "measure": "iambic.tri"}}}}}