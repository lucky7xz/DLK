{"textgrid.poem.32483": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "[auf eine Dissertation des", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Neid, dies Werk wirst du verschonen m\u00fcssen!", "tokens": ["O", "Neid", ",", "dies", "Werk", "wirst", "du", "ver\u00b7scho\u00b7nen", "m\u00fcs\u00b7sen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PDS", "NN", "VAFIN", "PPER", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit Tantum abest f\u00e4ngt es an.", "tokens": ["Mit", "Tan\u00b7tum", "a\u00b7best", "f\u00e4ngt", "es", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nur eines fehlet noch daran!", "tokens": ["Nur", "ei\u00b7nes", "feh\u00b7let", "noch", "da\u00b7ran", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "ADV", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit parum adest sollt es schlie\u00dfen.", "tokens": ["Mit", "pa\u00b7rum", "a\u00b7dest", "sollt", "es", "schlie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "FM", "FM", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}