{"textgrid.poem.46830": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[still, nur still! die b\u00f6sen Stunden gehen auch]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Still, nur still! die b\u00f6sen Stunden gehen auch;", "tokens": ["Still", ",", "nur", "still", "!", "die", "b\u00f6\u00b7sen", "Stun\u00b7den", "ge\u00b7hen", "auch", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADV", "PTKVZ", "$.", "ART", "ADJA", "NN", "VVFIN", "ADV", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Lange stands du, kannst noch l\u00e4nger stehen auch.", "tokens": ["Lan\u00b7ge", "stands", "du", ",", "kannst", "noch", "l\u00e4n\u00b7ger", "ste\u00b7hen", "auch", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "VMFIN", "ADV", "ADJD", "VVFIN", "ADV", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Und wenn dichs zu Boden dr\u00fcckt, erliegest du;", "tokens": ["Und", "wenn", "dichs", "zu", "Bo\u00b7den", "dr\u00fcckt", ",", "er\u00b7lie\u00b7gest", "du", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "APPR", "NN", "VVFIN", "$,", "VVFIN", "PPER", "$."], "meter": "--+-+-+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "So das Unertr\u00e4gliche besiegest du.", "tokens": ["So", "das", "Un\u00b7er\u00b7tr\u00e4g\u00b7li\u00b7che", "be\u00b7sie\u00b7gest", "du", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}}}}