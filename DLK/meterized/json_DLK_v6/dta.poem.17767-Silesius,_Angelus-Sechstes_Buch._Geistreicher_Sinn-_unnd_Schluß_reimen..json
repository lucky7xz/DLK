{"dta.poem.17767": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "Sechstes Buch.  \n Geistreicher Sinn- unnd  \n Schlu\u00df reimen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "In GOtt vollkommen seyn/ das heist er Alt zumahl.", "tokens": ["In", "Gott", "voll\u00b7kom\u00b7men", "seyn", "/", "das", "heist", "er", "Alt", "zu\u00b7mahl", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VAINF", "$(", "PDS", "VVFIN", "PPER", "ADJD", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Sonne leuchtet jhm in seinen Aker ein/", "tokens": ["Die", "Son\u00b7ne", "leuch\u00b7tet", "jhm", "in", "sei\u00b7nen", "A\u00b7ker", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und wenns gleich abend wird/ so bleibt ihm doch ihr", "tokens": ["Und", "wenns", "gleich", "a\u00b7bend", "wird", "/", "so", "bleibt", "ihm", "doch", "ihr"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ADV", "VVPP", "VAFIN", "$(", "ADV", "VVFIN", "PPER", "ADV", "PPOSAT"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Schein.", "tokens": ["Schein", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Er siht des Lebens Baum im Geist begierlich an/", "tokens": ["Er", "siht", "des", "Le\u00b7bens", "Baum", "im", "Geist", "be\u00b7gier\u00b7lich", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NE", "APPRART", "NN", "ADJD", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und geht mit allem flei\u00df zu ihm die n\u00e4chste bahn.", "tokens": ["Und", "geht", "mit", "al\u00b7lem", "flei\u00df", "zu", "ihm", "die", "n\u00e4chs\u00b7te", "bahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PIS", "NN", "APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Er k\u00fcmmert sich umb nichts; was neben ihm geschieht/", "tokens": ["Er", "k\u00fcm\u00b7mert", "sich", "umb", "nichts", ";", "was", "ne\u00b7ben", "ihm", "ge\u00b7schieht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PIS", "$.", "PWS", "APPR", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ist ihm so frembt und klar/ als was ein blinder sieht/", "tokens": ["Ist", "ihm", "so", "frembt", "und", "klar", "/", "als", "was", "ein", "blin\u00b7der", "sieht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "KON", "ADJD", "$(", "KOUS", "PIS", "ART", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Doch ist er stark und frisch/ er scheuet keinen Feind/", "tokens": ["Doch", "ist", "er", "stark", "und", "frisch", "/", "er", "scheu\u00b7et", "kei\u00b7nen", "Feind", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "$(", "PPER", "VVFIN", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wenn gleich Welt/ Teuffel/ Fleisch/ und mehr beysam-", "tokens": ["Wenn", "gleich", "Welt", "/", "Teuf\u00b7fel", "/", "Fleisch", "/", "und", "mehr", "bey\u00b7sam"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "ADV", "NN", "$(", "NE", "$(", "NN", "$(", "KON", "ADV", "TRUNC"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "men seind.", "tokens": ["men", "seind", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VAFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.12": {"text": "Ein ander lauffe hin/ zerstrew sich mit der Welt/", "tokens": ["Ein", "an\u00b7der", "lauf\u00b7fe", "hin", "/", "zer\u00b7strew", "sich", "mit", "der", "Welt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "PTKVZ", "$(", "VVFIN", "PRF", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Di\u00df ist das Leben und die bahn/ so mir gef\u00e4llt.", "tokens": ["Di\u00df", "ist", "das", "Le\u00b7ben", "und", "die", "bahn", "/", "so", "mir", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "KON", "ART", "NN", "$(", "ADV", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}