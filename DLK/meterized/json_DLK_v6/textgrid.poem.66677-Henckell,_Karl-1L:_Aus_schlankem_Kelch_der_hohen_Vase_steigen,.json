{"textgrid.poem.66677": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Aus schlankem Kelch der hohen Vase steigen,", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus schlankem Kelch der hohen Vase steigen,", "tokens": ["Aus", "schlan\u00b7kem", "Kelch", "der", "ho\u00b7hen", "Va\u00b7se", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von gelber Rosen zartem Samt umschmiegt,", "tokens": ["Von", "gel\u00b7ber", "Ro\u00b7sen", "zar\u00b7tem", "Samt", "um\u00b7schmiegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Vier wei\u00dfe Lilien, die sich himmlisch neigen,", "tokens": ["Vier", "wei\u00b7\u00dfe", "Li\u00b7li\u00b7en", ",", "die", "sich", "himm\u00b7lisch", "nei\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "$,", "PRELS", "PRF", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Als sei f\u00fcr ewig alle Schuld besiegt.", "tokens": ["Als", "sei", "f\u00fcr", "e\u00b7wig", "al\u00b7le", "Schuld", "be\u00b7siegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "APPR", "ADJD", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Mir wird zu Sinn: ich h\u00f6re leise Geigen,", "tokens": ["Mir", "wird", "zu", "Sinn", ":", "ich", "h\u00f6\u00b7re", "lei\u00b7se", "Gei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "$.", "PPER", "VVFIN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ein Engelstraum durch meine Seele fliegt:", "tokens": ["Ein", "En\u00b7gel\u00b7straum", "durch", "mei\u00b7ne", "See\u00b7le", "fliegt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Vier wei\u00dfe Frauen, schwebende Gestalten,", "tokens": ["Vier", "wei\u00b7\u00dfe", "Frau\u00b7en", ",", "schwe\u00b7ben\u00b7de", "Ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.8": {"text": "Die in den schmalen H\u00e4nden Lilien halten.", "tokens": ["Die", "in", "den", "schma\u00b7len", "H\u00e4n\u00b7den", "Li\u00b7li\u00b7en", "hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Sie gr\u00fc\u00dfen mich, sie sind mir greifbar nahe:", "tokens": ["Sie", "gr\u00fc\u00b7\u00dfen", "mich", ",", "sie", "sind", "mir", "greif\u00b7bar", "na\u00b7he", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPER", "VAFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Erste singt: \u00bbSei Sch\u00f6nheit dein Geschick!\u00ab", "tokens": ["Die", "Ers\u00b7te", "singt", ":", "\u00bb", "Sei", "Sch\u00f6n\u00b7heit", "dein", "Ge\u00b7schick", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "VVFIN", "$.", "$(", "VAFIN", "NN", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Zweite: \u00bbStaubesblanken Glanz empfahe!\u00ab", "tokens": ["Die", "Zwei\u00b7te", ":", "\u00bb", "Stau\u00b7bes\u00b7blan\u00b7ken", "Glanz", "em\u00b7pfa\u00b7he", "!", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "$.", "$(", "NN", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Dritte: \u00bbMild erschlossen sei dein Blick!\u00ab", "tokens": ["Die", "Drit\u00b7te", ":", "\u00bb", "Mild", "er\u00b7schlos\u00b7sen", "sei", "dein", "Blick", "!", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "$.", "$(", "NN", "VVPP", "VAFIN", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die Vierte: \u00bbStolze Neigung ich bejahe.", "tokens": ["Die", "Vier\u00b7te", ":", "\u00bb", "Stol\u00b7ze", "Nei\u00b7gung", "ich", "be\u00b7ja\u00b7he", "."], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "$(", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sieg im Besiegtsein!\u00ab Silbern t\u00f6nt Musik.", "tokens": ["Sieg", "im", "Be\u00b7siegt\u00b7sein", "!", "\u00ab", "Sil\u00b7bern", "t\u00f6nt", "Mu\u00b7sik", "."], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$.", "$(", "NN", "VVFIN", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.7": {"text": "Und alle drauf in seligem Quartette:", "tokens": ["Und", "al\u00b7le", "drauf", "in", "se\u00b7li\u00b7gem", "Quar\u00b7tet\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PAV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "\u00bbsei frei und rein! Leid l\u00f6ste deine Kette.\u00ab", "tokens": ["\u00bb", "sei", "frei", "und", "rein", "!", "Leid", "l\u00f6s\u00b7te", "dei\u00b7ne", "Ket\u00b7te", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VAFIN", "ADJD", "KON", "ADJD", "$.", "NN", "VVFIN", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ein Zittern. Der Gestalten Linien schwinden,", "tokens": ["Ein", "Zit\u00b7tern", ".", "Der", "Ge\u00b7stal\u00b7ten", "Li\u00b7ni\u00b7en", "schwin\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Vier Kelche hauchen feinen Duft mir zu.", "tokens": ["Vier", "Kel\u00b7che", "hau\u00b7chen", "fei\u00b7nen", "Duft", "mir", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "ADJA", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "O k\u00f6nnt' ich ihrem Sinn Erf\u00fcllung finden", "tokens": ["O", "k\u00f6nnt'", "ich", "ih\u00b7rem", "Sinn", "Er\u00b7f\u00fcl\u00b7lung", "fin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VMFIN", "PPER", "PPOSAT", "NN", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und bl\u00fchn in leuchtend hoheitsvoller Ruh!", "tokens": ["Und", "bl\u00fchn", "in", "leuch\u00b7tend", "ho\u00b7heits\u00b7vol\u00b7ler", "Ruh", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Sie stehn so grad, kein G\u00e4rtner mag sie binden,", "tokens": ["Sie", "stehn", "so", "grad", ",", "kein", "G\u00e4rt\u00b7ner", "mag", "sie", "bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$,", "PIAT", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Vorbild f\u00fcr uns, Geliebte, Schwester du,", "tokens": ["Vor\u00b7bild", "f\u00fcr", "uns", ",", "Ge\u00b7lieb\u00b7te", ",", "Schwes\u00b7ter", "du", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "$,", "NN", "$,", "NN", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Bis wir im Garten dieser Welt erwarben", "tokens": ["Bis", "wir", "im", "Gar\u00b7ten", "die\u00b7ser", "Welt", "er\u00b7war\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN", "PDAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Staubf\u00e4den auch so goldorangefarben ...", "tokens": ["Staub\u00b7f\u00e4\u00b7den", "auch", "so", "gol\u00b7do\u00b7ran\u00b7ge\u00b7far\u00b7ben", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "ADJA", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}