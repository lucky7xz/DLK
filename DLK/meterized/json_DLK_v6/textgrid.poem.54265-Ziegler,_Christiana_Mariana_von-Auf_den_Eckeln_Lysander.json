{"textgrid.poem.54265": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "Auf den Eckeln Lysander", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lysander d\u00fcnckt sich klug, wenn er sich eckel nennt,", "tokens": ["Ly\u00b7san\u00b7der", "d\u00fcnckt", "sich", "klug", ",", "wenn", "er", "sich", "ec\u00b7kel", "nennt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "ADJD", "$,", "KOUS", "PPER", "PRF", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "F\u00fcrwahr das Weiber-Volck wird sich nicht um ihn reissen,", "tokens": ["F\u00fcr\u00b7wahr", "das", "Wei\u00b7ber\u00b7Volck", "wird", "sich", "nicht", "um", "ihn", "reis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "PRF", "PTKNEG", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Denn wer den Vogel schon an seinen Federn kennt,", "tokens": ["Denn", "wer", "den", "Vo\u00b7gel", "schon", "an", "sei\u00b7nen", "Fe\u00b7dern", "kennt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der sucht ihm nicht so leicht die Schwingen auszureissen.", "tokens": ["Der", "sucht", "ihm", "nicht", "so", "leicht", "die", "Schwin\u00b7gen", "aus\u00b7zu\u00b7reis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PTKNEG", "ADV", "ADJD", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}