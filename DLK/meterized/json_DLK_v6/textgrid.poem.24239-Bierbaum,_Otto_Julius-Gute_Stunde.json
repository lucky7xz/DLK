{"textgrid.poem.24239": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Gute Stunde", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gebt mir ein Blatt Papier.", "tokens": ["Gebt", "mir", "ein", "Blatt", "Pa\u00b7pier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die Feder l\u00e4uft geschwind.", "tokens": ["Die", "Fe\u00b7der", "l\u00e4uft", "ge\u00b7schwind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich wei\u00df, da\u00df jetzt um mich", "tokens": ["Ich", "wei\u00df", ",", "da\u00df", "jetzt", "um", "mich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ADV", "APPR", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Huldreiche Geister sind.", "tokens": ["Huld\u00b7rei\u00b7che", "Geis\u00b7ter", "sind", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "$."], "meter": "++-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ihr Wort geht in mich ein,", "tokens": ["Ihr", "Wort", "geht", "in", "mich", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ihr Wort geht aus mir aus,", "tokens": ["Ihr", "Wort", "geht", "aus", "mir", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Drum schimmert mein Gedicht", "tokens": ["Drum", "schim\u00b7mert", "mein", "Ge\u00b7dicht"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Gleich einem Blumenstrau\u00df.", "tokens": ["Gleich", "ei\u00b7nem", "Blu\u00b7men\u00b7strau\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}