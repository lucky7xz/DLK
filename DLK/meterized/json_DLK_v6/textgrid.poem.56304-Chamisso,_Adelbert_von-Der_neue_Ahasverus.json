{"textgrid.poem.56304": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "Der neue Ahasverus", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hegst im Herzen du die Stunden", "tokens": ["Hegst", "im", "Her\u00b7zen", "du", "die", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unsrer Kindheit noch, die Tr\u00e4ume,", "tokens": ["Uns\u00b7rer", "Kind\u00b7heit", "noch", ",", "die", "Tr\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "All mein Lieben, all mein Hoffen?", "tokens": ["All", "mein", "Lie\u00b7ben", ",", "all", "mein", "Hof\u00b7fen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "ADJA", "$,", "PIAT", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Siehst du wandeln uns verbunden", "tokens": ["Siehst", "du", "wan\u00b7deln", "uns", "ver\u00b7bun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "VVFIN", "PPER", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Durch des Paradieses R\u00e4ume,", "tokens": ["Durch", "des", "Pa\u00b7ra\u00b7die\u00b7ses", "R\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und die Zukunft vor uns offen,", "tokens": ["Und", "die", "Zu\u00b7kunft", "vor", "uns", "of\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Sternbegl\u00e4nzt und ungemessen,", "tokens": ["Stern\u00b7be\u00b7gl\u00e4nzt", "und", "un\u00b7ge\u00b7mes\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wie des \u00c4thers reines Blau?", "tokens": ["Wie", "des", "\u00c4\u00b7thers", "rei\u00b7nes", "Blau", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Nein, Sie haben das vergessen,", "tokens": ["Nein", ",", "Sie", "ha\u00b7ben", "das", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "PDS", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Gn\u00e4d'ge Frau.", "tokens": ["Gn\u00e4d'\u00b7ge", "Frau", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Ja vergessen! und es sollen", "tokens": ["Ja", "ver\u00b7ges\u00b7sen", "!", "und", "es", "sol\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "VVPP", "$.", "KON", "PPER", "VMFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die franz\u00f6sisch wohlgestellten", "tokens": ["Die", "fran\u00b7z\u00f6\u00b7sisch", "wohl\u00b7ge\u00b7stell\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJD", "ADJA"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Worte f\u00fcr Erinnrung gelten!", "tokens": ["Wor\u00b7te", "f\u00fcr", "E\u00b7rinn\u00b7rung", "gel\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mitleid also und Erbarmen", "tokens": ["Mit\u00b7leid", "al\u00b7so", "und", "Er\u00b7bar\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADV", "KON", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Schenken gn\u00e4dig Sie dem Armen,", "tokens": ["Schen\u00b7ken", "gn\u00e4\u00b7dig", "Sie", "dem", "Ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dessen Tr\u00e4nen Sie entrollen", "tokens": ["Des\u00b7sen", "Tr\u00e4\u00b7nen", "Sie", "ent\u00b7rol\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PDAT", "NN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Sehen, ohne nur zu wissen,", "tokens": ["Se\u00b7hen", ",", "oh\u00b7ne", "nur", "zu", "wis\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUI", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Welch ein D\u00e4mon ihn bet\u00f6rt.", "tokens": ["Welch", "ein", "D\u00e4\u00b7mon", "ihn", "be\u00b7t\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "O du hast mein Herz zerrissen", "tokens": ["O", "du", "hast", "mein", "Herz", "zer\u00b7ris\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "VAFIN", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Unerh\u00f6rt!", "tokens": ["Un\u00b7er\u00b7h\u00f6rt", "!"], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Hab in altem Buch gelesen", "tokens": ["Hab", "in", "al\u00b7tem", "Buch", "ge\u00b7le\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eine wundersame Sage,", "tokens": ["Ei\u00b7ne", "wun\u00b7der\u00b7sa\u00b7me", "Sa\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wer der ew'ge Jud gewesen.", "tokens": ["Wer", "der", "ew'\u00b7ge", "Jud", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "VAPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nicht kann Ahasverus sterben,", "tokens": ["Nicht", "kann", "A\u00b7has\u00b7ve\u00b7rus", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VMFIN", "NE", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sterben nicht, noch Ruh erwerben,", "tokens": ["Ster\u00b7ben", "nicht", ",", "noch", "Ruh", "er\u00b7wer\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "$,", "ADV", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bis der Herr am j\u00fcngsten Tage", "tokens": ["Bis", "der", "Herr", "am", "j\u00fcng\u00b7sten", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Ruft die Toten aus dem Grabe,", "tokens": ["Ruft", "die", "To\u00b7ten", "aus", "dem", "Gra\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und auch er vernimmt das Wort;", "tokens": ["Und", "auch", "er", "ver\u00b7nimmt", "das", "Wort", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.9": {"text": "Und er wankt am Wanderstabe", "tokens": ["Und", "er", "wankt", "am", "Wan\u00b7der\u00b7sta\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Fort und fort.", "tokens": ["Fort", "und", "fort", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "F\u00fcrder durch der Erde Weiten", "tokens": ["F\u00fcr\u00b7der", "durch", "der", "Er\u00b7de", "Wei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Rastlos, m\u00fcden Fu\u00dfes wallt er,", "tokens": ["Rast\u00b7los", ",", "m\u00fc\u00b7den", "Fu\u00b7\u00dfes", "wallt", "er", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJA", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "L\u00e4\u00dft die Weltgeschichte fluten.", "tokens": ["L\u00e4\u00dft", "die", "Welt\u00b7ge\u00b7schich\u00b7te", "flu\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Menschenalter ihm Minuten,", "tokens": ["Men\u00b7schen\u00b7al\u00b7ter", "ihm", "Mi\u00b7nu\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und Minuten Menschenalter,", "tokens": ["Und", "Mi\u00b7nu\u00b7ten", "Men\u00b7schen\u00b7al\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Stehen still vor ihm die Zeiten,", "tokens": ["Ste\u00b7hen", "still", "vor", "ihm", "die", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Bleibt in ihm sein Herz, das alte,", "tokens": ["Bleibt", "in", "ihm", "sein", "Herz", ",", "das", "al\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "PPOSAT", "NN", "$,", "ART", "ADJA", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.8": {"text": "Drin der alte Schmerz gebannt,", "tokens": ["Drin", "der", "al\u00b7te", "Schmerz", "ge\u00b7bannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Lastend \u00fcber ihm die kalte", "tokens": ["Las\u00b7tend", "\u00fc\u00b7ber", "ihm", "die", "kal\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "PPER", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Schicksalshand.", "tokens": ["Schick\u00b7sals\u00b7hand", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.5": {"line.1": {"text": "Aber stets nach hundert Jahren", "tokens": ["A\u00b7ber", "stets", "nach", "hun\u00b7dert", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Treibt's nach Salem ihn zu wandern,", "tokens": ["Treibt's", "nach", "Sa\u00b7lem", "ihn", "zu", "wan\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NE", "PPER", "PTKZU", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Von der Heimat zu erfahren.", "tokens": ["Von", "der", "Hei\u00b7mat", "zu", "er\u00b7fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "R\u00f6mer, Sarazenen, Franken", "tokens": ["R\u00f6\u00b7mer", ",", "Sa\u00b7ra\u00b7ze\u00b7nen", ",", "Fran\u00b7ken"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["NE", "$,", "NN", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wechselten, verdr\u00e4ngt von andern,", "tokens": ["Wech\u00b7sel\u00b7ten", ",", "ver\u00b7dr\u00e4ngt", "von", "an\u00b7dern", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "APPR", "ADJA", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Tempel und Alt\u00e4re sanken,", "tokens": ["Tem\u00b7pel", "und", "Al\u00b7t\u00e4\u00b7re", "san\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Mauern und Pal\u00e4ste brachen,", "tokens": ["Mau\u00b7ern", "und", "Pa\u00b7l\u00e4s\u00b7te", "bra\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Fl\u00fcsse wandten ihren Lauf,", "tokens": ["Fl\u00fcs\u00b7se", "wand\u00b7ten", "ih\u00b7ren", "Lauf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Neue G\u00f6tter, neue Sprachen", "tokens": ["Neu\u00b7e", "G\u00f6t\u00b7ter", ",", "neu\u00b7e", "Spra\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Steigen auf.", "tokens": ["Stei\u00b7gen", "auf", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.6": {"line.1": {"text": "D\u00fcster sinnt der Fremdgewordne", "tokens": ["D\u00fcs\u00b7ter", "sinnt", "der", "Fremd\u00b7ge\u00b7word\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00dcber unbekannten Tr\u00fcmmern,", "tokens": ["\u00dc\u00b7ber", "un\u00b7be\u00b7kann\u00b7ten", "Tr\u00fcm\u00b7mern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df im Geist er's wieder ordne;", "tokens": ["Da\u00df", "im", "Geist", "er's", "wie\u00b7der", "ord\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "PIS", "ADV", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und er fragt, und fragt vergebens,", "tokens": ["Und", "er", "fragt", ",", "und", "fragt", "ver\u00b7ge\u00b7bens", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "KON", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Keiner will um ihn sich k\u00fcmmern,", "tokens": ["Kei\u00b7ner", "will", "um", "ihn", "sich", "k\u00fcm\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "APPR", "PPER", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Auf dem Grabe seines Lebens", "tokens": ["Auf", "dem", "Gra\u00b7be", "sei\u00b7nes", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Steht versteint der Sohn der Schmerzen,", "tokens": ["Steht", "ver\u00b7steint", "der", "Sohn", "der", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "\u00dcber ihn hin braust der Sturm,", "tokens": ["\u00dc\u00b7ber", "ihn", "hin", "braust", "der", "Sturm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Und in seinem alten Herzen", "tokens": ["Und", "in", "sei\u00b7nem", "al\u00b7ten", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Nagt der Wurm.", "tokens": ["Nagt", "der", "Wurm", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.7": {"line.1": {"text": "Ich bin Ahasverus, sag ich!", "tokens": ["Ich", "bin", "A\u00b7has\u00b7ve\u00b7rus", ",", "sag", "ich", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NE", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sieh darauf mich an verwundert,", "tokens": ["Sieh", "da\u00b7rauf", "mich", "an", "ver\u00b7wun\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PAV", "PRF", "APZR", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Salem du, wovor mir grauet.", "tokens": ["Sa\u00b7lem", "du", ",", "wo\u00b7vor", "mir", "grau\u00b7et", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Irrens m\u00fcd, das Haar ergrauet,", "tokens": ["Ir\u00b7rens", "m\u00fcd", ",", "das", "Haar", "er\u00b7grau\u00b7et", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wank ich heim nach aber hundert", "tokens": ["Wank", "ich", "heim", "nach", "a\u00b7ber", "hun\u00b7dert"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKVZ", "APPR", "ADV", "CARD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Jahren und vergebens frag ich,", "tokens": ["Jah\u00b7ren", "und", "ver\u00b7ge\u00b7bens", "frag", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADV", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Ruf ich \u2013 in den \u00f6den Mauern", "tokens": ["Ruf", "ich", "\u2013", "in", "den", "\u00f6\u00b7den", "Mau\u00b7ern"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPER", "$(", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Weck ich keinen Widerhall; \u2013", "tokens": ["Weck", "ich", "kei\u00b7nen", "Wi\u00b7der\u00b7hall", ";", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PPER", "PIAT", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Sieh Versteinten mich betrauern", "tokens": ["Sieh", "Ver\u00b7stein\u00b7ten", "mich", "be\u00b7trau\u00b7ern"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Salems Fall.", "tokens": ["Sa\u00b7lems", "Fall", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}