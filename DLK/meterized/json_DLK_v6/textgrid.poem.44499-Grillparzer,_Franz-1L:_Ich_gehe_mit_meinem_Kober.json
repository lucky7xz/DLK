{"textgrid.poem.44499": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich gehe mit meinem Kober", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich gehe mit meinem Kober", "tokens": ["Ich", "ge\u00b7he", "mit", "mei\u00b7nem", "Ko\u00b7ber"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und meinem Hakenstab,", "tokens": ["Und", "mei\u00b7nem", "Ha\u00b7ken\u00b7stab", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wo von Mist ein Schober,", "tokens": ["Und", "wo", "von", "Mist", "ein", "Scho\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Setz ich die B\u00fcrde ab.", "tokens": ["Setz", "ich", "die", "B\u00fcr\u00b7de", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Da wird geforscht, zerstochen", "tokens": ["Da", "wird", "ge\u00b7forscht", ",", "zer\u00b7sto\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ADV", "VAFIN", "VVPP", "$,", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Kehricht weit und tief,", "tokens": ["Der", "Keh\u00b7richt", "weit", "und", "tief", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ob irgend ein Abfall, ein Knochen", "tokens": ["Ob", "ir\u00b7gend", "ein", "Ab\u00b7fall", ",", "ein", "Kno\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "$,", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Sich etwa hinein verlief.", "tokens": ["Sich", "et\u00b7wa", "hin\u00b7ein", "ver\u00b7lief", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und was ich da gefunden,", "tokens": ["Und", "was", "ich", "da", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Trag ich vergn\u00fcgt nach Haus", "tokens": ["Trag", "ich", "ver\u00b7gn\u00fcgt", "nach", "Haus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "VVFIN", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und sied in einsamen Stunden", "tokens": ["Und", "sied", "in", "ein\u00b7sa\u00b7men", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Manch sch\u00f6ne Notiz heraus.", "tokens": ["Manch", "sch\u00f6\u00b7ne", "No\u00b7tiz", "he\u00b7raus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}