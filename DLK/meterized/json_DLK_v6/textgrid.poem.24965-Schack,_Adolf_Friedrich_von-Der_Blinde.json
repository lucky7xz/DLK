{"textgrid.poem.24965": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Der Blinde", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht im Fr\u00fchrot siehst du mehr", "tokens": ["Nicht", "im", "Fr\u00fch\u00b7rot", "siehst", "du", "mehr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "APPRART", "NN", "VVFIN", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Purpurn gl\u00fchn die Himmelsr\u00e4nder,", "tokens": ["Pur\u00b7purn", "gl\u00fchn", "die", "Him\u00b7mels\u00b7r\u00e4n\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nicht den Tag, der hoch daher", "tokens": ["Nicht", "den", "Tag", ",", "der", "hoch", "da\u00b7her"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "ART", "NN", "$,", "PRELS", "ADJD", "PAV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wandelt um die Erdenl\u00e4nder,", "tokens": ["Wan\u00b7delt", "um", "die", "Er\u00b7den\u00b7l\u00e4n\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nicht des Mondes milden Schein,", "tokens": ["Nicht", "des", "Mon\u00b7des", "mil\u00b7den", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Noch den Fr\u00fchling und die Rose:", "tokens": ["Noch", "den", "Fr\u00fch\u00b7ling", "und", "die", "Ro\u00b7se", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Ewig starrt dein Blick allein", "tokens": ["E\u00b7wig", "starrt", "dein", "Blick", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "In die Nacht, die grenzenlose.", "tokens": ["In", "die", "Nacht", ",", "die", "gren\u00b7zen\u00b7lo\u00b7se", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ART", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Aber herrlich strahlend bricht,", "tokens": ["A\u00b7ber", "herr\u00b7lich", "strah\u00b7lend", "bricht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie Arktur durch Wolkenrisse,", "tokens": ["Wie", "Ark\u00b7tur", "durch", "Wol\u00b7ken\u00b7ris\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Deiner Seele klares Licht", "tokens": ["Dei\u00b7ner", "See\u00b7le", "kla\u00b7res", "Licht"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Durch des Auges Finsternisse;", "tokens": ["Durch", "des", "Au\u00b7ges", "Fins\u00b7ter\u00b7nis\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn was andern Blindheit hei\u00dft,", "tokens": ["Denn", "was", "an\u00b7dern", "Blind\u00b7heit", "hei\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Gab der Himmel dir als H\u00fclle,", "tokens": ["Gab", "der", "Him\u00b7mel", "dir", "als", "H\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "KOUS", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Drunter ungest\u00f6rt dein Geist", "tokens": ["Drun\u00b7ter", "un\u00b7ge\u00b7st\u00f6rt", "dein", "Geist"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "ADJD", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Schwelg' in reinen Glanzes F\u00fclle.", "tokens": ["Schwel\u00b7g'", "in", "rei\u00b7nen", "Glan\u00b7zes", "F\u00fcl\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Hell wie durch ein Seherohr,", "tokens": ["Hell", "wie", "durch", "ein", "Se\u00b7he\u00b7rohr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Schaut er tief im sternbes\u00e4ten", "tokens": ["Schaut", "er", "tief", "im", "stern\u00b7be\u00b7s\u00e4\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "APPRART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aetherblau den Reigenchor", "tokens": ["A\u00b7e\u00b7ther\u00b7blau", "den", "Rei\u00b7gen\u00b7chor"], "token_info": ["word", "word", "word"], "pos": ["NE", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aller Sonnen und Planeten,", "tokens": ["Al\u00b7ler", "Son\u00b7nen", "und", "Pla\u00b7ne\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$,"], "meter": "--+--+--", "measure": "anapaest.di.plus"}, "line.5": {"text": "Und das Kreuz, das \u00fcberm Haupt", "tokens": ["Und", "das", "Kreuz", ",", "das", "\u00fc\u00b7berm", "Haupt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Unsrer Elterv\u00e4ter kreiste \u2013", "tokens": ["Uns\u00b7rer", "El\u00b7ter\u00b7v\u00e4\u00b7ter", "kreis\u00b7te", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "L\u00e4ngst ist seiner nun beraubt", "tokens": ["L\u00e4ngst", "ist", "sei\u00b7ner", "nun", "be\u00b7raubt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADV", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Unser Himmel, der verwaiste.", "tokens": ["Un\u00b7ser", "Him\u00b7mel", ",", "der", "ver\u00b7wais\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Fernehin des Orients", "tokens": ["Fer\u00b7ne\u00b7hin", "des", "O\u00b7rients"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ART", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Thore sieht er aufgeschlossen", "tokens": ["Tho\u00b7re", "sieht", "er", "auf\u00b7ge\u00b7schlos\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und den ersten Erdenlenz", "tokens": ["Und", "den", "ers\u00b7ten", "Er\u00b7den\u00b7lenz"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ueber Eden ausgegossen,", "tokens": ["Ue\u00b7ber", "E\u00b7den", "aus\u00b7ge\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sieht von Indiens Kaukasus", "tokens": ["Sieht", "von", "In\u00b7di\u00b7ens", "Kau\u00b7ka\u00b7sus"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "NE", "NE"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Hoch aufgl\u00fchn die Gletscherzinnen", "tokens": ["Hoch", "auf\u00b7gl\u00fchn", "die", "Glet\u00b7scher\u00b7zin\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.7": {"text": "Und den Paradiesesflu\u00df", "tokens": ["Und", "den", "Pa\u00b7ra\u00b7die\u00b7ses\u00b7flu\u00df"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Vierfach durch die L\u00e4nder rinnen;", "tokens": ["Vier\u00b7fach", "durch", "die", "L\u00e4n\u00b7der", "rin\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Sieht die Inseln Griechenlands", "tokens": ["Sieht", "die", "In\u00b7seln", "Grie\u00b7chen\u00b7lands"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Glorreich tauchen aus dem Meere,", "tokens": ["Glor\u00b7reich", "tau\u00b7chen", "aus", "dem", "Mee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Ch\u00f6re Feiertanz", "tokens": ["Und", "der", "Ch\u00f6\u00b7re", "Fei\u00b7er\u00b7tanz"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Um die flammenden Alt\u00e4re,", "tokens": ["Um", "die", "flam\u00b7men\u00b7den", "Al\u00b7t\u00e4\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und mit Rossen, die den Tag", "tokens": ["Und", "mit", "Ros\u00b7sen", ",", "die", "den", "Tag"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "$,", "PRELS", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Aus den m\u00e4cht'gen N\u00fcstern spr\u00fchen,", "tokens": ["Aus", "den", "m\u00e4cht'\u00b7gen", "N\u00fcs\u00b7tern", "spr\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Bei der Wogen h\u00f6herm Schlag", "tokens": ["Bei", "der", "Wo\u00b7gen", "h\u00f6\u00b7herm", "Schlag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Helios nahn im Morgengl\u00fchen.", "tokens": ["He\u00b7li\u00b7os", "nahn", "im", "Mor\u00b7gen\u00b7gl\u00fc\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "APPRART", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.6": {"line.1": {"text": "Milde leuchtend immerdar,", "tokens": ["Mil\u00b7de", "leuch\u00b7tend", "im\u00b7mer\u00b7dar", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVPP", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "D\u00e4mmert durch der Zukunft Schleier", "tokens": ["D\u00e4m\u00b7mert", "durch", "der", "Zu\u00b7kunft", "Schlei\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dir das neue Erdenjahr", "tokens": ["Dir", "das", "neu\u00b7e", "Er\u00b7den\u00b7jahr"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die gro\u00dfe Fr\u00fchlingsfeier,", "tokens": ["Und", "die", "gro\u00b7\u00dfe", "Fr\u00fch\u00b7lings\u00b7fei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wenn die Menschen sich, befreit,", "tokens": ["Wenn", "die", "Men\u00b7schen", "sich", ",", "be\u00b7freit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PRF", "$,", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Nur dem Joch der Liebe f\u00fcgen,", "tokens": ["Nur", "dem", "Joch", "der", "Lie\u00b7be", "f\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und, wie in der goldnen Zeit,", "tokens": ["Und", ",", "wie", "in", "der", "gold\u00b7nen", "Zeit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Lamm und Leu beisammen liegen.", "tokens": ["Lamm", "und", "Leu", "bei\u00b7sam\u00b7men", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVIZU", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "In der Nacht der Blindheit so", "tokens": ["In", "der", "Nacht", "der", "Blind\u00b7heit", "so"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mahnst du mich, begl\u00fcckter Seher,", "tokens": ["Mahnst", "du", "mich", ",", "be\u00b7gl\u00fcck\u00b7ter", "Se\u00b7her", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "An den Aar, der sonnenfroh", "tokens": ["An", "den", "Aar", ",", "der", "son\u00b7nen\u00b7froh"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Droben schwebt, dem Lichtquell n\u00e4her;", "tokens": ["Dro\u00b7ben", "schwebt", ",", "dem", "Licht\u00b7quell", "n\u00e4\u00b7her", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ach! uns Seh'nde labt sie nicht,", "tokens": ["Ach", "!", "uns", "Seh'n\u00b7de", "labt", "sie", "nicht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPER", "NN", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Jene lautre Strahlenquelle;", "tokens": ["Je\u00b7ne", "laut\u00b7re", "Strah\u00b7len\u00b7quel\u00b7le", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Uns erstirbt das h\u00f6hre Licht", "tokens": ["Uns", "er\u00b7stirbt", "das", "h\u00f6h\u00b7re", "Licht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "In des Tags gemeiner Helle.", "tokens": ["In", "des", "Tags", "ge\u00b7mei\u00b7ner", "Hel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}