{"textgrid.poem.26174": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Das erste Heu liegt schon im Wiesenland", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einf\u00f6rmig sind des Fr\u00fchlings letzte N\u00e4chte.", "tokens": ["Ein\u00b7f\u00f6r\u00b7mig", "sind", "des", "Fr\u00fch\u00b7lings", "letz\u00b7te", "N\u00e4ch\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Heckenrose blinkt im Dunkel wie aus Porzellan", "tokens": ["Die", "He\u00b7cken\u00b7ro\u00b7se", "blinkt", "im", "Dun\u00b7kel", "wie", "aus", "Por\u00b7zel\u00b7lan"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "KOKOM", "APPR", "NE"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Und sieht dich aus den Dornen lockend an,", "tokens": ["Und", "sieht", "dich", "aus", "den", "Dor\u00b7nen", "lo\u00b7ckend", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Als ob sie gerne deine Hand zum Pfl\u00fccken br\u00e4chte.", "tokens": ["Als", "ob", "sie", "ger\u00b7ne", "dei\u00b7ne", "Hand", "zum", "Pfl\u00fc\u00b7cken", "br\u00e4ch\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "ADV", "PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Das erste Heu liegt schon im Wiesenland,", "tokens": ["Das", "ers\u00b7te", "Heu", "liegt", "schon", "im", "Wie\u00b7sen\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Der Viertelmond lehnt wie die Sense an der Himmelswand,", "tokens": ["Der", "Vier\u00b7tel\u00b7mond", "lehnt", "wie", "die", "Sen\u00b7se", "an", "der", "Him\u00b7mels\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KOKOM", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.7": {"text": "Als ob er Arbeit in der Nacht noch fand", "tokens": ["Als", "ob", "er", "Ar\u00b7beit", "in", "der", "Nacht", "noch", "fand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "PPER", "NN", "APPR", "ART", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und blank und stark die Felder niedermacht", "tokens": ["Und", "blank", "und", "stark", "die", "Fel\u00b7der", "nie\u00b7der\u00b7macht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PTKVZ", "KON", "ADJD", "ART", "NN", "PTKNEG"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Und alles, was die Fr\u00fchlingsleidenschaft erdacht.", "tokens": ["Und", "al\u00b7les", ",", "was", "die", "Fr\u00fch\u00b7lings\u00b7lei\u00b7den\u00b7schaft", "er\u00b7dacht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PRELS", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}