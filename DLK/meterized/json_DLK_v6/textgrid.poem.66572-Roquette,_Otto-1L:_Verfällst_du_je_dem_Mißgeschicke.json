{"textgrid.poem.66572": {"metadata": {"author": {"name": "Roquette, Otto", "birth": "N.A.", "death": "N.A."}, "title": "1L: Verf\u00e4llst du je dem Mi\u00dfgeschicke", "genre": "verse", "period": "N.A.", "pub_year": 1860, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verf\u00e4llst du je dem Mi\u00dfgeschicke", "tokens": ["Ver\u00b7f\u00e4llst", "du", "je", "dem", "Mi\u00df\u00b7ge\u00b7schi\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Tint' und Feder zu rumoren,", "tokens": ["Mit", "Tint'", "und", "Fe\u00b7der", "zu", "ru\u00b7mo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dann, J\u00fcngling, schaff dir eine Clique,", "tokens": ["Dann", ",", "J\u00fcng\u00b7ling", ",", "schaff", "dir", "ei\u00b7ne", "Cli\u00b7que", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "$,", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Sonst bist du rettungslos verloren!", "tokens": ["Sonst", "bist", "du", "ret\u00b7tungs\u00b7los", "ver\u00b7lo\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Kannst du der Clique dienen kr\u00e4ftig,", "tokens": ["Kannst", "du", "der", "Cli\u00b7que", "die\u00b7nen", "kr\u00e4f\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Macht sie daf\u00fcr dir kr\u00e4ftig Claque,", "tokens": ["Macht", "sie", "da\u00b7f\u00fcr", "dir", "kr\u00e4f\u00b7tig", "Cla\u00b7que", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PAV", "PPER", "ADJD", "NE", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Und dienstbeflissen und gesch\u00e4ftig", "tokens": ["Und", "dienst\u00b7be\u00b7flis\u00b7sen", "und", "ge\u00b7sch\u00e4f\u00b7tig"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PDS", "KON", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Kauft sie dir ab die Katz' im Sacke.", "tokens": ["Kauft", "sie", "dir", "ab", "die", "Katz'", "im", "Sa\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "APPR", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "O J\u00fcngling, wirf dich in die Masse,", "tokens": ["O", "J\u00fcng\u00b7ling", ",", "wirf", "dich", "in", "die", "Mas\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVIMP", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auf da\u00df du lernest \u00fcberwinden,", "tokens": ["Auf", "da\u00df", "du", "ler\u00b7nest", "\u00fc\u00b7berw\u00b7in\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mit welcher niedertr\u00e4chtgen Rasse", "tokens": ["Mit", "wel\u00b7cher", "nie\u00b7der\u00b7tr\u00e4cht\u00b7gen", "Ras\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PWAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dein Leben du hast abzufinden!", "tokens": ["Dein", "Le\u00b7ben", "du", "hast", "ab\u00b7zu\u00b7fin\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VAFIN", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}