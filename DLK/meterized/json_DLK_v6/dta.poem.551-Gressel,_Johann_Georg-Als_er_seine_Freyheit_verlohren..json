{"dta.poem.551": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Als er seine Freyheit verlohren.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Ich bin verliebt/", "tokens": ["Ich", "bin", "ver\u00b7liebt", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Die Freyheit ist nun hin/", "tokens": ["Die", "Frey\u00b7heit", "ist", "nun", "hin", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PTKVZ", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Womit mein harter Sinn", "tokens": ["Wo\u00b7mit", "mein", "har\u00b7ter", "Sinn"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Sich sonst zu kitzeln pflegte/", "tokens": ["Sich", "sonst", "zu", "kit\u00b7zeln", "pfleg\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "PTKZU", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Als ob er keinen Brand in seinem Hertzen hegte.", "tokens": ["Als", "ob", "er", "kei\u00b7nen", "Brand", "in", "sei\u00b7nem", "Hert\u00b7zen", "heg\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "PIAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ich wei\u00df nicht wie mir ist", "tokens": ["Ich", "wei\u00df", "nicht", "wie", "mir", "ist"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "PWAV", "PPER", "VAFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wenn ich ", "tokens": ["Wenn", "ich"], "token_info": ["word", "word"], "pos": ["KOUS", "PPER"], "meter": "++", "measure": "spondeus"}, "line.8": {"text": "Die mir mein Seelen-Licht/", "tokens": ["Die", "mir", "mein", "See\u00b7len\u00b7Licht", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPOSAT", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Die ich zur Herrscherin mir auserkiest/", "tokens": ["Die", "ich", "zur", "Herr\u00b7sche\u00b7rin", "mir", "aus\u00b7er\u00b7kiest", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Kan vor den Augen sehn.", "tokens": ["Kan", "vor", "den", "Au\u00b7gen", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Das Hertz erstarrt vor Lust/", "tokens": ["Das", "Hertz", "er\u00b7starrt", "vor", "Lust", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Und schl\u00e4get in der Brust", "tokens": ["Und", "schl\u00e4\u00b7get", "in", "der", "Brust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Wenn mich ihr Gnaden-Schein", "tokens": ["Wenn", "mich", "ihr", "Gna\u00b7den\u00b7Schein"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "L\u00e4st gl\u00fccklich seyn/", "tokens": ["L\u00e4st", "gl\u00fcck\u00b7lich", "seyn", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAINF", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.15": {"text": "Und mich mit seinem Strahl erquickt/", "tokens": ["Und", "mich", "mit", "sei\u00b7nem", "Strahl", "er\u00b7quickt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Der mehr mein kranckes Hertz begl\u00fcckt/", "tokens": ["Der", "mehr", "mein", "kran\u00b7ckes", "Hertz", "be\u00b7gl\u00fcckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPOSAT", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Als k\u00fchler Morgen-Than", "tokens": ["Als", "k\u00fch\u00b7ler", "Mor\u00b7gen\u00b7Than"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.18": {"text": "Der d\u00fcrren Au", "tokens": ["Der", "d\u00fcr\u00b7ren", "Au"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.19": {"text": "Lieb und erfreulich ist", "tokens": ["Lieb", "und", "er\u00b7freu\u00b7lich", "ist"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "ADJD", "VAFIN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.20": {"text": "Und wie der Balsam St\u00e4rcke giebt/", "tokens": ["Und", "wie", "der", "Bal\u00b7sam", "St\u00e4r\u00b7cke", "giebt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Wenn die Lebens-Kr\u00e4fft\u2019 entgehn.", "tokens": ["Wenn", "die", "Le\u00b7bens\u00b7Kr\u00e4fft'", "ent\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.22": {"text": "Wann aber die Entfernung macht", "tokens": ["Wann", "a\u00b7ber", "die", "Ent\u00b7fer\u00b7nung", "macht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Da\u00df ich ihr Gnaden-Licht", "tokens": ["Da\u00df", "ich", "ihr", "Gna\u00b7den\u00b7Licht"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.24": {"text": "Nicht kriege zu Gesicht/", "tokens": ["Nicht", "krie\u00b7ge", "zu", "Ge\u00b7sicht", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "APPR", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.25": {"text": "So schrecket mich Egyptens Macht", "tokens": ["So", "schre\u00b7cket", "mich", "E\u00b7gyp\u00b7tens", "Macht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NE", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Mit Kummer-vollen Tr\u00e4umen.", "tokens": ["Mit", "Kum\u00b7mer\u00b7vol\u00b7len", "Tr\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.27": {"text": "Bald stirbt der Geist/", "tokens": ["Bald", "stirbt", "der", "Geist", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.28": {"text": "Der doch unsterblich heist/", "tokens": ["Der", "doch", "uns\u00b7terb\u00b7lich", "heist", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "VAFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.29": {"text": "Bald mu\u00df ich mehr denn K\u00f6nig seyn/", "tokens": ["Bald", "mu\u00df", "ich", "mehr", "denn", "K\u00f6\u00b7nig", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADV", "NN", "VAINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Bald Thron und Scepter r\u00e4umen.", "tokens": ["Bald", "Thron", "und", "Scep\u00b7ter", "r\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.31": {"text": "Bald f\u00e4llt mir dis bald jenes ein/", "tokens": ["Bald", "f\u00e4llt", "mir", "dis", "bald", "je\u00b7nes", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PDS", "ADV", "PDAT", "ART", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Ich zancke mich/ und habe keinen Zwist.", "tokens": ["Ich", "zan\u00b7cke", "mich", "/", "und", "ha\u00b7be", "kei\u00b7nen", "Zwist", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$(", "KON", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.33": {"text": "Ich tr\u00e4ume wachend/ und wei\u00df nicht was ich thu/", "tokens": ["Ich", "tr\u00e4u\u00b7me", "wa\u00b7chend", "/", "und", "wei\u00df", "nicht", "was", "ich", "thu", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$(", "KON", "VVFIN", "PTKNEG", "PWS", "PPER", "VVFIN", "$("], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.34": {"text": "Mein Geist besitzet keine Ruh/", "tokens": ["Mein", "Geist", "be\u00b7sit\u00b7zet", "kei\u00b7ne", "Ruh", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PIAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Er ist mit Gedancken/", "tokens": ["Er", "ist", "mit", "Ge\u00b7dan\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.36": {"text": "So der Traurigkeit-Schrancken", "tokens": ["So", "der", "Trau\u00b7rig\u00b7keit\u00b7Schran\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.37": {"text": "Weit \u00fcbertreten/ beschweret/", "tokens": ["Weit", "\u00fc\u00b7bert\u00b7re\u00b7ten", "/", "be\u00b7schwe\u00b7ret", "/"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJD", "VVFIN", "$(", "VVFIN", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.38": {"text": "Die Freyheit ist in Hafft verkehret.", "tokens": ["Die", "Frey\u00b7heit", "ist", "in", "Hafft", "ver\u00b7keh\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.39": {"text": "Ein eintziger Anblick und freundliche Zurede", "tokens": ["Ein", "eint\u00b7zi\u00b7ger", "An\u00b7blick", "und", "freund\u00b7li\u00b7che", "Zu\u00b7re\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "ADJA", "NN"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.40": {"text": "Jhrer lachenden Augen/ und lieblichen Zungen", "tokens": ["Ih\u00b7rer", "la\u00b7chen\u00b7den", "Au\u00b7gen", "/", "und", "lieb\u00b7li\u00b7chen", "Zun\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$(", "KON", "ADJA", "NN"], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.41": {"text": "Haben mein vestes Hertz bezwungen/", "tokens": ["Ha\u00b7ben", "mein", "ves\u00b7tes", "Hertz", "be\u00b7zwun\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "VVPP", "$("], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.42": {"text": "So da\u00df ichs mich zu sagen fast entbl\u00f6de.", "tokens": ["So", "da\u00df", "ichs", "mich", "zu", "sa\u00b7gen", "fast", "ent\u00b7bl\u00f6\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PIS", "PRF", "PTKZU", "VVINF", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.43": {"text": "Der Diamantne Sinn ist wie weiches Wachs zerronnen/", "tokens": ["Der", "Di\u00b7a\u00b7mant\u00b7ne", "Sinn", "ist", "wie", "wei\u00b7ches", "Wachs", "zer\u00b7ron\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "KOKOM", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.44": {"text": "Wenn es die Hitze schmeltzt der ", "tokens": ["Wenn", "es", "die", "Hit\u00b7ze", "schmeltzt", "der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.45": {"text": "Ich bin nicht/ der ich bin/", "tokens": ["Ich", "bin", "nicht", "/", "der", "ich", "bin", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "$(", "PRELS", "PPER", "VAFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.46": {"text": "Ich bin gantz anderst worden", "tokens": ["Ich", "bin", "gantz", "an\u00b7derst", "wor\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "VAPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.47": {"text": "Nun ich mich schreib\u2019 in dem verliebten Orden.", "tokens": ["Nun", "ich", "mich", "schreib'", "in", "dem", "ver\u00b7lieb\u00b7ten", "Or\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PRF", "ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.48": {"text": "Der eyserne Sinn", "tokens": ["Der", "ey\u00b7ser\u00b7ne", "Sinn"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.49": {"text": "Lenckt sich zu dem ", "tokens": ["Lenckt", "sich", "zu", "dem"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "ART"], "meter": "+-+-", "measure": "trochaic.di"}, "line.50": {"text": "Wie dieser hin nach Norden geht.", "tokens": ["Wie", "die\u00b7ser", "hin", "nach", "Nor\u00b7den", "geht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.51": {"text": "Ich f\u00fchle eine angenehme Pein", "tokens": ["Ich", "f\u00fch\u00b7le", "ei\u00b7ne", "an\u00b7ge\u00b7neh\u00b7me", "Pein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.52": {"text": "Die mir durchw\u00fchlet Marck und Bein;", "tokens": ["Die", "mir", "durch\u00b7w\u00fch\u00b7let", "Marck", "und", "Bein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.53": {"text": "Bald bin ich froh", "tokens": ["Bald", "bin", "ich", "froh"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.54": {"text": "Und sing\u2019 ", "tokens": ["Und", "sing'"], "token_info": ["word", "word"], "pos": ["KON", "NE"], "meter": "-+", "measure": "iambic.single"}, "line.55": {"text": "Bald aber stimm\u2019 ich ", "tokens": ["Bald", "a\u00b7ber", "stimm'", "ich"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER"], "meter": "-+-+-", "measure": "iambic.di"}, "line.56": {"text": "Und f\u00fchr die Trauer-Fahn.", "tokens": ["Und", "f\u00fchr", "die", "Trau\u00b7e\u00b7rFahn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.57": {"text": "Ich bin fast halb verwirrt", "tokens": ["Ich", "bin", "fast", "halb", "ver\u00b7wirrt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.58": {"text": "Und in der Zeit verirrt/", "tokens": ["Und", "in", "der", "Zeit", "ver\u00b7irrt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.59": {"text": "Ich wei\u00df nicht was mich plagt/ und Freude giebt/", "tokens": ["Ich", "wei\u00df", "nicht", "was", "mich", "plagt", "/", "und", "Freu\u00b7de", "giebt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PWS", "PPER", "VVFIN", "$(", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.60": {"text": "Als da\u00df ich bin verliebt.", "tokens": ["Als", "da\u00df", "ich", "bin", "ver\u00b7liebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}