{"dta.poem.19235": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "82.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die L\u00f6win gieng auf Raub, und lie\u00df daheim zwei Jungen,", "tokens": ["Die", "L\u00f6\u00b7win", "gieng", "auf", "Raub", ",", "und", "lie\u00df", "da\u00b7heim", "zwei", "Jun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,", "KON", "VVFIN", "ADV", "CARD", "NN", "$,"], "meter": "-+---+-+-+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Die hatten noch kein Blut geleckt mit ihren Zungen.", "tokens": ["Die", "hat\u00b7ten", "noch", "kein", "Blut", "ge\u00b7leckt", "mit", "ih\u00b7ren", "Zun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "PIAT", "NN", "VVPP", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Sie hatten nur die Milch der Mutterbrust gesogen,", "tokens": ["Sie", "hat\u00b7ten", "nur", "die", "Milch", "der", "Mut\u00b7ter\u00b7brust", "ge\u00b7so\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und ihren Kindern war der Mutter Herz gewogen.", "tokens": ["Und", "ih\u00b7ren", "Kin\u00b7dern", "war", "der", "Mut\u00b7ter", "Herz", "ge\u00b7wo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Sie schlang den blut'gen Raub nun mit zwiefacher Lust,", "tokens": ["Sie", "schlang", "den", "blut'\u00b7gen", "Raub", "nun", "mit", "zwie\u00b7fac\u00b7her", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Um ihrem Paar mit Milch zu f\u00fcllen jede Brust.", "tokens": ["Um", "ih\u00b7rem", "Paar", "mit", "Milch", "zu", "f\u00fcl\u00b7len", "je\u00b7de", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "APPR", "NN", "PTKZU", "VVINF", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Doch als sie heim nun kam, war ihr zuvorgekommen", "tokens": ["Doch", "als", "sie", "heim", "nun", "kam", ",", "war", "ihr", "zu\u00b7vor\u00b7ge\u00b7kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "PTKVZ", "ADV", "VVFIN", "$,", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein k\u00fchner J\u00e4ger, der die Jungen weggenommen.", "tokens": ["Ein", "k\u00fch\u00b7ner", "J\u00e4\u00b7ger", ",", "der", "die", "Jun\u00b7gen", "weg\u00b7ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Die L\u00f6win, wie sie sah sich ihrer Brut beraubt,", "tokens": ["Die", "L\u00f6\u00b7win", ",", "wie", "sie", "sah", "sich", "ih\u00b7rer", "Brut", "be\u00b7raubt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PPER", "VVFIN", "PRF", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie hat sie mit Gebr\u00fcll den Wald durchras't, durchschnaubt!", "tokens": ["Wie", "hat", "sie", "mit", "Ge\u00b7br\u00fcll", "den", "Wald", "durch\u00b7ras't", ",", "durch\u00b7schnaubt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "APPR", "NN", "ART", "NN", "VVFIN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Die Aeffin auf dem Baum (sie hielt im Arm ein Kind)", "tokens": ["Die", "A\u00b7ef\u00b7fin", "auf", "dem", "Baum", "(", "sie", "hielt", "im", "Arm", "ein", "Kind", ")"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$(", "PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "$("], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Sah zu, und rief: Warum tobst du so ungelind?", "tokens": ["Sah", "zu", ",", "und", "rief", ":", "Wa\u00b7rum", "tobst", "du", "so", "un\u00b7ge\u00b7lind", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "KON", "VVFIN", "$.", "PWAV", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Sie sprach: Wie sollt' ich nicht, wenn ihre Lust die Affen", "tokens": ["Sie", "sprach", ":", "Wie", "sollt'", "ich", "nicht", ",", "wenn", "ih\u00b7re", "Lust", "die", "Af\u00b7fen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "PWAV", "VMFIN", "PPER", "PTKNEG", "$,", "KOUS", "PPOSAT", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Behalten, und ich mir die meine sah entraffen?", "tokens": ["Be\u00b7hal\u00b7ten", ",", "und", "ich", "mir", "die", "mei\u00b7ne", "sah", "en\u00b7traf\u00b7fen", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "PPER", "PPER", "ART", "PPOSAT", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Die Aeffin sprach: M\u00f6g' ich stets meine Freude sehn!", "tokens": ["Die", "A\u00b7ef\u00b7fin", "sprach", ":", "M\u00f6g'", "ich", "stets", "mei\u00b7ne", "Freu\u00b7de", "sehn", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "VMFIN", "PPER", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Dir aber ist villeicht verdientes Leid geschehn.", "tokens": ["Dir", "a\u00b7ber", "ist", "vil\u00b7leicht", "ver\u00b7dien\u00b7tes", "Leid", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ADV", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Sprich: wovon n\u00e4hrst du dich? von Fr\u00fcchten wol und Laube? \u2014", "tokens": ["Sprich", ":", "wo\u00b7von", "n\u00e4hrst", "du", "dich", "?", "von", "Fr\u00fcch\u00b7ten", "wol", "und", "Lau\u00b7be", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVIMP", "$.", "PWAV", "VVFIN", "PPER", "PRF", "$.", "APPR", "NN", "ADV", "KON", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201enein! meinem Stamm und Stand gem\u00e4\u00df, von blut'gem Raube.\u201c \u2014", "tokens": ["\u201e", "nein", "!", "mei\u00b7nem", "Stamm", "und", "Stand", "ge\u00b7m\u00e4\u00df", ",", "von", "blut'\u00b7gem", "Rau\u00b7be", ".", "\u201c"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "PTKANT", "$.", "PPOSAT", "NN", "KON", "NN", "ADJD", "$,", "APPR", "ADJA", "NN", "$.", "$(", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Und fragtest du erst, die du fra\u00dfest, ob sie Gatten,", "tokens": ["Und", "frag\u00b7test", "du", "erst", ",", "die", "du", "fra\u00b7\u00dfest", ",", "ob", "sie", "Gat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,", "PRELS", "PPER", "VVFIN", "$,", "KOUS", "PPER", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ob Eltern sie daheim, oder ob Kinder hatten? \u2014", "tokens": ["Ob", "El\u00b7tern", "sie", "da\u00b7heim", ",", "o\u00b7der", "ob", "Kin\u00b7der", "hat\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "NN", "PPER", "ADV", "$,", "KON", "KOUS", "NN", "VAFIN", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Sie sprach: Nein, Alt und Jung fra\u00df ich ohn' Unterscheid;", "tokens": ["Sie", "sprach", ":", "Nein", ",", "Alt", "und", "Jung", "fra\u00df", "ich", "ohn'", "Un\u00b7ter\u00b7scheid", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PTKANT", "$,", "ADJD", "KON", "NN", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch das that ich, wem that die Unschuld was zu Leid?", "tokens": ["Doch", "das", "that", "ich", ",", "wem", "that", "die", "Un\u00b7schuld", "was", "zu", "Leid", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "PPER", "$,", "PWS", "VVFIN", "ART", "NN", "PRELS", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.12": {"line.1": {"text": "Die Aeffin sprach: Zu Leid wird sie auch nie was thun;", "tokens": ["Die", "A\u00b7ef\u00b7fin", "sprach", ":", "Zu", "Leid", "wird", "sie", "auch", "nie", "was", "thun", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "APPR", "NN", "VAFIN", "PPER", "ADV", "ADV", "PIS", "VVINF", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Der Kinder Unschuld b\u00fc\u00dft die Schuld der Mutter nun.", "tokens": ["Der", "Kin\u00b7der", "Un\u00b7schuld", "b\u00fc\u00dft", "die", "Schuld", "der", "Mut\u00b7ter", "nun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ART", "NN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.13": {"line.1": {"text": "Doch ists ein Widerspruch, unschuld'ge L\u00f6wenbrut;", "tokens": ["Doch", "ists", "ein", "Wi\u00b7der\u00b7spruch", ",", "un\u00b7schuld'\u00b7ge", "L\u00f6\u00b7wen\u00b7brut", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die Milch, die sie an dir getrunken, war schon Blut.", "tokens": ["Die", "Milch", ",", "die", "sie", "an", "dir", "ge\u00b7trun\u00b7ken", ",", "war", "schon", "Blut", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "APPR", "PPER", "VVPP", "$,", "VAFIN", "ADV", "NN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}}}}