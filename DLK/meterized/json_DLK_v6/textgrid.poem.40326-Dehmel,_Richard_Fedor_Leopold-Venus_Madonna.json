{"textgrid.poem.40326": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Venus Madonna", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus Mannesadel w\u00e4chst des Weibes Tugend:", "tokens": ["Aus", "Man\u00b7ne\u00b7sa\u00b7del", "w\u00e4chst", "des", "Wei\u00b7bes", "Tu\u00b7gend", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "G\u00f6tter vermag sein Geist ihr zu geb\u00e4ren.", "tokens": ["G\u00f6t\u00b7ter", "ver\u00b7mag", "sein", "Geist", "ihr", "zu", "ge\u00b7b\u00e4\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Des Griechen Sch\u00f6nheitswille sah die Sph\u00e4ren", "tokens": ["Des", "Grie\u00b7chen", "Sch\u00f6n\u00b7heits\u00b7wil\u00b7le", "sah", "die", "Sph\u00e4\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "beherrscht von Aphroditens Reiz und Jugend;", "tokens": ["be\u00b7herrscht", "von", "A\u00b7phro\u00b7di\u00b7tens", "Reiz", "und", "Ju\u00b7gend", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NE", "NE", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "dem Christen aber ward die Reinheit Wesen,", "tokens": ["dem", "Chris\u00b7ten", "a\u00b7ber", "ward", "die", "Rein\u00b7heit", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "selbst noch die Mutter will er sich verkl\u00e4ren", "tokens": ["selbst", "noch", "die", "Mut\u00b7ter", "will", "er", "sich", "ver\u00b7kl\u00e4\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN", "VMFIN", "PPER", "PRF", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "und beugt sich vor Marias Hochalt\u00e4ren,", "tokens": ["und", "beugt", "sich", "vor", "Ma\u00b7rias", "Hoch\u00b7al\u00b7t\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "NE", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "die keusch des Sohns, des keuscheren, genesen.", "tokens": ["die", "keusch", "des", "Sohns", ",", "des", "keu\u00b7sche\u00b7ren", ",", "ge\u00b7ne\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJD", "ART", "NN", "$,", "ART", "ADJA", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nun kommt die Zeit, da\u00df M\u00e4nner freier denken", "tokens": ["Nun", "kommt", "die", "Zeit", ",", "da\u00df", "M\u00e4n\u00b7ner", "frei\u00b7er", "den\u00b7ken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "KOUS", "NN", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "und ihren eignen Stamm von Gottess\u00f6hnen", "tokens": ["und", "ih\u00b7ren", "eig\u00b7nen", "Stamm", "von", "Got\u00b7tes\u00b7s\u00f6h\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "hell mit dem Huldbild ihrer Freiheit kr\u00f6nen,", "tokens": ["hell", "mit", "dem", "Huld\u00b7bild", "ih\u00b7rer", "Frei\u00b7heit", "kr\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "bis Alle Allen die Erleuchtung schenken,", "tokens": ["bis", "Al\u00b7le", "Al\u00b7len", "die", "Er\u00b7leuch\u00b7tung", "schen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "die Wir uns schenkten, Sonne meiner Wonne,", "tokens": ["die", "Wir", "uns", "schenk\u00b7ten", ",", "Son\u00b7ne", "mei\u00b7ner", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "VVFIN", "$,", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "du keusche Venus, reizende Madonne!", "tokens": ["du", "keu\u00b7sche", "Ve\u00b7nus", ",", "rei\u00b7zen\u00b7de", "Ma\u00b7don\u00b7ne", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": ". . . . . . . . . . . . . . . . . . . . . . . . . . . . . . .", "tokens": [".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", "."], "token_info": ["punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct"], "pos": ["$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$."]}}, "stanza.5": {"line.1": {"text": "Doch da sa\u00df mit seligem H\u00e4ndefalten,", "tokens": ["Doch", "da", "sa\u00df", "mit", "se\u00b7li\u00b7gem", "H\u00e4n\u00b7de\u00b7fal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "sa\u00df mit einem L\u00e4cheln stillen Wehrens,", "tokens": ["sa\u00df", "mit", "ei\u00b7nem", "L\u00e4\u00b7cheln", "stil\u00b7len", "Weh\u00b7rens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "wie befremdet von den Traumgestalten", "tokens": ["wie", "be\u00b7frem\u00b7det", "von", "den", "Traum\u00b7ge\u00b7stal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVPP", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "meines \u00fcbersinnlichen Begehrens,", "tokens": ["mei\u00b7nes", "\u00fc\u00b7ber\u00b7sinn\u00b7li\u00b7chen", "Be\u00b7geh\u00b7rens", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "sa\u00df als Eine, die Gott siegen sieht,", "tokens": ["sa\u00df", "als", "Ei\u00b7ne", ",", "die", "Gott", "sie\u00b7gen", "sieht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "ART", "$,", "ART", "NN", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "wie er siegte schon zu Evas Zeit,", "tokens": ["wie", "er", "sieg\u00b7te", "schon", "zu", "E\u00b7vas", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "ADV", "APPR", "NE", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "sa\u00df und sang ein frommes Wiegenlied,", "tokens": ["sa\u00df", "und", "sang", "ein", "from\u00b7mes", "Wie\u00b7gen\u00b7lied", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "ganz erf\u00fcllt vom Gl\u00fcck der Wirklichkeit,", "tokens": ["ganz", "er\u00b7f\u00fcllt", "vom", "Gl\u00fcck", "der", "Wirk\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}