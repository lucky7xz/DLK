{"textgrid.poem.38979": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Abreise", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Endlich ist der Tag gekommen,", "tokens": ["End\u00b7lich", "ist", "der", "Tag", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Endlich ist die Stunde da,", "tokens": ["End\u00b7lich", "ist", "die", "Stun\u00b7de", "da", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die ich stets unm\u00f6glich glaubte,", "tokens": ["Die", "ich", "stets", "un\u00b7m\u00f6g\u00b7lich", "glaub\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weil der Schmerz die Kraft genommen,", "tokens": ["Weil", "der", "Schmerz", "die", "Kraft", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Weil der Wahn den Entschlu\u00df raubte,", "tokens": ["Weil", "der", "Wahn", "den", "Ent\u00b7schlu\u00df", "raub\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Da ich nur mein Leiden sah.", "tokens": ["Da", "ich", "nur", "mein", "Lei\u00b7den", "sah", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.2": {"line.1": {"text": "Welcher heitre Sommertag!", "tokens": ["Wel\u00b7cher", "heit\u00b7re", "Som\u00b7mer\u00b7tag", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Diese H\u00e4user, diese Gassen,", "tokens": ["Die\u00b7se", "H\u00e4u\u00b7ser", ",", "die\u00b7se", "Gas\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die ich nun seit vielen Wochen", "tokens": ["Die", "ich", "nun", "seit", "vie\u00b7len", "Wo\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "T\u00e4glich sah mit Zorn und Hassen,", "tokens": ["T\u00e4g\u00b7lich", "sah", "mit", "Zorn", "und", "Has\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sollen mir entschwinden,", "tokens": ["Sol\u00b7len", "mir", "ent\u00b7schwin\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Und mein Blick die sonnbegl\u00e4nzten Fluren finden.", "tokens": ["Und", "mein", "Blick", "die", "sonn\u00b7be\u00b7gl\u00e4nz\u00b7ten", "Flu\u00b7ren", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.7": {"text": "Einmal noch betracht' ich mir die alten", "tokens": ["Ein\u00b7mal", "noch", "be\u00b7tracht'", "ich", "mir", "die", "al\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PPER", "ART", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "H\u00e4user dort, bemerke die Gestalten", "tokens": ["H\u00e4u\u00b7ser", "dort", ",", "be\u00b7mer\u00b7ke", "die", "Ge\u00b7stal\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ADV", "$,", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "An den Fenstern dr\u00fcben; wie ein Vorhang", "tokens": ["An", "den", "Fens\u00b7tern", "dr\u00fc\u00b7ben", ";", "wie", "ein", "Vor\u00b7hang"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADV", "$.", "PWAV", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "F\u00e4llt es zu, der liebste Freund", "tokens": ["F\u00e4llt", "es", "zu", ",", "der", "liebs\u00b7te", "Freund"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Sitzt schon neben mir im Wagen,", "tokens": ["Sitzt", "schon", "ne\u00b7ben", "mir", "im", "Wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "Abschiedsworte, \u2013 und es jagen", "tokens": ["Ab\u00b7schieds\u00b7wor\u00b7te", ",", "\u2013", "und", "es", "ja\u00b7gen"], "token_info": ["word", "punct", "punct", "word", "word", "word"], "pos": ["NN", "$,", "$(", "KON", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "H\u00e4user, Gassen, Thore, schwindelnd mir vor\u00fcber.", "tokens": ["H\u00e4u\u00b7ser", ",", "Gas\u00b7sen", ",", "Tho\u00b7re", ",", "schwin\u00b7delnd", "mir", "vor\u00b7\u00fc\u00b7ber", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "ADJD", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.14": {"text": "Welch Entz\u00fccken! welche Wehmuth!", "tokens": ["Welch", "Ent\u00b7z\u00fc\u00b7cken", "!", "wel\u00b7che", "Weh\u00b7muth", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$.", "PWAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Bin ich's noch, der wie an Ketten", "tokens": ["Bin", "ich's", "noch", ",", "der", "wie", "an", "Ket\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PIS", "ADV", "$,", "PRELS", "KOKOM", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Dort in tr\u00fcben Mauern sa\u00df?", "tokens": ["Dort", "in", "tr\u00fc\u00b7ben", "Mau\u00b7ern", "sa\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Ja, der Schmerz ist mir gefolgt", "tokens": ["Ja", ",", "der", "Schmerz", "ist", "mir", "ge\u00b7folgt"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "ART", "NN", "VAFIN", "PPER", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "Und spannt \u00fcber Feld und Wald", "tokens": ["Und", "spannt", "\u00fc\u00b7ber", "Feld", "und", "Wald"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.19": {"text": "Einen schwarzen Schleyer aus.", "tokens": ["Ei\u00b7nen", "schwar\u00b7zen", "Schle\u00b7yer", "aus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}