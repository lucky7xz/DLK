{"textgrid.poem.56394": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "15", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da hub Heimdall an,", "tokens": ["Da", "hub", "Heim\u00b7dall", "an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Der helleuchtende Gott,", "tokens": ["Der", "hel\u00b7leuch\u00b7ten\u00b7de", "Gott", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-++--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Welcher da weise", "tokens": ["Wel\u00b7cher", "da", "wei\u00b7se"], "token_info": ["word", "word", "word"], "pos": ["PWAT", "ADV", "VVFIN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Wu\u00dfte die Zukunft:", "tokens": ["Wu\u00df\u00b7te", "die", "Zu\u00b7kunft", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "\u00bbbr\u00e4utliches Leinen", "tokens": ["\u00bb", "br\u00e4ut\u00b7li\u00b7ches", "Lei\u00b7nen"], "token_info": ["punct", "word", "word"], "pos": ["$(", "ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Legen dem Thor wir an;", "tokens": ["Le\u00b7gen", "dem", "Thor", "wir", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Er habe den hehren,", "tokens": ["Er", "ha\u00b7be", "den", "heh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Den funkelnden Halsschmuck;\u00ab", "tokens": ["Den", "fun\u00b7keln\u00b7den", "Hals\u00b7schmuck", ";", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}}}}