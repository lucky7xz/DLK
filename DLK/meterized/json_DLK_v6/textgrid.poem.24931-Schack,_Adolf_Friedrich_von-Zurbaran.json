{"textgrid.poem.24931": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Zurbaran", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Meister legt den Pinsel aus der Hand;", "tokens": ["Der", "Meis\u00b7ter", "legt", "den", "Pin\u00b7sel", "aus", "der", "Hand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Noch ist sein liebstes Bild nicht ganz vollendet,", "tokens": ["Noch", "ist", "sein", "liebs\u00b7tes", "Bild", "nicht", "ganz", "voll\u00b7en\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "PTKNEG", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Auferstehung, der er unverwandt", "tokens": ["Die", "Auf\u00b7er\u00b7ste\u00b7hung", ",", "der", "er", "un\u00b7ver\u00b7wandt"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Seit Jahren seine ganze Kraft gespendet.", "tokens": ["Seit", "Jah\u00b7ren", "sei\u00b7ne", "gan\u00b7ze", "Kraft", "ge\u00b7spen\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Da sinkt er todesmatt zur\u00fcck und spricht:", "tokens": ["Da", "sinkt", "er", "to\u00b7des\u00b7matt", "zu\u00b7r\u00fcck", "und", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PTKVZ", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "\u00bbo Herr, du rufst; nicht beb' ich vor dem Grabe;", "tokens": ["\u00bb", "o", "Herr", ",", "du", "rufst", ";", "nicht", "beb'", "ich", "vor", "dem", "Gra\u00b7be", ";"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "NN", "$,", "PPER", "VVFIN", "$.", "PTKNEG", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Doch willst du gn\u00e4dig sein, so nimm mich nicht", "tokens": ["Doch", "willst", "du", "gn\u00e4\u00b7dig", "sein", ",", "so", "nimm", "mich", "nicht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "ADJD", "VAINF", "$,", "ADV", "VVIMP", "PPER", "PTKNEG"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Hinweg, bis ich dies Bild vollendet habe!\u00ab", "tokens": ["Hin\u00b7weg", ",", "bis", "ich", "dies", "Bild", "voll\u00b7en\u00b7det", "ha\u00b7be", "!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "PDS", "NN", "VVPP", "VAFIN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Umsonst \u2013 die Lippen regen sich nicht mehr;", "tokens": ["Um\u00b7sonst", "\u2013", "die", "Lip\u00b7pen", "re\u00b7gen", "sich", "nicht", "mehr", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "ART", "NN", "ADJA", "PRF", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit einem Seufzer ist sein Geist geschieden;", "tokens": ["Mit", "ei\u00b7nem", "Seuf\u00b7zer", "ist", "sein", "Geist", "ge\u00b7schie\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Still weinend stehn die M\u00f6nche um ihn her,", "tokens": ["Still", "wei\u00b7nend", "stehn", "die", "M\u00f6n\u00b7che", "um", "ihn", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "VVFIN", "ART", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und trauernd spricht der Prior: \u00bbRuh in Frieden!\u00ab", "tokens": ["Und", "trau\u00b7ernd", "spricht", "der", "Pri\u00b7or", ":", "\u00bb", "Ruh", "in", "Frie\u00b7den", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$.", "$(", "NN", "APPR", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Bald wird die Leiche in die Gruft hinab", "tokens": ["Bald", "wird", "die", "Lei\u00b7che", "in", "die", "Gruft", "hin\u00b7ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Bei Fackelschein und Psalmgesang getragen,", "tokens": ["Bei", "Fa\u00b7ckel\u00b7schein", "und", "Psalm\u00b7ge\u00b7sang", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und in der Klosterkirche ragt ein Grab,", "tokens": ["Und", "in", "der", "Klos\u00b7ter\u00b7kir\u00b7che", "ragt", "ein", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Ein neues, bei den alten Sarkophagen.", "tokens": ["Ein", "neu\u00b7es", ",", "bei", "den", "al\u00b7ten", "Sar\u00b7ko\u00b7pha\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Es kommt die Nacht; die Sakristane knien", "tokens": ["Es", "kommt", "die", "Nacht", ";", "die", "Sak\u00b7ris\u00b7ta\u00b7ne", "kni\u00b7en"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Noch betend um die Gruft; im Tempelrunde", "tokens": ["Noch", "be\u00b7tend", "um", "die", "Gruft", ";", "im", "Tem\u00b7pel\u00b7run\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVPP", "APPR", "ART", "NN", "$.", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Verwehn der Sterbemette Melodien,", "tokens": ["Ver\u00b7wehn", "der", "Ster\u00b7be\u00b7met\u00b7te", "Me\u00b7lo\u00b7dien", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und von dem Turme hallt die zw\u00f6lfte Stunde.", "tokens": ["Und", "von", "dem", "Tur\u00b7me", "hallt", "die", "zw\u00f6lf\u00b7te", "Stun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Da horch! Im Grabe drunten wird es laut;", "tokens": ["Da", "horch", "!", "Im", "Gra\u00b7be", "drun\u00b7ten", "wird", "es", "laut", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$.", "APPRART", "NN", "ADV", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Aus ihren Fugen springt die Marmorplatte,", "tokens": ["Aus", "ih\u00b7ren", "Fu\u00b7gen", "springt", "die", "Mar\u00b7mor\u00b7plat\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und durch das Dunkel, das den Platz umgraut,", "tokens": ["Und", "durch", "das", "Dun\u00b7kel", ",", "das", "den", "Platz", "um\u00b7graut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Steigt feierlichen Schritts ein ernster Schatte.", "tokens": ["Steigt", "fei\u00b7er\u00b7li\u00b7chen", "Schritts", "ein", "erns\u00b7ter", "Schat\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Der Maler ist es, wie er lebend war;", "tokens": ["Der", "Ma\u00b7ler", "ist", "es", ",", "wie", "er", "le\u00b7bend", "war", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "$,", "PWAV", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit schwachem Schimmer nur auf seinen Z\u00fcgen", "tokens": ["Mit", "schwa\u00b7chem", "Schim\u00b7mer", "nur", "auf", "sei\u00b7nen", "Z\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Scheint, bleiche Streifen werfend, wunderbar", "tokens": ["Scheint", ",", "blei\u00b7che", "Strei\u00b7fen", "wer\u00b7fend", ",", "wun\u00b7der\u00b7bar"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "$,", "ADJA", "NN", "VVPP", "$,", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Morgenglanz der Ewigkeit zu liegen;", "tokens": ["Der", "Mor\u00b7gen\u00b7glanz", "der", "E\u00b7wig\u00b7keit", "zu", "lie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die M\u00f6nche weichen scheu im Bogengang", "tokens": ["Die", "M\u00f6n\u00b7che", "wei\u00b7chen", "scheu", "im", "Bo\u00b7gen\u00b7gang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und sehn ihn schwebend durch die Kirchenhallen,", "tokens": ["Und", "sehn", "ihn", "schwe\u00b7bend", "durch", "die", "Kir\u00b7chen\u00b7hal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Den Kreuzgang und die Chorstuhlreihn entlang,", "tokens": ["Den", "Kreuz\u00b7gang", "und", "die", "Chor\u00b7stuhl\u00b7reihn", "ent\u00b7lang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Zu der Kapelle, die er malte, wallen.", "tokens": ["Zu", "der", "Ka\u00b7pel\u00b7le", ",", "die", "er", "mal\u00b7te", ",", "wal\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Er tritt hinein und sieht den heil'gen Raum", "tokens": ["Er", "tritt", "hin\u00b7ein", "und", "sieht", "den", "heil'\u00b7gen", "Raum"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Vom Glanz der ew'gen Lampe matt umflimmert;", "tokens": ["Vom", "Glanz", "der", "ew'\u00b7gen", "Lam\u00b7pe", "matt", "um\u00b7flim\u00b7mert", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Er nimmt sein Malger\u00e4t, und, wenn auch kaum", "tokens": ["Er", "nimmt", "sein", "Mal\u00b7ge\u00b7r\u00e4t", ",", "und", ",", "wenn", "auch", "kaum"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "KON", "$,", "KOUS", "ADV", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein blasser Schein am Hochaltare schimmert,", "tokens": ["Ein", "blas\u00b7ser", "Schein", "am", "Hoc\u00b7hal\u00b7ta\u00b7re", "schim\u00b7mert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ihn st\u00f6rt die Finsternis der Erde nicht;", "tokens": ["Ihn", "st\u00f6rt", "die", "Fins\u00b7ter\u00b7nis", "der", "Er\u00b7de", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Er f\u00fchrt als Meister Pinsel und Palette,", "tokens": ["Er", "f\u00fchrt", "als", "Meis\u00b7ter", "Pin\u00b7sel", "und", "Pa\u00b7let\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Bis morgens ihn das erste Sonnenlicht", "tokens": ["Bis", "mor\u00b7gens", "ihn", "das", "ers\u00b7te", "Son\u00b7nen\u00b7licht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Aufs neue scheucht in seine Ruhest\u00e4tte.", "tokens": ["Aufs", "neu\u00b7e", "scheucht", "in", "sei\u00b7ne", "Ru\u00b7he\u00b7st\u00e4t\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "So steigt er aus dem Grabe Nacht f\u00fcr Nacht,", "tokens": ["So", "steigt", "er", "aus", "dem", "Gra\u00b7be", "Nacht", "f\u00fcr", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das Werk, an dem sein Leben hing, zu malen;", "tokens": ["Das", "Werk", ",", "an", "dem", "sein", "Le\u00b7ben", "hing", ",", "zu", "ma\u00b7len", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ART", "PPOSAT", "NN", "VVFIN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und als er nun den letzten Zug vollbracht,", "tokens": ["Und", "als", "er", "nun", "den", "letz\u00b7ten", "Zug", "voll\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da leuchtet glorreich in des Fr\u00fchlichts Strahlen", "tokens": ["Da", "leuch\u00b7tet", "glor\u00b7reich", "in", "des", "Fr\u00fch\u00b7lichts", "Strah\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Das Bild ihn an, er sieht im Morgenrot", "tokens": ["Das", "Bild", "ihn", "an", ",", "er", "sieht", "im", "Mor\u00b7gen\u00b7rot"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "PTKVZ", "$,", "PPER", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Den Gottessohn sich aus der Gruft erheben", "tokens": ["Den", "Got\u00b7tes\u00b7sohn", "sich", "aus", "der", "Gruft", "er\u00b7he\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PRF", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und spricht verkl\u00e4rt: \u00bbNun sei willkommen, Tod!", "tokens": ["Und", "spricht", "ver\u00b7kl\u00e4rt", ":", "\u00bb", "Nun", "sei", "will\u00b7kom\u00b7men", ",", "Tod", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "VVPP", "$.", "$(", "ADV", "VAFIN", "ADJD", "$,", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Nun kann ich frei zu andern R\u00e4umen schweben!\u00ab", "tokens": ["Nun", "kann", "ich", "frei", "zu", "an\u00b7dern", "R\u00e4u\u00b7men", "schwe\u00b7ben", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "APPR", "ADJA", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}