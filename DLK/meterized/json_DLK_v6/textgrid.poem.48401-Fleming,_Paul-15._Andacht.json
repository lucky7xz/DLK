{"textgrid.poem.48401": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "15. Andacht", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich lebe, doch nicht ich; derselbe lebt in mir,", "tokens": ["Ich", "le\u00b7be", ",", "doch", "nicht", "ich", ";", "der\u00b7sel\u00b7be", "lebt", "in", "mir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADV", "PTKNEG", "PPER", "$.", "PDAT", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "der mir durch seinen Tod das Leben bringt herf\u00fcr.", "tokens": ["der", "mir", "durch", "sei\u00b7nen", "Tod", "das", "Le\u00b7ben", "bringt", "her\u00b7f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPOSAT", "NN", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mein Leben war sein Tod, sein Tod war mir mein Leben,", "tokens": ["Mein", "Le\u00b7ben", "war", "sein", "Tod", ",", "sein", "Tod", "war", "mir", "mein", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "VAFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "nur geh' ich wieder ihm, was er mir hat gegeben.", "tokens": ["nur", "geh'", "ich", "wie\u00b7der", "ihm", ",", "was", "er", "mir", "hat", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PPER", "$,", "PWS", "PPER", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Er lebt durch meinen Tod, mir sterb' ich t\u00e4glich ab.", "tokens": ["Er", "lebt", "durch", "mei\u00b7nen", "Tod", ",", "mir", "sterb'", "ich", "t\u00e4g\u00b7lich", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der Leib, mein irdnes Teil, der ist der Seelen Grab,", "tokens": ["Der", "Leib", ",", "mein", "ird\u00b7nes", "Teil", ",", "der", "ist", "der", "See\u00b7len", "Grab", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,", "PRELS", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "er lebt nur auf den Schein. Wer ewig nicht wil sterben,", "tokens": ["er", "lebt", "nur", "auf", "den", "Schein", ".", "Wer", "e\u00b7wig", "nicht", "wil", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "$.", "PWS", "ADJD", "PTKNEG", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "der mu\u00df hier in der Zeit verwesen und verderben,", "tokens": ["der", "mu\u00df", "hier", "in", "der", "Zeit", "ver\u00b7we\u00b7sen", "und", "ver\u00b7der\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "APPR", "ART", "NN", "VVPP", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "weil er noch sterben kan. Der Tod, der geistlich hei\u00dft,", "tokens": ["weil", "er", "noch", "ster\u00b7ben", "kan", ".", "Der", "Tod", ",", "der", "geist\u00b7lich", "hei\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVINF", "VMFIN", "$.", "ART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "der ist alsdann zu spat, wann uns sein Freund hinrei\u00dft,", "tokens": ["der", "ist", "als\u00b7dann", "zu", "spat", ",", "wann", "uns", "sein", "Freund", "hin\u00b7rei\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VAFIN", "ADV", "PTKZU", "VVFIN", "$,", "PWAV", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "der unsern Leib bringt um. Herr, gieb mir die Genade,", "tokens": ["der", "un\u00b7sern", "Leib", "bringt", "um", ".", "Herr", ",", "gieb", "mir", "die", "Ge\u00b7na\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$.", "NN", "$,", "VVIMP", "PPER", "ART", "NN", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "da\u00df dieses Leibes Brauch nicht meiner Seelen schade.", "tokens": ["da\u00df", "die\u00b7ses", "Lei\u00b7bes", "Brauch", "nicht", "mei\u00b7ner", "See\u00b7len", "scha\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "NN", "PTKNEG", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Mein Alles und mein Nichts, mein Leben, meinen Tod,", "tokens": ["Mein", "Al\u00b7les", "und", "mein", "Nichts", ",", "mein", "Le\u00b7ben", ",", "mei\u00b7nen", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "PIS", "KON", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "das hab' ich bei mir selbst. Hilfst du, so hats nicht Not.", "tokens": ["das", "hab'", "ich", "bei", "mir", "selbst", ".", "Hilfst", "du", ",", "so", "hats", "nicht", "Not", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "APPR", "PPER", "ADV", "$.", "VVFIN", "PPER", "$,", "ADV", "VAFIN", "PTKNEG", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ich wil, ich mag, ich sol, ich kan mir selbst nicht raten,", "tokens": ["Ich", "wil", ",", "ich", "mag", ",", "ich", "sol", ",", "ich", "kan", "mir", "selbst", "nicht", "ra\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PPER", "VMFIN", "$,", "PPER", "VMFIN", "$,", "PPER", "VMFIN", "PPER", "ADV", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "dich wil ichs lassen tun, du hast bei dir die Taten.", "tokens": ["dich", "wil", "ichs", "las\u00b7sen", "tun", ",", "du", "hast", "bei", "dir", "die", "Ta\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIS", "VVINF", "VVINF", "$,", "PPER", "VAFIN", "APPR", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Die W\u00fcndsche tu ich nur, ich lasse mich ganz dir.", "tokens": ["Die", "W\u00fcnd\u00b7sche", "tu", "ich", "nur", ",", "ich", "las\u00b7se", "mich", "ganz", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$,", "PPER", "VVFIN", "PPER", "ADV", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Ich wil nicht meine sein. Nim mich nur, gieb dich mir!", "tokens": ["Ich", "wil", "nicht", "mei\u00b7ne", "sein", ".", "Nim", "mich", "nur", ",", "gieb", "dich", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "PPOSAT", "VAINF", "$.", "NE", "PPER", "ADV", "$,", "VVIMP", "PPER", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}