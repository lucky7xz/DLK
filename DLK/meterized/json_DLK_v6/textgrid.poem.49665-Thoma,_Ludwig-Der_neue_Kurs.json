{"textgrid.poem.49665": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Der neue Kurs", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fr\u00fcher war das so in Bayern:", "tokens": ["Fr\u00fc\u00b7her", "war", "das", "so", "in", "Bay\u00b7ern", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PDS", "ADV", "APPR", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jubil\u00e4en, gro\u00dfe Feiern", "tokens": ["Ju\u00b7bi\u00b7l\u00e4\u00b7en", ",", "gro\u00b7\u00dfe", "Fei\u00b7ern"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Waren nicht beliebt bei Hof,", "tokens": ["Wa\u00b7ren", "nicht", "be\u00b7liebt", "bei", "Hof", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ADJD", "APPR", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dahingegen eine Birsche", "tokens": ["Da\u00b7hin\u00b7ge\u00b7gen", "ei\u00b7ne", "Bir\u00b7sche"], "token_info": ["word", "word", "word"], "pos": ["PAV", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Auf die Gamsb\u00f6ck und die Hirsche", "tokens": ["Auf", "die", "Gams\u00b7b\u00f6ck", "und", "die", "Hir\u00b7sche"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "KON", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Galt viel mehr als wie ein Schwof.", "tokens": ["Galt", "viel", "mehr", "als", "wie", "ein", "Schwof", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "KOUS", "KOKOM", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Aber in dem letzten Jahre", "tokens": ["A\u00b7ber", "in", "dem", "letz\u00b7ten", "Jah\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist ein Hin- und Hergefahre", "tokens": ["Ist", "ein", "Hin", "und", "Her\u00b7ge\u00b7fah\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "TRUNC", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf der schnellen Eisenbahn;", "tokens": ["Auf", "der", "schnel\u00b7len", "Ei\u00b7sen\u00b7bahn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Jedes Mitglied von Vereinen", "tokens": ["Je\u00b7des", "Mit\u00b7glied", "von", "Ver\u00b7ei\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sieht jetzt oft und reichlich seinen", "tokens": ["Sieht", "jetzt", "oft", "und", "reich\u00b7lich", "sei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV", "KON", "ADJD", "PPOSAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "F\u00fcrsten in der N\u00e4he an.", "tokens": ["F\u00fcrs\u00b7ten", "in", "der", "N\u00e4\u00b7he", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Die Beamtenk\u00f6rper huld'gen;", "tokens": ["Die", "Be\u00b7am\u00b7ten\u00b7k\u00f6r\u00b7per", "huld'\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jeder Mensch bringt seinen schuld'gen", "tokens": ["Je\u00b7der", "Mensch", "bringt", "sei\u00b7nen", "schuld'\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "PPOSAT", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Vollen Akt der Treue dar;", "tokens": ["Vol\u00b7len", "Akt", "der", "Treu\u00b7e", "dar", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Untertanenherzen pochen;", "tokens": ["Un\u00b7ter\u00b7ta\u00b7nen\u00b7her\u00b7zen", "po\u00b7chen", ";"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Viele werden angesprochen", "tokens": ["Vie\u00b7le", "wer\u00b7den", "an\u00b7ge\u00b7spro\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["PIS", "VAFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Aus der braven B\u00fcrgerschar.", "tokens": ["Aus", "der", "bra\u00b7ven", "B\u00fcr\u00b7ger\u00b7schar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Meier, Huber, Huber, Meier", "tokens": ["Mei\u00b7er", ",", "Hu\u00b7ber", ",", "Hu\u00b7ber", ",", "Mei\u00b7er"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["NE", "$,", "NE", "$,", "NE", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Werden sichtlich k\u00f6nigstreuer", "tokens": ["Wer\u00b7den", "sicht\u00b7lich", "k\u00f6\u00b7nigs\u00b7treu\u00b7er"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "PRF", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und f\u00fcr ihren Thron entflammt.", "tokens": ["Und", "f\u00fcr", "ih\u00b7ren", "Thron", "ent\u00b7flammt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und trotz aller Widersacher", "tokens": ["Und", "trotz", "al\u00b7ler", "Wi\u00b7der\u00b7sa\u00b7cher"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wird das Haus der Wittelsbacher", "tokens": ["Wird", "das", "Haus", "der", "Wit\u00b7tels\u00b7ba\u00b7cher"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Intensiver angestammt.", "tokens": ["In\u00b7ten\u00b7si\u00b7ver", "an\u00b7ge\u00b7stammt", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}