{"textgrid.poem.47310": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00df mein Streben dir gefallen", "tokens": ["La\u00df", "mein", "Stre\u00b7ben", "dir", "ge\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPOSAT", "NN", "PPER", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und mich strebend weiter wallen!", "tokens": ["Und", "mich", "stre\u00b7bend", "wei\u00b7ter", "wal\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJD", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00df mich stehn durch deine Huld, wo", "tokens": ["La\u00df", "mich", "stehn", "durch", "dei\u00b7ne", "Huld", ",", "wo"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["VVIMP", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,", "PWAV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ich durch meine Schuld gefallen.", "tokens": ["Ich", "durch", "mei\u00b7ne", "Schuld", "ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Von des Berges Gipfel gl\u00e4nzen", "tokens": ["Von", "des", "Ber\u00b7ges", "Gip\u00b7fel", "gl\u00e4n\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mir entgegen deine Hallen;", "tokens": ["Mir", "ent\u00b7ge\u00b7gen", "dei\u00b7ne", "Hal\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und die heil'gen Chorges\u00e4nge", "tokens": ["Und", "die", "heil'\u00b7gen", "Chor\u00b7ge\u00b7s\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "H\u00f6r' ich mir entgegenschallen.", "tokens": ["H\u00f6r'", "ich", "mir", "ent\u00b7ge\u00b7gen\u00b7schal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "La\u00df den Glanz und la\u00df den Klang nicht,", "tokens": ["La\u00df", "den", "Glanz", "und", "la\u00df", "den", "Klang", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "KON", "VVIMP", "ART", "NN", "PTKNEG", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Eh' ich nah', in Duft zerwallen;", "tokens": ["Eh'", "ich", "nah'", ",", "in", "Duft", "zer\u00b7wal\u00b7len", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKVZ", "$,", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "H\u00fcben ich, du dr\u00fcben! la\u00df mich", "tokens": ["H\u00fc\u00b7ben", "ich", ",", "du", "dr\u00fc\u00b7ben", "!", "la\u00df", "mich"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "PPER", "$,", "PPER", "ADV", "$.", "VVIMP", "PPER"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.12": {"text": "Von der Kluft zur\u00fcck nicht prallen.", "tokens": ["Von", "der", "Kluft", "zu\u00b7r\u00fcck", "nicht", "pral\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Zeige, die mich dr\u00fcber trage,", "tokens": ["Zei\u00b7ge", ",", "die", "mich", "dr\u00fc\u00b7ber", "tra\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PRF", "PAV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Mir die Br\u00fccke von Kristallen!", "tokens": ["Mir", "die", "Br\u00fc\u00b7cke", "von", "Kris\u00b7tal\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "APPR", "NN", "$."], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.15": {"text": "Und dem Abgrundsungeheuer,", "tokens": ["Und", "dem", "Ab\u00b7grund\u00b7sun\u00b7ge\u00b7heu\u00b7er", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Schwindel, seien stumpf die Krallen.", "tokens": ["Schwin\u00b7del", ",", "sei\u00b7en", "stumpf", "die", "Kral\u00b7len", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "ADJD", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Meiner Pilgerreise Schritte", "tokens": ["Mei\u00b7ner", "Pil\u00b7ger\u00b7rei\u00b7se", "Schrit\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Z\u00e4hl' ich ab an Betkorallen;", "tokens": ["Z\u00e4hl'", "ich", "ab", "an", "Bet\u00b7ko\u00b7ral\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKVZ", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Wie den Rosenkranz der Himmel", "tokens": ["Wie", "den", "Ro\u00b7sen\u00b7kranz", "der", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Betet ab an Sonnenballen.", "tokens": ["Be\u00b7tet", "ab", "an", "Son\u00b7nen\u00b7bal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Manches hab' ich nicht verstanden,", "tokens": ["Man\u00b7ches", "hab'", "ich", "nicht", "ver\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Das ich wagte nachzulallen:", "tokens": ["Das", "ich", "wag\u00b7te", "nach\u00b7zu\u00b7lal\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "VVFIN", "VVIZU", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Also singen dir zum Preise", "tokens": ["Al\u00b7so", "sin\u00b7gen", "dir", "zum", "Prei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Unverstandnes Nachtigallen;", "tokens": ["Un\u00b7ver\u00b7stand\u00b7nes", "Nach\u00b7ti\u00b7gal\u00b7len", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Also lernen Kinder reden,", "tokens": ["Al\u00b7so", "ler\u00b7nen", "Kin\u00b7der", "re\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Welche lieb dir sind vor allen.", "tokens": ["Wel\u00b7che", "lieb", "dir", "sind", "vor", "al\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "PPER", "VAFIN", "APPR", "PIAT", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}