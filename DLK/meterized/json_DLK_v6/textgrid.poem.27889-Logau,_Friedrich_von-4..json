{"textgrid.poem.27889": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Simon w\u00fcntschet, da\u00df sein Weib", "tokens": ["Si\u00b7mon", "w\u00fcnt\u00b7schet", ",", "da\u00df", "sein", "Weib"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "$,", "KOUS", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Eine Moschkowitin w\u00e4re;", "tokens": ["Ei\u00b7ne", "Moschko\u00b7wi\u00b7tin", "w\u00e4\u00b7re", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Wann er ihr gleich bleut den Leib,", "tokens": ["Wann", "er", "ihr", "gleich", "bleut", "den", "Leib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df sie sich doch nicht beschwere.", "tokens": ["Da\u00df", "sie", "sich", "doch", "nicht", "be\u00b7schwe\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Aber weil sie deutsch gesinnet,", "tokens": ["A\u00b7ber", "weil", "sie", "deutsch", "ge\u00b7sin\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schaut sie, wie sie sich erwehrt,", "tokens": ["Schaut", "sie", ",", "wie", "sie", "sich", "er\u00b7wehrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "PPER", "PRF", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wie sie Oberhand gewinnet", "tokens": ["Wie", "sie", "O\u00b7ber\u00b7hand", "ge\u00b7win\u00b7net"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PPER", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und die Stube mit ihm kehrt.", "tokens": ["Und", "die", "Stu\u00b7be", "mit", "ihm", "kehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}