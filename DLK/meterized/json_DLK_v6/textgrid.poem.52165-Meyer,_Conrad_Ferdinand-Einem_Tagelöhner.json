{"textgrid.poem.52165": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Einem Tagel\u00f6hner", "genre": "verse", "period": "N.A.", "pub_year": 1861, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lange Jahre sah ich dich", "tokens": ["Lan\u00b7ge", "Jah\u00b7re", "sah", "ich", "dich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "PRF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "F\u00fchren deinen Spaten,", "tokens": ["F\u00fch\u00b7ren", "dei\u00b7nen", "Spa\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und ein jeder Schaufelstich", "tokens": ["Und", "ein", "je\u00b7der", "Schau\u00b7fel\u00b7stich"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist dir wohlgeraten", "tokens": ["Ist", "dir", "wohl\u00b7ge\u00b7ra\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "PPER", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Nie hat dir des Lebens Flucht", "tokens": ["Nie", "hat", "dir", "des", "Le\u00b7bens", "Flucht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Bang gemacht, ich glaube \u2013", "tokens": ["Bang", "ge\u00b7macht", ",", "ich", "glau\u00b7be", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVPP", "$,", "PPER", "VVFIN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Sorgtest f\u00fcr die fremde Frucht,", "tokens": ["Sorg\u00b7test", "f\u00fcr", "die", "frem\u00b7de", "Frucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fcr die fremde Traube.", "tokens": ["F\u00fcr", "die", "frem\u00b7de", "Trau\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Nie gelodert hat die Glut", "tokens": ["Nie", "ge\u00b7lo\u00b7dert", "hat", "die", "Glut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVPP", "VAFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dir in eignem Herde,", "tokens": ["Dir", "in", "eig\u00b7nem", "Her\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Doch du fu\u00dftest fest und gut", "tokens": ["Doch", "du", "fu\u00df\u00b7test", "fest", "und", "gut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PTKVZ", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf der Mutter Erde.", "tokens": ["Auf", "der", "Mut\u00b7ter", "Er\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Nun hast du das Land erreicht,", "tokens": ["Nun", "hast", "du", "das", "Land", "er\u00b7reicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Das du flei\u00dfig grubest,", "tokens": ["Das", "du", "flei\u00b7\u00dfig", "gru\u00b7best", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Laste dir die Scholle leicht,", "tokens": ["Las\u00b7te", "dir", "die", "Schol\u00b7le", "leicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Die du t\u00e4glich hubest!", "tokens": ["Die", "du", "t\u00e4g\u00b7lich", "hu\u00b7best", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}