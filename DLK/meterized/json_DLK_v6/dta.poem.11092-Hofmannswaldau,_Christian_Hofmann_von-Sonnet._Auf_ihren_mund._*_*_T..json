{"dta.poem.11092": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Sonnet.  \n  Auf ihren mund.  \n * *  T.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der purpur, Flavia! der deinen mund bedecket,", "tokens": ["Der", "pur\u00b7pur", ",", "Fla\u00b7via", "!", "der", "dei\u00b7nen", "mund", "be\u00b7de\u00b7cket", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "$,", "NE", "$.", "ART", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Kommt (ich gesteh es zu,) von keiner farbe her:", "tokens": ["Kommt", "(", "ich", "ge\u00b7steh", "es", "zu", ",", ")", "von", "kei\u00b7ner", "far\u00b7be", "her", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$(", "PPER", "VVFIN", "PPER", "PTKVZ", "$,", "$(", "APPR", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nein, blitze kommen nicht aus firni\u00df ohngef\u00e4hr,", "tokens": ["Nein", ",", "blit\u00b7ze", "kom\u00b7men", "nicht", "aus", "fir\u00b7ni\u00df", "ohn\u00b7ge\u00b7f\u00e4hr", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "VVFIN", "PTKNEG", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und in der schnecken blut wird keine gluth gehecket.", "tokens": ["Und", "in", "der", "schne\u00b7cken", "blut", "wird", "kei\u00b7ne", "gluth", "ge\u00b7he\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VAFIN", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Was aber hast du sonst vor wunder hier verstecket?", "tokens": ["Was", "a\u00b7ber", "hast", "du", "sonst", "vor", "wun\u00b7der", "hier", "ver\u00b7ste\u00b7cket", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VAFIN", "PPER", "ADV", "APPR", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Dir eckelt vor der lieb, und bist vom feuer leer;", "tokens": ["Dir", "ec\u00b7kelt", "vor", "der", "lieb", ",", "und", "bist", "vom", "feu\u00b7er", "leer", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJD", "$,", "KON", "VAFIN", "APPRART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und dennoch f\u00fchrt dein mund der Venus ihr gewehr,", "tokens": ["Und", "den\u00b7noch", "f\u00fchrt", "dein", "mund", "der", "Ve\u00b7nus", "ihr", "ge\u00b7wehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPOSAT", "NN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Das tausend m\u00e4nner trifft, und steten brand erwecket.", "tokens": ["Das", "tau\u00b7send", "m\u00e4n\u00b7ner", "trifft", ",", "und", "ste\u00b7ten", "brand", "er\u00b7we\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "ADJA", "VVFIN", "$,", "KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ist ei\u00df und winter hier bey rosen und bey gluth?", "tokens": ["Ist", "ei\u00df", "und", "win\u00b7ter", "hier", "bey", "ro\u00b7sen", "und", "bey", "gluth", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KON", "NE", "ADV", "APPR", "ADJA", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+-+++", "measure": "unknown.measure.septa"}, "line.10": {"text": "Bl\u00fchn blumen, da der frost den gantzen stock verderbet?", "tokens": ["Bl\u00fchn", "blu\u00b7men", ",", "da", "der", "frost", "den", "gant\u00b7zen", "stock", "ver\u00b7der\u00b7bet", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "KOUS", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Doch ja: Ich sehe schon, was deine lippen f\u00e4rbet;", "tokens": ["Doch", "ja", ":", "Ich", "se\u00b7he", "schon", ",", "was", "dei\u00b7ne", "lip\u00b7pen", "f\u00e4r\u00b7bet", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$.", "PPER", "VVFIN", "ADV", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Jhr bunter purpur kommt von dem erhitzten blut,", "tokens": ["Ihr", "bun\u00b7ter", "pur\u00b7pur", "kommt", "von", "dem", "er\u00b7hitz\u00b7ten", "blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADJD", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Das dein bezaubrend aug\u2019 aus m\u00e4nner-hertzen ziehet:", "tokens": ["Das", "dein", "be\u00b7zau\u00b7brend", "aug'", "aus", "m\u00e4n\u00b7ner\u00b7hert\u00b7zen", "zie\u00b7het", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "VVPP", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Was wunder, da\u00df dein mund von r\u00f6th und feuer gl\u00fchet?", "tokens": ["Was", "wun\u00b7der", ",", "da\u00df", "dein", "mund", "von", "r\u00f6th", "und", "feu\u00b7er", "gl\u00fc\u00b7het", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "$,", "KOUS", "PPOSAT", "NN", "APPR", "NE", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}