{"textgrid.poem.60939": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Urbanit\u00e4t", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht mehr l\u00e4nger besch\u00fctzt der gefl\u00fcgelte L\u00f6we Venedig,", "tokens": ["Nicht", "mehr", "l\u00e4n\u00b7ger", "be\u00b7sch\u00fctzt", "der", "ge\u00b7fl\u00fc\u00b7gel\u00b7te", "L\u00f6\u00b7we", "Ve\u00b7ne\u00b7dig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADJD", "VVFIN", "ART", "ADJA", "NE", "NE", "$,"], "meter": "--+--+--+--+-+--", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Auch Sankt Markus entwich samt dem geweihten Panier.", "tokens": ["Auch", "Sankt", "Mar\u00b7kus", "ent\u00b7wich", "samt", "dem", "ge\u00b7weih\u00b7ten", "Pa\u00b7nier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Aber es blieb doch eine der Schutzg\u00f6ttinnen, und Tempel,", "tokens": ["A\u00b7ber", "es", "blieb", "doch", "ei\u00b7ne", "der", "Schutz\u00b7g\u00f6t\u00b7tin\u00b7nen", ",", "und", "Tem\u00b7pel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ART", "ART", "NN", "$,", "KON", "NN", "$,"], "meter": "+--+-+--++-+-+-", "measure": "iambic.septa.invert"}, "line.4": {"text": "Aus der verwilderten Welt fl\u00fcchtend, erbaute sie hier:", "tokens": ["Aus", "der", "ver\u00b7wil\u00b7der\u00b7ten", "Welt", "fl\u00fcch\u00b7tend", ",", "er\u00b7bau\u00b7te", "sie", "hier", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$,", "VVFIN", "PPER", "ADV", "$."], "meter": "---+--++--+--+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Wi\u00dft, Urbanitas hei\u00dft die Beseligerin der Gem\u00fcter,", "tokens": ["Wi\u00dft", ",", "Ur\u00b7ba\u00b7ni\u00b7tas", "hei\u00dft", "die", "Be\u00b7se\u00b7li\u00b7ge\u00b7rin", "der", "Ge\u00b7m\u00fc\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NE", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+---+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Die sich hier im Gefolg ewiger Grazien zeigt.", "tokens": ["Die", "sich", "hier", "im", "Ge\u00b7folg", "e\u00b7wi\u00b7ger", "Gra\u00b7zi\u00b7en", "zeigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADV", "APPRART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Fremdling! Selten vermagst du dem magischen Netz zu entziehn dich,", "tokens": ["Fremd\u00b7ling", "!", "Sel\u00b7ten", "ver\u00b7magst", "du", "dem", "ma\u00b7gi\u00b7schen", "Netz", "zu", "ent\u00b7ziehn", "dich", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "PTKZU", "VVINF", "PPER", "$,"], "meter": "+-+--+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "Welches um dich huldreich jene Gef\u00e4llige spinnt.", "tokens": ["Wel\u00b7ches", "um", "dich", "huld\u00b7reich", "je\u00b7ne", "Ge\u00b7f\u00e4l\u00b7li\u00b7ge", "spinnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PRF", "ADJD", "PDAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.9": {"text": "Sie auch bildete selbst die bezaubernden Kl\u00e4nge der Mundart:", "tokens": ["Sie", "auch", "bil\u00b7de\u00b7te", "selbst", "die", "be\u00b7zau\u00b7bern\u00b7den", "Kl\u00e4n\u00b7ge", "der", "Mund\u00b7art", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ADV", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.10": {"text": "S\u00fc\u00dferes Wort hat nie menschliche Lippen beseelt.", "tokens": ["S\u00fc\u00b7\u00dfe\u00b7res", "Wort", "hat", "nie", "menschli\u00b7che", "Lip\u00b7pen", "be\u00b7seelt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "ADV", "ADJA", "NN", "VVPP", "$."], "meter": "+--+-+--+--+", "measure": "iambic.penta.invert"}}}}}