{"textgrid.poem.67362": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Der Vorhang", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schlaf' hinterm Vorhang, wer da will;", "tokens": ["Schlaf'", "hin\u00b7term", "Vor\u00b7hang", ",", "wer", "da", "will", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,", "PWS", "ADV", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich mag ihn nicht mein ganzes Leben", "tokens": ["Ich", "mag", "ihn", "nicht", "mein", "gan\u00b7zes", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und will Euch gleich zur Nachricht geben,", "tokens": ["Und", "will", "Euch", "gleich", "zur", "Nach\u00b7richt", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Warum ich ihn nicht mag noch will.", "tokens": ["Wa\u00b7rum", "ich", "ihn", "nicht", "mag", "noch", "will", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "PTKNEG", "VMFIN", "ADV", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Zuerst und ", "tokens": ["Zu\u00b7erst", "und"], "token_info": ["word", "word"], "pos": ["ADV", "KON"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Die sich zu sehr des Dunkels freuet,", "tokens": ["Die", "sich", "zu", "sehr", "des", "Dun\u00b7kels", "freu\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Traum, der auch Auroren scheuet,", "tokens": ["Der", "Traum", ",", "der", "auch", "Au\u00b7ro\u00b7ren", "scheu\u00b7et", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sind ihrer sich nur halb bewu\u00dft.", "tokens": ["Sind", "ih\u00b7rer", "sich", "nur", "halb", "be\u00b7wu\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "ADV", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wo Morgen- nicht noch Abendroth", "tokens": ["Wo", "Mor\u00b7gen", "nicht", "noch", "A\u00b7ben\u00b7droth"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "TRUNC", "PTKNEG", "ADV", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Noch D\u00e4mmerung uns mag erreichen,", "tokens": ["Noch", "D\u00e4m\u00b7me\u00b7rung", "uns", "mag", "er\u00b7rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nur schwarze Schatten um uns schleichen,", "tokens": ["Nur", "schwar\u00b7ze", "Schat\u00b7ten", "um", "uns", "schlei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ist Phantasie der Liebe todt.", "tokens": ["Ist", "Phan\u00b7ta\u00b7sie", "der", "Lie\u00b7be", "todt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Die Muse liebt des Tages Schein;", "tokens": ["Die", "Mu\u00b7se", "liebt", "des", "Ta\u00b7ges", "Schein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Grazien und Liebesg\u00f6tter,", "tokens": ["Die", "Gra\u00b7zi\u00b7en", "und", "Lie\u00b7bes\u00b7g\u00f6t\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie betten sich auf Rosenbl\u00e4tter;", "tokens": ["Sie", "bet\u00b7ten", "sich", "auf", "Ro\u00b7sen\u00b7bl\u00e4t\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Im Freien schlafen alle Neun.", "tokens": ["Im", "Frei\u00b7en", "schla\u00b7fen", "al\u00b7le", "Neun", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PIAT", "CARD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Mein Vorhang ist die Unschuld mir;", "tokens": ["Mein", "Vor\u00b7hang", "ist", "die", "Un\u00b7schuld", "mir", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das scheue Wild kriecht in die H\u00f6hlen.", "tokens": ["Das", "scheu\u00b7e", "Wild", "kriecht", "in", "die", "H\u00f6h\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mich soll der Welthauch frei beseelen;", "tokens": ["Mich", "soll", "der", "Welt\u00b7hauch", "frei", "be\u00b7see\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den zieh' ich mir zum Vorhang f\u00fcr.", "tokens": ["Den", "zieh'", "ich", "mir", "zum", "Vor\u00b7hang", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PPER", "APPRART", "NN", "APPR", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}