{"textgrid.poem.32298": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mein Esel sicherlich", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Esel sicherlich", "tokens": ["Mein", "E\u00b7sel", "si\u00b7cher\u00b7lich"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "PRF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Mu\u00df kl\u00fcger sein, als ich.", "tokens": ["Mu\u00df", "kl\u00fc\u00b7ger", "sein", ",", "als", "ich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "VAINF", "$,", "KOUS", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ja, kl\u00fcger mu\u00df er sein!", "tokens": ["Ja", ",", "kl\u00fc\u00b7ger", "mu\u00df", "er", "sein", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADJD", "VMFIN", "PPER", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Er fand sich selbst in Stall hinein,", "tokens": ["Er", "fand", "sich", "selbst", "in", "Stall", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und kam doch von der Tr\u00e4nke.", "tokens": ["Und", "kam", "doch", "von", "der", "Tr\u00e4n\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Man denke!", "tokens": ["Man", "den\u00b7ke", "!"], "token_info": ["word", "word", "punct"], "pos": ["PIS", "VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}}}}