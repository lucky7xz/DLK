{"textgrid.poem.55200": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Angebinde zur R\u00fcckkehr", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Freundin war hinausgegangen,", "tokens": ["Die", "Freun\u00b7din", "war", "hin\u00b7aus\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Um in der Welt sich umzutun;", "tokens": ["Um", "in", "der", "Welt", "sich", "um\u00b7zu\u00b7tun", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "APPR", "ART", "NN", "PRF", "VVIZU", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nun wird sie bald nach Haus gelangen", "tokens": ["Nun", "wird", "sie", "bald", "nach", "Haus", "ge\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und auf gewohnte Weise ruhn.", "tokens": ["Und", "auf", "ge\u00b7wohn\u00b7te", "Wei\u00b7se", "ruhn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und neigt sie dann das art'ge K\u00f6pfchen,", "tokens": ["Und", "neigt", "sie", "dann", "das", "art'\u00b7ge", "K\u00f6pf\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Umwunden reich von Zopf und Z\u00f6pfchen,", "tokens": ["Um\u00b7wun\u00b7den", "reich", "von", "Zopf", "und", "Z\u00f6pf\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Nach einem kissenweichen Sitzchen,", "tokens": ["Nach", "ei\u00b7nem", "kis\u00b7sen\u00b7wei\u00b7chen", "Sitz\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "So bietet freundlich ihr das M\u00fctzchen.", "tokens": ["So", "bie\u00b7tet", "freund\u00b7lich", "ihr", "das", "M\u00fctz\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}