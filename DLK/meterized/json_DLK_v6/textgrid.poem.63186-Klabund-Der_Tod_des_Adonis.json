{"textgrid.poem.63186": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Der Tod des Adonis", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sieben Wochen schon schreit Kypris,", "tokens": ["Sie\u00b7ben", "Wo\u00b7chen", "schon", "schreit", "Kyp\u00b7ris", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "ADV", "ADJD", "NE", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Denn Adonis starb,", "tokens": ["Denn", "A\u00b7do\u00b7nis", "starb", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Der sch\u00f6nste der Menschen.", "tokens": ["Der", "sch\u00f6ns\u00b7te", "der", "Men\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Die Sterne weinen nachts Sternschnuppen,", "tokens": ["Die", "Ster\u00b7ne", "wei\u00b7nen", "nachts", "Stern\u00b7schnup\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "APPR", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und salzig von Tr\u00e4nen ist", "tokens": ["Und", "sal\u00b7zig", "von", "Tr\u00e4\u00b7nen", "ist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "NN", "VAFIN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Das Gew\u00e4sser der Fl\u00fcsse.", "tokens": ["Das", "Ge\u00b7w\u00e4s\u00b7ser", "der", "Fl\u00fcs\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.7": {"text": "An den Quellen sitzen die Nymphen", "tokens": ["An", "den", "Quel\u00b7len", "sit\u00b7zen", "die", "Nym\u00b7phen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Und schluchzen,", "tokens": ["Und", "schluch\u00b7zen", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVINF", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.9": {"text": "Und jammernd durch Feld und Hain", "tokens": ["Und", "jam\u00b7mernd", "durch", "Feld", "und", "Hain"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "NN", "KON", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Streifen Eroten.", "tokens": ["Strei\u00b7fen", "E\u00b7ro\u00b7ten", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Ihr Klagegeschrei", "tokens": ["Ihr", "Kla\u00b7ge\u00b7ge\u00b7schrei"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.12": {"text": "Ai ai ai", "tokens": ["Ai", "ai", "ai"], "token_info": ["word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la"], "meter": "+-+", "measure": "trochaic.di"}, "line.13": {"text": "Durchhallt die Schluchten und schreckt", "tokens": ["Durch\u00b7hallt", "die", "Schluch\u00b7ten", "und", "schreckt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "KON", "VVFIN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.14": {"text": "Den einsamen Wanderer.", "tokens": ["Den", "ein\u00b7sa\u00b7men", "Wan\u00b7de\u00b7rer", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+--", "measure": "unknown.measure.tri"}}, "stanza.2": {"line.1": {"text": "Unseligen Tod", "tokens": ["Un\u00b7se\u00b7li\u00b7gen", "Tod"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Starb der Geliebte.", "tokens": ["Starb", "der", "Ge\u00b7lieb\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Denn als er wandelt", "tokens": ["Denn", "als", "er", "wan\u00b7delt"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Durch den Wald,", "tokens": ["Durch", "den", "Wald", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Begegnet ihm ein wilder Eber,", "tokens": ["Be\u00b7geg\u00b7net", "ihm", "ein", "wil\u00b7der", "E\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der alsogleich entbrennt wider den Sch\u00f6nen", "tokens": ["Der", "al\u00b7so\u00b7gleich", "ent\u00b7brennt", "wi\u00b7der", "den", "Sch\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In Liebe.", "tokens": ["In", "Lie\u00b7be", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Liebkosend er gegen ihn sprang.", "tokens": ["Lieb\u00b7ko\u00b7send", "er", "ge\u00b7gen", "ihn", "sprang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "APPR", "PPER", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Aber so rauh war seine Z\u00e4rtlichkeit,", "tokens": ["A\u00b7ber", "so", "rauh", "war", "sei\u00b7ne", "Z\u00e4rt\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.5": {"text": "Dass mit den Hauern er", "tokens": ["Dass", "mit", "den", "Hau\u00b7ern", "er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "PPER"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "Dem sch\u00f6nen Knaben", "tokens": ["Dem", "sch\u00f6\u00b7nen", "Kna\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Die Brust zerriss.", "tokens": ["Die", "Brust", "zer\u00b7riss", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Unbeerdigt lag er im Moose", "tokens": ["Un\u00b7be\u00b7er\u00b7digt", "lag", "er", "im", "Moo\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "APPRART", "NN"], "meter": "+---+--+-", "measure": "trochaic.tri.relaxed"}, "line.2": {"text": "Unverwest.", "tokens": ["Un\u00b7ver\u00b7west", "."], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Kein Wurm ihn benagte", "tokens": ["Kein", "Wurm", "ihn", "be\u00b7nag\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "PPER", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Und keine Kr\u00e4he ihn hackte.", "tokens": ["Und", "kei\u00b7ne", "Kr\u00e4\u00b7he", "ihn", "hack\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Der Mond hielt mit bleicher Fackel", "tokens": ["Der", "Mond", "hielt", "mit", "blei\u00b7cher", "Fa\u00b7ckel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Die Totenwacht.", "tokens": ["Die", "To\u00b7ten\u00b7wacht", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Die Geister der untern Welt,", "tokens": ["Die", "Geis\u00b7ter", "der", "un\u00b7tern", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Sie kamen", "tokens": ["Sie", "ka\u00b7men"], "token_info": ["word", "word"], "pos": ["PPER", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.9": {"text": "Schleichend und schillernd", "tokens": ["Schlei\u00b7chend", "und", "schil\u00b7lernd"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "KON", "ADJD"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Herauf", "tokens": ["Her\u00b7auf"], "token_info": ["word"], "pos": ["NN"], "meter": "-+", "measure": "iambic.single"}, "line.11": {"text": "Und sassen am weissen Strom seines Leibes", "tokens": ["Und", "sas\u00b7sen", "am", "weis\u00b7sen", "Strom", "sei\u00b7nes", "Lei\u00b7bes"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "NN", "PPOSAT", "NN"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Wie an den Ufern des heiligen Flusses.", "tokens": ["Wie", "an", "den", "U\u00b7fern", "des", "hei\u00b7li\u00b7gen", "Flus\u00b7ses", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}}, "stanza.5": {"line.1": {"text": "Und Charon nahm", "tokens": ["Und", "Cha\u00b7ron", "nahm"], "token_info": ["word", "word", "word"], "pos": ["KON", "NE", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Am siebenten Tage", "tokens": ["Am", "sie\u00b7ben\u00b7ten", "Ta\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Den leuchtenden Leichnam", "tokens": ["Den", "leuch\u00b7ten\u00b7den", "Leich\u00b7nam"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Auf seine Schulter wie ein totes Reh,", "tokens": ["Auf", "sei\u00b7ne", "Schul\u00b7ter", "wie", "ein", "to\u00b7tes", "Reh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Das der J\u00e4ger nach Hause tr\u00e4gt", "tokens": ["Das", "der", "J\u00e4\u00b7ger", "nach", "Hau\u00b7se", "tr\u00e4gt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ART", "NN", "APPR", "NN", "VVFIN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Zu den Seinen.", "tokens": ["Zu", "den", "Sei\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPOSS", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.6": {"line.1": {"text": "Der Leichnam blinkte", "tokens": ["Der", "Leich\u00b7nam", "blink\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "In den Grotten der Unterwelt", "tokens": ["In", "den", "Grot\u00b7ten", "der", "Un\u00b7ter\u00b7welt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.7": {"line.1": {"text": "Wie eine weisse Ampel.", "tokens": ["Wie", "ei\u00b7ne", "weis\u00b7se", "Am\u00b7pel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von allen Seiten", "tokens": ["Von", "al\u00b7len", "Sei\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Die toten Seelen", "tokens": ["Die", "to\u00b7ten", "See\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Wie n\u00e4chtliche Falter zum Lichte flogen,", "tokens": ["Wie", "n\u00e4cht\u00b7li\u00b7che", "Fal\u00b7ter", "zum", "Lich\u00b7te", "flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Bis sie ihn deckten", "tokens": ["Bis", "sie", "ihn", "deck\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Bedeckten", "tokens": ["Be\u00b7deck\u00b7ten"], "token_info": ["word"], "pos": ["NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Und er", "tokens": ["Und", "er"], "token_info": ["word", "word"], "pos": ["KON", "PPER"], "meter": "+-", "measure": "trochaic.single"}, "line.8": {"text": "Unter den schwarzen Fl\u00fcgelschl\u00e4gen", "tokens": ["Un\u00b7ter", "den", "schwar\u00b7zen", "Fl\u00fc\u00b7gel\u00b7schl\u00e4\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.9": {"text": "Erlosch.", "tokens": ["Er\u00b7losch", "."], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "-+", "measure": "iambic.single"}}}}}