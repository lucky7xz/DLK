{"textgrid.poem.59454": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Verloren", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Still bei Nacht f\u00e4hrt manches Schiff,", "tokens": ["Still", "bei", "Nacht", "f\u00e4hrt", "man\u00b7ches", "Schiff", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Meerfei k\u00e4mmt ihr Haar am Riff,", "tokens": ["Meer\u00b7fei", "k\u00e4mmt", "ihr", "Haar", "am", "Riff", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "Hebt von Inseln an zu singen,", "tokens": ["Hebt", "von", "In\u00b7seln", "an", "zu", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die im Meer dort untergingen.", "tokens": ["Die", "im", "Meer", "dort", "un\u00b7ter\u00b7gin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wann die Morgenwinde wehn,", "tokens": ["Wann", "die", "Mor\u00b7gen\u00b7win\u00b7de", "wehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist nicht Riff noch Fei zu sehn,", "tokens": ["Ist", "nicht", "Riff", "noch", "Fei", "zu", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "NN", "ADV", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und das Schifflein ist versunken,", "tokens": ["Und", "das", "Schif\u00b7flein", "ist", "ver\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und der Schiffer ist ertrunken.", "tokens": ["Und", "der", "Schif\u00b7fer", "ist", "er\u00b7trun\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}