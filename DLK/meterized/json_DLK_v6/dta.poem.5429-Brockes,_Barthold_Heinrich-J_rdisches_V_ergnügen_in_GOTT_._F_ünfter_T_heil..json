{"dta.poem.5429": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "J rdisches  \n  V ergn\u00fcgen  \n in  \n  GOTT .  \n  F \u00fcnfter  T heil.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein wahrer Danck ist eine Frucht von einer Seelen, die", "tokens": ["Ein", "wah\u00b7rer", "Danck", "ist", "ei\u00b7ne", "Frucht", "von", "ei\u00b7ner", "See\u00b7len", ",", "die"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "NN", "APPR", "ART", "NN", "$,", "PRELS"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "ger\u00fchrt", "tokens": ["ge\u00b7r\u00fchrt"], "token_info": ["word"], "pos": ["VVPP"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Durch Wolthat, welche sie empfangen, und die, durch", "tokens": ["Durch", "Wolt\u00b7hat", ",", "wel\u00b7che", "sie", "emp\u00b7fan\u00b7gen", ",", "und", "die", ",", "durch"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["APPR", "NN", "$,", "PRELS", "PPER", "VVPP", "$,", "KON", "ART", "$,", "APPR"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "solche Lust getrieben,", "tokens": ["sol\u00b7che", "Lust", "ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Entflammte Danck-Bewegungen und eine frohe Sehnsucht", "tokens": ["Ent\u00b7flamm\u00b7te", "Dan\u00b7ck\u00b7Be\u00b7we\u00b7gun\u00b7gen", "und", "ei\u00b7ne", "fro\u00b7he", "Sehn\u00b7sucht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "ART", "ADJA", "NN"], "meter": "---+--+---+-+--+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "sp\u00fchrt,", "tokens": ["sp\u00fchrt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-", "measure": "single.down"}, "line.7": {"text": "Dem Geber angenehm zu seyn, aus allen Kr\u00e4ften ihn zu", "tokens": ["Dem", "Ge\u00b7ber", "an\u00b7ge\u00b7nehm", "zu", "seyn", ",", "aus", "al\u00b7len", "Kr\u00e4f\u00b7ten", "ihn", "zu"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "PTKZU", "VAINF", "$,", "APPR", "PIAT", "NN", "PPER", "PTKZU"], "meter": "-+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.8": {"text": "lieben,", "tokens": ["lie\u00b7ben", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "Und, in Erwegung des Geschencks, Erk\u00e4nntlichkeiten aus-", "tokens": ["Und", ",", "in", "Er\u00b7we\u00b7gung", "des", "Ge\u00b7schencks", ",", "Er\u00b7k\u00e4nnt\u00b7lich\u00b7kei\u00b7ten", "aus"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "$,", "APPR", "NN", "ART", "NN", "$,", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.10": {"text": "zu \u00fcben,", "tokens": ["zu", "\u00fc\u00b7ben", ","], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.11": {"text": "So weit nur ihr Verm\u00f6gen reicht. Je mehr erhaben nun,", "tokens": ["So", "weit", "nur", "ihr", "Ver\u00b7m\u00f6\u00b7gen", "reicht", ".", "Je", "mehr", "er\u00b7ha\u00b7ben", "nun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "PPOSAT", "NN", "VVFIN", "$.", "ADV", "ADV", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.12": {"text": "je gr\u00f6sser", "tokens": ["je", "gr\u00f6s\u00b7ser"], "token_info": ["word", "word"], "pos": ["ADV", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.13": {"text": "Und m\u00e4chtiger der Geber ist; je w\u00fcrdiger zugleich und", "tokens": ["Und", "m\u00e4ch\u00b7ti\u00b7ger", "der", "Ge\u00b7ber", "ist", ";", "je", "w\u00fcr\u00b7di\u00b7ger", "zu\u00b7gleich", "und"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "ART", "NN", "VAFIN", "$.", "ADV", "ADJD", "ADV", "KON"], "meter": "-+-+-+-+++-+-+-", "measure": "unknown.measure.octa.plus"}, "line.14": {"text": "besser", "tokens": ["bes\u00b7ser"], "token_info": ["word"], "pos": ["ADJD"], "meter": "+-", "measure": "trochaic.single"}, "line.15": {"text": "Die Gabe: je gewaltiger entstehn die Feuer-reichen Triebe", "tokens": ["Die", "Ga\u00b7be", ":", "je", "ge\u00b7wal\u00b7ti\u00b7ger", "ent\u00b7stehn", "die", "Feu\u00b7er\u00b7rei\u00b7chen", "Trie\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$.", "ADV", "ADJA", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.16": {"text": "Von einer Ehrfurcht-vollen Neigung und dienst-begier\u2019gen", "tokens": ["Von", "ei\u00b7ner", "Ehr\u00b7furcht\u00b7vol\u00b7len", "Nei\u00b7gung", "und", "dienst\u00b7be\u00b7gier'\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.17": {"text": "Gegen-Liebe.", "tokens": ["Ge\u00b7gen\u00b7Lie\u00b7be", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.18": {"text": "Wo nun vom menschlichen Geschlecht dem Sch\u00f6pfer etwas", "tokens": ["Wo", "nun", "vom", "menschli\u00b7chen", "Ge\u00b7schlecht", "dem", "Sch\u00f6p\u00b7fer", "et\u00b7was"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "APPRART", "ADJA", "NN", "ART", "NN", "ADV"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.19": {"text": "kann gefallen,", "tokens": ["kann", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "punct"], "pos": ["VMFIN", "VVPP", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.20": {"text": "Mu\u00df es von einer frohen Seelen solch Danck- und Lie-", "tokens": ["Mu\u00df", "es", "von", "ei\u00b7ner", "fro\u00b7hen", "See\u00b7len", "solch", "Dan\u00b7ck", "und", "Lie"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "ART", "ADJA", "NN", "PIAT", "TRUNC", "KON", "NN"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.21": {"text": "bes-Opffer seyn!", "tokens": ["bes\u00b7Opf\u00b7fer", "seyn", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VAINF", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.22": {"text": "Allein! wenn wir dem Sch\u00f6pfer dancken, versp\u00fchrt man", "tokens": ["Al\u00b7lein", "!", "wenn", "wir", "dem", "Sch\u00f6p\u00b7fer", "dan\u00b7cken", ",", "ver\u00b7sp\u00fchrt", "man"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "$.", "KOUS", "PPER", "ART", "NN", "VVINF", "$,", "VVFIN", "PIS"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.23": {"text": "wol ein solches Wallen", "tokens": ["wol", "ein", "sol\u00b7ches", "Wal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "PIAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.24": {"text": "Von Lieb und Lust in unsern Seelen? solch eine Sehn-", "tokens": ["Von", "Lieb", "und", "Lust", "in", "un\u00b7sern", "See\u00b7len", "?", "solch", "ei\u00b7ne", "Sehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "APPR", "PPOSAT", "NN", "$.", "PIAT", "ART", "TRUNC"], "meter": "-+-+-+-+-++-+", "measure": "unknown.measure.septa"}, "line.25": {"text": "sucht? leider, nein!", "tokens": ["sucht", "?", "lei\u00b7der", ",", "nein", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$.", "ADV", "$,", "PTKANT", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.26": {"text": "Wir f\u00fchlen nicht einmahl die Sch\u00e4tze, die GOtt in solcher", "tokens": ["Wir", "f\u00fch\u00b7len", "nicht", "ein\u00b7mahl", "die", "Sch\u00e4t\u00b7ze", ",", "die", "Gott", "in", "sol\u00b7cher"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "ART", "NN", "$,", "ART", "NN", "APPR", "PIAT"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.27": {"text": "F\u00fcll' uns giebet,", "tokens": ["F\u00fcll'", "uns", "gie\u00b7bet", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.28": {"text": "Wer schmeckt den Reichthum seiner G\u00fcte? wir riechen,", "tokens": ["Wer", "schmeckt", "den", "Reicht\u00b7hum", "sei\u00b7ner", "G\u00fc\u00b7te", "?", "wir", "rie\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.29": {"text": "sehn, und h\u00f6ren nicht.", "tokens": ["sehn", ",", "und", "h\u00f6\u00b7ren", "nicht", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVINF", "$,", "KON", "VVFIN", "PTKNEG", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.30": {"text": "Bewundert und erwegt man wol, was blos durch seine", "tokens": ["Be\u00b7wun\u00b7dert", "und", "er\u00b7wegt", "man", "wol", ",", "was", "blos", "durch", "sei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "KON", "VVFIN", "PIS", "ADV", "$,", "PRELS", "ADV", "APPR", "PPOSAT"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Macht geschicht?", "tokens": ["Macht", "ge\u00b7schicht", "?"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.32": {"text": "Bedenckt man, mit vergn\u00fcgter Seele, wie sehr uns unser", "tokens": ["Be\u00b7denckt", "man", ",", "mit", "ver\u00b7gn\u00fcg\u00b7ter", "See\u00b7le", ",", "wie", "sehr", "uns", "un\u00b7ser"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "$,", "APPR", "ADJA", "NN", "$,", "PWAV", "ADV", "PPER", "PPOSAT"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.33": {"text": "Sch\u00f6pfer liebet?", "tokens": ["Sch\u00f6p\u00b7fer", "lie\u00b7bet", "?"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}