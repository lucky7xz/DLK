{"textgrid.poem.67730": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Sehnsucht nach Ruhe und Tod", "genre": "verse", "period": "N.A.", "pub_year": 1765, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und nun! voll Armuth, ohne Ruh", "tokens": ["Und", "nun", "!", "voll", "Ar\u00b7muth", ",", "oh\u00b7ne", "Ruh"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "$.", "ADJD", "NN", "$,", "KOUI", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sitz' ich und wein' den Wolken zu.", "tokens": ["Sitz'", "ich", "und", "wein'", "den", "Wol\u00b7ken", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ach, w\u00e4r' ich \u2013 w\u00e4r' ich reich!", "tokens": ["Ach", ",", "w\u00e4r'", "ich", "\u2013", "w\u00e4r'", "ich", "reich", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VAFIN", "PPER", "$(", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Da klingt's. \u2013 Liebst Reichthum Du?", "tokens": ["Da", "klingt'", "s.", "\u2013", "Liebst", "Reicht\u00b7hum", "Du", "?"], "token_info": ["word", "word", "abbreviation", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "$(", "NN", "NN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Doch \u2013 hinter Dir schleicht Sorge!", "tokens": ["Doch", "\u2013", "hin\u00b7ter", "Dir", "schleicht", "Sor\u00b7ge", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "APPR", "PPER", "VVFIN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "O la\u00df mich! \u2013 la\u00df mich gleich!", "tokens": ["O", "la\u00df", "mich", "!", "\u2013", "la\u00df", "mich", "gleich", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$.", "$(", "VVIMP", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und nun! noch sitz' ich ohne Ruh", "tokens": ["Und", "nun", "!", "noch", "sitz'", "ich", "oh\u00b7ne", "Ruh"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$.", "ADV", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und weine Gram dem Staube zu.", "tokens": ["Und", "wei\u00b7ne", "Gram", "dem", "Stau\u00b7be", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Im Staube? W\u00e4r', o w\u00e4r' ich gro\u00df!", "tokens": ["Im", "Stau\u00b7be", "?", "W\u00e4r'", ",", "o", "w\u00e4r'", "ich", "gro\u00df", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$.", "VAFIN", "$,", "FM", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Da rauscht's. \u2013 Mach, lieber Ruhm, mich gro\u00df!", "tokens": ["Da", "rauscht'", "s.", "\u2013", "Mach", ",", "lie\u00b7ber", "Ruhm", ",", "mich", "gro\u00df", "!"], "token_info": ["word", "word", "abbreviation", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "$(", "NE", "$,", "ADV", "NN", "$,", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Doch \u2013 hinter Dir schleicht T\u00fccke!", "tokens": ["Doch", "\u2013", "hin\u00b7ter", "Dir", "schleicht", "T\u00fc\u00b7cke", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "APPR", "PPER", "VVFIN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "O la\u00df mich! \u2013 la\u00df mich blos!", "tokens": ["O", "la\u00df", "mich", "!", "\u2013", "la\u00df", "mich", "blos", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$.", "$(", "VVIMP", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Voll Ungl\u00fcck, ruhmlos und noch ohne Ruh", "tokens": ["Voll", "Un\u00b7gl\u00fcck", ",", "ruh\u00b7mlos", "und", "noch", "oh\u00b7ne", "Ruh"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "NN", "$,", "ADJD", "KON", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Lieg' ich und ruf' der W\u00fcste zu.", "tokens": ["Lieg'", "ich", "und", "ruf'", "der", "W\u00fcs\u00b7te", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Wo? h\u00f6rst Du mich nicht, Gl\u00fcck?", "tokens": ["Wo", "?", "h\u00f6rst", "Du", "mich", "nicht", ",", "Gl\u00fcck", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "$.", "VVFIN", "PPER", "PRF", "PTKNEG", "$,", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.16": {"text": "Da kommt's. \u2013 Gieb, liebes Gl\u00fcck, mir Ruh!", "tokens": ["Da", "kommt'", "s.", "\u2013", "Gieb", ",", "lie\u00b7bes", "Gl\u00fcck", ",", "mir", "Ruh", "!"], "token_info": ["word", "word", "abbreviation", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "$(", "VVIMP", "$,", "ADJA", "NN", "$,", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Doch \u2013 bringst Du schwarze Laster?", "tokens": ["Doch", "\u2013", "bringst", "Du", "schwar\u00b7ze", "Las\u00b7ter", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "O kehre nur zur\u00fcck!", "tokens": ["O", "keh\u00b7re", "nur", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.19": {"text": "Komm, Buch! \u2013 Doch sitz' ich ohne Ruh", "tokens": ["Komm", ",", "Buch", "!", "\u2013", "Doch", "sitz'", "ich", "oh\u00b7ne", "Ruh"], "token_info": ["word", "punct", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "NN", "$.", "$(", "KON", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Und weine selbst den B\u00fcchern zu.", "tokens": ["Und", "wei\u00b7ne", "selbst", "den", "B\u00fc\u00b7chern", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Denn wenn auch tief gelehrt,", "tokens": ["Denn", "wenn", "auch", "tief", "ge\u00b7lehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.22": {"text": "Von lieber Mus' erh\u00f6rt,", "tokens": ["Von", "lie\u00b7ber", "Mus'", "er\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.23": {"text": "Doch Thorheit! hinter Dir schleicht finstre Plage.", "tokens": ["Doch", "Thor\u00b7heit", "!", "hin\u00b7ter", "Dir", "schleicht", "finst\u00b7re", "Pla\u00b7ge", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$.", "APPR", "PPER", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Nein, das bist Du nicht werth!", "tokens": ["Nein", ",", "das", "bist", "Du", "nicht", "werth", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PDS", "VAFIN", "PPER", "PTKNEG", "ADJD", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.25": {"text": "Ohn' Buch und Gl\u00fcck und ohne Ruh,", "tokens": ["Ohn'", "Buch", "und", "Gl\u00fcck", "und", "oh\u00b7ne", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Seh' ich dem Gl\u00fcck der Freundschaft zu.", "tokens": ["Seh'", "ich", "dem", "Gl\u00fcck", "der", "Freund\u00b7schaft", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "O w\u00e4r' ich Mensch \u2013 und Freund!", "tokens": ["O", "w\u00e4r'", "ich", "Mensch", "\u2013", "und", "Freund", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "NN", "$(", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.28": {"text": "O Erde! \u2013 bildet mich zur Ruh!", "tokens": ["O", "Er\u00b7de", "!", "\u2013", "bil\u00b7det", "mich", "zur", "Ruh", "!"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "$(", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Doch \u2013 Falschheit \u2013 und Beschwerde \u2013!", "tokens": ["Doch", "\u2013", "Falschheit", "\u2013", "und", "Be\u00b7schwer\u00b7de", "\u2013", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["KON", "$(", "NN", "$(", "KON", "NN", "$(", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.30": {"text": "Ich Unmensch, Keines Freund! \u2013", "tokens": ["Ich", "Un\u00b7mensch", ",", "Kei\u00b7nes", "Freund", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PPER", "NN", "$,", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.31": {"text": "Was sitz' ich immer ohne Ruh \u2013", "tokens": ["Was", "sitz'", "ich", "im\u00b7mer", "oh\u00b7ne", "Ruh", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "APPR", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Und weine Blut Tyrannen zu?", "tokens": ["Und", "wei\u00b7ne", "Blut", "Ty\u00b7ran\u00b7nen", "zu", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Wer f\u00fchlte, was mich traf?", "tokens": ["Wer", "f\u00fchl\u00b7te", ",", "was", "mich", "traf", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.34": {"text": "O lieber Tod, Du k\u00f6mmst \u2013 Dich w\u00fcnscht' ich, holder Schlaf!", "tokens": ["O", "lie\u00b7ber", "Tod", ",", "Du", "k\u00f6mmst", "\u2013", "Dich", "w\u00fcnscht'", "ich", ",", "hol\u00b7der", "Schlaf", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "ADV", "NN", "$,", "PPER", "VVFIN", "$(", "PPER", "VVFIN", "PPER", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}