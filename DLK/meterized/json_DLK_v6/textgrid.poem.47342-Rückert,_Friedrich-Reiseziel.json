{"textgrid.poem.47342": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Reiseziel", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun ist das Leben an seinem Ziel,", "tokens": ["Nun", "ist", "das", "Le\u00b7ben", "an", "sei\u00b7nem", "Ziel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und ohne Zweck war die Reise.", "tokens": ["Und", "oh\u00b7ne", "Zweck", "war", "die", "Rei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "O J\u00fcngling, r\u00fchre das Saitenspiel,", "tokens": ["O", "J\u00fcng\u00b7ling", ",", "r\u00fch\u00b7re", "das", "Sai\u00b7ten\u00b7spiel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Schon morgen wirst du zum Greise.", "tokens": ["Schon", "mor\u00b7gen", "wirst", "du", "zum", "Grei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Das lecke Schiff und der morsche Kiel", "tokens": ["Das", "le\u00b7cke", "Schiff", "und", "der", "mor\u00b7sche", "Kiel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "In Meeren ohne Geleise,", "tokens": ["In", "Mee\u00b7ren", "oh\u00b7ne", "Ge\u00b7lei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Der Winde Ball und der Wellen Spiel,", "tokens": ["Der", "Win\u00b7de", "Ball", "und", "der", "Wel\u00b7len", "Spiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "ART", "NN", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Unn\u00fctz gewirbelt im Kreise.", "tokens": ["Un\u00b7n\u00fctz", "ge\u00b7wir\u00b7belt", "im", "Krei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "APPRART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "So viel gehofft und gew\u00fcnscht so viel,", "tokens": ["So", "viel", "ge\u00b7hofft", "und", "ge\u00b7w\u00fcnscht", "so", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "KON", "VVPP", "ADV", "ADV", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Get\u00e4uscht in jeglicher Weise,", "tokens": ["Ge\u00b7t\u00e4uscht", "in", "jeg\u00b7li\u00b7cher", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Hindurch durchs ewige Widerspiel,", "tokens": ["Hin\u00b7durch", "durchs", "e\u00b7wi\u00b7ge", "Wi\u00b7der\u00b7spiel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Gequ\u00e4lt von Glut und von Eise.", "tokens": ["Ge\u00b7qu\u00e4lt", "von", "Glut", "und", "von", "Ei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Nun sinkt die Rose auf mattem Stiel,", "tokens": ["Nun", "sinkt", "die", "Ro\u00b7se", "auf", "mat\u00b7tem", "Stiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Bl\u00e4tter fallen vom Reise,", "tokens": ["Die", "Bl\u00e4t\u00b7ter", "fal\u00b7len", "vom", "Rei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Nun ist das Leben an seinem Ziel,", "tokens": ["Nun", "ist", "das", "Le\u00b7ben", "an", "sei\u00b7nem", "Ziel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und ohne Zweck war die Reise.", "tokens": ["Und", "oh\u00b7ne", "Zweck", "war", "die", "Rei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}