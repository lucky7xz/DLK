{"textgrid.poem.60470": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "9.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Finster sitzt, abseit vom Heere,", "tokens": ["Fins\u00b7ter", "sitzt", ",", "ab\u00b7seit", "vom", "Hee\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "ADV", "APPRART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Ein Hussit im Walde dort,", "tokens": ["Ein", "Hus\u00b7sit", "im", "Wal\u00b7de", "dort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "ADV", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Einsam in des Baches Rauschen", "tokens": ["Ein\u00b7sam", "in", "des", "Ba\u00b7ches", "Rau\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Murmelt er sein Trauerwort.", "tokens": ["Mur\u00b7melt", "er", "sein", "Trau\u00b7er\u00b7wort", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Waschend in der Flut die Waffen,", "tokens": ["Wa\u00b7schend", "in", "der", "Flut", "die", "Waf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ruft er: \u00bbHeule, B\u00e4chlein, heule!", "tokens": ["Ruft", "er", ":", "\u00bb", "Heu\u00b7le", ",", "B\u00e4ch\u00b7lein", ",", "heu\u00b7le", "!"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "$(", "NN", "$,", "NN", "$,", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ziska liegt im Zelte sterbend,", "tokens": ["Zis\u00b7ka", "liegt", "im", "Zel\u00b7te", "ster\u00b7bend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Schwingt nicht Lanze mehr, noch Keule!", "tokens": ["Schwingt", "nicht", "Lan\u00b7ze", "mehr", ",", "noch", "Keu\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "NN", "ADV", "$,", "ADV", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ziska liegt in seinem Zelte,", "tokens": ["Zis\u00b7ka", "liegt", "in", "sei\u00b7nem", "Zel\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sterbend liegt er auf dem Grunde;", "tokens": ["Ster\u00b7bend", "liegt", "er", "auf", "dem", "Grun\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch es ist kein Weibgeborner,", "tokens": ["Doch", "es", "ist", "kein", "Weib\u00b7ge\u00b7bor\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der ihm schlug die Todeswunde.", "tokens": ["Der", "ihm", "schlug", "die", "To\u00b7des\u00b7wun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ha! wie kamen sie geritten,", "tokens": ["Ha", "!", "wie", "ka\u00b7men", "sie", "ge\u00b7rit\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWAV", "VVFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einen Kampf mit ihm zu wagen,", "tokens": ["Ei\u00b7nen", "Kampf", "mit", "ihm", "zu", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hoch auf schwarzen, wei\u00dfen Rossen;", "tokens": ["Hoch", "auf", "schwar\u00b7zen", ",", "wei\u00b7\u00dfen", "Ros\u00b7sen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "APPR", "VVINF", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alle hat er sie erschlagen.", "tokens": ["Al\u00b7le", "hat", "er", "sie", "er\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Ja, der Tod, der andre M\u00e4nner", "tokens": ["Ja", ",", "der", "Tod", ",", "der", "and\u00b7re", "M\u00e4n\u00b7ner"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "ART", "NN", "$,", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Niederschmettert und zerschellt,", "tokens": ["Nie\u00b7der\u00b7schmet\u00b7tert", "und", "zer\u00b7schellt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hat dem Ziska, dem Gewaltgen,", "tokens": ["Hat", "dem", "Zis\u00b7ka", ",", "dem", "Ge\u00b7walt\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NE", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Feig und t\u00fcckisch nachgestellt.", "tokens": ["Feig", "und", "t\u00fc\u00b7ckisch", "nach\u00b7ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Heule, B\u00e4chlein, heult ihr W\u00e4lder,", "tokens": ["Heu\u00b7le", ",", "B\u00e4ch\u00b7lein", ",", "heult", "ihr", "W\u00e4l\u00b7der", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aller Welt den Schmerz zu melden,", "tokens": ["Al\u00b7ler", "Welt", "den", "Schmerz", "zu", "mel\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "B\u00f6hmen und der ganze Erdkreis", "tokens": ["B\u00f6h\u00b7men", "und", "der", "gan\u00b7ze", "Erd\u00b7kreis"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sind verwaist des gr\u00f6\u00dften Helden.\u00ab \u2013", "tokens": ["Sind", "ver\u00b7waist", "des", "gr\u00f6\u00df\u00b7ten", "Hel\u00b7den", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VAFIN", "VVFIN", "ART", "ADJA", "NN", "$.", "$(", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Ziska tr\u00f6stet die Betr\u00fcbten,", "tokens": ["Zis\u00b7ka", "tr\u00f6s\u00b7tet", "die", "Be\u00b7tr\u00fcb\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die an seinem Lager trauern:", "tokens": ["Die", "an", "sei\u00b7nem", "La\u00b7ger", "trau\u00b7ern", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbbr\u00fcder, heute werd ich sterben;", "tokens": ["\u00bb", "br\u00fc\u00b7der", ",", "heu\u00b7te", "werd", "ich", "ster\u00b7ben", ";"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKVZ", "$,", "ADV", "VAFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch die Taten werden dauern.", "tokens": ["Doch", "die", "Ta\u00b7ten", "wer\u00b7den", "dau\u00b7ern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Denn es wird in sp\u00e4ten Tagen", "tokens": ["Denn", "es", "wird", "in", "sp\u00e4\u00b7ten", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unsern Leid- und Kampfgenossen", "tokens": ["Un\u00b7sern", "Lei\u00b7d", "und", "Kampf\u00b7ge\u00b7nos\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "TRUNC", "KON", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "St\u00e4rkend aus Hussitengr\u00e4bern", "tokens": ["St\u00e4r\u00b7kend", "aus", "Hus\u00b7si\u00b7ten\u00b7gr\u00e4\u00b7bern"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Trost und gr\u00fcner Mut entsprossen.", "tokens": ["Trost", "und", "gr\u00fc\u00b7ner", "Mut", "ent\u00b7spros\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Darum sollt ihr meinem Tode", "tokens": ["Da\u00b7rum", "sollt", "ihr", "mei\u00b7nem", "To\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stark, nicht tr\u00fcb und weich erscheinen;", "tokens": ["Stark", ",", "nicht", "tr\u00fcb", "und", "weich", "er\u00b7schei\u00b7nen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PTKNEG", "ADJD", "KON", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Habt ihr nicht gelernt von Ziska,", "tokens": ["Habt", "ihr", "nicht", "ge\u00b7lernt", "von", "Zis\u00b7ka", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "VVPP", "APPR", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Keinen Toten zu beweinen?", "tokens": ["Kei\u00b7nen", "To\u00b7ten", "zu", "be\u00b7wei\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Seid gehorsam, wackre Br\u00fcder,", "tokens": ["Seid", "ge\u00b7hor\u00b7sam", ",", "wack\u00b7re", "Br\u00fc\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meinem letzten Tagsbefehle:", "tokens": ["Mei\u00b7nem", "letz\u00b7ten", "Tags\u00b7be\u00b7feh\u00b7le", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nehmt mein Sterben, nehmt mein Scheiden", "tokens": ["Nehmt", "mein", "Ster\u00b7ben", ",", "nehmt", "mein", "Schei\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hin mit heitrer Kriegerseele.", "tokens": ["Hin", "mit", "hei\u00b7trer", "Krie\u00b7ger\u00b7see\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Hochzeit ist in diesem Zelte,", "tokens": ["Hoch\u00b7zeit", "ist", "in", "die\u00b7sem", "Zel\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit der Pest bin ich getraut;", "tokens": ["Mit", "der", "Pest", "bin", "ich", "ge\u00b7traut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Furchtbar war Johannes Ziska,", "tokens": ["Furcht\u00b7bar", "war", "Jo\u00b7han\u00b7nes", "Zis\u00b7ka", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "NE", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Furchtbar auch ist seine Braut.", "tokens": ["Furcht\u00b7bar", "auch", "ist", "sei\u00b7ne", "Braut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+---+-+", "measure": "dactylic.init"}}, "stanza.12": {"line.1": {"text": "Mit der Rache hei\u00dfen Tr\u00e4umen", "tokens": ["Mit", "der", "Ra\u00b7che", "hei\u00b7\u00dfen", "Tr\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hat kein Weib mein Bett geteilt,", "tokens": ["Hat", "kein", "Weib", "mein", "Bett", "ge\u00b7teilt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sie allein, von deren Kusse", "tokens": ["Sie", "al\u00b7lein", ",", "von", "de\u00b7ren", "Kus\u00b7se"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADV", "$,", "APPR", "PRELAT", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Nimmer wird mein Herz geheilt.", "tokens": ["Nim\u00b7mer", "wird", "mein", "Herz", "ge\u00b7heilt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Da\u00df ein Teil von mir noch immer", "tokens": ["Da\u00df", "ein", "Teil", "von", "mir", "noch", "im\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "PPER", "ADV", "ADV"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "In der Schlacht den Mut euch wecke,", "tokens": ["In", "der", "Schlacht", "den", "Mut", "euch", "we\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Spannet lustig auf die Trommel", "tokens": ["Span\u00b7net", "lus\u00b7tig", "auf", "die", "Trom\u00b7mel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "meines Leibes kalte Decke.", "tokens": ["mei\u00b7nes", "Lei\u00b7bes", "kal\u00b7te", "De\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Ha! schon h\u00f6r ich Schlachten brausen;", "tokens": ["Ha", "!", "schon", "h\u00f6r", "ich", "Schlach\u00b7ten", "brau\u00b7sen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "VVFIN", "PPER", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fliehend geben sie die Sporen,", "tokens": ["Flie\u00b7hend", "ge\u00b7ben", "sie", "die", "Spo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da den Feinden mein Verm\u00e4chtnis", "tokens": ["Da", "den", "Fein\u00b7den", "mein", "Ver\u00b7m\u00e4cht\u00b7nis"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schrecken trommelt in die Ohren.\u00ab", "tokens": ["Schre\u00b7cken", "trom\u00b7melt", "in", "die", "Oh\u00b7ren", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Also sprach er, wieder sinkt er", "tokens": ["Al\u00b7so", "sprach",