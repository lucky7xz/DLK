{"textgrid.poem.65425": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Die Verlobung", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn ", "tokens": ["Wenn"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Goldne Locken, warum wollt ihr tausend um das Herz mir winden?", "tokens": ["Gold\u00b7ne", "Lo\u00b7cken", ",", "wa\u00b7rum", "wollt", "ihr", "tau\u00b7send", "um", "das", "Herz", "mir", "win\u00b7den", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PWAV", "VMFIN", "PPER", "CARD", "APPR", "ART", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "M\u00e4dchen, mit so vielen Ringen hast du dich verlobt an mich:", "tokens": ["M\u00e4d\u00b7chen", ",", "mit", "so", "vie\u00b7len", "Rin\u00b7gen", "hast", "du", "dich", "ver\u00b7lobt", "an", "mich", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ADV", "PIAT", "NN", "VAFIN", "PPER", "PRF", "VVFIN", "APPR", "PPER", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "La\u00df es dich nicht mehr verdrie\u00dfen, nenn' ich nun die Meine dich.", "tokens": ["La\u00df", "es", "dich", "nicht", "mehr", "ver\u00b7drie\u00b7\u00dfen", ",", "nenn'", "ich", "nun", "die", "Mei\u00b7ne", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PRF", "PTKNEG", "ADV", "VVINF", "$,", "VVFIN", "PPER", "ADV", "ART", "PPOSAT", "PPER", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "tr