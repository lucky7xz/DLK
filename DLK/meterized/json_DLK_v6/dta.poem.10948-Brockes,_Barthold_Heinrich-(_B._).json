{"dta.poem.10948": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "( B. )", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So weit geht meine Meynung nicht. Nur ist mir dieses", "tokens": ["So", "weit", "geht", "mei\u00b7ne", "Mey\u00b7nung", "nicht", ".", "Nur", "ist", "mir", "die\u00b7ses"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "PPOSAT", "NN", "PTKNEG", "$.", "ADV", "VAFIN", "PPER", "PDAT"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df, da wir sonst auf Gottes Werken nur gar zu selten un-", "tokens": ["Da\u00df", ",", "da", "wir", "sonst", "auf", "Got\u00b7tes", "Wer\u00b7ken", "nur", "gar", "zu", "sel\u00b7ten", "un"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "$,", "KOUS", "PPER", "ADV", "APPR", "NN", "NN", "ADV", "ADV", "PTKA", "ADJD", "TRUNC"], "meter": "-+-+-+----+-+-+", "measure": "unknown.measure.hexa"}, "line.3": {"text": "Und in demselben die Betrachtung noch minder auf den Mei-", "tokens": ["Und", "in", "dem\u00b7sel\u00b7ben", "die", "Be\u00b7trach\u00b7tung", "noch", "min\u00b7der", "auf", "den", "Mei"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PDAT", "ART", "NN", "ADV", "ADV", "APPR", "ART", "TRUNC"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Man, wenn es einmal noch geschicht, durchs Wort Natur so", "tokens": ["Man", ",", "wenn", "es", "ein\u00b7mal", "noch", "ge\u00b7schicht", ",", "durchs", "Wort", "Na\u00b7tur", "so"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "$,", "KOUS", "PPER", "ADV", "ADV", "VVPP", "$,", "APPRART", "NN", "NN", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Vom Sch\u00f6pfer unbedachtsam zieht; und wenn man einmal", "tokens": ["Vom", "Sch\u00f6p\u00b7fer", "un\u00b7be\u00b7dacht\u00b7sam", "zieht", ";", "und", "wenn", "man", "ein\u00b7mal"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADJD", "VVFIN", "$.", "KON", "KOUS", "PIS", "ADV"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.6": {"text": "Von Gottes wunderbaren Werken, und ihre Pracht gezwungen", "tokens": ["Von", "Got\u00b7tes", "wun\u00b7der\u00b7ba\u00b7ren", "Wer\u00b7ken", ",", "und", "ih\u00b7re", "Pracht", "ge\u00b7zwun\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ADJA", "NN", "$,", "KON", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+----+-+-+-", "measure": "unknown.measure.hexa"}, "line.7": {"text": "Man meynt, genug gethan zu haben, wenn man mit kurzen", "tokens": ["Man", "meynt", ",", "ge\u00b7nug", "ge\u00b7than", "zu", "ha\u00b7ben", ",", "wenn", "man", "mit", "kur\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "$,", "ADV", "VVPP", "PTKZU", "VAINF", "$,", "KOUS", "PIS", "APPR", "ADJA"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "(vom Sch\u00f6pfer nicht) wenns hoch k\u00f6mmt, spricht: ", "tokens": ["(", "vom", "Sch\u00f6p\u00b7fer", "nicht", ")", "wenns", "hoch", "k\u00f6mmt", ",", "spricht", ":"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "APPRART", "NN", "PTKNEG", "$(", "KOUS", "ADJD", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "wunderbar ist die Natur!", "tokens": ["wun\u00b7der\u00b7bar", "ist", "die", "Na\u00b7tur", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}