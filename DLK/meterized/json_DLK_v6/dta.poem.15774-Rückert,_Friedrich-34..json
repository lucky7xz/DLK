{"dta.poem.15774": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "34.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was allerbestes je von Weisen ward gesprochen,", "tokens": ["Was", "al\u00b7ler\u00b7bes\u00b7tes", "je", "von", "Wei\u00b7sen", "ward", "ge\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "ADV", "APPR", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie ein lebend'ger Quell aus ihrer Brust gebrochen,", "tokens": ["Wie", "ein", "le\u00b7ben\u00b7d'\u00b7ger", "Quell", "aus", "ih\u00b7rer", "Brust", "ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "War nie so allgemein gemeint alswie es scheint,", "tokens": ["War", "nie", "so", "all\u00b7ge\u00b7mein", "ge\u00b7meint", "als\u00b7wie", "es", "scheint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADJD", "VVPP", "KON", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein ganz besonderes war stets damit vereint.", "tokens": ["Ein", "ganz", "be\u00b7son\u00b7de\u00b7res", "war", "stets", "da\u00b7mit", "ver\u00b7eint", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "VAFIN", "ADV", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch das Besondre hat sich unserm Blick entzogen,", "tokens": ["Doch", "das", "Be\u00b7sond\u00b7re", "hat", "sich", "un\u00b7serm", "Blick", "ent\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "PRF", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die allgemeine Kraft ist nicht damit entflogen.", "tokens": ["Die", "all\u00b7ge\u00b7mei\u00b7ne", "Kraft", "ist", "nicht", "da\u00b7mit", "ent\u00b7flo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PTKNEG", "PAV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Gerade da\u00df es auf besonderm Grunde ruht,", "tokens": ["Ge\u00b7ra\u00b7de", "da\u00df", "es", "auf", "be\u00b7son\u00b7derm", "Grun\u00b7de", "ruht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Macht da\u00df es nun die allgemeine Wirkung thut.", "tokens": ["Macht", "da\u00df", "es", "nun", "die", "all\u00b7ge\u00b7mei\u00b7ne", "Wir\u00b7kung", "thut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOUS", "PPER", "ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Aus Herzbedarf f\u00fcr Herzbedarf war es gesprochen,", "tokens": ["Aus", "Herz\u00b7be\u00b7darf", "f\u00fcr", "Herz\u00b7be\u00b7darf", "war", "es", "ge\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NE", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Das f\u00fchlt dein Herz heraus und mu\u00df Theilnahme pochen.", "tokens": ["Das", "f\u00fchlt", "dein", "Herz", "he\u00b7raus", "und", "mu\u00df", "Theil\u00b7nah\u00b7me", "po\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "PTKVZ", "KON", "VMFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}