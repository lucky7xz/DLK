{"dta.poem.23740": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "Die wahre Gr\u00f6\u00dfe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1767", "urn": "urn:nbn:de:kobv:b4-20643-5", "language": ["de:0.99"], "booktitle": "Wei\u00dfe, Christian Felix: Lieder f\u00fcr Kinder. Leipzig, 1767."}, "poem": {"stanza.1": {"line.1": {"text": "Der Krieger d\u00fcrstet nach Ehre", "tokens": ["Der", "Krie\u00b7ger", "d\u00fcrs\u00b7tet", "nach", "Eh\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "In eisernem Feld,", "tokens": ["In", "ei\u00b7ser\u00b7nem", "Feld", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Und glaubt, er bau ihr Alt\u00e4re,", "tokens": ["Und", "glaubt", ",", "er", "bau", "ihr", "Al\u00b7t\u00e4\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PPER", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wenn mancher edle Held", "tokens": ["Wenn", "man\u00b7cher", "ed\u00b7le", "Held"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PIAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Von seinem Schwerdtstreich f\u00e4llt.", "tokens": ["Von", "sei\u00b7nem", "Schwerdt\u00b7streich", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und wenn er L\u00e4nder verw\u00fcstet", "tokens": ["Und", "wenn", "er", "L\u00e4n\u00b7der", "ver\u00b7w\u00fcs\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "NN", "VVPP"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und St\u00e4dte verbrannt,", "tokens": ["Und", "St\u00e4d\u00b7te", "ver\u00b7brannt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVPP", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Und sich auf Leichen gebr\u00fcstet", "tokens": ["Und", "sich", "auf", "Lei\u00b7chen", "ge\u00b7br\u00fcs\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PRF", "APPR", "NN", "VVPP"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Mit Blut bespritzter Hand;", "tokens": ["Mit", "Blut", "be\u00b7spritz\u00b7ter", "Hand", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wird er oft ", "tokens": ["Wird", "er", "oft"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV"], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Doch wer sich selber bestreitet,", "tokens": ["Doch", "wer", "sich", "sel\u00b7ber", "be\u00b7strei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PRF", "ADV", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die Tugend verehrt,", "tokens": ["Die", "Tu\u00b7gend", "ver\u00b7ehrt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Um sich das Gl\u00fccke verbreitet,", "tokens": ["Um", "sich", "das", "Gl\u00fc\u00b7cke", "ver\u00b7brei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PRF", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und durch sein Beyspiel lehrt,", "tokens": ["Und", "durch", "sein", "Bey\u00b7spiel", "lehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ist nur des Ramens werth.", "tokens": ["Ist", "nur", "des", "Ra\u00b7mens", "werth", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ADJD", "$."], "meter": "---+-+", "measure": "unknown.measure.di"}}}}}