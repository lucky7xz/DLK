{"textgrid.poem.33832": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Warnung", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "H\u00fcte sich, wer klug ist, sehr,", "tokens": ["H\u00fc\u00b7te", "sich", ",", "wer", "klug", "ist", ",", "sehr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "PRF", "$,", "PWS", "ADJD", "VAFIN", "$,", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "G\u00f6tzen b\u00f6slich anzur\u00fchren,", "tokens": ["G\u00f6t\u00b7zen", "b\u00f6s\u00b7lich", "an\u00b7zu\u00b7r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVIZU", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn er b\u00fc\u00dft den Frevel schwer,", "tokens": ["Denn", "er", "b\u00fc\u00dft", "den", "Fre\u00b7vel", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird es lange noch versp\u00fcren!", "tokens": ["Wird", "es", "lan\u00b7ge", "noch", "ver\u00b7sp\u00fc\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Dann erst, wenn die Erde bebt,", "tokens": ["Dann", "erst", ",", "wenn", "die", "Er\u00b7de", "bebt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Blitzstrahl f\u00e4hrt in Riesen\u00e4ste,", "tokens": ["Blitz\u00b7strahl", "f\u00e4hrt", "in", "Rie\u00b7sen\u00b7\u00e4s\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Tempel st\u00fcrzen und Pal\u00e4ste,", "tokens": ["Tem\u00b7pel", "st\u00fcr\u00b7zen", "und", "Pa\u00b7l\u00e4s\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Tausende der Schutt begr\u00e4bt,", "tokens": ["Tau\u00b7sen\u00b7de", "der", "Schutt", "be\u00b7gr\u00e4bt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Dann erst darf man ohne Zagen", "tokens": ["Dann", "erst", "darf", "man", "oh\u00b7ne", "Za\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VMFIN", "PIS", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Auch die G\u00f6tzen niederschlagen", "tokens": ["Auch", "die", "G\u00f6t\u00b7zen", "nie\u00b7der\u00b7schla\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Uns zugef\u00fcgter Schimpf, den nicht", "tokens": ["Uns", "zu\u00b7ge\u00b7f\u00fcg\u00b7ter", "Schimpf", ",", "den", "nicht"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ADJA", "NN", "$,", "PRELS", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wir selbst verschuldet, geht vorbei.", "tokens": ["Wir", "selbst", "ver\u00b7schul\u00b7det", ",", "geht", "vor\u00b7bei", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "$,", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn dich kein inneres Gericht", "tokens": ["Wenn", "dich", "kein", "in\u00b7ne\u00b7res", "Ge\u00b7richt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verklagen mu\u00df, so bist du frei.", "tokens": ["Ver\u00b7kla\u00b7gen", "mu\u00df", ",", "so", "bist", "du", "frei", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "$,", "ADV", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}