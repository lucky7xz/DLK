{"dta.poem.9323": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "Von der Ewigkeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was ist dis f\u00fcr ein schweres wort/ das marck ein bein ver-", "tokens": ["Was", "ist", "dis", "f\u00fcr", "ein", "schwe\u00b7res", "wort", "/", "das", "marck", "ein", "bein", "ver"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "PDS", "APPR", "ART", "ADJA", "NN", "$(", "ART", "NN", "ART", "PIAT", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "zehret?", "tokens": ["zeh\u00b7ret", "?"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Das selbsten der Bered samkeit das sprechen bem\u0303t und wehret?", "tokens": ["Das", "selbs\u00b7ten", "der", "Be\u00b7red", "sam\u00b7keit", "das", "spre\u00b7chen", "bem\u0303t", "und", "weh\u00b7ret", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "Vor dem die welt erzittern mu\u00df/ vor dem ich tag und nacht/", "tokens": ["Vor", "dem", "die", "welt", "er\u00b7zit\u00b7tern", "mu\u00df", "/", "vor", "dem", "ich", "tag", "und", "nacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "VVINF", "VMFIN", "$(", "APPR", "PRELS", "PPER", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.5": {"text": "Wenn gleich kein einig stern noch licht am hohen Himmel", "tokens": ["Wenn", "gleich", "kein", "ei\u00b7nig", "stern", "noch", "licht", "am", "ho\u00b7hen", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PIAT", "ADJD", "VVFIN", "ADV", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "wacht/", "tokens": ["wacht", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Gantz schlaffle\u00df und voll schrecken bin? das mier erregt viel", "tokens": ["Gantz", "schlaf\u00b7fle\u00df", "und", "voll", "schre\u00b7cken", "bin", "?", "das", "mier", "er\u00b7regt", "viel"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "KON", "ADJD", "VVINF", "VAFIN", "$.", "PRELS", "PPER", "VVFIN", "ADV"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.8": {"text": "schmertzen/", "tokens": ["schmert\u00b7zen", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "Das durch sein langes ewig-seyn ein pfeil in meinem hertzen/", "tokens": ["Das", "durch", "sein", "lan\u00b7ges", "e\u00b7wig\u00b7seyn", "ein", "pfeil", "in", "mei\u00b7nem", "hert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "PPOSAT", "ADJA", "NN", "ART", "NN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.10": {"text": "Das ich durch diesen Reim und Vers/ wie lang er auch mag", "tokens": ["Das", "ich", "durch", "die\u00b7sen", "Reim", "und", "Vers", "/", "wie", "lang", "er", "auch", "mag"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "PPER", "APPR", "PDAT", "NN", "KON", "NN", "$(", "PWAV", "ADJD", "PPER", "ADV", "VMFIN"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.11": {"text": "seyn/", "tokens": ["seyn", "/"], "token_info": ["word", "punct"], "pos": ["VAINF", "$("], "meter": "+", "measure": "single.up"}, "line.12": {"text": "Mitt nichten doch aussprechen kann/ ", "tokens": ["Mitt", "nich\u00b7ten", "doch", "aus\u00b7spre\u00b7chen", "kann", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Ein wort von staal und Deamant/ so Ewigkeit genennet", "tokens": ["Ein", "wort", "von", "staal", "und", "Dea\u00b7mant", "/", "so", "E\u00b7wig\u00b7keit", "ge\u00b7nen\u00b7net"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJD", "KON", "NN", "$(", "ADV", "NN", "VVPP"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "F\u00fcr dem der Him\u0303el selbsten sich noch viel zuschwach erkennet/", "tokens": ["F\u00fcr", "dem", "der", "Him\u0303el", "selbs\u00b7ten", "sich", "noch", "viel", "zu\u00b7schwach", "er\u00b7ken\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "VVFIN", "PRF", "ADV", "ADV", "ADV", "VVFIN", "$("], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.15": {"text": "Ein wort so hertz und sinn erregt nach unverg\u00e4ngligkeit", "tokens": ["Ein", "wort", "so", "hertz", "und", "sinn", "er\u00b7regt", "nach", "un\u00b7ver\u00b7g\u00e4n\u00b7glig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "ADJD", "KON", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.16": {"text": "Mit feuriger begier zustehn; ein End unendlich weit.", "tokens": ["Mit", "feu\u00b7ri\u00b7ger", "be\u00b7gier", "zu\u00b7stehn", ";", "ein", "End", "un\u00b7end\u00b7lich", "weit", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$.", "ART", "NN", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.17": {"text": "Es wird der Hellen schwartzer schwall je mehr und mehr ent-", "tokens": ["Es", "wird", "der", "Hel\u00b7len", "schwart\u00b7zer", "schwall", "je", "mehr", "und", "mehr", "ent"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJA", "NN", "ADV", "ADV", "KON", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "z\u00fcndet/", "tokens": ["z\u00fcn\u00b7det", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.19": {"text": "Das Feuer brennt und wird doch nicht die Endschafft ausge-", "tokens": ["Das", "Feu\u00b7er", "brennt", "und", "wird", "doch", "nicht", "die", "End\u00b7schafft", "aus\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "KON", "VAFIN", "ADV", "PTKNEG", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "gr\u00fcndet/", "tokens": ["gr\u00fcn\u00b7det", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.21": {"text": "Es schmeckt nach lauter Ewigkeit so nimmermehr zerrinnt/", "tokens": ["Es", "schmeckt", "nach", "lau\u00b7ter", "E\u00b7wig\u00b7keit", "so", "nim\u00b7mer\u00b7mehr", "zer\u00b7rinnt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "ADV", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.22": {"text": "Das dreygeschnautzte Hellenthier kein ende ja gewinnt.", "tokens": ["Das", "drey\u00b7ge\u00b7schnautz\u00b7te", "Hel\u00b7len\u00b7thier", "kein", "en\u00b7de", "ja", "ge\u00b7winnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PIAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.23": {"text": "Hingegen aber bleibet auch unendlich solche wonne/", "tokens": ["Hin\u00b7ge\u00b7gen", "a\u00b7ber", "blei\u00b7bet", "auch", "un\u00b7end\u00b7lich", "sol\u00b7che", "won\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ADV", "ADJD", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.24": {"text": "Die allen Frommen hatt bereit die Secl- und Lebens-Sonne;", "tokens": ["Die", "al\u00b7len", "From\u00b7men", "hatt", "be\u00b7reit", "die", "Se\u00b7cl", "und", "Le\u00b7bens\u00b7Son\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VAFIN", "ADJD", "ART", "TRUNC", "KON", "NN", "$."], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.25": {"text": "Dr\u00fcmd la\u00df mich ja nur b\u00fc\u00dfen hier la\u00df leiden angst vn\u0303 spott/", "tokens": ["Dr\u00fcmd", "la\u00df", "mich", "ja", "nur", "b\u00fc\u00b7\u00dfen", "hier", "la\u00df", "lei\u00b7den", "angst", "v\u00f1", "spott", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "ADV", "ADV", "VVFIN", "ADV", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$("], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.26": {"text": "Da\u00df ich in freuden ewig sey hernach dey dier/ ", "tokens": ["Da\u00df", "ich", "in", "freu\u00b7den", "e\u00b7wig", "sey", "her\u00b7nach", "dey", "dier", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "ADJD", "VAFIN", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}