{"textgrid.poem.66048": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Konfirmation", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Warst eben aus der Kirche gekommen,", "tokens": ["Warst", "e\u00b7ben", "aus", "der", "Kir\u00b7che", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das junge Herz noch heilig beklommen,", "tokens": ["Das", "jun\u00b7ge", "Herz", "noch", "hei\u00b7lig", "be\u00b7klom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Aber doch wieder weltlich so weit,", "tokens": ["A\u00b7ber", "doch", "wie\u00b7der", "welt\u00b7lich", "so", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADJD", "ADV", "ADJD", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Da\u00df du mir zur linken Seit", "tokens": ["Da\u00df", "du", "mir", "zur", "lin\u00b7ken", "Seit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Brav Br\u00f6tchen schmaustest am Fr\u00fchst\u00fcckstisch,", "tokens": ["Brav", "Br\u00f6t\u00b7chen", "schmaus\u00b7test", "am", "Fr\u00fch\u00b7st\u00fccks\u00b7tisch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Und nach dem Br\u00f6tchen frech und frisch", "tokens": ["Und", "nach", "dem", "Br\u00f6t\u00b7chen", "frech", "und", "frisch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Das gr\u00f6\u00dfte St\u00fcck nahmst von der Torte.", "tokens": ["Das", "gr\u00f6\u00df\u00b7te", "St\u00fcck", "nahmst", "von", "der", "Tor\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Zur Rechten mir sprach w\u00fcrdige Worte", "tokens": ["Zur", "Rech\u00b7ten", "mir", "sprach", "w\u00fcr\u00b7di\u00b7ge", "Wor\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "PPER", "VVFIN", "ADJA", "NN"], "meter": "-+--++--+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Der Gro\u00dfpapa. Ihm hatte vor allen", "tokens": ["Der", "Gro\u00df\u00b7pa\u00b7pa", ".", "Ihm", "hat\u00b7te", "vor", "al\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$.", "PPER", "VAFIN", "APPR", "PIAT"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Des Herrn Pastoren Text gefallen:", "tokens": ["Des", "Herrn", "Pas\u00b7to\u00b7ren", "Text", "ge\u00b7fal\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "\u00bbder Glaube macht es, der Glaube allein.", "tokens": ["\u00bb", "der", "Glau\u00b7be", "macht", "es", ",", "der", "Glau\u00b7be", "al\u00b7lein", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "PPER", "$,", "ART", "NN", "ADV", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Des sollen wir denn getr\u00f6stet sein,", "tokens": ["Des", "sol\u00b7len", "wir", "denn", "ge\u00b7tr\u00f6s\u00b7tet", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VMFIN", "PPER", "ADV", "VVPP", "VAINF", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Nicht lassen durch Spott und Hohn uns rauben", "tokens": ["Nicht", "las\u00b7sen", "durch", "Spott", "und", "Hohn", "uns", "rau\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "VVINF", "APPR", "NN", "KON", "NN", "PPER", "VVINF"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Den wahren, einf\u00e4ltigen Christenglauben.\u00ab", "tokens": ["Den", "wah\u00b7ren", ",", "ein\u00b7f\u00e4l\u00b7ti\u00b7gen", "Chris\u00b7ten\u00b7glau\u00b7ben", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$.", "$("], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.15": {"text": "Der Papa nach seiner stillen Art", "tokens": ["Der", "Pa\u00b7pa", "nach", "sei\u00b7ner", "stil\u00b7len", "Art"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.16": {"text": "L\u00e4chelt in den wei\u00dfen Bart:", "tokens": ["L\u00e4\u00b7chelt", "in", "den", "wei\u00b7\u00dfen", "Bart", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "\u00bbich w\u00fcrde das alles auch unterschreiben,", "tokens": ["\u00bb", "ich", "w\u00fcr\u00b7de", "das", "al\u00b7les", "auch", "un\u00b7ter\u00b7schrei\u00b7ben", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "PDS", "PIS", "ADV", "VVINF", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.18": {"text": "W\u00e4r ich Pastor. Bins nicht, la\u00df es bleiben.\u00ab", "tokens": ["W\u00e4r", "ich", "Pas\u00b7tor", ".", "Bins", "nicht", ",", "la\u00df", "es", "blei\u00b7ben", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPER", "NN", "$.", "NN", "PTKNEG", "$,", "VVIMP", "PPER", "VVINF", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.19": {"text": "Auch ich h\u00e4tt gern vermerkt, was ich dacht,", "tokens": ["Auch", "ich", "h\u00e4tt", "gern", "ver\u00b7merkt", ",", "was", "ich", "dacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "ADV", "VVPP", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.20": {"text": "Aber es war nicht angebracht.", "tokens": ["A\u00b7ber", "es", "war", "nicht", "an\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PTKNEG", "VVPP", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.21": {"text": "Achte den Glauben nicht gering,", "tokens": ["Ach\u00b7te", "den", "Glau\u00b7ben", "nicht", "ge\u00b7ring", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKNEG", "ADJD", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.22": {"text": "Es ist um den Glauben ein trefflich Ding,", "tokens": ["Es", "ist", "um", "den", "Glau\u00b7ben", "ein", "treff\u00b7lich", "Ding", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "ART", "ADJD", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.23": {"text": "Und ging er dir \u00fcber in Fleisch und Blut,", "tokens": ["Und", "ging", "er", "dir", "\u00fc\u00b7ber", "in", "Fleisch", "und", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "APPR", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.24": {"text": "F\u00e4hrst du wahrlich mit ihm gut.", "tokens": ["F\u00e4hrst", "du", "wahr\u00b7lich", "mit", "ihm", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.25": {"text": "Von mir schon l\u00e4ngst er Abschied nahm,", "tokens": ["Von", "mir", "schon", "l\u00e4ngst", "er", "Ab\u00b7schied", "nahm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "VVFIN", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Irgendwo mir abhanden kam.", "tokens": ["Ir\u00b7gend\u00b7wo", "mir", "ab\u00b7han\u00b7den", "kam", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVINF", "VVFIN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.27": {"text": "Sind mir nur die zwei andern geblieben:", "tokens": ["Sind", "mir", "nur", "die", "zwei", "an\u00b7dern", "ge\u00b7blie\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "CARD", "ADJA", "VVPP", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.28": {"text": "Das Hoffen, Kind, und das Lieben, das Lieben.", "tokens": ["Das", "Hof\u00b7fen", ",", "Kind", ",", "und", "das", "Lie\u00b7ben", ",", "das", "Lie\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "$,", "KON", "ART", "ADJA", "$,", "ART", "ADJA", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Mit diesen beiden kam ich bisher", "tokens": ["Mit", "die\u00b7sen", "bei\u00b7den", "kam", "ich", "bis\u00b7her"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "PIAT", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Leidlich zurecht, oft etwas f\u00fcr quer,", "tokens": ["Leid\u00b7lich", "zu\u00b7recht", ",", "oft", "et\u00b7was", "f\u00fcr", "quer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$,", "ADV", "ADV", "APPR", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Aber alles in allem genommen,", "tokens": ["A\u00b7ber", "al\u00b7les", "in", "al\u00b7lem", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "APPR", "PIS", "VVPP", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Bin ich dabei zu Gewinn gekommen,", "tokens": ["Bin", "ich", "da\u00b7bei", "zu", "Ge\u00b7winn", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PAV", "APPR", "NN", "VVPP", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.5": {"text": "Und m\u00f6chte im Leben nicht anders fahren,", "tokens": ["Und", "m\u00f6ch\u00b7te", "im", "Le\u00b7ben", "nicht", "an\u00b7ders", "fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPRART", "NN", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Und bin doch schon einigerma\u00dfen bei Jahren.", "tokens": ["Und", "bin", "doch", "schon", "ei\u00b7ni\u00b7ger\u00b7ma\u00b7\u00dfen", "bei", "Jah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "ADV", "APPR", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.7": {"text": "Du hast nun die Schule erst hinter dir,", "tokens": ["Du", "hast", "nun", "die", "Schu\u00b7le", "erst", "hin\u00b7ter", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "ADV", "APPR", "PPER", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Die Welt liegt vor dir, ein bl\u00fchend Revier,", "tokens": ["Die", "Welt", "liegt", "vor", "dir", ",", "ein", "bl\u00fc\u00b7hend", "Re\u00b7vier", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "$,", "ART", "ADJD", "NN", "$,"], "meter": "-+--+-+---", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "In das deine sch\u00f6nen, gro\u00dfen, grauen", "tokens": ["In", "das", "dei\u00b7ne", "sch\u00f6\u00b7nen", ",", "gro\u00b7\u00dfen", ",", "grau\u00b7en"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["APPR", "ART", "PPOSAT", "ADJA", "$,", "ADJA", "$,", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Augen erwartend und ahnend schauen.", "tokens": ["Au\u00b7gen", "er\u00b7war\u00b7tend", "und", "ah\u00b7nend", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "KON", "ADJD", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Dein schwarzes Kleid, dein s\u00fc\u00dfes Gesicht,", "tokens": ["Dein", "schwar\u00b7zes", "Kleid", ",", "dein", "s\u00fc\u00b7\u00dfes", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Deines Z\u00f6pfchens blondbraun Licht,", "tokens": ["Dei\u00b7nes", "Z\u00f6pf\u00b7chens", "blond\u00b7braun", "Licht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dein kindlich Wesen, dein schwellender Mund,", "tokens": ["Dein", "kind\u00b7lich", "We\u00b7sen", ",", "dein", "schwel\u00b7len\u00b7der", "Mund", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Dein junger Leib, frisch und gesund:", "tokens": ["Dein", "jun\u00b7ger", "Leib", ",", "frisch", "und", "ge\u00b7sund", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie ich so neben dir sitze, geht", "tokens": ["Wie", "ich", "so", "ne\u00b7ben", "dir", "sit\u00b7ze", ",", "geht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PWAV", "PPER", "ADV", "APPR", "PPER", "VVFIN", "$,", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Es wie ein tief und fromm Gebet", "tokens": ["Es", "wie", "ein", "tief", "und", "fromm", "Ge\u00b7bet"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "KOKOM", "ART", "ADJD", "KON", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Durch mein ungl\u00e4ubig Herz, und leis,", "tokens": ["Durch", "mein", "un\u00b7gl\u00e4u\u00b7big", "Herz", ",", "und", "leis", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJD", "NN", "$,", "KON", "ADJD", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.8": {"text": "Verstohlen, nach frommer Beter Weis,", "tokens": ["Ver\u00b7stoh\u00b7len", ",", "nach", "from\u00b7mer", "Be\u00b7ter", "Weis", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Kreuz ich die Finger: Hoffen und Lieben,", "tokens": ["Kreuz", "ich", "die", "Fin\u00b7ger", ":", "Hof\u00b7fen", "und", "Lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "$.", "NN", "KON", "ADJA", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.10": {"text": "Die treu mir alle Tage geblieben,", "tokens": ["Die", "treu", "mir", "al\u00b7le", "Ta\u00b7ge", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPER", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Wendet auch diesem Kind euch zu,", "tokens": ["Wen\u00b7det", "auch", "die\u00b7sem", "Kind", "euch", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PDAT", "NN", "PPER", "PTKVZ", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.12": {"text": "Streut eure Rosen vor seinem Schuh,", "tokens": ["Streut", "eu\u00b7re", "Ro\u00b7sen", "vor", "sei\u00b7nem", "Schuh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN"