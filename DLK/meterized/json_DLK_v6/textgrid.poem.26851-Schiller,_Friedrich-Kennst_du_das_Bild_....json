{"textgrid.poem.26851": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Kennst du das Bild ...", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kennst du das Bild auf zartem Grunde,", "tokens": ["Kennst", "du", "das", "Bild", "auf", "zar\u00b7tem", "Grun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Es gibt sich selber Licht und Glanz.", "tokens": ["Es", "gibt", "sich", "sel\u00b7ber", "Licht", "und", "Glanz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein andres ists zu jeder Stunde,", "tokens": ["Ein", "and\u00b7res", "ists", "zu", "je\u00b7der", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VAFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und immer ist es frisch und ganz.", "tokens": ["Und", "im\u00b7mer", "ist", "es", "frisch", "und", "ganz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADJD", "KON", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Im engsten Raum ists ausgef\u00fchret,", "tokens": ["Im", "engs\u00b7ten", "Raum", "ists", "aus\u00b7ge\u00b7f\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der kleinste Rahmen fa\u00dft es ein,", "tokens": ["Der", "kleins\u00b7te", "Rah\u00b7men", "fa\u00dft", "es", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Doch alle Gr\u00f6\u00dfe, die dich r\u00fchret,", "tokens": ["Doch", "al\u00b7le", "Gr\u00f6\u00b7\u00dfe", ",", "die", "dich", "r\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Kennst du durch dieses Bild allein.", "tokens": ["Kennst", "du", "durch", "die\u00b7ses", "Bild", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PDAT", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und kannst du den Kristall mir nennen,", "tokens": ["Und", "kannst", "du", "den", "Kris\u00b7tall", "mir", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.10": {"text": "Ihm gleicht an Wert kein Edelstein,", "tokens": ["Ihm", "gleicht", "an", "Wert", "kein", "E\u00b7del\u00b7stein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NE", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Er leuchtet, ohne je zu brennen,", "tokens": ["Er", "leuch\u00b7tet", ",", "oh\u00b7ne", "je", "zu", "bren\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUI", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Das ganze Weltall saugt er ein.", "tokens": ["Das", "gan\u00b7ze", "Wel\u00b7tall", "saugt", "er", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Der Himmel selbst ist abgemalet", "tokens": ["Der", "Him\u00b7mel", "selbst", "ist", "ab\u00b7ge\u00b7ma\u00b7let"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VAFIN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "In seinem wundervollen Ring,", "tokens": ["In", "sei\u00b7nem", "wun\u00b7der\u00b7vol\u00b7len", "Ring", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Und doch ist, was er von sich strahlet,", "tokens": ["Und", "doch", "ist", ",", "was", "er", "von", "sich", "strah\u00b7let", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "$,", "PWS", "PPER", "APPR", "PRF", "VVFIN", "$,"], "meter": "--+-++-+-", "measure": "anapaest.init"}, "line.16": {"text": "Noch sch\u00f6ner, als was er empfing.", "tokens": ["Noch", "sch\u00f6\u00b7ner", ",", "als", "was", "er", "emp\u00b7fing", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dies zarte Bild, das in den kleinsten Rahmen", "tokens": ["Dies", "zar\u00b7te", "Bild", ",", "das", "in", "den", "kleins\u00b7ten", "Rah\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADJA", "NN", "$,", "PRELS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gefa\u00dft, das Unerme\u00dfliche uns zeigt,", "tokens": ["Ge\u00b7fa\u00dft", ",", "das", "Un\u00b7er\u00b7me\u00df\u00b7li\u00b7che", "uns", "zeigt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und der Kristall, in dem dies Bild sich malt,", "tokens": ["Und", "der", "Kris\u00b7tall", ",", "in", "dem", "dies", "Bild", "sich", "malt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "APPR", "ART", "PDS", "NN", "PRF", "VVFIN", "$,"], "meter": "--++-+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Und der noch Sch\u00f6nres von sich strahlt,", "tokens": ["Und", "der", "noch", "Sch\u00f6n\u00b7res", "von", "sich", "strahlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADV", "NN", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er ist das ", "tokens": ["Er", "ist", "das"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}}}}}