{"dta.poem.5279": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "J rdisches  \n  V ergn\u00fcgen  \n in  \n  GOTT .  \n  F \u00fcnfter  T heil.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es scheint, ob trag\u2019 ein still-bedeckter Himmel", "tokens": ["Es", "scheint", ",", "ob", "trag'", "ein", "still\u00b7be\u00b7deck\u00b7ter", "Him\u00b7mel"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In holder D\u00e4mmrung ohn Get\u00fcmmel,", "tokens": ["In", "hol\u00b7der", "D\u00e4mm\u00b7rung", "ohn", "Ge\u00b7t\u00fcm\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Zu einer Art Gelassenheit", "tokens": ["Zu", "ei\u00b7ner", "Art", "Ge\u00b7las\u00b7sen\u00b7heit"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In unsern Hertzen etwas bey;", "tokens": ["In", "un\u00b7sern", "Hert\u00b7zen", "et\u00b7was", "bey", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und find ich, da\u00df dergleichen tr\u00fcbe Zeit", "tokens": ["Und", "find", "ich", ",", "da\u00df", "derg\u00b7lei\u00b7chen", "tr\u00fc\u00b7be", "Zeit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "$,", "KOUS", "PIS", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Jm Fr\u00fchling ebenfals nicht unvergn\u00fcglich sey.", "tokens": ["Jm", "Fr\u00fch\u00b7ling", "e\u00b7ben\u00b7fals", "nicht", "un\u00b7ver\u00b7gn\u00fcg\u00b7lich", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "PTKNEG", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}