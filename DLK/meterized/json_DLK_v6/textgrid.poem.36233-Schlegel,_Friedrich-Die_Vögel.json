{"textgrid.poem.36233": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Die V\u00f6gel", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie lieblich und fr\u00f6hlich,", "tokens": ["Wie", "lieb\u00b7lich", "und", "fr\u00f6h\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Zu schweben, zu singen;", "tokens": ["Zu", "schwe\u00b7ben", ",", "zu", "sin\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Von gl\u00e4nzender H\u00f6he", "tokens": ["Von", "gl\u00e4n\u00b7zen\u00b7der", "H\u00f6\u00b7he"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Zur Erde zu blicken!", "tokens": ["Zur", "Er\u00b7de", "zu", "bli\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Die Menschen sind t\u00f6richt,", "tokens": ["Die", "Men\u00b7schen", "sind", "t\u00f6\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Sie k\u00f6nnen nicht fliegen;", "tokens": ["Sie", "k\u00f6n\u00b7nen", "nicht", "flie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Sie jammern in N\u00f6ten,", "tokens": ["Sie", "jam\u00b7mern", "in", "N\u00f6\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Wir flattern gen Himmel.", "tokens": ["Wir", "flat\u00b7tern", "gen", "Him\u00b7mel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Der J\u00e4ger will t\u00f6ten,", "tokens": ["Der", "J\u00e4\u00b7ger", "will", "t\u00f6\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Dem Fr\u00fcchte wir pickten;", "tokens": ["Dem", "Fr\u00fcch\u00b7te", "wir", "pick\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Wir m\u00fcssen ihn h\u00f6hnen,", "tokens": ["Wir", "m\u00fcs\u00b7sen", "ihn", "h\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Und Beute gewinnen.", "tokens": ["Und", "Beu\u00b7te", "ge\u00b7win\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}}}}