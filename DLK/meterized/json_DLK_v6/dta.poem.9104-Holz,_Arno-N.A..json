{"dta.poem.9104": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1898", "urn": "urn:nbn:de:kobv:b4-200905192685", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Um mein erleuchtetes Schloss wehn Cypressen.", "tokens": ["Um", "mein", "er\u00b7leuch\u00b7te\u00b7tes", "Schloss", "wehn", "Cyp\u00b7res\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN", "VVFIN", "NE", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Ich h\u00f6re sie nicht. Ich f\u00fchle sie.", "tokens": ["Ich", "h\u00f6\u00b7re", "sie", "nicht", ".", "Ich", "f\u00fch\u00b7le", "sie", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$.", "PPER", "VVFIN", "PPER", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Alle meine Lichter werden erl\u00f6schen,", "tokens": ["Al\u00b7le", "mei\u00b7ne", "Lich\u00b7ter", "wer\u00b7den", "er\u00b7l\u00f6\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "VAFIN", "VVINF", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "der letzte Geigenton verklingt,", "tokens": ["der", "letz\u00b7te", "Gei\u00b7gen\u00b7ton", "ver\u00b7klingt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "durchs Fenster", "tokens": ["durchs", "Fens\u00b7ter"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "in meinem brechenden Blick", "tokens": ["in", "mei\u00b7nem", "bre\u00b7chen\u00b7den", "Blick"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "spiegelt sich der Mond.", "tokens": ["spie\u00b7gelt", "sich", "der", "Mond", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}