{"textgrid.poem.25519": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "Bei Uebersendung einer Locke", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vor meinem Spiegel stand ich fr\u00fch,", "tokens": ["Vor", "mei\u00b7nem", "Spie\u00b7gel", "stand", "ich", "fr\u00fch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hielt Musterung der Locken, zog von allen", "tokens": ["Hielt", "Mus\u00b7te\u00b7rung", "der", "Lo\u00b7cken", ",", "zog", "von", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "NN", "ART", "NN", "$,", "VVFIN", "APPR", "PIAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Nadeln aus, da\u00df auf die Schultern sie", "tokens": ["Die", "Na\u00b7deln", "aus", ",", "da\u00df", "auf", "die", "Schul\u00b7tern", "sie"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "$,", "KOUS", "APPR", "ART", "NN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wie B\u00e4ch' herab von Felsen fallen.", "tokens": ["Wie", "B\u00e4ch'", "her\u00b7ab", "von", "Fel\u00b7sen", "fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die sch\u00f6nste sucht' ich dir heraus;", "tokens": ["Die", "sch\u00f6ns\u00b7te", "sucht'", "ich", "dir", "he\u00b7raus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PPER", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich schnitt sie ab mit deiner Bilderscheere,", "tokens": ["Ich", "schnitt", "sie", "ab", "mit", "dei\u00b7ner", "Bil\u00b7der\u00b7schee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und weinend stie\u00df ich da den Seufzer aus:", "tokens": ["Und", "wei\u00b7nend", "stie\u00df", "ich", "da", "den", "Seuf\u00b7zer", "aus", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Ach! da\u00df es eine Krone w\u00e4re!", "tokens": ["Ach", "!", "da\u00df", "es", "ei\u00b7ne", "Kro\u00b7ne", "w\u00e4\u00b7re", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "KOUS", "PPER", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Doch so \u2013 nur eine Locke, Freund!", "tokens": ["Doch", "so", "\u2013", "nur", "ei\u00b7ne", "Lo\u00b7cke", ",", "Freund", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "$(", "ADV", "ART", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Die nicht verdient, da\u00df sie hinauf sich schwinge,", "tokens": ["Die", "nicht", "ver\u00b7dient", ",", "da\u00df", "sie", "hin\u00b7auf", "sich", "schwin\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "VVPP", "$,", "KOUS", "PPER", "ADV", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Wo hell das Haar von Berenicen scheint,", "tokens": ["Wo", "hell", "das", "Haar", "von", "Be\u00b7re\u00b7ni\u00b7cen", "scheint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Noch da\u00df ein ", "tokens": ["Noch", "da\u00df", "ein"], "token_info": ["word", "word", "word"], "pos": ["ADV", "KOUS", "ART"], "meter": "+-+", "measure": "trochaic.di"}, "line.13": {"text": "Und dennoch hat sie Werth, o Mann!", "tokens": ["Und", "den\u00b7noch", "hat", "sie", "Werth", ",", "o", "Mann", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "NN", "$,", "FM", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Denn du erh\u00e4ltst mit ihr mein Herz voll Liebe;", "tokens": ["Denn", "du", "er\u00b7h\u00e4ltst", "mit", "ihr", "mein", "Herz", "voll", "Lie\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPER", "PPOSAT", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Und b\u00f6t' ein F\u00fcrst f\u00fcr ", "tokens": ["Und", "b\u00f6t'", "ein", "F\u00fcrst", "f\u00fcr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR"], "meter": "-+-+-", "measure": "iambic.di"}, "line.16": {"text": "So glaube, da\u00df die Kron' ihm bliebe.", "tokens": ["So", "glau\u00b7be", ",", "da\u00df", "die", "Kron'", "ihm", "blie\u00b7be", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "KOUS", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Freund! nimm denn meine Locke hin!", "tokens": ["Freund", "!", "nimm", "denn", "mei\u00b7ne", "Lo\u00b7cke", "hin", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVIMP", "KON", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Dann werd' ich doch, nicht ", "tokens": ["Dann", "werd'", "ich", "doch", ",", "nicht"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "$,", "PTKNEG"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.19": {"text": "Und wenn ich l\u00e4ngst ein Spiel der Winde bin,", "tokens": ["Und", "wenn", "ich", "l\u00e4ngst", "ein", "Spiel", "der", "Win\u00b7de", "bin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ART", "NN", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Wirst du von mir ", "tokens": ["Wirst", "du", "von", "mir"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "PPER"], "meter": "-+-+", "measure": "iambic.di"}}}}}