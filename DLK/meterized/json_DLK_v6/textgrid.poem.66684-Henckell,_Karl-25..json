{"textgrid.poem.66684": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "25.", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Mond, der Tr\u00e4ume Meister, schwebt im Blauen,", "tokens": ["Der", "Mond", ",", "der", "Tr\u00e4u\u00b7me", "Meis\u00b7ter", ",", "schwebt", "im", "Blau\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "NE", "$,", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf Strahlenbr\u00fccken gleitet leise hin,", "tokens": ["Auf", "Strah\u00b7len\u00b7br\u00fc\u00b7cken", "glei\u00b7tet", "lei\u00b7se", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Versunken in ein selbstverlornes Schauen,", "tokens": ["Ver\u00b7sun\u00b7ken", "in", "ein", "selbst\u00b7ver\u00b7lor\u00b7nes", "Schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der nachtgel\u00f6ste, weltallk\u00fchle Sinn.", "tokens": ["Der", "nacht\u00b7ge\u00b7l\u00f6s\u00b7te", ",", "welt\u00b7all\u00b7k\u00fch\u00b7le", "Sinn", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PWAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ich f\u00fchle feuchten Glanz herniedertauen", "tokens": ["Ich", "f\u00fch\u00b7le", "feuch\u00b7ten", "Glanz", "her\u00b7nie\u00b7der\u00b7tau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Mit silbermaschig perlendem Gespinn,", "tokens": ["Mit", "sil\u00b7ber\u00b7ma\u00b7schig", "per\u00b7len\u00b7dem", "Ge\u00b7spinn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Die B\u00e4ume tropfen von verkl\u00e4rtem Lichte,", "tokens": ["Die", "B\u00e4u\u00b7me", "trop\u00b7fen", "von", "ver\u00b7kl\u00e4r\u00b7tem", "Lich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und durch die Seele rieseln Sterngedichte.", "tokens": ["Und", "durch", "die", "See\u00b7le", "rie\u00b7seln", "Stern\u00b7ge\u00b7dich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Gefangen von des Schimmers Zaubernetze,", "tokens": ["Ge\u00b7fan\u00b7gen", "von", "des", "Schim\u00b7mers", "Zau\u00b7ber\u00b7net\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Schaukl' ich im Scho\u00dfe der Unendlichkeit ...", "tokens": ["Schau\u00b7kl'", "ich", "im", "Scho\u00b7\u00dfe", "der", "Un\u00b7end\u00b7lich\u00b7keit", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "NN", "ART", "NN", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Und meines Geistes reinste Sehnsucht letze", "tokens": ["Und", "mei\u00b7nes", "Geis\u00b7tes", "reins\u00b7te", "Sehn\u00b7sucht", "let\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "ADJA", "NN", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vollkommen ich, von W\u00fcnschen unentweiht.", "tokens": ["Voll\u00b7kom\u00b7men", "ich", ",", "von", "W\u00fcn\u00b7schen", "un\u00b7ent\u00b7weiht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wie selbstentr\u00fcckt genie\u00df' ich die Gesetze,", "tokens": ["Wie", "selbs\u00b7ten\u00b7tr\u00fcckt", "ge\u00b7ni\u00b7e\u00df'", "ich", "die", "Ge\u00b7set\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Vom Wahne meines engen Ich befreit,", "tokens": ["Vom", "Wah\u00b7ne", "mei\u00b7nes", "en\u00b7gen", "Ich", "be\u00b7freit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "ADJA", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Das sich am Tage bannt in n\u00e4chste Kreise", "tokens": ["Das", "sich", "am", "Ta\u00b7ge", "bannt", "in", "n\u00e4chs\u00b7te", "Krei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "PRF", "APPRART", "NN", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und wirkt nach seines Triebs beschr\u00e4nkter Weise.", "tokens": ["Und", "wirkt", "nach", "sei\u00b7nes", "Triebs", "be\u00b7schr\u00e4nk\u00b7ter", "Wei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Hier geh' ich auf, hier geb ich mich der Gr\u00f6\u00dfe", "tokens": ["Hier", "geh'", "ich", "auf", ",", "hier", "geb", "ich", "mich", "der", "Gr\u00f6\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "ADV", "VVFIN", "PPER", "PRF", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und ihrem unverbr\u00fcchlich sichern Gang,", "tokens": ["Und", "ih\u00b7rem", "un\u00b7ver\u00b7br\u00fcch\u00b7lich", "si\u00b7chern", "Gang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Kein Himmelsst\u00fcrmer mit Pygm\u00e4enbl\u00f6\u00dfe,", "tokens": ["Kein", "Him\u00b7mels\u00b7st\u00fcr\u00b7mer", "mit", "Pyg\u00b7m\u00e4\u00b7en\u00b7bl\u00f6\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der aufbegehrt gen \u00fcberm\u00e4chtigen Zwang.", "tokens": ["Der", "auf\u00b7be\u00b7gehrt", "gen", "\u00fc\u00b7ber\u00b7m\u00e4ch\u00b7ti\u00b7gen", "Zwang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "Ich wittre weltweit \u00c4therwellenst\u00f6\u00dfe,", "tokens": ["Ich", "witt\u00b7re", "welt\u00b7weit", "\u00c4t\u00b7her\u00b7wel\u00b7len\u00b7st\u00f6\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PWAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der feinsten Elemente Flu\u00df und Drang,", "tokens": ["Der", "feins\u00b7ten", "E\u00b7le\u00b7men\u00b7te", "Flu\u00df", "und", "Drang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Der mondesaugigen Gottheit Atemz\u00fcge", "tokens": ["Der", "mon\u00b7des\u00b7au\u00b7gi\u00b7gen", "Got\u00b7theit", "A\u00b7tem\u00b7z\u00fc\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.8": {"text": "Atm' ich im Gleichtakt, fern der Lebensl\u00fcge.", "tokens": ["Atm'", "ich", "im", "Gleich\u00b7takt", ",", "fern", "der", "Le\u00b7bens\u00b7l\u00fc\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPRART", "NN", "$,", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und meiner Eigensuchten B\u00fcrde sinken", "tokens": ["Und", "mei\u00b7ner", "Ei\u00b7gen\u00b7such\u00b7ten", "B\u00fcr\u00b7de", "sin\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "F\u00fchl' ich wie Ballast auf der Erde Grund,", "tokens": ["F\u00fchl'", "ich", "wie", "Bal\u00b7last", "auf", "der", "Er\u00b7de", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "KOKOM", "ADV", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Aus leichtern Sph\u00e4ren Allgef\u00fchl zu trinken,", "tokens": ["Aus", "leich\u00b7tern", "Sph\u00e4\u00b7ren", "All\u00b7ge\u00b7f\u00fchl", "zu", "trin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Heb' ich den Himmelsbecher an den Mund.", "tokens": ["Heb'", "ich", "den", "Him\u00b7mels\u00b7be\u00b7cher", "an", "den", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Unendlich Schl\u00fcrfen! Mondesfeen winken", "tokens": ["Un\u00b7end\u00b7lich", "Schl\u00fcr\u00b7fen", "!", "Mon\u00b7des\u00b7feen", "win\u00b7ken"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJD", "NN", "$.", "NN", "VVINF"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Und schlie\u00dfen mich in ihren Schwebebund,", "tokens": ["Und", "schlie\u00b7\u00dfen", "mich", "in", "ih\u00b7ren", "Schwe\u00b7be\u00b7bund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "In ihrem zartgewobnen Strahlenreigen", "tokens": ["In", "ih\u00b7rem", "zart\u00b7ge\u00b7wob\u00b7nen", "Strah\u00b7len\u00b7rei\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Seh ich mit mir die Liebste sternw\u00e4rts steigen.", "tokens": ["Seh", "ich", "mit", "mir", "die", "Liebs\u00b7te", "stern\u00b7w\u00e4rts", "stei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "In keuscher Formenklarheit offenbaren", "tokens": ["In", "keu\u00b7scher", "For\u00b7men\u00b7klar\u00b7heit", "of\u00b7fen\u00b7ba\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sich irdischer Bilder Umri\u00df und Gestalt,", "tokens": ["Sich", "ir\u00b7di\u00b7scher", "Bil\u00b7der", "Um\u00b7ri\u00df", "und", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJA", "NN", "NN", "KON", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "So rein, wie wir sie Tages nie gewahren,", "tokens": ["So", "rein", ",", "wie", "wir", "sie", "Ta\u00b7ges", "nie", "ge\u00b7wah\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "PWAV", "PPER", "PPER", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wo von den Dingen grell der Lichtpfeil prallt.", "tokens": ["Wo", "von", "den", "Din\u00b7gen", "grell", "der", "Licht\u00b7pfeil", "prallt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der Menschen Wohnungen wir neu gewahren", "tokens": ["Der", "Men\u00b7schen", "Woh\u00b7nun\u00b7gen", "wir", "neu", "ge\u00b7wah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wie marmorwei\u00df \u2013 mit magischer Gewalt", "tokens": ["Wie", "mar\u00b7mor\u00b7wei\u00df", "\u2013", "mit", "ma\u00b7gi\u00b7scher", "Ge\u00b7walt"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "$(", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Umgie\u00dft ein Abglanz \u00fcberirdischer Helle", "tokens": ["Um\u00b7gie\u00dft", "ein", "Ab\u00b7glanz", "\u00fc\u00b7be\u00b7rir\u00b7di\u00b7scher", "Hel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Der Villen G\u00e4rten und des Sees Welle.", "tokens": ["Der", "Vil\u00b7len", "G\u00e4r\u00b7ten", "und", "des", "Sees", "Wel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "KON", "ART", "NN", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Nun sind die Stunden, wo die Zeit vergangen", "tokens": ["Nun", "sind", "die", "Stun\u00b7den", ",", "wo", "die", "Zeit", "ver\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "PWAV", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und alles zittert unerme\u00dflich tief,", "tokens": ["Und", "al\u00b7les", "zit\u00b7tert", "un\u00b7er\u00b7me\u00df\u00b7lich", "tief", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Durchsichtige Silberschleier niederhangen", "tokens": ["Durch\u00b7sich\u00b7ti\u00b7ge", "Sil\u00b7ber\u00b7schlei\u00b7er", "nie\u00b7der\u00b7han\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVINF"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Vom Haupt der Mondfrau, die geblendet schlief \u2013", "tokens": ["Vom", "Haupt", "der", "Mond\u00b7frau", ",", "die", "ge\u00b7blen\u00b7det", "schlief", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "PRELS", "VVPP", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Nun h\u00e4lt in allumfassendem Verlangen", "tokens": ["Nun", "h\u00e4lt", "in", "al\u00b7lum\u00b7fas\u00b7sen\u00b7dem", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die Nacht in wei\u00dfer Hand den goldnen Brief,", "tokens": ["Die", "Nacht", "in", "wei\u00b7\u00dfer", "Hand", "den", "gold\u00b7nen", "Brief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Drin sie die Welt verschwenderisch begnadet,", "tokens": ["Drin", "sie", "die", "Welt", "ver\u00b7schwen\u00b7de\u00b7risch", "be\u00b7gna\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Zum Traum in blaue Sternens\u00e4le ladet ...", "tokens": ["Zum", "Traum", "in", "blau\u00b7e", "Ster\u00b7nen\u00b7s\u00e4\u00b7le", "la\u00b7det", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}