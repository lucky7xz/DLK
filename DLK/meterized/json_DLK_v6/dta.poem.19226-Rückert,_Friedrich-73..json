{"dta.poem.19226": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "73.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Betrachtest du die Welt als einzig da f\u00fcr dich,", "tokens": ["Be\u00b7trach\u00b7test", "du", "die", "Welt", "als", "ein\u00b7zig", "da", "f\u00fcr", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "KOKOM", "ADJD", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bist du ein Thier, das Thier thut ebendis f\u00fcr sich.", "tokens": ["Bist", "du", "ein", "Thier", ",", "das", "Thier", "thut", "e\u00b7ben\u00b7dis", "f\u00fcr", "sich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$,", "ART", "NN", "VVFIN", "ADV", "APPR", "PRF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nur wenn du selbst die Welt f\u00fcr sich wirst anerkennen,", "tokens": ["Nur", "wenn", "du", "selbst", "die", "Welt", "f\u00fcr", "sich", "wirst", "an\u00b7er\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ADV", "ART", "NN", "APPR", "PRF", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dich selbst auch f\u00fcr die Welt, bist du ein Mensch zu nennen.", "tokens": ["Dich", "selbst", "auch", "f\u00fcr", "die", "Welt", ",", "bist", "du", "ein", "Mensch", "zu", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "wor