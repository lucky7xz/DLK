{"dta.poem.19370": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "103.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Kind, f\u00e4llt's auf den Kopf, steht wieder auf sogleich,", "tokens": ["Ein", "Kind", ",", "f\u00e4llt's", "auf", "den", "Kopf", ",", "steht", "wie\u00b7der", "auf", "sog\u00b7leich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "APPR", "ART", "NN", "$,", "VVFIN", "ADV", "APPR", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht weil sein Hirn so hart, nur weil es ist so weich,", "tokens": ["Nicht", "weil", "sein", "Hirn", "so", "hart", ",", "nur", "weil", "es", "ist", "so", "weich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "KOUS", "PPOSAT", "NN", "ADV", "ADJD", "$,", "ADV", "KOUS", "PPER", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "So weich, um einem Druck sich schadlos zu bequemen,", "tokens": ["So", "weich", ",", "um", "ei\u00b7nem", "Druck", "sich", "schad\u00b7los", "zu", "be\u00b7que\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUI", "ART", "NN", "PRF", "ADJD", "APPR", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und gleichsam eine Form beliebig anzunehmen.", "tokens": ["Und", "gleich\u00b7sam", "ei\u00b7ne", "Form", "be\u00b7lie\u00b7big", "an\u00b7zu\u00b7neh\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "ADJD", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ein Alter, dessen Hirn sich nicht mehr so kann schmiegen,", "tokens": ["Ein", "Al\u00b7ter", ",", "des\u00b7sen", "Hirn", "sich", "nicht", "mehr", "so", "kann", "schmie\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELAT", "NN", "PRF", "PTKNEG", "ADV", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+++-+-", "measure": "unknown.measure.septa"}, "line.2": {"text": "Wo er den Kopf aufschl\u00e4gt, bleibt er bewu\u00dftlos liegen.", "tokens": ["Wo", "er", "den", "Kopf", "auf\u00b7schl\u00e4gt", ",", "bleibt", "er", "be\u00b7wu\u00dft\u00b7los", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN", "$,", "VVFIN", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Drum braucht der Mann auch nicht alswie ein Kind zu fallen;", "tokens": ["Drum", "braucht", "der", "Mann", "auch", "nicht", "als\u00b7wie", "ein", "Kind", "zu", "fal\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "ADV", "PTKNEG", "KOKOM", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn laufen lernt das Kind, der Mann bed\u00e4chtig wallen.", "tokens": ["Denn", "lau\u00b7fen", "lernt", "das", "Kind", ",", "der", "Mann", "be\u00b7d\u00e4ch\u00b7tig", "wal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VVFIN", "ART", "NN", "$,", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}