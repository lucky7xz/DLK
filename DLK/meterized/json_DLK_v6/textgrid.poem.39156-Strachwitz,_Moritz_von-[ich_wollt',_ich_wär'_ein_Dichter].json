{"textgrid.poem.39156": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "[ich wollt', ich w\u00e4r' ein Dichter]", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich wollt', ich w\u00e4r' ein Dichter,", "tokens": ["Ich", "wollt'", ",", "ich", "w\u00e4r'", "ein", "Dich\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Dichter reich und gro\u00df,", "tokens": ["Ein", "Dich\u00b7ter", "reich", "und", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Perlen, meine Lieder,", "tokens": ["Die", "Per\u00b7len", ",", "mei\u00b7ne", "Lie\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die w\u00fcrf' ich in deinen Scho\u00df.", "tokens": ["Die", "w\u00fcr\u00b7f'", "ich", "in", "dei\u00b7nen", "Scho\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Auf meiner Dichtung Bl\u00fcten,", "tokens": ["Auf", "mei\u00b7ner", "Dich\u00b7tung", "Bl\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da sollte wandeln dein Fu\u00df,", "tokens": ["Da", "soll\u00b7te", "wan\u00b7deln", "dein", "Fu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Die Geister meiner Tr\u00e4ume,", "tokens": ["Die", "Geis\u00b7ter", "mei\u00b7ner", "Tr\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie b\u00f6ten dir Knechtesgru\u00df.", "tokens": ["Sie", "b\u00f6\u00b7ten", "dir", "Knech\u00b7tes\u00b7gru\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Sie m\u00fc\u00dften dir dienend huld'gen,", "tokens": ["Sie", "m\u00fc\u00df\u00b7ten", "dir", "die\u00b7nend", "huld'\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Als ihrer K\u00f6nigin;", "tokens": ["Als", "ih\u00b7rer", "K\u00f6\u00b7ni\u00b7gin", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich wollt', ich w\u00e4r' ein Dichter,", "tokens": ["Ich", "wollt'", ",", "ich", "w\u00e4r'", "ein", "Dich\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Weh' mir, da\u00df ich's nicht bin.", "tokens": ["Weh'", "mir", ",", "da\u00df", "ich's", "nicht", "bin", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "KOUS", "PIS", "PTKNEG", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}