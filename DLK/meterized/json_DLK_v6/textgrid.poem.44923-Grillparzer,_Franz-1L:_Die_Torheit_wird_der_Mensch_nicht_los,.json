{"textgrid.poem.44923": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Torheit wird der Mensch nicht los,", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Torheit wird der Mensch nicht los,", "tokens": ["Die", "Tor\u00b7heit", "wird", "der", "Mensch", "nicht", "los", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Spr\u00f6dsten wei\u00df sie selbst zu haschen,", "tokens": ["Den", "Spr\u00f6ds\u00b7ten", "wei\u00df", "sie", "selbst", "zu", "ha\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Gib ihr dich drum im Scherze blo\u00df,", "tokens": ["Gib", "ihr", "dich", "drum", "im", "Scher\u00b7ze", "blo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PRF", "PAV", "APPRART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie wird dich sonst beim Ernste \u00fcberraschen.", "tokens": ["Sie", "wird", "dich", "sonst", "beim", "Erns\u00b7te", "\u00fc\u00b7berr\u00b7a\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}