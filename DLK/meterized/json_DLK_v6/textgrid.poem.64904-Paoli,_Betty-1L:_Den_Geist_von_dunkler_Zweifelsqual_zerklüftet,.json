{"textgrid.poem.64904": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Den Geist von dunkler Zweifelsqual zerkl\u00fcftet,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Den Geist von dunkler Zweifelsqual zerkl\u00fcftet,", "tokens": ["Den", "Geist", "von", "dunk\u00b7ler", "Zwei\u00b7fels\u00b7qual", "zer\u00b7kl\u00fcf\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Durchbl\u00e4tterst du der Sch\u00f6pfung Buch,", "tokens": ["Durch\u00b7bl\u00e4t\u00b7terst", "du", "der", "Sch\u00f6p\u00b7fung", "Buch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Kern des Lebens aufzufinden, l\u00fcftet", "tokens": ["Den", "Kern", "des", "Le\u00b7bens", "auf\u00b7zu\u00b7fin\u00b7den", ",", "l\u00fcf\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "ART", "NN", "VVIZU", "$,", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dein Forschen manches Leichentuch.", "tokens": ["Dein", "For\u00b7schen", "man\u00b7ches", "Lei\u00b7chen\u00b7tuch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Geharnischt eherne Gedanken schlagen", "tokens": ["Ge\u00b7har\u00b7nischt", "e\u00b7her\u00b7ne", "Ge\u00b7dan\u00b7ken", "schla\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In dir sich die Verzweiflungsschlacht:", "tokens": ["In", "dir", "sich", "die", "Ver\u00b7zwei\u00b7flungs\u00b7schlacht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch keine Antwort stillet deine Fragen", "tokens": ["Doch", "kei\u00b7ne", "Ant\u00b7wort", "stil\u00b7let", "dei\u00b7ne", "Fra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und finstrer stets wird deine Nacht.", "tokens": ["Und", "finst\u00b7rer", "stets", "wird", "dei\u00b7ne", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Denn ewig wird, mit allem seinem Sp\u00e4hen,", "tokens": ["Denn", "e\u00b7wig", "wird", ",", "mit", "al\u00b7lem", "sei\u00b7nem", "Sp\u00e4\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "$,", "APPR", "PIS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Mensch, wie m\u00e4chtig er sich d\u00fcnkt,", "tokens": ["Der", "Mensch", ",", "wie", "m\u00e4ch\u00b7tig", "er", "sich", "d\u00fcnkt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "ADJD", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im Himmel nichts und nichts auf Erden sehen,", "tokens": ["Im", "Him\u00b7mel", "nichts", "und", "nichts", "auf", "Er\u00b7den", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PIS", "KON", "PIS", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn er nicht auf die Kniee sinkt!", "tokens": ["Wenn", "er", "nicht", "auf", "die", "Kni\u00b7ee", "sinkt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}