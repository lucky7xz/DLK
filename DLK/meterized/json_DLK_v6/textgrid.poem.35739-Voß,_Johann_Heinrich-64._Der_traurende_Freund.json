{"textgrid.poem.35739": {"metadata": {"author": {"name": "Vo\u00df, Johann Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "64. Der traurende Freund", "genre": "verse", "period": "N.A.", "pub_year": 1801, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dennoch lieb' ich, wenn auch unerwidert", "tokens": ["Den\u00b7noch", "lieb'", "ich", ",", "wenn", "auch", "un\u00b7er\u00b7wi\u00b7dert"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "ADV", "ADJD"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Meine Lieb' in Thr\u00e4nen sich verweint!", "tokens": ["Mei\u00b7ne", "Lieb'", "in", "Thr\u00e4\u00b7nen", "sich", "ver\u00b7weint", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "PRF", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Ach wir waren innig einst verbr\u00fcdert,", "tokens": ["Ach", "wir", "wa\u00b7ren", "in\u00b7nig", "einst", "ver\u00b7br\u00fc\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PPER", "VAFIN", "ADJD", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und, wie Gold, so lauter schien mein Freund!", "tokens": ["Und", ",", "wie", "Gold", ",", "so", "lau\u00b7ter", "schien", "mein", "Freund", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "NN", "$,", "ADV", "ADJD", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Zeugin schien selbst Wahrheit unserm Bunde,", "tokens": ["Zeu\u00b7gin", "schien", "selbst", "Wahr\u00b7heit", "un\u00b7serm", "Bun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Selbst Gerechtigkeit Erhalterin;", "tokens": ["Selbst", "Ge\u00b7rech\u00b7tig\u00b7keit", "Er\u00b7hal\u00b7te\u00b7rin", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "So vereinigt, wie mit Hand und Munde,", "tokens": ["So", "ver\u00b7ei\u00b7nigt", ",", "wie", "mit", "Hand", "und", "Mun\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "PWAV", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "So vereinigt w\u00e4hnt' ich uns an Sinn!", "tokens": ["So", "ver\u00b7ei\u00b7nigt", "w\u00e4hnt'", "ich", "uns", "an", "Sinn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VVFIN", "PPER", "PRF", "APPR", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Goldne Freundschaft, wo du mit der Wahrheit", "tokens": ["Gold\u00b7ne", "Freund\u00b7schaft", ",", "wo", "du", "mit", "der", "Wahr\u00b7heit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PWAV", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und Gerechtigkeit die Erde flohst;", "tokens": ["Und", "Ge\u00b7rech\u00b7tig\u00b7keit", "die", "Er\u00b7de", "flohst", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Send' in lieblich nachgef\u00e4lschter Klarheit", "tokens": ["Send'", "in", "lieb\u00b7lich", "nach\u00b7ge\u00b7f\u00e4lschter", "Klar\u00b7heit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "ADJD", "ADJA", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Nur dein Bild mir Schmachtenden zum Trost!", "tokens": ["Nur", "dein", "Bild", "mir", "Schmach\u00b7ten\u00b7den", "zum", "Trost", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "PPER", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Mag der Freund durch K\u00e4lte mich betr\u00fcben;", "tokens": ["Mag", "der", "Freund", "durch", "K\u00e4l\u00b7te", "mich", "be\u00b7tr\u00fc\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "APPR", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Eigner W\u00e4rme soll mein Herz sich freun!", "tokens": ["Eig\u00b7ner", "W\u00e4r\u00b7me", "soll", "mein", "Herz", "sich", "freun", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VMFIN", "PPOSAT", "NN", "PRF", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Mehr beseligt's, ungeliebt zu lieben,", "tokens": ["Mehr", "be\u00b7se\u00b7ligt's", ",", "un\u00b7ge\u00b7liebt", "zu", "lie\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "$,", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Als geliebt kein Liebender zu sein.", "tokens": ["Als", "ge\u00b7liebt", "kein", "Lie\u00b7ben\u00b7der", "zu", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVPP", "PIAT", "NN", "PTKZU", "VAINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}