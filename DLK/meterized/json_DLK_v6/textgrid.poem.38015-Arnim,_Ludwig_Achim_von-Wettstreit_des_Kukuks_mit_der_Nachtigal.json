{"textgrid.poem.38015": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Wettstreit des Kukuks mit der Nachtigal", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einsmals in einem tiefen Thal", "tokens": ["Eins\u00b7mals", "in", "ei\u00b7nem", "tie\u00b7fen", "Thal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Kukuk und die Nachtigal", "tokens": ["Der", "Ku\u00b7kuk", "und", "die", "Nach\u00b7ti\u00b7gal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Th\u00e4ten ein Wett anschlagen,", "tokens": ["Th\u00e4\u00b7ten", "ein", "Wett", "an\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Zu singen um das Meisterst\u00fcck:", "tokens": ["Zu", "sin\u00b7gen", "um", "das", "Meis\u00b7ter\u00b7st\u00fcck", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "\u00bbgewinn es Kunst, gewinn es Gl\u00fcck,", "tokens": ["\u00bb", "ge\u00b7winn", "es", "Kunst", ",", "ge\u00b7winn", "es", "Gl\u00fcck", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PPER", "NN", "$,", "KON", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dank soll er davon tragen.\u00ab", "tokens": ["Dank", "soll", "er", "da\u00b7von", "tra\u00b7gen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "VMFIN", "PPER", "PAV", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der Kukuk sprach: So dirs gef\u00e4llt,", "tokens": ["Der", "Ku\u00b7kuk", "sprach", ":", "So", "dirs", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "ADV", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich hab zur Sach ein Richter w\u00e4hlt,", "tokens": ["Ich", "hab", "zur", "Sach", "ein", "Rich\u00b7ter", "w\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und th\u00e4t den Esel nennen,", "tokens": ["Und", "th\u00e4t", "den", "E\u00b7sel", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-----", "measure": "dactylic.init"}, "line.4": {"text": "Denn weil er hat zwey Ohren gro\u00df,", "tokens": ["Denn", "weil", "er", "hat", "zwey", "Oh\u00b7ren", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VAFIN", "CARD", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So kann er h\u00f6ren desto bas,", "tokens": ["So", "kann", "er", "h\u00f6\u00b7ren", "des\u00b7to", "bas", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "ADV", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und was recht ist, erkennen.", "tokens": ["Und", "was", "recht", "ist", ",", "er\u00b7ken\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PWS", "ADJD", "VAFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sie flogen vor den Richter bald,", "tokens": ["Sie", "flo\u00b7gen", "vor", "den", "Rich\u00b7ter", "bald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie ihm die Sache ward erz\u00e4hlt,", "tokens": ["Wie", "ihm", "die", "Sa\u00b7che", "ward", "er\u00b7z\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Schuf er, sie sollten singen:", "tokens": ["Schuf", "er", ",", "sie", "soll\u00b7ten", "sin\u00b7gen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Nachtigal sang lieblich aus,", "tokens": ["Die", "Nach\u00b7ti\u00b7gal", "sang", "lieb\u00b7lich", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Esel sprach, du machst mirs kraus,", "tokens": ["Der", "E\u00b7sel", "sprach", ",", "du", "machst", "mirs", "kraus", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "NE", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich kanns in Kopf nicht bringen.", "tokens": ["Ich", "kanns", "in", "Kopf", "nicht", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Der Kukuk drauf anfing geschwind", "tokens": ["Der", "Ku\u00b7kuk", "drauf", "an\u00b7fing", "ge\u00b7schwind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PAV", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kukuk! sein Sang durch Terz, Quart, Quint", "tokens": ["Ku\u00b7kuk", "!", "sein", "Sang", "durch", "Terz", ",", "Quart", ",", "Quint"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["NN", "$.", "PPOSAT", "NN", "APPR", "NN", "$,", "NN", "$,", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und th\u00e4t die Noten brechen;", "tokens": ["Und", "th\u00e4t", "die", "No\u00b7ten", "bre\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Er lacht auch drein nach seiner Art,", "tokens": ["Er", "lacht", "auch", "drein", "nach", "sei\u00b7ner", "Art", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dem Esel gefiels, er sagt, nun wart,", "tokens": ["Dem", "E\u00b7sel", "ge\u00b7fiels", ",", "er", "sagt", ",", "nun", "wart", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Ein Urtheil will ich sprechen.", "tokens": ["Ein", "Ur\u00b7theil", "will", "ich", "spre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wohl sungen hast du Nachtigal,", "tokens": ["Wohl", "sun\u00b7gen", "hast", "du", "Nach\u00b7ti\u00b7gal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VAFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aber Kukuk singst gut Choral,", "tokens": ["A\u00b7ber", "Ku\u00b7kuk", "singst", "gut", "Cho\u00b7ral", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und h\u00e4ltst den Takt fein innen;", "tokens": ["Und", "h\u00e4ltst", "den", "Takt", "fein", "in\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJD", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das sprech ich nach mein hohen Verstand,", "tokens": ["Das", "sprech", "ich", "nach", "mein", "ho\u00b7hen", "Ver\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Und kostets gleich ein ganzes Land,", "tokens": ["Und", "kos\u00b7tets", "gleich", "ein", "gan\u00b7zes", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So la\u00df ich dichs gewinnen.", "tokens": ["So", "la\u00df", "ich", "dichs", "ge\u00b7win\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "PIS", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}