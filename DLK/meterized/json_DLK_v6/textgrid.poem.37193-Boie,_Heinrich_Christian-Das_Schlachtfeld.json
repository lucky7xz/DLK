{"textgrid.poem.37193": {"metadata": {"author": {"name": "Boie, Heinrich Christian", "birth": "N.A.", "death": "N.A."}, "title": "Das Schlachtfeld", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sanft schl\u00e4ft der Helden Schaar, die hier zur Ruhe sank;", "tokens": ["Sanft", "schl\u00e4ft", "der", "Hel\u00b7den", "Schaar", ",", "die", "hier", "zur", "Ru\u00b7he", "sank", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "NN", "$,", "PRELS", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es segnet Ihre Gruft des Vaterlandes Dank.", "tokens": ["Es", "seg\u00b7net", "Ih\u00b7re", "Gruft", "des", "Va\u00b7ter\u00b7lan\u00b7des", "Dank", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So oft der Lenz mit Thau bethr\u00e4net", "tokens": ["So", "oft", "der", "Lenz", "mit", "Thau", "be\u00b7th\u00b7r\u00e4\u00b7net"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN", "APPR", "NN", "VVFIN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ihr heiliges Gebein versch\u00f6net,", "tokens": ["Ihr", "hei\u00b7li\u00b7ges", "Ge\u00b7bein", "ver\u00b7sch\u00f6\u00b7net", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Entbl\u00fcht mehr Anmut seinem Pfad,", "tokens": ["Ent\u00b7bl\u00fcht", "mehr", "An\u00b7mut", "sei\u00b7nem", "Pfad", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Als je der Fu\u00df der Fantasei betrat.", "tokens": ["Als", "je", "der", "Fu\u00df", "der", "Fan\u00b7ta\u00b7sei", "be\u00b7trat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nachts t\u00f6nt von Feenhand der Todtenklocke Klang", "tokens": ["Nachts", "t\u00f6nt", "von", "Fe\u00b7en\u00b7hand", "der", "Tod\u00b7ten\u00b7klo\u00b7cke", "Klang"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN", "ART", "NN", "NN"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Und Luftgebild erhebt den leisen Grabgesang.", "tokens": ["Und", "Luft\u00b7ge\u00b7bild", "er\u00b7hebt", "den", "lei\u00b7sen", "Grab\u00b7ge\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Ehr' in grauer Pilgerh\u00fclle", "tokens": ["Die", "Ehr'", "in", "grau\u00b7er", "Pil\u00b7ger\u00b7h\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Weiht oft der Rasenh\u00fcgel Stille;", "tokens": ["Weiht", "oft", "der", "Ra\u00b7sen\u00b7h\u00fc\u00b7gel", "Stil\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die ernste Freiheit w\u00e4hlt hinfort", "tokens": ["Die", "erns\u00b7te", "Frei\u00b7heit", "w\u00e4hlt", "hin\u00b7fort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Als trauernde Einsiedlerin den Ort.", "tokens": ["Als", "trau\u00b7ern\u00b7de", "Ein\u00b7sied\u00b7le\u00b7rin", "den", "Ort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+---+-+-+", "measure": "dactylic.init"}}}}}