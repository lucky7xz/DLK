{"textgrid.poem.41490": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Die Vorz\u00fcge der Thorheit", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Den Thoren ist ein Gl\u00fcck beschieden,", "tokens": ["Den", "Tho\u00b7ren", "ist", "ein", "Gl\u00fcck", "be\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das vielen klugen Leuten fehlt.", "tokens": ["Das", "vie\u00b7len", "klu\u00b7gen", "Leu\u00b7ten", "fehlt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Herren sind mit sich zufrieden", "tokens": ["Die", "Her\u00b7ren", "sind", "mit", "sich", "zu\u00b7frie\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "APPR", "PRF", "ADJD"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und haben immer wohl gew\u00e4hlt.", "tokens": ["Und", "ha\u00b7ben", "im\u00b7mer", "wohl", "ge\u00b7w\u00e4hlt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was hilft es auch, nach Weisheit schnappen,", "tokens": ["Was", "hilft", "es", "auch", ",", "nach", "Weis\u00b7heit", "schnap\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "$,", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die oft dem Wirbel wehe thut?", "tokens": ["Die", "oft", "dem", "Wir\u00b7bel", "we\u00b7he", "thut", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Den Thoren stehen ihre Kappen", "tokens": ["Den", "Tho\u00b7ren", "ste\u00b7hen", "ih\u00b7re", "Kap\u00b7pen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "So zierlich als ein Doctorhut.", "tokens": ["So", "zier\u00b7lich", "als", "ein", "Doc\u00b7tor\u00b7hut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Thorheit unverj\u00e4hrte Rechte", "tokens": ["Der", "Thor\u00b7heit", "un\u00b7ver\u00b7j\u00e4hr\u00b7te", "Rech\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Erstrecken sich auf jedes Haubt:", "tokens": ["Er\u00b7stre\u00b7cken", "sich", "auf", "je\u00b7des", "Haubt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es ist im menschlichen Geschlechte", "tokens": ["Es", "ist", "im", "menschli\u00b7chen", "Ge\u00b7schlech\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ihr Anhang gr\u00f6\u00dfer, als man glaubt.", "tokens": ["Ihr", "An\u00b7hang", "gr\u00f6\u00b7\u00dfer", ",", "als", "man", "glaubt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "$,", "KOUS", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch wenn sie nicht Vergn\u00fcgen br\u00e4chte:", "tokens": ["Doch", "wenn", "sie", "nicht", "Ver\u00b7gn\u00fc\u00b7gen", "br\u00e4ch\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PTKNEG", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So w\u00e4r' ihr schon die Macht geraubt.", "tokens": ["So", "w\u00e4r'", "ihr", "schon", "die", "Macht", "ge\u00b7raubt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der Thor, der allen Leuten glaubet;", "tokens": ["Der", "Thor", ",", "der", "al\u00b7len", "Leu\u00b7ten", "glau\u00b7bet", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Thor, der keinem Menschen traut;", "tokens": ["Der", "Thor", ",", "der", "kei\u00b7nem", "Men\u00b7schen", "traut", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der, dem die Kargheit nichts erlaubet;", "tokens": ["Der", ",", "dem", "die", "Karg\u00b7heit", "nichts", "er\u00b7lau\u00b7bet", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PRELS", "ART", "NN", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der sich sein Tollhaus f\u00fcrstlich baut;", "tokens": ["Der", "sich", "sein", "Toll\u00b7haus", "f\u00fcrst\u00b7lich", "baut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "PPOSAT", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Thor, der jeden Hof verachtet;", "tokens": ["Der", "Thor", ",", "der", "je\u00b7den", "Hof", "ver\u00b7ach\u00b7tet", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Thor, der nichts, als H\u00f6fe, liebt:", "tokens": ["Der", "Thor", ",", "der", "nichts", ",", "als", "H\u00f6\u00b7fe", ",", "liebt", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIS", "$,", "KOUS", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ein jeder, wenn er sich betrachtet,", "tokens": ["Ein", "je\u00b7der", ",", "wenn", "er", "sich", "be\u00b7trach\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "$,", "KOUS", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Sieht etwas, das ihm Hochmuth gibt.", "tokens": ["Sieht", "et\u00b7was", ",", "das", "ihm", "Hoch\u00b7muth", "gibt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "PRELS", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der Thorheit unverj\u00e4hrte Rechte", "tokens": ["Der", "Thor\u00b7heit", "un\u00b7ver\u00b7j\u00e4hr\u00b7te", "Rech\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Erstrecken sich auf jedes Haubt:", "tokens": ["Er\u00b7stre\u00b7cken", "sich", "auf", "je\u00b7des", "Haubt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es ist im menschlichen Geschlechte", "tokens": ["Es", "ist", "im", "menschli\u00b7chen", "Ge\u00b7schlech\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ihr Anhang gr\u00f6\u00dfer, als man glaubt.", "tokens": ["Ihr", "An\u00b7hang", "gr\u00f6\u00b7\u00dfer", ",", "als", "man", "glaubt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "$,", "KOUS", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch wenn sie nicht Vergn\u00fcgen br\u00e4chte:", "tokens": ["Doch", "wenn", "sie", "nicht", "Ver\u00b7gn\u00fc\u00b7gen", "br\u00e4ch\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PTKNEG", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So w\u00e4r' ihr schon die Macht geraubt.", "tokens": ["So", "w\u00e4r'", "ihr", "schon", "die", "Macht", "ge\u00b7raubt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ein Leitstern lichtbed\u00fcrft'ger K\u00fcnste,", "tokens": ["Ein", "Leits\u00b7tern", "licht\u00b7be\u00b7d\u00fcrft'\u00b7ger", "K\u00fcns\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein junger Metaphysicus,", "tokens": ["Ein", "jun\u00b7ger", "Me\u00b7ta\u00b7phy\u00b7si\u00b7cus", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Webt ein durchsichtiges Gespinnste", "tokens": ["Webt", "ein", "durch\u00b7sich\u00b7ti\u00b7ges", "Ge\u00b7spinns\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und stellt und heftet Schlu\u00df an Schlu\u00df.", "tokens": ["Und", "stellt", "und", "hef\u00b7tet", "Schlu\u00df", "an", "Schlu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So glaubt er dir, o Wolf, zu gleichen,", "tokens": ["So", "glaubt", "er", "dir", ",", "o", "Wolf", ",", "zu", "glei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$,", "FM", "NE", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und hat dennoch, du gro\u00dfer Mann!", "tokens": ["Und", "hat", "den\u00b7noch", ",", "du", "gro\u00b7\u00dfer", "Mann", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Von dir nur die Verbindungszeichen,", "tokens": ["Von", "dir", "nur", "die", "Ver\u00b7bin\u00b7dungs\u00b7zei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und sonst nichts, was dir gleichen kann.", "tokens": ["Und", "sonst", "nichts", ",", "was", "dir", "glei\u00b7chen", "kann", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIS", "$,", "PWS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Der Thorheit unverj\u00e4hrte Rechte", "tokens": ["Der", "Thor\u00b7heit", "un\u00b7ver\u00b7j\u00e4hr\u00b7te", "Rech\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Erstrecken sich auf jedes Haubt:", "tokens": ["Er\u00b7stre\u00b7cken", "sich", "auf", "je\u00b7des", "Haubt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es ist im menschlichen Geschlechte", "tokens": ["Es", "ist", "im", "menschli\u00b7chen", "Ge\u00b7schlech\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ihr Anhang gr\u00f6\u00dfer, als man glaubt.", "tokens": ["Ihr", "An\u00b7hang", "gr\u00f6\u00b7\u00dfer", ",", "als", "man", "glaubt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "$,", "KOUS", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch wenn sie nicht Vergn\u00fcgen br\u00e4chte:", "tokens": ["Doch", "wenn", "sie", "nicht", "Ver\u00b7gn\u00fc\u00b7gen", "br\u00e4ch\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PTKNEG", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So w\u00e4r' ihr schon die Macht geraubt.", "tokens": ["So", "w\u