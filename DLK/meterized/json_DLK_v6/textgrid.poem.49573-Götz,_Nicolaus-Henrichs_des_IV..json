{"textgrid.poem.49573": {"metadata": {"author": {"name": "G\u00f6tz, Nicolaus", "birth": "N.A.", "death": "N.A."}, "title": "Henrichs des IV.", "genre": "verse", "period": "N.A.", "pub_year": 1751, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Durchbohrt von tausend Pfeilen", "tokens": ["Durch\u00b7bohrt", "von", "tau\u00b7send", "Pfei\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "CARD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Entreiss' ich mich von dir, ins Feld.", "tokens": ["Ent\u00b7reiss'", "ich", "mich", "von", "dir", ",", "ins", "Feld", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PRF", "APPR", "PPER", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Ehre hei\u00dft mich eilen;", "tokens": ["Die", "Eh\u00b7re", "hei\u00dft", "mich", "ei\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da mich die Liebe h\u00e4lt.", "tokens": ["Da", "mich", "die", "Lie\u00b7be", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Doch, Abschied dir zu geben,", "tokens": ["Doch", ",", "Ab\u00b7schied", "dir", "zu", "ge\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "O Gabrielle, welche Pein!", "tokens": ["O", "Ga\u00b7bri\u00b7el\u00b7le", ",", "wel\u00b7che", "Pein", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PWAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ach k\u00f6nnt' ich ohne Leben", "tokens": ["Ach", "k\u00f6nnt'", "ich", "oh\u00b7ne", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und ohne Liebe seyn!", "tokens": ["Und", "oh\u00b7ne", "Lie\u00b7be", "seyn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Empfange meine Krone,", "tokens": ["Emp\u00b7fan\u00b7ge", "mei\u00b7ne", "Kro\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gepr\u00fcfter Tapferkeit Gewinn:", "tokens": ["Ge\u00b7pr\u00fcf\u00b7ter", "Tap\u00b7fer\u00b7keit", "Ge\u00b7winn", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mir schenkte sie Bellone;", "tokens": ["Mir", "schenk\u00b7te", "sie", "Bel\u00b7lo\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mein Herz gibt dir sie hin!", "tokens": ["Mein", "Herz", "gibt", "dir", "sie", "hin", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Gl\u00fcckseelig, l\u00e4\u00dft dein K\u00f6nig", "tokens": ["Gl\u00fcck\u00b7see\u00b7lig", ",", "l\u00e4\u00dft", "dein", "K\u00f6\u00b7nig"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADJD", "$,", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das Leben selbst f\u00fcr dich im Streit!", "tokens": ["Das", "Le\u00b7ben", "selbst", "f\u00fcr", "dich", "im", "Streit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Doch eines ist zu wenig", "tokens": ["Doch", "ei\u00b7nes", "ist", "zu", "we\u00b7nig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VAFIN", "PTKA", "PIS"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "F\u00fcr so viel Z\u00e4rtlichkeit!", "tokens": ["F\u00fcr", "so", "viel", "Z\u00e4rt\u00b7lich\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PIAT", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}