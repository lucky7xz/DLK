{"textgrid.poem.52005": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "24.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der K\u00fche Zoll ist schlecht. Der du es auffgebracht,", "tokens": ["Der", "K\u00fc\u00b7he", "Zoll", "ist", "schlecht", ".", "Der", "du", "es", "auff\u00b7ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ADJD", "$.", "ART", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df man den Anschlag hat auff unser Vieh gemacht:", "tokens": ["Da\u00df", "man", "den", "An\u00b7schlag", "hat", "auff", "un\u00b7ser", "Vieh", "ge\u00b7macht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "VAFIN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Esel h\u00e4tten dir was be\u00dfers eingetragen,", "tokens": ["Die", "E\u00b7sel", "h\u00e4t\u00b7ten", "dir", "was", "be\u00b7\u00dfers", "ein\u00b7ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "PIS", "ADJA", "VVPP", "$,"], "meter": "-+-+-+--++-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Im Fall du deiner Zunfft die Schatzung zugeschlagen.", "tokens": ["Im", "Fall", "du", "dei\u00b7ner", "Zunfft", "die", "Schat\u00b7zung", "zu\u00b7ge\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "PPOSAT", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}