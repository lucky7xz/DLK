{"textgrid.poem.42299": {"metadata": {"author": {"name": "B\u00fcrger, Gottfried August", "birth": "N.A.", "death": "N.A."}, "title": "Die Schatzgr\u00e4ber", "genre": "verse", "period": "N.A.", "pub_year": 1770, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Winzer, der am Tode lag,", "tokens": ["Ein", "Win\u00b7zer", ",", "der", "am", "To\u00b7de", "lag", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Rief seine Kinder an und sprach:", "tokens": ["Rief", "sei\u00b7ne", "Kin\u00b7der", "an", "und", "sprach", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbin unserm Weinberg liegt ein Schatz,", "tokens": ["\u00bb", "in", "un\u00b7serm", "Wein\u00b7berg", "liegt", "ein", "Schatz", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Grabt nur darnach!\u00ab \u2013 \u00bbAn welchem Platz?\u00ab \u2013", "tokens": ["Grabt", "nur", "dar\u00b7nach", "!", "\u00ab", "\u2013", "\u00bb", "An", "wel\u00b7chem", "Platz", "?", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct", "punct", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NE", "ADV", "PAV", "$.", "$(", "$(", "$(", "APPR", "PWAT", "NN", "$.", "$(", "$("], "meter": "-------+", "measure": "unknown.measure.single"}, "line.5": {"text": "Schrie alles laut den Vater an.", "tokens": ["Schrie", "al\u00b7les", "laut", "den", "Va\u00b7ter", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "\u00bbgrabt nur!\u00ab \u2013 O weh! da starb der Mann.", "tokens": ["\u00bb", "grabt", "nur", "!", "\u00ab", "\u2013", "O", "weh", "!", "da", "starb", "der", "Mann", "."], "token_info": ["punct", "word", "word", "punct", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ADV", "$.", "$(", "$(", "NE", "PTKVZ", "$.", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Kaum war der Alte beigeschafft,", "tokens": ["Kaum", "war", "der", "Al\u00b7te", "bei\u00b7ge\u00b7schafft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So grub man nach aus Leibeskraft.", "tokens": ["So", "grub", "man", "nach", "aus", "Lei\u00b7bes\u00b7kraft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit Hacke, Karst und Spaden ward", "tokens": ["Mit", "Ha\u00b7cke", ",", "Karst", "und", "Spa\u00b7den", "ward"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Weinberg um und um gescharrt.", "tokens": ["Der", "Wein\u00b7berg", "um", "und", "um", "ge\u00b7scharrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "KON", "APPR", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da war kein Klo\u00df, der ruhig blieb;", "tokens": ["Da", "war", "kein", "Klo\u00df", ",", "der", "ru\u00b7hig", "blieb", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Man warf die Erde gar durchs Sieb,", "tokens": ["Man", "warf", "die", "Er\u00b7de", "gar", "durchs", "Sieb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und zog die Harken kreuz und quer", "tokens": ["Und", "zog", "die", "Har\u00b7ken", "kreuz", "und", "quer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Nach jedem Steinchen hin und her.", "tokens": ["Nach", "je\u00b7dem", "Stein\u00b7chen", "hin", "und", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Allein da ward kein Schatz versp\u00fcrt", "tokens": ["Al\u00b7lein", "da", "ward", "kein", "Schatz", "ver\u00b7sp\u00fcrt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VAFIN", "PIAT", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und jeder hielt sich angef\u00fchrt.", "tokens": ["Und", "je\u00b7der", "hielt", "sich", "an\u00b7ge\u00b7f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch kaum erschien das n\u00e4chste Jahr,", "tokens": ["Doch", "kaum", "er\u00b7schien", "das", "n\u00e4chs\u00b7te", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So nahm man mit Erstaunen wahr,", "tokens": ["So", "nahm", "man", "mit", "Er\u00b7stau\u00b7nen", "wahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df jede Rebe dreifach trug.", "tokens": ["Da\u00df", "je\u00b7de", "Re\u00b7be", "drei\u00b7fach", "trug", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "PAV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da wurden erst die S\u00f6hne klug,", "tokens": ["Da", "wur\u00b7den", "erst", "die", "S\u00f6h\u00b7ne", "klug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und gruben nun Jahr ein Jahr aus", "tokens": ["Und", "gru\u00b7ben", "nun", "Jahr", "ein", "Jahr", "aus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "NN", "ART", "NN", "APPR"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "Des Schatzes immer mehr heraus.", "tokens": ["Des", "Schat\u00b7zes", "im\u00b7mer", "mehr", "he\u00b7raus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}