{"textgrid.poem.39378": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dich sollt' ich ha\u00dfen, und ich mu\u00df dich lieben,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dich sollt' ich ha\u00dfen, und ich mu\u00df dich lieben,", "tokens": ["Dich", "sollt'", "ich", "ha\u00b7\u00dfen", ",", "und", "ich", "mu\u00df", "dich", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVINF", "$,", "KON", "PPER", "VMFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ort! der mein Kleinod geizig wollte haben,", "tokens": ["Ort", "!", "der", "mein", "Klei\u00b7nod", "gei\u00b7zig", "woll\u00b7te", "ha\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "PPOSAT", "NN", "ADJD", "VMFIN", "VAINF", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Nicht um sich sein zu freun, es zu vergraben;", "tokens": ["Nicht", "um", "sich", "sein", "zu", "freun", ",", "es", "zu", "ver\u00b7gra\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "KOUI", "PRF", "PPOSAT", "PTKZU", "VVINF", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Selbst reicher nicht, inde\u00df ich arm geblieben.", "tokens": ["Selbst", "rei\u00b7cher", "nicht", ",", "in\u00b7de\u00df", "ich", "arm", "ge\u00b7blie\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKNEG", "$,", "ADV", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Hier sind noch ihre Spuren eingeschrieben:", "tokens": ["Hier", "sind", "noch", "ih\u00b7re", "Spu\u00b7ren", "ein\u00b7ge\u00b7schrie\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf diesen Wiesen sa\u00df sie; Schatten gaben", "tokens": ["Auf", "die\u00b7sen", "Wie\u00b7sen", "sa\u00df", "sie", ";", "Schat\u00b7ten", "ga\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "PPER", "$.", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ihr Busch und Baum, und Fr\u00fcchte, sie zu laben;", "tokens": ["Ihr", "Busch", "und", "Baum", ",", "und", "Fr\u00fcch\u00b7te", ",", "sie", "zu", "la\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NE", "$,", "KON", "NN", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Blumenlust lie\u00df Au und Feld sie \u00fcben.", "tokens": ["Die", "Blu\u00b7men\u00b7lust", "lie\u00df", "Au", "und", "Feld", "sie", "\u00fc\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "KON", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Hier sang sie noch dem Echo muntre Lieder;", "tokens": ["Hier", "sang", "sie", "noch", "dem", "E\u00b7cho", "mun\u00b7tre", "Lie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Jungfr\u00e4ulich wandelnd im Cyanenkranze", "tokens": ["Jung\u00b7fr\u00e4u\u00b7lich", "wan\u00b7delnd", "im", "Cy\u00b7a\u00b7nen\u00b7kran\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVPP", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Lie\u00df sie das goldne Haar anmuthig flattern.", "tokens": ["Lie\u00df", "sie", "das", "gold\u00b7ne", "Haar", "an\u00b7mut\u00b7hig", "flat\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Bald aber sank sie, ach! entseelt danieder,", "tokens": ["Bald", "a\u00b7ber", "sank", "sie", ",", "ach", "!", "ent\u00b7seelt", "da\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "$,", "ITJ", "$.", "VVFIN", "PAV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie den Gespielen weggerafft im Tanze", "tokens": ["Wie", "den", "Ge\u00b7spie\u00b7len", "weg\u00b7ge\u00b7rafft", "im", "Tan\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Eurydice vom Stiche falscher Nattern.", "tokens": ["Eu\u00b7ry\u00b7di\u00b7ce", "vom", "Sti\u00b7che", "fal\u00b7scher", "Nat\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}