{"textgrid.poem.48170": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Auf Goldgrund", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf Goldgrund stand die Sonne", "tokens": ["Auf", "Gold\u00b7grund", "stand", "die", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "und strahlte gro\u00df und feierlich.", "tokens": ["und", "strahl\u00b7te", "gro\u00df", "und", "fei\u00b7er\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Aus der Tiefe der Welten stieg", "tokens": ["Aus", "der", "Tie\u00b7fe", "der", "Wel\u00b7ten", "stieg"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "die scharfumrissene Wolkenwand", "tokens": ["die", "schar\u00b7fum\u00b7ris\u00b7se\u00b7ne", "Wol\u00b7ken\u00b7wand"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "mit rasender Schnelle am Himmel empor,", "tokens": ["mit", "ra\u00b7sen\u00b7der", "Schnel\u00b7le", "am", "Him\u00b7mel", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "stieg \u2013 stand.", "tokens": ["stieg", "\u2013", "stand", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVFIN", "$(", "VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.2": {"line.1": {"text": "Auf ihrem Gipfel aber hob das Kreuz,", "tokens": ["Auf", "ih\u00b7rem", "Gip\u00b7fel", "a\u00b7ber", "hob", "das", "Kreuz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "dran vor Jahrtausenden der Heiland hing,", "tokens": ["dran", "vor", "Jahr\u00b7tau\u00b7sen\u00b7den", "der", "Hei\u00b7land", "hing", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "sich finster, scharf und glorienumloht,", "tokens": ["sich", "fins\u00b7ter", ",", "scharf", "und", "glo\u00b7ri\u00b7en\u00b7um\u00b7loht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "$,", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "\u2013 drei schwarze Sterne schattend \u00fcber ihm \u2013", "tokens": ["\u2013", "drei", "schwar\u00b7ze", "Ster\u00b7ne", "schat\u00b7tend", "\u00fc\u00b7ber", "ihm", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "CARD", "ADJA", "NN", "ADJD", "APPR", "PPER", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "vom goldumflossnen Abendhimmel ab", "tokens": ["vom", "gold\u00b7um\u00b7floss\u00b7nen", "A\u00b7bend\u00b7him\u00b7mel", "ab"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "und breitete die nackten Arme aus", "tokens": ["und", "brei\u00b7te\u00b7te", "die", "nack\u00b7ten", "Ar\u00b7me", "aus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "und harrte, tausendj\u00e4hriger Sehnsucht voll,", "tokens": ["und", "harr\u00b7te", ",", "tau\u00b7send\u00b7j\u00e4h\u00b7ri\u00b7ger", "Sehn\u00b7sucht", "voll", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "des ", "tokens": ["des"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.9": {"text": "oder einer", "tokens": ["o\u00b7der", "ei\u00b7ner"], "token_info": ["word", "word"], "pos": ["KON", "ART"], "meter": "+-+-", "measure": "trochaic.di"}, "line.10": {"text": "versto\u00dfenen Seele?", "tokens": ["ver\u00b7sto\u00b7\u00dfe\u00b7nen", "See\u00b7le", "?"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Arme Seele du \u2013", "tokens": ["Ar\u00b7me", "See\u00b7le", "du", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "PPER", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}