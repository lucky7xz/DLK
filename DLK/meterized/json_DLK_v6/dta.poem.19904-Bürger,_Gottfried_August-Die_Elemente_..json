{"dta.poem.19904": {"metadata": {"author": {"name": "B\u00fcrger, Gottfried August", "birth": "N.A.", "death": "N.A."}, "title": "Die Elemente .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1778", "urn": "urn:nbn:de:kobv:b4-20090519672", "language": ["de:0.99"], "booktitle": "B\u00fcrger, Gottfried August: Gedichte. G\u00f6ttingen, 1778."}, "poem": {"stanza.1": {"line.1": {"text": "Horch! Hohe Dinge lehr' ich dich.               ", "tokens": ["Horch", "!", "Ho\u00b7he", "Din\u00b7ge", "lehr'", "ich", "dich", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADJA", "NN", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vier Elemente gatten sich;", "tokens": ["Vier", "E\u00b7le\u00b7men\u00b7te", "gat\u00b7ten", "sich", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PRF", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Sie gatten sich, wie Man und Weib,", "tokens": ["Sie", "gat\u00b7ten", "sich", ",", "wie", "Man", "und", "Weib", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "$,", "PWAV", "PIS", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vol Liebesglut in einen Leib.", "tokens": ["Vol", "Lie\u00b7bes\u00b7glut", "in", "ei\u00b7nen", "Leib", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Gott der Liebe rief: Es werde!", "tokens": ["Der", "Gott", "der", "Lie\u00b7be", "rief", ":", "Es", "wer\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$.", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da ward Luft, Feuer, Wasser, Erde.", "tokens": ["Da", "ward", "Luft", ",", "Feu\u00b7er", ",", "Was\u00b7ser", ",", "Er\u00b7de", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Des Feuers Quel die Sonne brent", "tokens": ["Des", "Feu\u00b7ers", "Quel", "die", "Son\u00b7ne", "brent"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NE", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Am blauen Himmelsfirmament.", "tokens": ["Am", "blau\u00b7en", "Him\u00b7mels\u00b7fir\u00b7ma\u00b7ment", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie stralet W\u00e4rme, Tagesschein;", "tokens": ["Sie", "stra\u00b7let", "W\u00e4r\u00b7me", ",", "Ta\u00b7ges\u00b7schein", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie reifet Korn und Obst und Wein;", "tokens": ["Sie", "rei\u00b7fet", "Korn", "und", "Obst", "und", "Wein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Macht alles Lebens S\u00e4fte kochen,", "tokens": ["Macht", "al\u00b7les", "Le\u00b7bens", "S\u00e4f\u00b7te", "ko\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und seine Pulse rascher pochen.", "tokens": ["Und", "sei\u00b7ne", "Pul\u00b7se", "ra\u00b7scher", "po\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Sie h\u00fclt den Mond in stillen Glanz,", "tokens": ["Sie", "h\u00fclt", "den", "Mond", "in", "stil\u00b7len", "Glanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und flicht ihm einen Sternenkranz.", "tokens": ["Und", "flicht", "ihm", "ei\u00b7nen", "Ster\u00b7nen\u00b7kranz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was leuchtet vor dem Wandrer her?", "tokens": ["Was", "leuch\u00b7tet", "vor", "dem", "Wand\u00b7rer", "her", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was f\u00fchrt den Schiffer, durch das Meer,", "tokens": ["Was", "f\u00fchrt", "den", "Schif\u00b7fer", ",", "durch", "das", "Meer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Viel tausend Meilen in die Ferne?", "tokens": ["Viel", "tau\u00b7send", "Mei\u00b7len", "in", "die", "Fer\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ihm leuchten Sonne, Mond und Sterne.", "tokens": ["Ihm", "leuch\u00b7ten", "Son\u00b7ne", ",", "Mond", "und", "Ster\u00b7ne", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Die Luft umf\u00e4ngt den Erdenbal,", "tokens": ["Die", "Luft", "um\u00b7f\u00e4ngt", "den", "Er\u00b7den\u00b7bal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Weht hie und dort, weht \u00fcberal;", "tokens": ["Weht", "hie", "und", "dort", ",", "weht", "\u00fc\u00b7be\u00b7ral", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "KON", "ADV", "$,", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ist Lebenshauch aus Gottes Mund,", "tokens": ["Ist", "Le\u00b7bens\u00b7hauch", "aus", "Got\u00b7tes", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durchwandelt gar das Erdenrund,", "tokens": ["Durch\u00b7wan\u00b7delt", "gar", "das", "Er\u00b7den\u00b7rund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wo sie durch alle H\u00f6lung webet,", "tokens": ["Wo", "sie", "durch", "al\u00b7le", "H\u00f6\u00b7lung", "we\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und selbst des W\u00fcrmchens Lunge hebet.", "tokens": ["Und", "selbst", "des", "W\u00fcrm\u00b7chens", "Lun\u00b7ge", "he\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Das Wasser braust durch Wald und Feld.", "tokens": ["Das", "Was\u00b7ser", "braust", "durch", "Wald", "und", "Feld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In tausend Arme nimt\u2019s die Welt.", "tokens": ["In", "tau\u00b7send", "Ar\u00b7me", "nimt's", "die", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie Gottes Odem, dringt es auch", "tokens": ["Wie", "Got\u00b7tes", "O\u00b7dem", ",", "dringt", "es", "auch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "NN", "NN", "$,", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Tief durch der Erde finstern Bauch.", "tokens": ["Tief", "durch", "der", "Er\u00b7de", "fins\u00b7tern", "Bauch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "NN", "$."], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Wesen schmachteten und s\u00e4nken,", "tokens": ["Die", "We\u00b7sen", "schmach\u00b7te\u00b7ten", "und", "s\u00e4n\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wo sie nicht seines Lebens tr\u00e4nken.", "tokens": ["Wo", "sie", "nicht", "sei\u00b7nes", "Le\u00b7bens", "tr\u00e4n\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PTKNEG", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Drei Br\u00e4utigamen hat, als Braut,", "tokens": ["Drei", "Br\u00e4u\u00b7ti\u00b7ga\u00b7men", "hat", ",", "als", "Braut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "$,", "KOUS", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gott seine Erde angetraut.", "tokens": ["Gott", "sei\u00b7ne", "Er\u00b7de", "an\u00b7ge\u00b7traut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Wann Luft und Wasser sie umarmt,", "tokens": ["Wann", "Luft", "und", "Was\u00b7ser", "sie", "um\u00b7armt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "PWS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und von der Sonn\u2019 ihr Schoos erwarmt,", "tokens": ["Und", "von", "der", "Sonn'", "ihr", "Schoos", "er\u00b7warmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dann wird ihr Schoos, zu allen Stunden,", "tokens": ["Dann", "wird", "ihr", "Schoos", ",", "zu", "al\u00b7len", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Von Kindern jeder Art entbunden.", "tokens": ["Von", "Kin\u00b7dern", "je\u00b7der", "Art", "ent\u00b7bun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "All\u2019 ihre Kindlein hegt und pflegt", "tokens": ["All'", "ih\u00b7re", "Kin\u00b7dlein", "hegt", "und", "pflegt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "PPOSAT", "NN", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie, an ihr liebend Herz gelegt.", "tokens": ["Sie", ",", "an", "ihr", "lie\u00b7bend", "Herz", "ge\u00b7legt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "APPR", "PPER", "ADJD", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie ist die beste Mutter sie;", "tokens": ["Sie", "ist", "die", "bes\u00b7te", "Mut\u00b7ter", "sie", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie s\u00e4uget sp\u00e4t, sie s\u00e4uget fr\u00fch.", "tokens": ["Sie", "s\u00e4u\u00b7get", "sp\u00e4t", ",", "sie", "s\u00e4u\u00b7get", "fr\u00fch", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "PPER", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Kein Kindlein, so ihr Schoos geboren,", "tokens": ["Kein", "Kin\u00b7dlein", ",", "so", "ihr", "Schoos", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "EehtGeht ihrem Schoosse je verloren.             ", "tokens": ["E\u00b7eht", "Geht", "ih\u00b7rem", "Schoos\u00b7se", "je", "ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Sieh hin und her! Sieh rund um dich!", "tokens": ["Sieh", "hin", "und", "her", "!", "Sieh", "rund", "um", "dich", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "KON", "PTKVZ", "$.", "NE", "ADJD", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Elemente lieben sich;", "tokens": ["Die", "E\u00b7le\u00b7men\u00b7te", "lie\u00b7ben", "sich", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Sie gatten sich in Himmelsglut;", "tokens": ["Sie", "gat\u00b7ten", "sich", "in", "Him\u00b7mels\u00b7glut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Je Eins dem Andern Liebes thut.", "tokens": ["Je", "Eins", "dem", "An\u00b7dern", "Lie\u00b7bes", "thut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Aus solchem Liebestrieb\u2019 empfangen,", "tokens": ["Aus", "sol\u00b7chem", "Lie\u00b7bes\u00b7trieb'", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Bist du, o Mensch, hervorgegangen.", "tokens": ["Bist", "du", ",", "o", "Mensch", ",", "her\u00b7vor\u00b7ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "FM", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Nun pr\u00fcfe dich, nun sage mir:", "tokens": ["Nun", "pr\u00fc\u00b7fe", "dich", ",", "nun", "sa\u00b7ge", "mir", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADV", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gl\u00fcht noch des Ursprungs Glut in dir?", "tokens": ["Gl\u00fcht", "noch", "des", "Ur\u00b7sprungs", "Glut", "in", "dir", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Erhelt, wie Sonne, dein Verstand,", "tokens": ["Er\u00b7helt", ",", "wie", "Son\u00b7ne", ",", "dein", "Ver\u00b7stand", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVPP", "$,", "PWAV", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erhelt er Haus und Stadt und Land?", "tokens": ["Er\u00b7helt", "er", "Haus", "und", "Stadt", "und", "Land", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Entlodert, gleich den Himmelskerzen,", "tokens": ["Ent\u00b7lo\u00b7dert", ",", "gleich", "den", "Him\u00b7mels\u00b7ker\u00b7zen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Noch Liebeslohe deinem Herzen?", "tokens": ["Noch", "Lie\u00b7bes\u00b7lo\u00b7he", "dei\u00b7nem", "Her\u00b7zen", "?"], "token_info": ["word", "word", "word", 