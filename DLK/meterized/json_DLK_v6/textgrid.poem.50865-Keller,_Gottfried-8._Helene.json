{"textgrid.poem.50865": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "8. Helene", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tretet ein, hoher Krieger,", "tokens": ["Tre\u00b7tet", "ein", ",", "ho\u00b7her", "Krie\u00b7ger", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "ADJA", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Der sein Herz mir ergab!", "tokens": ["Der", "sein", "Herz", "mir", "er\u00b7gab", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Legt den purpurnen Mantel", "tokens": ["Legt", "den", "pur\u00b7pur\u00b7nen", "Man\u00b7tel"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+-++-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Und die Goldsporen ab!", "tokens": ["Und", "die", "Gold\u00b7spo\u00b7ren", "ab", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PTKVZ", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Spannt das Ro\u00df in den Pflug,", "tokens": ["Spannt", "das", "Ro\u00df", "in", "den", "Pflug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Meinem Vater zum Gru\u00df!", "tokens": ["Mei\u00b7nem", "Va\u00b7ter", "zum", "Gru\u00df", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Die Schabrack mit dem Wappen", "tokens": ["Die", "Scha\u00b7brack", "mit", "dem", "Wap\u00b7pen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gibt 'nen Teppich meinem Fu\u00df.", "tokens": ["Gibt", "'nen", "Tep\u00b7pich", "mei\u00b7nem", "Fu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Euer Schwertgriff mu\u00df lassen", "tokens": ["Eu\u00b7er", "Schwert\u00b7griff", "mu\u00df", "las\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VMFIN", "VVINF"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "F\u00fcr mich Gold und Stein,", "tokens": ["F\u00fcr", "mich", "Gold", "und", "Stein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "NN", "KON", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Und die blitzende Klinge", "tokens": ["Und", "die", "blit\u00b7zen\u00b7de", "Klin\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Wird ein Sch\u00fcreisen sein.", "tokens": ["Wird", "ein", "Sch\u00fc\u00b7rei\u00b7sen", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und die schneewei\u00dfe Feder", "tokens": ["Und", "die", "schnee\u00b7wei\u00b7\u00dfe", "Fe\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Auf dem blutroten Hut", "tokens": ["Auf", "dem", "blut\u00b7ro\u00b7ten", "Hut"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "--++-+", "measure": "anapaest.init"}, "line.3": {"text": "Ist zu 'nem spielenden Wedel", "tokens": ["Ist", "zu", "'nem", "spie\u00b7len\u00b7den", "We\u00b7del"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ART", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "In der Sommerszeit gut.", "tokens": ["In", "der", "Som\u00b7mers\u00b7zeit", "gut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.5": {"line.1": {"text": "Und der Reitknecht mu\u00df lernen,", "tokens": ["Und", "der", "Reit\u00b7knecht", "mu\u00df", "ler\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Wie man Lebkuchen backt,", "tokens": ["Wie", "man", "Leb\u00b7ku\u00b7chen", "backt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie man Wurst und Gef\u00fcllsel", "tokens": ["Wie", "man", "Wurst", "und", "Ge\u00b7f\u00fcll\u00b7sel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "NN", "KON", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Auf die Weihnachtszeit hackt!", "tokens": ["Auf", "die", "Weih\u00b7nachts\u00b7zeit", "hackt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "Nun befehlt Leib und Seele", "tokens": ["Nun", "be\u00b7fehlt", "Leib", "und", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "KON", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Dem heiligen Christ!", "tokens": ["Dem", "hei\u00b7li\u00b7gen", "Christ", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Denn ihr seid verkauft,", "tokens": ["Denn", "ihr", "seid", "ver\u00b7kauft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Wo kein Erl\u00f6sen mehr ist!", "tokens": ["Wo", "kein", "Er\u00b7l\u00f6\u00b7sen", "mehr", "ist", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "ADV", "VAFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.7": {"line.1": {"text": "Seid der Liebe verfallen", "tokens": ["Seid", "der", "Lie\u00b7be", "ver\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["VAIMP", "ART", "NN", "VVINF"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Und verpf\u00e4nd't euer Blut!", "tokens": ["Und", "ver\u00b7pf\u00e4nd't", "eu\u00b7er", "Blut", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "M\u00fcsset leiden und brennen", "tokens": ["M\u00fcs\u00b7set", "lei\u00b7den", "und", "bren\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVINF", "KON", "VVINF"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "In ewiger Glut!", "tokens": ["In", "e\u00b7wi\u00b7ger", "Glut", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}