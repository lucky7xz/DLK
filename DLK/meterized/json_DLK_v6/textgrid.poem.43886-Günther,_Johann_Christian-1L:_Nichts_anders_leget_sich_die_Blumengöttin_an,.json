{"textgrid.poem.43886": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nichts anders leget sich die Blumeng\u00f6ttin an,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nichts anders leget sich die Blumeng\u00f6ttin an,", "tokens": ["Nichts", "an\u00b7ders", "le\u00b7get", "sich", "die", "Blu\u00b7men\u00b7g\u00f6t\u00b7tin", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "VVFIN", "PRF", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn ihr der nahe Lenz die Wiederkunft erlaubet,", "tokens": ["Wenn", "ihr", "der", "na\u00b7he", "Lenz", "die", "Wie\u00b7der\u00b7kunft", "er\u00b7lau\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Als meine Magdalis, von der man heute glaubet,", "tokens": ["Als", "mei\u00b7ne", "Mag\u00b7da\u00b7lis", ",", "von", "der", "man", "heu\u00b7te", "glau\u00b7bet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$,", "APPR", "PRELS", "PIS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sie habe der Natur es weit zuvor gethan.", "tokens": ["Sie", "ha\u00b7be", "der", "Na\u00b7tur", "es", "weit", "zu\u00b7vor", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PPER", "ADJD", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der Neid, so nichts an ihr als dieses tadeln kan,", "tokens": ["Der", "Neid", ",", "so", "nichts", "an", "ihr", "als", "die\u00b7ses", "ta\u00b7deln", "kan", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "PIS", "APPR", "PPER", "KOUS", "PDAT", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df sie die Sch\u00f6nheit auch mit ihrer Sch\u00f6nheit schraubet,", "tokens": ["Da\u00df", "sie", "die", "Sch\u00f6n\u00b7heit", "auch", "mit", "ih\u00b7rer", "Sch\u00f6n\u00b7heit", "schrau\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wird von der Majest\u00e4t selbst des Gesichts beraubet", "tokens": ["Wird", "von", "der", "Ma\u00b7jes\u00b7t\u00e4t", "selbst", "des", "Ge\u00b7sichts", "be\u00b7rau\u00b7bet"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ART", "NN", "ADV", "ART", "NN", "VVFIN"], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und findet nichts um sie vor seinen L\u00e4sterzahn.", "tokens": ["Und", "fin\u00b7det", "nichts", "um", "sie", "vor", "sei\u00b7nen", "L\u00e4s\u00b7ter\u00b7zahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "APPR", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ach, wohlgestaltes Kind, dein Halstuch tr\u00f6stet mich,", "tokens": ["Ach", ",", "wohl\u00b7ge\u00b7stal\u00b7tes", "Kind", ",", "dein", "Hal\u00b7stuch", "tr\u00f6s\u00b7tet", "mich", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADJA", "NN", "$,", "PPOSAT", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil es die Lieberey der gr\u00fcnen Hofnung tr\u00e4get,", "tokens": ["Weil", "es", "die", "Lie\u00b7be\u00b7rey", "der", "gr\u00fc\u00b7nen", "Hof\u00b7nung", "tr\u00e4\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mein W\u00fcntschen sey erf\u00fcllt, mein Bitten habe dich,", "tokens": ["Mein", "W\u00fcnt\u00b7schen", "sey", "er\u00b7f\u00fcllt", ",", "mein", "Bit\u00b7ten", "ha\u00b7be", "dich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,", "PPOSAT", "NN", "VAFIN", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mein Seufzen deine Brust zur Gegengunst beweget.", "tokens": ["Mein", "Seuf\u00b7zen", "dei\u00b7ne", "Brust", "zur", "Ge\u00b7gen\u00b7gunst", "be\u00b7we\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Da nun dein zarter Flor mir dieses wi\u00dfend macht,", "tokens": ["Da", "nun", "dein", "zar\u00b7ter", "Flor", "mir", "die\u00b7ses", "wi\u00b7\u00dfend", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "ADJA", "NN", "PPER", "PDAT", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So ist mein Ku\u00df bereits aufs Botenlohn bedacht.", "tokens": ["So", "ist", "mein", "Ku\u00df", "be\u00b7reits", "aufs", "Bo\u00b7ten\u00b7lohn", "be\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}