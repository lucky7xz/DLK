{"dta.poem.4316": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Das Stern-Schneuzen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Von denen Feuer-Theilchen nun im Mittelpunct der", "tokens": ["Von", "de\u00b7nen", "Feu\u00b7er\u00b7T\u00b7heil\u00b7chen", "nun", "im", "Mit\u00b7tel\u00b7punct", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "NN", "ADV", "APPRART", "NN", "ART"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bl\u00e4selein,", "tokens": ["Bl\u00e4\u00b7se\u00b7lein", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Und denen, welche schweflicht, \u00f6hlicht, auch trocken und", "tokens": ["Und", "de\u00b7nen", ",", "wel\u00b7che", "schwef\u00b7licht", ",", "\u00f6h\u00b7licht", ",", "auch", "tro\u00b7cken", "und"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KON", "PDS", "$,", "PRELS", "VVFIN", "$,", "VVFIN", "$,", "ADV", "ADJD", "KON"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "verbrennlich seyn,", "tokens": ["ver\u00b7brenn\u00b7lich", "seyn", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VAINF", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Die mit dem Wasser sich erhoben und aufgezogen sind,", "tokens": ["Die", "mit", "dem", "Was\u00b7ser", "sich", "er\u00b7ho\u00b7ben", "und", "auf\u00b7ge\u00b7zo\u00b7gen", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "PRF", "VVPP", "KON", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.6": {"text": "entstehen", "tokens": ["ent\u00b7ste\u00b7hen"], "token_info": ["word"], "pos": ["VVINF"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Entz\u00fcndete Materien, die wir denn schw\u00e4ch- und st\u00e4rker", "tokens": ["Ent\u00b7z\u00fcn\u00b7de\u00b7te", "Ma\u00b7te\u00b7ri\u00b7en", ",", "die", "wir", "denn", "schw\u00e4ch", "und", "st\u00e4r\u00b7ker"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PRELS", "PPER", "ADV", "TRUNC", "KON", "NN"], "meter": "-+--+--+-+-+-+-", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "sehen,", "tokens": ["se\u00b7hen", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "Nachdem dieselben schwach und stark. Wenn ihrer wenig", "tokens": ["Nach\u00b7dem", "die\u00b7sel\u00b7ben", "schwach", "und", "stark", ".", "Wenn", "ih\u00b7rer", "we\u00b7nig"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PDAT", "ADJD", "KON", "ADJD", "$.", "KOUS", "PPOSAT", "PIAT"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "ist; so f\u00e4llt", "tokens": ["ist", ";", "so", "f\u00e4llt"], "token_info": ["word", "punct", "word", "word"], "pos": ["VAFIN", "$.", "ADV", "VVFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.11": {"text": "Ein d\u00fcnner Strich, den alles Volk f\u00fcr einen Stern, der", "tokens": ["Ein", "d\u00fcn\u00b7ner", "Strich", ",", "den", "al\u00b7les", "Volk", "f\u00fcr", "ei\u00b7nen", "Stern", ",", "der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "PIAT", "NN", "APPR", "ART", "NN", "$,", "PRELS"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "sinket, h\u00e4lt.", "tokens": ["sin\u00b7ket", ",", "h\u00e4lt", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}