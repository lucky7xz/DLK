{"dta.poem.13198": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Z ehndes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Stimme Majest\u00e4t, der Vortrag, die Geb\u00e4rden,", "tokens": ["Der", "Stim\u00b7me", "Ma\u00b7jes\u00b7t\u00e4t", ",", "der", "Vor\u00b7trag", ",", "die", "Ge\u00b7b\u00e4r\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nichts konnte sonder Lieb und Furcht vernommen werden.", "tokens": ["Nichts", "konn\u00b7te", "son\u00b7der", "Lieb", "und", "Furcht", "ver\u00b7nom\u00b7men", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PIAT", "NN", "KON", "NN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "165Man s\u00e4hnte sich erregt fast jeder Silbe nach,", "tokens": ["s\u00e4hn\u00b7te", "sich", "er\u00b7regt", "fast", "je\u00b7der", "Sil\u00b7be", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "VVFIN", "ADV", "PIAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Da sie mit Eifer, Ernst und Gnade weiter sprach:", "tokens": ["Da", "sie", "mit", "Ei\u00b7fer", ",", "Ernst", "und", "Gna\u00b7de", "wei\u00b7ter", "sprach", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "$,", "NE", "KON", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201eerinnert euch der Stund, in welcher die Gefahren", "tokens": ["\u201e", "e\u00b7rin\u00b7nert", "euch", "der", "Stund", ",", "in", "wel\u00b7cher", "die", "Ge\u00b7fah\u00b7ren"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "VVFIN", "PPER", "ART", "NN", "$,", "APPR", "PRELS", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201eden L\u00e4ndern Angst und Furcht, euch Helden-Muth gebaren.", "tokens": ["\u201e", "den", "L\u00e4n\u00b7dern", "Angst", "und", "Furcht", ",", "euch", "Hel\u00b7den\u00b7Muth", "ge\u00b7ba\u00b7ren", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "NN", "KON", "NN", "$,", "PPER", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201ewie mein Vertraun und Wunsch in dem betr\u00fcbten Stand,", "tokens": ["\u201e", "wie", "mein", "Ver\u00b7traun", "und", "Wunsch", "in", "dem", "be\u00b7tr\u00fcb\u00b7ten", "Stand", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOKOM", "PPOSAT", "NN", "KON", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "170\u201dZu seiner Sch\u00fczung nichts als eure Tugend fand.", "tokens": ["\"", "Zu", "sei\u00b7ner", "Sch\u00fc\u00b7zung", "nichts", "als", "eu\u00b7re", "Tu\u00b7gend", "fand", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PPOSAT", "NN", "PIS", "KOKOM", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "\u201ebedenckt, wie nah der Feind auf unsre W\u00e4lle drunge;", "tokens": ["\u201e", "be\u00b7denckt", ",", "wie", "nah", "der", "Feind", "auf", "uns\u00b7re", "W\u00e4l\u00b7le", "drun\u00b7ge", ";"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "PWAV", "ADJD", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "\u201ewer ihn von dort zur\u00fcck in seine Grenzen zwunge.", "tokens": ["\u201e", "wer", "ihn", "von", "dort", "zu\u00b7r\u00fcck", "in", "sei\u00b7ne", "Gren\u00b7zen", "zwun\u00b7ge", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "PPER", "APPR", "ADV", "PTKVZ", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "\u201ejhr, ihr habt meinen Thron vom Untergang befreyt,", "tokens": ["\u201e", "jhr", ",", "ihr", "habt", "mei\u00b7nen", "Thron", "vom", "Un\u00b7ter\u00b7gang", "be\u00b7freyt", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "$,", "PPER", "VAFIN", "PPOSAT", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "\u201eund meiner Kronen Schmuck durch Rath und That erneut.", "tokens": ["\u201e", "und", "mei\u00b7ner", "Kro\u00b7nen", "Schmuck", "durch", "Rath", "und", "That", "er\u00b7neut", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PPOSAT", "NN", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "175\u201dEs w\u00fcrde mir an Macht, euch an Geduld gebrechen,", "tokens": ["\"", "Es", "w\u00fcr\u00b7de", "mir", "an", "Macht", ",", "euch", "an", "Ge\u00b7duld", "ge\u00b7bre\u00b7chen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "PPER", "APPR", "NN", "$,", "PPER", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "\u201ewann ich nur von dem Tag des Einbruchs wollte sprechen.", "tokens": ["\u201e", "wann", "ich", "nur", "von", "dem", "Tag", "des", "Ein\u00b7bruchs", "woll\u00b7te", "spre\u00b7chen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "PPER", "ADV", "APPR", "ART", "NN", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "\u201edahero wend\u2019 ich mich nur zu der Danckbarkeit:", "tokens": ["\u201e", "da\u00b7he\u00b7ro", "wend'", "ich", "mich", "nur", "zu", "der", "Dan\u00b7ck\u00b7bar\u00b7keit", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PAV", "VVFIN", "PPER", "PRF", "ADV", "APPR", "ART", "NN", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.16": {"text": "\u201eder hab ich diesen Tag und euern Streit geweiht.", "tokens": ["\u201e", "der", "hab", "ich", "die\u00b7sen", "Tag", "und", "eu\u00b7ern", "Streit", "ge\u00b7weiht", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VAFIN", "PPER", "PDAT", "NN", "KON", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "\u201edie will ich, Tugenden! euch offentlich bezeugen;", "tokens": ["\u201e", "die", "will", "ich", ",", "Tu\u00b7gen\u00b7den", "!", "euch", "of\u00b7fent\u00b7lich", "be\u00b7zeu\u00b7gen", ";"], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VMFIN", "PPER", "$,", "NN", "$.", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "180\u201dDie Rede doch vorher zu meiner Mutter neigen.", "tokens": ["\"", "Die", "Re\u00b7de", "doch", "vor\u00b7her", "zu", "mei\u00b7ner", "Mut\u00b7ter", "nei\u00b7gen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}