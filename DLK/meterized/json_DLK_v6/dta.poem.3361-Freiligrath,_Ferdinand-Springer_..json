{"dta.poem.3361": {"metadata": {"author": {"name": "Freiligrath, Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Springer .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1846", "urn": "urn:nbn:de:kobv:b4-200905191464", "language": ["de:0.99"], "booktitle": "Freiligrath, Ferdinand: \u00c7a ira! Herisau, 1846."}, "poem": {"stanza.1": {"line.1": {"text": "Kein besser Schachbrett, als die Welt:", "tokens": ["Kein", "bes\u00b7ser", "Schach\u00b7brett", ",", "als", "die", "Welt", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,", "KOUS", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zur Limmat r\u00fcck\u2019 ich von der Schelde!", "tokens": ["Zur", "Lim\u00b7mat", "r\u00fcck", "ich", "von", "der", "Schel\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr sprengt mich wohl von Feld zu Feld,", "tokens": ["Ihr", "sprengt", "mich", "wohl", "von", "Feld", "zu", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch schlagt ihr mich nicht aus dem Felde!", "tokens": ["Doch", "schlagt", "ihr", "mich", "nicht", "aus", "dem", "Fel\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "PTKNEG", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "So ist es eben in dem Schach", "tokens": ["So", "ist", "es", "e\u00b7ben", "in", "dem", "Schach"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Freien wider die Despoten:", "tokens": ["Der", "Frei\u00b7en", "wi\u00b7der", "die", "Des\u00b7po\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Zug \u00fcber Zug und Schlag auf Schlag,", "tokens": ["Zug", "\u00fc\u00b7ber", "Zug", "und", "Schlag", "auf", "Schlag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Ruh\u2019 wird keine nicht geboten!", "tokens": ["Und", "Ruh'", "wird", "kei\u00b7ne", "nicht", "ge\u00b7bo\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "PIAT", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Mir ist, als m\u00fc\u00dft\u2019 ich auch von hier", "tokens": ["Mir", "ist", ",", "als", "m\u00fc\u00dft'", "ich", "auch", "von", "hier"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "VMFIN", "PPER", "ADV", "APPR", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Stab noch in die Weite setzen;", "tokens": ["Den", "Stab", "noch", "in", "die", "Wei\u00b7te", "set\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Als w\u00fcrden auch aus Tell\u2019s Revier", "tokens": ["Als", "w\u00fcr\u00b7den", "auch", "aus", "Tell's", "Re\u00b7vier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VAFIN", "ADV", "APPR", "NE", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Launen dieses Spiels mich hetzen!", "tokens": ["Die", "Lau\u00b7nen", "die\u00b7ses", "Spiels", "mich", "het\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich bin bereit! Noch braust das Meer", "tokens": ["Ich", "bin", "be\u00b7reit", "!", "Noch", "braust", "das", "Meer"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "$.", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Um Norweg\u2019s freie Bauernst\u00e4tten;", "tokens": ["Um", "Nor\u00b7weg's", "frei\u00b7e", "Bau\u00b7ern\u00b7st\u00e4t\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Noch rasselt es von Frankreich her,", "tokens": ["Noch", "ras\u00b7selt", "es", "von", "Fran\u00b7kreich", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NE", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie Klirren von gebrochnen Ketten!", "tokens": ["Wie", "Klir\u00b7ren", "von", "ge\u00b7broch\u00b7nen", "Ket\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Kein fl\u00fcchtig Haupt hat Engelland", "tokens": ["Kein", "fl\u00fcch\u00b7tig", "Haupt", "hat", "En\u00b7gel\u00b7land"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "ADJD", "NN", "VAFIN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von seiner Schwelle noch gewiesen;", "tokens": ["Von", "sei\u00b7ner", "Schwel\u00b7le", "noch", "ge\u00b7wie\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Noch winkt mir eine Freundeshand", "tokens": ["Noch", "winkt", "mir", "ei\u00b7ne", "Freun\u00b7des\u00b7hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nach des Ohio lust\u2019gen Wiesen!", "tokens": ["Nach", "des", "O\u00b7hio", "lust'\u00b7gen", "Wie\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Von Dorf zu Dorf, von Stadt zu Stadt,", "tokens": ["Von", "Dorf", "zu", "Dorf", ",", "von", "Stadt", "zu", "Stadt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "$,", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von Land zu Land \u2014 mich schiert es wenig!", "tokens": ["Von", "Land", "zu", "Land", "mich", "schiert", "es", "we\u00b7nig", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "$(", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Kein Zug des Schicksals setzt mich matt: \u2014", "tokens": ["Kein", "Zug", "des", "Schick\u00b7sals", "setzt", "mich", "matt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Matt werden kann ja nur der K\u00f6nig!", "tokens": ["Matt", "wer\u00b7den", "kann", "ja", "nur", "der", "K\u00f6\u00b7nig", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAINF", "VMFIN", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}