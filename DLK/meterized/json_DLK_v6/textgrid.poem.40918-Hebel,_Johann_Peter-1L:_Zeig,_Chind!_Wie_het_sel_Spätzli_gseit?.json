{"textgrid.poem.40918": {"metadata": {"author": {"name": "Hebel, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zeig, Chind! Wie het sel Sp\u00e4tzli gseit?", "genre": "verse", "period": "N.A.", "pub_year": 1793, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zeig, Chind! Wie het sel Sp\u00e4tzli gseit?", "tokens": ["Zeig", ",", "Chind", "!", "Wie", "het", "sel", "Sp\u00e4tz\u00b7li", "gseit", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$.", "PWAV", "VAFIN", "ADV", "NE", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Weisch's n\u00fcmme recht? Was luegsch mi a?", "tokens": ["Weisch's", "n\u00fcm\u00b7me", "recht", "?", "Was", "lu\u00b7egsch", "mi", "a", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADJD", "$.", "PWS", "ADJD", "FM", "FM", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "\u00bb's het gseit: \u203aI bi der Vogt im Dorf,", "tokens": ["\u00bb", "'s", "het", "gseit", ":", "\u203a", "I", "bi", "der", "Vogt", "im", "Dorf", ","], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "NN", "$.", "FM", "FM", "FM", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+---+-+-+", "measure": "dactylic.init"}, "line.4": {"text": "i mue\u00df von allem d'Vorles ha\u2039.\u00ab", "tokens": ["i", "mu\u00b7e\u00df", "von", "al\u00b7lem", "d'\u00b7Vor\u00b7les", "ha", "\u2039", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NE", "VVFIN", "APPR", "PIS", "ART", "NE", "$(", "$.", "$("], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und wo der Sp\u00f6tlig seit: \u203a's isch gnueg!\u2039", "tokens": ["Und", "wo", "der", "Sp\u00f6t\u00b7lig", "seit", ":", "\u203a", "'s", "isch", "gnu\u00b7eg", "!", "\u2039"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "APPR", "$.", "$(", "PPER", "ADJD", "ADV", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Was tut mi Spatz, wo d'Vorles het?", "tokens": ["Was", "tut", "mi", "Spatz", ",", "wo", "d'\u00b7Vor\u00b7les", "het", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "NE", "NN", "$,", "PWAV", "PDS", "VAFIN", "$."], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "\u00bber liest am Bode d'Br\u00f6sli uf,", "tokens": ["\u00bb", "er", "liest", "am", "Bo\u00b7de", "d'\u00b7Br\u00f6s\u00b7li", "uf", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "APPRART", "NN", "NE", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "sust m\u00fce\u00dft er hungerig ins Bett.\u00ab", "tokens": ["sust", "m\u00fce\u00dft", "er", "hun\u00b7ge\u00b7rig", "ins", "Bett", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und wo der Winter d'Felder deckt,", "tokens": ["Und", "wo", "der", "Win\u00b7ter", "d'\u00b7Fel\u00b7der", "deckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "PDS", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "was tut mi Spatz in siner Not?", "tokens": ["was", "tut", "mi", "Spatz", "in", "si\u00b7ner", "Not", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "FM", "FM", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "\u00bber p\u00f6pperlet am Fenster a", "tokens": ["\u00bb", "er", "p\u00f6p\u00b7per\u00b7let", "am", "Fens\u00b7ter", "a"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "APPRART", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "und bettlet um e St\u00fcckli Brot.", "tokens": ["und", "bett\u00b7let", "um", "e", "St\u00fcck\u00b7li", "Brot", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NE", "NE", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Gang, gib em, Muetter! 's friert en sust.\u00ab", "tokens": ["Gang", ",", "gib", "em", ",", "Mu\u00b7et\u00b7ter", "!", "'s", "friert", "en", "sust", ".", "\u00ab"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "VVIMP", "PTKVZ", "$,", "NN", "$.", "PPER", "VVFIN", "NE", "VVFIN", "$.", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.14": {"text": "Zeig, sag mer z'erst, 's pressiert nit so,", "tokens": ["Zeig", ",", "sag", "mer", "z'\u00b7erst", ",", "'s", "pres\u00b7siert", "nit", "so", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "ADV", "VVFIN", "$,", "PPER", "VVFIN", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "wie chunnt's der mit dem Sp\u00e4tzli vor?", "tokens": ["wie", "chunnt's", "der", "mit", "dem", "Sp\u00e4tz\u00b7li", "vor", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "ART", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Meinsch nit, es ch\u00f6nnt eim au so goh?", "tokens": ["Meinsch", "nit", ",", "es", "ch\u00f6nnt", "eim", "au", "so", "goh", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "$,", "PPER", "VVFIN", "ART", "NE", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Chind, wird's der wohl, und 's goht der guet,", "tokens": ["Chind", ",", "wird's", "der", "wohl", ",", "und", "'s", "goht", "der", "guet", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "ART", "ADV", "$,", "KON", "PPER", "NN", "ART", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.18": {"text": "sag nit: \u203aI bi ne riche Her\u2039,", "tokens": ["sag", "nit", ":", "\u203a", "I", "bi", "ne", "ri\u00b7che", "Her", "\u2039", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVIMP", "PTKNEG", "$.", "FM", "FM", "FM", "FM", "ADJA", "NN", "$(", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.19": {"text": "und i\u00df nit Brotis alli Tag!", "tokens": ["und", "i\u00df", "nit", "Bro\u00b7tis", "al\u00b7li", "Tag", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "NE", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "'s ch\u00f6nnt anders werde, handumcher.", "tokens": ["'s", "ch\u00f6nnt", "an\u00b7ders", "wer\u00b7de", ",", "han\u00b7dum\u00b7cher", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "ADJD", "ADV", "VAFIN", "$,", "ADJD", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.21": {"text": "I\u00df nit der chrosplig Ranft vom Brot,", "tokens": ["I\u00df", "nit", "der", "chro\u00b7sp\u00b7lig", "Ranft", "vom", "Brot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.22": {"text": "und lo\u00df die weiche Brosme stoh!", "tokens": ["und", "lo\u00df", "die", "wei\u00b7che", "Bros\u00b7me", "stoh", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "\u2013 De hesch's im Bruuch \u2013 es chunnt e Zit,", "tokens": ["\u2013", "De", "he\u00b7sch's", "im", "Bruuch", "\u2013", "es", "chunnt", "e", "Zit", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "NE", "NE", "APPRART", "NN", "$(", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.24": {"text": "und wenn de's h\u00e4ttsch, wie w\u00e4rsch so froh!", "tokens": ["und", "wenn", "de's", "h\u00e4ttsch", ",", "wie", "w\u00e4rsch", "so", "froh", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "ADJD", "$,", "PWAV", "ADJD", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Ne blaue Mentig w\u00e4hrt nit lang,", "tokens": ["Ne", "blau\u00b7e", "Men\u00b7tig", "w\u00e4hrt", "nit", "lang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "und d'Wuche het no mengi Stund,", "tokens": ["und", "d'\u00b7Wu\u00b7che", "het", "no", "men\u00b7gi", "Stund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "FM", "FM", "NN", "$,"], "meter": "--+--++-+", "measure": "anapaest.di.plus"}, "line.27": {"text": "und mengi Wuche lauft durs Dorf,", "tokens": ["und", "men\u00b7gi", "Wu\u00b7che", "lauft", "durs", "Dorf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.28": {"text": "bis jedem au si lezti chunnt.", "tokens": ["bis", "je\u00b7dem", "au", "si", "lez\u00b7ti", "chunnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "FM", "FM", "FM", "FM", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Und was men in sim Fr\u00fcehlig lehrt,", "tokens": ["Und", "was", "men", "in", "sim", "Fr\u00fceh\u00b7lig", "lehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "APPR", "NE", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "me treit nit schwer, und het's emol,", "tokens": ["me", "treit", "nit", "schwer", ",", "und", "het's", "e\u00b7mol", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKNEG", "ADJD", "$,", "KON", "NE", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "und was men in sim Summer spart,", "tokens": ["und", "was", "men", "in", "sim", "Sum\u00b7mer", "spart", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "APPR", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "das chunnt eim in sim Sp\u00f6tlig wohl.", "tokens": ["das", "chunnt", "eim", "in", "sim", "Sp\u00f6t\u00b7lig", "wohl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "APPR", "NE", "NE", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Chind, denk mer dra, und halt di guet!", "tokens": ["Chind", ",", "denk", "mer", "dra", ",", "und", "halt", "di", "guet", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "FM.la", "FM.la", "FM.la", "$,", "KON", "VVFIN", "NE", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "\u00bbo Muetter lueg, der Spatz will goh!\u00ab", "tokens": ["\u00bb", "o", "Mu\u00b7et\u00b7ter", "lu\u00b7eg", ",", "der", "Spatz", "will", "goh", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "FM", "NN", "VVFIN", "$,", "ART", "NN", "VMFIN", "ADJD", "$.", "$("], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.35": {"text": "Se gang er! Leng die Hirse d\u00f6rt,", "tokens": ["Se", "gang", "er", "!", "Leng", "die", "Hir\u00b7se", "d\u00f6rt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$.", "NE", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.36": {"text": "und streu em! Er wird wieder cho!", "tokens": ["und", "streu", "em", "!", "Er", "wird", "wie\u00b7der", "cho", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PTKVZ", "$.", "PPER", "VAFIN", "ADV", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}