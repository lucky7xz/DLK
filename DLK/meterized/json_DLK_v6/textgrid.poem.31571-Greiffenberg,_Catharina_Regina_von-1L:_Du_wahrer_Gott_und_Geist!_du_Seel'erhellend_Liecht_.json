{"textgrid.poem.31571": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du wahrer Gott und Geist! du Seel'erhellend Liecht /", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du wahrer Gott und Geist! du Seel'erhellend Liecht /", "tokens": ["Du", "wah\u00b7rer", "Gott", "und", "Geist", "!", "du", "Seel'\u00b7er\u00b7hel\u00b7lend", "Liecht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "KON", "NN", "$.", "PPER", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "des Lebens Leben Geist / du fliegend' Himmelflammen!", "tokens": ["des", "Le\u00b7bens", "Le\u00b7ben", "Geist", "/", "du", "flie\u00b7gend'", "Him\u00b7mel\u00b7flam\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "NN", "$(", "PPER", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "du ziehst / im Mittel-Punct des Herzenskrei\u00df zusammen /", "tokens": ["du", "ziehst", "/", "im", "Mit\u00b7tel\u00b7Punct", "des", "Her\u00b7zen\u00b7skrei\u00df", "zu\u00b7sam\u00b7men", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "APPRART", "NN", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "was nur vor Freuden hat das ganze Welt Gewicht", "tokens": ["was", "nur", "vor", "Freu\u00b7den", "hat", "das", "gan\u00b7ze", "Welt", "Ge\u00b7wicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "APPR", "NN", "VAFIN", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "in deinem Strahlen-Glanz / man das unsichtbar sicht.", "tokens": ["in", "dei\u00b7nem", "Strah\u00b7len\u00b7Glanz", "/", "man", "das", "un\u00b7sicht\u00b7bar", "sicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$(", "PIS", "ART", "ADJD", "VVFIN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Die \u00e4ussern Gottes Zweck mir zu Gem\u00fcte kamen /", "tokens": ["Die", "\u00e4us\u00b7sern", "Got\u00b7tes", "Zweck", "mir", "zu", "Ge\u00b7m\u00fc\u00b7te", "ka\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "PPER", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "den Gegenw\u00e4rtigen schier Seyn und Schein benahmen:", "tokens": ["den", "Ge\u00b7gen\u00b7w\u00e4r\u00b7ti\u00b7gen", "schier", "Seyn", "und", "Schein", "be\u00b7nah\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "da\u00df in das k\u00fcnfftig' Ich fast v\u00f6llig mich gericht.", "tokens": ["da\u00df", "in", "das", "k\u00fcnf\u00b7ftig'", "Ich", "fast", "v\u00f6l\u00b7lig", "mich", "ge\u00b7richt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PDS", "VVFIN", "PPER", "ADV", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Entdeckte Wunder G\u00fct / ach klare wahre Luft /", "tokens": ["Ent\u00b7deck\u00b7te", "Wun\u00b7der", "G\u00fct", "/", "ach", "kla\u00b7re", "wah\u00b7re", "Luft", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$(", "XY", "ADJA", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "er\u00f6ffnets Gnaden-Ziel / erkl\u00e4rtes Wunder-\u00fcben /", "tokens": ["er\u00b7\u00f6ff\u00b7nets", "Gna\u00b7den\u00b7Ziel", "/", "er\u00b7kl\u00e4r\u00b7tes", "Wun\u00b7der\u00b7\u00fc\u00b7ben", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$(", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "du sch\u00f6ner Sonnen-Brunn / machst mir mit Lust bewust /", "tokens": ["du", "sch\u00f6\u00b7ner", "Son\u00b7nen\u00b7Brunn", "/", "machst", "mir", "mit", "Lust", "be\u00b7wust", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$(", "VVFIN", "PPER", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "da\u00df der allgute Gott verh\u00e4nget das betr\u00fcben:", "tokens": ["da\u00df", "der", "all\u00b7gu\u00b7te", "Gott", "ver\u00b7h\u00e4n\u00b7get", "das", "be\u00b7tr\u00fc\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "PDS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "da\u00df mit Erzs\u00fcsser Freud er nach der Angst erquick /", "tokens": ["da\u00df", "mit", "Erz\u00b7s\u00fcs\u00b7ser", "Freud", "er", "nach", "der", "Angst", "er\u00b7quick", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "NN", "PPER", "APPR", "ART", "NN", "NE", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "macht / wie aus finstern Liecht / aus allem Vbel Gl\u00fcck.", "tokens": ["macht", "/", "wie", "aus", "fins\u00b7tern", "Liecht", "/", "aus", "al\u00b7lem", "Vbel", "Gl\u00fcck", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "KOKOM", "APPR", "ADJA", "NN", "$(", "APPR", "PIS", "NN", "NN", "$."], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}}}}}