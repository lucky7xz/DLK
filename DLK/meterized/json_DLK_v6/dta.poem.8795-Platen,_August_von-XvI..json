{"dta.poem.8795": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "XvI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Die am vollsten wuchert, wuchert auf den Gr\u00fcften;", "tokens": ["Die", "am", "volls\u00b7ten", "wu\u00b7chert", ",", "wu\u00b7chert", "auf", "den", "Gr\u00fcf\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "ADJA", "VVFIN", "$,", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "La\u00df dich nicht verlocken vom Cypressenwuchse,", "tokens": ["La\u00df", "dich", "nicht", "ver\u00b7lo\u00b7cken", "vom", "Cyp\u00b7res\u00b7sen\u00b7wuch\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "VVINF", "APPRART", "NN", "$,"], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Denn Gew\u00fcrme nagen seine schlanken H\u00fcften;", "tokens": ["Denn", "Ge\u00b7w\u00fcr\u00b7me", "na\u00b7gen", "sei\u00b7ne", "schlan\u00b7ken", "H\u00fcf\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Staune nicht dem Felsen! St\u00fcrme, Winde, Blitze,", "tokens": ["Stau\u00b7ne", "nicht", "dem", "Fel\u00b7sen", "!", "St\u00fcr\u00b7me", ",", "Win\u00b7de", ",", "Blit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "PTKNEG", "ART", "NN", "$.", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.5": {"text": "Selbst der Menschen Aexte m\u00f6gen ihn zerkl\u00fcften;", "tokens": ["Selbst", "der", "Men\u00b7schen", "A\u00b7ex\u00b7te", "m\u00f6\u00b7gen", "ihn", "zer\u00b7kl\u00fcf\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "VMFIN", "PPER", "VVFIN", "$."], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "Flehst du zu den Sternen? Sterne sind nur Flocken,", "tokens": ["Flehst", "du", "zu", "den", "Ster\u00b7nen", "?", "Ster\u00b7ne", "sind", "nur", "Flo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$.", "NN", "VAFIN", "ADV", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.7": {"text": "Die nicht schmelzen k\u00f6nnen in den kalten L\u00fcften.", "tokens": ["Die", "nicht", "schmel\u00b7zen", "k\u00f6n\u00b7nen", "in", "den", "kal\u00b7ten", "L\u00fcf\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "VVINF", "VMFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}}}}