{"textgrid.poem.56408": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "29", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da trat in den Saal Thryms", "tokens": ["Da", "trat", "in", "den", "Saal", "Thryms"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "NE"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Traurige Schwester,", "tokens": ["Trau\u00b7ri\u00b7ge", "Schwes\u00b7ter", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Die gar sich die Gaben", "tokens": ["Die", "gar", "sich", "die", "Ga\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "PRF", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Zu begehren erk\u00fchnt:", "tokens": ["Zu", "be\u00b7geh\u00b7ren", "er\u00b7k\u00fchnt", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VVPP", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "\u00bbich reiche die roten", "tokens": ["\u00bb", "ich", "rei\u00b7che", "die", "ro\u00b7ten"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "ART", "ADJA"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Ringe dir dar,", "tokens": ["Rin\u00b7ge", "dir", "dar", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKVZ", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Verlangt' dich in Lust", "tokens": ["Ver\u00b7langt'", "dich", "in", "Lust"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Nach Frayas Liebe,", "tokens": ["Nach", "Fray\u00b7as", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Nach Frayas Liebe", "tokens": ["Nach", "Fray\u00b7as", "Lie\u00b7be"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NE", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "Und freudiger Huld?\u00ab", "tokens": ["Und", "freu\u00b7di\u00b7ger", "Huld", "?", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["KON", "ADJA", "NN", "$.", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}