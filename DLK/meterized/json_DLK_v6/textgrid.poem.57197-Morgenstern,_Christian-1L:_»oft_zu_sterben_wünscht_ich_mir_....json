{"textgrid.poem.57197": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bboft zu sterben w\u00fcnscht ich mir ...", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bboft zu sterben w\u00fcnscht ich mir ...", "tokens": ["\u00bb", "oft", "zu", "ster\u00b7ben", "w\u00fcnscht", "ich", "mir", "..."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "PTKZU", "VVINF", "VVFIN", "PPER", "PPER", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und wie dankbar bin ich doch,", "tokens": ["Und", "wie", "dank\u00b7bar", "bin", "ich", "doch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADJD", "VAFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "da\u00df ich leb und leide noch", "tokens": ["da\u00df", "ich", "leb", "und", "lei\u00b7de", "noch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "KON", "VVFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "im gesetzten Nun und Hier.", "tokens": ["im", "ge\u00b7setz\u00b7ten", "Nun", "und", "Hier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADV", "KON", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Bleibt mir doch damit noch Zeit,", "tokens": ["Bleibt", "mir", "doch", "da\u00b7mit", "noch", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PAV", "ADV", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "abzubauen manch Gebrest,", "tokens": ["ab\u00b7zu\u00b7bau\u00b7en", "manch", "Ge\u00b7brest", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIZU", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "komm ich nimmer auch zum Rest,", "tokens": ["komm", "ich", "nim\u00b7mer", "auch", "zum", "Rest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "werd ich besser doch bereit.", "tokens": ["werd", "ich", "bes\u00b7ser", "doch", "be\u00b7reit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wenn ich jetzt nicht wirken kann,", "tokens": ["Wenn", "ich", "jetzt", "nicht", "wir\u00b7ken", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "helf ich also doch dem Mir,", "tokens": ["helf", "ich", "al\u00b7so", "doch", "dem", "Mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ART", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "das dereinst nach Nun und Hier", "tokens": ["das", "de\u00b7reinst", "nach", "Nun", "und", "Hier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "APPR", "ADV", "KON", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "wirken wird im Dort und Dann.\u00ab", "tokens": ["wir\u00b7ken", "wird", "im", "Dort", "und", "Dann", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVINF", "VAFIN", "APPRART", "NN", "KON", "ADV", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}