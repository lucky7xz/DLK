{"textgrid.poem.28476": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "91.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der bey einer Jungfer schl\u00e4ft, ist der Straffe werth geacht;", "tokens": ["Der", "bey", "ei\u00b7ner", "Jung\u00b7fer", "schl\u00e4ft", ",", "ist", "der", "Straf\u00b7fe", "werth", "ge\u00b7acht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$,", "VAFIN", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Aber der hat offtmals Lohn, der bey einer Jungfer wacht.", "tokens": ["A\u00b7ber", "der", "hat", "offt\u00b7mals", "Lohn", ",", "der", "bey", "ei\u00b7ner", "Jung\u00b7fer", "wacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "ADV", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-++-+-+", "measure": "unknown.measure.octa.plus"}, "line.3": {"text": "Ist es billich? Ja; man frag' eine Jungfer selbst davon;", "tokens": ["Ist", "es", "bil\u00b7lich", "?", "Ja", ";", "man", "frag'", "ei\u00b7ne", "Jung\u00b7fer", "selbst", "da\u00b7von", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$.", "PTKANT", "$.", "PIS", "VVFIN", "ART", "NN", "ADV", "PAV", "$."], "meter": "+-+--+-+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Gebt dem faulen, spricht sie, (h\u00f6rt!) Straffe! gebt dem wackren Lohn!", "tokens": ["Gebt", "dem", "fau\u00b7len", ",", "spricht", "sie", ",", "(", "h\u00f6rt", "!", ")", "Straf\u00b7fe", "!", "gebt", "dem", "wack\u00b7ren", "Lohn", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "ADJA", "$,", "VVFIN", "PPER", "$,", "$(", "VVFIN", "$.", "$(", "NN", "$.", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}}}}}