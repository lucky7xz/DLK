{"textgrid.poem.53119": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "Johannes Vogler und Barbara Maack", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Liebster Ohm, was schreib ich schier,", "tokens": ["Liebs\u00b7ter", "Ohm", ",", "was", "schreib", "ich", "schier", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PWS", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sag es mir,", "tokens": ["Sag", "es", "mir", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Deinem Hochzeit-Fest zu Ehren?", "tokens": ["Dei\u00b7nem", "Hoch\u00b7zeit\u00b7Fest", "zu", "Eh\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sol ich deiner Liebe Gunst", "tokens": ["Sol", "ich", "dei\u00b7ner", "Lie\u00b7be", "Gunst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Durch die Kunst", "tokens": ["Durch", "die", "Kunst"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Heller Seiten lassen h\u00f6ren?", "tokens": ["Hel\u00b7ler", "Sei\u00b7ten", "las\u00b7sen", "h\u00f6\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVINF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Singen, wie du endlich noch", "tokens": ["Sin\u00b7gen", ",", "wie", "du", "end\u00b7lich", "noch"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "PPER", "ADV", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "An das Joch", "tokens": ["An", "das", "Joch"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "S\u00fcsser Heyraht auch bist kommen?", "tokens": ["S\u00fcs\u00b7ser", "Hey\u00b7raht", "auch", "bist", "kom\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie dich meiner Liebe Bandt,", "tokens": ["Wie", "dich", "mei\u00b7ner", "Lie\u00b7be", "Bandt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Diesen Standt", "tokens": ["Die\u00b7sen", "Standt"], "token_info": ["word", "word"], "pos": ["PDAT", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Zu ergreiffen, eingenommen?", "tokens": ["Zu", "er\u00b7greif\u00b7fen", ",", "ein\u00b7ge\u00b7nom\u00b7men", "?"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Zwar ich kan versichert seyn,", "tokens": ["Zwar", "ich", "kan", "ver\u00b7si\u00b7chert", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VMFIN", "VVPP", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df du fein", "tokens": ["Da\u00df", "du", "fein"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Dich zu friede werdest geben,", "tokens": ["Dich", "zu", "frie\u00b7de", "wer\u00b7dest", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "VAFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ba\u00df mit Gott vnd deinem Sinn',", "tokens": ["Ba\u00df", "mit", "Gott", "vnd", "dei\u00b7nem", "Sinn'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Als vorhin,", "tokens": ["Als", "vor\u00b7hin", ","], "token_info": ["word", "word", "punct"], "pos": ["KOUS", "ADV", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Friedlich vnd vergn\u00fcget leben.", "tokens": ["Fried\u00b7lich", "vnd", "ver\u00b7gn\u00fc\u00b7get", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "VVFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Kan auch ein beredter Mundt", "tokens": ["Kan", "auch", "ein", "be\u00b7red\u00b7ter", "Mundt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Auff den Grundt", "tokens": ["Auff", "den", "Grundt"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Alle Heyrahts-Fr\u00fcchte preisen?", "tokens": ["Al\u00b7le", "Hey\u00b7rahts\u00b7Fr\u00fcch\u00b7te", "prei\u00b7sen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wer mir die\u00dffals wiederspricht,", "tokens": ["Wer", "mir", "die\u00df\u00b7fals", "wie\u00b7der\u00b7spricht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "K\u00f6mpt mir nicht", "tokens": ["K\u00f6mpt", "mir", "nicht"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "In die sch\u00f6ne Zahl der Weisen.", "tokens": ["In", "die", "sch\u00f6\u00b7ne", "Zahl", "der", "Wei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Du wirst nicht, wie ich auch pflag,", "tokens": ["Du", "wirst", "nicht", ",", "wie", "ich", "auch", "pflag", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "$,", "PWAV", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Jeden Tag", "tokens": ["Je\u00b7den", "Tag"], "token_info": ["word", "word"], "pos": ["PIAT", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Mehr an frembde Tische gehen,", "tokens": ["Mehr", "an", "fremb\u00b7de", "Ti\u00b7sche", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nicht, so lang der Hau\u00dfwirth wil,", "tokens": ["Nicht", ",", "so", "lang", "der", "Hau\u00df\u00b7wirth", "wil", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "ADV", "ADJD", "ART", "NN", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Jetzund still", "tokens": ["Je\u00b7tzund", "still"], "token_info": ["word", "word"], "pos": ["ADV", "ADJD"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Sitzen bleiben, dann auffstehen.", "tokens": ["Sit\u00b7zen", "blei\u00b7ben", ",", "dann", "auffs\u00b7te\u00b7hen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "ADV", "VVINF", "$."], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Kanst nun essen vnbekr\u00e4nckt,", "tokens": ["Kanst", "nun", "es\u00b7sen", "vn\u00b7be\u00b7kr\u00e4nckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Was Gott schenckt,", "tokens": ["Was", "Gott", "schenckt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Vnd auch trincken nach belieben,", "tokens": ["Vnd", "auch", "trin\u00b7cken", "nach", "be\u00b7lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "APPR", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tr\u00e4gst f\u00fcr keinen Stunden Schew,", "tokens": ["Tr\u00e4gst", "f\u00fcr", "kei\u00b7nen", "Stun\u00b7den", "Schew", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "NN", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Schl\u00e4ffest frey", "tokens": ["Schl\u00e4f\u00b7fest", "frey"], "token_info": ["word", "word"], "pos": ["VVFIN", "ADJD"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "In den Tag auch bi\u00df nach Sieben.", "tokens": ["In", "den", "Tag", "auch", "bi\u00df", "nach", "Sie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "ADV", "APPR", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Sind wir vnterdessen gleich", "tokens": ["Sind", "wir", "vn\u00b7ter\u00b7des\u00b7sen", "gleich"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Nicht so reich,", "tokens": ["Nicht", "so", "reich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Da\u00df wir stets viel Tausent heben,", "tokens": ["Da\u00df", "wir", "stets", "viel", "Tau\u00b7sent", "he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ey so giebt Gott t\u00e4glich Brodt,", "tokens": ["Ey", "so", "giebt", "Gott", "t\u00e4g\u00b7lich", "Brodt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "NN", "ADJD", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Heisset Noht", "tokens": ["Heis\u00b7set", "Noht"], "token_info": ["word", "word"], "pos": ["NE", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Von vns fern seyn, da\u00df wir leben.", "tokens": ["Von", "vns", "fern", "seyn", ",", "da\u00df", "wir", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADJD", "VAINF", "$,", "KOUS", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Niemand stirbt nicht, da\u00df er Geldt", "tokens": ["Nie\u00b7mand", "stirbt", "nicht", ",", "da\u00df", "er", "Geldt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Filtzig h\u00e4lt", "tokens": ["Filt\u00b7zig", "h\u00e4lt"], "token_info": ["word", "word"], "pos": ["NE", "VVFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Tieff vnd H\u00f6llen-ab vergraben,", "tokens": ["Tieff", "vnd", "H\u00f6l\u00b7len\u00b7ab", "ver\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sondern, da\u00df er vmb vnd an", "tokens": ["Son\u00b7dern", ",", "da\u00df", "er", "vmb", "vnd", "an"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "KOUS", "PPER", "APPR", "KON", "APPR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Das nur kan,", "tokens": ["Das", "nur", "kan", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VMFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "So vns t\u00e4glich sat macht, haben.", "tokens": ["So", "vns", "t\u00e4g\u00b7lich", "sat", "macht", ",", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "PPER", "ADJD", "ADJD", "VVFIN", "$,", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Die\u00df vnd mehr noch sollt' ich wol", "tokens": ["Die\u00df", "vnd", "mehr", "noch", "sollt'", "ich", "wol"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "KON", "ADV", "ADV", "VMFIN", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Frewden-voll", "tokens": ["Fre\u00b7wden\u00b7voll"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Dir, mein Ohm, zu Ehren schlagen,", "tokens": ["Dir", ",", "mein", "Ohm", ",", "zu", "Eh\u00b7ren", "schla\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "NN", "$,", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aber vnsre Trawer-Zeit", "tokens": ["A\u00b7ber", "vns\u00b7re", "Tra\u00b7wer\u00b7Zeit"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Zwingt mich heut", "tokens": ["Zwingt", "mich", "heut"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Aller Frewde zu entsagen.", "tokens": ["Al\u00b7ler", "Frew\u00b7de", "zu", "ent\u00b7sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Gestern ward mit Klag'vnd Pein", "tokens": ["Ge\u00b7stern", "ward", "mit", "Klag'v\u00b7nd", "Pein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "APPR", "NN", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Das Gebein", "tokens": ["Das", "Ge\u00b7bein"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Adam Oeders eingesencket,", "tokens": ["A\u00b7dam", "O\u00b7e\u00b7ders", "ein\u00b7ge\u00b7sen\u00b7cket", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ach! des Mannes Todt hat mich", "tokens": ["Ach", "!", "des", "Man\u00b7nes", "Todt", "hat", "mich"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "ART", "NN", "NN", "VAFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Gleich wie dich", "tokens": ["Gleich", "wie", "dich"], "token_info": ["word", "word", "word"], "pos": ["ADV", "KOKOM", "PPER"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Ebenm\u00e4ssig gantz durchkr\u00e4ncket.", "tokens": ["E\u00b7ben\u00b7m\u00e4s\u00b7sig", "gantz", "durch\u00b7kr\u00e4n\u00b7cket", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Ach! wer hat an jhm doch nicht", "tokens": ["Ach", "!", "wer", "hat", "an", "jhm", "doch", "nicht"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "PWS", "VAFIN", "APPR", "PPER", "ADV", "PTKNEG"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Freundschafft, Pflicht,", "tokens": ["Freund\u00b7schafft", ",", "Pflicht", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Trew vnd Redlicheit geehret?", "tokens": ["Trew", "vnd", "Red\u00b7lic\u00b7heit", "ge\u00b7eh\u00b7ret", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Drumb er auch nicht in die Welt,", "tokens": ["Drumb", "er", "auch", "nicht", "in", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "ADV", "PTKNEG", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Die nur h\u00e4lt", "tokens": ["Die", "nur", "h\u00e4lt"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADV", "VVFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Auff politisch seyn, geh\u00f6ret.", "tokens": ["Auff", "po\u00b7li\u00b7tisch", "seyn", ",", "ge\u00b7h\u00f6\u00b7ret", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJD", "VAINF", "$