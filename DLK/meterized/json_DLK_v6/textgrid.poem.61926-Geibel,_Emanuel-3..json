{"textgrid.poem.61926": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun hast du, Fl\u00fcchtling, uns verlassen,", "tokens": ["Nun", "hast", "du", ",", "Fl\u00fccht\u00b7ling", ",", "uns", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "NN", "$,", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und Licht und Lust floh mit dahin:", "tokens": ["Und", "Licht", "und", "Lust", "floh", "mit", "da\u00b7hin", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVFIN", "APPR", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verwaist im Nebel ruhn die Gassen,", "tokens": ["Ver\u00b7waist", "im", "Ne\u00b7bel", "ruhn", "die", "Gas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und kaum begreif' ich, wo ich bin.", "tokens": ["Und", "kaum", "be\u00b7greif'", "ich", ",", "wo", "ich", "bin", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$,", "PWAV", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Bedeutungslos erschallt der Menge", "tokens": ["Be\u00b7deu\u00b7tungs\u00b7los", "er\u00b7schallt", "der", "Men\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gesch\u00e4ft'ger L\u00e4rm zu mir empor;", "tokens": ["Ge\u00b7sch\u00e4ft'\u00b7ger", "L\u00e4rm", "zu", "mir", "em\u00b7por", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was wei\u00df ich von des Tags Gedr\u00e4nge?", "tokens": ["Was", "wei\u00df", "ich", "von", "des", "Tags", "Ge\u00b7dr\u00e4n\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich wei\u00df nur, da\u00df ich dich verlor.", "tokens": ["Ich", "wei\u00df", "nur", ",", "da\u00df", "ich", "dich", "ver\u00b7lor", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und fl\u00fccht' ich abends zu den Brettern,", "tokens": ["Und", "fl\u00fccht'", "ich", "a\u00b7bends", "zu", "den", "Bret\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die mir dein Zauber j\u00fcngst beseelt,", "tokens": ["Die", "mir", "dein", "Zau\u00b7ber", "j\u00fcngst", "be\u00b7seelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPOSAT", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ach, klanglos stehn sie, von den G\u00f6ttern", "tokens": ["Ach", ",", "klang\u00b7los", "stehn", "sie", ",", "von", "den", "G\u00f6t\u00b7tern"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "ADJD", "VVFIN", "PPER", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Verlassen, da die Priestrin fehlt.", "tokens": ["Ver\u00b7las\u00b7sen", ",", "da", "die", "Priest\u00b7rin", "fehlt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Da rettet sich der Schmerz nach innen,", "tokens": ["Da", "ret\u00b7tet", "sich", "der", "Schmerz", "nach", "in\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "APPR", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und wie die m\u00fcde Wimper fiel,", "tokens": ["Und", "wie", "die", "m\u00fc\u00b7de", "Wim\u00b7per", "fiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Beginnt vor halb entschlafnen Sinnen", "tokens": ["Be\u00b7ginnt", "vor", "halb", "ent\u00b7schlaf\u00b7nen", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Erinnrung ihr phantastisch Spiel.", "tokens": ["E\u00b7rinn\u00b7rung", "ihr", "phan\u00b7tas\u00b7tisch", "Spiel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "All die Gestalten seh' ich wieder,", "tokens": ["All", "die", "Ge\u00b7stal\u00b7ten", "seh'", "ich", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Drin du dich wechselnd offenbart,", "tokens": ["Drin", "du", "dich", "wech\u00b7selnd", "of\u00b7fen\u00b7bart", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PRF", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Blick, den Gang, den Schwung der Glieder,", "tokens": ["Den", "Blick", ",", "den", "Gang", ",", "den", "Schwung", "der", "Glie\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den s\u00fc\u00dfen Leib, der Sprache ward.", "tokens": ["Den", "s\u00fc\u00b7\u00dfen", "Leib", ",", "der", "Spra\u00b7che", "ward", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Bet\u00f6rend dringt zu meinen Ohren", "tokens": ["Be\u00b7t\u00f6\u00b7rend", "dringt", "zu", "mei\u00b7nen", "Oh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Stimme wieder, deren Klang,", "tokens": ["Die", "Stim\u00b7me", "wie\u00b7der", ",", "de\u00b7ren", "Klang", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "PRELAT", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Aus wildbewegter Brust geboren,", "tokens": ["Aus", "wild\u00b7be\u00b7weg\u00b7ter", "Brust", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die ganze Seele mir bezwang.", "tokens": ["Die", "gan\u00b7ze", "See\u00b7le", "mir", "be\u00b7zwang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "So schleicht in schattenhaftem Sehnen", "tokens": ["So", "schleicht", "in", "schat\u00b7ten\u00b7haf\u00b7tem", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Nacht mir, die kein Schlummer k\u00fcrzt,", "tokens": ["Die", "Nacht", "mir", ",", "die", "kein", "Schlum\u00b7mer", "k\u00fcrzt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bis endlich wild ein Strom von Tr\u00e4nen", "tokens": ["Bis", "end\u00b7lich", "wild", "ein", "Strom", "von", "Tr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "ADJD", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Erleichternd aus den Augen st\u00fcrzt.", "tokens": ["Er\u00b7leich\u00b7ternd", "aus", "den", "Au\u00b7gen", "st\u00fcrzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "O h\u00e4tt' ich niemals kosten d\u00fcrfen", "tokens": ["O", "h\u00e4tt'", "ich", "nie\u00b7mals", "kos\u00b7ten", "d\u00fcr\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "PPER", "ADV", "VVINF", "VMINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vom Kelch, der mir mein Selbst entrafft!", "tokens": ["Vom", "Kelch", ",", "der", "mir", "mein", "Selbst", "en\u00b7trafft", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nur Poesie dacht' ich zu schl\u00fcrfen", "tokens": ["Nur", "Poe\u00b7sie", "dacht'", "ich", "zu", "schl\u00fcr\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "VVFIN", "PPER", "PTKZU", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und trank das Gift der Leidenschaft.", "tokens": ["Und", "trank", "das", "Gift", "der", "Lei\u00b7den\u00b7schaft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}