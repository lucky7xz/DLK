{"textgrid.poem.48657": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "20. An die Stolze", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und gleichwol kan ich anders nicht,", "tokens": ["Und", "gleich\u00b7wol", "kan", "ich", "an\u00b7ders", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "ADV", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "ich mu\u00df ihr g\u00f6nstig sein,", "tokens": ["ich", "mu\u00df", "ihr", "g\u00f6ns\u00b7tig", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADJD", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "obgleich der Augen stolzes Licht", "tokens": ["ob\u00b7gleich", "der", "Au\u00b7gen", "stol\u00b7zes", "Licht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "mir mi\u00dfg\u00f6nnt seinen Schein.", "tokens": ["mir", "mi\u00df\u00b7g\u00f6nnt", "sei\u00b7nen", "Schein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich will, ich soll, ich mu\u00df dich lieben,", "tokens": ["Ich", "will", ",", "ich", "soll", ",", "ich", "mu\u00df", "dich", "lie\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PPER", "VMFIN", "$,", "PPER", "VMFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "dadurch wir beid' uns nur betr\u00fcben,", "tokens": ["da\u00b7durch", "wir", "beid'", "uns", "nur", "be\u00b7tr\u00fc\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "VVFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "weil mein Wundsch doch nicht gilt", "tokens": ["weil", "mein", "Wund\u00b7sch", "doch", "nicht", "gilt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "PTKNEG", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "und du nicht h\u00f6ren wilt.", "tokens": ["und", "du", "nicht", "h\u00f6\u00b7ren", "wilt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wie manchen Tag, wie manche Nacht,", "tokens": ["Wie", "man\u00b7chen", "Tag", ",", "wie", "man\u00b7che", "Nacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "$,", "PWAV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "wie manche liebe Zeit", "tokens": ["wie", "man\u00b7che", "lie\u00b7be", "Zeit"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PIAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "hab' ich mit Klagen durchgebracht,", "tokens": ["hab'", "ich", "mit", "Kla\u00b7gen", "durch\u00b7ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und du verlachst mein Leid!", "tokens": ["und", "du", "ver\u00b7lachst", "mein", "Leid", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da wei\u00dft, du h\u00f6rst, da siehst die Schmerzen", "tokens": ["Da", "wei\u00dft", ",", "du", "h\u00f6rst", ",", "da", "siehst", "die", "Schmer\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "und nimmst der keinen doch zu Herzen,", "tokens": ["und", "nimmst", "der", "kei\u00b7nen", "doch", "zu", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "PIAT", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "so da\u00df ich zweifle fast,", "tokens": ["so", "da\u00df", "ich", "zweif\u00b7le", "fast", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "ob du ein Herze hast.", "tokens": ["ob", "du", "ein", "Her\u00b7ze", "hast", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Bist du denn harter Stein und Stahl,", "tokens": ["Bist", "du", "denn", "har\u00b7ter", "Stein", "und", "Stahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "die man doch zwingen kan?", "tokens": ["die", "man", "doch", "zwin\u00b7gen", "kan", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Feld, Wiesen, W\u00e4lder, Berg und Tal", "tokens": ["Feld", ",", "Wie\u00b7sen", ",", "W\u00e4l\u00b7der", ",", "Berg", "und", "Tal"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "seh'n meinen Wehmut an.", "tokens": ["seh'n", "mei\u00b7nen", "Weh\u00b7mut", "an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die V\u00f6gel seufzen, was ich klage.", "tokens": ["Die", "V\u00f6\u00b7gel", "seuf\u00b7zen", ",", "was", "ich", "kla\u00b7ge", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der hole Pusch ruft, was ich sage.", "tokens": ["Der", "ho\u00b7le", "Pusch", "ruft", ",", "was", "ich", "sa\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Du nur, du Stolze du,", "tokens": ["Du", "nur", ",", "du", "Stol\u00b7ze", "du", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "PPER", "ADJA", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "h\u00e4ltst Ohr- und Augen zu.", "tokens": ["h\u00e4ltst", "Ohr", "und", "Au\u00b7gen", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "TRUNC", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ach, denke, denke, was du tust!", "tokens": ["Ach", ",", "den\u00b7ke", ",", "den\u00b7ke", ",", "was", "du", "tust", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "$,", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich kan nicht anders sein.", "tokens": ["Ich", "kan", "nicht", "an\u00b7ders", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADV", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich hab' an meinem Leiden Lust,", "tokens": ["Ich", "hab'", "an", "mei\u00b7nem", "Lei\u00b7den", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "du hassest meine Pein.", "tokens": ["du", "has\u00b7sest", "mei\u00b7ne", "Pein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Kan ich denn keine Huld' erlangen,", "tokens": ["Kan", "ich", "denn", "kei\u00b7ne", "Huld'", "er\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "so la\u00df mich die Gunst nur empfangen", "tokens": ["so", "la\u00df", "mich", "die", "Gunst", "nur", "emp\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "PPER", "ART", "NN", "ADV", "VVPP"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "und wolle doch mit mir,", "tokens": ["und", "wol\u00b7le", "doch", "mit", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "da\u00df ich stracks sterbe hier!", "tokens": ["da\u00df", "ich", "stracks", "ster\u00b7be", "hier", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}