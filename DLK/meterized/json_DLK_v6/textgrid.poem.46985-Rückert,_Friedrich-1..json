{"textgrid.poem.46985": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als Bl\u00fccher auf dem Feld der Schlacht", "tokens": ["Als", "Bl\u00fc\u00b7cher", "auf", "dem", "Feld", "der", "Schlacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gewaltig disputieret,", "tokens": ["Ge\u00b7wal\u00b7tig", "dis\u00b7pu\u00b7tie\u00b7ret", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wo Gott der Herr mit seiner Macht", "tokens": ["Wo", "Gott", "der", "Herr", "mit", "sei\u00b7ner", "Macht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihm selber pr\u00e4sidieret;", "tokens": ["Ihm", "sel\u00b7ber", "pr\u00e4\u00b7si\u00b7die\u00b7ret", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Hat England ihn daf\u00fcr", "tokens": ["Hat", "En\u00b7gland", "ihn", "da\u00b7f\u00fcr"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "NE", "PPER", "PAV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Nach Recht und nach Geb\u00fchr", "tokens": ["Nach", "Recht", "und", "nach", "Ge\u00b7b\u00fchr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Gemacht zum Doctor juris.", "tokens": ["Ge\u00b7macht", "zum", "Doc\u00b7tor", "ju\u00b7ris", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Doctor von echtem Ritterrang,", "tokens": ["Doc\u00b7tor", "von", "ech\u00b7tem", "Rit\u00b7ter\u00b7rang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.2": {"text": "Das Schwert ist deine Feder,", "tokens": ["Das", "Schwert", "ist", "dei\u00b7ne", "Fe\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Streitsach' ist ein Waffengang,", "tokens": ["Die", "Streit\u00b7sach'", "ist", "ein", "Waf\u00b7fen\u00b7gang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Schlachtfeld der Katheder;", "tokens": ["Das", "Schlacht\u00b7feld", "der", "Ka\u00b7the\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Da trittst du mit Gewicht", "tokens": ["Da", "trittst", "du", "mit", "Ge\u00b7wicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Dem Feind vors Angesicht,", "tokens": ["Dem", "Feind", "vors", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Als rechter Doctor juris.", "tokens": ["Als", "rech\u00b7ter", "Doc\u00b7tor", "ju\u00b7ris", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Fahr' nur in dem Prozesse fort,", "tokens": ["Fahr'", "nur", "in", "dem", "Pro\u00b7zes\u00b7se", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+---+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Den du mit ihm begonnen,", "tokens": ["Den", "du", "mit", "ihm", "be\u00b7gon\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "F\u00fchr' mit Kanonenschall dein Wort,", "tokens": ["F\u00fchr'", "mit", "Ka\u00b7no\u00b7nen\u00b7schall", "dein", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Bis da\u00df du hast gewonnen.", "tokens": ["Bis", "da\u00df", "du", "hast", "ge\u00b7won\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Lehr' unser deutsches Recht", "tokens": ["Lehr'", "un\u00b7ser", "deut\u00b7sches", "Recht"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Dem Franzmann im Gefecht,", "tokens": ["Dem", "Franz\u00b7mann", "im", "Ge\u00b7fecht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Held Bl\u00fccher, Doctor juris!", "tokens": ["Held", "Bl\u00fc\u00b7cher", ",", "Doc\u00b7tor", "ju\u00b7ris", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "FM.la", "FM.la", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}