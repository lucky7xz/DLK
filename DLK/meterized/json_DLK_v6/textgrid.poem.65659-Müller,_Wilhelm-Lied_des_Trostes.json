{"textgrid.poem.65659": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Lied des Trostes", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit uns, mit uns ist Gott, der Herr! Drum Br\u00fcder, zaget nicht,", "tokens": ["Mit", "uns", ",", "mit", "uns", "ist", "Gott", ",", "der", "Herr", "!", "Drum", "Br\u00fc\u00b7der", ",", "za\u00b7get", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "APPR", "PPER", "VAFIN", "NN", "$,", "ART", "NN", "$.", "PAV", "NN", "$,", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Wenn \u00fcber unsern H\u00e4uptern auch die Wetterwolke bricht,", "tokens": ["Wenn", "\u00fc\u00b7ber", "un\u00b7sern", "H\u00e4up\u00b7tern", "auch", "die", "Wet\u00b7ter\u00b7wol\u00b7ke", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPOSAT", "NN", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Die Donnerpfeile niederschie\u00dft und rothe Flammen speit!", "tokens": ["Die", "Don\u00b7ner\u00b7pfei\u00b7le", "nie\u00b7der\u00b7schie\u00dft", "und", "ro\u00b7the", "Flam\u00b7men", "speit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Mit uns, mit uns ist Gott, der Herr! Zum Zagen ist nicht Zeit.", "tokens": ["Mit", "uns", ",", "mit", "uns", "ist", "Gott", ",", "der", "Herr", "!", "Zum", "Za\u00b7gen", "ist", "nicht", "Zeit", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "APPR", "PPER", "VAFIN", "NN", "$,", "ART", "NN", "$.", "APPRART", "NN", "VAFIN", "PTKNEG", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}, "stanza.2": {"line.1": {"text": "Ob unter solchen Schl\u00e4gen auch der Heide niederf\u00e4llt,", "tokens": ["Ob", "un\u00b7ter", "sol\u00b7chen", "Schl\u00e4\u00b7gen", "auch", "der", "Hei\u00b7de", "nie\u00b7der\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PIAT", "NN", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Die Faust geballt, das Haar gestr\u00e4ubt, allein auf weiter Welt,", "tokens": ["Die", "Faust", "ge\u00b7ballt", ",", "das", "Haar", "ge\u00b7str\u00e4ubt", ",", "al\u00b7lein", "auf", "wei\u00b7ter", "Welt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "ART", "NN", "VVPP", "$,", "ADV", "APPR", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Ob er den Boden w\u00fchlt und stampft, und in den Rasen bei\u00dft,", "tokens": ["Ob", "er", "den", "Bo\u00b7den", "w\u00fchlt", "und", "stampft", ",", "und", "in", "den", "Ra\u00b7sen", "bei\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "KON", "VVFIN", "$,", "KON", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Und, seinen Blick zur Gruft gekehrt, verflucht den L\u00fcgengeist,", "tokens": ["Und", ",", "sei\u00b7nen", "Blick", "zur", "Gruft", "ge\u00b7kehrt", ",", "ver\u00b7flucht", "den", "L\u00fc\u00b7gen\u00b7geist", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PPOSAT", "NN", "APPRART", "NN", "VVPP", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.5": {"text": "Der ihm Triumph und Heil verhie\u00df im Kampfe f\u00fcr den Mond,", "tokens": ["Der", "ihm", "Tri\u00b7umph", "und", "Heil", "ver\u00b7hie\u00df", "im", "Kamp\u00b7fe", "f\u00fcr", "den", "Mond", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "NN", "KON", "NN", "VVFIN", "APPRART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.6": {"text": "Und nun mit Wunden, Schmach und Tod den Gl\u00e4ubigen belohnt:", "tokens": ["Und", "nun", "mit", "Wun\u00b7den", ",", "Schmach", "und", "Tod", "den", "Gl\u00e4u\u00b7bi\u00b7gen", "be\u00b7lohnt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "$,", "NN", "KON", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.7": {"text": "Wir Christen haben andern Brauch: sind auch die H\u00e4nde wund,", "tokens": ["Wir", "Chris\u00b7ten", "ha\u00b7ben", "an\u00b7dern", "Brauch", ":", "sind", "auch", "die", "H\u00e4n\u00b7de", "wund", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VAFIN", "ADJA", "NN", "$.", "VAFIN", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.8": {"text": "Wir falten sie zusammen doch in unsrer letzten Stund',", "tokens": ["Wir", "fal\u00b7ten", "sie", "zu\u00b7sam\u00b7men", "doch", "in", "uns\u00b7rer", "letz\u00b7ten", "Stund'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.9": {"text": "Und sinken wir zur Erde hin, wir sinken auf die Knie',", "tokens": ["Und", "sin\u00b7ken", "wir", "zur", "Er\u00b7de", "hin", ",", "wir", "sin\u00b7ken", "auf", "die", "Knie'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$,", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.10": {"text": "Und brechen unsre Augen auch, gen Himmel brechen sie.", "tokens": ["Und", "bre\u00b7chen", "uns\u00b7re", "Au\u00b7gen", "auch", ",", "gen", "Him\u00b7mel", "bre\u00b7chen", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "ADV", "$,", "APPR", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.11": {"text": "Mit uns, mit uns ist Gott, der Herr! Wir k\u00fcssen fromm die Hand,", "tokens": ["Mit", "uns", ",", "mit", "uns", "ist", "Gott", ",", "der", "Herr", "!", "Wir", "k\u00fcs\u00b7sen", "fromm", "die", "Hand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "APPR", "PPER", "VAFIN", "NN", "$,", "ART", "NN", "$.", "PPER", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.12": {"text": "Die Wonn' und Sieg, die Pein und Tod auf uns herab gesandt.", "tokens": ["Die", "Wonn'", "und", "Sieg", ",", "die", "Pein", "und", "Tod", "auf", "uns", "her\u00b7ab", "ge\u00b7sandt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$,", "ART", "NN", "KON", "NN", "APPR", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.13": {"text": "Aus Noth und Tod in's Morgenroth! sei unser Feldgeschrei.", "tokens": ["Aus", "Noth", "und", "Tod", "in's", "Mor\u00b7gen\u00b7roth", "!", "sei", "un\u00b7ser", "Feld\u00b7ge\u00b7schrei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "APPRART", "NN", "$.", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.14": {"text": "Ist es nicht ehr, ", "tokens": ["Ist", "es", "nicht", "ehr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}}}}}