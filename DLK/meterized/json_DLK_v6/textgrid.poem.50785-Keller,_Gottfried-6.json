{"textgrid.poem.50785": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "6", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Horch! Stimmen und Geschrei, doch kaum zu h\u00f6ren,", "tokens": ["Horch", "!", "Stim\u00b7men", "und", "Ge\u00b7schrei", ",", "doch", "kaum", "zu", "h\u00f6\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "NN", "KON", "NN", "$,", "ADV", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dumpf und verworren t\u00f6nt es, wie von ferne,", "tokens": ["Dumpf", "und", "ver\u00b7wor\u00b7ren", "t\u00f6nt", "es", ",", "wie", "von", "fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VVFIN", "PPER", "$,", "PWAV", "APPR", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und ich erkenn sie, die alln\u00e4chtlich st\u00f6ren", "tokens": ["Und", "ich", "er\u00b7kenn", "sie", ",", "die", "all\u00b7n\u00e4cht\u00b7lich", "st\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "$,", "PRELS", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Toten Schlaf, den stillen Gang der Sterne.", "tokens": ["Der", "To\u00b7ten", "Schlaf", ",", "den", "stil\u00b7len", "Gang", "der", "Ster\u00b7ne", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der trunkne K\u00fcster, aus dem Schank gekommen,", "tokens": ["Der", "trunk\u00b7ne", "K\u00fcs\u00b7ter", ",", "aus", "dem", "Schank", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Setzt sich noch in den Mondschein vor dem Hause,", "tokens": ["Setzt", "sich", "noch", "in", "den", "Mond\u00b7schein", "vor", "dem", "Hau\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "APPR", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.3": {"text": "Kr\u00e4ht einen Psalm; doch kaum hat sie's vernommen,", "tokens": ["Kr\u00e4ht", "ei\u00b7nen", "Psalm", ";", "doch", "kaum", "hat", "sie's", "ver\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$.", "ADV", "ADV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So st\u00fcrzt sein Weib hervor mit Zorngebrause,", "tokens": ["So", "st\u00fcrzt", "sein", "Weib", "her\u00b7vor", "mit", "Zorn\u00b7ge\u00b7brau\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PTKVZ", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Hei\u00dft ihn hereingehn und beschilt ihn grimmig,", "tokens": ["Hei\u00dft", "ihn", "her\u00b7ein\u00b7gehn", "und", "be\u00b7schilt", "ihn", "grim\u00b7mig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVINF", "KON", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hell kr\u00e4ht zum Mond indessen der Geselle:", "tokens": ["Hell", "kr\u00e4ht", "zum", "Mond", "in\u00b7des\u00b7sen", "der", "Ge\u00b7sel\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPRART", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So mischet sich, erbost und eulenstimmig,", "tokens": ["So", "mi\u00b7schet", "sich", ",", "er\u00b7bost", "und", "eu\u00b7len\u00b7stim\u00b7mig", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "$,", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ihr Zanken in sein trunkenes Gebelle.", "tokens": ["Ihr", "Zan\u00b7ken", "in", "sein", "trun\u00b7ke\u00b7nes", "Ge\u00b7bel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Sie mu\u00df ganz nah sein, da ich es kann h\u00f6ren,", "tokens": ["Sie", "mu\u00df", "ganz", "nah", "sein", ",", "da", "ich", "es", "kann", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADJD", "VAINF", "$,", "KOUS", "PPER", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die \u00fcberkommne, alte Pfr\u00fcnderh\u00f6hle;", "tokens": ["Die", "\u00fc\u00b7ber\u00b7komm\u00b7ne", ",", "al\u00b7te", "Pfr\u00fcn\u00b7der\u00b7h\u00f6h\u00b7le", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "La\u00df sehn, ob das Gesindel ist zu st\u00f6ren:", "tokens": ["La\u00df", "sehn", ",", "ob", "das", "Ge\u00b7sin\u00b7del", "ist", "zu", "st\u00f6\u00b7ren", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVINF", "$,", "KOUS", "ART", "NN", "VAFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Schrei, was du kannst, angstvoll gepre\u00dfte Kehle!", "tokens": ["Schrei", ",", "was", "du", "kannst", ",", "angst\u00b7voll", "ge\u00b7pre\u00df\u00b7te", "Keh\u00b7le", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "PPER", "VMFIN", "$,", "ADJD", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.5": {"line.1": {"text": "Die T\u00fcr schl\u00e4gt zu \u2013 der L\u00e4rm hat sich verloren,", "tokens": ["Die", "T\u00fcr", "schl\u00e4gt", "zu", "\u2013", "der", "L\u00e4rm", "hat", "sich", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "$(", "ART", "NN", "VAFIN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es h\u00fclfe nichts, wenn ich zu Tod mich riefe;", "tokens": ["Es", "h\u00fcl\u00b7fe", "nichts", ",", "wenn", "ich", "zu", "Tod", "mich", "rie\u00b7fe", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "$,", "KOUS", "PPER", "APPR", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie stopfen furchtsam ihre langen Ohren", "tokens": ["Sie", "stop\u00b7fen", "furcht\u00b7sam", "ih\u00b7re", "lan\u00b7gen", "Oh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vor meinem Hilferufen aus der Tiefe.", "tokens": ["Vor", "mei\u00b7nem", "Hil\u00b7fe\u00b7ru\u00b7fen", "aus", "der", "Tie\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}