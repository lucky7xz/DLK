{"textgrid.poem.39455": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: So wie der kluge G\u00e4rtner saubre G\u00e4nge", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So wie der kluge G\u00e4rtner saubre G\u00e4nge", "tokens": ["So", "wie", "der", "klu\u00b7ge", "G\u00e4rt\u00b7ner", "saub\u00b7re", "G\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "ART", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Um zierlich eingefa\u00dfte Beete ziehet,", "tokens": ["Um", "zier\u00b7lich", "ein\u00b7ge\u00b7fa\u00df\u00b7te", "Bee\u00b7te", "zie\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJD", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Allein nicht hemmt, nur pflegt, was drinnen bl\u00fchet,", "tokens": ["Al\u00b7lein", "nicht", "hemmt", ",", "nur", "pflegt", ",", "was", "drin\u00b7nen", "bl\u00fc\u00b7het", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "VVFIN", "$,", "ADV", "VVFIN", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df sich die Kraft der Pflanzen \u00fcppig dr\u00e4nge:", "tokens": ["Da\u00df", "sich", "die", "Kraft", "der", "Pflan\u00b7zen", "\u00fcp\u00b7pig", "dr\u00e4n\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "So ist Boccaccio, der Geschichten Menge", "tokens": ["So", "ist", "Boc\u00b7cac\u00b7cio", ",", "der", "Ge\u00b7schich\u00b7ten", "Men\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "NE", "$,", "ART", "NN", "NN"], "meter": "-++-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Als Blumenflor zu ordnen, wohl bem\u00fchet;", "tokens": ["Als", "Blu\u00b7men\u00b7flor", "zu", "ord\u00b7nen", ",", "wohl", "be\u00b7m\u00fc\u00b7het", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "NN", "PTKZU", "VVINF", "$,", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Rings schm\u00fccken, wie ein goldner Rahmen gl\u00fchet,", "tokens": ["Rings", "schm\u00fc\u00b7cken", ",", "wie", "ein", "gold\u00b7ner", "Rah\u00b7men", "gl\u00fc\u00b7het", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "PWAV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie heitre Reden, Landlust, Spiel, Ges\u00e4nge.", "tokens": ["Sie", "heit\u00b7re", "Re\u00b7den", ",", "Land\u00b7lust", ",", "Spiel", ",", "Ge\u00b7s\u00e4n\u00b7ge", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Bet\u00e4ubt des Gartens Duft die zarte Jugend,", "tokens": ["Be\u00b7t\u00e4ubt", "des", "Gar\u00b7tens", "Duft", "die", "zar\u00b7te", "Ju\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verdammt die Spr\u00f6de, wo sie gern err\u00f6thet,", "tokens": ["Ver\u00b7dammt", "die", "Spr\u00f6\u00b7de", ",", "wo", "sie", "gern", "er\u00b7r\u00f6t\u00b7het", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PWAV", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und lernen neue T\u00fccken selbst die Schlauen:", "tokens": ["Und", "ler\u00b7nen", "neu\u00b7e", "T\u00fc\u00b7cken", "selbst", "die", "Schlau\u00b7en", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJA", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "So wirft sich, glaubensvoll an ihre Tugend", "tokens": ["So", "wirft", "sich", ",", "glau\u00b7bens\u00b7voll", "an", "ih\u00b7re", "Tu\u00b7gend"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "$,", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und Sittsamkeit, die nicht ein Hauch ert\u00f6dtet,", "tokens": ["Und", "Sitt\u00b7sam\u00b7keit", ",", "die", "nicht", "ein", "Hauch", "er\u00b7t\u00f6d\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PRELS", "PTKNEG", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Dichter in den Schutz der edlen Frauen.", "tokens": ["Der", "Dich\u00b7ter", "in", "den", "Schutz", "der", "ed\u00b7len", "Frau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}