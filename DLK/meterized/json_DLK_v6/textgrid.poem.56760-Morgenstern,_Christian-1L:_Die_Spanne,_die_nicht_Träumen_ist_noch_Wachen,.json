{"textgrid.poem.56760": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Spanne, die nicht Tr\u00e4umen ist noch Wachen,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Spanne, die nicht Tr\u00e4umen ist noch Wachen,", "tokens": ["Die", "Span\u00b7ne", ",", "die", "nicht", "Tr\u00e4u\u00b7men", "ist", "noch", "Wa\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PTKNEG", "NN", "VAFIN", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "beschenkt mich oft mit seltsamen Gedichten:", "tokens": ["be\u00b7schenkt", "mich", "oft", "mit", "selt\u00b7sa\u00b7men", "Ge\u00b7dich\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Der Geist, erregt, aus Chaos Welt zu machen,", "tokens": ["Der", "Geist", ",", "er\u00b7regt", ",", "aus", "Chaos", "Welt", "zu", "ma\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVPP", "$,", "APPR", "NE", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "gebiert ein Heer von landschaftlichen Sichten.", "tokens": ["ge\u00b7biert", "ein", "Heer", "von", "land\u00b7schaft\u00b7li\u00b7chen", "Sich\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Da wechseln Berge, T\u00e4ler, Ebnen, Fl\u00fcsse,", "tokens": ["Da", "wech\u00b7seln", "Ber\u00b7ge", ",", "T\u00e4\u00b7ler", ",", "Eb\u00b7nen", ",", "Fl\u00fcs\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "da gr\u00fcnt ein Wald, da t\u00fcrmt es sich graniten,", "tokens": ["da", "gr\u00fcnt", "ein", "Wald", ",", "da", "t\u00fcrmt", "es", "sich", "gra\u00b7ni\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADV", "VVFIN", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "da zuckt ein Blitz, da rauschen Regeng\u00fcsse,", "tokens": ["da", "zuckt", "ein", "Blitz", ",", "da", "rau\u00b7schen", "Re\u00b7gen\u00b7g\u00fcs\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "KOUS", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "und Mensch und Tier bewegen sich inmitten.", "tokens": ["und", "Mensch", "und", "Tier", "be\u00b7we\u00b7gen", "sich", "in\u00b7mit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVFIN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Das sind der Vordern fortgepflanzte Wellen,", "tokens": ["Das", "sind", "der", "Vor\u00b7dern", "fort\u00b7ge\u00b7pflanz\u00b7te", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "die meinen Sinn bereitet und bereichert,", "tokens": ["die", "mei\u00b7nen", "Sinn", "be\u00b7rei\u00b7tet", "und", "be\u00b7rei\u00b7chert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "das Erbe ihrer Form- und Farbenzellen,", "tokens": ["das", "Er\u00b7be", "ih\u00b7rer", "For\u00b7m", "und", "Far\u00b7ben\u00b7zel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "TRUNC", "KON", "NN", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "darin die halbe Erde aufgespeichert.", "tokens": ["da\u00b7rin", "die", "hal\u00b7be", "Er\u00b7de", "auf\u00b7ge\u00b7spei\u00b7chert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}