{"textgrid.poem.32864": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ja, liebe Mutter, ihr habt Recht,", "genre": "verse", "period": "N.A.", "pub_year": 1765, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ja, liebe Mutter, ihr habt Recht,", "tokens": ["Ja", ",", "lie\u00b7be", "Mut\u00b7ter", ",", "ihr", "habt", "Recht", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADJA", "NN", "$,", "PPER", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df ich der M\u00e4nner falsch Geschlecht,", "tokens": ["Da\u00df", "ich", "der", "M\u00e4n\u00b7ner", "falsch", "Ge\u00b7schlecht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie ihr verlangt, zu hassen schwur:", "tokens": ["Wie", "ihr", "ver\u00b7langt", ",", "zu", "has\u00b7sen", "schwur", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch da ich schwur, da kannt ich meine Vettern nur:", "tokens": ["Doch", "da", "ich", "schwur", ",", "da", "kannt", "ich", "mei\u00b7ne", "Vet\u00b7tern", "nur", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Da glaubt ich, alle M\u00e4nner w\u00e4ren", "tokens": ["Da", "glaubt", "ich", ",", "al\u00b7le", "M\u00e4n\u00b7ner", "w\u00e4\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PIAT", "NN", "VAFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Den alten Z\u00e4nkern gleich:", "tokens": ["Den", "al\u00b7ten", "Z\u00e4n\u00b7kern", "gleich", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Doch Mutter, wenn ihrs glaubt, o so betr\u00fcgt ihr euch!", "tokens": ["Doch", "Mut\u00b7ter", ",", "wenn", "ihrs", "glaubt", ",", "o", "so", "be\u00b7tr\u00fcgt", "ihr", "euch", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "KOUS", "PPER", "VVFIN", "$,", "FM", "ADV", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da solltet ihr den jungen Damon sehn:", "tokens": ["Da", "soll\u00b7tet", "ihr", "den", "jun\u00b7gen", "Da\u00b7mon", "sehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "O der ist frisch! o der ist sch\u00f6n!", "tokens": ["O", "der", "ist", "frisch", "!", "o", "der", "ist", "sch\u00f6n", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "VAFIN", "ADJD", "$.", "FM", "ART", "VAFIN", "ADJD", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.10": {"text": "Ich wei\u00df, ihr w\u00fcrdet ihn zu lieben nicht verschw\u00f6ren!", "tokens": ["Ich", "wei\u00df", ",", "ihr", "w\u00fcr\u00b7det", "ihn", "zu", "lie\u00b7ben", "nicht", "ver\u00b7schw\u00f6\u00b7ren", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "PPER", "PTKZU", "VVINF", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}