{"textgrid.poem.39221": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nieder, nieder, stolzes Herz!", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nieder, nieder, stolzes Herz!", "tokens": ["Nie\u00b7der", ",", "nie\u00b7der", ",", "stol\u00b7zes", "Herz", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "PTKVZ", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie du auch im Sturme flutest,", "tokens": ["Wie", "du", "auch", "im", "Stur\u00b7me", "flu\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie du ringst, und wie du blutest,", "tokens": ["Wie", "du", "ringst", ",", "und", "wie", "du", "blu\u00b7test", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "KON", "PWAV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dennoch mu\u00dft du niederw\u00e4rts!", "tokens": ["Den\u00b7noch", "mu\u00dft", "du", "nie\u00b7der\u00b7w\u00e4rts", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sahst du auch nach langen, k\u00fchnen", "tokens": ["Sahst", "du", "auch", "nach", "lan\u00b7gen", ",", "k\u00fch\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ADJA", "$,", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Z\u00fcgen durch das w\u00fcste Meer,", "tokens": ["Z\u00fc\u00b7gen", "durch", "das", "w\u00fcs\u00b7te", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Sahst du auch das Ufer gr\u00fcnen,", "tokens": ["Sahst", "du", "auch", "das", "U\u00b7fer", "gr\u00fc\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Du erreichst es doch nicht mehr!", "tokens": ["Du", "er\u00b7reichst", "es", "doch", "nicht", "mehr", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKNEG", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Nieder, nieder, stolzes Herz!", "tokens": ["Nie\u00b7der", ",", "nie\u00b7der", ",", "stol\u00b7zes", "Herz", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "PTKVZ", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hast du fr\u00fcher lindbefeuert", "tokens": ["Hast", "du", "fr\u00fc\u00b7her", "lind\u00b7be\u00b7feu\u00b7ert"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADJD", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf Morganas Kahn gesteuert,", "tokens": ["Auf", "Mor\u00b7ga\u00b7nas", "Kahn", "ge\u00b7steu\u00b7ert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Dennoch mu\u00dft du niederw\u00e4rts!", "tokens": ["Den\u00b7noch", "mu\u00dft", "du", "nie\u00b7der\u00b7w\u00e4rts", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Was dein k\u00fchnster Traum erdichtet,", "tokens": ["Was", "dein", "k\u00fchns\u00b7ter", "Traum", "er\u00b7dich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Es erscheint dir sonnenklar;", "tokens": ["Es", "er\u00b7scheint", "dir", "son\u00b7nen\u00b7klar", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Doch \u2013 das Traumbild ist vernichtet", "tokens": ["Doch", "\u2013", "das", "Traum\u00b7bild", "ist", "ver\u00b7nich\u00b7tet"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$(", "ART", "NN", "VAFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und die Wahrheit unnahbar!", "tokens": ["Und", "die", "Wahr\u00b7heit", "un\u00b7nah\u00b7bar", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Nieder, nieder, stolzes Herz!", "tokens": ["Nie\u00b7der", ",", "nie\u00b7der", ",", "stol\u00b7zes", "Herz", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "PTKVZ", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hast du sonst von Mut gelodert,", "tokens": ["Hast", "du", "sonst", "von", "Mut", "ge\u00b7lo\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Kampf geatmet, Kampf gefodert,", "tokens": ["Kampf", "ge\u00b7at\u00b7met", ",", "Kampf", "ge\u00b7fo\u00b7dert", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVPP", "$,", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dennoch mu\u00dft du niederw\u00e4rts!", "tokens": ["Den\u00b7noch", "mu\u00dft", "du", "nie\u00b7der\u00b7w\u00e4rts", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Deines Himmels ehr'ne Mauern", "tokens": ["Dei\u00b7nes", "Him\u00b7mels", "ehr'\u00b7ne", "Mau\u00b7ern"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sind zu steif f\u00fcr dein Gel\u00fcst,", "tokens": ["Sind", "zu", "steif", "f\u00fcr", "dein", "Ge\u00b7l\u00fcst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKA", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und du kannst es nur betrauern,", "tokens": ["Und", "du", "kannst", "es", "nur", "be\u00b7trau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Da\u00df du noch nicht Asche bist!", "tokens": ["Da\u00df", "du", "noch", "nicht", "A\u00b7sche", "bist", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKNEG", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Nieder, nieder, stolzes Herz!", "tokens": ["Nie\u00b7der", ",", "nie\u00b7der", ",", "stol\u00b7zes", "Herz", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "PTKVZ", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Feurig wollen, trotzig ringen,", "tokens": ["Feu\u00b7rig", "wol\u00b7len", ",", "trot\u00b7zig", "rin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "$,", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Untergehn und nie vollbringen,", "tokens": ["Un\u00b7ter\u00b7gehn", "und", "nie", "voll\u00b7brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist der alte, gro\u00dfe Schmerz! \u2013", "tokens": ["Ist", "der", "al\u00b7te", ",", "gro\u00b7\u00dfe", "Schmerz", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["VAFIN", "ART", "ADJA", "$,", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Was das Leben nie verziehen,", "tokens": ["Was", "das", "Le\u00b7ben", "nie", "ver\u00b7zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wird im Tode Wort und Klang.", "tokens": ["Wird", "im", "To\u00b7de", "Wort", "und", "Klang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Blute drum in Melodieen,", "tokens": ["Blu\u00b7te", "drum", "in", "Me\u00b7lo\u00b7di\u00b7e\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PAV", "APPR", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Jeder Tropfen ein Gesang!", "tokens": ["Je\u00b7der", "Trop\u00b7fen", "ein", "Ge\u00b7sang", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Blute, blute, stolzes Herz!", "tokens": ["Blu\u00b7te", ",", "blu\u00b7te", ",", "stol\u00b7zes", "Herz", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus der Brust geheimsten Seeen", "tokens": ["Aus", "der", "Brust", "ge\u00b7heims\u00b7ten", "Se\u00b7e\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.3": {"text": "La\u00df den Strom der Liebe wehen,", "tokens": ["La\u00df", "den", "Strom", "der", "Lie\u00b7be", "we\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "La\u00df ihn brausen, wundes Herz!", "tokens": ["La\u00df", "ihn", "brau\u00b7sen", ",", "wun\u00b7des", "Herz", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "La\u00df ihn rinnen und verrinnen; \u2013", "tokens": ["La\u00df", "ihn", "rin\u00b7nen", "und", "ver\u00b7rin\u00b7nen", ";", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVIMP", "PPER", "VVINF", "KON", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Eh' er tot im Meere ruht,", "tokens": ["Eh'", "er", "tot", "im", "Mee\u00b7re", "ruht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "F\u00e4llt vielleicht von stolzen Zinnen", "tokens": ["F\u00e4llt", "viel\u00b7leicht", "von", "stol\u00b7zen", "Zin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Eine Tr\u00e4ne in die Flut!", "tokens": ["Ei\u00b7ne", "Tr\u00e4\u00b7ne", "in", "die", "Flut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}