{"dta.poem.12273": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Liebesdienst .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1806", "urn": "urn:nbn:de:kobv:b4-20090519157", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es war ein Markgraf \u00fcber dem Rhein,               ", "tokens": ["Es", "war", "ein", "Mark\u00b7graf", "\u00fc\u00b7ber", "dem", "Rhein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "ART", "NE", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Der hatte drey sch\u00f6ne T\u00f6chterlein;", "tokens": ["Der", "hat\u00b7te", "drey", "sch\u00f6\u00b7ne", "T\u00f6ch\u00b7ter\u00b7lein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "CARD", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Zwey T\u00f6chterlein fr\u00fch heirathen weg,", "tokens": ["Zwey", "T\u00f6ch\u00b7ter\u00b7lein", "fr\u00fch", "hei\u00b7ra\u00b7then", "weg", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "ADJD", "VVINF", "PTKVZ", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Die dritt hat ihn ins Grab gelegt.", "tokens": ["Die", "dritt", "hat", "ihn", "ins", "Grab", "ge\u00b7legt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dann ging sie singen vor Schwesters Th\u00fcr:", "tokens": ["Dann", "ging", "sie", "sin\u00b7gen", "vor", "Schwes\u00b7ters", "Th\u00fcr", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "APPR", "NN", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "\u201each braucht ihr keine Dienstmagd hier?\u201c", "tokens": ["\u201e", "ach", "braucht", "ihr", "kei\u00b7ne", "Dienst\u00b7magd", "hier", "?", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ITJ", "VVFIN", "PPER", "PIAT", "NN", "ADV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "\u201eei M\u00e4dchen, du bist mir viel zu fein,", "tokens": ["\u201e", "ei", "M\u00e4d\u00b7chen", ",", "du", "bist", "mir", "viel", "zu", "fein", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "NN", "$,", "PPER", "VAFIN", "PPER", "ADV", "PTKA", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "\u201edu gehst gern mit den Herrelein.\u201c", "tokens": ["\u201e", "du", "gehst", "gern", "mit", "den", "Her\u00b7re\u00b7lein", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u201each nein! ach nein! das thu ich nicht,", "tokens": ["\u201e", "ach", "nein", "!", "ach", "nein", "!", "das", "thu", "ich", "nicht", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "PTKANT", "$.", "XY", "PTKANT", "$.", "PDS", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u201eda\u00df ich so mit den Herrlein geh!\u201c", "tokens": ["\u201e", "da\u00df", "ich", "so", "mit", "den", "Herr\u00b7lein", "geh", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KOUS", "PPER", "ADV", "APPR", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie dingt das M\u00e4gdlein ein halbes Jahr,", "tokens": ["Sie", "dingt", "das", "M\u00e4gd\u00b7lein", "ein", "hal\u00b7bes", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Das M\u00e4gdlein dient ihr sieben Jahr.", "tokens": ["Das", "M\u00e4gd\u00b7lein", "dient", "ihr", "sie\u00b7ben", "Jahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "CARD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und als die sieben Jahr um warn,", "tokens": ["Und", "als", "die", "sie\u00b7ben", "Jahr", "um", "warn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "CARD", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da wurd das M\u00e4gdlein t\u00e4glich krank;", "tokens": ["Da", "wurd", "das", "M\u00e4gd\u00b7lein", "t\u00e4g\u00b7lich", "krank", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u201esag M\u00e4gdlein, wenn du krank willst seyn,", "tokens": ["\u201e", "sag", "M\u00e4gd\u00b7lein", ",", "wenn", "du", "krank", "willst", "seyn", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "NN", "$,", "KOUS", "PPER", "ADJD", "VMFIN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u201eso sag mir, wer sind die Aeltern dein?\u201c", "tokens": ["\u201e", "so", "sag", "mir", ",", "wer", "sind", "die", "A\u00b7el\u00b7tern", "dein", "?", "\u201c"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "$,", "PWS", "VAFIN", "ART", "NN", "PPOSAT", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "\u201emein Vater war Markgraf \u00fcber dem Rhein,", "tokens": ["\u201e", "mein", "Va\u00b7ter", "war", "Mark\u00b7graf", "\u00fc\u00b7ber", "dem", "Rhein", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "VAFIN", "NE", "APPR", "ART", "NE", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "\u201eund ich bin sein j\u00fcngstes T\u00f6chterlein.\u201c", "tokens": ["\u201e", "und", "ich", "bin", "sein", "j\u00fcngs\u00b7tes", "T\u00f6ch\u00b7ter\u00b7lein", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KON", "PPER", "VAFIN", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "\u201each nein! ach nein, das glaub ich nicht,", "tokens": ["\u201e", "ach", "nein", "!", "ach", "nein", ",", "das", "glaub", "ich", "nicht", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "PTKANT", "$.", "XY", "PTKANT", "$,", "PDS", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201eda\u00df du meine j\u00fcngste Schwester bist!\u201c", "tokens": ["\u201e", "da\u00df", "du", "mei\u00b7ne", "j\u00fcngs\u00b7te", "Schwes\u00b7ter", "bist", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KOUS", "PPER", "PPOSAT", "ADJA", "NN", "VAFIN", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "\u201eund wenn du mir's nicht glauben willst,", "tokens": ["\u201e", "und", "wenn", "du", "mir's", "nicht", "glau\u00b7ben", "willst", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "KOUS", "PPER", "NE", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u201eso geh nur an meine Kiste hin,", "tokens": ["\u201e", "so", "geh", "nur", "an", "mei\u00b7ne", "Kis\u00b7te", "hin", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "\u201edaran wird es geschrieben stehn.\u201c", "tokens": ["\u201e", "da\u00b7ran", "wird", "es", "ge\u00b7schrie\u00b7ben", "stehn", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PAV", "VAFIN", "PPER", "VVPP", "VVINF", "$.", "$("], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.6": {"text": "Und als sie an die Kiste kam,", "tokens": ["Und", "als", "sie", "an", "die", "Kis\u00b7te", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Da rannen ihr die Backen ab:", "tokens": ["Da", "ran\u00b7nen", "ihr", "die", "Ba\u00b7cken", "ab", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201each bringt mir Weck, ach bringt mir Wein,", "tokens": ["\u201e", "ach", "bringt", "mir", "Weck", ",", "ach", "bringt", "mir", "Wein", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "VVFIN", "PPER", "NN", "$,", "ADV", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u201edas ist mein j\u00fcngstes Schwesterlein!\u201c", "tokens": ["\u201e", "das", "ist", "mein", "j\u00fcngs\u00b7tes", "Schwes\u00b7ter\u00b7lein", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PDS", "VAFIN", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u201eich will auch kein Weck, ich will auch kein Wein,", "tokens": ["\u201e", "ich", "will", "auch", "kein", "Weck", ",", "ich", "will", "auch", "kein", "Wein", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "ADV", "PIAT", "NN", "$,", "PPER", "VMFIN", "ADV", "PIAT", "NN", "$,"], "meter": "-+-++-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "\u201ewill nur ein kleines L\u00e4delein,", "tokens": ["\u201e", "will", "nur", "ein", "klei\u00b7nes", "L\u00e4\u00b7de\u00b7lein", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VMFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "\u201edarin ich will begraben seyn.\u201c", "tokens": ["\u201e", "da\u00b7rin", "ich", "will", "be\u00b7gra\u00b7ben", "seyn", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PAV", "PPER", "VMFIN", "VVPP", "VAINF", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}