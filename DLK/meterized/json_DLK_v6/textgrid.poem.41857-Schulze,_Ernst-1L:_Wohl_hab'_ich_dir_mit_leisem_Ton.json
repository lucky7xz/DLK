{"textgrid.poem.41857": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wohl hab' ich dir mit leisem Ton", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohl hab' ich dir mit leisem Ton", "tokens": ["Wohl", "hab'", "ich", "dir", "mit", "lei\u00b7sem", "Ton"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Manch zartempfund'nes Lied gesungen,", "tokens": ["Manch", "zar\u00b7temp\u00b7fun\u00b7d'\u00b7nes", "Lied", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Doch nie des Liedes s\u00fc\u00dfen Lohn,", "tokens": ["Doch", "nie", "des", "Lie\u00b7des", "s\u00fc\u00b7\u00dfen", "Lohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Minne L\u00e4cheln, mir errungen;", "tokens": ["Der", "Min\u00b7ne", "L\u00e4\u00b7cheln", ",", "mir", "er\u00b7run\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Drum seufz' ich oft mit stillem Schmerz:", "tokens": ["Drum", "seuf\u00b7z'", "ich", "oft", "mit", "stil\u00b7lem", "Schmerz", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Verstummt, verstummt ihr gold'nen Saiten!", "tokens": ["Ver\u00b7stummt", ",", "ver\u00b7stummt", "ihr", "gold'\u00b7nen", "Sai\u00b7ten", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Denn ach, der Liebsten kaltes Herz", "tokens": ["Denn", "ach", ",", "der", "Liebs\u00b7ten", "kal\u00b7tes", "Herz"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "XY", "$,", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Kann eure Kl\u00e4nge doch nicht deuten!", "tokens": ["Kann", "eu\u00b7re", "Kl\u00e4n\u00b7ge", "doch", "nicht", "deu\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch nah' ich dir, du holdes Bild,", "tokens": ["Doch", "nah'", "ich", "dir", ",", "du", "hol\u00b7des", "Bild", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und sitze still zu deinen F\u00fc\u00dfen", "tokens": ["Und", "sit\u00b7ze", "still", "zu", "dei\u00b7nen", "F\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und sehe, wie so wundermild", "tokens": ["Und", "se\u00b7he", ",", "wie", "so", "wun\u00b7der\u00b7mild"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "PWAV", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mich deine klaren Blicke gr\u00fc\u00dfen,", "tokens": ["Mich", "dei\u00b7ne", "kla\u00b7ren", "Bli\u00b7cke", "gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und wie der Unschuld keuscher Kranz", "tokens": ["Und", "wie", "der", "Un\u00b7schuld", "keu\u00b7scher", "Kranz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und wie die Bl\u00fcthen alles Sch\u00f6nen", "tokens": ["Und", "wie", "die", "Bl\u00fc\u00b7then", "al\u00b7les", "Sch\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Dein Angesicht mit reinem Glanz,", "tokens": ["Dein", "An\u00b7ge\u00b7sicht", "mit", "rei\u00b7nem", "Glanz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mit heil'gem Schmuck dein Leben kr\u00f6nen;", "tokens": ["Mit", "heil'\u00b7gem", "Schmuck", "dein", "Le\u00b7ben", "kr\u00f6\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Dann schwillt mein Herz von s\u00fc\u00dfer Lust", "tokens": ["Dann", "schwillt", "mein", "Herz", "von", "s\u00fc\u00b7\u00dfer", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und kann's nicht bergen, nicht enthalten,", "tokens": ["Und", "kann's", "nicht", "ber\u00b7gen", ",", "nicht", "ent\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PTKNEG", "VVINF", "$,", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und bunt beginnt in tiefer Brust", "tokens": ["Und", "bunt", "be\u00b7ginnt", "in", "tie\u00b7fer", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Bilder holdes Reich zu walten,", "tokens": ["Der", "Bil\u00b7der", "hol\u00b7des", "Reich", "zu", "wal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und was dein Mund, dein Auge spricht,", "tokens": ["Und", "was", "dein", "Mund", ",", "dein", "Au\u00b7ge", "spricht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "T\u00f6nt lieblich mir im Herzen wieder,", "tokens": ["T\u00f6nt", "lieb\u00b7lich", "mir", "im", "Her\u00b7zen", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "APPRART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und deiner Strenge denk' ich nicht,", "tokens": ["Und", "dei\u00b7ner", "Stren\u00b7ge", "denk'", "ich", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und denke nur auf zarte Lieder.", "tokens": ["Und", "den\u00b7ke", "nur", "auf", "zar\u00b7te", "Lie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Dein Aug' ist meine Fantasie,", "tokens": ["Dein", "Aug'", "ist", "mei\u00b7ne", "Fan\u00b7ta\u00b7sie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dein Athem giebt mir Mild' und Feuer,", "tokens": ["Dein", "A\u00b7them", "giebt", "mir", "Mild'", "und", "Feu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dein Wort mir Klang und Harmonie,", "tokens": ["Dein", "Wort", "mir", "Klang", "und", "Har\u00b7mo\u00b7nie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dein Wangenroth der Anmuth Schleier.", "tokens": ["Dein", "Wan\u00b7gen\u00b7roth", "der", "An\u00b7muth", "Schlei\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "O wollte nur der Genius", "tokens": ["O", "woll\u00b7te", "nur", "der", "Ge\u00b7nius"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VMFIN", "ADV", "ART", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Der Liebe meinem Leben lachen,", "tokens": ["Der", "Lie\u00b7be", "mei\u00b7nem", "Le\u00b7ben", "la\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Dann k\u00f6nnte leicht dein s\u00fc\u00dfer Ku\u00df", "tokens": ["Dann", "k\u00f6nn\u00b7te", "leicht", "dein", "s\u00fc\u00b7\u00dfer", "Ku\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ADJD", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Den S\u00e4nger noch unsterblich machen!", "tokens": ["Den", "S\u00e4n\u00b7ger", "noch", "uns\u00b7terb\u00b7lich", "ma\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}