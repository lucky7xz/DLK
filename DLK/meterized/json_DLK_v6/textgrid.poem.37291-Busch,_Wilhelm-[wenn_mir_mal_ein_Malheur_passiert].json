{"textgrid.poem.37291": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "[wenn mir mal ein Malheur passiert]", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn mir mal ein Malheur passiert,", "tokens": ["Wenn", "mir", "mal", "ein", "Mal\u00b7heur", "pas\u00b7siert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich wei\u00df, so bist du sehr ger\u00fchrt.", "tokens": ["Ich", "wei\u00df", ",", "so", "bist", "du", "sehr", "ge\u00b7r\u00fchrt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADV", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du denkst, es w\u00e4re doch fatal,", "tokens": ["Du", "denkst", ",", "es", "w\u00e4\u00b7re", "doch", "fa\u00b7tal", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Passierte dir das auch einmal.", "tokens": ["Pas\u00b7sier\u00b7te", "dir", "das", "auch", "ein\u00b7mal", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PDS", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch weil das b\u00f6se Schmerzensding", "tokens": ["Doch", "weil", "das", "b\u00f6\u00b7se", "Schmer\u00b7zens\u00b7ding"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zum Gl\u00fcck an dir vor\u00fcber ging,", "tokens": ["Zum", "Gl\u00fcck", "an", "dir", "vor\u00b7\u00fc\u00b7ber", "ging", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So ist die Sache anderseits", "tokens": ["So", "ist", "die", "Sa\u00b7che", "an\u00b7der\u00b7seits"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "F\u00fcr dich nicht ohne allen Reiz.", "tokens": ["F\u00fcr", "dich", "nicht", "oh\u00b7ne", "al\u00b7len", "Reiz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKNEG", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Du merkst, da\u00df die Bedaurerei", "tokens": ["Du", "merkst", ",", "da\u00df", "die", "Be\u00b7dau\u00b7re\u00b7rei"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "So eine Art von Wonne sei.", "tokens": ["So", "ei\u00b7ne", "Art", "von", "Won\u00b7ne", "sei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}