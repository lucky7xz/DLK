], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mach meine Lieben fromm und gut,", "tokens": ["Mach", "mei\u00b7ne", "Lie\u00b7ben", "fromm", "und", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df ich sie wiedersehe.", "tokens": ["Da\u00df", "ich", "sie", "wie\u00b7der\u00b7se\u00b7he", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Auch mich erhalte dir getreu;", "tokens": ["Auch", "mich", "er\u00b7hal\u00b7te", "dir", "ge\u00b7treu", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dann, Vater, bin ich auch dabei,", "tokens": ["Dann", ",", "Va\u00b7ter", ",", "bin", "ich", "auch", "da\u00b7bei", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "$,", "VAFIN", "PPER", "ADV", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wenn sich die Lieben finden.", "tokens": ["Wenn", "sich", "die", "Lie\u00b7ben", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Weltrichter, welch ein Tag f\u00fcr dich,", "tokens": ["Welt\u00b7rich\u00b7ter", ",", "welch", "ein", "Tag", "f\u00fcr", "dich", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAT", "ART", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Festtag voll Erbarmen,", "tokens": ["Ein", "Fest\u00b7tag", "voll", "Er\u00b7bar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wenn so viel tausend Seelen sich", "tokens": ["Wenn", "so", "viel", "tau\u00b7send", "See\u00b7len", "sich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ADV", "CARD", "NN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vor deinem Thron umarmen;", "tokens": ["Vor", "dei\u00b7nem", "Thron", "um\u00b7ar\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dann bluten Herzen nimmermehr", "tokens": ["Dann", "blu\u00b7ten", "Her\u00b7zen", "nim\u00b7mer\u00b7mehr"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und dann versiegt die Abschiedsz\u00e4hr';", "tokens": ["Und", "dann", "ver\u00b7siegt", "die", "Ab\u00b7schieds\u00b7z\u00e4hr'", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Du trocknest ja die Thr\u00e4nen.", "tokens": ["Du", "trock\u00b7nest", "ja", "die", "Thr\u00e4\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Drum weint nicht, Freunde! denn gewi\u00df", "tokens": ["Drum", "weint", "nicht", ",", "Freun\u00b7de", "!", "denn", "ge\u00b7wi\u00df"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PAV", "VVFIN", "PTKNEG", "$,", "NN", "$.", "KON", "ADV"], "meter": 