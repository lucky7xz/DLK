{"textgrid.poem.66315": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Trennung Last liegt schwer auf mir;", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Trennung Last liegt schwer auf mir;", "tokens": ["Der", "Tren\u00b7nung", "Last", "liegt", "schwer", "auf", "mir", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VVFIN", "ADJD", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich liebe sie, die Meinen.", "tokens": ["Ich", "lie\u00b7be", "sie", ",", "die", "Mei\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "La\u00df, meines Herzens Gott! vor dir", "tokens": ["La\u00df", ",", "mei\u00b7nes", "Her\u00b7zens", "Gott", "!", "vor", "dir"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["VVIMP", "$,", "PPOSAT", "NN", "NN", "$.", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mich diese Frage weinen:", "tokens": ["Mich", "die\u00b7se", "Fra\u00b7ge", "wei\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Werd' ich nach meinem Tode gleich,", "tokens": ["Werd'", "ich", "nach", "mei\u00b7nem", "To\u00b7de", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Werd' ich in Christus K\u00f6nigreich", "tokens": ["Werd'", "ich", "in", "Chris\u00b7tus", "K\u00f6\u00b7nig\u00b7reich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "NE", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die Meinen wiedersehen?", "tokens": ["Die", "Mei\u00b7nen", "wie\u00b7der\u00b7se\u00b7hen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Vielleicht! O Gott, ich h\u00f6re schon", "tokens": ["Viel\u00b7leicht", "!", "O", "Gott", ",", "ich", "h\u00f6\u00b7re", "schon"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$.", "NE", "NN", "$,", "PPER", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Geistes Stimme wehen;", "tokens": ["Des", "Geis\u00b7tes", "Stim\u00b7me", "we\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie lispelt mir, wie Harfenton:", "tokens": ["Sie", "lis\u00b7pelt", "mir", ",", "wie", "Har\u00b7fen\u00b7ton", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PWAV", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du wirst sie wiedersehen,", "tokens": ["Du", "wirst", "sie", "wie\u00b7der\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Bist du nur fromm, sind sie nur gut,", "tokens": ["Bist", "du", "nur", "fromm", ",", "sind", "sie", "nur", "gut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "$,", "VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sind sie aus Wasser, Geist und Blut", "tokens": ["Sind", "sie", "aus", "Was\u00b7ser", ",", "Geist", "und", "Blut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Gezeugt zum ew'gen Leben.", "tokens": ["Ge\u00b7zeugt", "zum", "ew'\u00b7gen", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die fromme Seele trennt sich nicht", "tokens": ["Die", "from\u00b7me", "See\u00b7le", "trennt", "sich", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von gleichgestimmten Frommen,", "tokens": ["Von", "gleich\u00b7ges\u00b7timm\u00b7ten", "From\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie werden all' in Gottes Licht", "tokens": ["Sie", "wer\u00b7den", "all'", "in", "Got\u00b7tes", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PIS", "APPR", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aufs neu' zusammenkommen,", "tokens": ["Aufs", "neu'", "zu\u00b7sam\u00b7men\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Von keinem Tode mehr getrennt;", "tokens": ["Von", "kei\u00b7nem", "To\u00b7de", "mehr", "ge\u00b7trennt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Denn Gottes Engel, der sie kennt,", "tokens": ["Denn", "Got\u00b7tes", "En\u00b7gel", ",", "der", "sie", "kennt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "NE", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Versammelt die Erw\u00e4hlten.", "tokens": ["Ver\u00b7sam\u00b7melt", "die", "Er\u00b7w\u00e4hl\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+---+-", "measure": "dactylic.init"}}, "stanza.4": {"line.1": {"text": "Die Einfalt sieht den Biedermann,", "tokens": ["Die", "Ein\u00b7falt", "sieht", "den", "Bie\u00b7der\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Christ den Christen wieder,", "tokens": ["Der", "Christ", "den", "Chris\u00b7ten", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das Weib trifft ihren Gatten an,", "tokens": ["Das", "Weib", "trifft", "ih\u00b7ren", "Gat\u00b7ten", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Bruder seine Br\u00fcder.", "tokens": ["Der", "Bru\u00b7der", "sei\u00b7ne", "Br\u00fc\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die fromme Mutter k\u00fc\u00dft ihr Kind;", "tokens": ["Die", "from\u00b7me", "Mut\u00b7ter", "k\u00fc\u00dft", "ihr", "Kind", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Denn alle gute Seelen sind", "tokens": ["Denn", "al\u00b7le", "gu\u00b7te", "See\u00b7len", "sind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "ADJA", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "In Gottes Stadt beisammen.", "tokens": ["In", "Got\u00b7tes", "Stadt", "bei\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Elias der Thisbite kam", "tokens": ["E\u00b7lias", "der", "This\u00b7bi\u00b7te", "kam"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ART", "NN", "VVFIN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Im Wettertod zu Mose;", "tokens": ["Im", "Wet\u00b7ter\u00b7tod", "zu", "Mo\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein Lazarus zu Abraham,", "tokens": ["Ein", "La\u00b7za\u00b7rus", "zu", "Ab\u00b7ra\u00b7ham", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu ruhn in seinem Scho\u00dfe.", "tokens": ["Zu", "ruhn", "in", "sei\u00b7nem", "Scho\u00b7\u00dfe", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der Sch\u00e4cher h\u00f6rt das gro\u00dfe Wort:", "tokens": ["Der", "Sch\u00e4\u00b7cher", "h\u00f6rt", "das", "gro\u00b7\u00dfe", "Wort", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Im Paradiese wirst du dort", "tokens": ["Im", "Pa\u00b7ra\u00b7die\u00b7se", "wirst", "du", "dort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Noch heute mit mir leben.", "tokens": ["Noch", "heu\u00b7te", "mit", "mir", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und dennoch sollten Christen sich", "tokens": ["Und", "den\u00b7noch", "soll\u00b7ten", "Chris\u00b7ten", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PIAT", "NN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In ihrem Tode gr\u00e4men?", "tokens": ["In", "ih\u00b7rem", "To\u00b7de", "gr\u00e4\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sprach nicht der Herr so br\u00fcderlich:", "tokens": ["Sprach", "nicht", "der", "Herr", "so", "br\u00fc\u00b7der\u00b7lich", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich will euch zu mir nehmen,", "tokens": ["Ich", "will", "euch", "zu", "mir", "neh\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Damit ihr alle bei mir seid,", "tokens": ["Da\u00b7mit", "ihr", "al\u00b7le", "bei", "mir", "seid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "APPR", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zu schauen meine Herrlichkeit,", "tokens": ["Zu", "schau\u00b7en", "mei\u00b7ne", "Herr\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und ewig mit mir herrschet. \u2013?", "tokens": ["Und", "e\u00b7wig", "mit", "mir", "herr\u00b7schet", ".", "\u2013", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "ADJD", "APPR", "PPER", "VVFIN", "$.", "$(", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Zum Berge Zion kommen wir!", "tokens": ["Zum", "Ber\u00b7ge", "Zi\u00b7on", "kom\u00b7men", "wir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NE", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu Gottes Stadt im Himmel!", "tokens": ["Zu", "Got\u00b7tes", "Stadt", "im", "Him\u00b7mel", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wo Engel stehen, Herr, vor dir", "tokens": ["Wo", "En\u00b7gel", "ste\u00b7hen", ",", "Herr", ",", "vor", "dir"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PWAV", "NE", "VVFIN", "$,", "NN", "$,", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im jauchzenden Gewimmel!", "tokens": ["Im", "jauch\u00b7zen\u00b7den", "Ge\u00b7wim\u00b7mel", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Zu der Gemeinde Strahlenchor!", "tokens": ["Zu", "der", "Ge\u00b7mein\u00b7de", "Strah\u00b7len\u00b7chor", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zu Geistern steigen wir empor", "tokens": ["Zu", "Geis\u00b7tern", "stei\u00b7gen", "wir", "em\u00b7por"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Vollendeter Gerechten!", "tokens": ["Voll\u00b7en\u00b7de\u00b7ter", "Ge\u00b7rech\u00b7ten", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.8": {"line.1": {"text": "Und ach, zu Gott! zu Gott! zu Gott!", "tokens": ["Und", "ach", ",", "zu", "Gott", "!", "zu", "Gott", "!", "zu", "Gott", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "XY", "$,", "APPR", "NN", "$.", "APPR", "NN", "$.", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem Herrscher \u00fcber alle!", "tokens": ["Dem", "Herr\u00b7scher", "\u00fc\u00b7ber", "al\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PIS", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "O komm nur, Tod, komm, s\u00fc\u00dfer Tod!", "tokens": ["O", "komm", "nur", ",", "Tod", ",", "komm", ",", "s\u00fc\u00b7\u00dfer", "Tod", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "$,", "NN", "$,", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Damit ich niederfalle", "tokens": ["Da\u00b7mit", "ich", "nie\u00b7der\u00b7fal\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "PIS"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Am Thron und meinen Vater seh',", "tokens": ["Am", "Thron", "und", "mei\u00b7nen", "Va\u00b7ter", "seh'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und meinen Mittler Jesum seh',", "tokens": ["Und", "mei\u00b7nen", "Mitt\u00b7ler", "Je\u00b7sum", "seh'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und mich sein Blut besprenge.", "tokens": ["Und", "mich", "sein", "Blut", "be\u00b7spren\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Drum bitt' ich euch mit hoher Hand,", "tokens": ["Drum", "bitt'", "ich", "euch", "mit", "ho\u00b7her", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Seid fromm und gut, ihr Meinen!", "tokens": ["Seid", "fromm", "und", "gut", ",", "ihr", "Mei\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "KON", "ADJD", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dann wird uns Gott im Vaterland", "tokens": ["Dann", "wird", "uns", "Gott", "im", "Va\u00b7ter\u00b7land"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf ewiglich vereinen.", "tokens": ["Auf", "e\u00b7wig\u00b7lich", "ver\u00b7ei\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wie wird's uns sein, wenn wir uns", "tokens": ["Wie", "wird's", "uns", "sein", ",", "wenn", "wir", "uns"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "VAINF", "$,", "KOUS", "PPER", "PRF"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Wenn wir uns froh entgegen gehn", "tokens": ["Wenn", "wir", "uns", "froh", "ent\u00b7ge\u00b7gen", "gehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "PTKVZ", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und uns am Halse weinen.", "tokens": ["Und", "uns", "am", "Hal\u00b7se", "wei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Da wollen wir vor Gottes Thron", "tokens": ["Da", "wol\u00b7len", "wir", "vor", "Got\u00b7tes", "Thron"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Einander alles sagen,", "tokens": ["Ein\u00b7an\u00b7der", "al\u00b7les", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Was wir auf dieser Welt f\u00fcr Hohn", "tokens": ["Was", "wir", "auf", "die\u00b7ser", "Welt", "f\u00fcr", "Hohn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "APPR", "PDAT", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Ungemach ertragen;", "tokens": ["Und", "Un\u00b7ge\u00b7mach", "er\u00b7tra\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Uns freuen, da\u00df des Lebens Zwist", "tokens": ["Uns", "freu\u00b7en", ",", "da\u00df", "des", "Le\u00b7bens", "Zwist"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVINF", "$,", "KOUS", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und F\u00e4hrlichkeit vor\u00fcber ist,", "tokens": ["Und", "F\u00e4hr\u00b7lich\u00b7keit", "vor\u00b7\u00fc\u00b7ber", "ist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und Gott mit Thr\u00e4nen danken.", "tokens": ["Und", "Gott", "mit", "Thr\u00e4\u00b7nen", "dan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Doch Frommen nur wird's Freude sein,", "tokens": ["Doch", "From\u00b7men", "nur", "wird's", "Freu\u00b7de", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "VAFIN", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn sie sich wiedersehen.", "tokens": ["Wenn", "sie", "sich", "wie\u00b7der\u00b7se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gottlose h\u00e4ufen nur die Pein", "tokens": ["Gott\u