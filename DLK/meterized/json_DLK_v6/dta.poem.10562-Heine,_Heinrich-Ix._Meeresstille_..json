{"dta.poem.10562": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Ix.  \n  Meeresstille .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Meeresstille! Ihre Strahlen", "tokens": ["Mee\u00b7res\u00b7stil\u00b7le", "!", "Ih\u00b7re", "Strah\u00b7len"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$.", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wirft die Sonne auf das Wasser,", "tokens": ["Wirft", "die", "Son\u00b7ne", "auf", "das", "Was\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und im wogenden Geschmeide", "tokens": ["Und", "im", "wo\u00b7gen\u00b7den", "Ge\u00b7schmei\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Zieht das Schiff die gr\u00fcnen Furchen.", "tokens": ["Zieht", "das", "Schiff", "die", "gr\u00fc\u00b7nen", "Fur\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Bei dem Steuer liegt der Bootsmann", "tokens": ["Bei", "dem", "Steu\u00b7er", "liegt", "der", "Boots\u00b7mann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf dem Bauch, und schnarchet leise.", "tokens": ["Auf", "dem", "Bauch", ",", "und", "schnar\u00b7chet", "lei\u00b7se", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "KON", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bei dem Mastbaum, seegelflickend,", "tokens": ["Bei", "dem", "Mast\u00b7baum", ",", "see\u00b7gel\u00b7fli\u00b7ckend", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kauert der betheerte Schiffsjung.", "tokens": ["Kau\u00b7ert", "der", "be\u00b7theer\u00b7te", "Schiffs\u00b7jung", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Hinter'm Schmutze seiner Wangen", "tokens": ["Hin\u00b7ter'm", "Schmut\u00b7ze", "sei\u00b7ner", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Spr\u00fcht es roth, wehm\u00fcthig zuckt es", "tokens": ["Spr\u00fcht", "es", "roth", ",", "weh\u00b7m\u00fct\u00b7hig", "zuckt", "es"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "$,", "ADJD", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Um das breite Maul, und schmerzlich", "tokens": ["Um", "das", "brei\u00b7te", "Maul", ",", "und", "schmerz\u00b7lich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUI", "ART", "ADJA", "NN", "$,", "KON", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schau'n die gro\u00dfen, sch\u00f6nen Augen.", "tokens": ["Schau'n", "die", "gro\u00b7\u00dfen", ",", "sch\u00f6\u00b7nen", "Au\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Denn der Capit\u00e4n steht vor ihm,", "tokens": ["Denn", "der", "Ca\u00b7pi\u00b7t\u00e4n", "steht", "vor", "ihm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "PPER", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Tobt und flucht und schilt ihn: Spitzbub.", "tokens": ["Tobt", "und", "flucht", "und", "schilt", "ihn", ":", "Spitz\u00b7bub", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "KON", "VVFIN", "KON", "VVFIN", "PPER", "$.", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "\u201espitzbub! einen Hering hast du", "tokens": ["\u201e", "spitz\u00b7bub", "!", "ei\u00b7nen", "He\u00b7ring", "hast", "du"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "VVFIN", "$.", "ART", "NN", "VAFIN", "PPER"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Aus der Tonne mir gestohlen!\u201c", "tokens": ["Aus", "der", "Ton\u00b7ne", "mir", "ge\u00b7stoh\u00b7len", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "VVPP", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Meeresstille! Aus den Wellen", "tokens": ["Mee\u00b7res\u00b7stil\u00b7le", "!", "Aus", "den", "Wel\u00b7len"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Taucht hervor ein kluges Fischlein,", "tokens": ["Taucht", "her\u00b7vor", "ein", "klu\u00b7ges", "Fisc\u00b7hlein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "W\u00e4rmt das K\u00f6pfchen in der Sonne,", "tokens": ["W\u00e4rmt", "das", "K\u00f6pf\u00b7chen", "in", "der", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Pl\u00e4tschert lustig mit dem Schw\u00e4nzchen.", "tokens": ["Pl\u00e4t\u00b7schert", "lus\u00b7tig", "mit", "dem", "Schw\u00e4nz\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Doch die M\u00f6ve, aus den L\u00fcften,", "tokens": ["Doch", "die", "M\u00f6\u00b7ve", ",", "aus", "den", "L\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schie\u00dft herunter auf das Fischlein,", "tokens": ["Schie\u00dft", "her\u00b7un\u00b7ter", "auf", "das", "Fisc\u00b7hlein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und den raschen Raub im Schnabel", "tokens": ["Und", "den", "ra\u00b7schen", "Raub", "im", "Schna\u00b7bel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "APPRART", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Schwingt sie sich hinauf in's Blaue.", "tokens": ["Schwingt", "sie", "sich", "hin\u00b7auf", "in's", "Blau\u00b7e", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}