{"dta.poem.901": {"metadata": {"author": {"name": "Trakl, Georg", "birth": "N.A.", "death": "N.A."}, "title": "WiNKEL AM WALD", "genre": "Lyrik", "period": "N.A.", "pub_year": "1913", "urn": "urn:nbn:de:kobv:b4-30357-9", "language": ["de:0.99"], "booktitle": "Trakl, Georg: Gedichte. Leipzig, 1913."}, "poem": {"stanza.1": {"line.1": {"text": "Braune Kastanien. Leise gleiten die alten Leute", "tokens": ["Brau\u00b7ne", "Kas\u00b7ta\u00b7ni\u00b7en", ".", "Lei\u00b7se", "glei\u00b7ten", "die", "al\u00b7ten", "Leu\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "$.", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+--+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "In stilleren Abend; weich verwelken sch\u00f6ne Bl\u00e4tter.", "tokens": ["In", "stil\u00b7le\u00b7ren", "A\u00b7bend", ";", "weich", "ver\u00b7wel\u00b7ken", "sch\u00f6\u00b7ne", "Bl\u00e4t\u00b7ter", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$.", "ADJD", "VVINF", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Am Friedhof scherzt die Amsel mit dem toten Vetter,", "tokens": ["Am", "Fried\u00b7hof", "scherzt", "die", "Am\u00b7sel", "mit", "dem", "to\u00b7ten", "Vet\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Angelen gibt der blonde Lehrer das Geleite.", "tokens": ["An\u00b7ge\u00b7len", "gibt", "der", "blon\u00b7de", "Leh\u00b7rer", "das", "Ge\u00b7lei\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}, "stanza.2": {"line.1": {"text": "Des Todes reine Bilder schaun von Kirchenfenstern;", "tokens": ["Des", "To\u00b7des", "rei\u00b7ne", "Bil\u00b7der", "schaun", "von", "Kir\u00b7chen\u00b7fens\u00b7tern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch wirkt ein blutiger Grund sehr trauervoll und d\u00fcster.", "tokens": ["Doch", "wirkt", "ein", "blu\u00b7ti\u00b7ger", "Grund", "sehr", "trau\u00b7er\u00b7voll", "und", "d\u00fcs\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Das Tor blieb heut verschlossen. Den Schl\u00fcssel hat der K\u00fcster.", "tokens": ["Das", "Tor", "blieb", "heut", "ver\u00b7schlos\u00b7sen", ".", "Den", "Schl\u00fcs\u00b7sel", "hat", "der", "K\u00fcs\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "VVPP", "$.", "ART", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Im Garten spricht die Schwester freundlich mit Gespenstern.", "tokens": ["Im", "Gar\u00b7ten", "spricht", "die", "Schwes\u00b7ter", "freund\u00b7lich", "mit", "Ge\u00b7spens\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "In alten Kellern reift der Wein ins Goldne, Klare.", "tokens": ["In", "al\u00b7ten", "Kel\u00b7lern", "reift", "der", "Wein", "ins", "Gold\u00b7ne", ",", "Kla\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "APPRART", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "S\u00fc\u00df duften \u00c4pfel. Freude gl\u00e4nzt nicht allzu ferne.", "tokens": ["S\u00fc\u00df", "duf\u00b7ten", "\u00c4p\u00b7fel", ".", "Freu\u00b7de", "gl\u00e4nzt", "nicht", "all\u00b7zu", "fer\u00b7ne", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$.", "NN", "VVFIN", "PTKNEG", "PTKA", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Den langen Abend h\u00f6ren Kinder M\u00e4rchen gerne;", "tokens": ["Den", "lan\u00b7gen", "A\u00b7bend", "h\u00f6\u00b7ren", "Kin\u00b7der", "M\u00e4r\u00b7chen", "ger\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "NN", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Auch zeigt sich sanftem Wahnsinn oft das Goldne, Wahre.", "tokens": ["Auch", "zeigt", "sich", "sanf\u00b7tem", "Wahn\u00b7sinn", "oft", "das", "Gold\u00b7ne", ",", "Wah\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ADJA", "NN", "ADV", "ART", "NN", "$,", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Das Blau flie\u00dft voll Reseden; in Zimmern Kerzenhelle.", "tokens": ["Das", "Blau", "flie\u00dft", "voll", "Re\u00b7se\u00b7den", ";", "in", "Zim\u00b7mern", "Ker\u00b7zen\u00b7hel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "NN", "$.", "APPR", "NN", "NN", "$."], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Bescheidenen ist ihre St\u00e4tte wohl bereitet.", "tokens": ["Be\u00b7schei\u00b7de\u00b7nen", "ist", "ih\u00b7re", "St\u00e4t\u00b7te", "wohl", "be\u00b7rei\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Den Saum des Walds hinab ein einsam Schicksal gleitet;", "tokens": ["Den", "Saum", "des", "Walds", "hin\u00b7ab", "ein", "ein\u00b7sam", "Schick\u00b7sal", "glei\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADV", "ART", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Nacht erscheint, der Ruhe Engel, auf der Schwelle.", "tokens": ["Die", "Nacht", "er\u00b7scheint", ",", "der", "Ru\u00b7he", "En\u00b7gel", ",", "auf", "der", "Schwel\u00b7le", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "NE", "$,", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}