{"textgrid.poem.50775": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ja, das ist der alte Kirchhof,", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ja, das ist der alte Kirchhof,", "tokens": ["Ja", ",", "das", "ist", "der", "al\u00b7te", "Kirch\u00b7hof", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der in blauer Flut sich spiegelt,", "tokens": ["Der", "in", "blau\u00b7er", "Flut", "sich", "spie\u00b7gelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und in seiner dunkeln Erde", "tokens": ["Und", "in", "sei\u00b7ner", "dun\u00b7keln", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Liegt mein Heiligstes versiegelt;", "tokens": ["Liegt", "mein", "Hei\u00b7ligs\u00b7tes", "ver\u00b7sie\u00b7gelt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Hier das Beet voll roter Rosen,", "tokens": ["Hier", "das", "Beet", "voll", "ro\u00b7ter", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dicht und \u00fcppig aufgesprossen:", "tokens": ["Dicht", "und", "\u00fcp\u00b7pig", "auf\u00b7ge\u00b7spros\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Drunter liegt die wei\u00dfe Lilie,", "tokens": ["Drun\u00b7ter", "liegt", "die", "wei\u00b7\u00dfe", "Li\u00b7lie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Eng im Blumenschrein verschlossen!", "tokens": ["Eng", "im", "Blu\u00b7men\u00b7schrein", "ver\u00b7schlos\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Durch die Rosen, durch die Erde,", "tokens": ["Durch", "die", "Ro\u00b7sen", ",", "durch", "die", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch die Bretter dringt mein Sehnen;", "tokens": ["Durch", "die", "Bret\u00b7ter", "dringt", "mein", "Seh\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dort, wie eben erst gestorben,", "tokens": ["Dort", ",", "wie", "e\u00b7ben", "erst", "ge\u00b7stor\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ADV", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Will mein Herz sie schlummernd w\u00e4hnen!", "tokens": ["Will", "mein", "Herz", "sie", "schlum\u00b7mernd", "w\u00e4h\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "PPER", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Schl\u00e4fst du, schl\u00e4fst du noch, mein Liebchen?", "tokens": ["Schl\u00e4fst", "du", ",", "schl\u00e4fst", "du", "noch", ",", "mein", "Lieb\u00b7chen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VVFIN", "PPER", "ADV", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Zuckt kein Strahl durch deine Leiche,", "tokens": ["Zuckt", "kein", "Strahl", "durch", "dei\u00b7ne", "Lei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Weil auf deinem stillen Grabe", "tokens": ["Weil", "auf", "dei\u00b7nem", "stil\u00b7len", "Gra\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Nun dein Buhle irrt, der bleiche?", "tokens": ["Nun", "dein", "Buh\u00b7le", "irrt", ",", "der", "blei\u00b7che", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "$,", "ART", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "F\u00e4hrt kein Stern in deine Augen?", "tokens": ["F\u00e4hrt", "kein", "Stern", "in", "dei\u00b7ne", "Au\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hebt dein Herz nicht an zu schlagen?", "tokens": ["Hebt", "dein", "Herz", "nicht", "an", "zu", "schla\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKNEG", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Quellen nicht von deinen Lippen", "tokens": ["Quel\u00b7len", "nicht", "von", "dei\u00b7nen", "Lip\u00b7pen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Frische, s\u00fc\u00dfe Liebesklagen?", "tokens": ["Fri\u00b7sche", ",", "s\u00fc\u00b7\u00dfe", "Lie\u00b7bes\u00b7kla\u00b7gen", "?"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Zieht kein roter Morgenschimmer", "tokens": ["Zieht", "kein", "ro\u00b7ter", "Mor\u00b7gen\u00b7schim\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "\u00dcber deine wei\u00dfen Wangen,", "tokens": ["\u00dc\u00b7ber", "dei\u00b7ne", "wei\u00b7\u00dfen", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Weil daran die Lebensgluten", "tokens": ["Weil", "da\u00b7ran", "die", "Le\u00b7bens\u00b7glu\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PAV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Meiner hei\u00dfen Blicke hangen?", "tokens": ["Mei\u00b7ner", "hei\u00b7\u00dfen", "Bli\u00b7cke", "han\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Eitler Traum! um eine Leiche,", "tokens": ["Eit\u00b7ler", "Traum", "!", "um", "ei\u00b7ne", "Lei\u00b7che", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Um den Tod hab ich geworben!", "tokens": ["Um", "den", "Tod", "hab", "ich", "ge\u00b7wor\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nun, so sei auch meine Liebe", "tokens": ["Nun", ",", "so", "sei", "auch", "mei\u00b7ne", "Lie\u00b7be"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "ADV", "VAFIN", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fcrhin tot und abgestorben!", "tokens": ["F\u00fcr\u00b7hin", "tot", "und", "ab\u00b7ge\u00b7stor\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Zitternd rei\u00df ich aus dem Busen", "tokens": ["Zit\u00b7ternd", "rei\u00df", "ich", "aus", "dem", "Bu\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Noch die letzten zarten Bl\u00fcten,", "tokens": ["Noch", "die", "letz\u00b7ten", "zar\u00b7ten", "Bl\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Gebe sie dem toten Liebchen", "tokens": ["Ge\u00b7be", "sie", "dem", "to\u00b7ten", "Lieb\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Bis zum J\u00fcngsten Tag zu h\u00fcten.", "tokens": ["Bis", "zum", "J\u00fcng\u00b7sten", "Tag", "zu", "h\u00fc\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Schwarzer G\u00e4rtner, Totengr\u00e4ber!", "tokens": ["Schwar\u00b7zer", "G\u00e4rt\u00b7ner", ",", "To\u00b7ten\u00b7gr\u00e4\u00b7ber", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "La\u00df, o la\u00df das Grab verwildern!", "tokens": ["La\u00df", ",", "o", "la\u00df", "das", "Grab", "ver\u00b7wil\u00b7dern", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "FM", "FM", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Seine wermutbittern Schauer", "tokens": ["Sei\u00b7ne", "wer\u00b7mut\u00b7bit\u00b7tern", "Schau\u00b7er"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Soll kein Lenz mehr freundlich mildern!", "tokens": ["Soll", "kein", "Lenz", "mehr", "freund\u00b7lich", "mil\u00b7dern", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "NN", "ADV", "ADJD", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Binde nicht mehr diese Zweige,", "tokens": ["Bin\u00b7de", "nicht", "mehr", "die\u00b7se", "Zwei\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ADV", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Pflege nicht mehr diese Rosen,", "tokens": ["Pfle\u00b7ge", "nicht", "mehr", "die\u00b7se", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ADV", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und mit dem verdorrten Kranze", "tokens": ["Und", "mit", "dem", "ver\u00b7dorr\u00b7ten", "Kran\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Mag der kalte Nordwind kosen!", "tokens": ["Mag", "der", "kal\u00b7te", "Nord\u00b7wind", "ko\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}