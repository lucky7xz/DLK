{"textgrid.poem.38653": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "9. Lausanne", "genre": "verse", "period": "N.A.", "pub_year": 1796, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seit auch ein Modenkalender die Wunder der Traubenkur feiert,", "tokens": ["Seit", "auch", "ein", "Mo\u00b7den\u00b7ka\u00b7len\u00b7der", "die", "Wun\u00b7der", "der", "Trau\u00b7ben\u00b7kur", "fei\u00b7ert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ART", "NN", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+--+--+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Welche doch Tissot, mein Sohn, l\u00e4ngst als Apostel verhie\u00df,", "tokens": ["Wel\u00b7che", "doch", "Tis\u00b7sot", ",", "mein", "Sohn", ",", "l\u00e4ngst", "als", "A\u00b7pos\u00b7tel", "ver\u00b7hie\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "NN", "$,", "PPOSAT", "NN", "$,", "ADV", "KOUS", "NN", "VVFIN", "$,"], "meter": "+--++-+-+-+--+", "measure": "iambic.septa.chol"}, "line.3": {"text": "Bin ich ein Mekka geworden f\u00fcr pre\u00dfhafte Standespersonen;", "tokens": ["Bin", "ich", "ein", "Mek\u00b7ka", "ge\u00b7wor\u00b7den", "f\u00fcr", "pre\u00df\u00b7haf\u00b7te", "Stan\u00b7des\u00b7per\u00b7so\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VAPP", "APPR", "ADJA", "NN", "$."], "meter": "---+--+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Eng' in Gem\u00e4chern des Dachs hausen die Meinen mit Noth.", "tokens": ["Eng'", "in", "Ge\u00b7m\u00e4\u00b7chern", "des", "Dachs", "hau\u00b7sen", "die", "Mei\u00b7nen", "mit", "Noth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "ART", "NN", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}}}}}