{"textgrid.poem.56926": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Und ", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Da t\u00fcrmte das g\u00f6ttlich emp\u00f6rte", "tokens": ["Da", "t\u00fcrm\u00b7te", "das", "g\u00f6tt\u00b7lich", "em\u00b7p\u00f6r\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJD", "VVFIN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "der Brandung B\u00e4nke", "tokens": ["der", "Bran\u00b7dung", "B\u00e4n\u00b7ke"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "zu sch\u00e4umenden Mauern", "tokens": ["zu", "sch\u00e4u\u00b7men\u00b7den", "Mau\u00b7ern"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "und brach sie", "tokens": ["und", "brach", "sie"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "in langen, br\u00fcnstigen Donnern", "tokens": ["in", "lan\u00b7gen", ",", "br\u00fcns\u00b7ti\u00b7gen", "Don\u00b7nern"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "weithin auf den Strand.", "tokens": ["weit\u00b7hin", "auf", "den", "Strand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "So sto\u00dfen tausend Hengste zugleich", "tokens": ["So", "sto\u00b7\u00dfen", "tau\u00b7send", "Hengs\u00b7te", "zu\u00b7gleich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "CARD", "NN", "ADV"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.9": {"text": "den Dampf durch die schrecklich gebl\u00e4hte N\u00fcster.", "tokens": ["den", "Dampf", "durch", "die", "schreck\u00b7lich", "ge\u00b7bl\u00e4h\u00b7te", "N\u00fcs\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Und ich, der schwache, eintagige Mensch,", "tokens": ["Und", "ich", ",", "der", "schwa\u00b7che", ",", "ein\u00b7ta\u00b7gi\u00b7ge", "Mensch", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "stand davor,", "tokens": ["stand", "da\u00b7vor", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PAV", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "mit fliegenden Gliedern,", "tokens": ["mit", "flie\u00b7gen\u00b7den", "Glie\u00b7dern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "und meine H\u00e4nde", "tokens": ["und", "mei\u00b7ne", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "\u00f6ffneten sich gegen das Meer,", "tokens": ["\u00f6ff\u00b7ne\u00b7ten", "sich", "ge\u00b7gen", "das", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "----+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "als wollten sie's versteinern,", "tokens": ["als", "woll\u00b7ten", "sie's", "ver\u00b7stei\u00b7nern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "dies dionysische Schauspiel,", "tokens": ["dies", "di\u00b7o\u00b7ny\u00b7si\u00b7sche", "Schau\u00b7spiel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "ADJA", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.8": {"text": "dieses k\u00f6nigliche Wogen-Sterben,", "tokens": ["die\u00b7ses", "k\u00f6\u00b7nig\u00b7li\u00b7che", "Wo\u00b7gen\u00b7Ster\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "diese morituri te salutant, Wagner!", "tokens": ["die\u00b7se", "mo\u00b7ri\u00b7tu\u00b7ri", "te", "sa\u00b7lu\u00b7tant", ",", "Wag\u00b7ner", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$,", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.10": {"text": "te salutant, ", "tokens": ["te", "sa\u00b7lu\u00b7tant", ","], "token_info": ["word", "word", "punct"], "pos": ["FM.la", "FM.la", "$,"], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Und da reckt' ich mich auf.", "tokens": ["Und", "da", "reckt'", "ich", "mich", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PRF", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und da lag mein Auge", "tokens": ["Und", "da", "lag", "mein", "Au\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPOSAT", "NN"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.3": {"text": "l\u00f6wenfunkelnd", "tokens": ["l\u00f6\u00b7wen\u00b7fun\u00b7kelnd"], "token_info": ["word"], "pos": ["VVPP"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "\u00fcber dem sterbenden Meer.", "tokens": ["\u00fc\u00b7ber", "dem", "ster\u00b7ben\u00b7den", "Meer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}}}}