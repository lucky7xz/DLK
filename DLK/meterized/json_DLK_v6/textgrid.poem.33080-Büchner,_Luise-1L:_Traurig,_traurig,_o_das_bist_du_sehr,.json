{"textgrid.poem.33080": {"metadata": {"author": {"name": "B\u00fcchner, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Traurig, traurig, o das bist du sehr,", "genre": "verse", "period": "N.A.", "pub_year": 1849, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Traurig, traurig, o das bist du sehr,", "tokens": ["Trau\u00b7rig", ",", "trau\u00b7rig", ",", "o", "das", "bist", "du", "sehr", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,", "FM", "PDS", "VAFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Armes Herz, so freudenlos und schwer!", "tokens": ["Ar\u00b7mes", "Herz", ",", "so", "freu\u00b7den\u00b7los", "und", "schwer", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Doch warum, warum darf ich nicht fragen,", "tokens": ["Doch", "wa\u00b7rum", ",", "wa\u00b7rum", "darf", "ich", "nicht", "fra\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "$,", "PWAV", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+--++--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Keine Antwort wei\u00dft du mir zu sagen.", "tokens": ["Kei\u00b7ne", "Ant\u00b7wort", "wei\u00dft", "du", "mir", "zu", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Wei\u00df der Himmel, warum \u00fcber ihn", "tokens": ["Wei\u00df", "der", "Him\u00b7mel", ",", "wa\u00b7rum", "\u00fc\u00b7ber", "ihn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "PWAV", "APPR", "PPER"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "D\u00fcstre Wolkenschleier trauernd zieh'n,", "tokens": ["D\u00fcst\u00b7re", "Wol\u00b7ken\u00b7schlei\u00b7er", "trau\u00b7ernd", "zieh'n", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und die Blume, die erst aufgesprossen,", "tokens": ["Und", "die", "Blu\u00b7me", ",", "die", "erst", "auf\u00b7ge\u00b7spros\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Warum sie der Tod so schnell geschlossen?", "tokens": ["Wa\u00b7rum", "sie", "der", "Tod", "so", "schnell", "ge\u00b7schlos\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "ADV", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Wei\u00df der l\u00e4chelnd heit're, junge Tag,", "tokens": ["Wei\u00df", "der", "l\u00e4\u00b7chelnd", "heit'\u00b7re", ",", "jun\u00b7ge", "Tag", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJD", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Der nur Gl\u00fcck und Freude spenden mag,", "tokens": ["Der", "nur", "Gl\u00fcck", "und", "Freu\u00b7de", "spen\u00b7den", "mag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "NN", "KON", "NN", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Warum kraftlos er mu\u00df still erbleichen,", "tokens": ["Wa\u00b7rum", "kraft\u00b7los", "er", "mu\u00df", "still", "er\u00b7blei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "VMFIN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wenn sich n\u00e4cht'ge Schatten auf ihn neigen?", "tokens": ["Wenn", "sich", "n\u00e4cht'\u00b7ge", "Schat\u00b7ten", "auf", "ihn", "nei\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ADJA", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Wei\u00df die Erde, die in Fr\u00fchlingspracht,", "tokens": ["Wei\u00df", "die", "Er\u00b7de", ",", "die", "in", "Fr\u00fch\u00b7lings\u00b7pracht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PRELS", "APPR", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Wie ein Kind so hold und lieblich lacht,", "tokens": ["Wie", "ein", "Kind", "so", "hold", "und", "lieb\u00b7lich", "lacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Warum bald in Winters eis'gem Wehen", "tokens": ["Wa\u00b7rum", "bald", "in", "Win\u00b7ters", "eis'\u00b7gem", "We\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "APPR", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ihre s\u00fc\u00dfe Sch\u00f6nheit mu\u00df vergehen?", "tokens": ["Ih\u00b7re", "s\u00fc\u00b7\u00dfe", "Sch\u00f6n\u00b7heit", "mu\u00df", "ver\u00b7ge\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Armes Herz und willst du mehr denn sein,", "tokens": ["Ar\u00b7mes", "Herz", "und", "willst", "du", "mehr", "denn", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "VMFIN", "PPER", "ADV", "ADV", "VAINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Als der Fr\u00fchling und der Sonnenschein?", "tokens": ["Als", "der", "Fr\u00fch\u00b7ling", "und", "der", "Son\u00b7nen\u00b7schein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Alles Heitre sinkt zum fr\u00fchen Sterben,", "tokens": ["Al\u00b7les", "Heit\u00b7re", "sinkt", "zum", "fr\u00fc\u00b7hen", "Ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Allem Sch\u00f6nen nahet das Verderben.", "tokens": ["Al\u00b7lem", "Sch\u00f6\u00b7nen", "na\u00b7het", "das", "Ver\u00b7der\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Ew'ge Trauer ist Gesetz der Welt,", "tokens": ["Ew'\u00b7ge", "Trau\u00b7er", "ist", "Ge\u00b7setz", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Nur im Wechsel ist ihr Lust gesellt,", "tokens": ["Nur", "im", "Wech\u00b7sel", "ist", "ihr", "Lust", "ge\u00b7sellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und je mehr zum Sch\u00f6nen du erkoren,", "tokens": ["Und", "je", "mehr", "zum", "Sch\u00f6\u00b7nen", "du", "er\u00b7ko\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "APPRART", "NN", "PPER", "VVINF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Um so tiefer wird sie dich durchbohren!", "tokens": ["Um", "so", "tie\u00b7fer", "wird", "sie", "dich", "durch\u00b7boh\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "ADJD", "VAFIN", "PPER", "PRF", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}