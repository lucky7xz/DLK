{"textgrid.poem.40600": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: In B\u00f6hmens Bergen hocheinsam liegt", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In B\u00f6hmens Bergen hocheinsam liegt", "tokens": ["In", "B\u00f6h\u00b7mens", "Ber\u00b7gen", "hoch\u00b7ein\u00b7sam", "liegt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "NN", "ADJD", "VVFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "In Tr\u00fcmmern eine Veste,", "tokens": ["In", "Tr\u00fcm\u00b7mern", "ei\u00b7ne", "Ves\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dran Epheu sich statt des M\u00f6rtels schmiegt,", "tokens": ["Dran", "E\u00b7pheu", "sich", "statt", "des", "M\u00f6r\u00b7tels", "schmiegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "NN", "PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "----+-+-+", "measure": "unknown.measure.tri"}, "line.4": {"text": "Drin Geier die schmausenden G\u00e4ste.", "tokens": ["Drin", "Gei\u00b7er", "die", "schmau\u00b7sen\u00b7den", "G\u00e4s\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Der Feind zerbrach einst Wall und Thurm,", "tokens": ["Der", "Feind", "zer\u00b7brach", "einst", "Wall", "und", "Thurm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "NE", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Geb\u00e4lk und Get\u00e4fel fra\u00df der Wurm,", "tokens": ["Ge\u00b7b\u00e4lk", "und", "Ge\u00b7t\u00e4\u00b7fel", "fra\u00df", "der", "Wurm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Die Zeit zerrieb die Reste.", "tokens": ["Die", "Zeit", "zer\u00b7rieb", "die", "Res\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "\u00bbo Wunderblick ins Thal hinein", "tokens": ["\u00bb", "o", "Wun\u00b7der\u00b7blick", "ins", "Thal", "hin\u00b7ein"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "FM", "NN", "APPRART", "NN", "APZR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und \u00fcber die Berg' und Lande!", "tokens": ["Und", "\u00fc\u00b7ber", "die", "Ber\u00b7g'", "und", "Lan\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "KON", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Raff' auf die Knochen, dein morsch Gestein,", "tokens": ["Raff'", "auf", "die", "Kno\u00b7chen", ",", "dein", "morsch", "Ge\u00b7stein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Steig auf im alten Gewande,", "tokens": ["Steig", "auf", "im", "al\u00b7ten", "Ge\u00b7wan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Du Leiche jetzt, o V\u00e4terschlo\u00df,", "tokens": ["Du", "Lei\u00b7che", "jetzt", ",", "o", "V\u00e4\u00b7ter\u00b7schlo\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "NN", "ADV", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ersteh' zum Leben neu und gro\u00df,", "tokens": ["Er\u00b7steh'", "zum", "Le\u00b7ben", "neu", "und", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ein Schmuck und Stolz dem Lande!\u00ab", "tokens": ["Ein", "Schmuck", "und", "Stolz", "dem", "Lan\u00b7de", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "KON", "NN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der junge Ritter sprach's und gebot;", "tokens": ["Der", "jun\u00b7ge", "Rit\u00b7ter", "sprach's", "und", "ge\u00b7bot", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NE", "KON", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Die Felsen im Bruch zerknallen,", "tokens": ["Die", "Fel\u00b7sen", "im", "Bruch", "zer\u00b7knal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Im Flammengew\u00f6lk der Kalkstein loht,", "tokens": ["Im", "Flam\u00b7men\u00b7ge\u00b7w\u00f6lk", "der", "Kalk\u00b7stein", "loht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die Riesen des Forstes fallen,", "tokens": ["Die", "Rie\u00b7sen", "des", "Fors\u00b7tes", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Und st\u00e4mmige Stiere keuchen bergan", "tokens": ["Und", "st\u00e4m\u00b7mi\u00b7ge", "Stie\u00b7re", "keu\u00b7chen", "ber\u00b7gan"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVFIN", "NE"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Mit Sparren und Quadern, mit Sims und Altan,", "tokens": ["Mit", "Spar\u00b7ren", "und", "Qua\u00b7dern", ",", "mit", "Sims", "und", "Al\u00b7tan", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "APPR", "NE", "KON", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "Mit Balken und S\u00e4ulen der Hallen.", "tokens": ["Mit", "Bal\u00b7ken", "und", "S\u00e4u\u00b7len", "der", "Hal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.4": {"line.1": {"text": "Hei, an den Bau griff Hand an Hand,", "tokens": ["Hei", ",", "an", "den", "Bau", "griff", "Hand", "an", "Hand", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPR", "ART", "NN", "VVFIN", "NN", "APPR", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Ein Tagwerk gab's aufs Beste:", "tokens": ["Ein", "Tag\u00b7werk", "gab's", "aufs", "Bes\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der neue Bau zwier mannshoch stand", "tokens": ["Der", "neu\u00b7e", "Bau", "zwier", "manns\u00b7hoch", "stand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV", "ADV", "VVFIN"], "meter": "-+-+--++", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Schon \u00fcber dem Tr\u00fcmmerreste.", "tokens": ["Schon", "\u00fc\u00b7ber", "dem", "Tr\u00fcm\u00b7mer\u00b7res\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Doch weh, was der Tag zu Werk gebracht,", "tokens": ["Doch", "weh", ",", "was", "der", "Tag", "zu", "Werk", "ge\u00b7bracht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PRELS", "ART", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Zerfallen ist's wieder \u00fcber Nacht,", "tokens": ["Zer\u00b7fal\u00b7len", "ist's", "wie\u00b7der", "\u00fc\u00b7ber", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "In Schutt liegt Morgens die Veste.", "tokens": ["In", "Schutt", "liegt", "Mor\u00b7gens", "die", "Ves\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "\u00bbo schlechter M\u00f6rtel, schlechtre Hand!", "tokens": ["\u00bb", "o", "schlech\u00b7ter", "M\u00f6r\u00b7tel", ",", "schlecht\u00b7re", "Hand", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "FM", "ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gebt Kraft ihm mit starkem Weine", "tokens": ["Gebt", "Kraft", "ihm", "mit", "star\u00b7kem", "Wei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "NN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und zwingt mit eiserner Klammern Band", "tokens": ["Und", "zwingt", "mit", "ei\u00b7ser\u00b7ner", "Klam\u00b7mern", "Band"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die ungehorsamen Steine!\u00ab", "tokens": ["Die", "un\u00b7ge\u00b7hor\u00b7sa\u00b7men", "Stei\u00b7ne", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Und so geschah's, doch \u00fcber Nacht", "tokens": ["Und", "so", "ge\u00b7scha\u00b7h's", ",", "doch", "\u00fc\u00b7ber", "Nacht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "ADJA", "$,", "ADV", "APPR", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Zerfiel, was der Tag zu Werk gebracht;", "tokens": ["Zer\u00b7fiel", ",", "was", "der", "Tag", "zu", "Werk", "ge\u00b7bracht", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Nur Tr\u00fcmmer im Morgenscheine!", "tokens": ["Nur", "Tr\u00fcm\u00b7mer", "im", "Mor\u00b7gen\u00b7schei\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPRART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Zum Ritter tritt ein Werkmann alt:", "tokens": ["Zum", "Rit\u00b7ter", "tritt", "ein", "Werk\u00b7mann", "alt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbsieh hin und uns nicht fluche:", "tokens": ["\u00bb", "sieh", "hin", "und", "uns", "nicht", "flu\u00b7che", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PTKVZ", "KON", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das R\u00fcstholz liegt, wo sie's f\u00e4llten, im Wald,", "tokens": ["Das", "R\u00fcs\u00b7tholz", "liegt", ",", "wo", "sie's", "f\u00e4ll\u00b7ten", ",", "im", "Wald", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "$,", "APPRART", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die Quadern unten im Bruche!", "tokens": ["Die", "Qua\u00b7dern", "un\u00b7ten", "im", "Bru\u00b7che", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "In solcher Art kein Bau zerf\u00e4llt,", "tokens": ["In", "sol\u00b7cher", "Art", "kein", "Bau", "zer\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den hat ein gewaltiger Feind zerschellt!", "tokens": ["Den", "hat", "ein", "ge\u00b7wal\u00b7ti\u00b7ger", "Feind", "zer\u00b7schellt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "La\u00df W\u00e4chter stehn dem Besuche.\u00ab", "tokens": ["La\u00df", "W\u00e4ch\u00b7ter", "stehn", "dem", "Be\u00b7su\u00b7che", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVIMP", "NE", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Die W\u00e4chter lehnen bei Nacht am Wall.", "tokens": ["Die", "W\u00e4ch\u00b7ter", "leh\u00b7nen", "bei", "Nacht", "am", "Wall", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "APPR", "NN", "APPRART", "NE", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da f\u00e4cheln so lau die Weste,", "tokens": ["Da", "f\u00e4\u00b7cheln", "so", "lau", "die", "Wes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADJD", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Der Mond bestreut ihr Aug' mit Metall,", "tokens": ["Der", "Mond", "be\u00b7streut", "ihr", "Aug'", "mit", "Me\u00b7tall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "In Tr\u00e4umen fl\u00fcstern die Aeste;", "tokens": ["In", "Tr\u00e4u\u00b7men", "fl\u00fcs\u00b7tern", "die", "A\u00b7es\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Da schlummern sie leise, leise ein.", "tokens": ["Da", "schlum\u00b7mern", "sie", "lei\u00b7se", ",", "lei\u00b7se", "ein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$,", "ADJD", "PTKVZ", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Man fand sie am Morgen unterm Gestein,", "tokens": ["Man", "fand", "sie", "am", "Mor\u00b7gen", "un\u00b7term", "Ge\u00b7stein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "APPRART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "In Tr\u00fcmmern lag die Veste.", "tokens": ["In", "Tr\u00fcm\u00b7mern", "lag", "die", "Ves\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Der Ritter sprach: \u00bbNur Muth bewahrt!", "tokens": ["Der", "Rit\u00b7ter", "sprach", ":", "\u00bb", "Nur", "Muth", "be\u00b7wahrt", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "ADV", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ans Werk, und la\u00dft das Trauern!\u00ab", "tokens": ["Ans", "Werk", ",", "und", "la\u00dft", "das", "Trau\u00b7ern", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "$,", "KON", "VVIMP", "ART", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das geht nicht zu in rechter Art,", "tokens": ["Das", "geht", "nicht", "zu", "in", "rech\u00b7ter", "Art", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKNEG", "PTKZU", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Denkt er bei sich mit Schauern.", "tokens": ["Denkt", "er", "bei", "sich", "mit", "Schau\u00b7ern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PRF", "APPR", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Gen Kloster Kukus trabt er dann:", "tokens": ["Gen", "Klos\u00b7ter", "Ku\u00b7kus", "trabt", "er", "dann", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "\u00bbherr Abt, o schlie\u00dft des Segens Bann,", "tokens": ["\u00bb", "herr", "Abt", ",", "o", "schlie\u00dft", "des", "Se\u00b7gens", "Bann", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NN", "NE", "$,", "FM", "VVFIN", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ihr k\u00f6nnt's, um meine Mauern!\u00ab", "tokens": ["Ihr", "k\u00f6nnt's", ",", "um", "mei\u00b7ne", "Mau\u00b7ern", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VMFIN", "$,", "KOUI", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Zu Nacht umwallten des Tages Bau", "tokens": ["Zu", "Nacht", "um\u00b7wall\u00b7ten", "des", "Ta\u00b7ges", "Bau"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Abt und seine Genossen,", "tokens": ["Der", "Abt", "und", "sei\u00b7ne", "Ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Der Weihrauch wirbelt' ins n\u00e4cht'ge Blau,", "tokens": ["Der", "Weih\u00b7rauch", "wir\u00b7belt'", "ins", "n\u00e4cht'\u00b7ge", "Blau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Vom Glanz der Fackeln umflossen.", "tokens": ["Vom", "Glanz", "der", "Fa\u00b7ckeln", "um\u00b7flos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Sie trugen ihm Kreuz und Weihbronn vor,", "tokens": ["Sie", "tru\u00b7gen", "ihm", "Kreuz", "und", "Weih\u00b7bronn", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Der M\u00f6nche Lieder in ernstem Chor", "tokens": ["Der", "M\u00f6n\u00b7che", "Lie\u00b7der", "in", "erns\u00b7tem", "Chor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Sich durch die Nacht ergossen.", "tokens": ["Sich", "durch", "die", "Nacht", "er\u00b7gos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Seht dort, behelmt, langb\u00e4rtig am Wall", "tokens": ["Seht", "dort", ",", "be\u00b7helmt", ",", "lang\u00b7b\u00e4r\u00b7tig", "am", "Wall"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADV", "$,", "VVFIN", "$,", "ADJD", "APPRART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Von riesigem Leib drei Recken,", "tokens": ["Von", "rie\u00b7si\u00b7gem", "Leib", "drei", "Re\u00b