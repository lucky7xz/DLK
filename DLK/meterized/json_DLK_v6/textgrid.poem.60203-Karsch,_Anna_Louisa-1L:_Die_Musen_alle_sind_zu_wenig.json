{"textgrid.poem.60203": {"metadata": {"author": {"name": "Karsch, Anna Louisa", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Musen alle sind zu wenig", "genre": "verse", "period": "N.A.", "pub_year": 1762, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Musen alle sind zu wenig", "tokens": ["Die", "Mu\u00b7sen", "al\u00b7le", "sind", "zu", "we\u00b7nig"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PIS", "VAFIN", "PTKA", "PIS"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Lied zu singen so voll Pracht!", "tokens": ["Ein", "Lied", "zu", "sin\u00b7gen", "so", "voll", "Pracht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVFIN", "ADV", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dein: ", "tokens": ["Dein", ":"], "token_info": ["word", "punct"], "pos": ["PPOSAT", "$."], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Drang tief in mich mit Gottes Macht.", "tokens": ["Drang", "tief", "in", "mich", "mit", "Got\u00b7tes", "Macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "PRF", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Im Tempel horchten alle Frommen.", "tokens": ["Im", "Tem\u00b7pel", "horch\u00b7ten", "al\u00b7le", "From\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der ganze Himmel, dachten sie,", "tokens": ["Der", "gan\u00b7ze", "Him\u00b7mel", ",", "dach\u00b7ten", "sie", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Rauscht itzt, und wird hernieder kommen,", "tokens": ["Rauscht", "itzt", ",", "und", "wird", "her\u00b7nie\u00b7der", "kom\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "KON", "VAFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Majest\u00e4t und Harmonie!", "tokens": ["Mit", "Ma\u00b7jes\u00b7t\u00e4t", "und", "Har\u00b7mo\u00b7nie", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Entz\u00fcckung f\u00fchlten alle Seelen,", "tokens": ["Ent\u00b7z\u00fc\u00b7ckung", "f\u00fchl\u00b7ten", "al\u00b7le", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Als n\u00e4hm ein s\u00e4uselnder Zephyr", "tokens": ["Als", "n\u00e4hm", "ein", "s\u00e4u\u00b7seln\u00b7der", "Ze\u00b7phyr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das sch\u00f6nste Lied von Philomelen", "tokens": ["Das", "sch\u00f6ns\u00b7te", "Lied", "von", "Phi\u00b7lo\u00b7me\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In seinen Mund, und br\u00e4cht es dir.", "tokens": ["In", "sei\u00b7nen", "Mund", ",", "und", "br\u00e4cht", "es", "dir", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "KON", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und du mit zaubrischen Verbreiten", "tokens": ["Und", "du", "mit", "zaub\u00b7ri\u00b7schen", "Ver\u00b7brei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Belebetest im Augenblick", "tokens": ["Be\u00b7le\u00b7be\u00b7test", "im", "Au\u00b7gen\u00b7blick"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "APPRART", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Das Lied auf mehr als tausend Saiten", "tokens": ["Das", "Lied", "auf", "mehr", "als", "tau\u00b7send", "Sai\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PIAT", "KOKOM", "CARD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und s\u00e4ngst es in den Hayn zur\u00fcck!", "tokens": ["Und", "s\u00e4ngst", "es", "in", "den", "Hayn", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}