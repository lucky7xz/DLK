{"textgrid.poem.64183": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbnun eile, Sohn Lindsay, Laird von Fle\u00df,", "genre": "verse", "period": "N.A.", "pub_year": 1873, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbnun eile, Sohn Lindsay, Laird von Fle\u00df,", "tokens": ["\u00bb", "nun", "ei\u00b7le", ",", "Sohn", "Lind\u00b7say", ",", "Laird", "von", "Fle\u00df", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "$,", "NN", "NE", "$,", "NN", "APPR", "NN", "$,"], "meter": "-+-++-+-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Leg' an das Hochzeitgewand:", "tokens": ["Leg'", "an", "das", "Hoch\u00b7zeit\u00b7ge\u00b7wand", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Die K\u00f6nigin harret zu Inverne\u00df,", "tokens": ["Die", "K\u00f6\u00b7ni\u00b7gin", "har\u00b7ret", "zu", "In\u00b7ver\u00b7ne\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NE", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Den Brautring in der Hand.", "tokens": ["Den", "Brau\u00b7tring", "in", "der", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sie schenkt dir Thron und Reichsgewalt,", "tokens": ["Sie", "schenkt", "dir", "Thron", "und", "Reichs\u00b7ge\u00b7walt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sohn Badwin, eile dich doch. \u2013\u00ab", "tokens": ["Sohn", "Bad\u00b7win", ",", "ei\u00b7le", "dich", "doch", ".", "\u2013", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NN", "NE", "$,", "VVFIN", "PPER", "ADV", "$.", "$(", "$("], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "\u00bbdie K\u00f6nigin-Witwe wird vierzig bald,", "tokens": ["\u00bb", "die", "K\u00f6\u00b7ni\u00b7gin\u00b7Wit\u00b7we", "wird", "vier\u00b7zig", "bald", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "CARD", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ich bin nicht drei\u00dfig noch.", "tokens": ["Ich", "bin", "nicht", "drei\u00b7\u00dfig", "noch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Zu alt ist weit mir die K\u00f6nigin!", "tokens": ["Zu", "alt", "ist", "weit", "mir", "die", "K\u00f6\u00b7ni\u00b7gin", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VAFIN", "ADJD", "PPER", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mylady, Ihr wi\u00dft es gut,", "tokens": ["My\u00b7la\u00b7dy", ",", "Ihr", "wi\u00dft", "es", "gut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ich trug ganz andere Lieb' in Sinn, \u2013", "tokens": ["Ich", "trug", "ganz", "an\u00b7de\u00b7re", "Lieb'", "in", "Sinn", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJA", "NN", "APPR", "NN", "$,", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Jung Ellen, das s\u00fc\u00dfe Blut!", "tokens": ["Jung", "El\u00b7len", ",", "das", "s\u00fc\u00b7\u00dfe", "Blut", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Wei\u00df war sie wie Schlehbl\u00fct', vom Morgen betaut,", "tokens": ["Wei\u00df", "war", "sie", "wie", "Schleh\u00b7bl\u00fct'", ",", "vom", "Mor\u00b7gen", "be\u00b7taut", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "KOKOM", "NN", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und ihr Mund war rosenzart:", "tokens": ["Und", "ihr", "Mund", "war", "ro\u00b7senz\u00b7art", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die K\u00f6nigin hat eine quittgelbe Haut,", "tokens": ["Die", "K\u00f6\u00b7ni\u00b7gin", "hat", "ei\u00b7ne", "quitt\u00b7gel\u00b7be", "Haut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Auf den Lippen steht ihr ein Bart.", "tokens": ["Auf", "den", "Lip\u00b7pen", "steht", "ihr", "ein", "Bart", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.5": {"line.1": {"text": "Wie war Ellen so hold, wann \u00fcber das Korn", "tokens": ["Wie", "war", "El\u00b7len", "so", "hold", ",", "wann", "\u00fc\u00b7ber", "das", "Korn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "NN", "ADV", "ADJD", "$,", "PWAV", "APPR", "ART", "NN"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Die Lerche mit Trillern flog,", "tokens": ["Die", "Ler\u00b7che", "mit", "Tril\u00b7lern", "flog", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wann die zarte Gestalt, am Wildrosdorn,", "tokens": ["Wann", "die", "zar\u00b7te", "Ge\u00b7stalt", ",", "am", "Wild\u00b7ros\u00b7dorn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$,", "APPRART", "NN", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Ich, die Bebende, zu mir zog.", "tokens": ["Ich", ",", "die", "Be\u00b7ben\u00b7de", ",", "zu", "mir", "zog", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "NN", "$,", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ich hing in den Busch da mein J\u00e4gerhorn", "tokens": ["Ich", "hing", "in", "den", "Busch", "da", "mein", "J\u00e4\u00b7ger\u00b7horn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ADV", "PPOSAT", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Und mein reiherbefiedert Barett,", "tokens": ["Und", "mein", "rei\u00b7her\u00b7be\u00b7fie\u00b7dert", "Ba\u00b7rett", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "VVFIN", "NN", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Das Brautgemach w\u00f6lbte der Wildrosdorn,", "tokens": ["Das", "Braut\u00b7ge\u00b7mach", "w\u00f6lb\u00b7te", "der", "Wild\u00b7ros\u00b7dorn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-++--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und das Heidekraut unser Bett.", "tokens": ["Und", "das", "Hei\u00b7de\u00b7kraut", "un\u00b7ser", "Bett", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.7": {"line.1": {"text": "Vom Kloster her\u00fcber das Ave klang,", "tokens": ["Vom", "Klos\u00b7ter", "her\u00b7\u00fc\u00b7ber", "das", "A\u00b7ve", "klang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Leis trug es verschwingend der West,", "tokens": ["Leis", "trug", "es", "ver\u00b7schwin\u00b7gend", "der", "West", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "VVPP", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Wir waren so still: \u2013 Rotkehlchen sang", "tokens": ["Wir", "wa\u00b7ren", "so", "still", ":", "\u2013", "Rot\u00b7kehl\u00b7chen", "sang"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$.", "$(", "NN", "VVFIN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Zutraulich zu Haupt uns im Nest.", "tokens": ["Zu\u00b7trau\u00b7lich", "zu", "Haupt", "uns", "im", "Nest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "PPER", "APPRART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.8": {"line.1": {"text": "Doch einst, als nach Hushydorp wieder ich kam,", "tokens": ["Doch", "einst", ",", "als", "nach", "Hus\u00b7hy\u00b7dorp", "wie\u00b7der", "ich", "kam", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "KOUS", "APPR", "NE", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Da war sie verschwunden \u2013 im Grab.\u00ab", "tokens": ["Da", "war", "sie", "ver\u00b7schwun\u00b7den", "\u2013", "im", "Grab", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVPP", "$(", "APPRART", "NN", "$.", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "\u00bbdem Himmel danke, der dir sie nahm,", "tokens": ["\u00bb", "dem", "Him\u00b7mel", "dan\u00b7ke", ",", "der", "dir", "sie", "nahm", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "$,", "PRELS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Und dir die K\u00f6nigin gab.", "tokens": ["Und", "dir", "die", "K\u00f6\u00b7ni\u00b7gin", "gab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.9": {"line.1": {"text": "Vergi\u00df, Laird Lindsay, der Sch\u00e4ferdirn,", "tokens": ["Ver\u00b7gi\u00df", ",", "Laird", "Lind\u00b7say", ",", "der", "Sch\u00e4\u00b7fer\u00b7dirn", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "$,", "NE", "NE", "$,", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mit ihrem Wildrosenkranz,", "tokens": ["Mit", "ih\u00b7rem", "Wild\u00b7ro\u00b7sen\u00b7kranz", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Die Krone von Schottland auf der Stirn,", "tokens": ["Die", "Kro\u00b7ne", "von", "Schott\u00b7land", "auf", "der", "Stirn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Um die Schultern Purpurglanz.\u00ab", "tokens": ["Um", "die", "Schul\u00b7tern", "Pur\u00b7pur\u00b7glanz", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KOUI", "ART", "NN", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Die Glocken l\u00e4uteten \u00fcber das Land:", "tokens": ["Die", "Glo\u00b7cken", "l\u00e4u\u00b7te\u00b7ten", "\u00fc\u00b7ber", "das", "Land", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Es empfingen, wohin er kam,", "tokens": ["Es", "emp\u00b7fin\u00b7gen", ",", "wo\u00b7hin", "er", "kam", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Die sch\u00f6nen M\u00e4dchen, den Kranz in der Hand,", "tokens": ["Die", "sch\u00f6\u00b7nen", "M\u00e4d\u00b7chen", ",", "den", "Kranz", "in", "der", "Hand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der K\u00f6nigin Br\u00e4utigam.", "tokens": ["Der", "K\u00f6\u00b7ni\u00b7gin", "Br\u00e4u\u00b7ti\u00b7gam", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.11": {"line.1": {"text": "Doch die sch\u00f6nen M\u00e4dchen staunten ihn an:", "tokens": ["Doch", "die", "sch\u00f6\u00b7nen", "M\u00e4d\u00b7chen", "staun\u00b7ten", "ihn", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "\u00bbwie h\u00e4ngt ihm das Haupt so schwer?", "tokens": ["\u00bb", "wie", "h\u00e4ngt", "ihm", "das", "Haupt", "so", "schwer", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "VVFIN", "PPER", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich n\u00e4hme wahrhaftig keinen zum Mann,", "tokens": ["Ich", "n\u00e4h\u00b7me", "wahr\u00b7haf\u00b7tig", "kei\u00b7nen", "zum", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PIAT", "APPRART", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der dabei so traurig w\u00e4r'.", "tokens": ["Der", "da\u00b7bei", "so", "trau\u00b7rig", "w\u00e4r'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PAV", "ADV", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Und er ist so sch\u00f6n, der stolze Knab',", "tokens": ["Und", "er", "ist", "so", "sch\u00f6n", ",", "der", "stol\u00b7ze", "Knab'", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "ADJD", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Und er darf die K\u00f6nigin frein, \u2013", "tokens": ["Und", "er", "darf", "die", "K\u00f6\u00b7ni\u00b7gin", "frein", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "VMFIN", "ART", "NN", "PTKVZ", "$,", "$("], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Doch er, als ritt' er in sein Grab,", "tokens": ["Doch", "er", ",", "als", "ritt'", "er", "in", "sein", "Grab", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "KOUS", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So gramschwer schaut er d'rein.\u00ab", "tokens": ["So", "gramsc\u00b7hwer", "schaut", "er", "d'\u00b7rein", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "PTKVZ", "$.", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.13": {"line.1": {"text": "Und als durch Hushydorp er ritt,", "tokens": ["Und", "als", "durch", "Hus\u00b7hy\u00b7dorp", "er", "ritt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPR", "NE", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da wies sein Geleit er weg,", "tokens": ["Da", "wies", "sein", "Ge\u00b7leit", "er", "weg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und stieg vom Ro\u00df und weinend schritt", "tokens": ["Und", "stieg", "vom", "Ro\u00df", "und", "wei\u00b7nend", "schritt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART",