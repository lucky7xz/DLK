{"dta.poem.13259": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "E ilftes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eich wiederhohle nicht, was ich nur kurz geh\u00f6rt,", "tokens": ["\u201e", "ich", "wie\u00b7der\u00b7hoh\u00b7le", "nicht", ",", "was", "ich", "nur", "kurz", "ge\u00b7h\u00f6rt", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PTKNEG", "$,", "PWS", "PPER", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "350\u201dMit was vor Fr\u00f6hlichkeit das Volck den Zufall ehrt;", "tokens": ["\"", "Mit", "was", "vor", "Fr\u00f6h\u00b7lich\u00b7keit", "das", "Volck", "den", "Zu\u00b7fall", "ehrt", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PRELS", "APPR", "NN", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201ekein Mensch ist, der nicht glaubt, es sey die Vorbedeutung,", "tokens": ["\u201e", "kein", "Mensch", "ist", ",", "der", "nicht", "glaubt", ",", "es", "sey", "die", "Vor\u00b7be\u00b7deu\u00b7tung", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PIAT", "NN", "VAFIN", "$,", "PRELS", "PTKNEG", "VVFIN", "$,", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201eder Vorgang und des Wercks gewisse Vorbereitung,", "tokens": ["\u201e", "der", "Vor\u00b7gang", "und", "des", "Wercks", "ge\u00b7wis\u00b7se", "Vor\u00b7be\u00b7rei\u00b7tung", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "KON", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201ewodurch das Vaterland der Feinde Groll zu Truz", "tokens": ["\u201e", "wo\u00b7durch", "das", "Va\u00b7ter\u00b7land", "der", "Fein\u00b7de", "Groll", "zu", "Truz"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PWAV", "ART", "NN", "ART", "NN", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201esein Heil erwerben soll, und den gew\u00fcnschten Schuz;", "tokens": ["\u201e", "sein", "Heil", "er\u00b7wer\u00b7ben", "soll", ",", "und", "den", "ge\u00b7w\u00fcnschten", "Schuz", ";"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "VVINF", "VMFIN", "$,", "KON", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.7": {"text": "355\u201dJa den Erfolg davon schon nach und nach empfinde,", "tokens": ["\"", "Ja", "den", "Er\u00b7folg", "da\u00b7von", "schon", "nach", "und", "nach", "emp\u00b7fin\u00b7de", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKANT", "ART", "NN", "PAV", "ADV", "APPR", "KON", "APPR", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201eund seine Zuversicht auf wahre S\u00e4ze gr\u00fcnde.", "tokens": ["\u201e", "und", "sei\u00b7ne", "Zu\u00b7ver\u00b7sicht", "auf", "wah\u00b7re", "S\u00e4\u00b7ze", "gr\u00fcn\u00b7de", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}