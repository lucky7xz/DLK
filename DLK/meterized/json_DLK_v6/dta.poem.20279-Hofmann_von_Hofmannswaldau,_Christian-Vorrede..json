{"dta.poem.20279": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Vorrede.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was aber klagt ihr an? Vor gieng ich wenig ein/", "tokens": ["Was", "a\u00b7ber", "klagt", "ihr", "an", "?", "Vor", "gieng", "ich", "we\u00b7nig", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "PPER", "PTKVZ", "$.", "APPR", "VVFIN", "PPER", "ADV", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Itzt leider! nur zuviel/ und mu\u00df verdammet seyn.", "tokens": ["Itzt", "lei\u00b7der", "!", "nur", "zu\u00b7viel", "/", "und", "mu\u00df", "ver\u00b7dam\u00b7met", "seyn", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$.", "ADV", "PIS", "$(", "KON", "VMFIN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Weil ich das schwerd entbl\u00f6st/ trug ich beschimpffte bande/", "tokens": ["Weil", "ich", "das", "schwerd", "ent\u00b7bl\u00f6st", "/", "trug", "ich", "be\u00b7schimpff\u00b7te", "ban\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$(", "VVFIN", "PPER", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und nun ich frieden will/ la\u00df ich den kopff zum pfande.", "tokens": ["Und", "nun", "ich", "frie\u00b7den", "will", "/", "la\u00df", "ich", "den", "kopff", "zum", "pfan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "VVINF", "VMFIN", "$(", "VVIMP", "PPER", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Habt ihr zum f\u00fcrsten mich und k\u00f6nig nicht gekr\u00f6nt?", "tokens": ["Habt", "ihr", "zum", "f\u00fcrs\u00b7ten", "mich", "und", "k\u00f6\u00b7nig", "nicht", "ge\u00b7kr\u00f6nt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "VVFIN", "PPER", "KON", "VMFIN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Warum denn werd ich mehr als sclaven itzt verh\u00f6hnt?", "tokens": ["Wa\u00b7rum", "denn", "werd", "ich", "mehr", "als", "scla\u00b7ven", "itzt", "ver\u00b7h\u00f6hnt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "PPER", "PIAT", "KOKOM", "VVFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ich k\u00f6nte frau und kind in wollust bey mir wissen;", "tokens": ["Ich", "k\u00f6n\u00b7te", "frau", "und", "kind", "in", "wol\u00b7lust", "bey", "mir", "wis\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "KON", "NN", "APPR", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Itzt mu\u00df ich frau und kind und ruh und friede missen.", "tokens": ["Itzt", "mu\u00df", "ich", "frau", "und", "kind", "und", "ruh", "und", "frie\u00b7de", "mis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "KON", "NN", "KON", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Mir schwur mein unterthan: itzt bin ich mehr denn knecht!", "tokens": ["Mir", "schwur", "mein", "un\u00b7ter\u00b7than", ":", "itzt", "bin", "ich", "mehr", "denn", "knecht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJD", "$.", "ADV", "VAFIN", "PPER", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Gebt antwort! sprecht frey aus! sind eure sachen recht? rc.", "tokens": ["Gebt", "ant\u00b7wort", "!", "sprecht", "frey", "aus", "!", "sind", "eu\u00b7re", "sa\u00b7chen", "recht", "?", "rc", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVIMP", "NN", "$.", "VVFIN", "ADJD", "PTKVZ", "$.", "VAFIN", "PPOSAT", "ADJA", "NN", "$.", "NE", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}}}}}