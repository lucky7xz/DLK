{"textgrid.poem.51170": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Venus von Milo", "genre": "verse", "period": "N.A.", "pub_year": 1878, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie einst die Mediz\u00e4erin", "tokens": ["Wie", "einst", "die", "Me\u00b7di\u00b7z\u00e4\u00b7e\u00b7rin"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "NN"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.2": {"text": "Bist, \u00c4rmste, du jetzt in der Mode", "tokens": ["Bist", ",", "\u00c4rms\u00b7te", ",", "du", "jetzt", "in", "der", "Mo\u00b7de"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "$,", "NN", "$,", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "-+-+++-+-", "measure": "unknown.measure.penta"}, "line.3": {"text": "Und stehst in Gips, Porz'lan und Zinn", "tokens": ["Und", "stehst", "in", "Gips", ",", "Porz'\u00b7lan", "und", "Zinn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NE", "$,", "NE", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf Schreibtisch, Ofen und Kommode.", "tokens": ["Auf", "Schreib\u00b7tisch", ",", "O\u00b7fen", "und", "Kom\u00b7mo\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Suppe dampft, Geplauder t\u00f6nt,", "tokens": ["Die", "Sup\u00b7pe", "dampft", ",", "Ge\u00b7plau\u00b7der", "t\u00f6nt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gez\u00e4nk und schn\u00f6des Kindsgeschrei;", "tokens": ["Ge\u00b7z\u00e4nk", "und", "schn\u00f6\u00b7des", "Kinds\u00b7ge\u00b7schrei", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "An das Ger\u00fcmpel l\u00e4ngst gew\u00f6hnt,", "tokens": ["An", "das", "Ge\u00b7r\u00fcm\u00b7pel", "l\u00e4ngst", "ge\u00b7w\u00f6hnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Schaust du an allem still vorbei.", "tokens": ["Schaust", "du", "an", "al\u00b7lem", "still", "vor\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PIS", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie durch den Glanz des Tempeltors", "tokens": ["Wie", "durch", "den", "Glanz", "des", "Tem\u00b7pel\u00b7tors"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sieht man dich in die Ferne lauschen,", "tokens": ["Sieht", "man", "dich", "in", "die", "Fer\u00b7ne", "lau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PRF", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und in der Muschel deines Ohrs", "tokens": ["Und", "in", "der", "Mu\u00b7schel", "dei\u00b7nes", "Ohrs"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "H\u00f6rst du azurne Wogen rauschen!", "tokens": ["H\u00f6rst", "du", "a\u00b7zur\u00b7ne", "Wo\u00b7gen", "rau\u00b7schen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "VVINF", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}}}}