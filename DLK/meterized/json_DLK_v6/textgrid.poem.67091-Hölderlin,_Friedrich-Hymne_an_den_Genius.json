{"textgrid.poem.67091": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Hymne an den Genius", "genre": "verse", "period": "N.A.", "pub_year": 1792, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heil! das schlummernde Gefieder", "tokens": ["Heil", "!", "das", "schlum\u00b7mern\u00b7de", "Ge\u00b7fie\u00b7der"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist zu neuem Flug erwacht,", "tokens": ["Ist", "zu", "neu\u00b7em", "Flug", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Triumphierend f\u00fchl ich wieder", "tokens": ["Tri\u00b7um\u00b7phie\u00b7rend", "f\u00fchl", "ich", "wie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lieb und stolze Geistesmacht;", "tokens": ["Lieb", "und", "stol\u00b7ze", "Geis\u00b7tes\u00b7macht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Siehe! deiner Himmelsflamme,", "tokens": ["Sie\u00b7he", "!", "dei\u00b7ner", "Him\u00b7mels\u00b7flam\u00b7me", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "$.", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Deiner Freud und St\u00e4rke voll,", "tokens": ["Dei\u00b7ner", "Freud", "und", "St\u00e4r\u00b7ke", "voll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Herrscher in der G\u00f6tter Stamme!", "tokens": ["Herr\u00b7scher", "in", "der", "G\u00f6t\u00b7ter", "Stam\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sei der k\u00fchnen Liebe Zoll.", "tokens": ["Sei", "der", "k\u00fch\u00b7nen", "Lie\u00b7be", "Zoll", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ha! der br\u00fcderlichen Milde,", "tokens": ["Ha", "!", "der", "br\u00fc\u00b7der\u00b7li\u00b7chen", "Mil\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "So von deiner Stirne spricht!", "tokens": ["So", "von", "dei\u00b7ner", "Stir\u00b7ne", "spricht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Solch harmonisches Gebilde", "tokens": ["Solch", "har\u00b7mo\u00b7ni\u00b7sches", "Ge\u00b7bil\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weidete kein Auge nicht;", "tokens": ["Wei\u00b7de\u00b7te", "kein", "Au\u00b7ge", "nicht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie um ihn die Aare schweben,", "tokens": ["Wie", "um", "ihn", "die", "Aa\u00b7re", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie die Lock im Fluge weht! \u2013", "tokens": ["Wie", "die", "Lock", "im", "Flu\u00b7ge", "weht", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN", "VVFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wo im ungeme\u00dfnen Leben", "tokens": ["Wo", "im", "un\u00b7ge\u00b7me\u00df\u00b7nen", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Lebt so s\u00fc\u00dfe Majest\u00e4t?", "tokens": ["Lebt", "so", "s\u00fc\u00b7\u00dfe", "Ma\u00b7jes\u00b7t\u00e4t", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "L\u00e4chelnd sah der Holde nieder", "tokens": ["L\u00e4\u00b7chelnd", "sah", "der", "Hol\u00b7de", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "PTKVZ"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf die winterliche Flur,", "tokens": ["Auf", "die", "win\u00b7ter\u00b7li\u00b7che", "Flur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und sie lebt und liebet wieder,", "tokens": ["Und", "sie", "lebt", "und", "lie\u00b7bet", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "KON", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die entschlummerte Natur;", "tokens": ["Die", "ent\u00b7schlum\u00b7mer\u00b7te", "Na\u00b7tur", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Um die H\u00fcgel und die Tale", "tokens": ["Um", "die", "H\u00fc\u00b7gel", "und", "die", "Ta\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "NN", "KON", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Jauchz ich nun im Vollgenu\u00df,", "tokens": ["Jauchz", "ich", "nun", "im", "Voll\u00b7ge\u00b7nu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "\u00dcber deinem Freudenmahle,", "tokens": ["\u00dc\u00b7ber", "dei\u00b7nem", "Freu\u00b7den\u00b7mah\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "K\u00f6niglicher Genius!", "tokens": ["K\u00f6\u00b7nig\u00b7li\u00b7cher", "Ge\u00b7nius", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Ha! wie diese G\u00f6tteraue", "tokens": ["Ha", "!", "wie", "die\u00b7se", "G\u00f6t\u00b7te\u00b7rau\u00b7e"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ITJ", "$.", "PWAV", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wieder l\u00e4chelt und gedeiht!", "tokens": ["Wie\u00b7der", "l\u00e4\u00b7chelt", "und", "ge\u00b7deiht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Alles, was ich f\u00fchl und schaue,", "tokens": ["Al\u00b7les", ",", "was", "ich", "f\u00fchl", "und", "schau\u00b7e", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PWS", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Felsen hat der Falk erschwungen,", "tokens": ["Fel\u00b7sen", "hat", "der", "Falk", "er\u00b7schwun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sich, wie dieses Herz, zu freun,", "tokens": ["Sich", ",", "wie", "die\u00b7ses", "Herz", ",", "zu", "freun", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PRF", "$,", "PWAV", "PDAT", "NN", "$,", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und, von gleicher Kraft durchdrungen,", "tokens": ["Und", ",", "von", "glei\u00b7cher", "Kraft", "durch\u00b7drun\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Strebt und rauscht der Eichenhain.", "tokens": ["Strebt", "und", "rauscht", "der", "Ei\u00b7chen\u00b7hain", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Unter liebendem Gekose", "tokens": ["Un\u00b7ter", "lie\u00b7ben\u00b7dem", "Ge\u00b7ko\u00b7se"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schmieget Well an Welle sich;", "tokens": ["Schmie\u00b7get", "Well", "an", "Wel\u00b7le", "sich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "APPR", "NN", "PRF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Liebend f\u00fchlt die s\u00fc\u00dfe Rose,", "tokens": ["Lie\u00b7bend", "f\u00fchlt", "die", "s\u00fc\u00b7\u00dfe", "Ro\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fchlt die heilge Myrte dich;", "tokens": ["F\u00fchlt", "die", "heil\u00b7ge", "Myr\u00b7te", "dich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Tausend frohe Leben winden", "tokens": ["Tau\u00b7send", "fro\u00b7he", "Le\u00b7ben", "win\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["CARD", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sch\u00fcchtern sich um Tellus Brust,", "tokens": ["Sch\u00fcch\u00b7tern", "sich", "um", "Tel\u00b7lus", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "NE", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und dem blauen Aether k\u00fcnden", "tokens": ["Und", "dem", "blau\u00b7en", "A\u00b7e\u00b7ther", "k\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Tausend Jubel deine Lust.", "tokens": ["Tau\u00b7send", "Ju\u00b7bel", "dei\u00b7ne", "Lust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Doch des Herzens sch\u00f6ne Flamme,", "tokens": ["Doch", "des", "Her\u00b7zens", "sch\u00f6\u00b7ne", "Flam\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die mir deine Huld verlieh,", "tokens": ["Die", "mir", "dei\u00b7ne", "Huld", "ver\u00b7lieh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Herrscher in der G\u00f6tter Stamme!", "tokens": ["Herr\u00b7scher", "in", "der", "G\u00f6t\u00b7ter", "Stam\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "S\u00fc\u00dfer, stolzer f\u00fchl ich sie;", "tokens": ["S\u00fc\u00b7\u00dfer", ",", "stol\u00b7zer", "f\u00fchl", "ich", "sie", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "VVFIN", "PPER", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Deine Fr\u00fchlinge verbl\u00fchten,", "tokens": ["Dei\u00b7ne", "Fr\u00fch\u00b7lin\u00b7ge", "ver\u00b7bl\u00fch\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Manch Geliebtes welkte dir; \u2013", "tokens": ["Manch", "Ge\u00b7lieb\u00b7tes", "welk\u00b7te", "dir", ";", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wie vor Jahren sie ergl\u00fchten,", "tokens": ["Wie", "vor", "Jah\u00b7ren", "sie", "er\u00b7gl\u00fch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Gl\u00fchen Herz und Stirne mir.", "tokens": ["Gl\u00fc\u00b7hen", "Herz", "und", "Stir\u00b7ne", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "O! du lohnst die stille Bitte", "tokens": ["O", "!", "du", "lohnst", "die", "stil\u00b7le", "Bit\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Noch mit innigem Genu\u00df,", "tokens": ["Noch", "mit", "in\u00b7ni\u00b7gem", "Ge\u00b7nu\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Leitest noch des Pilgers Tritte", "tokens": ["Lei\u00b7test", "noch", "des", "Pil\u00b7gers", "Trit\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zu der Freude G\u00f6tterku\u00df;", "tokens": ["Zu", "der", "Freu\u00b7de", "G\u00f6t\u00b7ter\u00b7ku\u00df", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mit der Balsamtropfe k\u00fchlen", "tokens": ["Mit", "der", "Bal\u00b7sam\u00b7trop\u00b7fe", "k\u00fch\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Hoffnungen die Wunde doch,", "tokens": ["Hoff\u00b7nun\u00b7gen", "die", "Wun\u00b7de", "doch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "S\u00fc\u00dfe T\u00e4uschungen umspielen", "tokens": ["S\u00fc\u00b7\u00dfe", "T\u00e4u\u00b7schun\u00b7gen", "um\u00b7spie\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVINF"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.8": {"text": "Doch die d\u00fcrren Pfade noch.", "tokens": ["Doch", "die", "d\u00fcr\u00b7ren", "Pfa\u00b7de", "noch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Jedem Adel hingegeben,", "tokens": ["Je\u00b7dem", "A\u00b7del", "hin\u00b7ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jeder lesbischen Gestalt,", "tokens": ["Je\u00b7der", "les\u00b7bi\u00b7schen", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Huldiget das trunkne Leben", "tokens": ["Hul\u00b7di\u00b7get", "das", "trunk\u00b7ne", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.4": {"text": "Noch der Sch\u00f6nheit Allgewalt;", "tokens": ["Noch", "der", "Sch\u00f6n\u00b7heit", "All\u00b7ge\u00b7walt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "T\u00f6rig hab ich oft gerungen,", "tokens": ["T\u00f6\u00b7rig", "hab", "ich", "oft", "ge\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dennoch herrscht 