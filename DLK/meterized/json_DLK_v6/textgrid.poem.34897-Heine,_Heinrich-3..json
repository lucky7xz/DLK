{"textgrid.poem.34897": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gestern noch f\u00fcrs liebe Brot", "tokens": ["Ge\u00b7stern", "noch", "f\u00fcrs", "lie\u00b7be", "Brot"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "W\u00e4lzte sie sich tief im Kot,", "tokens": ["W\u00e4lz\u00b7te", "sie", "sich", "tief", "im", "Kot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber heute schon mit vieren", "tokens": ["A\u00b7ber", "heu\u00b7te", "schon", "mit", "vie\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "APPR", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00e4hrt das stolze Weib spazieren.", "tokens": ["F\u00e4hrt", "das", "stol\u00b7ze", "Weib", "spa\u00b7zie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "In die seidnen Kissen dr\u00fcckt", "tokens": ["In", "die", "seid\u00b7nen", "Kis\u00b7sen", "dr\u00fcckt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Sie das Lockenhaupt, und blickt", "tokens": ["Sie", "das", "Lo\u00b7cken\u00b7haupt", ",", "und", "blickt"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ART", "NN", "$,", "KON", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Vornehm auf den gro\u00dfen Haufen", "tokens": ["Vor\u00b7nehm", "auf", "den", "gro\u00b7\u00dfen", "Hau\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Derer, die zu Fu\u00dfe laufen.", "tokens": ["De\u00b7rer", ",", "die", "zu", "Fu\u00b7\u00dfe", "lau\u00b7fen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wenn ich dich so fahren seh,", "tokens": ["Wenn", "ich", "dich", "so", "fah\u00b7ren", "seh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Tut es mir im Herzen weh!", "tokens": ["Tut", "es", "mir", "im", "Her\u00b7zen", "weh", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach, es wird dich dieser Wagen", "tokens": ["Ach", ",", "es", "wird", "dich", "die\u00b7ser", "Wa\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PPER", "VAFIN", "PPER", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nach dem Hospitale tragen,", "tokens": ["Nach", "dem", "Hos\u00b7pi\u00b7ta\u00b7le", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wo der grausenhafte Tod", "tokens": ["Wo", "der", "grau\u00b7sen\u00b7haf\u00b7te", "Tod"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Endlich endigt deine Not,", "tokens": ["End\u00b7lich", "en\u00b7digt", "dei\u00b7ne", "Not", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und der Carabin mit schmierig", "tokens": ["Und", "der", "Ca\u00b7ra\u00b7bin", "mit", "schmie\u00b7rig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Plumper Hand und lernbegierig", "tokens": ["Plum\u00b7per", "Hand", "und", "lern\u00b7be\u00b7gie\u00b7rig"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "NN", "KON", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Deinen sch\u00f6nen Leib zerfetzt,", "tokens": ["Dei\u00b7nen", "sch\u00f6\u00b7nen", "Leib", "zer\u00b7fetzt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Anatomisch ihn zersetzt \u2013", "tokens": ["A\u00b7nat\u00b7o\u00b7misch", "ihn", "zer\u00b7setzt", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Deine Rosse trifft nicht minder", "tokens": ["Dei\u00b7ne", "Ros\u00b7se", "trifft", "nicht", "min\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKNEG", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Einst zu Montfaucon der Schinder.", "tokens": ["Einst", "zu", "Mont\u00b7fau\u00b7con", "der", "Schin\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}