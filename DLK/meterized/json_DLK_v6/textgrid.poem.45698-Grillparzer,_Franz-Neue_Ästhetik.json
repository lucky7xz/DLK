{"textgrid.poem.45698": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Neue \u00c4sthetik", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Unsre Dichter hassen die Gem\u00fctlich-Schwachen,", "tokens": ["Uns\u00b7re", "Dich\u00b7ter", "has\u00b7sen", "die", "Ge\u00b7m\u00fct\u00b7lich\u00b7Schwa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Das Starke, Gro\u00dfe ist jetzt ihre Puppe;", "tokens": ["Das", "Star\u00b7ke", ",", "Gro\u00b7\u00dfe", "ist", "jetzt", "ih\u00b7re", "Pup\u00b7pe", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJA", "VAFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie wollen die Deutschen zu Spartanern machen", "tokens": ["Sie", "wol\u00b7len", "die", "Deut\u00b7schen", "zu", "Spar\u00b7ta\u00b7nern", "ma\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "NN", "APPR", "NN", "VVINF"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und kochen daher alle schwarze Suppe.", "tokens": ["Und", "ko\u00b7chen", "da\u00b7her", "al\u00b7le", "schwar\u00b7ze", "Sup\u00b7pe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}