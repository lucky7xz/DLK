{"dta.poem.16108": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "162.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Schiff vor Anker, doch die Segel aufgespannt;", "tokens": ["Ein", "Schiff", "vor", "An\u00b7ker", ",", "doch", "die", "Se\u00b7gel", "auf\u00b7ge\u00b7spannt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mein Sohn, dis Sinnbild ist der Widersinn genannt.", "tokens": ["Mein", "Sohn", ",", "dis", "Sinn\u00b7bild", "ist", "der", "Wi\u00b7der\u00b7sinn", "ge\u00b7nannt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PDS", "NN", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nicht Widersinn, mein Sohn, du darfst es Unsinn nennen:", "tokens": ["Nicht", "Wi\u00b7der\u00b7sinn", ",", "mein", "Sohn", ",", "du", "darfst", "es", "Un\u00b7sinn", "nen\u00b7nen", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "$,", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Fest unten wurzeln und in L\u00fcften weiter rennen.", "tokens": ["Fest", "un\u00b7ten", "wur\u00b7zeln", "und", "in", "L\u00fcf\u00b7ten", "wei\u00b7ter", "ren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVINF", "KON", "APPR", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wenn nicht das Segel rei\u00dft, so rei\u00dft das Ankerseil;", "tokens": ["Wenn", "nicht", "das", "Se\u00b7gel", "rei\u00dft", ",", "so", "rei\u00dft", "das", "An\u00b7ker\u00b7seil", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ART", "NN", "VVFIN", "$,", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und stets gef\u00e4hrdet ist so oder so das Heil.", "tokens": ["Und", "stets", "ge\u00b7f\u00e4hr\u00b7det", "ist", "so", "o\u00b7der", "so", "das", "Heil", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "VAFIN", "ADV", "KON", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}