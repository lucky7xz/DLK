{"dta.poem.20933": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Holdenreich an Adelinden.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Das Schreiben so du schickst/ das f\u00fchr' ich zu", "tokens": ["Das", "Schrei\u00b7ben", "so", "du", "schickst", "/", "das", "f\u00fchr'", "ich", "zu"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "PPER", "VVFIN", "$(", "PDS", "VVFIN", "PPER", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "dem Hertzen/", "tokens": ["dem", "Hert\u00b7zen", "/"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Denn was von Hertzen kom\u0303t/ mu\u00df auch beym", "tokens": ["Denn", "was", "von", "Hert\u00b7zen", "kom\u0303t", "/", "mu\u00df", "auch", "beym"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PWS", "APPR", "NN", "VVFIN", "$(", "VMFIN", "ADV", "APPRART"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Hertzen stehn/", "tokens": ["Hert\u00b7zen", "stehn", "/"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Es zeiget keine Nacht so viel entbranter Kertzen/", "tokens": ["Es", "zei\u00b7get", "kei\u00b7ne", "Nacht", "so", "viel", "ent\u00b7bran\u00b7ter", "Kert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "ADV", "PIAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Als Seuffzer diesen Tag aus meiner Seele gehn.", "tokens": ["Als", "Seuff\u00b7zer", "die\u00b7sen", "Tag", "aus", "mei\u00b7ner", "See\u00b7le", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PDAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der Himmel solte dich mit dem Gestirne kr\u00f6nen/", "tokens": ["Der", "Him\u00b7mel", "sol\u00b7te", "dich", "mit", "dem", "Ge\u00b7stir\u00b7ne", "kr\u00f6\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PRF", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Du bist/ wie mich bedeucht/ viel gr\u00f6\u00dfrer Ehre werth/", "tokens": ["Du", "bist", "/", "wie", "mich", "be\u00b7deucht", "/", "viel", "gr\u00f6\u00df\u00b7rer", "Eh\u00b7re", "werth", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$(", "PWAV", "PPER", "VVFIN", "$(", "PIAT", "ADJA", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "So mustu deinen Mann dich schmertzlich lassen h\u00f6h-", "tokens": ["So", "mus\u00b7tu", "dei\u00b7nen", "Mann", "dich", "schmertz\u00b7lich", "las\u00b7sen", "h\u00f6h"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "PPER", "ADJD", "VVINF", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "nen/", "tokens": ["nen", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "-", "measure": "single.down"}, "line.11": {"text": "Dem Wein und Alter hat Verstand und Kraft ver-", "tokens": ["Dem", "Wein", "und", "Al\u00b7ter", "hat", "Ver\u00b7stand", "und", "Kraft", "ver"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "VAFIN", "NN", "KON", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "zehrt.", "tokens": ["zehrt", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}, "line.13": {"text": "Ach k\u00f6nt\u2019 ich deine Noth mit Bluthe doch beweinen/", "tokens": ["Ach", "k\u00f6nt'", "ich", "dei\u00b7ne", "Noth", "mit", "Blu\u00b7the", "doch", "be\u00b7wei\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PPOSAT", "NN", "APPR", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Denn schlechte Thr\u00e4nen seyn zu diesem zugemein/", "tokens": ["Denn", "schlech\u00b7te", "Thr\u00e4\u00b7nen", "seyn", "zu", "die\u00b7sem", "zu\u00b7ge\u00b7mein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "VAINF", "APPR", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ich wolte meinen Sinn dir sattsam lassen scheinen/", "tokens": ["Ich", "wol\u00b7te", "mei\u00b7nen", "Sinn", "dir", "satt\u00b7sam", "las\u00b7sen", "schei\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "PPER", "ADJD", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und zeigen/ was in mir vor treue Geister seyn.", "tokens": ["Und", "zei\u00b7gen", "/", "was", "in", "mir", "vor", "treu\u00b7e", "Geis\u00b7ter", "seyn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$(", "PWS", "APPR", "PPER", "APPR", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Das Hertze bleht sich auf/ wenn ich bey mir betrachte/", "tokens": ["Das", "Hert\u00b7ze", "bleht", "sich", "auf", "/", "wenn", "ich", "bey", "mir", "be\u00b7trach\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "VVFIN", "PRF", "APPR", "$(", "KOUS", "PPER", "APPR", "PPER", "VVFIN", "$("], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "Wie deine sch\u00f6ne Brust nichts als der Flor bedeckt/", "tokens": ["Wie", "dei\u00b7ne", "sch\u00f6\u00b7ne", "Brust", "nichts", "als", "der", "Flor", "be\u00b7deckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "PIS", "KOKOM", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Wie du verlassen bist/ und sich sonst nichts bey Nachte/", "tokens": ["Wie", "du", "ver\u00b7las\u00b7sen", "bist", "/", "und", "sich", "sonst", "nichts", "bey", "Nach\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVINF", "VAFIN", "$(", "KON", "PRF", "ADV", "PIS", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Als nur die Einsamkeit dir an die Seite streckt.", "tokens": ["Als", "nur", "die", "Ein\u00b7sam\u00b7keit", "dir", "an", "die", "Sei\u00b7te", "streckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Wie deine Jugend soll unendlich Brache liegen/", "tokens": ["Wie", "dei\u00b7ne", "Ju\u00b7gend", "soll", "un\u00b7end\u00b7lich", "Bra\u00b7che", "lie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VMFIN", "ADJD", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Und deine Lippen nicht der rechte Ku\u00df benetzt;", "tokens": ["Und", "dei\u00b7ne", "Lip\u00b7pen", "nicht", "der", "rech\u00b7te", "Ku\u00df", "be\u00b7netzt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PTKNEG", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Da\u00df deiner Bluhmen Pracht wie Stoppel soll verflie-", "tokens": ["Da\u00df", "dei\u00b7ner", "Bluh\u00b7men", "Pracht", "wie", "Stop\u00b7pel", "soll", "ver\u00b7flie"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "KOKOM", "NN", "VMFIN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "gen/", "tokens": ["gen", "/"], "token_info": ["word", "punct"], "pos": ["APPR", "$("], "meter": "-", "measure": "single.down"}, "line.25": {"text": "Eh\u2019 als die Liebe sich auf ihre Bl\u00e4tter setzt.", "tokens": ["Eh'", "als", "die", "Lie\u00b7be", "sich", "auf", "ih\u00b7re", "Bl\u00e4t\u00b7ter", "setzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Ach deine Jugend ist nur eine lange Faste/", "tokens": ["Ach", "dei\u00b7ne", "Ju\u00b7gend", "ist", "nur", "ei\u00b7ne", "lan\u00b7ge", "Fas\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PPOSAT", "NN", "VAFIN", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "An die sich Gall und Pein der Marter-Woche h\u00e4ngt/", "tokens": ["An", "die", "sich", "Gall", "und", "Pein", "der", "Mar\u00b7ter\u00b7Wo\u00b7che", "h\u00e4ngt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PRF", "NE", "KON", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Es ruft die D\u00fcrfftigkeit fast st\u00fcndlich dich zu Gaste/", "tokens": ["Es", "ruft", "die", "D\u00fcr\u00b7ff\u00b7tig\u00b7keit", "fast", "st\u00fcnd\u00b7lich", "dich", "zu", "Gas\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "ADJD", "PPER", "APPR", "NN", "$("], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.29": {"text": "So dich mit H\u00fclsen speist und leeren B\u00e4chern tr\u00e4nckt.", "tokens": ["So", "dich", "mit", "H\u00fcl\u00b7sen", "speist", "und", "lee\u00b7ren", "B\u00e4\u00b7chern", "tr\u00e4nckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "APPR", "NN", "VVFIN", "KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Man will dich der Natur zur Mammeluck in machen/", "tokens": ["Man", "will", "dich", "der", "Na\u00b7tur", "zur", "Mam\u00b7me\u00b7luck", "in", "ma\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PRF", "ART", "NN", "APPRART", "NN", "APPR", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Doch wer verleugnet dis was er im Busen tr\u00e4gt/", "tokens": ["Doch", "wer", "ver\u00b7leug\u00b7net", "dis", "was", "er", "im", "Bu\u00b7sen", "tr\u00e4gt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "PDS", "PRELS", "PPER", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Und was auch wenn du schl\u00e4fst geschworen hat zuwa-", "tokens": ["Und", "was", "auch", "wenn", "du", "schl\u00e4fst", "ge\u00b7schwo\u00b7ren", "hat", "zu\u00b7wa"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "ADV", "KOUS", "PPER", "VVFIN", "VVPP", "VAFIN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "chen/", "tokens": ["chen", "/"], "token_info": ["word", "punct"], "pos": ["NE", "$("], "meter": "-", "measure": "single.down"}, "line.34": {"text": "Und in dem Traume dir gar manchen Sturm erregt.", "tokens": ["Und", "in", "dem", "Trau\u00b7me", "dir", "gar", "man\u00b7chen", "Sturm", "er\u00b7regt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PPER", "ADV", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Will deine Sch\u00f6nheit nur auf mich die Strahlen", "tokens": ["Will", "dei\u00b7ne", "Sch\u00f6n\u00b7heit", "nur", "auf", "mich", "die", "Strah\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPOSAT", "NN", "ADV", "APPR", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.36": {"text": "werffen/", "tokens": ["werf\u00b7fen", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.37": {"text": "Vermeinst du/ da\u00df mein Arm dich itzt entbinden kan?", "tokens": ["Ver\u00b7meinst", "du", "/", "da\u00df", "mein", "Arm", "dich", "itzt", "ent\u00b7bin\u00b7den", "kan", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "KOUS", "PPOSAT", "NN", "PPER", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "So will ich heute noch Gewehr und Eisen sch\u00e4rffen/", "tokens": ["So", "will", "ich", "heu\u00b7te", "noch", "Ge\u00b7wehr", "und", "Ei\u00b7sen", "sch\u00e4rf\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADV", "NN", "KON", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Und greiffe dieses Werck mit vollen Kr\u00e4fften an.", "tokens": ["Und", "greif\u00b7fe", "die\u00b7ses", "Werck", "mit", "vol\u00b7len", "Kr\u00e4ff\u00b7ten", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PDAT", "NN", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Ein Blick ist mir genug/ kein Bitten ist vonn\u00f6then/", "tokens": ["Ein", "Blick", "ist", "mir", "ge\u00b7nug", "/", "kein", "Bit\u00b7ten", "ist", "von\u00b7n\u00f6\u00b7then", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "$(", "PIAT", "NN", "VAFIN", "ADV", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Mein Willen ist bereit/ mein Arm der ist ger\u00fcst/", "tokens": ["Mein", "Wil\u00b7len", "ist", "be\u00b7reit", "/", "mein", "Arm", "der", "ist", "ge\u00b7r\u00fcst", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$(", "PPOSAT", "NN", "ART", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Ich will mit solcher Lust dir deinen Alten t\u00f6dten/", "tokens": ["Ich", "will", "mit", "sol\u00b7cher", "Lust", "dir", "dei\u00b7nen", "Al\u00b7ten", "t\u00f6d\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PIAT", "NN", "PPER", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Als ich verwichner Zeit dich auf den Mund gek\u00fcst.", "tokens": ["Als", "ich", "ver\u00b7wich\u00b7ner", "Zeit", "dich", "auf", "den", "Mund", "ge\u00b7k\u00fcst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "PPER", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Die Fehler so er hat auf deiner Brust begangen/", "tokens": ["Die", "Feh\u00b7ler", "so", "er", "hat", "auf", "dei\u00b7ner", "Brust", "be\u00b7gan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PPER", "VAFIN", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Vertilget nichts so wohl als sein vergossen Bluth/", "tokens": ["Ver\u00b7til\u00b7get", "nichts", "so", "wohl", "als", "sein", "ver\u00b7gos\u00b7sen", "Bluth", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "ADV", "KOKOM", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-