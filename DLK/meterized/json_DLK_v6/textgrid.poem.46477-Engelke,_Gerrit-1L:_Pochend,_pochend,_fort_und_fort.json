text": "Siebzehn Kreislauf-Stunden lang:", "tokens": ["Sieb\u00b7zehn", "Kreis\u00b7lauf\u00b7Stun\u00b7den", "lang", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Pocht und treibt die Herz-Turbine. \u2013", "tokens": ["Pocht", "und", "treibt", "die", "Her\u00b7zTur\u00b7bi\u00b7ne", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "KON", "VVFIN", "ART", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Dann stellt die Alles-Hand", "tokens": ["Dann", "stellt", "die", "Al\u00b7les\u00b7Hand"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die Saug- und Speimaschine,", "tokens": ["Die", "Saug", "und", "Spei\u00b7ma\u00b7schi\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "TRUNC", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Den Hebelschaft", "tokens": ["Den", "He\u00b7bel\u00b7schaft"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Auf zehntel Kraft.", "tokens": ["Auf", "zehn\u00b7tel", "Kraft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Es ruht das Kraftgewelle eine Nacht.", "tokens": ["Es", "ruht", "das", "Kraft\u00b7ge\u00b7wel\u00b7le", "ei\u00b7ne", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Doch fr\u00fch beim Sechs-Uhr-Morgen-Pfiff", "tokens": ["Doch", "fr\u00fch", "beim", "Sechs\u00b7Uhr\u00b7Mor\u00b7gen\u0