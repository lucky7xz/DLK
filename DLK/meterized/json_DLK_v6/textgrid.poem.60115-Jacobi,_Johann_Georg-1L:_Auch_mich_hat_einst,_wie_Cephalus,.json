{"textgrid.poem.60115": {"metadata": {"author": {"name": "Jacobi, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Auch mich hat einst, wie Cephalus,", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auch mich hat einst, wie Cephalus,", "tokens": ["Auch", "mich", "hat", "einst", ",", "wie", "Ce\u00b7pha\u00b7lus", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "ADV", "$,", "PWAV", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aurora! deines Mundes Ku\u00df", "tokens": ["Au\u00b7ro\u00b7ra", "!", "dei\u00b7nes", "Mun\u00b7des", "Ku\u00df"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$.", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Geweckt aus jugendlichen Tr\u00e4umen;", "tokens": ["Ge\u00b7weckt", "aus", "ju\u00b7gend\u00b7li\u00b7chen", "Tr\u00e4u\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auch mich hat einst, wenn an begl\u00e4nzten B\u00e4umen", "tokens": ["Auch", "mich", "hat", "einst", ",", "wenn", "an", "be\u00b7gl\u00e4nz\u00b7ten", "B\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VAFIN", "ADV", "$,", "KOUS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Das frische Blatt des Fr\u00fchlings Hauch versp\u00fcrt,", "tokens": ["Das", "fri\u00b7sche", "Blatt", "des", "Fr\u00fch\u00b7lings", "Hauch", "ver\u00b7sp\u00fcrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Dein G\u00f6tterarm hinweggef\u00fchrt.", "tokens": ["Dein", "G\u00f6t\u00b7ter\u00b7arm", "hin\u00b7weg\u00b7ge\u00b7f\u00fchrt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da schwebt ich \u00fcber gr\u00fcnen H\u00f6hen,", "tokens": ["Da", "schwebt", "ich", "\u00fc\u00b7ber", "gr\u00fc\u00b7nen", "H\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Da flammten unter mir die Seen;", "tokens": ["Da", "flamm\u00b7ten", "un\u00b7ter", "mir", "die", "Seen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Zu Balsam ward ein jeder Tropfen Thau;", "tokens": ["Zu", "Bal\u00b7sam", "ward", "ein", "je\u00b7der", "Trop\u00b7fen", "Thau", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VAFIN", "ART", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Es stieg von blumenreicher Au", "tokens": ["Es", "stieg", "von", "blu\u00b7men\u00b7rei\u00b7cher", "Au"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Ein s\u00fc\u00dfer Weihrauch; V\u00f6gel sangen", "tokens": ["Ein", "s\u00fc\u00b7\u00dfer", "Weih\u00b7rauch", ";", "V\u00f6\u00b7gel", "san\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Von Liebe nur; und alle Sph\u00e4ren klangen", "tokens": ["Von", "Lie\u00b7be", "nur", ";", "und", "al\u00b7le", "Sph\u00e4\u00b7ren", "klan\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ADV", "$.", "KON", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Von Erdengl\u00fcck und Menschenseligkeit.", "tokens": ["Von", "Er\u00b7den\u00b7gl\u00fcck", "und", "Men\u00b7schen\u00b7se\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wohin, wohin die goldne Zeit?", "tokens": ["Wo\u00b7hin", ",", "wo\u00b7hin", "die", "gold\u00b7ne", "Zeit", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "PWAV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo blieb der Ku\u00df von deinem Nektarmunde,", "tokens": ["Wo", "blieb", "der", "Ku\u00df", "von", "dei\u00b7nem", "Nek\u00b7tar\u00b7mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bey welchem mich, in froher Sch\u00e4ferstunde,", "tokens": ["Bey", "wel\u00b7chem", "mich", ",", "in", "fro\u00b7her", "Sch\u00e4\u00b7fer\u00b7stun\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Begeisterung, wie Morgenluft, umflo\u00df,", "tokens": ["Be\u00b7geis\u00b7te\u00b7rung", ",", "wie", "Mor\u00b7gen\u00b7luft", ",", "um\u00b7flo\u00df", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "PWAV", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Unsterblichkeit mich fest an ihren Busen schlo\u00df?", "tokens": ["U\u00b7nsterb\u00b7lich\u00b7keit", "mich", "fest", "an", "ih\u00b7ren", "Bu\u00b7sen", "schlo\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.6": {"text": "Verblichen ist an deinem Wagen", "tokens": ["Ver\u00b7bli\u00b7chen", "ist", "an", "dei\u00b7nem", "Wa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Der Purpur mir; es endet sich in Klagen", "tokens": ["Der", "Pur\u00b7pur", "mir", ";", "es", "en\u00b7det", "sich", "in", "Kla\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "$.", "PPER", "VVFIN", "PRF", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Des Waldes laute Melodie;", "tokens": ["Des", "Wal\u00b7des", "lau\u00b7te", "Me\u00b7lo\u00b7die", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und ausgezaubert hat f\u00fcr mich die Pantasie.", "tokens": ["Und", "aus\u00b7ge\u00b7zau\u00b7bert", "hat", "f\u00fcr", "mich", "die", "Pan\u00b7ta\u00b7sie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "VAFIN", "APPR", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Aurora! wenn in bessern Tagen", "tokens": ["Au\u00b7ro\u00b7ra", "!", "wenn", "in", "bes\u00b7sern", "Ta\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$.", "KOUS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dir sorgenlos mein Herz entgegen schlug,", "tokens": ["Dir", "sor\u00b7gen\u00b7los", "mein", "Herz", "ent\u00b7ge\u00b7gen", "schlug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PPOSAT", "NN", "APPO", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wenn in dein Rosenlicht ich meine Leyer trug,", "tokens": ["Wenn", "in", "dein", "Ro\u00b7sen\u00b7licht", "ich", "mei\u00b7ne", "Le\u00b7yer", "trug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPOSAT", "NN", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So la\u00df, umwallt von diesen Bl\u00fcthenhainen,", "tokens": ["So", "la\u00df", ",", "um\u00b7wallt", "von", "die\u00b7sen", "Bl\u00fc\u00b7then\u00b7hai\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,", "VVFIN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Mit deinem Strahl die Weisheit mir erscheinen,", "tokens": ["Mit", "dei\u00b7nem", "Strahl", "die", "Weis\u00b7heit", "mir", "er\u00b7schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die nicht, als Zauberinn, empor den J\u00fcngling hebt,", "tokens": ["Die", "nicht", ",", "als", "Zau\u00b7be\u00b7rinn", ",", "em\u00b7por", "den", "J\u00fcng\u00b7ling", "hebt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "$,", "KOUS", "NN", "$,", "VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Als Freundinn aber, treu, mit ihm auf Erden lebt,", "tokens": ["Als", "Freun\u00b7dinn", "a\u00b7ber", ",", "treu", ",", "mit", "ihm", "auf", "Er\u00b7den", "lebt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "$,", "ADJD", "$,", "APPR", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Aus Klarheit uns in Klarheit leitet,", "tokens": ["Aus", "Klar\u00b7heit", "uns", "in", "Klar\u00b7heit", "lei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Und nach und nach zum Himmel vorbereitet!", "tokens": ["Und", "nach", "und", "nach", "zum", "Him\u00b7mel", "vor\u00b7be\u00b7rei\u00b7tet", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "KON", "APPR", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Sie tr\u00f6ste mich, wenn Lieb' und May", "tokens": ["Sie", "tr\u00f6s\u00b7te", "mich", ",", "wenn", "Lieb'", "und", "May"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Verstummen, alle Feerey", "tokens": ["Ver\u00b7stum\u00b7men", ",", "al\u00b7le", "Fee\u00b7rey"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Der Hoffnung flieht, die Jugendtr\u00e4ume schwinden,", "tokens": ["Der", "Hoff\u00b7nung", "flieht", ",", "die", "Ju\u00b7gend\u00b7tr\u00e4u\u00b7me", "schwin\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Und ach! um Gr\u00e4ber nur sich Veilchenkr\u00e4nze winden.", "tokens": ["Und", "ach", "!", "um", "Gr\u00e4\u00b7ber", "nur", "sich", "Veil\u00b7chen\u00b7kr\u00e4n\u00b7ze", "win\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$.", "APPR", "NN", "ADV", "PRF", "NE", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}