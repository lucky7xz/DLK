{"textgrid.poem.48798": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "23. An die Nacht, als er bei ihr wachete", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie aber eilst du so, du meiner Schmerzen Rast?", "tokens": ["Wie", "a\u00b7ber", "eilst", "du", "so", ",", "du", "mei\u00b7ner", "Schmer\u00b7zen", "Rast", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "PPER", "ADV", "$,", "PPER", "PPOSAT", "NN", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Deucht michs doch, da\u00df ich kaum auf eine Viertelstunde", "tokens": ["Deucht", "michs", "doch", ",", "da\u00df", "ich", "kaum", "auf", "ei\u00b7ne", "Vier\u00b7tel\u00b7stun\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PIS", "ADV", "$,", "KOUS", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "allhier gesessen bin bei diesem Rosenmunde,", "tokens": ["all\u00b7hier", "ge\u00b7ses\u00b7sen", "bin", "bei", "die\u00b7sem", "Ro\u00b7sen\u00b7mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VAFIN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "der meinen machet bla\u00df; so merk' ich, da\u00df du fast", "tokens": ["der", "mei\u00b7nen", "ma\u00b7chet", "bla\u00df", ";", "so", "merk'", "ich", ",", "da\u00df", "du", "fast"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PPOSAT", "VVFIN", "ADJD", "$.", "ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "dich an die H\u00e4lfte schon von uns entzogen hast.", "tokens": ["dich", "an", "die", "H\u00e4lf\u00b7te", "schon", "von", "uns", "ent\u00b7zo\u00b7gen", "hast", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "ADV", "APPR", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kehr um und halte Fu\u00df und gib uns Zeit zum Bunde,", "tokens": ["Kehr", "um", "und", "hal\u00b7te", "Fu\u00df", "und", "gib", "uns", "Zeit", "zum", "Bun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "KON", "VVFIN", "NN", "KON", "VVIMP", "PPER", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "den wir hier richten auf von ganzem Herzengrunde,", "tokens": ["den", "wir", "hier", "rich\u00b7ten", "auf", "von", "gan\u00b7zem", "Her\u00b7zen\u00b7grun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVFIN", "APPR", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "kehr' um und sei bei uns ein nicht so kurzer Gast.", "tokens": ["kehr'", "um", "und", "sei", "bei", "uns", "ein", "nicht", "so", "kur\u00b7zer", "Gast", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "KON", "VAFIN", "APPR", "PPER", "ART", "PTKNEG", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Dein Sohn, der sanfte Schlaf, schleicht durch das stille Haus", "tokens": ["Dein", "Sohn", ",", "der", "sanf\u00b7te", "Schlaf", ",", "schleicht", "durch", "das", "stil\u00b7le", "Haus"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "ART", "ADJA", "NN", "$,", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "und streut die leise Saat der Tr\u00e4ume h\u00e4ufig aus,", "tokens": ["und", "streut", "die", "lei\u00b7se", "Saat", "der", "Tr\u00e4u\u00b7me", "h\u00e4u\u00b7fig", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "darmit du l\u00e4nger kanst bei unsrer Lust verweilen.", "tokens": ["dar\u00b7mit", "du", "l\u00e4n\u00b7ger", "kanst", "bei", "uns\u00b7rer", "Lust", "ver\u00b7wei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Verh\u00fcll' uns in ein Tuch, bis da\u00df das dunkle Licht", "tokens": ["Ver\u00b7h\u00fcll'", "uns", "in", "ein", "Tuch", ",", "bis", "da\u00df", "das", "dunk\u00b7le", "Licht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPR", "ART", "NN", "$,", "KON", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "des halben Morgens dir durch deine Kleider bricht,", "tokens": ["des", "hal\u00b7ben", "Mor\u00b7gens", "dir", "durch", "dei\u00b7ne", "Klei\u00b7der", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "denn ist es Zeit, da\u00df wir mit dir von hinnen eilen.", "tokens": ["denn", "ist", "es", "Zeit", ",", "da\u00df", "wir", "mit", "dir", "von", "hin\u00b7nen", "ei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "NN", "$,", "KOUS", "PPER", "APPR", "PPER", "APPR", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}