{"textgrid.poem.39385": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Zwei Weisen. Von Frau B*", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Blumen, ihr seid stille Zeichen,", "tokens": ["Blu\u00b7men", ",", "ihr", "seid", "stil\u00b7le", "Zei\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die aus gr\u00fcnem Boden sprie\u00dfen,", "tokens": ["Die", "aus", "gr\u00fc\u00b7nem", "Bo\u00b7den", "sprie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "D\u00fcfte in die L\u00fcfte gie\u00dfen,", "tokens": ["D\u00fcf\u00b7te", "in", "die", "L\u00fcf\u00b7te", "gie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "So das Herz zur Lieb' erweichen.", "tokens": ["So", "das", "Herz", "zur", "Lieb'", "er\u00b7wei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Dennoch m\u00f6gt ihr nicht erreichen", "tokens": ["Den\u00b7noch", "m\u00f6gt", "ihr", "nicht", "er\u00b7rei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PTKNEG", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "So das Herz, den Schmerz vers\u00f6hnen,", "tokens": ["So", "das", "Herz", ",", "den", "Schmerz", "ver\u00b7s\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Enden alles Leid und St\u00f6hnen,", "tokens": ["En\u00b7den", "al\u00b7les", "Leid", "und", "St\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Da\u00df ihr k\u00f6nntet als Gedanken", "tokens": ["Da\u00df", "ihr", "k\u00f6nn\u00b7tet", "als", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VMFIN", "KOUS", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "In den gr\u00fcnen Bl\u00e4ttern schwanken:", "tokens": ["In", "den", "gr\u00fc\u00b7nen", "Bl\u00e4t\u00b7tern", "schwan\u00b7ken", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wollt' ich meine Liebe sprechen,", "tokens": ["Wollt'", "ich", "mei\u00b7ne", "Lie\u00b7be", "spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ach! als Botin meiner Klagen", "tokens": ["Ach", "!", "als", "Bo\u00b7tin", "mei\u00b7ner", "Kla\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "KOUS", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sollte meine Hand nicht wagen", "tokens": ["Soll\u00b7te", "mei\u00b7ne", "Hand", "nicht", "wa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPOSAT", "NN", "PTKNEG", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bunte Blumen abzubrechen.", "tokens": ["Bun\u00b7te", "Blu\u00b7men", "ab\u00b7zu\u00b7bre\u00b7chen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVIZU", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Still la\u00df' ich die Dornen stechen,", "tokens": ["Still", "la\u00df'", "ich", "die", "Dor\u00b7nen", "ste\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wag' die s\u00fc\u00dfen Schmerzen gern,", "tokens": ["Wag'", "die", "s\u00fc\u00b7\u00dfen", "Schmer\u00b7zen", "gern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Denn mir scheint kein g\u00fcnst'ger Stern,", "tokens": ["Denn", "mir", "scheint", "kein", "g\u00fcnst'\u00b7ger", "Stern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Drum will ich nicht Worte hauchen,", "tokens": ["Drum", "will", "ich", "nicht", "Wor\u00b7te", "hau\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "PTKNEG", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Mag auch nicht Gedanken brauchen,", "tokens": ["Mag", "auch", "nicht", "Ge\u00b7dan\u00b7ken", "brau\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PTKNEG", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Blumen, Worte und Gedanken,", "tokens": ["Blu\u00b7men", ",", "Wor\u00b7te", "und", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Manche Sehnsucht m\u00f6gt ihr stillen,", "tokens": ["Man\u00b7che", "Sehn\u00b7sucht", "m\u00f6gt", "ihr", "stil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Manchen holden Wunsch erf\u00fcllen,", "tokens": ["Man\u00b7chen", "hol\u00b7den", "Wunsch", "er\u00b7f\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Manches Herz mag wohl euch danken.", "tokens": ["Man\u00b7ches", "Herz", "mag", "wohl", "euch", "dan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "ADV", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Tr\u00e4ume, s\u00fc\u00df, wie mich umwanken,", "tokens": ["Tr\u00e4u\u00b7me", ",", "s\u00fc\u00df", ",", "wie", "mich", "um\u00b7wan\u00b7ken", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$,", "PWAV", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Denen bleibt ihr ewig fern;", "tokens": ["De\u00b7nen", "bleibt", "ihr", "e\u00b7wig", "fern", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Sie regiert ein andrer Stern.", "tokens": ["Sie", "re\u00b7giert", "ein", "an\u00b7drer", "Stern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Selbst der Purpurglanz der Rosen", "tokens": ["Selbst", "der", "Pur\u00b7pur\u00b7glanz", "der", "Ro\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Ist zu matt der Liebe: kosen", "tokens": ["Ist", "zu", "matt", "der", "Lie\u00b7be", ":", "ko\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["VAFIN", "PTKA", "ADJD", "ART", "NN", "$.", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "H\u00e4tt' ich zarte Melodien,", "tokens": ["H\u00e4tt'", "ich", "zar\u00b7te", "Me\u00b7lo\u00b7dien", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sie als Boten wegzusenden,", "tokens": ["Sie", "als", "Bo\u00b7ten", "weg\u00b7zu\u00b7sen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "KOUS", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "W\u00fcrde bald mein Leid sich enden,", "tokens": ["W\u00fcr\u00b7de", "bald", "mein", "Leid", "sich", "en\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und mir alle Freude bl\u00fchn.", "tokens": ["Und", "mir", "al\u00b7le", "Freu\u00b7de", "bl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PIAT", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Holde Liebe zu mir ziehn", "tokens": ["Hol\u00b7de", "Lie\u00b7be", "zu", "mir", "ziehn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPR", "PPER", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "W\u00fcrd' ich dann mit s\u00fc\u00dfen T\u00f6nen,", "tokens": ["W\u00fcrd'", "ich", "dann", "mit", "s\u00fc\u00b7\u00dfen", "T\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Meinen Bund auf ewig kr\u00f6nen:", "tokens": ["Mei\u00b7nen", "Bund", "auf", "e\u00b7wig", "kr\u00f6\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Denn mit himmlischen Ges\u00e4ngen", "tokens": ["Denn", "mit", "himm\u00b7li\u00b7schen", "Ge\u00b7s\u00e4n\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Kann Musik in goldnen Kl\u00e4ngen", "tokens": ["Kann", "Mu\u00b7sik", "in", "gold\u00b7nen", "Kl\u00e4n\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}