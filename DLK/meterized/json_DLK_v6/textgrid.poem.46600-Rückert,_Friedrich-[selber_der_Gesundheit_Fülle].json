{"textgrid.poem.46600": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[selber der Gesundheit F\u00fclle]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Selber der Gesundheit F\u00fclle", "tokens": ["Sel\u00b7ber", "der", "Ge\u00b7sund\u00b7heit", "F\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dient dir jetzt nur zum Verderben;", "tokens": ["Dient", "dir", "jetzt", "nur", "zum", "Ver\u00b7der\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schwerer sprengt der Geist die H\u00fclle,", "tokens": ["Schwe\u00b7rer", "sprengt", "der", "Geist", "die", "H\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und wir seh'n dich langsam sterben.", "tokens": ["Und", "wir", "seh'n", "dich", "lang\u00b7sam", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Nicht als wie ein Licht verlischt", "tokens": ["Nicht", "als", "wie", "ein", "Licht", "ver\u00b7lischt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "KOUS", "KOKOM", "ART", "NN", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "In des Windzugs Hauche,", "tokens": ["In", "des", "Wind\u00b7zugs", "Hau\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wie ein Feuer dampft und zischt", "tokens": ["Wie", "ein", "Feu\u00b7er", "dampft", "und", "zischt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VVFIN", "KON", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und erstickt im Rauche.", "tokens": ["Und", "er\u00b7stickt", "im", "Rau\u00b7che", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}