{"textgrid.poem.44712": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Volk", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seis denn geb\u00fcckt, mu\u00df man sich b\u00fccken,", "tokens": ["Seis", "denn", "ge\u00b7b\u00fcckt", ",", "mu\u00df", "man", "sich", "b\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "$,", "VMFIN", "PIS", "PRF", "VVFIN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Am Ende schaut mans doch mit Lust,", "tokens": ["Am", "En\u00b7de", "schaut", "mans", "doch", "mit", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PIS", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich schlage gl\u00e4ubig meine Brust,", "tokens": ["Ich", "schla\u00b7ge", "gl\u00e4u\u00b7big", "mei\u00b7ne", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sonst schl\u00e4gt ein andrer mir den R\u00fccken.", "tokens": ["Sonst", "schl\u00e4gt", "ein", "an\u00b7drer", "mir", "den", "R\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}