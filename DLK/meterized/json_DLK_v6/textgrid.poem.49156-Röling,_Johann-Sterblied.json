{"textgrid.poem.49156": {"metadata": {"author": {"name": "R\u00f6ling, Johann", "birth": "N.A.", "death": "N.A."}, "title": "Sterblied", "genre": "verse", "period": "N.A.", "pub_year": 1656, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wol dem, der sich der Welt entschl\u00e4gt,", "tokens": ["Wol", "dem", ",", "der", "sich", "der", "Welt", "ent\u00b7schl\u00e4gt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "$,", "PRELS", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Stets seinen Gott im Herzen tr\u00e4gt", "tokens": ["Stets", "sei\u00b7nen", "Gott", "im", "Her\u00b7zen", "tr\u00e4gt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und immer auf den Himmel siehet,", "tokens": ["Und", "im\u00b7mer", "auf", "den", "Him\u00b7mel", "sie\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nichts denket, als die Ewigkeit,", "tokens": ["Nichts", "den\u00b7ket", ",", "als", "die", "E\u00b7wig\u00b7keit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "KOUS", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nichts sorget, als wie er bereit", "tokens": ["Nichts", "sor\u00b7get", ",", "als", "wie", "er", "be\u00b7reit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "$,", "KOUS", "PWAV", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mag sein, wenn er von hinnen ziehet,", "tokens": ["Mag", "sein", ",", "wenn", "er", "von", "hin\u00b7nen", "zie\u00b7het", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VAINF", "$,", "KOUS", "PPER", "APPR", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und so ohn' Schein und in der That", "tokens": ["Und", "so", "ohn'", "Schein", "und", "in", "der", "That"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "NN", "KON", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Den Namen des Gerechten hat.", "tokens": ["Den", "Na\u00b7men", "des", "Ge\u00b7rech\u00b7ten", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der lebt erst dann recht, wenn er stirbt,", "tokens": ["Der", "lebt", "erst", "dann", "recht", ",", "wenn", "er", "stirbt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADV", "ADJD", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und gr\u00fcnt hervor, wenn er verdirbt.", "tokens": ["Und", "gr\u00fcnt", "her\u00b7vor", ",", "wenn", "er", "ver\u00b7dirbt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hat er viel M\u00fch' und Noth erlitten,", "tokens": ["Hat", "er", "viel", "M\u00fch'", "und", "Noth", "er\u00b7lit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PIAT", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Angst und Pein die Zeit verbracht", "tokens": ["Mit", "Angst", "und", "Pein", "die", "Zeit", "ver\u00b7bracht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "ART", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und mit der H\u00f6ll' und S\u00fcndenmacht", "tokens": ["Und", "mit", "der", "H\u00f6ll'", "und", "S\u00fcn\u00b7den\u00b7macht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bis an die Seele stets gestritten,", "tokens": ["Bis", "an", "die", "See\u00b7le", "stets", "ge\u00b7strit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "So ist der H\u00f6chste dort und hier", "tokens": ["So", "ist", "der", "H\u00f6chs\u00b7te", "dort", "und", "hier"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "KON", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ihm ein sehr gro\u00dfer Lohn daf\u00fcr.", "tokens": ["Ihm", "ein", "sehr", "gro\u00b7\u00dfer", "Lohn", "da\u00b7f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADV", "ADJA", "NN", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der sorget f\u00fcr ihn fr\u00fch und spat,", "tokens": ["Der", "sor\u00b7get", "f\u00fcr", "ihn", "fr\u00fch", "und", "spat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PPER", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erh\u00e4lt ihn auf der Unschuld Pfad", "tokens": ["Er\u00b7h\u00e4lt", "ihn", "auf", "der", "Un\u00b7schuld", "Pfad"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und f\u00fchrt ihn fr\u00f6hlich von der Erden", "tokens": ["Und", "f\u00fchrt", "ihn", "fr\u00f6h\u00b7lich", "von", "der", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hinauf, vor sich und seinen Thron,", "tokens": ["Hin\u00b7auf", ",", "vor", "sich", "und", "sei\u00b7nen", "Thron", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "PRF", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da, wo ihm eine sch\u00f6ne Kron'", "tokens": ["Da", ",", "wo", "ihm", "ei\u00b7ne", "sch\u00f6\u00b7ne", "Kron'"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und \u00fcberherrlichs Reich mu\u00df werden,", "tokens": ["Und", "\u00fc\u00b7berh\u00b7err\u00b7lichs", "Reich", "mu\u00df", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VMFIN", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Drin er mit unbeschriebnen Schein", "tokens": ["Drin", "er", "mit", "un\u00b7be\u00b7schrieb\u00b7nen", "Schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wird ewig Herr und K\u00f6nig sein.", "tokens": ["Wird", "e\u00b7wig", "Herr", "und", "K\u00f6\u00b7nig", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "NN", "KON", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ist sonst was gro\u00df, dies \u00fcbersehr,", "tokens": ["Ist", "sonst", "was", "gro\u00df", ",", "dies", "\u00fc\u00b7ber\u00b7sehr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PWS", "ADJD", "$,", "PDS", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist sonst was reich, dies weit viel mehr,", "tokens": ["Ist", "sonst", "was", "reich", ",", "dies", "weit", "viel", "mehr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PWS", "ADJD", "$,", "PDS", "ADJD", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ist sonst was s\u00fc\u00df, dies ist das S\u00fc\u00dfte,", "tokens": ["Ist", "sonst", "was", "s\u00fc\u00df", ",", "dies", "ist", "das", "S\u00fc\u00df\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PWS", "ADJD", "$,", "PDS", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ist pr\u00e4chtig was, dem gleicht es nicht,", "tokens": ["Ist", "pr\u00e4ch\u00b7tig", "was", ",", "dem", "gleicht", "es", "nicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "PWS", "$,", "PRELS", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ist sch\u00f6n was, hier ist g\u00f6ttlichs Licht,", "tokens": ["Ist", "sch\u00f6n", "was", ",", "hier", "ist", "g\u00f6tt\u00b7lichs", "Licht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "PWS", "$,", "ADV", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ist stark was, dies ist das Gewi\u00dfte,", "tokens": ["Ist", "stark", "was", ",", "dies", "ist", "das", "Ge\u00b7wi\u00df\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "PWS", "$,", "PDS", "VAFIN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Ja aller Welt Gut, Ehr' und Gl\u00fcck", "tokens": ["Ja", "al\u00b7ler", "Welt", "Gut", ",", "Ehr'", "und", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "PIAT", "NN", "ADJD", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Gleicht nichtes dem geringsten St\u00fcck.", "tokens": ["Gleicht", "nich\u00b7tes", "dem", "ge\u00b7rings\u00b7ten", "St\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "O Wonne, die kein Ohr geh\u00f6rt,", "tokens": ["O", "Won\u00b7ne", ",", "die", "kein", "Ohr", "ge\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O Freude, die kein Mund gelehrt,", "tokens": ["O", "Freu\u00b7de", ",", "die", "kein", "Mund", "ge\u00b7lehrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PRELS", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O Wollust, die kein Herz vernommen,", "tokens": ["O", "Wol\u00b7lust", ",", "die", "kein", "Herz", "ver\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PRELS", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie seelig ist, der sie erw\u00e4gt", "tokens": ["Wie", "see\u00b7lig", "ist", ",", "der", "sie", "er\u00b7w\u00e4gt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VAFIN", "$,", "PRELS", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und immer in den Augen tr\u00e4gt,", "tokens": ["Und", "im\u00b7mer", "in", "den", "Au\u00b7gen", "tr\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Noch seel'ger, der sie soll bekommen;", "tokens": ["Noch", "seel'\u00b7ger", ",", "der", "sie", "soll", "be\u00b7kom\u00b7men", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "PRELS", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Der seeligst' ist, der sie schon itzt", "tokens": ["Der", "see\u00b7ligst'", "ist", ",", "der", "sie", "schon", "itzt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "$,", "PRELS", "PPER", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ohn' ferner Sorg' und Kreuz besitzt.", "tokens": ["Ohn'", "fer\u00b7ner", "Sor\u00b7g'", "und", "Kreuz", "be\u00b7sitzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "VVPP", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Gieb, liebster Sch\u00f6pfer, o mein Licht,", "tokens": ["Gieb", ",", "liebs\u00b7ter", "Sch\u00f6p\u00b7fer", ",", "o", "mein", "Licht", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "ADJA", "NN", "$,", "FM", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein ein'ger Trost und Zuversicht,", "tokens": ["Mein", "ein'\u00b7ger", "Trost", "und", "Zu\u00b7ver\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gieb, da\u00df auch ich hier mag so leben,", "tokens": ["Gieb", ",", "da\u00df", "auch", "ich", "hier", "mag", "so", "le\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "KOUS", "ADV", "PPER", "ADV", "VMFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df mir mein Tod nicht schaden mag", "tokens": ["Da\u00df", "mir", "mein", "Tod", "nicht", "scha\u00b7den", "mag"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "PTKNEG", "VVINF", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und da\u00df ich wachsam Nacht und Tag", "tokens": ["Und", "da\u00df", "ich", "wach\u00b7sam", "Nacht", "und", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADJD", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Durch dich nach dir stets m\u00f6ge streben.", "tokens": ["Durch", "dich", "nach", "dir", "stets", "m\u00f6\u00b7ge", "stre\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "APPR", "PPER", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Dein Lohn wird zwar in's K\u00fcnft'ge mein,", "tokens": ["Dein", "Lohn", "wird", "zwar", "in's", "K\u00fcnft'\u00b7ge", "mein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "APPRART", "NN", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Doch das Verdienst bleibt dein allein.", "tokens": ["Doch", "das", "Ver\u00b7dienst", "bleibt", "dein", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPOSAT", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}