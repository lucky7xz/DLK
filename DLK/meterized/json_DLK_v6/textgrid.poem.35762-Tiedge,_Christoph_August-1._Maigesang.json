{"textgrid.poem.35762": {"metadata": {"author": {"name": "Tiedge, Christoph August", "birth": "N.A.", "death": "N.A."}, "title": "1. Maigesang", "genre": "verse", "period": "N.A.", "pub_year": 1796, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Greis des Silberhaares,", "tokens": ["Der", "Greis", "des", "Sil\u00b7ber\u00b7haa\u00b7res", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Winter, sank ins Grab;", "tokens": ["Der", "Win\u00b7ter", ",", "sank", "ins", "Grab", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der J\u00fcnglingstraum des Jahres,", "tokens": ["Der", "J\u00fcng\u00b7lings\u00b7traum", "des", "Jah\u00b7res", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Fr\u00fchling, l\u00f6st ihn ab.", "tokens": ["Der", "Fr\u00fch\u00b7ling", ",", "l\u00f6st", "ihn", "ab", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er zieht, von Melodien", "tokens": ["Er", "zieht", ",", "von", "Me\u00b7lo\u00b7dien"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "APPR", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Der jungen Freud' umhallt,", "tokens": ["Der", "jun\u00b7gen", "Freud'", "um\u00b7hallt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "In goldnen Phantasien", "tokens": ["In", "gold\u00b7nen", "Phan\u00b7ta\u00b7si\u00b7en"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Durch den bekr\u00e4nzten Wald.", "tokens": ["Durch", "den", "be\u00b7kr\u00e4nz\u00b7ten", "Wald", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es fl\u00fcstern leise Weste", "tokens": ["Es", "fl\u00fcs\u00b7tern", "lei\u00b7se", "Wes\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit jedem Halm der Flur", "tokens": ["Mit", "je\u00b7dem", "Halm", "der", "Flur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vom gro\u00dfen Liebesfeste", "tokens": ["Vom", "gro\u00b7\u00dfen", "Lie\u00b7bes\u00b7fes\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der br\u00e4utlichen Natur.", "tokens": ["Der", "br\u00e4ut\u00b7li\u00b7chen", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie wird den Lenz umfangen \u2013", "tokens": ["Sie", "wird", "den", "Lenz", "um\u00b7fan\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "O diese Wonne bricht", "tokens": ["O", "die\u00b7se", "Won\u00b7ne", "bricht"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "PDAT", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Hervor auf ihren Wangen,", "tokens": ["Her\u00b7vor", "auf", "ih\u00b7ren", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wie heitres Morgenlicht.", "tokens": ["Wie", "heit\u00b7res", "Mor\u00b7gen\u00b7licht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Zum Tanz begeistern Laute", "tokens": ["Zum", "Tanz", "be\u00b7geis\u00b7tern", "Lau\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Seligkeit den Bach;", "tokens": ["Der", "Se\u00b7lig\u00b7keit", "den", "Bach", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Im Moos, im kleinsten Kraute", "tokens": ["Im", "Moos", ",", "im", "kleins\u00b7ten", "Krau\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wird stiller Jubel wach.", "tokens": ["Wird", "stil\u00b7ler", "Ju\u00b7bel", "wach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "O f\u00fchlt, was in den Quellen", "tokens": ["O", "f\u00fchlt", ",", "was", "in", "den", "Quel\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Nach Finkenschl\u00e4gen tanzt,", "tokens": ["Nach", "Fin\u00b7ken\u00b7schl\u00e4\u00b7gen", "tanzt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und auf geheime Stellen", "tokens": ["Und", "auf", "ge\u00b7hei\u00b7me", "Stel\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der Liebe Myrten pflanzt!", "tokens": ["Der", "Lie\u00b7be", "Myr\u00b7ten", "pflanzt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "O f\u00fchlet! f\u00fchlt die Freude,", "tokens": ["O", "f\u00fch\u00b7let", "!", "f\u00fchlt", "die", "Freu\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$.", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die jeden Strauch belebt,", "tokens": ["Die", "je\u00b7den", "Strauch", "be\u00b7lebt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und \u00fcber Feld und Heide", "tokens": ["Und", "\u00fc\u00b7ber", "Feld", "und", "Hei\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "KON", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit Lerchenjubel schwebt!", "tokens": ["Mit", "Ler\u00b7chen\u00b7ju\u00b7bel", "schwebt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie ist ein Kind der Liebe,", "tokens": ["Sie", "ist", "ein", "Kind", "der", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Liebe, welche tief", "tokens": ["Der", "Lie\u00b7be", ",", "wel\u00b7che", "tief"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Aus N\u00e4chten das Getriebe", "tokens": ["Aus", "N\u00e4ch\u00b7ten", "das", "Ge\u00b7trie\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der Morgensterne rief;", "tokens": ["Der", "Mor\u00b7gens\u00b7ter\u00b7ne", "rief", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Die seliges Frohlocken", "tokens": ["Die", "se\u00b7li\u00b7ges", "Froh\u00b7lo\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In stumme W\u00e4lder haucht,", "tokens": ["In", "stum\u00b7me", "W\u00e4l\u00b7der", "haucht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und Hyazinthenglocken", "tokens": ["Und", "Hy\u00b7a\u00b7zin\u00b7then\u00b7glo\u00b7cken"], "token_info": ["word", "word"], "pos": ["KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ins Blau des Himmels taucht.", "tokens": ["Ins", "Blau", "des", "Him\u00b7mels", "taucht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es t\u00f6ne laut: Willkommen!", "tokens": ["Es", "t\u00f6\u00b7ne", "laut", ":", "Will\u00b7kom\u00b7men", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$.", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "O Freud', um deinen Pfad.", "tokens": ["O", "Freud'", ",", "um", "dei\u00b7nen", "Pfad", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "KOUI", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Sei festlich aufgenommen,", "tokens": ["Sei", "fest\u00b7lich", "auf\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wo deine Gottheit naht!", "tokens": ["Wo", "dei\u00b7ne", "Got\u00b7theit", "naht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Still, jedes Rauschget\u00fcmmel,", "tokens": ["Still", ",", "je\u00b7des", "Rauschge\u00b7t\u00fcm\u00b7mel", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "PIAT", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Wohin dein Wandel tritt!", "tokens": ["Wo\u00b7hin", "dein", "Wan\u00b7del", "tritt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du bringst aus deinem Himmel", "tokens": ["Du", "bringst", "aus", "dei\u00b7nem", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den sanftern Himmel mit,", "tokens": ["Den", "sanf\u00b7tern", "Him\u00b7mel", "mit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Voll Unschuld, wie die Jugend,", "tokens": ["Voll", "Un\u00b7schuld", ",", "wie", "die", "Ju\u00b7gend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die du in T\u00e4nzen \u00fcbst.", "tokens": ["Die", "du", "in", "T\u00e4n\u00b7zen", "\u00fcbst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wir brauchen wenig Tugend,", "tokens": ["Wir", "brau\u00b7chen", "we\u00b7nig", "Tu\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wenn du uns Unschuld giebst!", "tokens": ["Wenn", "du", "uns", "Un\u00b7schuld", "giebst", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}