{"textgrid.poem.31899": {"metadata": {"author": {"name": "Angelus Silesius", "birth": "N.A.", "death": "N.A."}, "title": "132. Gottes Eigenschaft", "genre": "verse", "period": "N.A.", "pub_year": 1650, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was ist Gotts Eigenschaft? Sich ins Gesch\u00f6pf ergie\u00dfen,", "tokens": ["Was", "ist", "Gotts", "Ei\u00b7gen\u00b7schaft", "?", "Sich", "ins", "Ge\u00b7sch\u00f6pf", "er\u00b7gie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "NE", "NN", "$.", "PRF", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Allzeit derselbe sein; nichts haben, wollen, wissen.", "tokens": ["All\u00b7zeit", "der\u00b7sel\u00b7be", "sein", ";", "nichts", "ha\u00b7ben", ",", "wol\u00b7len", ",", "wis\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "PDAT", "VAINF", "$.", "PIS", "VAFIN", "$,", "VMFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}