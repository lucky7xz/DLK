{"textgrid.poem.40605": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Durch das Schneefeld schie\u00dft mein Schlitten", "tokens": ["Durch", "das", "Schnee\u00b7feld", "schie\u00dft", "mein", "Schlit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Im Geschmeid des Tigerfells,", "tokens": ["Im", "Ge\u00b7schmeid", "des", "Ti\u00b7ger\u00b7fells", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Raschen Flugs vor\u00fcber glitten", "tokens": ["Ra\u00b7schen", "Flugs", "vor\u00b7\u00fc\u00b7ber", "glit\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADV", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Burg und Weiler, Busch und Fels.", "tokens": ["Burg", "und", "Wei\u00b7ler", ",", "Busch", "und", "Fels", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Lenz in Blumen, Herbst in Reben,", "tokens": ["Lenz", "in", "Blu\u00b7men", ",", "Herbst", "in", "Re\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sommer du im Garbenkranz,", "tokens": ["Som\u00b7mer", "du", "im", "Gar\u00b7ben\u00b7kranz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was ist eure Sch\u00f6nheit neben", "tokens": ["Was", "ist", "eu\u00b7re", "Sch\u00f6n\u00b7heit", "ne\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "PPOSAT", "NN", "APPR"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Einem Wintertag in Glanz!", "tokens": ["Ei\u00b7nem", "Win\u00b7ter\u00b7tag", "in", "Glanz", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie versinkt die bunte Kleinheit", "tokens": ["Wie", "ver\u00b7sinkt", "die", "bun\u00b7te", "Klein\u00b7heit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Vor so schlicht erhabner Pracht!", "tokens": ["Vor", "so", "schlicht", "er\u00b7hab\u00b7ner", "Pracht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Er verm\u00e4hlt das Wei\u00df der Reinheit", "tokens": ["Er", "ver\u00b7m\u00e4hlt", "das", "Wei\u00df", "der", "Rein\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit dem Hermelin der Macht.", "tokens": ["Mit", "dem", "Her\u00b7me\u00b7lin", "der", "Macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Seine Lagerzelte gl\u00e4nzen,", "tokens": ["Sei\u00b7ne", "La\u00b7ger\u00b7zel\u00b7te", "gl\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die Gebirge, weit im Kreis;", "tokens": ["Die", "Ge\u00b7bir\u00b7ge", ",", "weit", "im", "Kreis", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bis an seines Reiches Grenzen", "tokens": ["Bis", "an", "sei\u00b7nes", "Rei\u00b7ches", "Gren\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schimmert nur die\u00df stolze Wei\u00df.", "tokens": ["Schim\u00b7mert", "nur", "die\u00df", "stol\u00b7ze", "Wei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PDS", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wald und Strauch in Silberflocken,", "tokens": ["Wald", "und", "Strauch", "in", "Sil\u00b7ber\u00b7flo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Welch ein Hofstaat reich und steif!", "tokens": ["Welch", "ein", "Hof\u00b7staat", "reich", "und", "steif", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Wei\u00dfe Schleier auf den Locken", "tokens": ["Wei\u00b7\u00dfe", "Schlei\u00b7er", "auf", "den", "Lo\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und im Haar des Puders Reif;", "tokens": ["Und", "im", "Haar", "des", "Pu\u00b7ders", "Reif", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ART", "NN", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.6": {"line.1": {"text": "Zarte Fl\u00f6re, krause Spitzen", "tokens": ["Zar\u00b7te", "Fl\u00f6\u00b7re", ",", "krau\u00b7se", "Spit\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schm\u00fccken zierlich das Gewand,", "tokens": ["Schm\u00fc\u00b7cken", "zier\u00b7lich", "das", "Ge\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Spangen flimmern, Nadeln blitzen,", "tokens": ["Span\u00b7gen", "flim\u00b7mern", ",", "Na\u00b7deln", "blit\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Funkelnd spr\u00fcht der Diamant.", "tokens": ["Fun\u00b7kelnd", "spr\u00fcht", "der", "Di\u00b7a\u00b7mant", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Wintersonn' in eis'ger Klarheit", "tokens": ["Win\u00b7ter\u00b7sonn'", "in", "eis'\u00b7ger", "Klar\u00b7heit"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Streut aufs All ihr kaltes Licht,", "tokens": ["Streut", "aufs", "All", "ihr", "kal\u00b7tes", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Rein wie eine goldne Wahrheit,", "tokens": ["Rein", "wie", "ei\u00b7ne", "gold\u00b7ne", "Wahr\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gl\u00e4nzend zwar, doch w\u00e4rmend nicht.", "tokens": ["Gl\u00e4n\u00b7zend", "zwar", ",", "doch", "w\u00e4r\u00b7mend", "nicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "$,", "ADV", "VAFIN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Sorglich h\u00e4lt die Feuerbolzen", "tokens": ["Sorg\u00b7lich", "h\u00e4lt", "die", "Feu\u00b7er\u00b7bol\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Noch im K\u00f6cher sie bewacht,", "tokens": ["Noch", "im", "K\u00f6\u00b7cher", "sie", "be\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df nicht allzuschnell geschmolzen", "tokens": ["Da\u00df", "nicht", "all\u00b7zu\u00b7schnell", "ge\u00b7schmol\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Winters Herrlichkeit und Macht.", "tokens": ["Win\u00b7ters", "Herr\u00b7lich\u00b7keit", "und", "Macht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Sein Gesetz ist Ruhn und Schweigen,", "tokens": ["Sein", "Ge\u00b7setz", "ist", "Ruhn", "und", "Schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NE", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das er eisern strenge h\u00e4lt,", "tokens": ["Das", "er", "ei\u00b7sern", "stren\u00b7ge", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADJD", "ADJA", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und kein Vogel pfeift in Zweigen", "tokens": ["Und", "kein", "Vo\u00b7gel", "pfeift", "in", "Zwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und kein Pfl\u00fcger singt im Feld.", "tokens": ["Und", "kein", "Pfl\u00fc\u00b7ger", "singt", "im", "Feld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "In das M\u00fchlrad, das noch rollte,", "tokens": ["In", "das", "M\u00fchl\u00b7rad", ",", "das", "noch", "roll\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Greift er mit kristallner Hand,", "tokens": ["Greift", "er", "mit", "kris\u00b7tall\u00b7ner", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und den Bach, der murmeln wollte,", "tokens": ["Und", "den", "Bach", ",", "der", "mur\u00b7meln", "woll\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00e4lt im Fall er festgebannt.", "tokens": ["H\u00e4lt", "im", "Fall", "er", "fest\u00b7ge\u00b7bannt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Durch die feierliche Runde", "tokens": ["Durch", "die", "fei\u00b7er\u00b7li\u00b7che", "Run\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Geht ein Hauch von Majest\u00e4t,", "tokens": ["Geht", "ein", "Hauch", "von", "Ma\u00b7jes\u00b7t\u00e4t", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der das Lied verbannt vom Munde", "tokens": ["Der", "das", "Lied", "ver\u00b7bannt", "vom", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "VVPP", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ihn weiht zum Festgebet.", "tokens": ["Und", "ihn", "weiht", "zum", "Fest\u00b7ge\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Nur der Grund im Schlittengleise", "tokens": ["Nur", "der", "Grund", "im", "Schlit\u00b7ten\u00b7glei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "T\u00f6nt von lieblich leisem Klang,", "tokens": ["T\u00f6nt", "von", "lieb\u00b7lich", "lei\u00b7sem", "Klang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gleich als t\u00f6nte unterm Eise", "tokens": ["Gleich", "als", "t\u00f6n\u00b7te", "un\u00b7term", "Ei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der verbannten Blumen Sang.", "tokens": ["Der", "ver\u00b7bann\u00b7ten", "Blu\u00b7men", "Sang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Auch mein R\u00f6\u00dflein l\u00e4\u00dft nicht schweigen", "tokens": ["Auch", "mein", "R\u00f6\u00df\u00b7lein", "l\u00e4\u00dft", "nicht", "schwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "PTKNEG", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die Musik im Schellenkranz,", "tokens": ["Die", "Mu\u00b7sik", "im", "Schel\u00b7len\u00b7kranz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Stolzer tr\u00e4gt's sein Haupt zum Reigen,", "tokens": ["Stol\u00b7zer", "tr\u00e4gt's", "sein", "Haupt", "zum", "Rei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zierlich wirft's den Fu\u00df im Tanz.", "tokens": ["Zier\u00b7lich", "wirft's", "den", "Fu\u00df", "im", "Tanz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Und berauscht vom eignen Klingen", "tokens": ["Und", "be\u00b7rauscht", "vom", "eig\u00b7nen", "Klin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Saust's in Trunkenheit dahin,", "tokens": ["Saust's", "in", "Trun\u00b7ken\u00b7heit", "da\u00b7hin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "PAV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie am Kastagnettenschwingen", "tokens": ["Wie", "am", "Kas\u00b7tag\u00b7net\u00b7ten\u00b7schwin\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich entflammt die T\u00e4nzerin.", "tokens": ["Sich", "ent\u00b7flammt", "die", "T\u00e4n\u00b7ze\u00b7rin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Hier und dort wird von den T\u00f6nen", "tokens": ["Hier", "und", "dort", "wird", "von", "den", "T\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KON", "ADV", "VAFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ein entschlummert Echo wach;", "tokens": ["Ein", "ent\u00b7schlum\u00b7mert", "E\u00b7cho", "wach", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schl\u00e4frig, mit gutm\u00fcth'gem H\u00f6hnen", "tokens": ["Schl\u00e4f\u00b7rig", ",", "mit", "gut\u00