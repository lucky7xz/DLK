{"textgrid.poem.29138": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "53.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eine war von zwantzig Wochen schwanger, aber noch nicht Frau,", "tokens": ["Ei\u00b7ne", "war", "von", "zwant\u00b7zig", "Wo\u00b7chen", "schwan\u00b7ger", ",", "a\u00b7ber", "noch", "nicht", "Frau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "APPR", "CARD", "NN", "ADJD", "$,", "ADV", "ADV", "PTKNEG", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Gieng mit einem fromen Manne wie gebr\u00e4uchlich zu der Trau.", "tokens": ["Gieng", "mit", "ei\u00b7nem", "fro\u00b7men", "Man\u00b7ne", "wie", "ge\u00b7br\u00e4uch\u00b7lich", "zu", "der", "Trau", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "KOKOM", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Als er sie ein wenig hatte, merckt er, da\u00df sie ungesund,", "tokens": ["Als", "er", "sie", "ein", "we\u00b7nig", "hat\u00b7te", ",", "merckt", "er", ",", "da\u00df", "sie", "un\u00b7ge\u00b7sund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ART", "PIS", "VAFIN", "$,", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Weil er Schwulst an ihrem Leibe, vielmahls gar auch Beulen, fund;", "tokens": ["Weil", "er", "Schwulst", "an", "ih\u00b7rem", "Lei\u00b7be", ",", "viel\u00b7mahls", "gar", "auch", "Beu\u00b7len", ",", "fund", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "APPR", "PPOSAT", "NN", "$,", "ADV", "ADV", "ADV", "VVINF", "$,", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Klagte dr\u00fcber, fragte H\u00fclffe; letzlich ward es rau\u00df gebracht,", "tokens": ["Klag\u00b7te", "dr\u00fc\u00b7ber", ",", "frag\u00b7te", "H\u00fclf\u00b7fe", ";", "letz\u00b7lich", "ward", "es", "rau\u00df", "ge\u00b7bracht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "$,", "VVFIN", "NN", "$.", "ADV", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Da\u00df ihr solches b\u00f6se Leute hatten unversehns gemacht.", "tokens": ["Da\u00df", "ihr", "sol\u00b7ches", "b\u00f6\u00b7se", "Leu\u00b7te", "hat\u00b7ten", "un\u00b7ver\u00b7sehns", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "ADJA", "NN", "VAFIN", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}