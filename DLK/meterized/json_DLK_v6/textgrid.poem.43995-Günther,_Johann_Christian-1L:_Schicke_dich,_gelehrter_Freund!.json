{"textgrid.poem.43995": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Schicke dich, gelehrter Freund!", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schicke dich, gelehrter Freund!", "tokens": ["Schi\u00b7cke", "dich", ",", "ge\u00b7lehr\u00b7ter", "Freund", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Es erscheint", "tokens": ["Es", "er\u00b7scheint"], "token_info": ["word", "word"], "pos": ["PPER", "VVFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Des Geburthstags holder Morgen.", "tokens": ["Des", "Ge\u00b7burth\u00b7stags", "hol\u00b7der", "Mor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unterbrich der B\u00fccher Flei\u00df", "tokens": ["Un\u00b7ter\u00b7brich", "der", "B\u00fc\u00b7cher", "Flei\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und den Schwei\u00df", "tokens": ["Und", "den", "Schwei\u00df"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Aller klugen Sorgen!", "tokens": ["Al\u00b7ler", "klu\u00b7gen", "Sor\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Las des edlen Tages Schein", "tokens": ["Las", "des", "ed\u00b7len", "Ta\u00b7ges", "Schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Unser seyn!", "tokens": ["Un\u00b7ser", "seyn", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VAINF", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Las die freyen Jauchzer klingen!", "tokens": ["Las", "die", "frey\u00b7en", "Jauch\u00b7zer", "klin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Las des Bacchus Traubenblut", "tokens": ["Las", "des", "Bac\u00b7chus", "Trau\u00b7ben\u00b7blut"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ART", "NE", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie den Muth", "tokens": ["Wie", "den", "Muth"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "In dem Glase springen!", "tokens": ["In", "dem", "Gla\u00b7se", "sprin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Eben darum bind ich dich", "tokens": ["E\u00b7ben", "da\u00b7rum", "bind", "ich", "dich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PAV", "VAFIN", "PPER", "PRF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sicherlich", "tokens": ["Si\u00b7cher\u00b7lich"], "token_info": ["word"], "pos": ["PRF"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Mit den schlecht gestimmten Saythen,", "tokens": ["Mit", "den", "schlecht", "ge\u00b7stimm\u00b7ten", "Say\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df du dich durch eine Lust", "tokens": ["Da\u00df", "du", "dich", "durch", "ei\u00b7ne", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "L\u00f6sen must", "tokens": ["L\u00f6\u00b7sen", "must"], "token_info": ["word", "word"], "pos": ["NN", "VMFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Nach Manier der Zeiten.", "tokens": ["Nach", "Ma\u00b7nier", "der", "Zei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Unsre V\u00e4ter hielten auch", "tokens": ["Uns\u00b7re", "V\u00e4\u00b7ter", "hiel\u00b7ten", "auch"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Den Gebrauch,", "tokens": ["Den", "Ge\u00b7brauch", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Mit einander froh zu leben;", "tokens": ["Mit", "ein\u00b7an\u00b7der", "froh", "zu", "le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ADJD", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00e4tten wir als ihr Geschlecht", "tokens": ["H\u00e4t\u00b7ten", "wir", "als", "ihr", "Ge\u00b7schlecht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "KOUS", "PPOSAT", "NN"], "meter": "+--++-+", "measure": "dactylic.init"}, "line.5": {"text": "Wohl das Recht,", "tokens": ["Wohl", "das", "Recht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Solches aufzuheben?", "tokens": ["Sol\u00b7ches", "auf\u00b7zu\u00b7he\u00b7ben", "?"], "token_info": ["word", "word", "punct"], "pos": ["PIS", "VVIZU", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Nein, mein Bruder, auch dein Sinn", "tokens": ["Nein", ",", "mein", "Bru\u00b7der", ",", "auch", "dein", "Sinn"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPOSAT", "NN", "$,", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Geht dahin,", "tokens": ["Geht", "da\u00b7hin", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PAV", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Junger Jahre Lust zu schmecken;", "tokens": ["Jun\u00b7ger", "Jah\u00b7re", "Lust", "zu", "schme\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Du geh\u00f6rst nicht in die Schaar,", "tokens": ["Du", "ge\u00b7h\u00f6rst", "nicht", "in", "die", "Schaar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Die so gar", "tokens": ["Die", "so", "gar"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADV", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Immer Grillen hecken.", "tokens": ["Im\u00b7mer", "Gril\u00b7len", "he\u00b7cken", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Bacchus und der Venus Sohn", "tokens": ["Bac\u00b7chus", "und", "der", "Ve\u00b7nus", "Sohn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "KON", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Kennen schon", "tokens": ["Ken\u00b7nen", "schon"], "token_info": ["word", "word"], "pos": ["VVFIN", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Dein nicht murrisches Gem\u00fcthe,", "tokens": ["Dein", "nicht", "mur\u00b7ri\u00b7sches", "Ge\u00b7m\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "PTKNEG", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und durch ihren Zeitvertreib", "tokens": ["Und", "durch", "ih\u00b7ren", "Zeit\u00b7ver\u00b7treib"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "F\u00fchlt dein Leib", "tokens": ["F\u00fchlt", "dein", "Leib"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Feuriges Gebl\u00fcte.", "tokens": ["Feu\u00b7ri\u00b7ges", "Ge\u00b7bl\u00fc\u00b7te", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Manche liebe lange Nacht", "tokens": ["Man\u00b7che", "lie\u00b7be", "lan\u00b7ge", "Nacht"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ADV", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hat gewacht,", "tokens": ["Hat", "ge\u00b7wacht", ","], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Wenn wir auf dem Fa\u00dfe schliefen", "tokens": ["Wenn", "wir", "auf", "dem", "Fa\u00b7\u00dfe", "schlie\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Oder auch, nachdem es kam,", "tokens": ["O\u00b7der", "auch", ",", "nach\u00b7dem", "es", "kam", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "In den Kram", "tokens": ["In", "den", "Kram"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Artger M\u00e4gdgen liefen.", "tokens": ["Art\u00b7ger", "M\u00e4gd\u00b7gen", "lie\u00b7fen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Haben wir uns nicht zulezt", "tokens": ["Ha\u00b7ben", "wir", "uns", "nicht", "zu\u00b7lezt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PRF", "PTKNEG", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Erst erg\u00f6zt", "tokens": ["Erst", "er\u00b7g\u00f6zt"], "token_info": ["word", "word"], "pos": ["ADV", "VVFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Und den Tummel ausgela\u00dfen,", "tokens": ["Und", "den", "Tum\u00b7mel", "aus\u00b7ge\u00b7la\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Als wir neulich, weist du wo?", "tokens": ["Als", "wir", "neu\u00b7lich", ",", "weist", "du", "wo", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "$,", "VVFIN", "PPER", "PWAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Auf dem Stroh", "tokens": ["Auf", "dem", "Stroh"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Adams\u00e4pfel a\u00dfen?", "tokens": ["A\u00b7dam\u00b7s\u00e4p\u00b7fel", "a\u00b7\u00dfen", "?"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.9": {"line.1": {"text": "Dencke nur, wie wir zugleich", "tokens": ["Den\u00b7cke", "nur", ",", "wie", "wir", "zu\u00b7gleich"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADV", "$,", "PWAV", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Manchen Streich", "tokens": ["Man\u00b7chen", "Streich"], "token_info": ["word", "word"], "pos": ["PIAT", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Heimlich list- und lustig spielten", "tokens": ["Heim\u00b7lich", "list", "und", "lus\u00b7tig", "spiel\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "TRUNC", "KON", "ADJD", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und wie wir auf Amors H\u00f6h", "tokens": ["Und", "wie", "wir", "auf", "A\u00b7mors", "H\u00f6h"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "APPR", "NE", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "In dem Schnee", "tokens": ["In", "dem", "Schnee"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Warmer Br\u00fcste w\u00fchlten.", "tokens": ["War\u00b7mer", "Br\u00fcs\u00b7te", "w\u00fchl\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.10": {"line.1": {"text": "Doch Vergangnes hilft nicht mehr.", "tokens": ["Doch", "Ver\u00b7gang\u00b7nes", "hilft", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PTKNEG", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Gieb Geh\u00f6r!", "tokens": ["Gieb", "Ge\u00b7h\u00f6r", "!"], "token_info": ["word", "word", "punct"], "pos": ["VVIMP", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Heute geht es von dem Frischen,", "tokens": ["Heu\u00b7te", "geht", "es", "von", "dem", "Fri\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Heute soll sich Rauch und Tranck", "tokens": ["Heu\u00b7te", "soll", "sich", "Rauch", "und", "Tranck"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PRF", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und Gesang", "tokens": ["Und", "Ge\u00b7sang"], "token_info": ["word", "word"], "pos": ["KON", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "In der Gurgel mischen.", "tokens": ["In", "der", "Gur\u00b7gel", "mi\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.11": {"line.1": {"text": "Frey seyn ist der G\u00f6tter Art,", "tokens": ["Frey", "seyn", "ist", "der", "G\u00f6t\u00b7ter", "Art", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAINF", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Nichts gespart,", "tokens": ["Nichts", "ge\u00b7spart", ","], "token_info": ["word", "word", "punct"], "pos": ["PIS", "VVPP", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Was des Herzens Wuntsch begehret", "tokens": ["Was", "des", "Her\u00b7zens", "Wunt\u00b7sch", "be\u00b7ge\u00b7hret"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "ART", "NN", "NN", "VVFIN"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Und was die Gelegenheit", "tokens": ["Und", "was", "die", "Ge\u00b7le\u00b7gen\u00b7heit"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PWS", "ART", "NN"], "meter": "-+--++-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Jezt verleiht,", "tokens": ["Jezt", "ver\u00b7leiht", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "K\u00fcnftig nicht bescheeret.", "tokens": ["K\u00fcnf\u00b7tig", "nicht", "be\u00b7schee\u00b7ret", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.12": {"line.1": {"text": "Menschen haben, sind sie klug,", "tokens": ["Men\u00b7schen", "ha\u00b7ben", ",", "sind", "sie", "klug", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "VAFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ursach gnug,", "tokens": ["Ur\u00b7sach", "gnug", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADV", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Alle Stunden mitzunehmen", "tokens": ["Al\u00b7le", "Stun\u00b7den", "mit\u00b7zu\u00b7neh\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "NN", "VVIZU"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und, so lang es Rosen schneyt,", "tokens": ["Und", ",", "so", "lang", "es", "Ro\u00b7sen", "schneyt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADV", "ADJD", "PPER", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sich der Zeit", "tokens": ["Sich", "der", "Zeit"], "token_info": ["word", "word", "word"], "pos": ["PRF", "ART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Kl\u00fcglich zu bequemen.", "tokens": ["Kl\u00fcg\u00b7lich", "zu", "be\u00b7que\u00b7men", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PTKZU", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.13": {"line.1": {"text": "Jener Weise hat gewis", "tokens": ["Je\u00b7ner", "Wei\u00b7se", "hat", "ge\u00b7wis"], "token_info": ["word", "word", "word", "word"], "pos": ["PDAT", "NN", "VAFIN", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Auch den Ri\u00df", "tokens": ["Auch", "den", "Ri\u00df"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Unsers Ebenbilds getrofen;", "tokens": ["Un\u00b7sers", "E\u00b7ben\u00b7bilds", "ge\u00b7tro\u00b7fen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schwalben, rief er, baut das Nest,", "tokens": ["Schwal\u00b7ben", ",", "rief", "er", ",", "baut", "das", "Nest", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn man l\u00e4st", "tokens": ["Denn", "man", "l\u00e4st"], "token_info": ["word", "word", "word"], "pos": ["KON", "PIS", "VVFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Euch nicht ewig Sommer hofen!", "tokens": ["Euch", "nicht", "e\u00b7wig", "Som\u00b7mer", "ho\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "ADJD", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Pfeile, Str\u00f6me, Bliz und Wind", "tokens": ["Pfei\u00b7le", ",", "Str\u00f6\u00b7me", ",", "Bliz", "und", "Wind"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Fliehn geschwind,", "tokens": ["Fliehn", "ge\u00b7schwind", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Noch geschwinder unsre Jahre;", "tokens": ["Noch", "ge\u00b7schwin\u00b7der", "uns\u00b7re", "Jah\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nicht ein einzger Augenblick", "tokens": ["Nicht", "ein", "einz\u00b7ger", "Au\u00b7gen\u00b7blick"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKNEG", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Kehrt zur\u00fcck,", "tokens": ["Kehrt", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "T\u00e4glich droht die Baare.", "tokens": ["T\u00e4g\u00b7lich", "droht", "die", "Baa\u00b7re", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.15": {"line.1": {"text": "Bleibt es demnach festgestellt,", "tokens": ["Bleibt", "es", "dem\u00b7nach", "fest\u00b7ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PAV", "VVPP", "$,"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.2": {"text": "Auf der Welt", "tokens": ["Auf", "der", "Welt"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Minder Wirth als Gast zu hei\u00dfen,", "tokens": ["Min\u00b7der", "Wirth", "als", "Gast", "zu", "hei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KOUS", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ey, so last uns, weil es w\u00e4hrt,", "tokens": ["Ey", ",", "so", "last", "uns", ",", "weil", "es", "w\u00e4hrt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Eh man f\u00e4hrt,", "tokens": ["Eh", "man", "f\u00e4hrt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Unsrer Lust beflei\u00dfen.", "tokens": ["Uns\u00b7rer", "Lust", "be\u00b7flei\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.16": {"line.1": {"text": "Bruder, thu es auf mein Wort,", "tokens": ["Bru\u00b7der", ",", "thu", "es", "auf", "mein", "Wort", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Schicke fort,", "tokens": ["Schi\u00b7cke", "fort", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Las den Kellner scharf besprechen,", "tokens": ["Las", "den", "Kell\u00b7ner", "scharf", "be\u00b7spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da dein angebrochnes Fest", "tokens": ["Da", "dein", "an\u00b7ge\u00b7broch\u00b7nes", "Fest"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Melden l\u00e4st,", "tokens": ["Mel\u00b7den", "l\u00e4st", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Da\u00df wir tapfer zechen.", "tokens": ["Da\u00df", "wir", "tap\u00b7fer", "ze\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.17": {"line.1": {"text": "Lade G\u00e4ste, doch nicht viel,", "tokens": ["La\u00b7de", "G\u00e4s\u00b7te", ",", "doch", "nicht", "viel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "ADV", "PTKNEG", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Denn das Spiel", "tokens": ["Denn", "das", "Spiel"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Wird gemeiniglich verdorben;", "tokens": ["Wird", "ge\u00b7mei\u00b7nig\u00b7lich", "ver\u00b7dor\u00b7ben", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenig, die sich wohl verstehn,", "tokens": ["We\u00b7nig", ",", "die", "sich", "wohl", "ver\u00b7stehn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PRELS", "PRF", "ADV", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Gut begehn,", "tokens": ["Gut", "be\u00b7gehn", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Haben Ruhm erworben.", "tokens": ["Ha\u00b7ben", "Ruhm", "er\u00b7wor\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.18": {"line.1": {"text": "Knaster, Coffee, Wein und Bier,", "tokens": ["Knas\u00b7ter", ",", "Cof\u00b7fee", ",", "Wein", "und", "Bier", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Die wie wir", "tokens": ["Die", "wie", "wir"], "token_info": ["word", "word", "word"], "pos": ["ART", "PWAV", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Sich zusammen gut vertragen,", "tokens": ["Sich", "zu\u00b7sam\u00b7men", "gut", "ver\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sollen sich durch Kraft des Schalls", "tokens": ["Sol\u00b7len", "sich", "durch", "Kraft", "des", "Schalls"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PRF", "APPR", "NN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Durch den Hals", "tokens": ["Durch", "den", "Hals"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Mit einander jagen.", "tokens": ["Mit", "ein\u00b7an\u00b7der", "ja\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PRF", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.19": {"line.1": {"text": "Da\u00df man auch, was n\u00f6thig ist,", "tokens": ["Da\u00df", "man", "auch", ",", "was", "n\u00f6\u00b7thig", "ist", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "$,", "PRELS", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Nicht vergi\u00dft,", "tokens": ["Nicht", "ver\u00b7gi\u00dft", ","], "token_info": ["word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Las die Violinen holen!", "tokens": ["Las", "die", "Vi\u00b7o\u00b7li\u00b7nen", "ho\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Stimme sie wie unsern Sinn!", "tokens": ["Stim\u00b7me", "sie", "wie", "un\u00b7sern", "Sinn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "KOKOM", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mars, lauf hin", "tokens": ["Mars", ",", "lauf", "hin"], "token_info": ["word", "punct", "word", "word"], "pos": ["NE", "$,", "NN", "PTKVZ"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Und bereite Kohlen!", "tokens": ["Und", "be\u00b7rei\u00b7te", "Koh\u00b7len", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.20": {"line.1": {"text": "Trinck ein jeder, was er mag,", "tokens": ["Trinck", "ein", "je\u00b7der", ",", "was", "er", "mag", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "PIS", "$,", "PWS", "PPER", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Diesen Tag,", "tokens": ["Die\u00b7sen", "Tag", ","], "token_info": ["word", "word", "punct"], "pos": ["PDAT", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Trinckt zu meines Freundes Ehren!", "tokens": ["Trinckt", "zu", "mei\u00b7nes", "Freun\u00b7des", "Eh\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie so m\u00fc\u00dfig? Nicht gepast!", "tokens": ["Wie", "so", "m\u00fc\u00b7\u00dfig", "?", "Nicht", "ge\u00b7past", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "$.", "PTKNEG", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sauft und last", "tokens": ["Sauft", "und", "last"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "VVFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Tausend Vivat h\u00f6ren!", "tokens": ["Tau\u00b7send", "Vi\u00b7vat", "h\u00f6\u00b7ren", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.21": {"line.1": {"text": "Lehrt Herrn Pfeifers Wohlseyn aus,", "tokens": ["Lehrt", "Herrn", "Pfei\u00b7fers", "Wohl\u00b7seyn", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "NN", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Bis der Schmaus", "tokens": ["Bis", "der", "Schmaus"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Unsern K\u00f6pfen Schwindel gebe!", "tokens": ["Un\u00b7sern", "K\u00f6p\u00b7fen", "Schwin\u00b7del", "ge\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00dcberschreyt selbst das Geschrey,", "tokens": ["\u00dc\u00b7bersc\u00b7hreyt", "selbst", "das", "Ge\u00b7schrey", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART