rd", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.23": {"line.1": {"text": "Dieses w\u00fcntsch ich und darzu", "tokens": ["Die\u00b7ses", "w\u00fcnt\u00b7sch", "ich", "und", "dar\u00b7zu"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "KON", "PAV"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.2": {"text": "Alle Ruh", "tokens": ["Al\u00b7le", "Ruh"], "token_info": ["word", "word"], "pos": ["PIAT", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Von dem hoch- und heitern Gl\u00fccke,", "tokens": ["Von", "dem", "hoch", "und", "hei\u00b7tern", "Gl\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "TRUNC", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df sein Auge sonder Qual", "tokens": ["Da\u00df", "sein", "Au\u00b7ge", "son\u00b7der", "Qual"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Tausendmahl", "tokens": ["Tau\u00b7send\u00b7mahl"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Diesen Tag erblicke.", "tokens": ["Die\u00b7sen", "Tag", "er\u00b7bli\u00b7cke", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}