{"textgrid.poem.52887": {"metadata": {"author": {"name": "Dingelstedt, Franz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: In diesem Hause schl\u00e4ft ein Wicht,", "genre": "verse", "period": "N.A.", "pub_year": 1847, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In diesem Hause schl\u00e4ft ein Wicht,", "tokens": ["In", "die\u00b7sem", "Hau\u00b7se", "schl\u00e4ft", "ein", "Wicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df Gott sich sein erbarme,", "tokens": ["Da\u00df", "Gott", "sich", "sein", "er\u00b7bar\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PRF", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mit kreidewei\u00dfem Angesicht", "tokens": ["Mit", "krei\u00b7de\u00b7wei\u00b7\u00dfem", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und klapperd\u00fcrrem Arme.", "tokens": ["Und", "klap\u00b7per\u00b7d\u00fcr\u00b7rem", "Ar\u00b7me", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Er schl\u00e4ft? ... Er w\u00e4lzt auf seidnem Pf\u00fchl", "tokens": ["Er", "schl\u00e4ft", "?", "...", "Er", "w\u00e4lzt", "auf", "seid\u00b7nem", "Pf\u00fchl"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "$(", "PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Glieder mit Fluch und Gewimmer,", "tokens": ["Die", "Glie\u00b7der", "mit", "Fluch", "und", "Ge\u00b7wim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Ist's ihm zu hei\u00df, ist's ihm zu k\u00fchl,", "tokens": ["Ist's", "ihm", "zu", "hei\u00df", ",", "ist's", "ihm", "zu", "k\u00fchl", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PTKA", "ADJD", "$,", "VAFIN", "PPER", "PTKA", "ADJD", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Recht ist's dem Sch\u00e4cher nimmer.", "tokens": ["Recht", "ist's", "dem", "Sch\u00e4\u00b7cher", "nim\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und um ihn rauscht die Gardine schwer", "tokens": ["Und", "um", "ihn", "rauscht", "die", "Gar\u00b7di\u00b7ne", "schwer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUI", "PPER", "VVFIN", "ART", "NN", "ADJD"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Von goldenen Fransen und Falten,", "tokens": ["Von", "gol\u00b7de\u00b7nen", "Fran\u00b7sen", "und", "Fal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Der Nachttisch kann der Fl\u00e4schlein Heer", "tokens": ["Der", "Nacht\u00b7tisch", "kann", "der", "Fl\u00e4\u00b7schlein", "Heer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und der Tropfen Meer kaum halten.", "tokens": ["Und", "der", "Trop\u00b7fen", "Meer", "kaum", "hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "ADV", "VVINF", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}}, "stanza.4": {"line.1": {"text": "Warum er nicht schl\u00e4ft? Warum er in Wut", "tokens": ["Wa\u00b7rum", "er", "nicht", "schl\u00e4ft", "?", "Wa\u00b7rum", "er", "in", "Wut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PTKNEG", "VVFIN", "$.", "PWAV", "PPER", "APPR", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Spitzen am Hemde zerrissen?", "tokens": ["Die", "Spit\u00b7zen", "am", "Hem\u00b7de", "zer\u00b7ris\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Ein gutes Gewissen schl\u00e4ft \u00fcberall gut", "tokens": ["Ein", "gu\u00b7tes", "Ge\u00b7wis\u00b7sen", "schl\u00e4ft", "\u00fc\u00b7be\u00b7rall", "gut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "ADJD"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und nirgends ein b\u00f6ses Gewissen.", "tokens": ["Und", "nir\u00b7gends", "ein", "b\u00f6\u00b7ses", "Ge\u00b7wis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.5": {"line.1": {"text": "Er hat an des Landes Mark, die Schlang',", "tokens": ["Er", "hat", "an", "des", "Lan\u00b7des", "Mark", ",", "die", "Schlang'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "NN", "$,", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sich voll gefressen, gesogen,", "tokens": ["Sich", "voll", "ge\u00b7fres\u00b7sen", ",", "ge\u00b7so\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PRF", "ADJD", "VVPP", "$,", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Er hat \u2013 ein Menschenleben lang! \u2013", "tokens": ["Er", "hat", "\u2013", "ein", "Men\u00b7schen\u00b7le\u00b7ben", "lang", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "$(", "ART", "NN", "ADJD", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gestohlen, gelogen, betrogen.", "tokens": ["Ge\u00b7stoh\u00b7len", ",", "ge\u00b7lo\u00b7gen", ",", "be\u00b7tro\u00b7gen", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVPP", "$,", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.6": {"line.1": {"text": "Hei, Dir auf deinem Dunen-Bett,", "tokens": ["Hei", ",", "Dir", "auf", "dei\u00b7nem", "Du\u00b7nen\u00b7Bett", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Steinsarg deiner Pal\u00e4ste,", "tokens": ["Im", "Stein\u00b7sarg", "dei\u00b7ner", "Pa\u00b7l\u00e4s\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wenn ich itzt mein altes Horn noch h\u00e4tt',", "tokens": ["Wenn", "ich", "itzt", "mein", "al\u00b7tes", "Horn", "noch", "h\u00e4tt'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPOSAT", "ADJA", "NN", "ADV", "VAFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Dir br\u00e4cht' ich ein St\u00e4ndchen aufs Beste!", "tokens": ["Dir", "br\u00e4cht'", "ich", "ein", "St\u00e4nd\u00b7chen", "aufs", "Bes\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.7": {"line.1": {"text": "Du schrecktest wie vom Tarantelstich", "tokens": ["Du", "schreck\u00b7test", "wie", "vom", "Ta\u00b7ran\u00b7tel\u00b7stich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOKOM", "APPRART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Aus teuererkauftem Schlafe,", "tokens": ["Aus", "teu\u00b7e\u00b7rer\u00b7kauf\u00b7tem", "Schla\u00b7fe", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "W\u00e4hnend, die Posaune weckte dich", "tokens": ["W\u00e4h\u00b7nend", ",", "die", "Po\u00b7sau\u00b7ne", "weck\u00b7te", "dich"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "ART", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Und riefe zur endlichen Strafe!", "tokens": ["Und", "rie\u00b7fe", "zur", "end\u00b7li\u00b7chen", "Stra\u00b7fe", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}