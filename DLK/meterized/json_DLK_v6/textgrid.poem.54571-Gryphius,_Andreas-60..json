{"textgrid.poem.54571": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "60.", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Blume dieser Zeit! ist wohl ein Wunsch zu finden", "tokens": ["O", "Blu\u00b7me", "die\u00b7ser", "Zeit", "!", "ist", "wohl", "ein", "Wunsch", "zu", "fin\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "PDAT", "NN", "$.", "VAFIN", "ADV", "ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der recht bequem f\u00fcr euch/ indem die grosse Welt", "tokens": ["Der", "recht", "be\u00b7quem", "f\u00fcr", "euch", "/", "in\u00b7dem", "die", "gros\u00b7se", "Welt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADJD", "APPR", "PPER", "$(", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sich mit dem Jahr verneut/ und viel auf W\u00fcnschen h\u00e4lt/", "tokens": ["Sich", "mit", "dem", "Jahr", "ver\u00b7neut", "/", "und", "viel", "auf", "W\u00fcn\u00b7schen", "h\u00e4lt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "VVPP", "$(", "KON", "ADV", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und doch nichts w\u00fcnscht als was mit Zeit und Welt mu\u00df schwinden/", "tokens": ["Und", "doch", "nichts", "w\u00fcnscht", "als", "was", "mit", "Zeit", "und", "Welt", "mu\u00df", "schwin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIS", "VVFIN", "KOUS", "PIS", "APPR", "NN", "KON", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Eur ewig hoher Geist verlacht was uns wil binden", "tokens": ["Eur", "e\u00b7wig", "ho\u00b7her", "Geist", "ver\u00b7lacht", "was", "uns", "wil", "bin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJD", "ADJA", "NN", "VVPP", "PWS", "PPER", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und pocht die Eitelkeit die \u00fcber morgen f\u00e4llt/", "tokens": ["Und", "pocht", "die", "Ei\u00b7tel\u00b7keit", "die", "\u00fc\u00b7ber", "mor\u00b7gen", "f\u00e4llt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ART", "APPR", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Er h\u00f6hnt die rauhe Noth und lacht was nach uns stellt/", "tokens": ["Er", "h\u00f6hnt", "die", "rau\u00b7he", "Noth", "und", "lacht", "was", "nach", "uns", "stellt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "KON", "VVFIN", "PIS", "APPR", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und sucht nicht als durch Gott in Gott sich zu entz\u00fcnden.", "tokens": ["Und", "sucht", "nicht", "als", "durch", "Gott", "in", "Gott", "sich", "zu", "ent\u00b7z\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "KOKOM", "APPR", "NN", "APPR", "NN", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Gott/ den der in der Zeit euch \u00fcber Zeit bewehrt/", "tokens": ["Gott", "/", "den", "der", "in", "der", "Zeit", "euch", "\u00fc\u00b7ber", "Zeit", "be\u00b7wehrt", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "ART", "ART", "APPR", "ART", "NN", "PPER", "APPR", "NN", "VVPP", "$("], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.10": {"text": "Verg\u00f6nn euch/ die nunmehr das dritte Jahr beschwert/", "tokens": ["Ver\u00b7g\u00f6nn", "euch", "/", "die", "nun\u00b7mehr", "das", "drit\u00b7te", "Jahr", "be\u00b7schwert", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "ART", "ADV", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Mit diesem so viel Lusst/ als Drangsal euch betr\u00fcbet.", "tokens": ["Mit", "die\u00b7sem", "so", "viel", "Lusst", "/", "als", "Dran\u00b7gsal", "euch", "be\u00b7tr\u00fc\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADV", "PIAT", "NN", "$(", "KOUS", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Er g\u00f6nne mir/ da\u00df ich an eurem Beyspiel seh", "tokens": ["Er", "g\u00f6n\u00b7ne", "mir", "/", "da\u00df", "ich", "an", "eu\u00b7rem", "Bey\u00b7spiel", "seh"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$(", "KOUS", "PPER", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Da\u00df/ wenn er ferne scheint/ er gleichwol in der N\u00e4h/", "tokens": ["Da\u00df", "/", "wenn", "er", "fer\u00b7ne", "scheint", "/", "er", "gleich\u00b7wol", "in", "der", "N\u00e4h", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$(", "KOUS", "PPER", "ADV", "VVFIN", "$(", "PPER", "ADV", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Zu recht-und seiner Zeit auch k\u00fcsse die Er liebet.", "tokens": ["Zu", "recht\u00b7\u00b7und", "sei\u00b7ner", "Zeit", "auch", "k\u00fcs\u00b7se", "die", "Er", "lie\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "PPOSAT", "NN", "ADV", "VVFIN", "ART", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}