{"dta.poem.17842": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "85.  Man thut nicht was man Lobt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Man lobt den gutten Mann der ihm gen\u00fcgen l\u00e4st;", "tokens": ["Man", "lobt", "den", "gut\u00b7ten", "Mann", "der", "ihm", "ge\u00b7n\u00fc\u00b7gen", "l\u00e4st", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "ADJA", "NN", "ART", "PPER", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und frisset doch umb sich gleich wie der Krebs und Pest.", "tokens": ["Und", "fris\u00b7set", "doch", "umb", "sich", "gleich", "wie", "der", "Krebs", "und", "Pest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "PRF", "ADV", "KOKOM", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}