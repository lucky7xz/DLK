{"dta.poem.10869": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Erbauliche Betrachtung  \n der Blumen und Pflanzen.  \n Als  S. T.  Hr.  P. E.  mir eine Menge sch\u00f6ner  \n Tulpen zugeschickt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Unendliches, allgegenw\u00e4rtigs Seyn,", "tokens": ["Un\u00b7end\u00b7li\u00b7ches", ",", "all\u00b7ge\u00b7gen\u00b7w\u00e4r\u00b7tigs", "Seyn", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADJA", "$,", "ADV", "PPOSAT", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "La\u00df mich, nur blo\u00df vor dir, die Knie beugen!", "tokens": ["La\u00df", "mich", ",", "nur", "blo\u00df", "vor", "dir", ",", "die", "Knie", "beu\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "ADV", "ADV", "APPR", "PPER", "$,", "ART", "NN", "VVINF", "$."], "meter": "+--+-+-++-", "measure": "iambic.penta.invert"}, "line.3": {"text": "La\u00df mich, o Herr, zu dir allein,", "tokens": ["La\u00df", "mich", ",", "o", "Herr", ",", "zu", "dir", "al\u00b7lein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "FM", "NN", "$,", "APPR", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit meiner Seelen Kr\u00e4ften, steigen!", "tokens": ["Mit", "mei\u00b7ner", "See\u00b7len", "Kr\u00e4f\u00b7ten", ",", "stei\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Zu dir, durch dessen Wunder-Macht,", "tokens": ["Zu", "dir", ",", "durch", "des\u00b7sen", "Wun\u00b7der\u00b7Macht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "APPR", "PRELAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Himmel Heer entstand, die Welt hervorgebracht!", "tokens": ["Der", "Him\u00b7mel", "Heer", "ent\u00b7stand", ",", "die", "Welt", "her\u00b7vor\u00b7ge\u00b7bracht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$,", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der du der Pflanzen Reich so k\u00fcnstlich zugericht,", "tokens": ["Der", "du", "der", "Pflan\u00b7zen", "Reich", "so", "k\u00fcnst\u00b7lich", "zu\u00b7ge\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "NN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da\u00df sich der Fiebern Meng in solcher Ordnung flicht!", "tokens": ["Da\u00df", "sich", "der", "Fie\u00b7bern", "Meng", "in", "sol\u00b7cher", "Ord\u00b7nung", "flicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "NE", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Durch dessen Weisheit wir, in H\u00e4uten und in Rinden,", "tokens": ["Durch", "des\u00b7sen", "Weis\u00b7heit", "wir", ",", "in", "H\u00e4u\u00b7ten", "und", "in", "Rin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "PPER", "$,", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Sie kl\u00fcglich eingeschlossen finden!", "tokens": ["Sie", "kl\u00fcg\u00b7lich", "ein\u00b7ge\u00b7schlos\u00b7sen", "fin\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVPP", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Durch dessen weise Macht die Blumen, wenn sie bl\u00fchn,", "tokens": ["Durch", "des\u00b7sen", "wei\u00b7se", "Macht", "die", "Blu\u00b7men", ",", "wenn", "sie", "bl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "ADJA", "NN", "ART", "NN", "$,", "KOUS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Mit solcher zierlichen Figur,", "tokens": ["Mit", "sol\u00b7cher", "zier\u00b7li\u00b7chen", "Fi\u00b7gur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Sich lieblich bilden und formiren,", "tokens": ["Sich", "lieb\u00b7lich", "bil\u00b7den", "und", "for\u00b7mi\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "VVINF", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Und mit der Farben Schmuck nicht nur", "tokens": ["Und", "mit", "der", "Far\u00b7ben", "Schmuck", "nicht", "nur"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN", "PTKNEG", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Sich unbeschreiblich lieblich zieren;", "tokens": ["Sich", "un\u00b7be\u00b7schreib\u00b7lich", "lieb\u00b7lich", "zie\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Mit nicht zu z\u00e4hlenden Ver\u00e4ndrungen sich schm\u00fccken,", "tokens": ["Mit", "nicht", "zu", "z\u00e4h\u00b7len\u00b7den", "Ver\u00b7\u00e4n\u00b7drun\u00b7gen", "sich", "schm\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PTKNEG", "PTKZU", "ADJA", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-++--+-", "measure": "iambic.hexa.relaxed"}, "line.17": {"text": "Auf ungez\u00e4hlte Weis, uns unser Aug erquicken,", "tokens": ["Auf", "un\u00b7ge\u00b7z\u00e4hl\u00b7te", "Weis", ",", "uns", "un\u00b7ser", "Aug", "er\u00b7qui\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und um auf tausend Art den Geist uns zu erfrischen,", "tokens": ["Und", "um", "auf", "tau\u00b7send", "Art", "den", "Geist", "uns", "zu", "er\u00b7fri\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUI", "APPR", "CARD", "NN", "ART", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "So Farb als Zeichnungen in solcher Meng auch mischen,", "tokens": ["So", "Farb", "als", "Zeich\u00b7nun\u00b7gen", "in", "sol\u00b7cher", "Meng", "auch", "mi\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KOUS", "NN", "APPR", "PIAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Wie auch den Stiel und ihre Bl\u00e4tter ganz,", "tokens": ["Wie", "auch", "den", "Stiel", "und", "ih\u00b7re", "Bl\u00e4t\u00b7ter", "ganz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "KON", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Noch \u00fcber dem mit einem solchen Glanz,", "tokens": ["Noch", "\u00fc\u00b7ber", "dem", "mit", "ei\u00b7nem", "sol\u00b7chen", "Glanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "APPR", "ART", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Und Silber-Schimmer \u00fcberziehn,", "tokens": ["Und", "Sil\u00b7ber\u00b7Schim\u00b7mer", "\u00fc\u00b7ber\u00b7ziehn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Da\u00df kein Sineser-Lack an ihren Schmelz nicht reichet,", "tokens": ["Da\u00df", "kein", "Si\u00b7ne\u00b7ser\u00b7Lack", "an", "ih\u00b7ren", "Schmelz", "nicht", "rei\u00b7chet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "APPR", "PPOSAT", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.24": {"text": "Kein Firni\u00df ihrer Gl\u00e4tte gleichet.", "tokens": ["Kein", "Fir\u00b7ni\u00df", "ih\u00b7rer", "Gl\u00e4t\u00b7te", "glei\u00b7chet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Der du mit gr\u00fcnem Laub sie kleidest und sie schm\u00fcckest,", "tokens": ["Der", "du", "mit", "gr\u00fc\u00b7nem", "Laub", "sie", "klei\u00b7dest", "und", "sie", "schm\u00fc\u00b7ckest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN", "PPER", "VVFIN", "KON", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Der du die d\u00fcnne Luft in ihre R\u00f6hren dr\u00fcckest,", "tokens": ["Der", "du", "die", "d\u00fcn\u00b7ne", "Luft", "in", "ih\u00b7re", "R\u00f6h\u00b7ren", "dr\u00fc\u00b7ckest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Mit Regen und mit Thau sie n\u00e4hrest und sie tr\u00e4nkest,", "tokens": ["Mit", "Re\u00b7gen", "und", "mit", "Thau", "sie", "n\u00e4h\u00b7rest", "und", "sie", "tr\u00e4n\u00b7kest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "PPER", "VVFIN", "KON", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und uns, in ihrem Bau, so Lust, als Nahrung, schenkest.", "tokens": ["Und", "uns", ",", "in", "ih\u00b7rem", "Bau", ",", "so", "Lust", ",", "als", "Nah\u00b7rung", ",", "schen\u00b7kest", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "$,", "APPR", "PPOSAT", "NN", "$,", "ADV", "NN", "$,", "KOUS", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Durch dich empfangen sie die wunderbare Kraft,", "tokens": ["Durch", "dich", "emp\u00b7fan\u00b7gen", "sie", "die", "wun\u00b7der\u00b7ba\u00b7re", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Da\u00df sie, was ihnen dient, den Saft,", "tokens": ["Da\u00df", "sie", ",", "was", "ih\u00b7nen", "dient", ",", "den", "Saft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PWS", "PPER", "VVFIN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Durch so geschlungner R\u00f6hren Klumpen,", "tokens": ["Durch", "so", "ge\u00b7schlung\u00b7ner", "R\u00f6h\u00b7ren", "Klum\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.32": {"text": "Aus Elementen gleichsam pumpen,", "tokens": ["Aus", "E\u00b7le\u00b7men\u00b7ten", "gleich\u00b7sam", "pum\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.33": {"text": "Aus Wasser, Luft und Erde saugen.", "tokens": ["Aus", "Was\u00b7ser", ",", "Luft", "und", "Er\u00b7de", "sau\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "Durch deine m\u00e4chtige Befehle,", "tokens": ["Durch", "dei\u00b7ne", "m\u00e4ch\u00b7ti\u00b7ge", "Be\u00b7feh\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.35": {"text": "Erweckt des Fr\u00fchlings Licht der Luft und Erden Seele,", "tokens": ["Er\u00b7weckt", "des", "Fr\u00fch\u00b7lings", "Licht", "der", "Luft", "und", "Er\u00b7den", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "ART", "NN", "KON", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Die starren S\u00e4fte, da\u00df sie g\u00e4hren,", "tokens": ["Die", "star\u00b7ren", "S\u00e4f\u00b7te", ",", "da\u00df", "sie", "g\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.37": {"text": "Und alles, was die Erde ziert,", "tokens": ["Und", "al\u00b7les", ",", "was", "die", "Er\u00b7de", "ziert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Und uns so Nutz als Lust gebiert,", "tokens": ["Und", "uns", "so", "Nutz", "als", "Lust", "ge\u00b7biert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "NN", "KOUS", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Laub, Blumen, Kraut und Fr\u00fcchte n\u00e4hren.", "tokens": ["Laub", ",", "Blu\u00b7men", ",", "Kraut", "und", "Fr\u00fcch\u00b7te", "n\u00e4h\u00b7ren", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.40": {"text": "Es r\u00fchmt denn mein erquickt Gem\u00fcthe,", "tokens": ["Es", "r\u00fchmt", "denn", "mein", "er\u00b7quickt", "Ge\u00b7m\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "PPOSAT", "VVPP", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.41": {"text": "In der bebl\u00fchmten Erden Pracht,", "tokens": ["In", "der", "be\u00b7bl\u00fchm\u00b7ten", "Er\u00b7den", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Die Weisheit und die Wundermacht,", "tokens": ["Die", "Weis\u00b7heit", "und", "die", "Wun\u00b7der\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.43": {"text": "Sammt deiner v\u00e4terlichen G\u00fcte,", "tokens": ["Sammt", "dei\u00b7ner", "v\u00e4\u00b7ter\u00b7li\u00b7chen", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.44": {"text": "Herr! der du sie hervorgebracht.", "tokens": ["Herr", "!", "der", "du", "sie", "her\u00b7vor\u00b7ge\u00b7bracht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "PPER", "PPER", "VVPP", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}}}}