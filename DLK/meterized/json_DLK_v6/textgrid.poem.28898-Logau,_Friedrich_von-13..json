{"textgrid.poem.28898": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "13.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eine Witfrau gieng zur Trau, nam ietzund den vierdten Mann.", "tokens": ["Ei\u00b7ne", "Wit\u00b7frau", "gieng", "zur", "Trau", ",", "nam", "ie\u00b7tzund", "den", "vierd\u00b7ten", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Als die Zeit zum schlafen gehn auch nun endlich kam heran,", "tokens": ["Als", "die", "Zeit", "zum", "schla\u00b7fen", "gehn", "auch", "nun", "end\u00b7lich", "kam", "he\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPRART", "VVINF", "VVFIN", "ADV", "ADV", "ADV", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Ach! sprach sie, ach! ach! h\u00e4tt ich vor an dieses Ding gedacht,", "tokens": ["Ach", "!", "sprach", "sie", ",", "ach", "!", "ach", "!", "h\u00e4tt", "ich", "vor", "an", "die\u00b7ses", "Ding", "ge\u00b7dacht", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "PPER", "$,", "ITJ", "$.", "XY", "$.", "VAFIN", "PPER", "APPR", "APPR", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Niemand, niemand h\u00e4tt es mir nimmermehr mehr eingebracht!", "tokens": ["Nie\u00b7mand", ",", "nie\u00b7mand", "h\u00e4tt", "es", "mir", "nim\u00b7mer\u00b7mehr", "mehr", "ein\u00b7ge\u00b7bracht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PIS", "VAFIN", "PPER", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "Doch sie gieng, war gar getrost, und das Kind, das sie gebar", "tokens": ["Doch", "sie", "gieng", ",", "war", "gar", "ge\u00b7trost", ",", "und", "das", "Kind", ",", "das", "sie", "ge\u00b7bar"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$,", "VAFIN", "ADV", "VVPP", "$,", "KON", "ART", "NN", "$,", "PRELS", "PPER", "ADJD"], "meter": "--+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Kaum in zwantzig Wochen drauff, wie\u00df, wie sie verge\u00dflich war.", "tokens": ["Kaum", "in", "zwant\u00b7zig", "Wo\u00b7chen", "drauff", ",", "wie\u00df", ",", "wie", "sie", "ver\u00b7ge\u00df\u00b7lich", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "CARD", "NN", "PTKVZ", "$,", "VVFIN", "$,", "PWAV", "PPER", "ADJD", "VAFIN", "$."], "meter": "--+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}}}}}