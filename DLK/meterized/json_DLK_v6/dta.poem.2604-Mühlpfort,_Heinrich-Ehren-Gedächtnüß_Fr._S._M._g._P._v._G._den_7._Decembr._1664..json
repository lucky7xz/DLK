{"dta.poem.2604": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Ehren-Ged\u00e4chtn\u00fc\u00df  \n  Fr. S. M. g. P. v. G. den 7. Decembr.  \n 1664.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die letzte Stunde darf uns ja kein Schrecken machen/", "tokens": ["Die", "letz\u00b7te", "Stun\u00b7de", "darf", "uns", "ja", "kein", "Schre\u00b7cken", "ma\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PPER", "ADV", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil sie der Ewigkeit bestimmt Geburts-Tag ist.", "tokens": ["Weil", "sie", "der", "E\u00b7wig\u00b7keit", "be\u00b7stimmt", "Ge\u00b7burts\u00b7Tag", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wir schlaffen zwar im Grab/ doch da\u00df wir wieder wach\u1ebd/", "tokens": ["Wir", "schlaf\u00b7fen", "zwar", "im", "Grab", "/", "doch", "da\u00df", "wir", "wie\u00b7der", "wach\u1ebd", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN", "$(", "ADV", "KOUS", "PPER", "ADV", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wir leben/ ob gleich hier den Leib die F\u00e4ulni\u00df frist.", "tokens": ["Wir", "le\u00b7ben", "/", "ob", "gleich", "hier", "den", "Leib", "die", "F\u00e4ul\u00b7ni\u00df", "frist", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "ADV", "ADV", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Das minste Theil verdirbt/ der Seele G\u00f6ttlich Wesen", "tokens": ["Das", "mins\u00b7te", "Theil", "ver\u00b7dirbt", "/", "der", "See\u00b7le", "G\u00f6tt\u00b7lich", "We\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$(", "ART", "NN", "ADJD", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Kennt keinen Zufall nicht/ noch grimmer Zeiten Sturm.", "tokens": ["Kennt", "kei\u00b7nen", "Zu\u00b7fall", "nicht", "/", "noch", "grim\u00b7mer", "Zei\u00b7ten", "Sturm", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "PTKNEG", "$(", "ADV", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Was irrdisch ist und heist/ mag nicht vom Tod genesen/", "tokens": ["Was", "irr\u00b7disch", "ist", "und", "heist", "/", "mag", "nicht", "vom", "Tod", "ge\u00b7ne\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "KON", "VVFIN", "$(", "VMFIN", "PTKNEG", "APPRART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Man g\u00f6nne doch das Fleisch/ das S\u00fcnden-Nest/ dem Wurm.", "tokens": ["Man", "g\u00f6n\u00b7ne", "doch", "das", "Fleisch", "/", "das", "S\u00fcn\u00b7den\u00b7Nest", "/", "dem", "Wurm", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "ART", "NN", "$(", "ART", "NN", "$(", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ein Tagel\u00f6hner w\u00fcnscht/ der Abend wolle kommen/", "tokens": ["Ein", "Ta\u00b7ge\u00b7l\u00f6h\u00b7ner", "w\u00fcnscht", "/", "der", "A\u00b7bend", "wol\u00b7le", "kom\u00b7men", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$(", "ART", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und seiner Arbeit Last bes\u00e4nfftigen mit Ruh:", "tokens": ["Und", "sei\u00b7ner", "Ar\u00b7beit", "Last", "be\u00b7s\u00e4nff\u00b7ti\u00b7gen", "mit", "Ruh", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NE", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Begehren wir denn nicht/ da\u00df wir der Noth entnommen", "tokens": ["Be\u00b7geh\u00b7ren", "wir", "denn", "nicht", "/", "da\u00df", "wir", "der", "Noth", "ent\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ADV", "PTKNEG", "$(", "KOUS", "PPER", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Als wie in einem Schlaff die Augen dr\u00fccken zu?", "tokens": ["Als", "wie", "in", "ei\u00b7nem", "Schlaff", "die", "Au\u00b7gen", "dr\u00fc\u00b7cken", "zu", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Der Wandersman legt ab die B\u00fcrde die ihn kr\u00e4ncket/", "tokens": ["Der", "Wan\u00b7ders\u00b7man", "legt", "ab", "die", "B\u00fcr\u00b7de", "die", "ihn", "kr\u00e4n\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "ART", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und siehet hoch erfreut der schweren Reise Ziel;", "tokens": ["Und", "sie\u00b7het", "hoch", "er\u00b7freut", "der", "schwe\u00b7ren", "Rei\u00b7se", "Ziel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "VVFIN", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Versichert da\u00df die Seel\u2019 so hier der Leib \u00fcmschrencket/", "tokens": ["Ver\u00b7si\u00b7chert", "da\u00df", "die", "Seel'", "so", "hier", "der", "Leib", "\u00fcm\u00b7schren\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KOUS", "ART", "NN", "ADV", "ADV", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Wenn schon ihr Sch\u00f6pffer rufft/ nicht l\u00e4nger rasten wil.", "tokens": ["Wenn", "schon", "ihr", "Sch\u00f6pf\u00b7fer", "rufft", "/", "nicht", "l\u00e4n\u00b7ger", "ras\u00b7ten", "wil", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "VVFIN", "$(", "PTKNEG", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ein Z\u00e4rtling dieser Welt der mag di\u00df Leben ehren/", "tokens": ["Ein", "Z\u00e4rt\u00b7ling", "die\u00b7ser", "Welt", "der", "mag", "di\u00df", "Le\u00b7ben", "eh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "NN", "ART", "VMFIN", "PDS", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und als ein G\u00f6tzen-Knecht ihm Opffer z\u00fcnden an:", "tokens": ["Und", "als", "ein", "G\u00f6t\u00b7zen\u00b7Knecht", "ihm", "Opf\u00b7fer", "z\u00fcn\u00b7den", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PPER", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Uns soll viel mehr das Grab/ den Weg zum Himmel lehren/", "tokens": ["Uns", "soll", "viel", "mehr", "das", "Grab", "/", "den", "Weg", "zum", "Him\u00b7mel", "leh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "ART", "NN", "$(", "ART", "NN", "APPRART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Wenn jenes Tempel sind in lichten Brand gethan.", "tokens": ["Wenn", "je\u00b7nes", "Tem\u00b7pel", "sind", "in", "lich\u00b7ten", "Brand", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "VAFIN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Mecenas der in Perl und Purpur hat gesessen/", "tokens": ["Me\u00b7ce\u00b7nas", "der", "in", "Perl", "und", "Pur\u00b7pur", "hat", "ge\u00b7ses\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "APPR", "NE", "KON", "NN", "VAFIN", "VVPP", "$("], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.22": {"text": "Und des Augustus Hertz und Heimligkeit beschlo\u00df/", "tokens": ["Und", "des", "Au\u00b7gus\u00b7tus", "Hertz", "und", "Heim\u00b7lig\u00b7keit", "be\u00b7schlo\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Hat seiner Hoheit selbst zum schimpflichsten vergessen/", "tokens": ["Hat", "sei\u00b7ner", "Ho\u00b7heit", "selbst", "zum", "schimpf\u00b7lichs\u00b7ten", "ver\u00b7ges\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "APPRART", "ADJA", "VVPP", "$("], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}, "line.24": {"text": "Als er die Thr\u00e4nen umb des Lebens Frist vergo\u00df;", "tokens": ["Als", "er", "die", "Thr\u00e4\u00b7nen", "umb", "des", "Le\u00b7bens", "Frist", "ver\u00b7go\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Und bettelt als ein Knecht geraume Zeit zum sterben", "tokens": ["Und", "bet\u00b7telt", "als", "ein", "Knecht", "ge\u00b7rau\u00b7me", "Zeit", "zum", "ster\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KOKOM", "ART", "NN", "ADJA", "NN", "APPRART", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Wil Folter/ Zange/ Pfahl/ Gifft/ Brand und Dolch ausstehn/", "tokens": ["Wil", "Fol\u00b7ter", "/", "Zan\u00b7ge", "/", "Pfahl", "/", "Gifft", "/", "Brand", "und", "Dolch", "aus\u00b7stehn", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "$(", "NN", "$(", "NN", "$(", "NN", "$(", "NN", "KON", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Es mag das Glieder-Weh den gantzen Leib verderben/", "tokens": ["Es", "mag", "das", "Glie\u00b7der\u00b7Weh", "den", "gant\u00b7zen", "Leib", "ver\u00b7der\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Wenn er nur eine Zeit auff Erden noch sol gehn.", "tokens": ["Wenn", "er", "nur", "ei\u00b7ne", "Zeit", "auff", "Er\u00b7den", "noch", "sol", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "APPR", "NN", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Hier r\u00e4umt man freilich ein/ da\u00df aller Schrecken Schrecken", "tokens": ["Hier", "r\u00e4umt", "man", "frei\u00b7lich", "ein", "/", "da\u00df", "al\u00b7ler", "Schre\u00b7cken", "Schre\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "ART", "$(", "KOUS", "PIAT", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Der Tod/ so he\u00dflich sich dem Heyden f\u00fcrgestallt.", "tokens": ["Der", "Tod", "/", "so", "he\u00df\u00b7lich", "sich", "dem", "Hey\u00b7den", "f\u00fcr\u00b7ge\u00b7stallt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADV", "ADJD", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Bey uns wird er nur Freud und neue Lust erwecken/", "tokens": ["Bey", "uns", "wird", "er", "nur", "Freud", "und", "neu\u00b7e", "Lust", "er\u00b7we\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PPER", "ADV", "NN", "KON", "ADJA", "NN", "VVINF", "$("], "meter": "+----+-+-+-+-", "measure": "dactylic.init"}, "line.32": {"text": "Wir wissen/ da\u00df wir Tag f\u00fcr Tag zum sterben alt.", "tokens": ["Wir", "wis\u00b7sen", "/", "da\u00df", "wir", "Tag", "f\u00fcr", "Tag", "zum", "ster\u00b7ben", "alt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "PPER", "NN", "APPR", "NN", "APPRART", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Es ist auch nicht beschwert/ das Leimen-Haus zu lassen/", "tokens": ["Es", "ist", "auch", "nicht", "be\u00b7schwert", "/", "das", "Lei\u00b7men\u00b7Haus", "zu", "las\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PTKNEG", "VVPP", "$(", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Und von der irdnen Wand zu nehmen gute Nacht/", "tokens": ["Und", "von", "der", "ird\u00b7nen", "Wand", "zu", "neh\u00b7men", "gu\u00b7te", "Nacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "PTKZU", "VVINF", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Weil viel ein sch\u00f6ner Ort uns wieder wird \u00fcmfassen/", "tokens": ["Weil", "viel", "ein", "sch\u00f6\u00b7ner", "Ort", "uns", "wie\u00b7der", "wird", "\u00fcm\u00b7fas\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "PPER", "ADV", "VAFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Der den verkl\u00e4rten Leib zu Stern und Sonne macht.", "tokens": ["Der", "den", "ver\u00b7kl\u00e4r\u00b7ten", "Leib", "zu", "Stern", "und", "Son\u00b7ne", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Das schleussende Gewand ist denn nicht zu beklagen", "tokens": ["Das", "schleus\u00b7sen\u00b7de", "Ge\u00b7wand", "ist", "denn", "nicht", "zu", "be\u00b7kla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "PTKNEG", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Bricht doch nach der Geburth bald unser erstes Kleid!", "tokens": ["Bricht", "doch", "nach", "der", "Ge\u00b7burth", "bald", "un\u00b7ser", "ers\u00b7tes", "Kleid", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.39": {"text": "Wir Blinden/ die wir uns von Grab zu Grabe tragen/", "tokens": ["Wir", "Blin\u00b7den", "/", "die", "wir", "uns", "von", "Grab", "zu", "Gra\u00b7be", "tra\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$(", "PRELS", "PPER", "PRF", "APPR", "NN", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Beseuftzen/ da\u00df so schnell k\u00f6mt unsre Sterbens-Zeit.", "tokens": ["Be\u00b7seuft\u00b7zen", "/", "da\u00df", "so", "schnell", "k\u00f6mt", "uns\u00b7re", "Ster\u00b7bens\u00b7Zeit", "."],