{"textgrid.poem.26120": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Es irrt die Windsbraut ums Haus verst\u00f6rt", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es irrt die Windsbraut ums Haus verst\u00f6rt,", "tokens": ["Es", "irrt", "die", "Winds\u00b7braut", "ums", "Haus", "ver\u00b7st\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Durch die Schl\u00fcssell\u00f6cher man's klagen h\u00f6rt;", "tokens": ["Durch", "die", "Schl\u00fcs\u00b7sel\u00b7l\u00f6\u00b7cher", "man's", "kla\u00b7gen", "h\u00f6rt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PIS", "VVINF", "VVFIN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Ist wie ein Lied, das umgeht herrenlos", "tokens": ["Ist", "wie", "ein", "Lied", ",", "das", "um\u00b7geht", "her\u00b7ren\u00b7los"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "KOKOM", "ART", "NN", "$,", "PDS", "VVFIN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und sucht sich zum Bleiben heut einen Herd,", "tokens": ["Und", "sucht", "sich", "zum", "Blei\u00b7ben", "heut", "ei\u00b7nen", "Herd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPRART", "NN", "ADV", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Daran es wachsen kann, wie eine Feuersbrunst gro\u00df,", "tokens": ["Da\u00b7ran", "es", "wach\u00b7sen", "kann", ",", "wie", "ei\u00b7ne", "Feu\u00b7ers\u00b7brunst", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "VVINF", "VMFIN", "$,", "PWAV", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.6": {"text": "Und kommt wie ein Wolkenbruch \u00fcber die Erd'.", "tokens": ["Und", "kommt", "wie", "ein", "Wol\u00b7ken\u00b7bruch", "\u00fc\u00b7ber", "die", "Erd'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOKOM", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "Es klagt die Windsbraut verst\u00f6rt ums Haus,", "tokens": ["Es", "klagt", "die", "Winds\u00b7braut", "ver\u00b7st\u00f6rt", "ums", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Es gehn heut Schicksalsstimmen herein und hinaus,", "tokens": ["Es", "gehn", "heut", "Schick\u00b7salss\u00b7tim\u00b7men", "her\u00b7ein", "und", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Als ri\u00df man den Singv\u00f6geln die Zungen aus.", "tokens": ["Als", "ri\u00df", "man", "den", "Sing\u00b7v\u00f6\u00b7geln", "die", "Zun\u00b7gen", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PIS", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+---+--+-+", "measure": "iambic.tetra.relaxed"}}}}}