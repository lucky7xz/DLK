 "measure": "iambic.tetra"}, "line.6": {"text": "Herr, la\u00df mich Deiner Gaben", "tokens": ["Herr", ",", "la\u00df", "mich", "Dei\u00b7ner", "Ga\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVIMP", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Geringste brauchen treu und ganz,", "tokens": ["Ge\u00b7rings\u00b7te", "brau\u00b7chen", "treu", "und", "ganz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "VVFIN", "ADJD", "KON", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und ich werd' Alles haben.", "tokens": ["Und", "ich", "werd'", "Al\u00b7les", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PIS", "VAINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Der Engel, der die Perlen flicht", "tokens": ["Der", "En\u00b7gel", ",", "der", "die", "Per\u00b7len", "flicht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu unsrer Siegeskrone,", "tokens": ["Zu", "uns\u00b7rer", "Sie\u00b7ges\u00b7kro\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der ist es, der die Thr\u00e4nen z\u00e4hlt", "tokens": ["Der", "ist", "es", ",", "der", "die", "Thr\u00e4\u00b7nen", "z\u00e4hlt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PPER", "$,", "PRELS", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und sammelt uns zum Lohne;", "tokens": ["Und", "sam\u00b7melt", "uns", "zum", "Loh\u00b7ne", ";"], "token_info": ["word", "word", 