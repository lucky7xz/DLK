{"textgrid.poem.67800": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Die Pfunde", "genre": "verse", "period": "N.A.", "pub_year": 1774, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Edler zog fern \u00fcber Land,", "tokens": ["Ein", "Ed\u00b7ler", "zog", "fern", "\u00fc\u00b7ber", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df er sein Reich einn\u00e4hme", "tokens": ["Da\u00df", "er", "sein", "Reich", "ein\u00b7n\u00e4h\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und dann, gekr\u00f6nt mit Sieg und Huld,", "tokens": ["Und", "dann", ",", "ge\u00b7kr\u00f6nt", "mit", "Sieg", "und", "Huld", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "VVPP", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Vater wiederk\u00e4me.", "tokens": ["Ein", "Va\u00b7ter", "wie\u00b7der\u00b7k\u00e4\u00b7me", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "\u00bbwem soll ich meinen Schatz vertraun?\u00ab", "tokens": ["\u00bb", "wem", "soll", "ich", "mei\u00b7nen", "Schatz", "ver\u00b7traun", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWS", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sprach er zu seinen Treuen.", "tokens": ["Sprach", "er", "zu", "sei\u00b7nen", "Treu\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "\u00bbnehmt, handelt! und ich komme bald;", "tokens": ["\u00bb", "nehmt", ",", "han\u00b7delt", "!", "und", "ich", "kom\u00b7me", "bald", ";"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "VVFIN", "$.", "KON", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Es soll Euch nicht gereuen!\u00ab", "tokens": ["Es", "soll", "Euch", "nicht", "ge\u00b7reu\u00b7en", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sie handelten; er kam noch nicht,", "tokens": ["Sie", "han\u00b7del\u00b7ten", ";", "er", "kam", "noch", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "ADV", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Theil ward matt und m\u00fcde.", "tokens": ["Ein", "Theil", "ward", "matt", "und", "m\u00fc\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbund kommt er denn? Er kommt noch nicht!\u00ab", "tokens": ["\u00bb", "und", "kommt", "er", "denn", "?", "Er", "kommt", "noch", "nicht", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KON", "VVFIN", "PPER", "ADV", "$.", "PPER", "VVFIN", "ADV", "PTKNEG", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie schlummerten in Friede.", "tokens": ["Sie", "schlum\u00b7mer\u00b7ten", "in", "Frie\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Er kam! Auch in der Ferne war", "tokens": ["Er", "kam", "!", "Auch", "in", "der", "Fer\u00b7ne", "war"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "ADV", "APPR", "ART", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sein Herz tief an den Treuen.", "tokens": ["Sein", "Herz", "tief", "an", "den", "Treu\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "\u00bblegt dar nun,\u00ab sprach er, \u00bbPfund und Pfand!", "tokens": ["\u00bb", "legt", "dar", "nun", ",", "\u00ab", "sprach", "er", ",", "\u00bb", "Pfund", "und", "Pfand", "!"], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PTKVZ", "ADV", "$,", "$(", "VVFIN", "PPER", "$,", "$(", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Es soll Euch nicht gereuen!\u00ab", "tokens": ["Es", "soll", "Euch", "nicht", "ge\u00b7reu\u00b7en", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Mit Freuden trat der Erste dar,", "tokens": ["Mit", "Freu\u00b7den", "trat", "der", "Ers\u00b7te", "dar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "ADJA", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcr ", "tokens": ["F\u00fcr"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "\u00bbhier, Herr, ist Deiner G\u00fcte Pfand,", "tokens": ["\u00bb", "hier", ",", "Herr", ",", "ist", "Dei\u00b7ner", "G\u00fc\u00b7te", "Pfand", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$,", "NN", "$,", "VAFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und was ich Armer funden.\u00ab", "tokens": ["Und", "was", "ich", "Ar\u00b7mer", "fun\u00b7den", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PWS", "PPER", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "\u00bbdank, treuer Knecht, im Kleinen schon", "tokens": ["\u00bb", "dank", ",", "treu\u00b7er", "Knecht", ",", "im", "Klei\u00b7nen", "schon"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "VVFIN", "$,", "ADJA", "NN", "$,", "APPRART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So gro\u00dfer, reicher Treue!", "tokens": ["So", "gro\u00b7\u00dfer", ",", "rei\u00b7cher", "Treu\u00b7e", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJA", "$,", "ADJD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Komm, K\u00f6nig \u00fcber L\u00e4nder zehn,", "tokens": ["Komm", ",", "K\u00f6\u00b7nig", "\u00fc\u00b7ber", "L\u00e4n\u00b7der", "zehn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "APPR", "NN", "CARD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Zu Deines Herren Freude!\u00ab", "tokens": ["Zu", "Dei\u00b7nes", "Her\u00b7ren", "Freu\u00b7de", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Dem\u00fcthig trat der Andre dar,", "tokens": ["De\u00b7m\u00fct\u00b7hig", "trat", "der", "And\u00b7re", "dar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "PIS", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcr ", "tokens": ["F\u00fcr"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "\u00bbhier hast Du, Herr, Dein edles Pfand;", "tokens": ["\u00bb", "hier", "hast", "Du", ",", "Herr", ",", "Dein", "ed\u00b7les", "Pfand", ";"], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "PPER", "$,", "NN", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie wenig hat es funden!\u00ab", "tokens": ["Wie", "we\u00b7nig", "hat", "es", "fun\u00b7den", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "PIS", "VAFIN", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "\u00bbdank, Treuer, im Geringern schon", "tokens": ["\u00bb", "dank", ",", "Treu\u00b7er", ",", "im", "Ge\u00b7rin\u00b7gern", "schon"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["$(", "VVFIN", "$,", "NN", "$,", "APPRART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So gro\u00dfer, reicher Treue!", "tokens": ["So", "gro\u00b7\u00dfer", ",", "rei\u00b7cher", "Treu\u00b7e", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJA", "$,", "ADJD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Herr \u00fcber f\u00fcnf der L\u00e4nder, komm", "tokens": ["Herr", "\u00fc\u00b7ber", "f\u00fcnf", "der", "L\u00e4n\u00b7der", ",", "komm"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["NN", "APPR", "CARD", "ART", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Zu Deines Herren Freude!\u00ab", "tokens": ["Zu", "Dei\u00b7nes", "Her\u00b7ren", "Freu\u00b7de", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Mit Beben naht' der Dritte sich,", "tokens": ["Mit", "Be\u00b7ben", "naht'", "der", "Drit\u00b7te", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "ADJA", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Trotz verh\u00fcllt sein Beben.", "tokens": ["In", "Trotz", "ver\u00b7h\u00fcllt", "sein", "Be\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbherr,\u00ab sprach er, \u00bbnimm Dein Pfund und Pfand,", "tokens": ["\u00bb", "herr", ",", "\u00ab", "sprach", "er", ",", "\u00bb", "nimm", "Dein", "Pfund", "und", "Pfand", ","], "token_info": ["punct", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKVZ", "$,", "$(", "VVFIN", "PPER", "$,", "$(", "VVIMP", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "All, was Du mir gegeben!", "tokens": ["All", ",", "was", "Du", "mir", "ge\u00b7ge\u00b7ben", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ich kannte Dich wol, harten Mann,", "tokens": ["Ich", "kann\u00b7te", "Dich", "wol", ",", "har\u00b7ten", "Mann", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der erntet unges\u00e4et", "tokens": ["Der", "ern\u00b7tet", "un\u00b7ge\u00b7s\u00e4et"], "token_info": ["word", "word", "word"], "pos": ["PDS", "VVFIN", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und fremden Schwei\u00df und saures Gut", "tokens": ["Und", "frem\u00b7den", "Schwei\u00df", "und", "sau\u00b7res", "Gut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Auf 's Armen Aue m\u00e4het.", "tokens": ["Auf", "'s", "Ar\u00b7men", "Au\u00b7e", "m\u00e4\u00b7het", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Drum hatt' ich, Dir zu wuchern, Zorn;", "tokens": ["Drum", "hatt'", "ich", ",", "Dir", "zu", "wu\u00b7chern", ",", "Zorn", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "$,", "PPER", "PTKZU", "VVINF", "$,", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hier, Harter, ist das Deine!", "tokens": ["Hier", ",", "Har\u00b7ter", ",", "ist", "das", "Dei\u00b7ne", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "$,", "VAFIN", "ART", "PPOSAT", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die sichre Erde barg es Dir;", "tokens": ["Die", "sich\u00b7re", "Er\u00b7de", "barg", "es", "Dir", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dies Schwei\u00dftuch ist das Meine.\u00ab", "tokens": ["Dies", "Schwei\u00df\u00b7tuch", "ist", "das", "Mei\u00b7ne", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "NN", "VAFIN", "ART", "PPOSAT", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "\u00bbdein Mund spricht selber Dir Gericht,", "tokens": ["\u00bb", "dein", "Mund", "spricht", "sel\u00b7ber", "Dir", "Ge\u00b7richt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "VVFIN", "ADV", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Untreuer meiner Knechte;", "tokens": ["Un\u00b7treu\u00b7er", "mei\u00b7ner", "Knech\u00b7te", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "So wu\u00dftest Du mich harten Mann,", "tokens": ["So", "wu\u00df\u00b7test", "Du", "mich", "har\u00b7ten", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und wie so hart ich rechte,", "tokens": ["Und", "wie", "so", "hart", "ich", "rech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADV", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Und \u00fcbtest nicht, was Du gewu\u00dft,", "tokens": ["Und", "\u00fcb\u00b7test", "nicht", ",", "was", "Du", "ge\u00b7wu\u00dft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "$,", "PWS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Knecht, Deines Herren Willen,", "tokens": ["Knecht", ",", "Dei\u00b7nes", "Her\u00b7ren", "Wil\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Des harten Herren letztes Wort", "tokens": ["Des", "har\u00b7ten", "Her\u00b7ren", "letz\u00b7tes", "Wort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Wucher zu erf\u00fcllen?", "tokens": ["Mit", "Wu\u00b7cher", "zu", "er\u00b7f\u00fcl\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Nehmt hin von ihm sein treulos Pfand!", "tokens": ["Nehmt", "hin", "von", "ihm", "sein", "treu\u00b7los", "Pfand", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPER", "PPOSAT", "ADJD", "NN", "$."], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.6": {"text": "Dem Reichsten sei's gegeben.", "tokens": ["Dem", "Reichs\u00b7ten", "sei's", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wer nicht hat, b\u00fc\u00dfe, was er hat;", "tokens": ["Wer", "nicht", "hat", ",", "b\u00fc\u00b7\u00dfe", ",", "was", "er", "hat", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PW