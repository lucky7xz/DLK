pos": ["NN", "$,", "NN", "$,", "NN", "$,", "PWAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo sind itzt Eure Schatten?", "tokens": ["Wo", "sind", "itzt", "Eu\u00b7re", "Schat\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Seht, wie sich Licht und Wahrheit liebt,", "tokens": ["Seht", ",", "wie", "sich", "Licht", "und", "Wahr\u00b7heit", "liebt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "PRF", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Treu und Huld sich gatten!", "tokens": ["Und", "Treu", "und", "Huld", "sich", "gat\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Zu Freunden dr\u00e4ngt sich Freund und Freund,", "tokens": ["Zu", "Freun\u00b7den", "dr\u00e4ngt", "sich", "Freund", "und", "Freund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PRF", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Gleiches hier erlitten,", "tokens": ["Die", "Glei\u00b7ches", "hier", "er\u00b7lit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Erw\u00fcnscht, gewirkt, verloren und \u2013", "tokens": ["Er\u00b7w\u00fcnscht", ",", "ge\u00b7wirkt", ",", "ver\u00b7lo\u00b7ren", "und", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVPP", "$,", "VVPP", "KON", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und sich die Kron' erstritten.", "tokens": ["Und", "sich", "die", "Kron'", "er\u00b7strit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Hier trennten Nacht und Nebel sie,", "tokens": ["Hier", "trenn\u00b7ten", "Nacht", "und", "Ne\u00b7bel", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "KON", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Jahrhunderte und Lande;", "tokens": ["Jahr\u00b7hun\u00b7der\u00b7te", "und", "Lan\u00b7de", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "-+---+-", "measure": "dactylic.init"}, "line.3": {"text": "Dort Alle Glieder, Br\u00fcder nun", "tokens": ["Dort", "Al\u00b7le", "Glie\u00b7der", ",", "Br\u00fc\u00b7der", "nun"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "PIAT", "NN", "$,", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An ", "tokens": ["An"], "token_info": ["word"], "pos": ["APPR"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Ihr Wille fleu\u00dft wie Sonnenlicht", "tokens": ["Ihr", "Wil\u00b7le", "fleu\u00dft", "wie", "Son\u00b7nen\u00b7licht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "KOKOM", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Aus aller Welt zusammen;", "tokens": ["Aus", "al\u00b7ler", "Welt", "zu\u00b7sam\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Zusammenflammt da ihr Gebet,", "tokens": ["Zu\u00b7sam\u00b7men\u00b7flammt", "da", "ihr", "Ge\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ihr M\u00fchn in hellen Flammen.", "tokens": ["Ihr", "M\u00fchn", "in", "hel\u00b7len", "Flam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Elias, Moses werd' ich sehn", "tokens": ["E\u00b7lias", ",", "Mo\u00b7ses", "werd'", "ich", "sehn"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "NE", "VAFIN", "PPER", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit ihren tausend Pfunden,", "tokens": ["Mit", "ih\u00b7ren", "tau\u00b7send", "Pfun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "CARD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und Paulus, Luther vor mir stehn", "tokens": ["Und", "Pau\u00b7lus", ",", "Lu\u00b7ther", "vor", "mir", "stehn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "NE", "$,", "NE", "APPR", "PPER", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit ihren hundert Pfunden.", "tokens": ["Mit", "ih\u00b7ren", "hun\u00b7dert", "Pfun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "CARD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "O, legt' ich freudig sch\u00fcchtern dann", "tokens": ["O", ",", "legt'", "ich", "freu\u00b7dig", "sch\u00fcch\u00b7tern", "dann"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "VVFIN", "PPER", "ADJD", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nach Euch mein Quentlein nieder", "tokens": ["Nach", "Euch", "mein", "Quent\u00b7lein", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "PPOSAT", "NN", "PTKVZ"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und f\u00e4nde, gr\u00fc\u00dfte, f\u00fchlt' Euch dann", "tokens": ["Und", "f\u00e4n\u00b7de", ",", "gr\u00fc\u00df\u00b7te", ",", "f\u00fchlt'", "Euch", "dann"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mir V\u00e4ter, Freund' und Br\u00fcder!", "tokens": ["Mir", "V\u00e4\u00b7ter", ",", "Freund'", "und", "Br\u00fc\u00b7der", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "\u00bbdu locktest und Du hobest mich,", "tokens": ["\u00bb", "du", "lock\u00b7test", "und", "Du", "ho\u00b7best", "mich", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "KON", "PPER", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Warst bei mir im Gebete,", "tokens": ["Warst", "bei", "mir", "im", "Ge\u00b7be\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "APPRART", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Du strafetest, Du halfest mir,", "tokens": ["Du", "stra\u00b7fe\u00b7test", ",", "Du", "hal\u00b7fest", "mir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df freudig vor ich trete!", "tokens": ["Da\u00df", "freu\u00b7dig", "vor", "ich", "tre\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ich dank' Euch meine Seligkeit,", "tokens": ["Ich", "dank'", "Euch", "mei\u00b7ne", "Se\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ihr sch\u00f6n verkannten Seelen!", "tokens": ["Ihr", "sch\u00f6n", "ver\u00b7kann\u00b7ten", "See\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wir sind itzt Glieder, Br\u00fcder nun", "tokens": ["Wir", "sind", "itzt", "Glie\u00b7der", ",", "Br\u00fc\u00b7der", "nun"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "NN", "$,", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und sind es sonder W\u00e4hlen.\u00ab", "tokens": ["Und", "sind", "es", "son\u00b7der", "W\u00e4h\u00b7len", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VAFIN", "PPER", "PIAT", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Herr, Seligkeit und Himmel liegt", "tokens": ["Herr", ",", "Se\u00b7lig\u00b7keit", "und", "Him\u00b7mel", "liegt"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In jeder Deiner Gaben;", "tokens": ["In", "je\u00b7der", "Dei\u00b7ner", "Ga\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wer neidet und verscharret sie,", "tokens": ["Wer", "nei\u00b7det", "und", "ver\u00b7schar\u00b7ret", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "KON", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verdient er, mehr zu haben?", "tokens": ["Ver\u00b7di\u00b7ent", "er", ",", "mehr", "zu", "ha\u00b7ben", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADV", "PTKZU", "VAINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Wer treu ist, Alles hat er schon.", "tokens": ["Wer", "treu", "ist", ",", "Al\u00b7les", "hat", "er", "schon", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "$,", "PIS", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df ich mich ewig freue,", "tokens": ["Da\u00df", "ich", "mich", "e\u00b7wig", "freu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "O Geber, und mir Alles sei,", "tokens": ["O", "Ge\u00b7ber", ",", "und", "mir", "Al\u00b7les", "sei", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "KON", "PPER", "PIS", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Gieb mir im Kleinsten Treue!", "tokens": ["Gieb", "mir", "im", "Kleins\u00b7ten", "Treu\u00b7e", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}