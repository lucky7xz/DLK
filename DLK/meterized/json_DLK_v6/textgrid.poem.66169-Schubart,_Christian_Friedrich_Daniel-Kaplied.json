{"textgrid.poem.66169": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Kaplied", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf, auf! ihr Br\u00fcder und seid stark,", "tokens": ["Auf", ",", "auf", "!", "ihr", "Br\u00fc\u00b7der", "und", "seid", "stark", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "PTKVZ", "$.", "PPOSAT", "NN", "KON", "VAFIN", "ADJD", "$,"], "meter": "-+-+-++-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Der Abschiedstag ist da!", "tokens": ["Der", "Ab\u00b7schieds\u00b7tag", "ist", "da", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Schwer liegt er auf der Seele, schwer!", "tokens": ["Schwer", "liegt", "er", "auf", "der", "See\u00b7le", ",", "schwer", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "$,", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wir sollen \u00fcber Land und Meer", "tokens": ["Wir", "sol\u00b7len", "\u00fc\u00b7ber", "Land", "und", "Meer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ins hei\u00dfe Afrika.", "tokens": ["Ins", "hei\u00b7\u00dfe", "Af\u00b7ri\u00b7ka", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ein dichter Kreis von Lieben steht,", "tokens": ["Ein", "dich\u00b7ter", "Kreis", "von", "Lie\u00b7ben", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr Br\u00fcder, um uns her:", "tokens": ["Ihr", "Br\u00fc\u00b7der", ",", "um", "uns", "her", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KOUI", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Uns kn\u00fcpft so manches theure Band", "tokens": ["Uns", "kn\u00fcpft", "so", "man\u00b7ches", "theu\u00b7re", "Band"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An unser deutsches Vaterland,", "tokens": ["An", "un\u00b7ser", "deut\u00b7sches", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Drum f\u00e4llt der Abschied schwer.", "tokens": ["Drum", "f\u00e4llt", "der", "Ab\u00b7schied", "schwer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Dem bieten graue Eltern noch", "tokens": ["Dem", "bie\u00b7ten", "grau\u00b7e", "El\u00b7tern", "noch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zum letzten Mal die Hand;", "tokens": ["Zum", "letz\u00b7ten", "Mal", "die", "Hand", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den kosen Bruder, Schwester, Freund;", "tokens": ["Den", "ko\u00b7sen", "Bru\u00b7der", ",", "Schwes\u00b7ter", ",", "Freund", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und alles schweigt, und alles weint,", "tokens": ["Und", "al\u00b7les", "schweigt", ",", "und", "al\u00b7les", "weint", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "$,", "KON", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Todtbla\u00df von uns gewandt.", "tokens": ["Todt\u00b7bla\u00df", "von", "uns", "ge\u00b7wandt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und wie ein Geist schlingt um den Hals", "tokens": ["Und", "wie", "ein", "Geist", "schlingt", "um", "den", "Hals"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Liebchen sich herum:", "tokens": ["Das", "Lieb\u00b7chen", "sich", "he\u00b7rum", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Willst mich verlassen, liebes Herz,", "tokens": ["Willst", "mich", "ver\u00b7las\u00b7sen", ",", "lie\u00b7bes", "Herz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "VVINF", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf ewig? und der bittre Schmerz", "tokens": ["Auf", "e\u00b7wig", "?", "und", "der", "bitt\u00b7re", "Schmerz"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJD", "$.", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Macht's arme Liebchen stumm.", "tokens": ["Macht's", "ar\u00b7me", "Lieb\u00b7chen", "stumm", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ist hart! drum wirble du, Tambour,", "tokens": ["Ist", "hart", "!", "drum", "wirb\u00b7le", "du", ",", "Tam\u00b7bour", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "ADJD", "$.", "PAV", "VVFIN", "PPER", "$,", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Generalmarsch drein.", "tokens": ["Den", "Ge\u00b7ne\u00b7ral\u00b7marsch", "drein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Abschied macht uns sonst zu weich,", "tokens": ["Der", "Ab\u00b7schied", "macht", "uns", "sonst", "zu", "weich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wir weinten kleinen Kindern gleich;", "tokens": ["Wir", "wein\u00b7ten", "klei\u00b7nen", "Kin\u00b7dern", "gleich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es mu\u00df geschieden sein.", "tokens": ["Es", "mu\u00df", "ge\u00b7schie\u00b7den", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVPP", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Lebt wohl, ihr Freunde! Sehn wir uns", "tokens": ["Lebt", "wohl", ",", "ihr", "Freun\u00b7de", "!", "Sehn", "wir", "uns"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADV", "$,", "PPOSAT", "NN", "$.", "VVFIN", "PPER", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vielleicht zum letzten Mal,", "tokens": ["Viel\u00b7leicht", "zum", "letz\u00b7ten", "Mal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So denkt, nicht f\u00fcr die kurze Zeit,", "tokens": ["So", "denkt", ",", "nicht", "f\u00fcr", "die", "kur\u00b7ze", "Zeit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PTKNEG", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Freundschaft ist f\u00fcr die Ewigkeit,", "tokens": ["Freund\u00b7schaft", "ist", "f\u00fcr", "die", "E\u00b7wig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Und Gott ist \u00fcberall.", "tokens": ["Und", "Gott", "ist", "\u00fc\u00b7be\u00b7rall", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "An Deutschlands Grenze f\u00fcllen wir", "tokens": ["An", "Deutschlands", "Gren\u00b7ze", "f\u00fcl\u00b7len", "wir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "NE", "VVFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit Erde unsre Hand,", "tokens": ["Mit", "Er\u00b7de", "uns\u00b7re", "Hand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und k\u00fcssen sie, das sei der Dank", "tokens": ["Und", "k\u00fcs\u00b7sen", "sie", ",", "das", "sei", "der", "Dank"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "$,", "PDS", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr deine Pflege, Speis' und Trank,", "tokens": ["F\u00fcr", "dei\u00b7ne", "Pfle\u00b7ge", ",", "Speis'", "und", "Trank", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du liebes Vaterland!", "tokens": ["Du", "lie\u00b7bes", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Wenn dann die Meereswoge sich", "tokens": ["Wenn", "dann", "die", "Mee\u00b7res\u00b7wo\u00b7ge", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An unsern Schiffen bricht,", "tokens": ["An", "un\u00b7sern", "Schif\u00b7fen", "bricht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So segeln wir gelassen fort;", "tokens": ["So", "se\u00b7geln", "wir", "ge\u00b7las\u00b7sen", "fort", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVPP", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Denn Gott ist hier und Gott ist dort,", "tokens": ["Denn", "Gott", "ist", "hier", "und", "Gott", "ist", "dort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "ADV", "KON", "NN", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und der verl\u00e4\u00dft uns nicht!", "tokens": ["Und", "der", "ver\u00b7l\u00e4\u00dft", "uns", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Und ha, wenn sich der Tafelberg", "tokens": ["Und", "ha", ",", "wenn", "sich", "der", "Ta\u00b7fel\u00b7berg"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "NE", "$,", "KOUS", "PRF", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aus blauen D\u00fcften hebt,", "tokens": ["Aus", "blau\u00b7en", "D\u00fcf\u00b7ten", "hebt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So strecken wir empor die Hand,", "tokens": ["So", "stre\u00b7cken", "wir", "em\u00b7por", "die", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und jauchzen: Land! ihr Br\u00fcder, Land!", "tokens": ["Und", "jauch\u00b7zen", ":", "Land", "!", "ihr", "Br\u00fc\u00b7der", ",", "Land", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVINF", "$.", "NN", "$.", "PPOSAT", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df unser Schiff erbebt.", "tokens": ["Da\u00df", "un\u00b7ser", "Schiff", "er\u00b7bebt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Und wenn Soldat und Offizier", "tokens": ["Und", "wenn", "Sol\u00b7dat", "und", "Of\u00b7fi\u00b7zier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gesund ans Ufer springt,", "tokens": ["Ge\u00b7sund", "ans", "U\u00b7fer", "springt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dann jubeln wir, ihr Br\u00fcder, ha!", "tokens": ["Dann", "ju\u00b7beln", "wir", ",", "ihr", "Br\u00fc\u00b7der", ",", "ha", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "ITJ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nun sind wir ja in Afrika.", "tokens": ["Nun", "sind", "wir", "ja", "in", "Af\u00b7ri\u00b7ka", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und alles dankt und singt.", "tokens": ["Und", "al\u00b7les", "dankt", "und", "singt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Wir leben drauf in fernem Land", "tokens": ["Wir", "le\u00b7ben", "drauf", "in", "fer\u00b7nem", "Land"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PAV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als Deutsche brav und gut,", "tokens": ["Als", "Deut\u00b7sche", "brav", "und", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und sagen soll man weit und breit,", "tokens": ["Und", "sa\u00b7gen", "soll", "man", "weit", "und", "breit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "PIS", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Deutschen sind doch brave Leut',", "tokens": ["Die", "Deut\u00b7schen", "sind", "doch", "bra\u00b7ve", "Leut'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Se haben Geist und Muth.", "tokens": ["Se", "ha\u00b7ben", "Geist", "und", "Muth", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "NN", "KON", "NN", "$."], "meter": "---+-+", "measure": "unknown.measure.di"}}, "stanza.12": {"line.1": {"text": "Und trinken auf dem Hoffnungskap", "tokens": ["Und", "trin\u00b7ken", "auf", "dem", "Hoff\u00b7nungs\u00b7kap"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wir seinen G\u00f6tterwein,", "tokens": ["Wir", "sei\u00b7nen", "G\u00f6t\u00b7ter\u00b7wein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So denken wir, von Sehnsucht weich,", "tokens": ["So", "den\u00b7ken", "wir", ",", "von", "Sehn\u00b7sucht", "weich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr fernen Freunde, dann an euch;", "tokens": ["Ihr", "fer\u00b7nen", "Freun\u00b7de", ",", "dann", "an", "euch", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und Thr\u00e4nen flie\u00dfen drein.", "tokens": ["Und", "Thr\u00e4\u00b7nen", "flie\u00b7\u00dfen", "drein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}