{"dta.poem.19261": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "108.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was W\u00e4rme schnell annimmt, l\u00e4\u00dft schnell sie wieder fahren;", "tokens": ["Was", "W\u00e4r\u00b7me", "schnell", "an\u00b7nimmt", ",", "l\u00e4\u00dft", "schnell", "sie", "wie\u00b7der", "fah\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "ADJD", "VVFIN", "$,", "VVFIN", "ADJD", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was sie nimmt langsam an, wird lange sie bewahren.", "tokens": ["Was", "sie", "nimmt", "lang\u00b7sam", "an", ",", "wird", "lan\u00b7ge", "sie", "be\u00b7wah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "ADJD", "PTKVZ", "$,", "VAFIN", "ADV", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Das gilt vom Menschensinn alswie von Holz und Stein;", "tokens": ["Das", "gilt", "vom", "Men\u00b7schen\u00b7sinn", "als\u00b7wie", "von", "Holz", "und", "Stein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPRART", "NN", "KOKOM", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein leicht erw\u00e4rmter Freund wird leicht erk\u00e4ltet seyn.", "tokens": ["Ein", "leicht", "er\u00b7w\u00e4rm\u00b7ter", "Freund", "wird", "leicht", "er\u00b7k\u00e4l\u00b7tet", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "VAFIN", "ADJD", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Was schiltst du ihn? Er ist ein guter W\u00e4rmeleiter;", "tokens": ["Was", "schiltst", "du", "ihn", "?", "Er", "ist", "ein", "gu\u00b7ter", "W\u00e4r\u00b7me\u00b7lei\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPER", "$.", "PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was er von dir empfieng, gibt er an andre weiter.", "tokens": ["Was", "er", "von", "dir", "emp\u00b7fi\u00b7eng", ",", "gibt", "er", "an", "and\u00b7re", "wei\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "APPR", "PIS", "PTKVZ", "$."], "meter": "--+--+-+--+-+-", "measure": "anapaest.di.plus"}}}}}