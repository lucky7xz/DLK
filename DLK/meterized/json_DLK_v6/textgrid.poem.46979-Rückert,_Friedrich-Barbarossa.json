{"textgrid.poem.46979": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Barbarossa", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der alte Barbarossa,", "tokens": ["Der", "al\u00b7te", "Bar\u00b7ba\u00b7ros\u00b7sa", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Kaiser Friederich,", "tokens": ["Der", "Kai\u00b7ser", "Frie\u00b7de\u00b7rich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Im unterird'schen Schlosse", "tokens": ["Im", "un\u00b7ter\u00b7ird'\u00b7schen", "Schlos\u00b7se"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "H\u00e4lt er verzaubert sich.", "tokens": ["H\u00e4lt", "er", "ver\u00b7zau\u00b7bert", "sich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "PRF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Er ist niemals gestorben,", "tokens": ["Er", "ist", "nie\u00b7mals", "ge\u00b7stor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Er lebt darin noch jetzt;", "tokens": ["Er", "lebt", "da\u00b7rin", "noch", "jetzt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er hat im Schlo\u00df verborgen", "tokens": ["Er", "hat", "im", "Schlo\u00df", "ver\u00b7bor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zum Schlaf sich hingesetzt.", "tokens": ["Zum", "Schlaf", "sich", "hin\u00b7ge\u00b7setzt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Er hat hinabgenommen", "tokens": ["Er", "hat", "hin\u00b7ab\u00b7ge\u00b7nom\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Des Reiches Herrlichkeit,", "tokens": ["Des", "Rei\u00b7ches", "Herr\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wird einst wiederkommen", "tokens": ["Und", "wird", "einst", "wie\u00b7der\u00b7kom\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit ihr, zu seiner Zeit.", "tokens": ["Mit", "ihr", ",", "zu", "sei\u00b7ner", "Zeit", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Der Stuhl ist elfenbeinern,", "tokens": ["Der", "Stuhl", "ist", "el\u00b7fen\u00b7bei\u00b7nern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Darauf der Kaiser sitzt;", "tokens": ["Da\u00b7rauf", "der", "Kai\u00b7ser", "sitzt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Tisch ist marmelsteinern,", "tokens": ["Der", "Tisch", "ist", "mar\u00b7mel\u00b7stei\u00b7nern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Worauf sein Haupt er st\u00fctzt.", "tokens": ["Wo\u00b7rauf", "sein", "Haupt", "er", "st\u00fctzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Sein Bart ist nicht von Flachse,", "tokens": ["Sein", "Bart", "ist", "nicht", "von", "Flach\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKNEG", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Er ist von Feuersglut,", "tokens": ["Er", "ist", "von", "Feu\u00b7ers\u00b7glut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ist durch den Tisch gewachsen,", "tokens": ["Ist", "durch", "den", "Tisch", "ge\u00b7wach\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Worauf sein Kinn ausruht.", "tokens": ["Wo\u00b7rauf", "sein", "Kinn", "aus\u00b7ruht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Er nickt als wie im Traume,", "tokens": ["Er", "nickt", "als", "wie", "im", "Trau\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOUS", "KOKOM", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sein Aug' halb offen zwinkt;", "tokens": ["Sein", "Aug'", "halb", "of\u00b7fen", "zwinkt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und je nach langem Raume", "tokens": ["Und", "je", "nach", "lan\u00b7gem", "Rau\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Er einem Knaben winkt.", "tokens": ["Er", "ei\u00b7nem", "Kna\u00b7ben", "winkt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Er spricht im Schlaf zum Knaben:", "tokens": ["Er", "spricht", "im", "Schlaf", "zum", "Kna\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Geh hin vors Schlo\u00df, o Zwerg,", "tokens": ["Geh", "hin", "vors", "Schlo\u00df", ",", "o", "Zwerg", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADV", "APPRART", "NN", "$,", "FM", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und sieh, ob noch die Raben", "tokens": ["Und", "sieh", ",", "ob", "noch", "die", "Ra\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "KOUS", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Herfliegen um den Berg.", "tokens": ["Her\u00b7flie\u00b7gen", "um", "den", "Berg", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Und wenn die alten Raben", "tokens": ["Und", "wenn", "die", "al\u00b7ten", "Ra\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Noch fliegen immerdar,", "tokens": ["Noch", "flie\u00b7gen", "im\u00b7mer\u00b7dar", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So mu\u00df ich auch noch schlafen", "tokens": ["So", "mu\u00df", "ich", "auch", "noch", "schla\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Verzaubert hundert Jahr.", "tokens": ["Ver\u00b7zau\u00b7bert", "hun\u00b7dert", "Jahr", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "CARD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}