{"dta.poem.23403": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Neueste Beschreibung des Wiener Congresses.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was sie jeden Tag vollbrachten,", "tokens": ["Was", "sie", "je\u00b7den", "Tag", "voll\u00b7brach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ob sie scherzten, ob sie lachten,", "tokens": ["Ob", "sie", "scherz\u00b7ten", ",", "ob", "sie", "lach\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wird genau erz\u00e4hlt;", "tokens": ["Wird", "ge\u00b7nau", "er\u00b7z\u00e4hlt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Wie sie standen, wie sie sa\u00dfen,", "tokens": ["Wie", "sie", "stan\u00b7den", ",", "wie", "sie", "sa\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df sie tranken, da\u00df sie a\u00dfen,", "tokens": ["Da\u00df", "sie", "tran\u00b7ken", ",", "da\u00df", "sie", "a\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wird auch nicht verhehlt.", "tokens": ["Wird", "auch", "nicht", "ver\u00b7hehlt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Wann sie hin zu Balle gingen,", "tokens": ["Wann", "sie", "hin", "zu", "Bal\u00b7le", "gin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wann sie an zu tanzen fingen,", "tokens": ["Wann", "sie", "an", "zu", "tan\u00b7zen", "fin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PTKVZ", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wird genau erz\u00e4hlt;", "tokens": ["Wird", "ge\u00b7nau", "er\u00b7z\u00e4hlt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Ob das Schauspiel sie zerstreute,", "tokens": ["Ob", "das", "Schau\u00b7spiel", "sie", "zer\u00b7streu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ob sie das Ballett erfreute,", "tokens": ["Ob", "sie", "das", "Bal\u00b7lett", "er\u00b7freu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Wird auch nicht verhehlt.", "tokens": ["Wird", "auch", "nicht", "ver\u00b7hehlt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Wie sie gl\u00e4nzend bankettierten,", "tokens": ["Wie", "sie", "gl\u00e4n\u00b7zend", "ban\u00b7ket\u00b7tier\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie sie ritterlich turnierten,", "tokens": ["Wie", "sie", "rit\u00b7ter\u00b7lich", "tur\u00b7nier\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wird genau erz\u00e4hlt;", "tokens": ["Wird", "ge\u00b7nau", "er\u00b7z\u00e4hlt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Ob sie gro\u00dfe Heerschau hielten,", "tokens": ["Ob", "sie", "gro\u00b7\u00dfe", "Heer\u00b7schau", "hiel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Oder Schach und Dame spielten,", "tokens": ["O\u00b7der", "Schach", "und", "Da\u00b7me", "spiel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wird auch nicht verhehlt.", "tokens": ["Wird", "auch", "nicht", "ver\u00b7hehlt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Ob sie ritten, ob sie fuhren,", "tokens": ["Ob", "sie", "rit\u00b7ten", ",", "ob", "sie", "fuh\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ob im Frack, ob in Monturen,", "tokens": ["Ob", "im", "Frack", ",", "ob", "in", "Mon\u00b7tu\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "$,", "KOUS", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wird genau erz\u00e4hlt;", "tokens": ["Wird", "ge\u00b7nau", "er\u00b7z\u00e4hlt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Wie sie sich der Menge zeigten,", "tokens": ["Wie", "sie", "sich", "der", "Men\u00b7ge", "zeig\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie sie gn\u00e4digst sich verneigten,", "tokens": ["Wie", "sie", "gn\u00e4\u00b7digst", "sich", "ver\u00b7neig\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wird auch nicht verhehlt.", "tokens": ["Wird", "auch", "nicht", "ver\u00b7hehlt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Doch ihr sonstig Thun und Rathen \u2014", "tokens": ["Doch", "ihr", "sons\u00b7tig", "Thun", "und", "Ra\u00b7then"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJD", "NN", "KON", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was sie f\u00fcr die V\u00f6lker thaten,", "tokens": ["Was", "sie", "f\u00fcr", "die", "V\u00f6l\u00b7ker", "tha\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wird genau verhehlt;", "tokens": ["Wird", "ge\u00b7nau", "ver\u00b7hehlt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Ob sie sonst was Gutes dachten,", "tokens": ["Ob", "sie", "sonst", "was", "Gu\u00b7tes", "dach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PWS", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ueberhaupt was Gutes machten,", "tokens": ["Ue\u00b7ber\u00b7haupt", "was", "Gu\u00b7tes", "mach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wird auch nie erz\u00e4hlt.", "tokens": ["Wird", "auch", "nie", "er\u00b7z\u00e4hlt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}