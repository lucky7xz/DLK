{"dta.poem.10015": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Die Vergn\u00fcgungen  \n  der Melancholey.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20674-3", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Komm denn, du K\u00f6nigin ernster Gedanken, Me-", "tokens": ["Komm", "denn", ",", "du", "K\u00f6\u00b7ni\u00b7gin", "erns\u00b7ter", "Ge\u00b7dan\u00b7ken", ",", "Me"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "ADV", "$,", "PPER", "NN", "ADJA", "NN", "$,", "TRUNC"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Komm mit heiligem Blick, und festem best\u00e4ndigen", "tokens": ["Komm", "mit", "hei\u00b7li\u00b7gem", "Blick", ",", "und", "fes\u00b7tem", "be\u00b7st\u00e4n\u00b7di\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,", "KON", "ADJA", "NN"], "meter": "+-+--+-+--+--", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Aus der H\u00f6le hervor, vom traurigen Epheu umschatter,", "tokens": ["Aus", "der", "H\u00f6\u00b7le", "her\u00b7vor", ",", "vom", "trau\u00b7ri\u00b7gen", "E\u00b7pheu", "um\u00b7schat\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$,", "APPRART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "--+--+-+-+-+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Wo du dich bis zum Schall der Abendglocke verweilest,", "tokens": ["Wo", "du", "dich", "bis", "zum", "Schall", "der", "A\u00b7bend\u00b7glo\u00b7cke", "ver\u00b7wei\u00b7lest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "APPR", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Komm, und bekr\u00e4nze das Haar von deinem geweihten", "tokens": ["Komm", ",", "und", "be\u00b7kr\u00e4n\u00b7ze", "das", "Haar", "von", "dei\u00b7nem", "ge\u00b7weih\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KON", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "ADJA"], "meter": "+--+--+-+--+-", "measure": "dactylic.di.plus"}, "line.6": {"text": "Mit Cypressen; es m\u00fcsse mir nie die lachende Freude", "tokens": ["Mit", "Cyp\u00b7res\u00b7sen", ";", "es", "m\u00fcs\u00b7se", "mir", "nie", "die", "la\u00b7chen\u00b7de", "Freu\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$.", "PPER", "VMFIN", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Mein standhaftes Gem\u00fcth mit gauckelndem Scherzen", "tokens": ["Mein", "stand\u00b7haf\u00b7tes", "Ge\u00b7m\u00fcth", "mit", "gau\u00b7ckeln\u00b7dem", "Scher\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "+-+--+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "Noch mit Kr\u00e4nzen von Blumen von deinem Wege mich", "tokens": ["Noch", "mit", "Kr\u00e4n\u00b7zen", "von", "Blu\u00b7men", "von", "dei\u00b7nem", "We\u00b7ge", "mich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "APPR", "NN", "APPR", "PPOSAT", "NN", "PPER"], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.9": {"text": "Denn obgleich in ihrem Gefolge die l\u00e4chelnde Hebe", "tokens": ["Denn", "ob\u00b7gleich", "in", "ih\u00b7rem", "Ge\u00b7fol\u00b7ge", "die", "l\u00e4\u00b7cheln\u00b7de", "He\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "APPR", "PPOSAT", "NN", "ART", "ADJA", "NN"], "meter": "-+--+--+--+--+-", "measure": "amphibrach.penta.plus"}, "line.10": {"text": "Jhre blendende Brust den liebenden Augen enth\u00fcllet,", "tokens": ["Ih\u00b7re", "blen\u00b7den\u00b7de", "Brust", "den", "lie\u00b7ben\u00b7den", "Au\u00b7gen", "ent\u00b7h\u00fcl\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.11": {"text": "Obgleich Venus, die Mutter der Liebe, der Freuden,", "tokens": ["Ob\u00b7gleich", "Ve\u00b7nus", ",", "die", "Mut\u00b7ter", "der", "Lie\u00b7be", ",", "der", "Freu\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "ART", "NN", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "---+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Mit ihr Bacchus, mit Weinlaub gekr\u00e4nzt, am str\u00f6men-", "tokens": ["Mit", "ihr", "Bac\u00b7chus", ",", "mit", "Wein\u00b7laub", "ge\u00b7kr\u00e4nzt", ",", "am", "str\u00f6\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "APPR", "NN", "VVPP", "$,", "APPRART", "TRUNC"], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.13": {"text": "Sich in duftenden Lauben ergetzen, und selber der Himmel,", "tokens": ["Sich", "in", "duf\u00b7ten\u00b7den", "Lau\u00b7ben", "er\u00b7get\u00b7zen", ",", "und", "sel\u00b7ber", "der", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ADJA", "NN", "VVINF", "$,", "KON", "ADV", "ART", "NN", "$,"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.14": {"text": "Wenn sie sich nahn, sich erheitert, indem durch blaue", "tokens": ["Wenn", "sie", "sich", "nahn", ",", "sich", "er\u00b7hei\u00b7tert", ",", "in\u00b7dem", "durch", "blau\u00b7e"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "ADJA", "$,", "PRF", "VVPP", "$,", "KOUS", "APPR", "ADJA"], "meter": "+--+--+-+--+-", "measure": "dactylic.di.plus"}, "line.15": {"text": "Sich ein sch\u00f6nerer Tag verbreitet: so sind doch die", "tokens": ["Sich", "ein", "sch\u00f6\u00b7ne\u00b7rer", "Tag", "ver\u00b7brei\u00b7tet", ":", "so", "sind", "doch", "die"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PRF", "ART", "ADJA", "NN", "VVPP", "$.", "ADV", "VAFIN", "ADV", "ART"], "meter": "--+--+-+--+-+", "measure": "anapaest.di.plus"}, "line.16": {"text": "Die du, Melancholey, mir ertheilst, viel reiner, viel", "tokens": ["Die", "du", ",", "Me\u00b7lan\u00b7cho\u00b7ley", ",", "mir", "er\u00b7theilst", ",", "viel", "rei\u00b7ner", ",", "viel"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ART", "PPER", "$,", "NN", "$,", "PPER", "VVFIN", "$,", "PIAT", "ADJA", "$,", "ADV"], "meter": "--+--+--+-+-+", "measure": "anapaest.tri.plus"}, "line.17": {"text": "Als ihr fl\u00fcchtiger Tand; die Freuden, tiefer gef\u00fchlet,", "tokens": ["Als", "ihr", "fl\u00fcch\u00b7ti\u00b7ger", "Tand", ";", "die", "Freu\u00b7den", ",", "tie\u00b7fer", "ge\u00b7f\u00fch\u00b7let", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "$.", "ART", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.18": {"text": "Die in einsamen Stunden die hohe Betrachtung uns", "tokens": ["Die", "in", "ein\u00b7sa\u00b7men", "Stun\u00b7den", "die", "ho\u00b7he", "Be\u00b7trach\u00b7tung", "uns"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "PPER"], "meter": "--+--+--+--+-+", "measure": "anapaest.tetra.plus"}}}}}