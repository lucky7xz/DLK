{"textgrid.poem.33771": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Walpurgisnacht vorbei!", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Walpurgisnacht vorbei!", "tokens": ["Wal\u00b7pur\u00b7gis\u00b7nacht", "vor\u00b7bei", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Es st\u00fcrmt und wetterleuchtet,", "tokens": ["Es", "st\u00fcrmt", "und", "wet\u00b7ter\u00b7leuch\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Den Einzug h\u00e4lt der Mai,", "tokens": ["Den", "Ein\u00b7zug", "h\u00e4lt", "der", "Mai", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Von D\u00e4mmergrau'n umfeuchtet.", "tokens": ["Von", "D\u00e4m\u00b7mer\u00b7grau'n", "um\u00b7feuch\u00b7tet", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "An Felsen Flutgeroll,", "tokens": ["An", "Fel\u00b7sen", "Flut\u00b7ge\u00b7roll", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Verglimmend Sterngefunkel,", "tokens": ["Ver\u00b7glim\u00b7mend", "Stern\u00b7ge\u00b7fun\u00b7kel", ","], "token_info": ["word", "word", "punct"], "pos": ["VVPP", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Im Wald schl\u00e4gt sehnsuchtsvoll", "tokens": ["Im", "Wald", "schl\u00e4gt", "sehn\u00b7suchts\u00b7voll"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die Drossel tief im Dunkel.", "tokens": ["Die", "Dros\u00b7sel", "tief", "im", "Dun\u00b7kel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die Windfahn' kr\u00e4chzt am Dach,", "tokens": ["Die", "Wind\u00b7fahn'", "kr\u00e4chzt", "am", "Dach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Der Uhu im Gekl\u00fcfte;", "tokens": ["Der", "U\u00b7hu", "im", "Ge\u00b7kl\u00fcf\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Was wispert wie ein Ach", "tokens": ["Was", "wis\u00b7pert", "wie", "ein", "Ach"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "KOKOM", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Verhallend in die L\u00fcfte?", "tokens": ["Ver\u00b7hal\u00b7lend", "in", "die", "L\u00fcf\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ein Hexchen ist's, die just", "tokens": ["Ein", "Hex\u00b7chen", "ist's", ",", "die", "just"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VAFIN", "$,", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Vom Blocksberg heimgefahren,", "tokens": ["Vom", "Blocks\u00b7berg", "heim\u00b7ge\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Beschneit die volle Brust", "tokens": ["Be\u00b7schneit", "die", "vol\u00b7le", "Brust"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und Bl\u00fcten in den Haaren.", "tokens": ["Und", "Bl\u00fc\u00b7ten", "in", "den", "Haa\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Am gr\u00fcnen Fensterbrett", "tokens": ["Am", "gr\u00fc\u00b7nen", "Fens\u00b7ter\u00b7brett"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Da duften die Violen;", "tokens": ["Da", "duf\u00b7ten", "die", "Vi\u00b7o\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie wirft sich auf ihr Bett", "tokens": ["Sie", "wirft", "sich", "auf", "ihr", "Bett"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Mit schwerem Atemholen.", "tokens": ["Mit", "schwe\u00b7rem", "A\u00b7tem\u00b7ho\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Die H\u00e4ndchen ruhn im Scho\u00df,", "tokens": ["Die", "H\u00e4nd\u00b7chen", "ruhn", "im", "Scho\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ein Schleier h\u00e4ngt zerrissen", "tokens": ["Ein", "Schlei\u00b7er", "h\u00e4ngt", "zer\u00b7ris\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Um ihr Gesichtchen los,", "tokens": ["Um", "ihr", "Ge\u00b7sicht\u00b7chen", "los", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Sie dr\u00fcckt es in die Kissen.", "tokens": ["Sie", "dr\u00fcckt", "es", "in", "die", "Kis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Am Tisch brennt, tief im Docht,", "tokens": ["Am", "Tisch", "brennt", ",", "tief", "im", "Docht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Von gestern noch die Kerze,", "tokens": ["Von", "ge\u00b7stern", "noch", "die", "Ker\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADV", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Ihr Herzchen pocht, es pocht", "tokens": ["Ihr", "Herz\u00b7chen", "pocht", ",", "es", "pocht"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "PPER", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "In wildem Liebesschmerze.", "tokens": ["In", "wil\u00b7dem", "Lie\u00b7bes\u00b7schmer\u00b7ze", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Verschlafen kr\u00e4ht der Hahn,", "tokens": ["Ver\u00b7schla\u00b7fen", "kr\u00e4ht", "der", "Hahn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ein Blitz noch, und ein tr\u00fcber,", "tokens": ["Ein", "Blitz", "noch", ",", "und", "ein", "tr\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "KON", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Umw\u00f6lbter Tag bricht an \u2013", "tokens": ["Um\u00b7w\u00f6lb\u00b7ter", "Tag", "bricht", "an", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PTKVZ", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Walpurgisnacht vor\u00fcber!", "tokens": ["Wal\u00b7pur\u00b7gis\u00b7nacht", "vor\u00b7\u00fc\u00b7ber", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}