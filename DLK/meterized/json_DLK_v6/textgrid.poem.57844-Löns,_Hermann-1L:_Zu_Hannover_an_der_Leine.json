{"textgrid.poem.57844": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zu Hannover an der Leine", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu Hannover an der Leine", "tokens": ["Zu", "Han\u00b7no\u00b7ver", "an", "der", "Lei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stand ich als Kanonier;", "tokens": ["Stand", "ich", "als", "Ka\u00b7no\u00b7nier", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOUS", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du allersch\u00f6nste Rosa,", "tokens": ["Du", "al\u00b7ler\u00b7sch\u00f6ns\u00b7te", "Ro\u00b7sa", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Jetzt mu\u00df ich fort von dir.", "tokens": ["Jetzt", "mu\u00df", "ich", "fort", "von", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PTKVZ", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Auf der L\u00fcneburger Heide,", "tokens": ["Auf", "der", "L\u00fc\u00b7ne\u00b7bur\u00b7ger", "Hei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da geht der Staub so dicht;", "tokens": ["Da", "geht", "der", "Staub", "so", "dicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du allersch\u00f6nste Rosa,", "tokens": ["Du", "al\u00b7ler\u00b7sch\u00f6ns\u00b7te", "Ro\u00b7sa", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich vergesse dich nicht.", "tokens": ["Ich", "ver\u00b7ges\u00b7se", "dich", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Zu Munster in dem Lager,", "tokens": ["Zu", "Muns\u00b7ter", "in", "dem", "La\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da lebt es sich so frei;", "tokens": ["Da", "lebt", "es", "sich", "so", "frei", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du allersch\u00f6nste Rosa,", "tokens": ["Du", "al\u00b7ler\u00b7sch\u00f6ns\u00b7te", "Ro\u00b7sa", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich bleib' dir immer treu.", "tokens": ["Ich", "bleib'", "dir", "im\u00b7mer", "treu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Zu Celle an der Aller,", "tokens": ["Zu", "Cel\u00b7le", "an", "der", "Al\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da lag ich im Quartier;", "tokens": ["Da", "lag", "ich", "im", "Quar\u00b7tier", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Du allersch\u00f6nste Rosa,", "tokens": ["Du", "al\u00b7ler\u00b7sch\u00f6ns\u00b7te", "Ro\u00b7sa", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mein Herz, das ist bei dir.", "tokens": ["Mein", "Herz", ",", "das", "ist", "bei", "dir", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PDS", "VAFIN", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Zu Hildesheim im Biwack,", "tokens": ["Zu", "Hil\u00b7des\u00b7heim", "im", "Bi\u00b7wack", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPRART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Da war so kalt die Nacht;", "tokens": ["Da", "war", "so", "kalt", "die", "Nacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du allersch\u00f6nste Rosa,", "tokens": ["Du", "al\u00b7ler\u00b7sch\u00f6ns\u00b7te", "Ro\u00b7sa", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dein hab' ich stets gedacht.", "tokens": ["Dein", "hab'", "ich", "stets", "ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und morgen, da hei\u00dft es,", "tokens": ["Und", "mor\u00b7gen", ",", "da", "hei\u00dft", "es", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "ADV", "VVFIN", "PPER", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Da hei\u00dft's geschieden sein;", "tokens": ["Da", "hei\u00dft's", "ge\u00b7schie\u00b7den", "sein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "VVPP", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du allersch\u00f6nste Rosa,", "tokens": ["Du", "al\u00b7ler\u00b7sch\u00f6ns\u00b7te", "Ro\u00b7sa", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mein Herz bleibt ewig dein.", "tokens": ["Mein", "Herz", "bleibt", "e\u00b7wig", "dein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}