{"textgrid.poem.63366": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Das Meer", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich schwelle in meiner Flut \u00fcber die Erde.", "tokens": ["Ich", "schwel\u00b7le", "in", "mei\u00b7ner", "Flut", "\u00fc\u00b7ber", "die", "Er\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+-+---+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Es wirft meine wilde Welle Tang an den Strand,", "tokens": ["Es", "wirft", "mei\u00b7ne", "wil\u00b7de", "Wel\u00b7le", "Tang", "an", "den", "Strand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Muscheln, violette Quallen und kleine Seepferde.", "tokens": ["Mu\u00b7scheln", ",", "vi\u00b7o\u00b7let\u00b7te", "Qual\u00b7len", "und", "klei\u00b7ne", "See\u00b7pfer\u00b7de", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "----+-+--+-++-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Aber der Ekel zischt, da\u00df ich mich gezeigt.", "tokens": ["A\u00b7ber", "der", "E\u00b7kel", "zischt", ",", "da\u00df", "ich", "mich", "ge\u00b7zeigt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$,", "KOUS", "PPER", "PRF", "VVPP", "$."], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Ich krieche in mich zur\u00fcck,", "tokens": ["Ich", "krie\u00b7che", "in", "mich", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und der Nordwind schweigt.", "tokens": ["Und", "der", "Nord\u00b7wind", "schweigt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$."], "meter": "--+-+", "measure": "anapaest.init"}}, "stanza.3": {"line.1": {"text": "Ebbe ist... Kinder gehen, sammeln, suchen", "tokens": ["Eb\u00b7be", "ist", "...", "Kin\u00b7der", "ge\u00b7hen", ",", "sam\u00b7meln", ",", "su\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word"], "pos": ["NN", "VAFIN", "$(", "NN", "VVINF", "$,", "VVFIN", "$,", "VVFIN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Und sehen Krabben, nasse Sterne,", "tokens": ["Und", "se\u00b7hen", "Krab\u00b7ben", ",", "nas\u00b7se", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Erstaunlichstes Getier.", "tokens": ["Er\u00b7staun\u00b7lichs\u00b7tes", "Ge\u00b7tier", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ich aber bin l\u00e4ngst in der Ferne wieder bei mir.", "tokens": ["Ich", "a\u00b7ber", "bin", "l\u00e4ngst", "in", "der", "Fer\u00b7ne", "wie\u00b7der", "bei", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ADV", "APPR", "ART", "NN", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}}, "stanza.5": {"line.1": {"text": "Und was ich an den Strand warf, stirbt in der Luft", "tokens": ["Und", "was", "ich", "an", "den", "Strand", "warf", ",", "stirbt", "in", "der", "Luft"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PWS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Oder in des Menschen Hand. \u2013", "tokens": ["O\u00b7der", "in", "des", "Men\u00b7schen", "Hand", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur die Taschenkrebse graben sich", "tokens": ["Nur", "die", "Ta\u00b7schen\u00b7kreb\u00b7se", "gra\u00b7ben", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN", "PRF"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Mit ihren Scheren in den Sand.", "tokens": ["Mit", "ih\u00b7ren", "Sche\u00b7ren", "in", "den", "Sand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sechs Stunden warten sie bis zur n\u00e4chsten Flut. \u2013", "tokens": ["Sechs", "Stun\u00b7den", "war\u00b7ten", "sie", "bis", "zur", "n\u00e4chs\u00b7ten", "Flut", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["CARD", "NN", "VVFIN", "PPER", "ADV", "APPRART", "ADJA", "NN", "$.", "$("], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Die Taschenkrebse kennen mich gut.", "tokens": ["Die", "Ta\u00b7schen\u00b7kreb\u00b7se", "ken\u00b7nen", "mich", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}}}}