{"textgrid.poem.62383": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "1L: An dem Fest der Sonnenh\u00f6he", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "An dem Fest der Sonnenh\u00f6he", "tokens": ["An", "dem", "Fest", "der", "Son\u00b7nen\u00b7h\u00f6\u00b7he"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wall' ich hin zu dir, o Stein,", "tokens": ["Wall'", "ich", "hin", "zu", "dir", ",", "o", "Stein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "APPR", "PPER", "$,", "FM", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df mich alte Luft umwehe", "tokens": ["Da\u00df", "mich", "al\u00b7te", "Luft", "um\u00b7we\u00b7he"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In dem schauerlichen Hain.", "tokens": ["In", "dem", "schau\u00b7er\u00b7li\u00b7chen", "Hain", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wo die tapfern V\u00e4ter knieten", "tokens": ["Wo", "die", "tap\u00b7fern", "V\u00e4\u00b7ter", "knie\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Demuthsvoll im starken Muth,", "tokens": ["De\u00b7muths\u00b7voll", "im", "star\u00b7ken", "Muth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.3": {"text": "Hell die Freudenfeuer gl\u00fchten,", "tokens": ["Hell", "die", "Freu\u00b7den\u00b7feu\u00b7er", "gl\u00fch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Heller ihres Herzens Gluth,", "tokens": ["Hel\u00b7ler", "ih\u00b7res", "Her\u00b7zens", "Gluth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Seh' ich noch die Geister wallen", "tokens": ["Seh'", "ich", "noch", "die", "Geis\u00b7ter", "wal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Feiernd in der Sommernacht, \u2013", "tokens": ["Fei\u00b7ernd", "in", "der", "Som\u00b7mer\u00b7nacht", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$,", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nein, es kann nicht ganz zerfallen,", "tokens": ["Nein", ",", "es", "kann", "nicht", "ganz", "zer\u00b7fal\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VMFIN", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was ein frommer Mensch gedacht.", "tokens": ["Was", "ein", "from\u00b7mer", "Mensch", "ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}