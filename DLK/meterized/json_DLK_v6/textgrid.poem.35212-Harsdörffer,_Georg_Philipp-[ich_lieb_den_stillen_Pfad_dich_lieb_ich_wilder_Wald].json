{"textgrid.poem.35212": {"metadata": {"author": {"name": "Harsd\u00f6rffer, Georg Philipp", "birth": "N.A.", "death": "N.A."}, "title": "[ich lieb den stillen Pfad/ dich lieb ich/ wilder Wald]", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich lieb den stillen Pfad/ dich lieb ich/ wilder Wald/", "tokens": ["Ich", "lieb", "den", "stil\u00b7len", "Pfad", "/", "dich", "lieb", "ich", "/", "wil\u00b7der", "Wald", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ART", "ADJA", "NN", "$(", "PPER", "VVFIN", "PPER", "$(", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Entfernet von Gepl\u00e4rr/ der Einfalt Aufenthalt.", "tokens": ["Ent\u00b7fer\u00b7net", "von", "Ge\u00b7pl\u00e4rr", "/", "der", "Ein\u00b7falt", "Auf\u00b7ent\u00b7halt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$(", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Hier hat kein Wagenrad das Sorgengeld geh\u00e4uffet/", "tokens": ["Hier", "hat", "kein", "Wa\u00b7gen\u00b7rad", "das", "Sor\u00b7gen\u00b7geld", "ge\u00b7h\u00e4uf\u00b7fet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Fisch in diesem Deich wie ich/ in Ruhe schweiffet.", "tokens": ["Der", "Fisch", "in", "die\u00b7sem", "Deich", "wie", "ich", "/", "in", "Ru\u00b7he", "schweif\u00b7fet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PDAT", "NN", "KOKOM", "PPER", "$(", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es hat kein Wandersmann betreten diesen Platz/", "tokens": ["Es", "hat", "kein", "Wan\u00b7ders\u00b7mann", "be\u00b7tre\u00b7ten", "die\u00b7sen", "Platz", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "VVFIN", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Erhaben aus der Qwell der Perlenmutter Schatz.", "tokens": ["Er\u00b7ha\u00b7ben", "aus", "der", "Qwell", "der", "Per\u00b7len\u00b7mut\u00b7ter", "Schatz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Kein leichtgef\u00fcstes Reh hat Bezoar getrehnet/", "tokens": ["Kein", "leicht\u00b7ge\u00b7f\u00fcs\u00b7tes", "Reh", "hat", "Be\u00b7zoar", "ge\u00b7treh\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VAFIN", "NN", "VVPP", "$("], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Noch in dem dikken Busch sich nach der Lust gesehnet.", "tokens": ["Noch", "in", "dem", "dik\u00b7ken", "Busch", "sich", "nach", "der", "Lust", "ge\u00b7seh\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "PRF", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Es hegt in jenem Schlo\u00df kein List und Meucheltrug/", "tokens": ["Es", "hegt", "in", "je\u00b7nem", "Schlo\u00df", "kein", "List", "und", "Meu\u00b7chel\u00b7trug", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PDAT", "NN", "PIAT", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Da nur die Fledermau\u00df im Dunkeln hebt den Flug.", "tokens": ["Da", "nur", "die", "Fle\u00b7der\u00b7mau\u00df", "im", "Dun\u00b7keln", "hebt", "den", "Flug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "APPRART", "ADJA", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Das unverschlossne Haus heist offnes Hertzens trauen/", "tokens": ["Das", "un\u00b7ver\u00b7schloss\u00b7ne", "Haus", "heist", "off\u00b7nes", "Hert\u00b7zens", "trau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Der Last hat seinen Grund die Palmen machen schauen.", "tokens": ["Der", "Last", "hat", "sei\u00b7nen", "Grund", "die", "Pal\u00b7men", "ma\u00b7chen", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "ART", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Das K\u00e4utzlein unn der Dachs bewohnt den \u00f6den Sand/", "tokens": ["Das", "K\u00e4utz\u00b7lein", "unn", "der", "Dachs", "be\u00b7wohnt", "den", "\u00f6\u00b7den", "Sand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Es dekkt das Marderthier der Sorgen-Marterstand.", "tokens": ["Es", "de\u00b7kkt", "das", "Mar\u00b7der\u00b7thier", "der", "Sor\u00b7gen\u00b7Mar\u00b7ter\u00b7stand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.15": {"text": "Im Keller findet man noch Wein noch Bier z\u016b b\u00e4st\u0113", "tokens": ["Im", "Kel\u00b7ler", "fin\u00b7det", "man", "noch", "Wein", "noch", "Bier", "z\u016b", "b\u00e4s\u00b7t\u0113"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PIS", "ADV", "NN", "ADV", "NN", "XY", "XY"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.16": {"text": "Die Kr\u00f6ten samt der Maus sind von den st\u00e4ten G\u00e4sten.", "tokens": ["Die", "Kr\u00f6\u00b7ten", "samt", "der", "Maus", "sind", "von", "den", "st\u00e4\u00b7ten", "G\u00e4s\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VAFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ein Nusbaum w\u00e4chset dort mit Capuciner-Kost/", "tokens": ["Ein", "Nus\u00b7baum", "w\u00e4ch\u00b7set", "dort", "mit", "Capu\u00b7ci\u00b7ner\u00b7Kost", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "NN", "$("], "meter": "-+--++-+--+", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "Er stehet Wurtzelf\u00e4st/ das Wasser ist der Most.", "tokens": ["Er", "ste\u00b7het", "Wurt\u00b7zel\u00b7f\u00e4st", "/", "das", "Was\u00b7ser", "ist", "der", "Most", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$(", "ART", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Der d\u00fcsterrauhe Wald hegt die bejahrten Eichen/", "tokens": ["Der", "d\u00fcs\u00b7ter\u00b7rau\u00b7he", "Wald", "hegt", "die", "be\u00b7jahr\u00b7ten", "Ei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Dem nie-gepfl\u00fcgten Ort/ das \u00e4ltste Tr\u00e4id zu reichen.", "tokens": ["Dem", "nie\u00b7ge\u00b7pfl\u00fcg\u00b7ten", "Ort", "/", "das", "\u00e4lts\u00b7te", "Tr\u00e4id", "zu", "rei\u00b7chen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Wie nennet man den Flu\u00df? von der Vergessenheit/", "tokens": ["Wie", "nen\u00b7net", "man", "den", "Flu\u00df", "?", "von", "der", "Ver\u00b7ges\u00b7sen\u00b7heit", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PIS", "ART", "NN", "$.", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Sein Abfall dienet mir in mancher Sorgen Leid.", "tokens": ["Sein", "Ab\u00b7fall", "die\u00b7net", "mir", "in", "man\u00b7cher", "Sor\u00b7gen", "Leid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "APPR", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Ist dann der Schattenwald zur Einfalt-Ruh geheget?", "tokens": ["Ist", "dann", "der", "Schat\u00b7ten\u00b7wald", "zur", "Ein\u00b7fal\u00b7tRuh", "ge\u00b7he\u00b7get", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Sein gr\u00fcnbelaubter Thron noch Kron noch Purpur tr\u00e4get.", "tokens": ["Sein", "gr\u00fcn\u00b7be\u00b7laub\u00b7ter", "Thron", "noch", "Kron", "noch", "Pur\u00b7pur", "tr\u00e4\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADV", "NN", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "H\u00f6r/ leichtes Felsen-Kind/ was ist die schwerste Pein?", "tokens": ["H\u00f6r", "/", "leich\u00b7tes", "Fel\u00b7sen\u00b7Kind", "/", "was", "ist", "die", "schwers\u00b7te", "Pein", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "ADJA", "NN", "$(", "PWS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Der gelblich-gr\u00fcne Frosch spricht: falscher Warheitschein", "tokens": ["Der", "gelb\u00b7lich\u00b7gr\u00fc\u00b7ne", "Frosch", "spricht", ":", "fal\u00b7scher", "War\u00b7heit\u00b7schein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$.", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Mich d\u00fcnkt in dieser Gruft solt man geruhig greissen.", "tokens": ["Mich", "d\u00fcnkt", "in", "die\u00b7ser", "Gruft", "solt", "man", "ge\u00b7ru\u00b7hig", "greis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PDAT", "NN", "VMFIN", "PIS", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Die pfleget meiner Pfeiff den Gegenhall zu weissen.", "tokens": ["Die", "pfle\u00b7get", "mei\u00b7ner", "Pfeiff", "den", "Ge\u00b7gen\u00b7hall", "zu", "weis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Ich liebe diesen Ort/ der sonder Hofarts-pracht", "tokens": ["Ich", "lie\u00b7be", "die\u00b7sen", "Ort", "/", "der", "son\u00b7der", "Hof\u00b7arts\u00b7pracht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PDAT", "NN", "$(", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Mich auf so \u00f6dem Weg mir selbst selb-eigen macht.", "tokens": ["Mich", "auf", "so", "\u00f6\u00b7dem", "Weg", "mir", "selbst", "selb\u00b7\u00b7ei\u00b7gen", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADV", "ADJA", "NN", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Es \u00fcberschattet mich mein unversehrt Gewissen/", "tokens": ["Es", "\u00fc\u00b7bersc\u00b7hat\u00b7tet", "mich", "mein", "un\u00b7ver\u00b7sehrt", "Ge\u00b7wis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "ADJD", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Wo sich mit dem Gesang die Freuden reich ergiessen.", "tokens": ["Wo", "sich", "mit", "dem", "Ge\u00b7sang", "die", "Freu\u00b7den", "reich", "er\u00b7gies\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "APPR", "ART", "NN", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Von welcher Brunstbegierd entstehet nicht die Reu?", "tokens": ["Von", "wel\u00b7cher", "Brunst\u00b7be\u00b7gierd", "ent\u00b7ste\u00b7het", "nicht", "die", "Reu", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "VVFIN", "PTKNEG", "ART", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "H\u00f6rt/ wies im Thal erklingt Wann f\u00e4lt das Steingeb\u00e4u?", "tokens": ["H\u00f6rt", "/", "wies", "im", "Thal", "er\u00b7klingt", "Wann", "f\u00e4lt", "das", "Stein\u00b7ge\u00b7b\u00e4u", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$(", "VVFIN", "APPRART", "NN", "VVFIN", "PWAV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Wie? redet auch der Stein? so r\u00fchr ich auch die S\u00e4iten/", "tokens": ["Wie", "?", "re\u00b7det", "auch", "der", "Stein", "?", "so", "r\u00fchr", "ich", "auch", "die", "S\u00e4i\u00b7ten", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "VVFIN", "ADV", "ART", "NN", "$.", "ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Da\u00df meiner Fl\u00f6ten Spiel mu\u00df mit dem Echo streiten:", "tokens": ["Da\u00df", "mei\u00b7ner", "Fl\u00f6\u00b7ten", "Spiel", "mu\u00df", "mit", "dem", "E\u00b7cho", "strei\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "VMFIN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrin