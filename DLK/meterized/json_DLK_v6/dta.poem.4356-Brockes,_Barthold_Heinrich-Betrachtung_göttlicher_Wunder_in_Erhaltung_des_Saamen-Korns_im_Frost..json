{"dta.poem.4356": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtung g\u00f6ttlicher Wunder  \n in  \n  Erhaltung des Saamen-Korns  \n im Frost.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nicht minder zu verderben, st\u00fcnde. Es sehe denn doch", "tokens": ["Nicht", "min\u00b7der", "zu", "ver\u00b7der\u00b7ben", ",", "st\u00fcn\u00b7de", ".", "Es", "se\u00b7he", "denn", "doch"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "PTKZU", "VVFIN", "$,", "VVFIN", "$.", "PPER", "VVFIN", "ADV", "ADV"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "jedermann", "tokens": ["je\u00b7der\u00b7mann"], "token_info": ["word"], "pos": ["PIS"], "meter": "+--", "measure": "dactylic.init"}, "line.3": {"text": "Die\u00df leider nicht bemerkte Wunder als wie ein wirklichs", "tokens": ["Die\u00df", "lei\u00b7der", "nicht", "be\u00b7merk\u00b7te", "Wun\u00b7der", "als", "wie", "ein", "wirk\u00b7lichs"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "PTKNEG", "VVFIN", "NN", "KOUS", "KOKOM", "ART", "ADJA"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Wunder an!", "tokens": ["Wun\u00b7der", "an", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Erkenn\u2019 in dieser weisen Vorsicht des Sch\u00f6pfers Weis-", "tokens": ["Er\u00b7kenn'", "in", "die\u00b7ser", "wei\u00b7sen", "Vor\u00b7sicht", "des", "Sch\u00f6p\u00b7fers", "Weis"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PDAT", "ADJA", "NN", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "heit, Macht und Liebe,", "tokens": ["heit", ",", "Macht", "und", "Lie\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Und da\u00df Jhn nichts als Seine Huld f\u00fcr uns auch hier", "tokens": ["Und", "da\u00df", "Jhn", "nichts", "als", "Sei\u00b7ne", "Huld", "f\u00fcr", "uns", "auch", "hier"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "PIS", "KOKOM", "PPOSAT", "NN", "APPR", "PPER", "ADV", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "zu sorgen triebe.", "tokens": ["zu", "sor\u00b7gen", "trie\u00b7be", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}