{"textgrid.poem.63335": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Der Romanschriftsteller", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Graugelb ist sein Gesicht. Die Nase", "tokens": ["Grau\u00b7gelb", "ist", "sein", "Ge\u00b7sicht", ".", "Die", "Na\u00b7se"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "VAFIN", "PPOSAT", "NN", "$.", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Steigt klippenspitz empor. Die Augen liegen fleckig", "tokens": ["Steigt", "klip\u00b7pen\u00b7spitz", "em\u00b7por", ".", "Die", "Au\u00b7gen", "lie\u00b7gen", "flec\u00b7kig"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "PTKVZ", "$.", "ART", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.3": {"text": "Mi\u00dftrauisch von den Wimpern tief beschattet,", "tokens": ["Mi\u00df\u00b7trau\u00b7isch", "von", "den", "Wim\u00b7pern", "tief", "be\u00b7schat\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Geduckt zum Sprung wie Panther in der H\u00f6hlung.", "tokens": ["Ge\u00b7duckt", "zum", "Sprung", "wie", "Pan\u00b7ther", "in", "der", "H\u00f6h\u00b7lung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "NN", "KOKOM", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Der rechte Arm mit der Zigarre steht", "tokens": ["Der", "rech\u00b7te", "Arm", "mit", "der", "Zi\u00b7gar\u00b7re", "steht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Steif wie ein Schwert, als wolle er damit", "tokens": ["Steif", "wie", "ein", "Schwert", ",", "als", "wol\u00b7le", "er", "da\u00b7mit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "KOKOM", "ART", "NN", "$,", "KOUS", "VMFIN", "PPER", "PAV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Sich von den andern sondern, die ihm widerw\u00e4rtig", "tokens": ["Sich", "von", "den", "an\u00b7dern", "son\u00b7dern", ",", "die", "ihm", "wi\u00b7der\u00b7w\u00e4r\u00b7tig"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PRF", "APPR", "ART", "ADJA", "KON", "$,", "PRELS", "PPER", "ADJD"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und dennoch so sympathisch sind.", "tokens": ["Und", "den\u00b7noch", "so", "sym\u00b7pa\u00b7thisch", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADJD", "VAFIN", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.9": {"text": "Schl\u00e4gt er die Asche ab,", "tokens": ["Schl\u00e4gt", "er", "die", "A\u00b7sche", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "So f\u00e4llt wie Hohn sie aufs Gespr\u00e4ch.", "tokens": ["So", "f\u00e4llt", "wie", "Hohn", "sie", "aufs", "Ge\u00b7spr\u00e4ch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KOKOM", "NN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Ein kurzes \u00bbJa\u00ab, ein scharfes \u00bbNein\u00ab", "tokens": ["Ein", "kur\u00b7zes", "\u00bb", "Ja", "\u00ab", ",", "ein", "schar\u00b7fes", "\u00bb", "Nein", "\u00ab"], "token_info": ["word", "word", "punct", "word", "punct", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "$(", "PTKANT", "$(", "$,", "ART", "ADJA", "$(", "PTKANT", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Wirft er zuweilen in die Unterhaltung.", "tokens": ["Wirft", "er", "zu\u00b7wei\u00b7len", "in", "die", "Un\u00b7ter\u00b7hal\u00b7tung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Mit diesem spitzen \u00bbJa\u00ab und \u00bbNein\u00ab", "tokens": ["Mit", "die\u00b7sem", "spit\u00b7zen", "\u00bb", "Ja", "\u00ab", "und", "\u00bb", "Nein", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "PDAT", "VVINF", "$(", "PTKANT", "$(", "KON", "$(", "PTKANT", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Spie\u00dft er die Leute wie auf Nadeln auf", "tokens": ["Spie\u00dft", "er", "die", "Leu\u00b7te", "wie", "auf", "Na\u00b7deln", "auf"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "KOKOM", "APPR", "NN", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Und nimmt sie mit nach Hause", "tokens": ["Und", "nimmt", "sie", "mit", "nach", "Hau\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "F\u00fcr seine K\u00e4fersammlung.", "tokens": ["F\u00fcr", "sei\u00b7ne", "K\u00e4\u00b7fer\u00b7samm\u00b7lung", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "\u2013 \u2013 \u2013 Schl\u00e4gt man das n\u00e4chste Buch des Dichters auf.", "tokens": ["\u2013", "\u2013", "\u2013", "Schl\u00e4gt", "man", "das", "n\u00e4chs\u00b7te", "Buch", "des", "Dich\u00b7ters", "auf", "."], "token_info": ["punct", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "$(", "$(", "VVFIN", "PIS", "ART", "ADJA", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "O Gott! Schon ist man selber drin verzeichnet", "tokens": ["O", "Gott", "!", "Schon", "ist", "man", "sel\u00b7ber", "drin", "ver\u00b7zeich\u00b7net"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "$.", "ADV", "VAFIN", "PIS", "ADV", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Und wer sich in gerechter Selbsterkenntnis", "tokens": ["Und", "wer", "sich", "in", "ge\u00b7rech\u00b7ter", "Selbs\u00b7ter\u00b7kennt\u00b7nis"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "F\u00fcr ein libellen\u00e4hnlich' Wesen hielt,", "tokens": ["F\u00fcr", "ein", "li\u00b7bel\u00b7len\u00b7\u00e4hn\u00b7lich'", "We\u00b7sen", "hielt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Der findet sich erstaunt als Mistbock wieder.", "tokens": ["Der", "fin\u00b7det", "sich", "er\u00b7staunt", "als", "Mist\u00b7bock", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "ADJD", "KOKOM", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}