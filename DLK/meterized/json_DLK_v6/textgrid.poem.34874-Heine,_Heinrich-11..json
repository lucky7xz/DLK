{"textgrid.poem.34874": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "11.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Hospital f\u00fcr arme, kranke Juden,", "tokens": ["Ein", "Hos\u00b7pi\u00b7tal", "f\u00fcr", "ar\u00b7me", ",", "kran\u00b7ke", "Ju\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "F\u00fcr Menschenkinder, welche dreifach elend,", "tokens": ["F\u00fcr", "Men\u00b7schen\u00b7kin\u00b7der", ",", "wel\u00b7che", "drei\u00b7fach", "e\u00b7lend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Behaftet mit den b\u00f6sen drei Gebresten,", "tokens": ["Be\u00b7haf\u00b7tet", "mit", "den", "b\u00f6\u00b7sen", "drei", "Ge\u00b7bres\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit Armut, K\u00f6rperschmerz und Judentume!", "tokens": ["Mit", "Ar\u00b7mut", ",", "K\u00f6r\u00b7per\u00b7schmerz", "und", "Ju\u00b7den\u00b7tu\u00b7me", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Das schlimmste von den dreien ist das letzte,", "tokens": ["Das", "schlimms\u00b7te", "von", "den", "drei\u00b7en", "ist", "das", "letz\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPR", "ART", "CARD", "VAFIN", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das tausendj\u00e4hrige Familien\u00fcbel,", "tokens": ["Das", "tau\u00b7send\u00b7j\u00e4h\u00b7ri\u00b7ge", "Fa\u00b7mi\u00b7li\u00b7en\u00b7\u00fc\u00b7bel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+---+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die aus dem Niltal mitgeschleppte Plage,", "tokens": ["Die", "aus", "dem", "Nil\u00b7tal", "mit\u00b7ge\u00b7schlepp\u00b7te", "Pla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der alt\u00e4gyptisch ungesunde Glauben.", "tokens": ["Der", "al\u00b7t\u00e4\u00b7gypt\u00b7isch", "un\u00b7ge\u00b7sun\u00b7de", "Glau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Unheilbar tiefes Leid! Dagegen helfen", "tokens": ["Un\u00b7heil\u00b7bar", "tie\u00b7fes", "Leid", "!", "Da\u00b7ge\u00b7gen", "hel\u00b7fen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "ADJA", "NN", "$.", "PAV", "VVINF"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Nicht Dampfbad, Dusche, nicht die Apparate", "tokens": ["Nicht", "Dampf\u00b7bad", ",", "Du\u00b7sche", ",", "nicht", "die", "Ap\u00b7pa\u00b7ra\u00b7te"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "NE", "$,", "NN", "$,", "PTKNEG", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Chirurgie, noch all die Arzeneien,", "tokens": ["Der", "Chi\u00b7rur\u00b7gie", ",", "noch", "all", "die", "Ar\u00b7ze\u00b7nei\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "PIAT", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die dieses Haus den siechen G\u00e4sten bietet.", "tokens": ["Die", "die\u00b7ses", "Haus", "den", "sie\u00b7chen", "G\u00e4s\u00b7ten", "bie\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wird einst die Zeit, die ew'ge G\u00f6ttin, tilgen", "tokens": ["Wird", "einst", "die", "Zeit", ",", "die", "ew'\u00b7ge", "G\u00f6t\u00b7tin", ",", "til\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["VAFIN", "ADV", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das dunkle Weh, das sich vererbt vom Vater", "tokens": ["Das", "dunk\u00b7le", "Weh", ",", "das", "sich", "ver\u00b7erbt", "vom", "Va\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PRF", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Herunter auf den Sohn \u2013 wird einst der Enkel", "tokens": ["Her\u00b7un\u00b7ter", "auf", "den", "Sohn", "\u2013", "wird", "einst", "der", "En\u00b7kel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "$(", "VAFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Genesen und vern\u00fcnftig sein und gl\u00fccklich?", "tokens": ["Ge\u00b7ne\u00b7sen", "und", "ver\u00b7n\u00fcnf\u00b7tig", "sein", "und", "gl\u00fcck\u00b7lich", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VAINF", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ich wei\u00df es nicht! Doch mittlerweile wollen", "tokens": ["Ich", "wei\u00df", "es", "nicht", "!", "Doch", "mitt\u00b7ler\u00b7wei\u00b7le", "wol\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$.", "KON", "ADV", "VMFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wir preisen jenes Herz, das klug und liebreich", "tokens": ["Wir", "prei\u00b7sen", "je\u00b7nes", "Herz", ",", "das", "klug", "und", "lieb\u00b7reich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PDAT", "NN", "$,", "PRELS", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zu lindern suchte, was der Lindrung f\u00e4hig,", "tokens": ["Zu", "lin\u00b7dern", "such\u00b7te", ",", "was", "der", "Lind\u00b7rung", "f\u00e4\u00b7hig", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VVFIN", "$,", "PRELS", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zeitlichen Balsam tr\u00e4ufelnd in die Wunden.", "tokens": ["Zeit\u00b7li\u00b7chen", "Bal\u00b7sam", "tr\u00e4u\u00b7felnd", "in", "die", "Wun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.6": {"line.1": {"text": "Der teure Mann! Er baute hier ein Obdach", "tokens": ["Der", "teu\u00b7re", "Mann", "!", "Er", "bau\u00b7te", "hier", "ein", "Ob\u00b7dach"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "PPER", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "F\u00fcr Leiden, welche heilbar durch die K\u00fcnste", "tokens": ["F\u00fcr", "Lei\u00b7den", ",", "wel\u00b7che", "heil\u00b7bar", "durch", "die", "K\u00fcns\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "PRELS", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Des Arztes (oder auch des Todes!), sorgte", "tokens": ["Des", "Arz\u00b7tes", "(", "o\u00b7der", "auch", "des", "To\u00b7des", "!", ")", ",", "sorg\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct", "word"], "pos": ["ART", "NN", "$(", "KON", "ADV", "ART", "NN", "$.", "$(", "$,", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "F\u00fcr Polster, Labetrank, Wartung und Pflege \u2013", "tokens": ["F\u00fcr", "Pols\u00b7ter", ",", "La\u00b7be\u00b7trank", ",", "War\u00b7tung", "und", "Pfle\u00b7ge", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$("], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Ein Mann der Tat, tat er, was eben tunlich;", "tokens": ["Ein", "Mann", "der", "Tat", ",", "tat", "er", ",", "was", "e\u00b7ben", "tun\u00b7lich", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "VVFIN", "PPER", "$,", "PRELS", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "F\u00fcr gute Werke gab er hin den Taglohn", "tokens": ["F\u00fcr", "gu\u00b7te", "Wer\u00b7ke", "gab", "er", "hin", "den", "Tag\u00b7lohn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Am Abend seines Lebens, menschenfreundlich,", "tokens": ["Am", "A\u00b7bend", "sei\u00b7nes", "Le\u00b7bens", ",", "men\u00b7schen\u00b7freund\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Durch Wohltun sich erholend von der Arbeit.", "tokens": ["Durch", "Wohl\u00b7tun", "sich", "er\u00b7ho\u00b7lend", "von", "der", "Ar\u00b7beit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PRF", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Er gab mit reicher Hand \u2013 doch reichre Spende", "tokens": ["Er", "gab", "mit", "rei\u00b7cher", "Hand", "\u2013", "doch", "reich\u00b7re", "Spen\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$(", "ADV", "VVFIN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Entrollte manchmal seinem Aug', die Tr\u00e4ne,", "tokens": ["Ent\u00b7roll\u00b7te", "manch\u00b7mal", "sei\u00b7nem", "Aug'", ",", "die", "Tr\u00e4\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die kostbar sch\u00f6ne Tr\u00e4ne, die er weinte", "tokens": ["Die", "kost\u00b7bar", "sch\u00f6\u00b7ne", "Tr\u00e4\u00b7ne", ",", "die", "er", "wein\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJD", "ADJA", "NN", "$,", "PRELS", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ob der unheilbar gro\u00dfen Br\u00fcderkrankheit.", "tokens": ["Ob", "der", "un\u00b7heil\u00b7bar", "gro\u00b7\u00dfen", "Br\u00fc\u00b7der\u00b7krank\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}