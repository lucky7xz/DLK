{"dta.poem.4342": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Winter.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es f\u00fcllt die Welt, auch bey dem Frost, sich \u00f6fters mit so", "tokens": ["Es", "f\u00fcllt", "die", "Welt", ",", "auch", "bey", "dem", "Frost", ",", "sich", "\u00f6f\u00b7ters", "mit", "so"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ADV", "APPR", "ART", "NN", "$,", "PRF", "ADV", "APPR", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "sch\u00f6nen Bildern,", "tokens": ["sch\u00f6\u00b7nen", "Bil\u00b7dern", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Da\u00df ich mich nicht enthalten kann, wie sehr sie, auch im Win-", "tokens": ["Da\u00df", "ich", "mich", "nicht", "ent\u00b7hal\u00b7ten", "kann", ",", "wie", "sehr", "sie", ",", "auch", "im", "Win"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "PTKNEG", "VVINF", "VMFIN", "$,", "PWAV", "ADV", "PPER", "$,", "ADV", "APPRART", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "ter, sch\u00f6n,", "tokens": ["ter", ",", "sch\u00f6n", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "ADJD", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Dem Sch\u00f6pfer der Natur zum Preise, nicht nur aufmerksam", "tokens": ["Dem", "Sch\u00f6p\u00b7fer", "der", "Na\u00b7tur", "zum", "Prei\u00b7se", ",", "nicht", "nur", "auf\u00b7merk\u00b7sam"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "APPRART", "NN", "$,", "PTKNEG", "ADV", "ADJD"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "anzusehn;", "tokens": ["an\u00b7zu\u00b7sehn", ";"], "token_info": ["word", "punct"], "pos": ["VVIZU", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Ich mu\u00df noch eine Winter-Landschaft in meinen Liedern", "tokens": ["Ich", "mu\u00df", "noch", "ei\u00b7ne", "Win\u00b7ter\u00b7Land\u00b7schaft", "in", "mei\u00b7nen", "Lie\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "abzuschildern,", "tokens": ["ab\u00b7zu\u00b7schil\u00b7dern", ","], "token_info": ["word", "punct"], "pos": ["VVIZU", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Und zwar, so viel es m\u00f6glich ist, recht eigentlich und nach", "tokens": ["Und", "zwar", ",", "so", "viel", "es", "m\u00f6g\u00b7lich", "ist", ",", "recht", "ei\u00b7gent\u00b7lich", "und", "nach"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "ADV", "ADV", "PPER", "ADJD", "VAFIN", "$,", "ADV", "ADV", "KON", "APPR"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.10": {"text": "dem Leben,", "tokens": ["dem", "Le\u00b7ben", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.11": {"text": "So da\u00df sie auch im Sommer sichtbar, und zu bewundern,", "tokens": ["So", "da\u00df", "sie", "auch", "im", "Som\u00b7mer", "sicht\u00b7bar", ",", "und", "zu", "be\u00b7wun\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ADV", "APPRART", "NN", "ADJD", "$,", "KON", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "mich bestreben.", "tokens": ["mich", "be\u00b7stre\u00b7ben", "."], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVPP", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Der Erden Fl\u00e4ch\u2019 ist wei\u00df beschneit, die Fluht, so weit man", "tokens": ["Der", "Er\u00b7den", "Fl\u00e4ch'", "ist", "wei\u00df", "be\u00b7schneit", ",", "die", "Fluht", ",", "so", "weit", "man"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VAFIN", "VVFIN", "ADJD", "$,", "ART", "NN", "$,", "ADV", "ADJD", "PIS"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "sieht, gefroren,", "tokens": ["sieht", ",", "ge\u00b7fro\u00b7ren", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "VVPP", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Und ganz mit klarem Eis erf\u00fcllt. Dadurch erblickt man", "tokens": ["Und", "ganz", "mit", "kla\u00b7rem", "Eis", "er\u00b7f\u00fcllt", ".", "Da\u00b7durch", "er\u00b7blickt", "man"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN", "VVPP", "$.", "PAV", "VVFIN", "PIS"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u00fcberall,", "tokens": ["\u00fc\u00b7be\u00b7rall", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Wohin man sieht, dem Ansehn nach, fast nichts als Silber", "tokens": ["Wo\u00b7hin", "man", "sieht", ",", "dem", "An\u00b7sehn", "nach", ",", "fast", "nichts", "als", "Sil\u00b7ber"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "VVFIN", "$,", "ART", "NN", "PTKVZ", "$,", "ADV", "PIS", "KOKOM", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "und Krystall.", "tokens": ["und", "Krys\u00b7tall", "."], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$."], "meter": "-++", "measure": "unknown.measure.di"}, "line.7": {"text": "Wann nun an einem heitern Tag die scharfen Winde sich", "tokens": ["Wann", "nun", "an", "ei\u00b7nem", "hei\u00b7tern", "Tag", "die", "schar\u00b7fen", "Win\u00b7de", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "PRF"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.8": {"text": "verlohren,", "tokens": ["ver\u00b7loh\u00b7ren", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.9": {"text": "Und man sodann im Sonnenschein, wenn sie aufs aller-", "tokens": ["Und", "man", "so\u00b7dann", "im", "Son\u00b7nen\u00b7schein", ",", "wenn", "sie", "aufs", "al\u00b7ler"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PIS", "ADV", "APPRART", "NN", "$,", "KOUS", "PPER", "APPRART", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "h\u00f6chste steht,", "tokens": ["h\u00f6chs\u00b7te", "steht", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.11": {"text": "Am Ufer, oder auf dem Eise, bedachtsamlich spatzieren", "tokens": ["Am", "U\u00b7fer", ",", "o\u00b7der", "auf", "dem", "Ei\u00b7se", ",", "be\u00b7dacht\u00b7sam\u00b7lich", "spat\u00b7zie\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "$,", "KON", "APPR", "ART", "NN", "$,", "ADJD", "VVINF"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.12": {"text": "geht;", "tokens": ["geht", ";"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-", "measure": "single.down"}, "line.13": {"text": "Gl\u00e4nzt alles, was man um sich sieht, in tausendfach", "tokens": ["Gl\u00e4nzt", "al\u00b7les", ",", "was", "man", "um", "sich", "sieht", ",", "in", "tau\u00b7send\u00b7fach"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PIS", "$,", "PRELS", "PIS", "APPR", "PRF", "VVFIN", "$,", "APPR", "NE"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "gebrochnem Licht,", "tokens": ["ge\u00b7broch\u00b7nem", "Licht", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.15": {"text": "So da\u00df den fast durchstrahlten Augen, dem fast geblendeten", "tokens": ["So", "da\u00df", "den", "fast", "durch\u00b7strahl\u00b7ten", "Au\u00b7gen", ",", "dem", "fast", "ge\u00b7blen\u00b7de\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "KOUS", "ART", "ADV", "ADJA", "NN", "$,", "PRELS", "ADV", "ADJA"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.16": {"text": "Gesicht,", "tokens": ["Ge\u00b7sicht", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+", "measure": "iambic.single"}}, "stanza.3": {"line.1": {"text": "Kein Vorwurf sch\u00f6ner scheinen kann. Von angestrahlten", "tokens": ["Kein", "Vor\u00b7wurf", "sch\u00f6\u00b7ner", "schei\u00b7nen", "kann", ".", "Von", "an\u00b7ge\u00b7strahl\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PIAT", "NN", "ADJD", "VVINF", "VMFIN", "$.", "APPR", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Eises Spitzen", "tokens": ["Ei\u00b7ses", "Spit\u00b7zen"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Erhebet sich, an tausend Stellen, ein helles bunt-gef\u00e4rbtes", "tokens": ["Er\u00b7he\u00b7bet", "sich", ",", "an", "tau\u00b7send", "Stel\u00b7len", ",", "ein", "hel\u00b7les", "bunt\u00b7ge\u00b7f\u00e4rb\u00b7tes"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PRF", "$,", "APPR", "CARD", "NN", "$,", "ART", "ADJA", "ADJA"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Blitzen,", "tokens": ["Blit\u00b7zen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Wenn nemlich, an so vielen Ecken, das aufgefangne Son-", "tokens": ["Wenn", "nem\u00b7lich", ",", "an", "so", "vie\u00b7len", "E\u00b7cken", ",", "das", "auf\u00b7ge\u00b7fang\u00b7ne", "Son"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "ADV", "$,", "APPR", "ADV", "PIAT", "NN", "$,", "ART", "ADJA", "TRUNC"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.6": {"text": "nen-Licht,", "tokens": ["nen\u00b7Licht", ","], "token_info": ["word", "punct"], "pos": ["PTKANT", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.7": {"text": "In schnell zur\u00fcckgeschickten Strahlen, und reinem Glanz,", "tokens": ["In", "schnell", "zu\u00b7r\u00fcck\u00b7ge\u00b7schick\u00b7ten", "Strah\u00b7len", ",", "und", "rei\u00b7nem", "Glanz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$,", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "sich funkelnd bricht.", "tokens": ["sich", "fun\u00b7kelnd", "bricht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Wenn man in einer Landschaft w\u00e4re, wo alles voll von", "tokens": ["Wenn", "man", "in", "ei\u00b7ner", "Land\u00b7schaft", "w\u00e4\u00b7re", ",", "wo", "al\u00b7les", "voll", "von"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "APPR", "ART", "NN", "VAFIN", "$,", "PWAV", "PIS", "ADJD", "APPR"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Edelsteinen,", "tokens": ["E\u00b7del\u00b7stei\u00b7nen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.11": {"text": "Unm\u00f6glich k\u00f6nnte sie im hellern, poliert- und reinern", "tokens": ["Un\u00b7m\u00f6g\u00b7lich", "k\u00f6nn\u00b7te", "sie", "im", "hel\u00b7lern", ",", "po\u00b7lier\u00b7t", "und", "rei\u00b7nern"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VMFIN", "PPER", "APPRART", "VVINF", "$,", "TRUNC", "KON", "VVINF"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.12": {"text": "Glanze scheinen.", "tokens": ["Glan\u00b7ze", "schei\u00b7nen", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.13": {"text": "Es sieht, zu einer solchen Zeit, der Erden und das Wasser-", "tokens": ["Es", "sieht", ",", "zu", "ei\u00b7ner", "sol\u00b7chen", "Zeit", ",", "der", "Er\u00b7den", "und", "das", "Was\u00b7ser"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "APPR", "ART", "PIAT", "NN", "$,", "ART", "NN", "KON", "ART", "TRUNC"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.14": {"text": "Reich,", "tokens": ["Reich", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.15": {"text": "Am Schimmer, Glanz und buntem Feuer, fast wahren", "tokens": ["Am", "Schim\u00b7mer", ",", "Glanz", "und", "bun\u00b7tem", "Feu\u00b7er", ",", "fast", "wah\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "$,", "NN", "KON", "ADJA", "NN", "$,", "ADV", "VVINF"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "Diamanten gleich.", "tokens": ["Di\u00b7a\u00b7man\u00b7ten", "gleich", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.17": {"text": "Von selbst geformte Prismata sieht man bald hier, bald", "tokens": ["Von", "selbst", "ge\u00b7form\u00b7te", "Pris\u00b7ma\u00b7ta", "sieht", "man", "bald", "hier", ",", "bald"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "ADV", "ADJA", "NN", "VVFIN", "PIS", "ADV", "ADV", "$,", "ADV"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "dorten funkeln,", "tokens": ["dor\u00b7ten", "fun\u00b7keln", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.19": {"text": "Hier Purpur, dorten Gelb, wie Gold, hier Wei\u00df, dort", "tokens": ["Hier", "Pur\u00b7pur", ",", "dor\u00b7ten", "Gelb", ",", "wie", "Gold", ",", "hier", "Wei\u00df", ",", "dort"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ADV", "NN", "$,", "ADV", "ADJD", "$,", "PWAV", "NN", "$,", "ADV", "NN", "$,", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Blau, da Roht, dort Gr\u00fcn,", "tokens": ["Blau", ",", "da", "Roht", ",", "dort", "Gr\u00fcn", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "NN", "$,", "ADV", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.21": {"text": "In wandelbarer Farb\u2019 und Glanz, recht als im bunten Feuer,", "tokens": ["In", "wan\u00b7del\u00b7ba\u00b7rer", "Fa\u00b7rb'", "und", "Glanz", ",", "recht", "als", "im", "bun\u00b7ten", "Feu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$,", "ADJD", "KOKOM", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-++-+-+-+-+-", "measure": "unknown.measure.octa.plus"}, "line.22": {"text": "gl\u00fchn,", "tokens": ["gl\u00fchn", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "+", "measure": "single.up"}, "line.23": {"text": "In klein- und grossen Eises St\u00fccken, auf weissen bald, und", "tokens": ["In", "klein", "und", "gros\u00b7sen", "Ei\u00b7ses", "St\u00fc\u00b7cken", ",", "auf", "weis\u00b7sen", "bald", ",", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["APPR", "TRUNC", "KON", "ADJA", "NN", "NN", "$,", "APPR", "ADJA", "ADV", "$,", "KON"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.24": {"text": "bald auf dunkeln,", "tokens": ["bald", "auf", "dun\u00b7keln", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.25": {"text": "Mit schw\u00e4rzlich- blau gemischten Stellen des unbeschneiten", "tokens": ["Mit", "schw\u00e4rz\u00b7lich", "blau", "ge\u00b7mischten", "Stel\u00b7len", "des", "un\u00b7be\u00b7schnei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "T