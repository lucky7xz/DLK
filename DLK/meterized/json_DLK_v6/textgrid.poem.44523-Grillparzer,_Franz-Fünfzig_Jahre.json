{"textgrid.poem.44523": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "F\u00fcnfzig Jahre", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als du heraufkamst an der Tage Morgen,", "tokens": ["Als", "du", "her\u00b7auf\u00b7kamst", "an", "der", "Ta\u00b7ge", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da war die Welt bedeckt mit Mord und Blut,", "tokens": ["Da", "war", "die", "Welt", "be\u00b7deckt", "mit", "Mord", "und", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es hatte scheu das Recht sein Haupt verborgen,", "tokens": ["Es", "hat\u00b7te", "scheu", "das", "Recht", "sein", "Haupt", "ver\u00b7bor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "ART", "NN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den Himmel r\u00f6tete der Feuer Glut.", "tokens": ["Den", "Him\u00b7mel", "r\u00f6\u00b7te\u00b7te", "der", "Feu\u00b7er", "Glut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Du aber, dein bewu\u00dft erst in Gefahren,", "tokens": ["Du", "a\u00b7ber", ",", "dein", "be\u00b7wu\u00dft", "erst", "in", "Ge\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "PPOSAT", "ADJD", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit Feldherrnaug vereinend Kampfeslust,", "tokens": ["Mit", "Feld\u00b7herr\u00b7naug", "ver\u00b7ei\u00b7nend", "Kamp\u00b7fes\u00b7lust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du holtest aus erregter Feinde Scharen", "tokens": ["Du", "hol\u00b7test", "aus", "er\u00b7reg\u00b7ter", "Fein\u00b7de", "Scha\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Ahnfrau Zeichen dir als Schild der Brust.", "tokens": ["Der", "Ahn\u00b7frau", "Zei\u00b7chen", "dir", "als", "Schild", "der", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PPER", "KOUS", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und so bewehrt, bestrahlt von ihrem Geiste,", "tokens": ["Und", "so", "be\u00b7wehrt", ",", "be\u00b7strahlt", "von", "ih\u00b7rem", "Geis\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "$,", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Standst du in Fechterstellung sch\u00fctzend da,", "tokens": ["Standst", "du", "in", "Fech\u00b7ter\u00b7stel\u00b7lung", "sch\u00fct\u00b7zend", "da", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und hinter dir barg froh dein Volk das meiste,", "tokens": ["Und", "hin\u00b7ter", "dir", "barg", "froh", "dein", "Volk", "das", "meis\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "ADJD", "PPOSAT", "NN", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Was vor dir sich in Schutt und Tr\u00fcmmern sah.", "tokens": ["Was", "vor", "dir", "sich", "in", "Schutt", "und", "Tr\u00fcm\u00b7mern", "sah", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPER", "PRF", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Den Franken, als er trunken noch vom Weine,", "tokens": ["Den", "Fran\u00b7ken", ",", "als", "er", "trun\u00b7ken", "noch", "vom", "Wei\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "ADJD", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In dem der m\u00e4\u00dfge Trinker St\u00e4rke sucht,", "tokens": ["In", "dem", "der", "m\u00e4\u00df\u00b7ge", "Trin\u00b7ker", "St\u00e4r\u00b7ke", "sucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "ADJA", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Rangst du darnieder, da\u00df vom blutgen Rheine", "tokens": ["Rangst", "du", "dar\u00b7nie\u00b7der", ",", "da\u00df", "vom", "blut\u00b7gen", "Rhei\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PAV", "$,", "KOUS", "APPRART", "ADJA", "NE"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Er r\u00fcck die Grenze trug auf wilder Flucht.", "tokens": ["Er", "r\u00fcck", "die", "Gren\u00b7ze", "trug", "auf", "wil\u00b7der", "Flucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Als, kletternd dann auf Leichen seiner Br\u00fcder,", "tokens": ["Als", ",", "klet\u00b7ternd", "dann", "auf", "Lei\u00b7chen", "sei\u00b7ner", "Br\u00fc\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "ADJD", "ADV", "APPR", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Mann, wie Kleine klein, wie Gro\u00dfe gro\u00df,", "tokens": ["Der", "Mann", ",", "wie", "Klei\u00b7ne", "klein", ",", "wie", "Gro\u00b7\u00dfe", "gro\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "ADJA", "ADJD", "$,", "PWAV", "ADJA", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die hundert Schlangen eint zu ", "tokens": ["Die", "hun\u00b7dert", "Schlan\u00b7gen", "eint", "zu"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "CARD", "NN", "VVPP", "PTKZU"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Warst du des Ruhms ihm Gegner und Geno\u00df.", "tokens": ["Warst", "du", "des", "Ruhms", "ihm", "Geg\u00b7ner", "und", "Ge\u00b7no\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Ihm, den besiegt die Welt, da er alleine,", "tokens": ["Ihm", ",", "den", "be\u00b7siegt", "die", "Welt", ",", "da", "er", "al\u00b7lei\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "VVFIN", "ART", "NN", "$,", "KOUS", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Standst du allein, da mit ihm noch die Welt,", "tokens": ["Standst", "du", "al\u00b7lein", ",", "da", "mit", "ihm", "noch", "die", "Welt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "KOUS", "APPR", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Eh ihm ein Gott in blutgem Rachescheine", "tokens": ["Eh", "ihm", "ein", "Gott", "in", "blut\u00b7gem", "Ra\u00b7ch\u00b7e\u00b7schei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Die Rechte noch gel\u00e4hmt auf Moskaus Feld.", "tokens": ["Die", "Rech\u00b7te", "noch", "ge\u00b7l\u00e4hmt", "auf", "Mos\u00b7kaus", "Feld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "APPR", "NE", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Gemessen habt ihr euch, habt euch gewogen,", "tokens": ["Ge\u00b7mes\u00b7sen", "habt", "ihr", "euch", ",", "habt", "euch", "ge\u00b7wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "PPER", "$,", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wo jetzt die Donau schaut ein friedlich Reich;", "tokens": ["Wo", "jetzt", "die", "Do\u00b7nau", "schaut", "ein", "fried\u00b7lich", "Reich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NE", "VVFIN", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und da\u00df die Schale schwankte, neu gezogen,", "tokens": ["Und", "da\u00df", "die", "Scha\u00b7le", "schwank\u00b7te", ",", "neu", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zeigt h\u00f6chstens an, da\u00df die Gewichte gleich.", "tokens": ["Zeigt", "h\u00f6chs\u00b7tens", "an", ",", "da\u00df", "die", "Ge\u00b7wich\u00b7te", "gleich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKVZ", "$,", "KOUS", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Der Friede kam, das Grab der \u00dcberwinder.", "tokens": ["Der", "Frie\u00b7de", "kam", ",", "das", "Grab", "der", "\u00dc\u00b7berw\u00b7in\u00b7der", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du aber blicktest auf der Ahnfrau Stern,", "tokens": ["Du", "a\u00b7ber", "blick\u00b7test", "auf", "der", "Ahn\u00b7frau", "Stern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und mild wie sie, die Mutter ihrer Kinder,", "tokens": ["Und", "mild", "wie", "sie", ",", "die", "Mut\u00b7ter", "ih\u00b7rer", "Kin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KOKOM", "PPER", "$,", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Entwich der Groll und blieb dem Herzen fern.", "tokens": ["Ent\u00b7wich", "der", "Groll", "und", "blieb", "dem", "Her\u00b7zen", "fern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Aus den vom Streit noch halb gezognen Brauen", "tokens": ["Aus", "den", "vom", "Streit", "noch", "halb", "ge\u00b7zog\u00b7nen", "Brau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "APPRART", "NN", "ADV", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Brach, wie nach Sturm, die Sonne hell und klar,", "tokens": ["Brach", ",", "wie", "nach", "Sturm", ",", "die", "Son\u00b7ne", "hell", "und", "klar", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "APPR", "NN", "$,", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und lie\u00df uns als der B\u00fcrger ersten schauen,", "tokens": ["Und", "lie\u00df", "uns", "als", "der", "B\u00fcr\u00b7ger", "ers\u00b7ten", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "KOUS", "ART", "NN", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der kurz vorher im Kampf der erste war.", "tokens": ["Der", "kurz", "vor\u00b7her", "im", "Kampf", "der", "ers\u00b7te", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADV", "APPRART", "NN", "ART", "ADJA", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Zur Seite deiner Gattin, die gewesen,", "tokens": ["Zur", "Sei\u00b7te", "dei\u00b7ner", "Gat\u00b7tin", ",", "die", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "$,", "PRELS", "VAPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Umringt von deinen Kindern, die noch sind,", "tokens": ["Um\u00b7ringt", "von", "dei\u00b7nen", "Kin\u00b7dern", ",", "die", "noch", "sind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$,", "PRELS", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Gabst du der Welt den hohen Spruch zu lesen:", "tokens": ["Gabst", "du", "der", "Welt", "den", "ho\u00b7hen", "Spruch", "zu", "le\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df Gut und Gro\u00df aus einer Quelle rinnt.", "tokens": ["Da\u00df", "Gut", "und", "Gro\u00df", "aus", "ei\u00b7ner", "Quel\u00b7le", "rinnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Du echter F\u00fcrst! Vergessend nie der W\u00fcrde,", "tokens": ["Du", "ech\u00b7ter", "F\u00fcrst", "!", "Ver\u00b7ges\u00b7send", "nie", "der", "W\u00fcr\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$.", "VVPP", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nur mild weil schwach, gemeinsam weil gemein \u2013", "tokens": ["Nur", "mild", "weil", "schwach", ",", "ge\u00b7mein\u00b7sam", "weil", "ge\u00b7mein", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOUS", "ADJD", "$,", "ADJD", "KOUS", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Entzogst du dich ihr nicht als einer B\u00fcrde,", "tokens": ["Ent\u00b7zogst", "du", "dich", "ihr", "nicht", "als", "ei\u00b7ner", "B\u00fcr\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "PPER", "PTKNEG", "KOUS", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie uns erlassend, blieb sie immer dein.", "tokens": ["Sie", "uns", "er\u00b7las\u00b7send", ",", "blieb", "sie", "im\u00b7mer", "dein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "VVPP", "$,", "VVFIN", "PPER", "ADV", "PPOSAT", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Ja, von dem Ha\u00df, dem Dr\u00e4nger im Gefechte,", "tokens": ["Ja", ",", "von", "dem", "Ha\u00df", ",", "dem", "Dr\u00e4n\u00b7ger", "im", "Ge\u00b7fech\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "APPR", "ART", "NN", "$,", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hielt sich ein Tropfen auf der Seele Grund,", "tokens": ["Hielt", "sich", "ein", "Trop\u00b7fen", "auf", "der", "See\u00b7le", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "word",