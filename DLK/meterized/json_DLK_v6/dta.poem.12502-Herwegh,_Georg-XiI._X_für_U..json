{"dta.poem.12502": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "XiI.  \n  X f\u00fcr U.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1843", "urn": "urn:nbn:de:kobv:b4-200905192440", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Baut Dome oder Pyramiden,", "tokens": ["Baut", "Do\u00b7me", "o\u00b7der", "Py\u00b7ra\u00b7mi\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das stellt nicht Rhein noch Nil zufrieden!", "tokens": ["Das", "stellt", "nicht", "Rhein", "noch", "Nil", "zu\u00b7frie\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKNEG", "NE", "ADV", "NN", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie dienen ja beide nur dem Tod,", "tokens": ["Sie", "die\u00b7nen", "ja", "bei\u00b7de", "nur", "dem", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIS", "ADV", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Doch das Leben begehrt lebendig Brod.", "tokens": ["Doch", "das", "Le\u00b7ben", "be\u00b7gehrt", "le\u00b7ben\u00b7dig", "Brod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVPP", "ADJD", "NN", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}}}}}