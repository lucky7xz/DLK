{"textgrid.poem.42978": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "K\u00f6nigsberg in Preu\u00dfen", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In K\u00f6nigsberg zum zweitenmal.", "tokens": ["In", "K\u00f6\u00b7nigs\u00b7berg", "zum", "zwei\u00b7ten\u00b7mal", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPRART", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich wohnte im Hotel Central,", "tokens": ["Ich", "wohn\u00b7te", "im", "Ho\u00b7tel", "Cent\u00b7ral", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Dort war gut hausen.", "tokens": ["Dort", "war", "gut", "hau\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Doch drau\u00dfen:", "tokens": ["Doch", "drau\u00b7\u00dfen", ":"], "token_info": ["word", "word", "punct"], "pos": ["KON", "ADV", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "An K\u00e4lte zweiunddrei\u00dfig Grad.", "tokens": ["An", "K\u00e4l\u00b7te", "zwei\u00b7und\u00b7drei\u00b7\u00dfig", "Grad", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "CARD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich \u00e4chzte und ich st\u00f6hnte.", "tokens": ["Ich", "\u00e4chz\u00b7te", "und", "ich", "st\u00f6hn\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ja, K\u00f6nigsberg war stets ein Bad", "tokens": ["Ja", ",", "K\u00f6\u00b7nigs\u00b7berg", "war", "stets", "ein", "Bad"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "NE", "VAFIN", "ADV", "ART", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "F\u00fcr s\u00fcdwarm weich Verw\u00f6hnte.", "tokens": ["F\u00fcr", "s\u00fcd\u00b7warm", "weich", "Ver\u00b7w\u00f6hn\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Und weil ein Streik der Autos war,", "tokens": ["Und", "weil", "ein", "Streik", "der", "Au\u00b7tos", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Verfluchte ich den Februar,", "tokens": ["Ver\u00b7fluch\u00b7te", "ich", "den", "Feb\u00b7ru\u00b7ar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Was den durchaus nicht r\u00fchrte.", "tokens": ["Was", "den", "durc\u00b7haus", "nicht", "r\u00fchr\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADV", "PTKNEG", "VVFIN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.12": {"text": "Doch was ich so an Menschen sah,", "tokens": ["Doch", "was", "ich", "so", "an", "Men\u00b7schen", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Das war mir hell und war mir nah,", "tokens": ["Das", "war", "mir", "hell", "und", "war", "mir", "nah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADJD", "KON", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "So, da\u00df ich Freundschaft sp\u00fcrte.", "tokens": ["So", ",", "da\u00df", "ich", "Freund\u00b7schaft", "sp\u00fcr\u00b7te", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Die M\u00e4dchen, die mir's angetan,", "tokens": ["Die", "M\u00e4d\u00b7chen", ",", "die", "mir's", "an\u00b7ge\u00b7tan", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "NE", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Die wirkten so wie Walzen", "tokens": ["Die", "wirk\u00b7ten", "so", "wie", "Wal\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADV", "KOKOM", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "Und schmeckten doch wie Marzipan,", "tokens": ["Und", "schmeck\u00b7ten", "doch", "wie", "Mar\u00b7zi\u00b7pan", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "KOKOM", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Nur kr\u00e4ftig und gesalzen.", "tokens": ["Nur", "kr\u00e4f\u00b7tig", "und", "ge\u00b7sal\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Und sollte es hier einen Sarg,", "tokens": ["Und", "soll\u00b7te", "es", "hier", "ei\u00b7nen", "Sarg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "So krumm, wie ich bin, geben,", "tokens": ["So", "krumm", ",", "wie", "ich", "bin", ",", "ge\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "PWAV", "PPER", "VAFIN", "$,", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.21": {"text": "So m\u00f6cht ich gern in K\u00f6nigsbarg", "tokens": ["So", "m\u00f6cht", "ich", "gern", "in", "K\u00f6\u00b7nigs\u00b7barg"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Begraben sein und leben.", "tokens": ["Be\u00b7gra\u00b7ben", "sein", "und", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAINF", "KON", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}