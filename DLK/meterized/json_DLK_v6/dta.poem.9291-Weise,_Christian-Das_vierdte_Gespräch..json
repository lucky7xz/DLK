{"dta.poem.9291": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Das vierdte Gespr\u00e4ch.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "2. Wer wei\u00df was mancher hinterm r\u00fccken", "tokens": ["Wer", "wei\u00df", "was", "man\u00b7cher", "hin\u00b7term", "r\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PWS", "PIS", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von mir und meinem leben sagt/", "tokens": ["Von", "mir", "und", "mei\u00b7nem", "le\u00b7ben", "sagt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "KON", "PPOSAT", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der forne zu mit s\u00fcssen blicken", "tokens": ["Der", "for\u00b7ne", "zu", "mit", "s\u00fcs\u00b7sen", "bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Viel complimenten an mich wagt.", "tokens": ["Viel", "com\u00b7pli\u00b7men\u00b7ten", "an", "mich", "wagt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Weil mich nun niemand kan besch\u00fctzen/", "tokens": ["Weil", "mich", "nun", "nie\u00b7mand", "kan", "be\u00b7sch\u00fct\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PIS", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So mu\u00df die schuld auff mir ersitzen.", "tokens": ["So", "mu\u00df", "die", "schuld", "auff", "mir", "er\u00b7sit\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "ADJD", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}