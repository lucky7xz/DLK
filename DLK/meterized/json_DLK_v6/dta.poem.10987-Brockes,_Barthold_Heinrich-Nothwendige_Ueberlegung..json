{"dta.poem.10987": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Nothwendige Ueberlegung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Erwegt, um weniger zu fehlen, geliebte Menschen, zweyerley,", "tokens": ["Er\u00b7wegt", ",", "um", "we\u00b7ni\u00b7ger", "zu", "feh\u00b7len", ",", "ge\u00b7lieb\u00b7te", "Men\u00b7schen", ",", "zwey\u00b7er\u00b7ley", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVPP", "$,", "KOUI", "ADV", "PTKZU", "VVINF", "$,", "ADJA", "NN", "$,", "ADV", "$,"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Mit mehrerm Ernst, als wie bisher: ", "tokens": ["Mit", "meh\u00b7rerm", "Ernst", ",", "als", "wie", "bis\u00b7her", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "KOUS", "KOKOM", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "le wirklich frey,\nUnd die Vernunft vern\u00fcnftig sey.", "tokens": ["le", "wirk\u00b7lich", "frey", ",", "Und", "die", "Ver\u00b7nunft", "ver\u00b7n\u00fcnf\u00b7tig", "sey", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADJD", "$,", "KON", "ART", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}