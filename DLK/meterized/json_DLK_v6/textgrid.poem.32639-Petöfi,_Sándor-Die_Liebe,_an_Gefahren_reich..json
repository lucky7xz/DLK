{"textgrid.poem.32639": {"metadata": {"author": {"name": "Pet\u00f6fi, S\u00e1ndor", "birth": "N.A.", "death": "N.A."}, "title": "Die Liebe, an Gefahren reich.", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Liebe, an Gefahren reich,", "tokens": ["Die", "Lie\u00b7be", ",", "an", "Ge\u00b7fah\u00b7ren", "reich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist einer finstern Grube gleich:", "tokens": ["Ist", "ei\u00b7ner", "fins\u00b7tern", "Gru\u00b7be", "gleich", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wer da hineinf\u00e4llt, \u2013 weg ist er,", "tokens": ["Wer", "da", "hin\u00b7e\u00b7in\u00b7f\u00e4llt", ",", "\u2013", "weg", "ist", "er", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "$,", "$(", "ADV", "VAFIN", "PPER", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Er sieht nicht mehr und h\u00f6rt nicht mehr.", "tokens": ["Er", "sieht", "nicht", "mehr", "und", "h\u00f6rt", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "KON", "VVFIN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Des Vaters Herde h\u00fct' ich heut,", "tokens": ["Des", "Va\u00b7ters", "Her\u00b7de", "h\u00fct'", "ich", "heut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch acht' ich nicht auf ihr Gel\u00e4ut',", "tokens": ["Doch", "acht'", "ich", "nicht", "auf", "ihr", "Ge\u00b7l\u00e4ut'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und da\u00df sie in die Saaten geht,", "tokens": ["Und", "da\u00df", "sie", "in", "die", "Saa\u00b7ten", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das merk' ich, armer Narr, zu sp\u00e4t.", "tokens": ["Das", "merk'", "ich", ",", "ar\u00b7mer", "Narr", ",", "zu", "sp\u00e4t", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie sorglich packte M\u00fctterlein", "tokens": ["Wie", "sorg\u00b7lich", "pack\u00b7te", "M\u00fct\u00b7ter\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mir allerhand zum Essen ein \u2013", "tokens": ["Mir", "al\u00b7ler\u00b7hand", "zum", "Es\u00b7sen", "ein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "APPRART", "NN", "ART", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich hab's verloren, wei\u00df nicht wie,", "tokens": ["Ich", "hab's", "ver\u00b7lo\u00b7ren", ",", "wei\u00df", "nicht", "wie", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "VVFIN", "PTKNEG", "KOKOM", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch auch den Hunger sp\u00fcr' ich nie.", "tokens": ["Doch", "auch", "den", "Hun\u00b7ger", "sp\u00fcr'", "ich", "nie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "O Vater, Mutter, seid belehrt:", "tokens": ["O", "Va\u00b7ter", ",", "Mut\u00b7ter", ",", "seid", "be\u00b7lehrt", ":"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "NN", "$,", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich mach' ja alles ganz verkehrt,", "tokens": ["Ich", "mach'", "ja", "al\u00b7les", "ganz", "ver\u00b7kehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O, la\u00dft mich lieber ganz in Ruh' \u2013", "tokens": ["O", ",", "la\u00dft", "mich", "lie\u00b7ber", "ganz", "in", "Ruh'", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVIMP", "PPER", "ADV", "ADV", "APPR", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wei\u00df ich doch nimmer, was ich tu'!", "tokens": ["Wei\u00df", "ich", "doch", "nim\u00b7mer", ",", "was", "ich", "tu'", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}