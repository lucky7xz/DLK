{"textgrid.poem.61387": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohl kenn' ich vom Beginne", "tokens": ["Wohl", "kenn'", "ich", "vom", "Be\u00b7gin\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Neigung Jahreszeiten;", "tokens": ["Der", "Nei\u00b7gung", "Jah\u00b7res\u00b7zei\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Veilchen erster Minne", "tokens": ["Die", "Veil\u00b7chen", "ers\u00b7ter", "Min\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Brach ich und brach die Rosen dann der zweiten.", "tokens": ["Brach", "ich", "und", "brach", "die", "Ro\u00b7sen", "dann", "der", "zwei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "VVFIN", "ART", "NN", "ADV", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Doch seit ich dich erkannt mit Geist und Auge,", "tokens": ["Doch", "seit", "ich", "dich", "er\u00b7kannt", "mit", "Geist", "und", "Au\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "VVPP", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "War f\u00fcrderhin kein Streiten", "tokens": ["War", "f\u00fcr\u00b7der\u00b7hin", "kein", "Strei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "In dieser Brust, was mir zu lieben tauge.", "tokens": ["In", "die\u00b7ser", "Brust", ",", "was", "mir", "zu", "lie\u00b7ben", "tau\u00b7ge", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "PWS", "PPER", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Denn ein Gem\u00fct, tief innig", "tokens": ["Denn", "ein", "Ge\u00b7m\u00fct", ",", "tief", "in\u00b7nig"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "ADJD", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und spiegelklar zum Grunde,", "tokens": ["Und", "spie\u00b7gel\u00b7klar", "zum", "Grun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Denn einen Leib, so minnig,", "tokens": ["Denn", "ei\u00b7nen", "Leib", ",", "so", "min\u00b7nig", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie Gott ihn schafft in rechter Gnadenstunde,", "tokens": ["Wie", "Gott", "ihn", "schafft", "in", "rech\u00b7ter", "Gna\u00b7dens\u00b7tun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Dazu den Geist, f\u00fcr jede Weisheit offen,", "tokens": ["Da\u00b7zu", "den", "Geist", ",", "f\u00fcr", "je\u00b7de", "Weis\u00b7heit", "of\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "$,", "APPR", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die edlen Drei im Bunde", "tokens": ["Die", "ed\u00b7len", "Drei", "im", "Bun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Hab' ich, o Herrin, nur bei dir betroffen.", "tokens": ["Hab'", "ich", ",", "o", "Her\u00b7rin", ",", "nur", "bei", "dir", "be\u00b7trof\u00b7fen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "FM", "NN", "$,", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "O d\u00fcrft' ich all mein Wesen", "tokens": ["O", "d\u00fcrft'", "ich", "all", "mein", "We\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VMFIN", "PPER", "PIAT", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ergeben dir, du Hohe,", "tokens": ["Er\u00b7ge\u00b7ben", "dir", ",", "du", "Ho\u00b7he", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPER", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wie w\u00fcrde da genesen", "tokens": ["Wie", "w\u00fcr\u00b7de", "da", "ge\u00b7ne\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu s\u00fc\u00dfem Heil dies Herz, das liederfrohe!", "tokens": ["Zu", "s\u00fc\u00b7\u00dfem", "Heil", "dies", "Herz", ",", "das", "lie\u00b7der\u00b7fro\u00b7he", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PDS", "NN", "$,", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Nichts w\u00fc\u00dft' ich, was mir be\u00dfre Lust gew\u00e4hrte,", "tokens": ["Nichts", "w\u00fc\u00dft'", "ich", ",", "was", "mir", "be\u00df\u00b7re", "Lust", "ge\u00b7w\u00e4hr\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "$,", "PWS", "PPER", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Als meines Geistes Lohe", "tokens": ["Als", "mei\u00b7nes", "Geis\u00b7tes", "Lo\u00b7he"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Zu sch\u00fcren, da\u00df der Schimmer dich verkl\u00e4rte.", "tokens": ["Zu", "sch\u00fc\u00b7ren", ",", "da\u00df", "der", "Schim\u00b7mer", "dich", "ver\u00b7kl\u00e4r\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "KOUS", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch runzelst du die Brauen", "tokens": ["Doch", "run\u00b7zelst", "du", "die", "Brau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und sch\u00e4mst dich meines Strebens;", "tokens": ["Und", "sch\u00e4mst", "dich", "mei\u00b7nes", "Stre\u00b7bens", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ach, darin mu\u00df ich schauen", "tokens": ["Ach", ",", "da\u00b7rin", "mu\u00df", "ich", "schau\u00b7en"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PAV", "VMFIN", "PPER", "VVINF"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Gerechte Bu\u00dfe fr\u00fchern \u00dcberhebens.", "tokens": ["Ge\u00b7rech\u00b7te", "Bu\u00b7\u00dfe", "fr\u00fc\u00b7hern", "\u00dc\u00b7ber\u00b7he\u00b7bens", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Einst hab' ich, die mich liebte, kalt betr\u00fcbet,", "tokens": ["Einst", "hab'", "ich", ",", "die", "mich", "lieb\u00b7te", ",", "kalt", "be\u00b7tr\u00fc\u00b7bet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "PRELS", "PPER", "VVFIN", "$,", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Nun lieb' ich selbst vergebens \u2013", "tokens": ["Nun", "lieb'", "ich", "selbst", "ver\u00b7ge\u00b7bens", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Das ist die Minne, die Vergeltung \u00fcbet.", "tokens": ["Das", "ist", "die", "Min\u00b7ne", ",", "die", "Ver\u00b7gel\u00b7tung", "\u00fc\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "So will vor deinem Zorne", "tokens": ["So", "will", "vor", "dei\u00b7nem", "Zor\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich Flucht und Fahrt erk\u00fcren,", "tokens": ["Ich", "Flucht", "und", "Fahrt", "er\u00b7k\u00fc\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Will mich an fremdem Borne", "tokens": ["Will", "mich", "an", "frem\u00b7dem", "Bor\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Erlaben und will ruhn an fremden T\u00fcren.", "tokens": ["Er\u00b7la\u00b7ben", "und", "will", "ruhn", "an", "frem\u00b7den", "T\u00fc\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VMFIN", "VVINF", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und statt des lust'gen Spiels der Minnesinger", "tokens": ["Und", "statt", "des", "lust'\u00b7gen", "Spiels", "der", "Min\u00b7ne\u00b7sin\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die Harfe will ich r\u00fchren,", "tokens": ["Die", "Har\u00b7fe", "will", "ich", "r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ein d\u00fcstrer Pilgersmann, mit rauhem Finger.", "tokens": ["Ein", "d\u00fcst\u00b7rer", "Pil\u00b7gers\u00b7mann", ",", "mit", "rau\u00b7hem", "Fin\u00b7ger", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Du aber, h\u00f6rst du ferne", "tokens": ["Du", "a\u00b7ber", ",", "h\u00f6rst", "du", "fer\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADV", "$,", "VVFIN", "PPER", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Des S\u00e4ngers dumpfe T\u00f6ne,", "tokens": ["Des", "S\u00e4n\u00b7gers", "dump\u00b7fe", "T\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nur so viel Huld erlerne,", "tokens": ["Nur", "so", "viel", "Huld", "er\u00b7ler\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df ohne Ha\u00df dein Ohr sich dran gew\u00f6hne.", "tokens": ["Da\u00df", "oh\u00b7ne", "Ha\u00df", "dein", "Ohr", "sich", "dran", "ge\u00b7w\u00f6h\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "PPOSAT", "NN", "PRF", "PAV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und so fahr wohl du, die ich trag' im Sinne,", "tokens": ["Und", "so", "fahr", "wohl", "du", ",", "die", "ich", "trag'", "im", "Sin\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ADV", "PPER", "$,", "PRELS", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Fahr wohl, du stolze Sch\u00f6ne! \u2013", "tokens": ["Fahr", "wohl", ",", "du", "stol\u00b7ze", "Sch\u00f6\u00b7ne", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADV", "$,", "PPER", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Dies ist von mir das letzte Lied der Minne.", "tokens": ["Dies", "ist", "von", "mir", "das", "letz\u00b7te", "Lied", "der", "Min\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "PPER", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}