{"textgrid.poem.35093": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Hilf mir!", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hilf mir, o Gott, nur deinen Weg zu gehen,", "tokens": ["Hilf", "mir", ",", "o", "Gott", ",", "nur", "dei\u00b7nen", "Weg", "zu", "ge\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "$,", "FM", "NN", "$,", "ADV", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den einzgen Weg, der uns zum Heile f\u00fchrt.", "tokens": ["Den", "einz\u00b7gen", "Weg", ",", "der", "uns", "zum", "Hei\u00b7le", "f\u00fchrt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich f\u00fchl um meine Stirn ein lindes Wehen,", "tokens": ["Ich", "f\u00fchl", "um", "mei\u00b7ne", "Stirn", "ein", "lin\u00b7des", "We\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das wie ein Hauch von oben mich ber\u00fchrt.", "tokens": ["Das", "wie", "ein", "Hauch", "von", "o\u00b7ben", "mich", "be\u00b7r\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "KOKOM", "ART", "NN", "APPR", "ADV", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Dorthin will ich des Glaubens Fl\u00fcgel schlagen,", "tokens": ["Dor\u00b7thin", "will", "ich", "des", "Glau\u00b7bens", "Fl\u00fc\u00b7gel", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die mich durch dich empor und zu dir tragen.", "tokens": ["Die", "mich", "durch", "dich", "em\u00b7por", "und", "zu", "dir", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "PPER", "PTKVZ", "KON", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Hilf mir, o Gott, stets deiner zu gedenken,", "tokens": ["Hilf", "mir", ",", "o", "Gott", ",", "stets", "dei\u00b7ner", "zu", "ge\u00b7den\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "$,", "FM", "NN", "$,", "ADV", "PPOSAT", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und was ich thu, auf dich nur zu beziehn.", "tokens": ["Und", "was", "ich", "thu", ",", "auf", "dich", "nur", "zu", "be\u00b7ziehn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "$,", "APPR", "PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Woll dich in mich, la\u00df mich in dich versenken", "tokens": ["Woll", "dich", "in", "mich", ",", "la\u00df", "mich", "in", "dich", "ver\u00b7sen\u00b7ken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PRF", "APPR", "PPER", "$,", "VVIMP", "PPER", "APPR", "PRF", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Alles, was mich von dir scheidet, fliehn.", "tokens": ["Und", "Al\u00b7les", ",", "was", "mich", "von", "dir", "schei\u00b7det", ",", "fliehn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PIS", "$,", "PRELS", "PRF", "APPR", "PPER", "VVFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ich will nur dich allein im Aug behalten", "tokens": ["Ich", "will", "nur", "dich", "al\u00b7lein", "im", "Aug", "be\u00b7hal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "PPER", "ADV", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und geistig mich durch dich f\u00fcr dich gestalten.", "tokens": ["Und", "geis\u00b7tig", "mich", "durch", "dich", "f\u00fcr", "dich", "ge\u00b7stal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PRF", "APPR", "PPER", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Hilf mir, o Gott, mich endlich zu besiegen;", "tokens": ["Hilf", "mir", ",", "o", "Gott", ",", "mich", "end\u00b7lich", "zu", "be\u00b7sie\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "$,", "FM", "NN", "$,", "PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich wei\u00df es ja, ich bin mein \u00e4rgster Feind.", "tokens": ["Ich", "wei\u00df", "es", "ja", ",", "ich", "bin", "mein", "\u00e4rgs\u00b7ter", "Feind", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "PPER", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich will niemals, auch mir nicht, unterliegen;", "tokens": ["Ich", "will", "nie\u00b7mals", ",", "auch", "mir", "nicht", ",", "un\u00b7ter\u00b7lie\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "$,", "ADV", "PPER", "PTKNEG", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nur dieser Sieg ist's, der mich dir vereint.", "tokens": ["Nur", "die\u00b7ser", "Sieg", "ist's", ",", "der", "mich", "dir", "ver\u00b7eint", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "NN", "VAFIN", "$,", "PRELS", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "So hilf mir denn, hilf deinem schwachen Kinde,", "tokens": ["So", "hilf", "mir", "denn", ",", "hilf", "dei\u00b7nem", "schwa\u00b7chen", "Kin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "ADV", "$,", "VVIMP", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Da\u00df ich durchs Vaterherz den Himmel finde!", "tokens": ["Da\u00df", "ich", "durchs", "Va\u00b7ter\u00b7herz", "den", "Him\u00b7mel", "fin\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}