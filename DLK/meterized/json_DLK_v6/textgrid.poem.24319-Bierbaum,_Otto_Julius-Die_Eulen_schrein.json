{"textgrid.poem.24319": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Die Eulen schrein", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Eulen schrein. Es schmerzt wie Geierbi\u00df", "tokens": ["Die", "Eu\u00b7len", "schrein", ".", "Es", "schmerzt", "wie", "Gei\u00b7er\u00b7bi\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "$.", "PPER", "VVFIN", "KOKOM", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ratloser Reue dieser hohle Ton", "tokens": ["Rat\u00b7lo\u00b7ser", "Reu\u00b7e", "die\u00b7ser", "hoh\u00b7le", "Ton"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "NN", "PDAT", "ADJA", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Der n\u00e4chtgen V\u00f6gel dumpf und hei\u00df im Hirn.", "tokens": ["Der", "n\u00e4cht\u00b7gen", "V\u00f6\u00b7gel", "dumpf", "und", "hei\u00df", "im", "Hirn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "KON", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die leere Nacht st\u00f6hnt: stumm doch atemschwer.", "tokens": ["Die", "lee\u00b7re", "Nacht", "st\u00f6hnt", ":", "stumm", "doch", "a\u00b7tem\u00b7schwer", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$.", "ADJD", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Mir ist, als atmete ihr Schlund den Rest", "tokens": ["Mir", "ist", ",", "als", "at\u00b7me\u00b7te", "ihr", "Schlund", "den", "Rest"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "NN", "PPOSAT", "NN", "ART", "NN"], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.6": {"text": "Von Gl\u00fcck ein, den ein leerer Tag mir lie\u00df.", "tokens": ["Von", "Gl\u00fcck", "ein", ",", "den", "ein", "lee\u00b7rer", "Tag", "mir", "lie\u00df", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKVZ", "$,", "PRELS", "ART", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+---+-+-+", "measure": "dactylic.init"}}}}}