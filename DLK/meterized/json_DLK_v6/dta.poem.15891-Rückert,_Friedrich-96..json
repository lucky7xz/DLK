{"dta.poem.15891": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "96.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ei wie! an einem Tag verschlingst du alle Speise,", "tokens": ["Ei", "wie", "!", "an", "ei\u00b7nem", "Tag", "ver\u00b7schlingst", "du", "al\u00b7le", "Spei\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "$.", "APPR", "ART", "NN", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Womit ein Lebenlang den Geist gen\u00e4hrt der Weise,", "tokens": ["Wo\u00b7mit", "ein", "Le\u00b7ben\u00b7lang", "den", "Geist", "ge\u00b7n\u00e4hrt", "der", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "VVPP", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Den du dir eben heut vornahmest zu verdaun;", "tokens": ["Den", "du", "dir", "e\u00b7ben", "heut", "vor\u00b7nah\u00b7mest", "zu", "ver\u00b7daun", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "ADV", "ADV", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Uners\u00e4ttlichkeit, erweckt sie dir kein Graun?", "tokens": ["Die", "Un\u00b7er\u00b7s\u00e4tt\u00b7lich\u00b7keit", ",", "er\u00b7weckt", "sie", "dir", "kein", "Graun", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "PPER", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Du aber deutest nur aus deiner innern Welt", "tokens": ["Du", "a\u00b7ber", "deu\u00b7test", "nur", "aus", "dei\u00b7ner", "in\u00b7nern", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "ADV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hin auf die \u00e4u\u00dfere, die ebenso es h\u00e4lt;", "tokens": ["Hin", "auf", "die", "\u00e4u\u00b7\u00dfe\u00b7re", ",", "die", "e\u00b7ben\u00b7so", "es", "h\u00e4lt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "$,", "PRELS", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Da auch ein Prasser ja verpra\u00dft an einem Tage", "tokens": ["Da", "auch", "ein", "Pras\u00b7ser", "ja", "ver\u00b7pra\u00dft", "an", "ei\u00b7nem", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN", "ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mehr als erkarget hat des Kargers Jahresplage.", "tokens": ["Mehr", "als", "er\u00b7kar\u00b7get", "hat", "des", "Kar\u00b7gers", "Jah\u00b7res\u00b7pla\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "VVFIN", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Nur ist der Unterschied, da\u00df hier sich von den Aehren", "tokens": ["Nur", "ist", "der", "Un\u00b7ter\u00b7schied", ",", "da\u00df", "hier", "sich", "von", "den", "A\u00b7eh\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "KOUS", "ADV", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Der armen Flei\u00dfigen die faulen Reichen n\u00e4hren,", "tokens": ["Der", "ar\u00b7men", "Flei\u00b7\u00dfi\u00b7gen", "die", "fau\u00b7len", "Rei\u00b7chen", "n\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Doch du ein Aermerer zehrst von den geistig reichen.", "tokens": ["Doch", "du", "ein", "A\u00b7er\u00b7me\u00b7rer", "zehrst", "von", "den", "geis\u00b7tig", "rei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "ADV", "APPR", "ART", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "M\u00f6g' es zu deines Geists Bereicherung gereichen!", "tokens": ["M\u00f6g'", "es", "zu", "dei\u00b7nes", "Geists", "Be\u00b7rei\u00b7che\u00b7rung", "ge\u00b7rei\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PPOSAT", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}