{"dta.poem.4154": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Der Glockengu\u00df zu Breslau.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1821", "urn": "urn:nbn:de:kobv:b4-200905194209", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zu Breslau in der Stadt,", "tokens": ["Zu", "Bres\u00b7lau", "in", "der", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ein ehrenwerther Meister,", "tokens": ["Ein", "eh\u00b7ren\u00b7wert\u00b7her", "Meis\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gewandt in Rath und That.", "tokens": ["Ge\u00b7wandt", "in", "Rath", "und", "That", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Er hatte schon gegossen", "tokens": ["Er", "hat\u00b7te", "schon", "ge\u00b7gos\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Viel Glocken, gelb und wei\u00df,", "tokens": ["Viel", "Glo\u00b7cken", ",", "gelb", "und", "wei\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "F\u00fcr Kirchen und Kapellen", "tokens": ["F\u00fcr", "Kir\u00b7chen", "und", "Ka\u00b7pel\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu Gottes Lob und Preis.", "tokens": ["Zu", "Got\u00b7tes", "Lob", "und", "Preis", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und seine Glocken klangen", "tokens": ["Und", "sei\u00b7ne", "Glo\u00b7cken", "klan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So voll, so hell, so rein:", "tokens": ["So", "voll", ",", "so", "hell", ",", "so", "rein", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er go\u00df auch Lieb' und Glauben", "tokens": ["Er", "go\u00df", "auch", "Lieb'", "und", "Glau\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit in die Form hinein.", "tokens": ["Mit", "in", "die", "Form", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Doch aller Glocken Krone,", "tokens": ["Doch", "al\u00b7ler", "Glo\u00b7cken", "Kro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die er gegossen hat,", "tokens": ["Die", "er", "ge\u00b7gos\u00b7sen", "hat", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das ist die S\u00fcnderglocke", "tokens": ["Das", "ist", "die", "S\u00fcn\u00b7der\u00b7glo\u00b7cke"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu Breslau in der Stadt.", "tokens": ["Zu", "Bres\u00b7lau", "in", "der", "Stadt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Im Magdalenenthurme", "tokens": ["Im", "Mag\u00b7da\u00b7le\u00b7nen\u00b7thur\u00b7me"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da h\u00e4ngt das Meisterst\u00fcck,", "tokens": ["Da", "h\u00e4ngt", "das", "Meis\u00b7ter\u00b7st\u00fcck", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Rief schon manch starres Herze", "tokens": ["Rief", "schon", "manch", "star\u00b7res", "Her\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PIAT", "ADJA", "NN"], "meter": "---+-+-", "measure": "unknown.measure.di"}, "line.4": {"text": "Zu seinem Gott zur\u00fcck.", "tokens": ["Zu", "sei\u00b7nem", "Gott", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Wie hat der gute Meister", "tokens": ["Wie", "hat", "der", "gu\u00b7te", "Meis\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So treu das Werk bedacht!", "tokens": ["So", "treu", "das", "Werk", "be\u00b7dacht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie hat er seine H\u00e4nde", "tokens": ["Wie", "hat", "er", "sei\u00b7ne", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ger\u00fchrt bei Tag und Nacht!", "tokens": ["Ge\u00b7r\u00fchrt", "bei", "Tag", "und", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Und als die Stunde kommen,", "tokens": ["Und", "als", "die", "Stun\u00b7de", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df Alles fertig war,", "tokens": ["Da\u00df", "Al\u00b7les", "fer\u00b7tig", "war", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Form ist eingemauert,", "tokens": ["Die", "Form", "ist", "ein\u00b7ge\u00b7mau\u00b7ert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Speise gut und gar.", "tokens": ["Die", "Spei\u00b7se", "gut", "und", "gar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Da spricht der Glockenmeister", "tokens": ["Da", "spricht", "der", "Glo\u00b7cken\u00b7meis\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zu seinem B\u00fcbelein:", "tokens": ["Zu", "sei\u00b7nem", "B\u00fc\u00b7be\u00b7lein", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich lass' ein kurzes Weilchen", "tokens": ["Ich", "lass'", "ein", "kur\u00b7zes", "Weil\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Beim Kessel dich allein.", "tokens": ["Beim", "Kes\u00b7sel", "dich", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Will mich mit einem Trunke", "tokens": ["Will", "mich", "mit", "ei\u00b7nem", "Trun\u00b7ke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Noch st\u00e4rken zu dem Gu\u00df;", "tokens": ["Noch", "st\u00e4r\u00b7ken", "zu", "dem", "Gu\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das giebt der z\u00e4hen Speise", "tokens": ["Das", "giebt", "der", "z\u00e4\u00b7hen", "Spei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Erst einen rechten Flu\u00df.", "tokens": ["Erst", "ei\u00b7nen", "rech\u00b7ten", "Flu\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Doch h\u00fcte dich, und r\u00fchre", "tokens": ["Doch", "h\u00fc\u00b7te", "dich", ",", "und", "r\u00fch\u00b7re"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "$,", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den Hahn mir nimmer an:", "tokens": ["Den", "Hahn", "mir", "nim\u00b7mer", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sonst w\u00e4r' es um dein Leben,", "tokens": ["Sonst", "w\u00e4r'", "es", "um", "dein", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "F\u00fcrwitziger, gethan!", "tokens": ["F\u00fcr\u00b7wit\u00b7zi\u00b7ger", ",", "ge\u00b7than", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Der Bube steht am Kessel,", "tokens": ["Der", "Bu\u00b7be", "steht", "am", "Kes\u00b7sel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Schaut in die Gluth hinein:", "tokens": ["Schaut", "in", "die", "Gluth", "hin\u00b7ein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das wogt und wallt und wirbelt,", "tokens": ["Das", "wogt", "und", "wallt", "und", "wir\u00b7belt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Und will entfesselt sein.", "tokens": ["Und", "will", "ent\u00b7fes\u00b7selt", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "VVPP", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Und zischt ihm in die Ohren,", "tokens": ["Und", "zischt", "ihm", "in", "die", "Oh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und zuckt ihm durch den Sinn,", "tokens": ["Und", "zuckt", "ihm", "durch", "den", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und zieht an allen Fingern", "tokens": ["Und", "zieht", "an", "al\u00b7len", "Fin\u00b7gern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ihn nach dem Hahne hin.", "tokens": ["Ihn", "nach", "dem", "Hah\u00b7ne", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Er f\u00fchlt ihn in den H\u00e4nden,", "tokens": ["Er", "f\u00fchlt", "ihn", "in", "den", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hat schnell ihn umgedreht:", "tokens": ["Hat", "schnell", "ihn", "um\u00b7ge\u00b7dreht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da wird ihm angst und bange,", "tokens": ["Da", "wird", "ihm", "angst", "und", "ban\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVPP", "KON", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Er wei\u00df nicht, was er th\u00e4t.", "tokens": ["Er", "wei\u00df", "nicht", ",", "was", "er", "th\u00e4t", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Und l\u00e4uft hinaus zum Meister,", "tokens": ["Und", "l\u00e4uft", "hin\u00b7aus", "zum", "Meis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Schuld ihm zu gestehn,", "tokens": ["Die", "Schuld", "ihm", "zu", "ge\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Will seine Knie' umfassen", "tokens": ["Will", "sei\u00b7ne", "Knie'", "um\u00b7fas\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und ihn um Gnade flehn.", "tokens": ["Und", "ihn", "um", "Gna\u00b7de", "flehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Doch wie der nur vernommen", "tokens": ["Doch", "wie", "der", "nur", "ver\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Des Knaben erstes Wort,", "tokens": ["Des", "Kna\u00b7ben", "ers\u00b7tes", "Wort", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da rei\u00dft die kluge Rechte", "tokens": ["Da", "rei\u00dft", "die", "klu\u00b7ge", "Rech\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der j\u00e4he Zorn ihm fort.", "tokens": ["Der", "j\u00e4\u00b7he", "Zorn", "ihm", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Er st\u00f6\u00dft sein scharfes Messer", "tokens": ["Er", "st\u00f6\u00dft", "sein", "schar\u00b7fes", "Mes\u00b7ser"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dem Buben in die Brust,", "tokens": ["Dem", "Bu\u00b7ben", "in", "die", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dann st\u00fcrzt er nach dem Kessel,", "tokens": ["Dann", "st\u00fcrzt", "er", "nach", "dem", "Kes\u00b7sel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sein selber kaum bewu\u00dft.", "tokens": ["Sein", "sel\u00b7ber", "kaum", "be\u00b7wu\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.17": {"line.1": {"text": "Vielleicht, da\u00df er noch retten,", "tokens": ["Viel\u00b7leicht", ",", "da\u00df", "er", "noch", "ret\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den Strom noch hemmen kann: \u2014", "tokens": ["Den", "Strom", "noch", "hem\u00b7men", "kann", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "VMFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch sieh, der Gu\u00df ist fertig,", "tokens": ["Doch", "sieh", ",", "der", "Gu\u00df", "ist", "fer\u00b7tig", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Es fehlt kein Tropfen dran.", "tokens": ["Es", "fehlt", "kein", "Trop\u00b7fen", "dran", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.18": {"line.1": {"text": "Da eilt er abzur\u00e4umen,", "tokens": ["Da", "eilt", "er", "ab\u00b7zu\u00b7r\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVIZU", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und sieht, und will's nicht sehn,", "tokens": ["Und", "sieht", ",", "und", "will's", "nicht", "sehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KON", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ganz ohne Fleck und Makel", "tokens": ["Ganz", "oh\u00b7ne", "Fleck", "und", "Ma\u00b7kel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Glocke vor sich stehn.", "tokens": ["Die", "Glo\u00b7cke", "vor", "sich", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PRF", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.19": {"line.1": {"text": "Der Knabe liegt am Boden,", "tokens": ["Der", "Kna\u00b7be", "liegt", "am", "Bo\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Er schaut sein Werk nicht mehr.", "tokens": ["Er", "schaut", "sein", "Werk", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ach, Meister, wilder Meister,", "tokens": ["Ach", ",", "Meis\u00b7ter", ",", "wil\u00b7der", "Meis\u00b7ter", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du stie\u00dfest gar zu sehr!", "tokens": ["Du", "stie\u00b7\u00dfest", "gar", "zu", "sehr", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKA", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.20": {"line.1": {"text": "Er stellt sich dem Gerichte,", "tokens": ["Er", "stellt", "sich", "dem", "Ge\u00b7rich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und klagt sich selber an:", "tokens": ["Und", "klagt", "sich", "sel\u00b7ber", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es th\u00e4t den Richtern wehe", "tokens": ["Es", "th\u00e4t", "den", "Rich\u00b7tern", "we\u00b7he"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wohl um den wackern Mann.", "tokens": ["Wohl", "um", "den", "wa\u00b7ckern", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.21": {"line.1": {"text": "Doch kann ihn Keiner retten,", "tokens": ["Doch", "kann", "ihn", "Kei\u00b7ner", "ret\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PIS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und Blut will wieder Blut:", "tokens": ["Und", "Blut", "will", "wie\u00b7der", "Blut", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VMFIN", "ADV", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er h\u00f6rt sein Todesurthel", "tokens": ["Er", "h\u00f6rt", "sein", "To\u00b7des\u00b7ur\u00b7thel"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit gar gefa\u00dftem Muth.", "tokens": ["Mit", "gar", "ge\u00b7fa\u00df\u00b7tem", "Muth", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.22": {"line.1": {"text": "Und als der Tag gekommen,", "tokens": ["Und", "als", "der", "Tag", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df man ihn f\u00fchrt hinaus,", "tokens": ["Da\u00df", "man", "ihn", "f\u00fchrt", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da wird ihm angeboten", "tokens": ["Da", "wird", "ihm", "an\u00b7ge\u00b7bo\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der letzte Gnadenschmaus.", "tokens": ["Der", "letz\u00b7te", "Gna\u00b7den\u00b7schmaus", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.23": {"line.1": {"text": "Ich dank' euch, spricht der Meister,", "tokens": ["Ich", "dank'", "euch", ",", "spricht", "der", "Meis\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihr Herren lieb und werth,", "tokens": ["Ihr", "Her\u00b7ren", "lieb", "und", "werth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch eine andre Gnade", "tokens": ["Doch", "ei\u00b7ne", "and\u00b7re", "Gna\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mein Herz von euch begehrt.", "tokens": ["Mein", "Herz", "von", "euch", "be\u00b7gehrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.24": {"line.1": {"text": "La\u00dft mich nur einmal h\u00f6ren", "tokens": ["La\u00dft", "mich", "nur", "ein\u00b7mal", "h\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der neuen Glocke Klang!", "tokens": ["Der", "neu\u00b7en", "Glo\u00b7cke", "Klang", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich hab' sie ja bereitet:", "tokens": ["Ich", "hab'", "sie", "ja", "be\u00b7rei\u00b7tet", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "M\u00f6cht' wissen, ob's gelang.", "tokens": ["M\u00f6cht'", "wis\u00b7sen", ",", "ob's", "ge\u00b7lang", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "$,", "KOUS", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.25": {"line.1": {"text": "Die Bitte ward gew\u00e4hret,", "tokens": ["Die", "Bit\u00b7te", "ward", "ge\u00b7w\u00e4h\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sie schien den Herr'n gering,", "tokens": ["Sie", "schien", "den", "Herr'n", "ge\u00b7ring", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Glocke ward gel\u00e4utet,", "tokens": ["Die", "Glo\u00b7cke", "ward", "ge\u00b7l\u00e4u\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Als er zum Tode ging.", "tokens": ["Als", "er", "zum", "To\u00b7de", "ging", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.26": {"line.1": {"text": "Der Meister h\u00f6rt sie klingen,", "tokens": ["Der", "Meis\u00b7ter", "h\u00f6rt", "sie", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So voll, so hell, so rein:", "tokens": ["So", "voll", ",", "so", "hell", ",", "so", "rein", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Augen gehn ihm \u00fcber,", "tokens": ["Die", "Au\u00b7gen", "gehn", "ihm", "\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Es mu\u00df vor Freude sein.", "tokens": ["Es", "mu\u00df", "vor", "Freu\u00b7de", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.27": {"line.1": {"text": "Und seine Blicke leuchten,", "tokens": ["Und", "sei\u00b7ne", "Bli\u00b7cke", "leuch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Als w\u00e4ren sie verkl\u00e4rt:", "tokens": ["Als", "w\u00e4\u00b7ren", "sie", "ver\u00b7kl\u00e4rt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er hatte in dem Klange", "tokens": ["Er", "hat\u00b7te", "in", "dem", "Klan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wohl mehr als Klang geh\u00f6rt.", "tokens": ["Wohl", "mehr", "als", "Klang", "ge\u00b7h\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KOUS", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.28": {"line.1": {"text": "Hat auch geneigt den Nacken", "tokens": ["Hat", "auch", "ge\u00b7neigt", "den", "Na\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ADJD", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zum Streich voll Zuversicht;", "tokens": ["Zum", "Streich", "voll", "Zu\u00b7ver\u00b7sicht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und was der Tod versprochen,", "tokens": ["Und", "was", "der", "Tod", "ver\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das bricht das Leben nicht.", "tokens": ["Das", "bricht", "das", "Le\u00b7ben", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.29": {"line.1": {"text": "Das ist der Glocken Krone,", "tokens": ["Das", "ist", "der", "Glo\u00b7cken", "Kro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die er gegossen hat,", "tokens": ["Die", "er", "ge\u00b7gos\u00b7sen", "hat", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Magdalenenglocke", "tokens": ["Die", "Mag\u00b7da\u00b7le\u00b7nen\u00b7glo\u00b7cke"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu Breslau in der Stadt.", "tokens": ["Zu", "Bres\u00b7lau", "in", "der", "Stadt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.30": {"line.1": {"text": "Die ward zur S\u00fcnderglocke", "tokens": ["Die", "ward", "zur", "S\u00fcn\u00b7der\u00b7glo\u00b7cke"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Seit jenem Tag geweiht:", "tokens": ["Seit", "je\u00b7nem", "Tag", "ge\u00b7weiht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wei\u00df nicht, ob's anders worden", "tokens": ["Wei\u00df", "nicht", ",", "ob's", "an\u00b7ders", "wor\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "$,", "KOUS", "ADV", "VAPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In dieser neuen Zeit.", "tokens": ["In", "die\u00b7ser", "neu\u00b7en", "Zeit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}