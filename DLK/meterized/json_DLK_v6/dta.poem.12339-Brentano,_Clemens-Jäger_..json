{"dta.poem.12339": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "J\u00e4ger .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1806", "urn": "urn:nbn:de:kobv:b4-20090519157", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie kommts, da\u00df du so traurig bist,               ", "tokens": ["Wie", "kommts", ",", "da\u00df", "du", "so", "trau\u00b7rig", "bist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "$,", "KOUS", "PPER", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und gar nicht einmal lachst? :,:", "tokens": ["Und", "gar", "nicht", "ein\u00b7mal", "lachst", "?", ":", ",", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["KON", "ADV", "PTKNEG", "ADV", "VVFIN", "$.", "$.", "$,", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich seh dir's an den Augen an,", "tokens": ["Ich", "seh", "dir's", "an", "den", "Au\u00b7gen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df du geweinet hast.", "tokens": ["Da\u00df", "du", "ge\u00b7wei\u00b7net", "hast", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und wenn ich auch geweinet hab,", "tokens": ["Und", "wenn", "ich", "auch", "ge\u00b7wei\u00b7net", "hab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was geht es dich denn an? :,:", "tokens": ["Was", "geht", "es", "dich", "denn", "an", "?", ":", ",", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PRF", "ADV", "PTKVZ", "$.", "$.", "$,", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich wein', da\u00df du es wei\u00dft, um Freud,", "tokens": ["Ich", "wein'", ",", "da\u00df", "du", "es", "wei\u00dft", ",", "um", "Freud", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$,", "KOUI", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die mir nicht werden kann.", "tokens": ["Die", "mir", "nicht", "wer\u00b7den", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PTKNEG", "VAINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wenn ich in Freuden leben will,", "tokens": ["Wenn", "ich", "in", "Freu\u00b7den", "le\u00b7ben", "will", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Geh' ich in gr\u00fcnen Wald, :,:", "tokens": ["Geh'", "ich", "in", "gr\u00fc\u00b7nen", "Wald", ",", ":", ",", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["NE", "PPER", "APPR", "ADJA", "NN", "$,", "$.", "$,", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vergeht mir all mein Traurigkeit,", "tokens": ["Ver\u00b7geht", "mir", "all", "mein", "Trau\u00b7rig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und leb wie's mir gef\u00e4llt.", "tokens": ["Und", "leb", "wie's", "mir", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VVFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Mein Schatz ein wackrer J\u00e4ger ist,", "tokens": ["Mein", "Schatz", "ein", "wack\u00b7rer", "J\u00e4\u00b7ger", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er tr\u00e4gt ein gr\u00fcnes Kleid, :,:", "tokens": ["Er", "tr\u00e4gt", "ein", "gr\u00fc\u00b7nes", "Kleid", ",", ":", ",", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,", "$.", "$,", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er hat ein zart roth M\u00fcndelein,", "tokens": ["Er", "hat", "ein", "zart", "roth", "M\u00fcn\u00b7del\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJD", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das mir mein Herz erfreut.", "tokens": ["Das", "mir", "mein", "Herz", "er\u00b7freut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Mein Schatz ein holde Sch\u00e4frin ist,", "tokens": ["Mein", "Schatz", "ein", "hol\u00b7de", "Sch\u00e4f\u00b7rin", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie tr\u00e4gt ein wei\u00dfes Kleid, :,:", "tokens": ["Sie", "tr\u00e4gt", "ein", "wei\u00b7\u00dfes", "Kleid", ",", ":", ",", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,", "$.", "$,", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie hat zwei zarte Br\u00fcstelein,", "tokens": ["Sie", "hat", "zwei", "zar\u00b7te", "Br\u00fcs\u00b7tel\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "CARD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die mir mein Herz erfreun.", "tokens": ["Die", "mir", "mein", "Herz", "er\u00b7freun", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "So bin ich's wohl, so bist du's wohl", "tokens": ["So", "bin", "ich's", "wohl", ",", "so", "bist", "du's", "wohl"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PIS", "ADV", "$,", "ADV", "VAFIN", "PIS", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Feins Lieb, sch\u00f6ns Engelskind,", "tokens": ["Feins", "Lieb", ",", "sch\u00f6ns", "En\u00b7gels\u00b7kind", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So ist uns allen beiden wohl,", "tokens": ["So", "ist", "uns", "al\u00b7len", "bei\u00b7den", "wohl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIAT", "PIS", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da wir beisammen sind.", "tokens": ["Da", "wir", "bei\u00b7sam\u00b7men", "sind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}