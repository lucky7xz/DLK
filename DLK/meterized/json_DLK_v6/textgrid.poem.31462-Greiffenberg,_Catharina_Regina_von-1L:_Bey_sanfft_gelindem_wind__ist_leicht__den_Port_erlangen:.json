{"textgrid.poem.31462": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Bey sanfft gelindem wind / ist leicht / den Port erlangen:", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.85", "nl:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bey sanfft gelindem wind / ist leicht / den Port erlangen:", "tokens": ["Bey", "sanfft", "ge\u00b7lin\u00b7dem", "wind", "/", "ist", "leicht", "/", "den", "Port", "er\u00b7lan\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$(", "VAFIN", "ADJD", "$(", "ART", "NE", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "doch / wann Charybdis bellt / und Scylla uns verdreht /", "tokens": ["doch", "/", "wann", "Cha\u00b7ryb\u00b7dis", "bellt", "/", "und", "Scyl\u00b7la", "uns", "ver\u00b7dreht", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "PWAV", "NE", "VVFIN", "$(", "KON", "NE", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "wann dort ein Wellen-Berg / hier ein Meer Abgrund / steht /", "tokens": ["wann", "dort", "ein", "Wel\u00b7len\u00b7Berg", "/", "hier", "ein", "Meer", "Ab\u00b7grund", "/", "steht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "$(", "ADV", "ART", "NN", "NN", "$(", "VVFIN", "$("], "meter": "-+-+-+--++-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "und man kommt doch zu Land / dann kan man recht Siegprangen.", "tokens": ["und", "man", "kommt", "doch", "zu", "Land", "/", "dann", "kan", "man", "recht", "Sieg\u00b7pran\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ADV", "APPR", "NN", "$(", "ADV", "VMFIN", "PIS", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Nach langer Seefart / hat das goldne fell entfangen", "tokens": ["Nach", "lan\u00b7ger", "See\u00b7fart", "/", "hat", "das", "gold\u00b7ne", "fell", "ent\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$(", "VAFIN", "ART", "ADJA", "NN", "VVINF"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.6": {"text": "der dapfer Jason. Prei\u00df aus schwei\u00df und flei\u00df aufgeht.", "tokens": ["der", "dap\u00b7fer", "Ja\u00b7son", ".", "Prei\u00df", "aus", "schwei\u00df", "und", "flei\u00df", "auf\u00b7geht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "NN", "APPR", "ADJD", "KON", "NN", "VVFIN", "$."], "meter": "-+--++-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Dann / keine frucht / ohn Zucht / die Tugend nie entf\u00e4ht.", "tokens": ["Dann", "/", "kei\u00b7ne", "frucht", "/", "ohn", "Zucht", "/", "die", "Tu\u00b7gend", "nie", "ent\u00b7f\u00e4ht", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "PIAT", "NN", "$(", "APPR", "NN", "$(", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Dann heist es Sieg / wann man die Welt-gefahr umgangen.", "tokens": ["Dann", "heist", "es", "Sieg", "/", "wann", "man", "die", "Welt\u00b7ge\u00b7fahr", "um\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "$(", "PWAV", "PIS", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Man mu\u00df aus Ilium / wann soll die Tyber kr\u00f6nen /", "tokens": ["Man", "mu\u00df", "aus", "I\u00b7lium", "/", "wann", "soll", "die", "Ty\u00b7ber", "kr\u00f6\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "APPR", "NE", "$(", "PWAV", "VMFIN", "ART", "NN", "VVINF", "$("], "meter": "+---+-+-+-+-", "measure": "dactylic.init"}, "line.10": {"text": "auf kohl-und flammen gehn. Creutz ist des Gl\u00fcckes Thor.", "tokens": ["auf", "kohl\u00b7\u00b7und", "flam\u00b7men", "gehn", ".", "Creutz", "ist", "des", "Gl\u00fc\u00b7ckes", "Thor", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVINF", "VVINF", "$.", "NN", "VAFIN", "ART", "NN", "NN", "$."], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.11": {"text": "Der Vngl\u00fccks Schatten kan / ein Wunderbild versch\u00f6nen.", "tokens": ["Der", "Vn\u00b7gl\u00fccks", "Schat\u00b7ten", "kan", "/", "ein", "Wun\u00b7der\u00b7bild", "ver\u00b7sch\u00f6\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VMFIN", "$(", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Man schwingt / gebogen / sich viel herrlicher empor.", "tokens": ["Man", "schwingt", "/", "ge\u00b7bo\u00b7gen", "/", "sich", "viel", "herr\u00b7li\u00b7cher", "em\u00b7por", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$(", "VVPP", "$(", "PRF", "PIAT", "ADJA", "PTKVZ", "$."], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.13": {"text": "Die Kron / kommt aus dem Feur / dann auf des K\u00f6nigs Haar.", "tokens": ["Die", "Kron", "/", "kommt", "aus", "dem", "Feur", "/", "dann", "auf", "des", "K\u00f6\u00b7nigs", "Haar", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "VVFIN", "APPR", "ART", "NN", "$(", "ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "So wird ein Ehren-Stern aus Vngl\u00fcck und gefahr.", "tokens": ["So", "wird", "ein", "Eh\u00b7ren\u00b7S\u00b7tern", "aus", "Vn\u00b7gl\u00fcck", "und", "ge\u00b7fahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}