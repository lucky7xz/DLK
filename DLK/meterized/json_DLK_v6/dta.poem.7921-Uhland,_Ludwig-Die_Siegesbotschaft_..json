{"dta.poem.7921": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Die Siegesbotschaft .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Es war so tr\u00fcbe, dumpf und schwer,", "tokens": ["Es", "war", "so", "tr\u00fc\u00b7be", ",", "dumpf", "und", "schwer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJA", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die schlimme Sage schlich umher,", "tokens": ["Die", "schlim\u00b7me", "Sa\u00b7ge", "schlich", "um\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie kr\u00e4chzte, wie zur D\u00e4mmerzeit", "tokens": ["Sie", "kr\u00e4chz\u00b7te", ",", "wie", "zur", "D\u00e4m\u00b7mer\u00b7zeit"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein schwarzer Ungl\u00fccksvogel schreit.", "tokens": ["Ein", "schwar\u00b7zer", "Un\u00b7gl\u00fccks\u00b7vo\u00b7gel", "schreit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die schlimme Sage schlich im Land", "tokens": ["Die", "schlim\u00b7me", "Sa\u00b7ge", "schlich", "im", "Land"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit schn\u00f6der Schattenbilder Tand,", "tokens": ["Mit", "schn\u00f6\u00b7der", "Schat\u00b7ten\u00b7bil\u00b7der", "Tand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie zeigte Zwietracht und Verrath,", "tokens": ["Sie", "zeig\u00b7te", "Zwiet\u00b7racht", "und", "Ver\u00b7rath", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zernichtung aller edeln Saat.", "tokens": ["Zer\u00b7nich\u00b7tung", "al\u00b7ler", "e\u00b7deln", "Saat", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Des B\u00f6sen Freunde trotzen schon,", "tokens": ["Des", "B\u00f6\u00b7sen", "Freun\u00b7de", "trot\u00b7zen", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie lachen h\u00e4misch, sprechen Hohn,", "tokens": ["Sie", "la\u00b7chen", "h\u00e4\u00b7misch", ",", "spre\u00b7chen", "Hohn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Guten stehen ernst und still", "tokens": ["Die", "Gu\u00b7ten", "ste\u00b7hen", "ernst", "und", "still"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und harren, was da werden will.", "tokens": ["Und", "har\u00b7ren", ",", "was", "da", "wer\u00b7den", "will", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PRELS", "ADV", "VAINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Da schwingt sich\u2019s \u00fcber\u2019m Rhein empor", "tokens": ["Da", "schwingt", "sich's", "\u00fc\u00b7ber'm", "Rhein", "em\u00b7por"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "APPRART", "NE", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und bricht den d\u00fcstern Wolkenflor:", "tokens": ["Und", "bricht", "den", "d\u00fcs\u00b7tern", "Wol\u00b7ken\u00b7flor", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ist\u2019s stolzer Adler Sonnenflug?", "tokens": ["Ist's", "stol\u00b7zer", "Ad\u00b7ler", "Son\u00b7nen\u00b7flug", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist\u2019s t\u00f6nereicher Schw\u00e4ne Zug?", "tokens": ["Ist's", "t\u00f6\u00b7ne\u00b7rei\u00b7cher", "Schw\u00e4\u00b7ne", "Zug", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.5": {"line.1": {"text": "Es rauscht und singt im goldnen Licht:", "tokens": ["Es", "rauscht", "und", "singt", "im", "gold\u00b7nen", "Licht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Herr verl\u00e4\u00dft die Seinen nicht,", "tokens": ["Der", "Herr", "ver\u00b7l\u00e4\u00dft", "die", "Sei\u00b7nen", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "PPOSS", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er macht so Heil\u2019ges nicht zum Spott.", "tokens": ["Er", "macht", "so", "Heil'\u00b7ges", "nicht", "zum", "Spott", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJA", "PTKNEG", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Viktoria! Mit uns ist Gott!", "tokens": ["Vik\u00b7to\u00b7ria", "!", "Mit", "uns", "ist", "Gott", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "APPR", "PPER", "VAFIN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}