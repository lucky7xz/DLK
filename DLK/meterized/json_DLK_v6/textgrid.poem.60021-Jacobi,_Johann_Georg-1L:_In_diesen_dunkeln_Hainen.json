{"textgrid.poem.60021": {"metadata": {"author": {"name": "Jacobi, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: In diesen dunkeln Hainen", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In diesen dunkeln Hainen", "tokens": ["In", "die\u00b7sen", "dun\u00b7keln", "Hai\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ging ich den losen Kleinen,", "tokens": ["Ging", "ich", "den", "lo\u00b7sen", "Klei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die K\u00f6cher tragen, nach;", "tokens": ["Die", "K\u00f6\u00b7cher", "tra\u00b7gen", ",", "nach", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "APPR", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Hier, Chloe, hier im Gr\u00fcnen", "tokens": ["Hier", ",", "Chloe", ",", "hier", "im", "Gr\u00fc\u00b7nen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "NE", "$,", "ADV", "APPRART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "War Amor unter ihnen;", "tokens": ["War", "A\u00b7mor", "un\u00b7ter", "ih\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "APPR", "PPER", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ich h\u00f6rte, was er sprach.", "tokens": ["Ich", "h\u00f6r\u00b7te", ",", "was", "er", "sprach", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "O wenn in diesen Schlingen", "tokens": ["O", "wenn", "in", "die\u00b7sen", "Schlin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "KOUS", "APPR", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wir nun das T\u00e4ubchen fingen,", "tokens": ["Wir", "nun", "das", "T\u00e4ub\u00b7chen", "fin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das mir die Mutter wies!", "tokens": ["Das", "mir", "die", "Mut\u00b7ter", "wies", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "O lockt es! singt, ihr Br\u00fcder;", "tokens": ["O", "lockt", "es", "!", "singt", ",", "ihr", "Br\u00fc\u00b7der", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$.", "VVFIN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ihr wi\u00dft, da\u00df sich durch Lieder", "tokens": ["Ihr", "wi\u00dft", ",", "da\u00df", "sich", "durch", "Lie\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PRF", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Schon manches t\u00e4uschen lie\u00df.", "tokens": ["Schon", "man\u00b7ches", "t\u00e4u\u00b7schen", "lie\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVINF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "\u00bbkomm, T\u00e4ubchen, komm! Den Wagen", "tokens": ["\u00bb", "komm", ",", "T\u00e4ub\u00b7chen", ",", "komm", "!", "Den", "Wa\u00b7gen"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["$(", "VVFIN", "$,", "NN", "$,", "VVFIN", "$.", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Venus, sanft getragen", "tokens": ["Der", "Ve\u00b7nus", ",", "sanft", "ge\u00b7tra\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ADJD", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Vom Zephyr, sollst du ziehn;", "tokens": ["Vom", "Ze\u00b7phyr", ",", "sollst", "du", "ziehn", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Sollst unter Bl\u00fcthen wallen,", "tokens": ["Sollst", "un\u00b7ter", "Bl\u00fc\u00b7then", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wenn in des Adlers Krallen", "tokens": ["Wenn", "in", "des", "Ad\u00b7lers", "Kral\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Donnerkeile gl\u00fchn.", "tokens": ["Die", "Don\u00b7ner\u00b7kei\u00b7le", "gl\u00fchn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Er mu\u00df den Zevs begleiten,", "tokens": ["Er", "mu\u00df", "den", "Zevs", "be\u00b7glei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und gegen Riesen streiten,", "tokens": ["Und", "ge\u00b7gen", "Rie\u00b7sen", "strei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und mit ins Treffen gehn;", "tokens": ["Und", "mit", "ins", "Tref\u00b7fen", "gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Du kannst in kleinen Kriegen", "tokens": ["Du", "kannst", "in", "klei\u00b7nen", "Krie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Uns nur zur Seite fliegen,", "tokens": ["Uns", "nur", "zur", "Sei\u00b7te", "flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und \u00fcberwinden sehn.", "tokens": ["Und", "\u00fc\u00b7berw\u00b7in\u00b7den", "sehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "O komm! In wenig Tagen", "tokens": ["O", "komm", "!", "In", "we\u00b7nig", "Ta\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "$.", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wirst du verlassen klagen,", "tokens": ["Wirst", "du", "ver\u00b7las\u00b7sen", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dein Liebling eilt von hier:", "tokens": ["Dein", "Lieb\u00b7ling", "eilt", "von", "hier", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Getreuer sind die Gatten", "tokens": ["Ge\u00b7treu\u00b7er", "sind", "die", "Gat\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "In Paphos sichern Schatten;", "tokens": ["In", "Pa\u00b7phos", "si\u00b7chern", "Schat\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Kein Falke raubt sie dir.", "tokens": ["Kein", "Fal\u00b7ke", "raubt", "sie", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Du sollst mit Amoretten", "tokens": ["Du", "sollst", "mit", "A\u00b7mo\u00b7ret\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dich auf den G\u00fcrtel betten,", "tokens": ["Dich", "auf", "den", "G\u00fcr\u00b7tel", "bet\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der unsre G\u00f6ttin ziert;", "tokens": ["Der", "uns\u00b7re", "G\u00f6t\u00b7tin", "ziert", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Geschmeichelt von Najaden,", "tokens": ["Ge\u00b7schmei\u00b7chelt", "von", "Na\u00b7ja\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Soll dich die Quelle baden,", "tokens": ["Soll", "dich", "die", "Quel\u00b7le", "ba\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Venus nur ber\u00fchrt.\u00ab", "tokens": ["Die", "Ve\u00b7nus", "nur", "be\u00b7r\u00fchrt", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "So sangen sie, die Br\u00fcder!", "tokens": ["So", "san\u00b7gen", "sie", ",", "die", "Br\u00fc\u00b7der", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "O s\u00fc\u00dfe Macht der Lieder!", "tokens": ["O", "s\u00fc\u00b7\u00dfe", "Macht", "der", "Lie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "O zauberischer Wahn!", "tokens": ["O", "zau\u00b7be\u00b7ri\u00b7scher", "Wahn", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Das T\u00e4ubchen kommt geflogen,", "tokens": ["Das", "T\u00e4ub\u00b7chen", "kommt", "ge\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Setzt sich auf Amors Bogen,", "tokens": ["Setzt", "sich", "auf", "A\u00b7mors", "Bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und sieht den Knaben an.", "tokens": ["Und", "sieht", "den", "Kna\u00b7ben", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}