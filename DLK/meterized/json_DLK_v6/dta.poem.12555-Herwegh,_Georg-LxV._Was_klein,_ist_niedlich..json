{"dta.poem.12555": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "LxV.  \n  Was klein, ist niedlich.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1843", "urn": "urn:nbn:de:kobv:b4-200905192440", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Als ihm der Sch\u00f6n zu gro\u00df geworden,", "tokens": ["Als", "ihm", "der", "Sch\u00f6n", "zu", "gro\u00df", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NE", "PTKA", "ADJD", "VAPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Schickt' er ihn fort mit einem Orden;", "tokens": ["Schickt'", "er", "ihn", "fort", "mit", "ei\u00b7nem", "Or\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PTKVZ", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Doch, um bei der Familie zu bleiben,", "tokens": ["Doch", ",", "um", "bei", "der", "Fa\u00b7mi\u00b7lie", "zu", "blei\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUI", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Lie\u00df er ein ", "tokens": ["Lie\u00df", "er", "ein"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "ART"], "meter": "+-+", "measure": "trochaic.di"}}}}}