{"textgrid.poem.57519": {"metadata": {"author": {"name": "Gottsched, Johann Christoph", "birth": "N.A.", "death": "N.A."}, "title": "Da\u00df der Mensch selbst an seiner Verdammung Schuld sey", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So fahrt nur immer fort in eurer Sicherheit!", "tokens": ["So", "fahrt", "nur", "im\u00b7mer", "fort", "in", "eu\u00b7rer", "Si\u00b7cher\u00b7heit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "PTKVZ", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vers\u00e4umet unversch\u00e4mt die kurze Gnadenzeit,", "tokens": ["Ver\u00b7s\u00e4u\u00b7met", "un\u00b7ver\u00b7sch\u00e4mt", "die", "kur\u00b7ze", "Gna\u00b7den\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Verkehrte Sterbliche! die ihr den H\u00f6chsten hasset,", "tokens": ["Ver\u00b7kehr\u00b7te", "Sterb\u00b7li\u00b7che", "!", "die", "ihr", "den", "H\u00f6chs\u00b7ten", "has\u00b7set", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "PRELS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.4": {"text": "Und euer blindes Herz dem Frevel \u00fcberlasset.", "tokens": ["Und", "eu\u00b7er", "blin\u00b7des", "Herz", "dem", "Fre\u00b7vel", "\u00fc\u00b7ber\u00b7las\u00b7set", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wie l\u00e4uft doch euer Fu\u00df so hurtig h\u00f6llenw\u00e4rts!", "tokens": ["Wie", "l\u00e4uft", "doch", "eu\u00b7er", "Fu\u00df", "so", "hur\u00b7tig", "h\u00f6l\u00b7len\u00b7w\u00e4rts", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "PPOSAT", "NN", "ADV", "ADJD", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Erweichet doch einmal das felsenharte Herz.", "tokens": ["Er\u00b7wei\u00b7chet", "doch", "ein\u00b7mal", "das", "fel\u00b7sen\u00b7har\u00b7te", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Auch euch will Gottes Huld sehr gern zum Himmel bringen,", "tokens": ["Auch", "euch", "will", "Got\u00b7tes", "Huld", "sehr", "gern", "zum", "Him\u00b7mel", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VMFIN", "NN", "NN", "ADV", "ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Doch keinen mit Gewalt zum frommen Leben zwingen.", "tokens": ["Doch", "kei\u00b7nen", "mit", "Ge\u00b7walt", "zum", "from\u00b7men", "Le\u00b7ben", "zwin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "APPR", "NN", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Zween Wege hat uns Gott in Gnaden vorgelegt,", "tokens": ["Zween", "We\u00b7ge", "hat", "uns", "Gott", "in", "Gna\u00b7den", "vor\u00b7ge\u00b7legt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VAFIN", "PPER", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo einer dornicht ist, der andre Rosen tr\u00e4gt.", "tokens": ["Wo", "ei\u00b7ner", "dor\u00b7nicht", "ist", ",", "der", "and\u00b7re", "Ro\u00b7sen", "tr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VAFIN", "$,", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der eine f\u00fchret uns zum unverwelkten Leben,", "tokens": ["Der", "ei\u00b7ne", "f\u00fch\u00b7ret", "uns", "zum", "un\u00b7ver\u00b7welk\u00b7ten", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der andre kann uns nichts, als Tod und Marter, geben.", "tokens": ["Der", "and\u00b7re", "kann", "uns", "nichts", ",", "als", "Tod", "und", "Mar\u00b7ter", ",", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PIS", "VMFIN", "PPER", "PIS", "$,", "KOUS", "NN", "KON", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Aus Huld verstattet er uns Menschen allzumal", "tokens": ["Aus", "Huld", "ver\u00b7stat\u00b7tet", "er", "uns", "Men\u00b7schen", "all\u00b7zu\u00b7mal"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PRF", "NN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die unumschr\u00e4nkte Macht, die mehr als freye Wahl,", "tokens": ["Die", "un\u00b7um\u00b7schr\u00e4nk\u00b7te", "Macht", ",", "die", "mehr", "als", "frey\u00b7e", "Wahl", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PIS", "KOKOM", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Den Rosen hold zu seyn, die Dornen auszulesen,", "tokens": ["Den", "Ro\u00b7sen", "hold", "zu", "seyn", ",", "die", "Dor\u00b7nen", "aus\u00b7zu\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PTKZU", "VAINF", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der H\u00f6llen zu zu gehn, und ewig zu genesen.", "tokens": ["Der", "H\u00f6l\u00b7len", "zu", "zu", "gehn", ",", "und", "e\u00b7wig", "zu", "ge\u00b7ne\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "PTKZU", "VVINF", "$,", "KON", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Ihr S\u00fcnder! ist die Schuld nicht euer ganz allein,", "tokens": ["Ihr", "S\u00fcn\u00b7der", "!", "ist", "die", "Schuld", "nicht", "eu\u00b7er", "ganz", "al\u00b7lein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "VAFIN", "ART", "NN", "PTKNEG", "PPOSAT", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wenn ihr so bosheitvoll, so th\u00f6richt wollet seyn;", "tokens": ["Wenn", "ihr", "so", "bos\u00b7heit\u00b7voll", ",", "so", "th\u00f6\u00b7richt", "wol\u00b7let", "seyn", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "$,", "ADV", "ADJD", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Da\u00df der verirrte Geist den Himmel von sich schiebet,", "tokens": ["Da\u00df", "der", "ver\u00b7irr\u00b7te", "Geist", "den", "Him\u00b7mel", "von", "sich", "schie\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "ART", "NN", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und nach verkehrter Art die gr\u00f6bsten Laster liebet.", "tokens": ["Und", "nach", "ver\u00b7kehr\u00b7ter", "Art", "die", "gr\u00f6bs\u00b7ten", "Las\u00b7ter", "lie\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Indessen, gro\u00dfer Gott! bist du so liebesvoll,", "tokens": ["In\u00b7des\u00b7sen", ",", "gro\u00b7\u00dfer", "Gott", "!", "bist", "du", "so", "lie\u00b7bes\u00b7voll", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$.", "VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn dein ergrimmter Arm die Frevler strafen soll;", "tokens": ["Wenn", "dein", "er\u00b7grimm\u00b7ter", "Arm", "die", "Frev\u00b7ler", "stra\u00b7fen", "soll", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df du die Missethat nicht gleich so v\u00f6llig lohnest,", "tokens": ["Da\u00df", "du", "die", "Mis\u00b7se\u00b7that", "nicht", "gleich", "so", "v\u00f6l\u00b7lig", "loh\u00b7nest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKNEG", "ADV", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und erst die Leiber strafst, die Seelen noch verschonest.", "tokens": ["Und", "erst", "die", "Lei\u00b7ber", "strafst", ",", "die", "See\u00b7len", "noch", "ver\u00b7scho\u00b7nest", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "$,", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Gewi\u00df, es mangelt dir an schweren Strafen nicht;", "tokens": ["Ge\u00b7wi\u00df", ",", "es", "man\u00b7gelt", "dir", "an", "schwe\u00b7ren", "Stra\u00b7fen", "nicht", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "PPER", "APPR", "ADJA", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Du weist so manche Qual zu deinem Zorngericht.", "tokens": ["Du", "weist", "so", "man\u00b7che", "Qual", "zu", "dei\u00b7nem", "Zorn\u00b7ge\u00b7richt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIAT", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Es fehlt dir, H\u00f6chster! nie an scharfen Donnerschl\u00e4gen,", "tokens": ["Es", "fehlt", "dir", ",", "H\u00f6chs\u00b7ter", "!", "nie", "an", "schar\u00b7fen", "Don\u00b7ner\u00b7schl\u00e4\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "NN", "$.", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ein ungehorsam Volk ins schwarze Grab zu legen.", "tokens": ["Ein", "un\u00b7ge\u00b7hor\u00b7sam", "Volk", "ins", "schwar\u00b7ze", "Grab", "zu", "le\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "APPRART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Seht! wie der lichte Blitz der Wolken Dampf durchdringt;", "tokens": ["Seht", "!", "wie", "der", "lich\u00b7te", "Blitz", "der", "Wol\u00b7ken", "Dampf", "durch\u00b7dringt", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "PWAV", "ART", "ADJA", "NN", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "H\u00f6rt! wie der laute Knall in dicken L\u00fcften klingt;", "tokens": ["H\u00f6rt", "!", "wie", "der", "lau\u00b7te", "Knall", "in", "di\u00b7cken", "L\u00fcf\u00b7ten", "klingt", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$.", "PWAV", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und schlie\u00dft: wie gro\u00df der sey, der euch mit seinen Wettern,", "tokens": ["Und", "schlie\u00dft", ":", "wie", "gro\u00df", "der", "sey", ",", "der", "euch", "mit", "sei\u00b7nen", "Wet\u00b7tern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PWAV", "ADJD", "ART", "VAFIN", "$,", "PRELS", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Der Mauren und Gew\u00f6lb und Th\u00fcrme kann zerschmettern.", "tokens": ["Der", "Mau\u00b7ren", "und", "Ge\u00b7w\u00f6lb", "und", "Th\u00fcr\u00b7me", "kann", "zer\u00b7schmet\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "KON", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Verwegne! denkt dabey, was ihr f\u00fcr Gr\u00e4uel thut!", "tokens": ["Ver\u00b7weg\u00b7ne", "!", "denkt", "da\u00b7bey", ",", "was", "ihr", "f\u00fcr", "Gr\u00e4u\u00b7el", "thut", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "PAV", "$,", "PWS", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "O! macht den Glauben rein, und euren Wandel gut;", "tokens": ["O", "!", "macht", "den", "Glau\u00b7ben", "rein", ",", "und", "eu\u00b7ren", "Wan\u00b7del", "gut", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VVFIN", "ART", "NN", "PTKVZ", "$,", "KON", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sonst m\u00f6chte Gott dereinst, mit gleichen Schwefelkeilen,", "tokens": ["Sonst", "m\u00f6ch\u00b7te", "Gott", "de\u00b7reinst", ",", "mit", "glei\u00b7chen", "Schwe\u00b7fel\u00b7kei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "NN", "ADV", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zum wohlverdienten Lohn begangner S\u00fcnden eilen:", "tokens": ["Zum", "wohl\u00b7ver\u00b7dien\u00b7ten", "Lohn", "be\u00b7gang\u00b7ner", "S\u00fcn\u00b7den", "ei\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ja schont die Langmuth hier; so wird doch jene Pein,", "tokens": ["Ja", "schont", "die", "Lang\u00b7muth", "hier", ";", "so", "wird", "doch", "je\u00b7ne", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "VVFIN", "ART", "NN", "ADV", "$.", "ADV", "VAFIN", "ADV", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die unaufh\u00f6rlich w\u00e4hrt, der Laster Strafe seyn.", "tokens": ["Die", "un\u00b7auf\u00b7h\u00f6r\u00b7lich", "w\u00e4hrt", ",", "der", "Las\u00b7ter", "Stra\u00b7fe", "seyn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "$,", "ART", "NN", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}