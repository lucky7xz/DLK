{"textgrid.poem.59083": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "302. Gute Unbarmherzigkeit", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Natur mu\u00df sterben, wie du wei\u00dft;", "tokens": ["Na\u00b7tur", "mu\u00df", "ster\u00b7ben", ",", "wie", "du", "wei\u00dft", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "VVINF", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hab nur Mitleiden mit dem Geist!", "tokens": ["Hab", "nur", "Mit\u00b7lei\u00b7den", "mit", "dem", "Geist", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Tier mag immer murr'n und klagen;", "tokens": ["Das", "Tier", "mag", "im\u00b7mer", "murr'n", "und", "kla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Kein Gottesmensch mu\u00df darnach fragen.", "tokens": ["Kein", "Got\u00b7tes\u00b7mensch", "mu\u00df", "dar\u00b7nach", "fra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "PAV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}