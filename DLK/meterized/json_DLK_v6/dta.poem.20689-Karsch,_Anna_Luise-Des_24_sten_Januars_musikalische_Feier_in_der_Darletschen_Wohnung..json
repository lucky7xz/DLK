{"dta.poem.20689": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Des  \n  24 sten  Januars musikalische Feier  \n in der Darletschen Wohnung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "J\u00fcngst als im G\u00f6tterrath beschlossen                 ", "tokens": ["J\u00fcngst", "als", "im", "G\u00f6t\u00b7ter\u00b7rath", "be\u00b7schlos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Ruh der Erde war, und Becher \u00fcberflossen,", "tokens": ["Die", "Ruh", "der", "Er\u00b7de", "war", ",", "und", "Be\u00b7cher", "\u00fc\u00b7berf\u00b7los\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "$,", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Zu voll geschenkt vom Ganymed,", "tokens": ["Zu", "voll", "ge\u00b7schenkt", "vom", "Ga\u00b7ny\u00b7med", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VVPP", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der hinterm Jupiter am goldnen Stuhle steht;", "tokens": ["Der", "hin\u00b7term", "Ju\u00b7pi\u00b7ter", "am", "gold\u00b7nen", "Stuh\u00b7le", "steht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Da flog Merkurius unruhig hin und wieder,", "tokens": ["Da", "flog", "Mer\u00b7ku\u00b7rius", "un\u00b7ru\u00b7hig", "hin", "und", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "ADJD", "PTKVZ", "KON", "ADV", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Trat pl\u00f6tzlich vor das G\u00f6tterchor,", "tokens": ["Trat", "pl\u00f6tz\u00b7lich", "vor", "das", "G\u00f6t\u00b7ter\u00b7chor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und alle wurden lauter Ohr,", "tokens": ["Und", "al\u00b7le", "wur\u00b7den", "lau\u00b7ter", "Ohr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und alle lie\u00dfen sich auf Luftgew\u00f6lken nieder", "tokens": ["Und", "al\u00b7le", "lie\u00b7\u00dfen", "sich", "auf", "Luft\u00b7ge\u00b7w\u00f6l\u00b7ken", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "PRF", "APPR", "NN", "PTKVZ"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und schwebten \u00fcber jenem Saal,", "tokens": ["Und", "schweb\u00b7ten", "\u00fc\u00b7ber", "je\u00b7nem", "Saal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Wo ", "tokens": ["Wo"], "token_info": ["word"], "pos": ["PWAV"], "meter": "+", "measure": "single.up"}, "line.11": {"text": "Die G\u00f6tter sahen sich nicht m\u00fcde,", "tokens": ["Die", "G\u00f6t\u00b7ter", "sa\u00b7hen", "sich", "nicht", "m\u00fc\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Da hing des Helden Bild! Sein Auge glich dem Strahl", "tokens": ["Da", "hing", "des", "Hel\u00b7den", "Bild", "!", "Sein", "Au\u00b7ge", "glich", "dem", "Strahl"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "$.", "PPOSAT", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Des m\u00e4chtgen Donnergotts, wenn er in ernster Strenge", "tokens": ["Des", "m\u00e4cht\u00b7gen", "Don\u00b7ner\u00b7gotts", ",", "wenn", "er", "in", "erns\u00b7ter", "Stren\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die Menschen schelten mu\u00df; und aus der Stirne sprach", "tokens": ["Die", "Men\u00b7schen", "schel\u00b7ten", "mu\u00df", ";", "und", "aus", "der", "Stir\u00b7ne", "sprach"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "VMFIN", "$.", "KON", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Der Kriegesgott, der durch die Menge", "tokens": ["Der", "Krie\u00b7ges\u00b7gott", ",", "der", "durch", "die", "Men\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Von Agamemnons Heere brach,", "tokens": ["Von", "A\u00b7ga\u00b7mem\u00b7nons", "Hee\u00b7re", "brach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Und in des Mundes L\u00e4cheln sa\u00dfen", "tokens": ["Und", "in", "des", "Mun\u00b7des", "L\u00e4\u00b7cheln", "sa\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Die Huldg\u00f6ttinnen und Apoll,", "tokens": ["Die", "Huld\u00b7g\u00f6t\u00b7tin\u00b7nen", "und", "A\u00b7poll", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.19": {"text": "So da\u00df hier ganz erstaunensvoll", "tokens": ["So", "da\u00df", "hier", "ganz", "er\u00b7stau\u00b7nens\u00b7voll"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "ADV", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Die G\u00f6tter den Olymp verga\u00dfen,", "tokens": ["Die", "G\u00f6t\u00b7ter", "den", "O\u00b7lymp", "ver\u00b7ga\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.21": {"text": "Und fragten: welcher zweite ", "tokens": ["Und", "frag\u00b7ten", ":", "wel\u00b7cher", "zwei\u00b7te"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "$.", "PWAT", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.22": {"text": "Des Liedes Ton erschuf? Da nannte jemand ", "tokens": ["Des", "Lie\u00b7des", "Ton", "er\u00b7schuf", "?", "Da", "nann\u00b7te", "je\u00b7mand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$.", "ADV", "VVFIN", "PIS"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Und G\u00f6tter und G\u00f6ttinnen luden", "tokens": ["Und", "G\u00f6t\u00b7ter", "und", "G\u00f6t\u00b7tin\u00b7nen", "lu\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.24": {"text": "Sich ein zu dem Concert.", "tokens": ["Sich", "ein", "zu", "dem", "Con\u00b7cert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wenn Friedrich nach dem", "tokens": ["Wenn", "Fried\u00b7rich", "nach", "dem"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "NE", "APPR", "ART"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Bau\u2019n", "tokens": ["Bau'n"], "token_info": ["word"], "pos": ["NE"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Des Eintrachttempels kommt nach Sanssouci zur\u00fcck:", "tokens": ["Des", "Ein\u00b7tracht\u00b7tem\u00b7pels", "kommt", "nach", "Sans\u00b7sou\u00b7ci", "zu\u00b7r\u00fcck", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NE", "PTKVZ", "$."], "meter": "--+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Dann singet die Musik ", "tokens": ["Dann", "sin\u00b7get", "die", "Mu\u00b7sik"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}