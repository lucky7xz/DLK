{"textgrid.poem.57722": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Die rote Rune", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bleede Bluthand sprang von des Schiffes Rand,", "tokens": ["Blee\u00b7de", "Blut\u00b7hand", "sprang", "von", "des", "Schif\u00b7fes", "Rand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Seine Mannschaft hinter ihm drein,", "tokens": ["Sei\u00b7ne", "Mann\u00b7schaft", "hin\u00b7ter", "ihm", "drein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Und er sah nach dem gr\u00fcnen Marschenland,", "tokens": ["Und", "er", "sah", "nach", "dem", "gr\u00fc\u00b7nen", "Mar\u00b7schen\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Und er wog das Schwert in der schwieligen Hand,", "tokens": ["Und", "er", "wog", "das", "Schwert", "in", "der", "schwie\u00b7li\u00b7gen", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und begann ein Lied, und wer bei ihm stand,", "tokens": ["Und", "be\u00b7gann", "ein", "Lied", ",", "und", "wer", "bei", "ihm", "stand", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "KON", "PWS", "APPR", "PPER", "VVFIN", "$,"], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Der fiel voll Freuden ein:", "tokens": ["Der", "fiel", "voll", "Freu\u00b7den", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "\u00bbo du Rune rot, o du Rune fein,", "tokens": ["\u00bb", "o", "du", "Ru\u00b7ne", "rot", ",", "o", "du", "Ru\u00b7ne", "fein", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "PPER", "NN", "ADJD", "$,", "FM", "PPER", "NN", "ADJD", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "O du Rune so rot wie das Blut,", "tokens": ["O", "du", "Ru\u00b7ne", "so", "rot", "wie", "das", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "ADV", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Rote Runen werden am Himmel sein,", "tokens": ["Ro\u00b7te", "Ru\u00b7nen", "wer\u00b7den", "am", "Him\u00b7mel", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "APPRART", "NN", "VAINF", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Rote Runen trocknen am Schwerte mein,", "tokens": ["Ro\u00b7te", "Ru\u00b7nen", "trock\u00b7nen", "am", "Schwer\u00b7te", "mein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "APPRART", "NN", "PPOSAT", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Rote Runen schenk' ich dem M\u00e4gdelein,", "tokens": ["Ro\u00b7te", "Ru\u00b7nen", "schenk'", "ich", "dem", "M\u00e4g\u00b7del\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Da\u00df es wei\u00df, wie das Lieben tut!\u00ab", "tokens": ["Da\u00df", "es", "wei\u00df", ",", "wie", "das", "Lie\u00b7ben", "tut", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "PWAV", "ART", "ADJA", "NN", "$.", "$("], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Der Tage siebene gingen vorbei,", "tokens": ["Der", "Ta\u00b7ge", "sie\u00b7be\u00b7ne", "gin\u00b7gen", "vor\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sieben D\u00f6rfer standen in Brand;", "tokens": ["Sie\u00b7ben", "D\u00f6r\u00b7fer", "stan\u00b7den", "in", "Brand", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Und die Luft war voll von Rabengeschrei,", "tokens": ["Und", "die", "Luft", "war", "voll", "von", "Ra\u00b7ben\u00b7ge\u00b7schrei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADJD", "APPR", "NN", "$,"], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Und der Wolf verga\u00df, was Hei\u00dfhunger sei;", "tokens": ["Und", "der", "Wolf", "ver\u00b7ga\u00df", ",", "was", "Hei\u00df\u00b7hun\u00b7ger", "sei", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NE", "VVFIN", "$,", "PWS", "NN", "VAFIN", "$."], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Denn die Wikinger sangen die Blutlitanei,", "tokens": ["Denn", "die", "Wi\u00b7kin\u00b7ger", "san\u00b7gen", "die", "Blut\u00b7li\u00b7ta\u00b7nei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.6": {"text": "Die Kreuz und die Quere im Land.", "tokens": ["Die", "Kreuz", "und", "die", "Que\u00b7re", "im", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "Es kam die Nacht auf den siebenten Tag,", "tokens": ["Es", "kam", "die", "Nacht", "auf", "den", "sie\u00b7ben\u00b7ten", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Bleede Bluthand lag und schlief,", "tokens": ["Blee\u00b7de", "Blut\u00b7hand", "lag", "und", "schlief", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ein rosiges M\u00e4dchen im Arme ihm lag,", "tokens": ["Ein", "ro\u00b7si\u00b7ges", "M\u00e4d\u00b7chen", "im", "Ar\u00b7me", "ihm", "lag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Ihr Vater starb durch des Schwertes Schlag,", "tokens": ["Ihr", "Va\u00b7ter", "starb", "durch", "des", "Schwer\u00b7tes", "Schlag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Ihrer Mutter Herz vor Entsetzen brach;", "tokens": ["Ih\u00b7rer", "Mut\u00b7ter", "Herz", "vor", "Ent\u00b7set\u00b7zen", "brach", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Bleede Bluthand schlief ruhig und tief.", "tokens": ["Blee\u00b7de", "Blut\u00b7hand", "schlief", "ru\u00b7hig", "und", "tief", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Und es schlich herbei und es kroch heran,", "tokens": ["Und", "es", "schlich", "her\u00b7bei", "und", "es", "kroch", "he\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJD", "PTKVZ", "KON", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das nackende Messer im Mund;", "tokens": ["Das", "na\u00b7cken\u00b7de", "Mes\u00b7ser", "im", "Mund", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Und es w\u00fcrgte die Schildwachen Mann f\u00fcr Mann,", "tokens": ["Und", "es", "w\u00fcrg\u00b7te", "die", "Schild\u00b7wa\u00b7chen", "Mann", "f\u00fcr", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Und der Meerw\u00f6lfe Herzblut im Sande zerrann,", "tokens": ["Und", "der", "Meer\u00b7w\u00f6l\u00b7fe", "Herz\u00b7blut", "im", "San\u00b7de", "zer\u00b7rann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.5": {"text": "Unehrlichen Strohtod jedweder gewann,", "tokens": ["Un\u00b7ehr\u00b7li\u00b7chen", "Stroh\u00b7tod", "jed\u00b7we\u00b7der", "ge\u00b7wann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PIAT", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Der da schnarchte im grasigen Grund.", "tokens": ["Der", "da", "schnarch\u00b7te", "im", "gra\u00b7si\u00b7gen", "Grund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}}, "stanza.6": {"line.1": {"text": "Bleede Bluthand schlief und atmete schwer,", "tokens": ["Blee\u00b7de", "Blut\u00b7hand", "schlief", "und", "at\u00b7me\u00b7te", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "KON", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Denn ein Nachtmahr brachte ihm Not;", "tokens": ["Denn", "ein", "Nacht\u00b7mahr", "brach\u00b7te", "ihm", "Not", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "NN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Das Schiff gehorchte dem Steuer nicht mehr,", "tokens": ["Das", "Schiff", "ge\u00b7horch\u00b7te", "dem", "Steu\u00b7er", "nicht", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKNEG", "ADV", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Tote M\u00e4nner schwammen neben ihm her,", "tokens": ["To\u00b7te", "M\u00e4n\u00b7ner", "schwam\u00b7men", "ne\u00b7ben", "ihm", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Und die Wellen, die gingen die Kreuz und die Quer,", "tokens": ["Und", "die", "Wel\u00b7len", ",", "die", "gin\u00b7gen", "die", "Kreuz", "und", "die", "Quer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "VVFIN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.6": {"text": "Und vom Ufer her lachte der Tod.", "tokens": ["Und", "vom", "U\u00b7fer", "her", "lach\u00b7te", "der", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "APZR", "VVFIN", "ART", "NN", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}}, "stanza.7": {"line.1": {"text": "Bleede Bluthand stand vor dem gro\u00dfen Stein,", "tokens": ["Blee\u00b7de", "Blut\u00b7hand", "stand", "vor", "dem", "gro\u00b7\u00dfen", "Stein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+----+-+", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Und die Bauern hielten Gericht;", "tokens": ["Und", "die", "Bau\u00b7ern", "hiel\u00b7ten", "Ge\u00b7richt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "NN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und das bei ihm geschlafen das M\u00e4gdelein,", "tokens": ["Und", "das", "bei", "ihm", "ge\u00b7schla\u00b7fen", "das", "M\u00e4g\u00b7del\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "APPR", "PPER", "VVPP", "ART", "NN", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Das stie\u00df ihm den Dolch in die Augen hinein,", "tokens": ["Das", "stie\u00df", "ihm", "den", "Dolch", "in", "die", "Au\u00b7gen", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Nahm das Sonnenlicht ihm und den Mondenschein,", "tokens": ["Nahm", "das", "Son\u00b7nen\u00b7licht", "ihm", "und", "den", "Mon\u00b7den\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PPER", "KON", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.6": {"text": "Und spuckte ihm in das Gesicht.", "tokens": ["Und", "spuck\u00b7te", "ihm", "in", "das", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Und sie banden ihn in sein Drachenboot", "tokens": ["Und", "sie", "ban\u00b7den", "ihn", "in", "sein", "Dra\u00b7chen\u00b7boot"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Und stie\u00dfen's hinaus in die Flut;", "tokens": ["Und", "stie\u00b7\u00dfen's", "hin\u00b7aus", "in", "die", "Flut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "APZR", "APPR", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Und sie sangen das Lied von der Rune rot,", "tokens": ["Und", "sie", "san\u00b7gen", "das", "Lied", "von", "der", "Ru\u00b7ne", "rot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Von der Blutrune Lust, von der Blutrune Not,", "tokens": ["Von", "der", "Blut\u00b7ru\u00b7ne", "Lust", ",", "von", "der", "Blut\u00b7ru\u00b7ne", "Not", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,", "APPR", "ART", "NN", "NN", "$,"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.5": {"text": "Drei Runen sie gaben ihm mit in den Tod,", "tokens": ["Drei", "Ru\u00b7nen", "sie", "ga\u00b7ben", "ihm", "mit", "in", "den", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "PPER", "VVFIN", "PPER", "APPR", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Drei Runen, so rot wie das Blut:", "tokens": ["Drei", "Ru\u00b7nen", ",", "so", "rot", "wie", "das", "Blut", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "$,", "ADV", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.9": {"line.1": {"text": "\u00bbo du Rune rot, o du Rune sch\u00f6n,", "tokens": ["\u00bb", "o", "du", "Ru\u00b7ne", "rot", ",", "o", "du", "Ru\u00b7ne", "sch\u00f6n", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "p