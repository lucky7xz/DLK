{"textgrid.poem.50229": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "7. Auf einen Sch\u00f6nling", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dass ", "tokens": ["Dass"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Und mit viel M\u00fch' und Pracht sein ", "tokens": ["Und", "mit", "viel", "M\u00fch'", "und", "Pracht", "sein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "NN", "KON", "NN", "PPOSAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dass er sich machen Tag vergn\u00fcgt anschauen kan,", "tokens": ["Dass", "er", "sich", "ma\u00b7chen", "Tag", "ver\u00b7gn\u00fcgt", "an\u00b7schau\u00b7en", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVINF", "NN", "VVPP", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das zeiget kl\u00e4rlich uns des ", "tokens": ["Das", "zei\u00b7get", "kl\u00e4r\u00b7lich", "uns", "des"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADJD", "PPER", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Weil der, der in dem ", "tokens": ["Weil", "der", ",", "der", "in", "dem"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "ART", "$,", "PRELS", "APPR", "ART"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Sein ", "tokens": ["Sein"], "token_info": ["word"], "pos": ["PPOSAT"], "meter": "-", "measure": "single.down"}}}}}