{"textgrid.poem.24400": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Desgleichen", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich lebe nach der Schrifft/ und nach der alten Weise/", "tokens": ["Ich", "le\u00b7be", "nach", "der", "Schrifft", "/", "und", "nach", "der", "al\u00b7ten", "Wei\u00b7se", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$(", "KON", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn Brodt und Wasser war vor diesem aller Speise.", "tokens": ["Denn", "Brodt", "und", "Was\u00b7ser", "war", "vor", "die\u00b7sem", "al\u00b7ler", "Spei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VAFIN", "APPR", "PDAT", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Zwar nach der Mode hei\u00dfts/ ihr Prasser/ nicht gelebt.", "tokens": ["Zwar", "nach", "der", "Mo\u00b7de", "hei\u00dfts", "/", "ihr", "Pras\u00b7ser", "/", "nicht", "ge\u00b7lebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "$(", "PPOSAT", "NN", "$(", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Allein/ wer wei\u00df/ wen man zu erst von uns begr\u00e4bt.", "tokens": ["Al\u00b7lein", "/", "wer", "wei\u00df", "/", "wen", "man", "zu", "erst", "von", "uns", "be\u00b7gr\u00e4bt", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "wo