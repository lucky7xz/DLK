{"textgrid.poem.64525": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "149. Auf Herrn Basedow", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vielleicht ist Basedow ein Irrender, ein Ketzer;", "tokens": ["Viel\u00b7leicht", "ist", "Ba\u00b7se\u00b7dow", "ein", "Ir\u00b7ren\u00b7der", ",", "ein", "Ket\u00b7zer", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NE", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch redlich, Menschenfreund, kein Prahler und kein Schw\u00e4tzer.", "tokens": ["Doch", "red\u00b7lich", ",", "Men\u00b7schen\u00b7freund", ",", "kein", "Prah\u00b7ler", "und", "kein", "Schw\u00e4t\u00b7zer", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "NN", "$,", "PIAT", "NN", "KON", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}