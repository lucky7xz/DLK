{"dta.poem.1038": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Die 1. Antistrophe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1618", "urn": "urn:nbn:de:kobv:b4-20628-9", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Er stost/ vnwissend was geschehen/", "tokens": ["Er", "stost", "/", "vn\u00b7wis\u00b7send", "was", "ge\u00b7sche\u00b7hen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KON", "PWS", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sein mosechtes haubt au\u00df dem flu\u00df/", "tokens": ["Sein", "mo\u00b7sech\u00b7tes", "haubt", "au\u00df", "dem", "flu\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "ART", "NN", "$("], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Vnd weil er forcht einen verdru\u00df/", "tokens": ["Vnd", "weil", "er", "forcht", "ei\u00b7nen", "ver\u00b7dru\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "ART", "NN", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Will Er/ was solches sey/ bald sehen:", "tokens": ["Will", "Er", "/", "was", "sol\u00b7ches", "sey", "/", "bald", "se\u00b7hen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$(", "PWS", "PIS", "VAFIN", "$(", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Seine horn vnd geb\u00fcntzte haar", "tokens": ["Sei\u00b7ne", "horn", "vnd", "ge\u00b7b\u00fcntz\u00b7te", "haar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "ADJA", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "Vil hochspringende b\u00e4ch au\u00dfgiessen/", "tokens": ["Vil", "hoch\u00b7sprin\u00b7gen\u00b7de", "b\u00e4ch", "au\u00df\u00b7gies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Vor seiner nasen vnd bart fliessen", "tokens": ["Vor", "sei\u00b7ner", "na\u00b7sen", "vnd", "bart", "flies\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "KON", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Vil wasserzapffen lang vnd klar:", "tokens": ["Vil", "was\u00b7ser\u00b7zapf\u00b7fen", "lang", "vnd", "klar", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Er sihet nu zu seinen seitten", "tokens": ["Er", "si\u00b7het", "nu", "zu", "sei\u00b7nen", "seit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Ein h\u00f6r gleichsam berait zustreitten:", "tokens": ["Ein", "h\u00f6r", "gleich\u00b7sam", "be\u00b7rait", "zu\u00b7streit\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Er h\u00f6ret der Trometen klang/", "tokens": ["Er", "h\u00f6\u00b7ret", "der", "Tro\u00b7me\u00b7ten", "klang", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVFIN", "$("], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Der kuglen fliegendes gesang/", "tokens": ["Der", "kug\u00b7len", "flie\u00b7gen\u00b7des", "ge\u00b7sang", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Vnd verwundert sich ab dem wunder", "tokens": ["Vnd", "ver\u00b7wun\u00b7dert", "sich", "ab", "dem", "wun\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.14": {"text": "Wie ein eisiner hagel kompt", "tokens": ["Wie", "ein", "ei\u00b7si\u00b7ner", "ha\u00b7gel", "kompt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Mit so starcker kraft macht vnd dunder/", "tokens": ["Mit", "so", "star\u00b7cker", "kraft", "macht", "vnd", "dun\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "VVFIN", "KON", "ADJA", "$("], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.16": {"text": "Warumb widrumb die Trum nu brompt.", "tokens": ["Wa\u00b7rumb", "wid\u00b7rumb", "die", "Trum", "nu", "brompt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}