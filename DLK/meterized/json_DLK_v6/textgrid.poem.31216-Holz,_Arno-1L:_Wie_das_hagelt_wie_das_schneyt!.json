 "iambic.tri"}, "line.6": {"text": "da\u00df ich dich zahrt \u00fcmbbinde;", "tokens": ["da\u00df", "ich", "dich", "zahrt", "\u00fcmb\u00b7bin\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "ich will mich dan auch recht befleissen/", "tokens": ["ich", "will", "mich", "dan", "auch", "recht", "be\u00b7fleis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "ADV", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "nicht in sie hinein zubeissen!", "tokens": ["nicht", "in", "sie", "hin\u00b7ein", "zu\u00b7beis\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Frihrt uns/ gleich so kriechen wir", "tokens": ["Frihrt", "uns", "/", "gleich", "so", "krie\u00b7chen", "wir"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$(", "ADV", "ADV", "VVFIN", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "in das fehdrige Qwartir/", "tokens": ["in", "das", "feh\u00b7dri\u00b7ge", "Qwar\u00b7tir", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$("], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.11": {"text": "wo wir uns zur Seite ruhn", "tokens": ["wo", "wir", "uns", "zur", "Sei\u00b7te", "ruhn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PRF", "APPRART", "NN", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "und mit Recht vertraulich dhun.", "tokens": ["und", "mit", "Recht", "ver\u00b7trau\u00b7lich", "dhun", "."], "token_info": ["