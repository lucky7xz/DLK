{"textgrid.poem.31216": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie das hagelt/ wie das schneyt!", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie das hagelt/ wie das schneyt!", "tokens": ["Wie", "das", "ha\u00b7gelt", "/", "wie", "das", "schneyt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "VVFIN", "$(", "KOKOM", "PDS", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "O du angen\u00e4hme Zeit!", "tokens": ["O", "du", "an\u00b7ge\u00b7n\u00e4h\u00b7me", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der Ofen bufft und knallt/", "tokens": ["Der", "O\u00b7fen", "bufft", "und", "knallt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "das Feuer in ihm tukkert/", "tokens": ["das", "Feu\u00b7er", "in", "ihm", "tuk\u00b7kert", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "VVFIN", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "itzt steht der gantze Wald", "tokens": ["itzt", "steht", "der", "gant\u00b7ze", "Wald"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "mit Ey\u00df bezukkert.", "tokens": ["mit", "Ey\u00df", "be\u00b7zuk\u00b7kert", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Dorillgen