{"textgrid.poem.66012": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1L: R\u00f6tlich schimmern die Beeren aus dichtbebl\u00e4ttertem Buschwerk,", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "R\u00f6tlich schimmern die Beeren aus dichtbebl\u00e4ttertem Buschwerk,", "tokens": ["R\u00f6t\u00b7lich", "schim\u00b7mern", "die", "Bee\u00b7ren", "aus", "dicht\u00b7be\u00b7bl\u00e4t\u00b7ter\u00b7tem", "Busc\u00b7hwerk", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+--+-+---+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Und in den zierlichen Korb pfl\u00fcckst du die zierliche Frucht.", "tokens": ["Und", "in", "den", "zier\u00b7li\u00b7chen", "Korb", "pfl\u00fcckst", "du", "die", "zier\u00b7li\u00b7che", "Frucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Helfend nah ich mich dir, durchwandelnd langsam die Reihen,", "tokens": ["Hel\u00b7fend", "nah", "ich", "mich", "dir", ",", "durch\u00b7wan\u00b7delnd", "lang\u00b7sam", "die", "Rei\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PPER", "PPER", "$,", "VVPP", "ADJD", "ART", "NN", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Bis an einerlei Zweig trifft sich das flei\u00dfige Paar.", "tokens": ["Bis", "an", "ei\u00b7ner\u00b7lei", "Zweig", "trifft", "sich", "das", "flei\u00b7\u00dfi\u00b7ge", "Paar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "PIAT", "NN", "VVFIN", "PRF", "ART", "ADJA", "NN", "$."], "meter": "--+--+---+--+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Und statt der Beeren nun oft ich die Hand und den br\u00e4unlichen Arm dir,", "tokens": ["Und", "statt", "der", "Bee\u00b7ren", "nun", "oft", "ich", "die", "Hand", "und", "den", "br\u00e4un\u00b7li\u00b7chen", "Arm", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "ADV", "PPER", "ART", "NN", "KON", "ART", "ADJA", "NN", "PPER", "$,"], "meter": "-+-+--+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Wie du's dem Losen auch wehrst, hasche im neckischen Spiel.", "tokens": ["Wie", "du's", "dem", "Lo\u00b7sen", "auch", "wehrst", ",", "ha\u00b7sche", "im", "ne\u00b7cki\u00b7schen", "Spiel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ART", "NN", "ADV", "ADV", "$,", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.7": {"text": "Schalkhaft drohst du, ja schmollst, und musst es am Ende doch dulden,", "tokens": ["Schalk\u00b7haft", "drohst", "du", ",", "ja", "schmollst", ",", "und", "musst", "es", "am", "En\u00b7de", "doch", "dul\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "ADV", "VVFIN", "$,", "KON", "VMFIN", "PPER", "APPRART", "NN", "ADV", "VVINF", "$,"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.8": {"text": "Dass er statt r\u00f6tlicher Frucht rosige Lippen erwischt.", "tokens": ["Dass", "er", "statt", "r\u00f6t\u00b7li\u00b7cher", "Frucht", "ro\u00b7si\u00b7ge", "Lip\u00b7pen", "er\u00b7wischt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "ADJA", "NN", "VVPP", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}}}}}