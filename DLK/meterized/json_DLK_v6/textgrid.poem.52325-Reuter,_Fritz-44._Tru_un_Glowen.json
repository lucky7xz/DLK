{"textgrid.poem.52325": {"metadata": {"author": {"name": "Reuter, Fritz", "birth": "N.A.", "death": "N.A."}, "title": "44. Tru un Glowen", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn so de Bur mal in den Kraug", "tokens": ["Wenn", "so", "de", "Bur", "mal", "in", "den", "Kraug"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "NE", "NE", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bi sinen Sluck mit annern sitt,", "tokens": ["Bi", "si\u00b7nen", "Sluck", "mit", "an\u00b7nern", "sitt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "APPR", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn ward dor meistens dr\u00e4hnt ok naug.", "tokens": ["Denn", "ward", "dor", "meis\u00b7tens", "dr\u00e4hnt", "ok", "naug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "VVFIN", "NE", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sei reden denn von dat un dit;", "tokens": ["Sei", "re\u00b7den", "denn", "von", "dat", "un", "dit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVINF", "KON", "APPR", "ART", "FM", "FM", "$."], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.5": {"text": "Bald s\u00fcnd dat L\u00e4uschens, de sei sick vertellen,", "tokens": ["Bald", "s\u00fcnd", "dat", "L\u00e4u\u00b7schens", ",", "de", "sei", "sick", "ver\u00b7tel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "$,", "NE", "VAFIN", "ADJD", "VVINF", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Bald reden s' von de slichten Tiden,", "tokens": ["Bald", "re\u00b7den", "s'", "von", "de", "slich\u00b7ten", "Ti\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "APPR", "NE", "ADJA", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.7": {"text": "Un m\u00e4nnigmal, denn fangen s' an tau schellen:", "tokens": ["Un", "m\u00e4n\u00b7nig\u00b7mal", ",", "denn", "fan\u00b7gen", "s'", "an", "tau", "schel\u00b7len", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "KON", "VVFIN", "NE", "APPR", "NE", "VVINF", "$."], "meter": "+----+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Sei brukten ok nich all'ns tau liden;", "tokens": ["Sei", "bruk\u00b7ten", "ok", "nich", "all'ns", "tau", "li\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+--+-+--", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Un ob de Amtmann gl\u00f6wt, dat sei sin Naren,", "tokens": ["Un", "ob", "de", "Amt\u00b7mann", "gl\u00f6wt", ",", "dat", "sei", "sin", "Na\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "NE", "NE", "VVFIN", "$,", "PAV", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.10": {"text": "Un dat sei gor nich n\u00f6dig hadden,", "tokens": ["Un", "dat", "sei", "gor", "nich", "n\u00f6\u00b7dig", "had\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VAFIN", "NE", "PTKNEG", "ADJD", "VVINF", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.11": {"text": "In allen Order tau parieren,", "tokens": ["In", "al\u00b7len", "Or\u00b7der", "tau", "pa\u00b7rie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Dat sei nahgradens m\u00fcnnig wiren.", "tokens": ["Dat", "sei", "nah\u00b7gra\u00b7dens", "m\u00fcn\u00b7nig", "wi\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "\u00bbja\u00ab, s\u00e4d denn mal oll Bur P\u00e4sel,", "tokens": ["\u00bb", "ja", "\u00ab", ",", "s\u00e4d", "denn", "mal", "oll", "Bur", "P\u00e4\u00b7sel", ","], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKANT", "$(", "$,", "VVFIN", "ADV", "ADV", "ADJD", "NN", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "As sei eins seten in den Kraug tauhopen", "tokens": ["As", "sei", "eins", "se\u00b7ten", "in", "den", "Kraug", "tau\u00b7ho\u00b7pen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "PIS", "VVFIN", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Un em de Gall w\u00fcrd \u00e4werlopen,", "tokens": ["Un", "em", "de", "Gall", "w\u00fcrd", "\u00e4\u00b7wer\u00b7lo\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "NE", "VAFIN", "VVINF", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "\u00bbja! Uns' Herr Amtmann is en Esel!", "tokens": ["\u00bb", "ja", "!", "Un\u00b7s'", "Herr", "Amt\u00b7mann", "is", "en", "E\u00b7sel", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKANT", "$.", "PPOSAT", "NN", "NE", "FM", "FM", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Un wohr is't, un't is ganz gewi\u00df,", "tokens": ["Un", "wohr", "is't", ",", "un't", "is", "ganz", "ge\u00b7wi\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$,", "FM", "FM", "ADV", "ADV", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Dat hei en groten Swinhund is;", "tokens": ["Dat", "hei", "en", "gro\u00b7ten", "Swin\u00b7hund", "is", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Un den'n, de mi't nich will tau gl\u00f6wen,", "tokens": ["Un", "den'n", ",", "de", "mi't", "nich", "will", "tau", "gl\u00f6\u00b7wen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.fr", "FM.fr", "$,", "NE", "NE", "PTKNEG", "VMFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Den'n will ick dat ok schriftlich gewen.\u00ab", "tokens": ["Den'n", "will", "ick", "dat", "ok", "schrift\u00b7lich", "ge\u00b7wen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ART", "NN", "ADJD", "VVPP", "$.", "$("], "meter": "+-+---+-+", "measure": "unknown.measure.tetra"}, "line.9": {"text": "Na, dat w\u00fcrd ok so lang nich duren,", "tokens": ["Na", ",", "dat", "w\u00fcrd", "ok", "so", "lang", "nich", "du\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PAV", "VAFIN", "ADV", "ADV", "ADJD", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Dunn w\u00fc\u00dft de Amtmann, dat de Buren", "tokens": ["Dunn", "w\u00fc\u00dft", "de", "Amt\u00b7mann", ",", "dat", "de", "Bu\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "NE", "NE", "$,", "PRELS", "NE", "NE"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Em l\u00e4sterlich utschullen hadden,", "tokens": ["Em", "l\u00e4s\u00b7ter\u00b7lich", "ut\u00b7schul\u00b7len", "had\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Un namentlich, dat Bur P\u00e4sel", "tokens": ["Un", "na\u00b7ment\u00b7lich", ",", "dat", "Bur", "P\u00e4\u00b7sel"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ADV", "$,", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Hadd seggt, hei wir en wohren Esel.", "tokens": ["Hadd", "seggt", ",", "hei", "wir", "en", "woh\u00b7ren", "E\u00b7sel", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "VAFIN", "PPER", "FM", "ADJA", "NN", "$."], "meter": "--+--+---", "measure": "anapaest.di.plus"}, "line.14": {"text": "Hei let sei all tau Amt nu laden", "tokens": ["Hei", "let", "sei", "all", "tau", "Amt", "nu", "la\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["FM", "FM", "VAFIN", "PIAT", "CARD", "NN", "ADV", "VVINF"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.15": {"text": "Un let sei tau Gerichtsdag kamen.", "tokens": ["Un", "let", "sei", "tau", "Ge\u00b7richts\u00b7dag", "ka\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VAFIN", "CARD", "NN", "VVFIN", "$."], "meter": "+-++-+-+-", "measure": "unknown.measure.penta"}, "line.16": {"text": "Dor w\u00fcrden sei denn nu vernamen,", "tokens": ["Dor", "w\u00fcr\u00b7den", "sei", "denn", "nu", "ver\u00b7na\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "VAFIN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Un enzeln w\u00fcrd en jeder fragt:", "tokens": ["Un", "en\u00b7zeln", "w\u00fcrd", "en", "je\u00b7der", "fragt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVINF", "VAFIN", "FM", "PIS", "VVFIN", "$."], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.18": {"text": "\u00bbhat Bauer P\u00e4sel das gesagt?\u00ab", "tokens": ["\u00bb", "hat", "Bau\u00b7er", "P\u00e4\u00b7sel", "das", "ge\u00b7sagt", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VAFIN", "NE", "NE", "PDS", "VVPP", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "\u00bbick weit dat nich, ick was nich dor.\u00ab", "tokens": ["\u00bb", "ick", "weit", "dat", "nich", ",", "ick", "was", "nich", "dor.", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "abbreviation", "punct"], "pos": ["$(", "PPER", "ADJD", "PAV", "PTKNEG", "$,", "PPER", "PIS", "PTKNEG", "ADV", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.20": {"text": "\u00bbih, Gott bewohr! Dat is nich wohr.\u00ab", "tokens": ["\u00bb", "ih", ",", "Gott", "be\u00b7wohr", "!", "Dat", "is", "nich", "wohr", ".", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "$,", "NN", "VVFIN", "$.", "FM", "FM", "PTKNEG", "ADJD", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "\u00bbherr Amtmann, ne! Dat ick nich w\u00fc\u00dft.\u00ab", "tokens": ["\u00bb", "herr", "Amt\u00b7mann", ",", "ne", "!", "Dat", "ick", "nich", "w\u00fc\u00dft", ".", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "NN", "NE", "$,", "NE", "$.", "NN", "PPER", "PTKNEG", "VVFIN", "$.", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.22": {"text": "\u00bbdat hadd ick doch ok h\u00fcren m\u00fc\u00dft.\u00ab", "tokens": ["\u00bb", "dat", "hadd", "ick", "doch", "ok", "h\u00fc\u00b7ren", "m\u00fc\u00dft", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "PPER", "ADV", "ADJD", "VVINF", "VMFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "\u00bbwi heww'n von slichte Tiden seggt.\u00ab", "tokens": ["\u00bb", "wi", "hew\u00b7w'n", "von", "slich\u00b7te", "Ti\u00b7den", "seggt", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWAV", "VVFIN", "APPR", "ADJA", "NN", "VVFIN", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.24": {"text": "\u00bbick h\u00fcr up't linke Uhr nich recht.\u00ab", "tokens": ["\u00bb", "ick", "h\u00fcr", "up't", "lin\u00b7ke", "Uhr", "nich", "recht", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "ADV", "ADJD", "ADJA", "NN", "PTKNEG", "ADJD", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Kort, keiner wull dorvon wat weiten,", "tokens": ["Kort", ",", "kei\u00b7ner", "wull", "dor\u00b7von", "wat", "wei\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PIS", "VMFIN", "PAV", "VVFIN", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Dat P\u00e4sel em en Swinhund heiten.", "tokens": ["Dat", "P\u00e4\u00b7sel", "em", "en", "Swin\u00b7hund", "hei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "An Bur M\u00f6llern kamm tauletzt de Frag',", "tokens": ["An", "Bur", "M\u00f6l\u00b7lern", "kamm", "tau\u00b7letzt", "de", "Frag'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VVFIN", "ADV", "NE", "NE", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.28": {"text": "De was man dumm un ok man zag;", "tokens": ["De", "was", "man", "dumm", "un", "ok", "man", "zag", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PWS", "PIS", "ADJD", "FM", "FM", "FM", "FM", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "De Amtmann fohrt em eklig in de P'r\u00fck", "tokens": ["De", "Amt\u00b7mann", "fohrt", "em", "ek\u00b7lig", "in", "de", "P'\u00b7r\u00fck"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Un f\u00fchrt em h\u00e4\u00dflich an den Wagen.", "tokens": ["Un", "f\u00fchrt", "em", "h\u00e4\u00df\u00b7lich", "an", "den", "Wa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PIS", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "\u00bbwenn Hei nich seggt de Wohrheit glik,", "tokens": ["\u00bb", "wenn", "Hei", "nich", "seggt", "de", "Wohr\u00b7heit", "glik", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "NN", "PTKNEG", "VVFIN", "NE", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Denn lat ick krumm un lahm Em slagen.", "tokens": ["Denn", "lat", "ick", "krumm", "un", "lahm", "Em", "sla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "FM", "FM", "FM", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.33": {"text": "Wat s\u00e4d tau Em de Bur P\u00e4sel?", "tokens": ["Wat", "s\u00e4d", "tau", "Em", "de", "Bur", "P\u00e4\u00b7sel", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+--+-+--", "measure": "iambic.tri.invert"}, "line.34": {"text": "Herute mit de Sprak! Wat wir't?\u00ab", "tokens": ["He\u00b7ru\u00b7te", "mit", "de", "Sprak", "!", "Wat", "wir't", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["NN", "APPR", "NE", "NE", "$.", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "\u00bbach ja, Herr Amtmann, ja! Ick heww dat h\u00fcrt,", "tokens": ["\u00bb", "ach", "ja", ",", "Herr", "Amt\u00b7mann", ",", "ja", "!", "Ick", "heww", "dat", "h\u00fcrt", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "ADV", "$,", "NN", "NE", "$,", "ADV", "$.", "FM.nl", "FM.nl", "FM.nl", "FM.nl", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.36": {"text": "Hei s\u00e4d, Sei wir'n en rechten Esel.", "tokens": ["Hei", "s\u00e4d", ",", "Sei", "wir'n", "en", "rech\u00b7ten", "E\u00b7sel", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "$,", "FM", "FM", "FM", "ADJA", "NN", "$."], "meter": "+--+-+---