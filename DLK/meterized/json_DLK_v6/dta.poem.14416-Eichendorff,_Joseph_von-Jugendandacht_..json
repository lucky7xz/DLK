{"dta.poem.14416": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Jugendandacht .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.85", "nl:0.14"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df des verlor'nen Himmels es ged\u00e4chte,", "tokens": ["Da\u00df", "des", "ver\u00b7lor'\u00b7nen", "Him\u00b7mels", "es", "ge\u00b7d\u00e4ch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Schlagen an's Herz des Fr\u00fchlings linde Wellen,", "tokens": ["Schla\u00b7gen", "an's", "Herz", "des", "Fr\u00fch\u00b7lings", "lin\u00b7de", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Wie ew'ger Wonnen sch\u00fcchternes Vermuthen.", "tokens": ["Wie", "ew'\u00b7ger", "Won\u00b7nen", "sch\u00fcch\u00b7ter\u00b7nes", "Ver\u00b7mu\u00b7then", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Geheimer Glanz der lauen Sommern\u00e4chte,", "tokens": ["Ge\u00b7hei\u00b7mer", "Glanz", "der", "lau\u00b7en", "Som\u00b7mer\u00b7n\u00e4ch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Du gr\u00fcner Wald, verf\u00fchrend Lied der Quellen,", "tokens": ["Du", "gr\u00fc\u00b7ner", "Wald", ",", "ver\u00b7f\u00fch\u00b7rend", "Lied", "der", "Quel\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "VVPP", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Des Morgens Pracht, stillbl\u00fch'nde Abendgluthen,", "tokens": ["Des", "Mor\u00b7gens", "Pracht", ",", "still\u00b7bl\u00fch'n\u00b7de", "A\u00b7bend\u00b7glu\u00b7then", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADV", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ihr fragt: wo Schmerz und Lust so lange ruhten,", "tokens": ["Ihr", "fragt", ":", "wo", "Schmerz", "und", "Lust", "so", "lan\u00b7ge", "ruh\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PWAV", "NN", "KON", "NN", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die s\u00fc\u00df das Herz verdunkeln und es hellen?", "tokens": ["Die", "s\u00fc\u00df", "das", "Herz", "ver\u00b7dun\u00b7keln", "und", "es", "hel\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ART", "NN", "VVINF", "KON", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Wie thut ihr zaub'risch auf die alten Wunden,", "tokens": ["Wie", "thut", "ihr", "zaub'\u00b7risch", "auf", "die", "al\u00b7ten", "Wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Da\u00df losgebunden in das Licht sie bluten!", "tokens": ["Da\u00df", "los\u00b7ge\u00b7bun\u00b7den", "in", "das", "Licht", "sie", "blu\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "APPR", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "O seel'ge Zeit entfloss'ner Himmelbl\u00e4ue,", "tokens": ["O", "seel'\u00b7ge", "Zeit", "ent\u00b7floss'\u00b7ner", "Him\u00b7mel\u00b7bl\u00e4u\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Der ersten Andacht solch inbr\u00fcnst'ger Liebe,", "tokens": ["Der", "ers\u00b7ten", "An\u00b7dacht", "solch", "in\u00b7br\u00fcnst'\u00b7ger", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Die ewig wollte knieen vor der Einen!", "tokens": ["Die", "e\u00b7wig", "woll\u00b7te", "kni\u00b7e\u00b7en", "vor", "der", "Ei\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VMFIN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Dem\u00fcthig in der Glorie des Maien", "tokens": ["De\u00b7m\u00fct\u00b7hig", "in", "der", "Glo\u00b7rie", "des", "Mai\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+---+--+-", "measure": "iambic.tri.relaxed"}, "line.15": {"text": "Hob sie den Schleier oft, da\u00df offen bliebe", "tokens": ["Hob", "sie", "den", "Schlei\u00b7er", "oft", ",", "da\u00df", "of\u00b7fen", "blie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "$,", "KOUS", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Der Augen Himmel, in das Land zu scheinen.", "tokens": ["Der", "Au\u00b7gen", "Him\u00b7mel", ",", "in", "das", "Land", "zu", "schei\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Und stand ich still, und mu\u00dft' ich herzlich weinen", "tokens": ["Und", "stand", "ich", "still", ",", "und", "mu\u00dft'", "ich", "herz\u00b7lich", "wei\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "VMFIN", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "In Ihrem Blick gereinigt alle Triebe:", "tokens": ["In", "Ih\u00b7rem", "Blick", "ge\u00b7rei\u00b7nigt", "al\u00b7le", "Trie\u00b7be", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Da war nur Wonne, was ich mu\u00dfte klagen,", "tokens": ["Da", "war", "nur", "Won\u00b7ne", ",", "was", "ich", "mu\u00df\u00b7te", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "NN", "$,", "PWS", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Im Angesicht der Stillen, Ewigreinen", "tokens": ["Im", "An\u00b7ge\u00b7sicht", "der", "Stil\u00b7len", ",", "E\u00b7wig\u00b7rei\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Kein Schmerz, als solcher Liebe Lieb' ertragen!", "tokens": ["Kein", "Schmerz", ",", "als", "sol\u00b7cher", "Lie\u00b7be", "Lieb'", "er\u00b7tra\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "KOUS", "PIAT", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}