{"textgrid.poem.31362": {"metadata": {"author": {"name": "Kleist, Heinrich von", "birth": "N.A.", "death": "N.A."}, "title": "[3. Fassung]", "genre": "verse", "period": "N.A.", "pub_year": 1794, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Erw\u00e4g ich, wie in jenen Schreckenstagen,", "tokens": ["Er\u00b7w\u00e4g", "ich", ",", "wie", "in", "je\u00b7nen", "Schre\u00b7ckens\u00b7ta\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "PWAV", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Still deine Brust verschlossen, was sie litt,", "tokens": ["Still", "dei\u00b7ne", "Brust", "ver\u00b7schlos\u00b7sen", ",", "was", "sie", "litt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "VVPP", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie du das Ungl\u00fcck, mit der Grazie Tritt,", "tokens": ["Wie", "du", "das", "Un\u00b7gl\u00fcck", ",", "mit", "der", "Gra\u00b7zie", "Tritt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "$,", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Auf jungen Schultern herrlich hast getragen,", "tokens": ["Auf", "jun\u00b7gen", "Schul\u00b7tern", "herr\u00b7lich", "hast", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wie von des Kriegs zerri\u00dfnem Schlachtenwagen", "tokens": ["Wie", "von", "des", "Kriegs", "zer\u00b7ri\u00df\u00b7nem", "Schlach\u00b7ten\u00b7wa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Selbst oft die Schar der M\u00e4nner zu dir schritt,", "tokens": ["Selbst", "oft", "die", "Schar", "der", "M\u00e4n\u00b7ner", "zu", "dir", "schritt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "ART", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie, trotz der Wunde, die dein Herz durchschnitt,", "tokens": ["Wie", ",", "trotz", "der", "Wun\u00b7de", ",", "die", "dein", "Herz", "durch\u00b7schnitt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "APPR", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Du stets der Hoffnung Fahn uns vorgetragen:", "tokens": ["Du", "stets", "der", "Hoff\u00b7nung", "Fahn", "uns", "vor\u00b7ge\u00b7tra\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "NE", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "O Herrscherin, die Zeit dann m\u00f6cht ich segnen!", "tokens": ["O", "Herr\u00b7sche\u00b7rin", ",", "die", "Zeit", "dann", "m\u00f6cht", "ich", "seg\u00b7nen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ART", "NN", "ADV", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wir sahn dich Anmut endlos niederregnen,", "tokens": ["Wir", "sahn", "dich", "An\u00b7mut", "end\u00b7los", "nie\u00b7der\u00b7reg\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie gro\u00df du warst, das ahndeten wir nicht!", "tokens": ["Wie", "gro\u00df", "du", "warst", ",", "das", "ahn\u00b7de\u00b7ten", "wir", "nicht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "VAFIN", "$,", "PDS", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Dein Haupt scheint wie von Strahlen mir umschimmert;", "tokens": ["Dein", "Haupt", "scheint", "wie", "von", "Strah\u00b7len", "mir", "um\u00b7schim\u00b7mert", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "KOKOM", "APPR", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du bist der Stern, der voller Pracht erst flimmert,", "tokens": ["Du", "bist", "der", "Stern", ",", "der", "vol\u00b7ler", "Pracht", "erst", "flim\u00b7mert", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "ART", "ADJA", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn er durch finstre Wetterwolken bricht!", "tokens": ["Wenn", "er", "durch", "finst\u00b7re", "Wet\u00b7ter\u00b7wol\u00b7ken", "bricht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}