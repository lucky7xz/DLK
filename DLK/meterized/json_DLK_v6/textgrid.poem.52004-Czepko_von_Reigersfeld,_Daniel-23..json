{"textgrid.poem.52004": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "23.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es w\u00e4re bald geschehn, als um den Oder Port", "tokens": ["Es", "w\u00e4\u00b7re", "bald", "ge\u00b7schehn", ",", "als", "um", "den", "O\u00b7der", "Port"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "$,", "KOUS", "APPR", "ART", "NE", "NE"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auch in das Steuerbuch die K\u00fche man genommen,", "tokens": ["Auch", "in", "das", "Steu\u00b7er\u00b7buch", "die", "K\u00fc\u00b7he", "man", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "NN", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So br\u00fcllten sie weit aus (So kam Europa fort.)", "tokens": ["So", "br\u00fcll\u00b7ten", "sie", "weit", "aus", "(", "So", "kam", "Eu\u00b7ro\u00b7pa", "fort", ".", ")"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PTKVZ", "$(", "ADV", "VVFIN", "NE", "PTKVZ", "$.", "$("], "meter": "-+--++-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Ach da\u00df doch \u00fcber See ein Ochse wolte kommen.", "tokens": ["Ach", "da\u00df", "doch", "\u00fc\u00b7ber", "See", "ein", "O\u00b7chse", "wol\u00b7te", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "KOUS", "ADV", "APPR", "NN", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}