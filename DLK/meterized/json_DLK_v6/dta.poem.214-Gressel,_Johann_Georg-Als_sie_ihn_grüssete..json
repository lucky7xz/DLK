{"dta.poem.214": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Als sie ihn gr\u00fcssete.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "So wie der Morgen lacht die Welt mit Blicken an/", "tokens": ["So", "wie", "der", "Mor\u00b7gen", "lacht", "die", "Welt", "mit", "Bli\u00b7cken", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "VVFIN", "ART", "NN", "APPR", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So l\u00e4\u00dft du auch mein Kind/ mir Blick und Gru\u00df geniessen/", "tokens": ["So", "l\u00e4\u00dft", "du", "auch", "mein", "Kind", "/", "mir", "Blick", "und", "Gru\u00df", "ge\u00b7nies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$(", "PPER", "NN", "KON", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die jetzo auf mich zu aus deinem Antlitz schiessen/", "tokens": ["Die", "jet\u00b7zo", "auf", "mich", "zu", "aus", "dei\u00b7nem", "Ant\u00b7litz", "schies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PPER", "PTKZU", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df sich mein Geist fast nicht vor Freuden halten kan.", "tokens": ["Da\u00df", "sich", "mein", "Geist", "fast", "nicht", "vor", "Freu\u00b7den", "hal\u00b7ten", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PPOSAT", "NN", "ADV", "PTKNEG", "APPR", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es geht mein Hoffnungs-Stern beym hellen Morgen auf/", "tokens": ["Es", "geht", "mein", "Hoff\u00b7nungs\u00b7S\u00b7tern", "beym", "hel\u00b7len", "Mor\u00b7gen", "auf", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "APPR", "$("], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Die schwartze Trauer-Nacht und Finsterni\u00df verschwindet/", "tokens": ["Die", "schwart\u00b7ze", "Trau\u00b7e\u00b7rNacht", "und", "Fins\u00b7ter\u00b7ni\u00df", "ver\u00b7schwin\u00b7det", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "In deinem Antlitz man den frohen Mittag findet/", "tokens": ["In", "dei\u00b7nem", "Ant\u00b7litz", "man", "den", "fro\u00b7hen", "Mit\u00b7tag", "fin\u00b7det", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PIS", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Darinn die Sch\u00f6nheits-Sonn nie \u00e4ndert ihren Lauff.", "tokens": ["Da\u00b7rinn", "die", "Sch\u00f6n\u00b7heits\u00b7Sonn", "nie", "\u00e4n\u00b7dert", "ih\u00b7ren", "Lauff", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und da\u00df dieselbige stets unver\u00e4ndert sey/", "tokens": ["Und", "da\u00df", "die\u00b7sel\u00b7bi\u00b7ge", "stets", "un\u00b7ver\u00b7\u00e4n\u00b7dert", "sey", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PDS", "ADV", "ADJD", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Da\u00df zeigen/ Sch\u00f6nste/ an die angenehmen Augen/", "tokens": ["Da\u00df", "zei\u00b7gen", "/", "Sch\u00f6ns\u00b7te", "/", "an", "die", "an\u00b7ge\u00b7neh\u00b7men", "Au\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVINF", "$(", "NN", "$(", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Daraus man Flammen kan der holden Liebe saugen/", "tokens": ["Da\u00b7raus", "man", "Flam\u00b7men", "kan", "der", "hol\u00b7den", "Lie\u00b7be", "sau\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PIS", "NN", "VMFIN", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Man legt dir drum mit recht den Prei\u00df der Sch\u00f6nheit bey.", "tokens": ["Man", "legt", "dir", "drum", "mit", "recht", "den", "Prei\u00df", "der", "Sch\u00f6n\u00b7heit", "bey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PAV", "APPR", "ADJD", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "So strahlt da immerfort der Anmuth-Sonnenschein/", "tokens": ["So", "strahlt", "da", "im\u00b7mer\u00b7fort", "der", "An\u00b7muth\u00b7Son\u00b7nen\u00b7schein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Er kan und will niemahls bey dir zu Gnaden gehen/", "tokens": ["Er", "kan", "und", "will", "nie\u00b7mahls", "bey", "dir", "zu", "Gna\u00b7den", "ge\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "KON", "VMFIN", "ADV", "APPR", "PPER", "APPR", "NN", "VVINF", "$("], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "Des Adlers Auge kan wol in die Sonne sehen/", "tokens": ["Des", "Ad\u00b7lers", "Au\u00b7ge", "kan", "wol", "in", "die", "Son\u00b7ne", "se\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "ADV", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Doch in dein Auge schaut so gar kein ", "tokens": ["Doch", "in", "dein", "Au\u00b7ge", "schaut", "so", "gar", "kein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVFIN", "ADV", "ADV", "PIAT"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.17": {"text": "Denn das Geheimni\u00df/ so daselbst verborgen ist/", "tokens": ["Denn", "das", "Ge\u00b7heim\u00b7ni\u00df", "/", "so", "da\u00b7selbst", "ver\u00b7bor\u00b7gen", "ist", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$(", "ADV", "PAV", "VVPP", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Kan keiner/ als der liebt/ und da\u00df getreu/ ergr\u00fcnden/", "tokens": ["Kan", "kei\u00b7ner", "/", "als", "der", "liebt", "/", "und", "da\u00df", "ge\u00b7treu", "/", "er\u00b7gr\u00fcn\u00b7den", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "PIS", "$(", "KOUS", "ART", "VVFIN", "$(", "KON", "KOUS", "ADJD", "$(", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Wem du die Blicke g\u00f6nnst/ der kan es leichtlich finden/", "tokens": ["Wem", "du", "die", "Bli\u00b7cke", "g\u00f6nnst", "/", "der", "kan", "es", "leicht\u00b7lich", "fin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "NN", "VVFIN", "$(", "ART", "VMFIN", "PPER", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Wie sich die Lust dadurch mit der Vergn\u00fcgung k\u00fc\u00dft.", "tokens": ["Wie", "sich", "die", "Lust", "da\u00b7durch", "mit", "der", "Ver\u00b7gn\u00fc\u00b7gung", "k\u00fc\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "NN", "PAV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.21": {"text": "Du bist mein liebster Schatz die Blicke/ so du giebst/", "tokens": ["Du", "bist", "mein", "liebs\u00b7ter", "Schatz", "die", "Bli\u00b7cke", "/", "so", "du", "giebst", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "ADJA", "NN", "ART", "NN", "$(", "ADV", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Mich/ wenn ich traurig bin/ alsbald erfreuen k\u00f6nnen/", "tokens": ["Mich", "/", "wenn", "ich", "trau\u00b7rig", "bin", "/", "als\u00b7bald", "er\u00b7freu\u00b7en", "k\u00f6n\u00b7nen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "KOUS", "PPER", "ADJD", "VAFIN", "$(", "ADV", "VVINF", "VMINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Ich kan dich meine Lust und mein Vergn\u00fcgen nennen/", "tokens": ["Ich", "kan", "dich", "mei\u00b7ne", "Lust", "und", "mein", "Ver\u00b7gn\u00fc\u00b7gen", "nen\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Du gr\u00fc\u00dft mich/ weil ich dich/ und du mich wieder liebst.", "tokens": ["Du", "gr\u00fc\u00dft", "mich", "/", "weil", "ich", "dich", "/", "und", "du", "mich", "wie\u00b7der", "liebst", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$(", "KOUS", "PPER", "PRF", "$(", "KON", "PPER", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}