{"textgrid.poem.64716": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "343. Antwort auf einen Theil von der Fr. Pr. B. Briefe", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "W\u00e4r ich auch morgen nicht zu haben,", "tokens": ["W\u00e4r", "ich", "auch", "mor\u00b7gen", "nicht", "zu", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "PTKNEG", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den Truthahn fr\u00f6hlich zu begraben,", "tokens": ["Den", "Trut\u00b7hahn", "fr\u00f6h\u00b7lich", "zu", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So sende nicht herum nach Kr\u00fcppeln und nach Zwergen,", "tokens": ["So", "sen\u00b7de", "nicht", "he\u00b7rum", "nach", "Kr\u00fcp\u00b7peln", "und", "nach", "Zwer\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKNEG", "ADV", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ganz nah' hast du ja L -.", "tokens": ["Ganz", "nah'", "hast", "du", "ja", "L", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPER", "ADV", "NE", "$(", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}