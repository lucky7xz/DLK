{"textgrid.poem.24204": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Guter Rat", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mach dich von Sehnsucht frei und leb im Licht!", "tokens": ["Mach", "dich", "von", "Sehn\u00b7sucht", "frei", "und", "leb", "im", "Licht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "ADJD", "KON", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Haus nicht im Tr\u00e4umerschlo\u00df mit Spiegelw\u00e4nden!", "tokens": ["Haus", "nicht", "im", "Tr\u00e4u\u00b7mer\u00b7schlo\u00df", "mit", "Spie\u00b7gel\u00b7w\u00e4n\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "APPRART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du glaubst, das ist die Welt, was sie dir blenden, \u2013", "tokens": ["Du", "glaubst", ",", "das", "ist", "die", "Welt", ",", "was", "sie", "dir", "blen\u00b7den", ",", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "$,", "PDS", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "PPER", "VVFIN", "$,", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ach nein, die Welt, mein Junge, ist das nicht.", "tokens": ["Ach", "nein", ",", "die", "Welt", ",", "mein", "Jun\u00b7ge", ",", "ist", "das", "nicht", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKANT", "$,", "ART", "NN", "$,", "PPOSAT", "NN", "$,", "VAFIN", "PDS", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Es ist ein t\u00fcckisches Spiel des schiefen Schliffs", "tokens": ["Es", "ist", "ein", "t\u00fc\u00b7cki\u00b7sches", "Spiel", "des", "schie\u00b7fen", "Schliffs"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und oft wohl gar Betrug, was sie dir zeigen.", "tokens": ["Und", "oft", "wohl", "gar", "Be\u00b7trug", ",", "was", "sie", "dir", "zei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADV", "NN", "$,", "PRELS", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hinaus! Schlag sie entzwei, aufs Pferd zu steigen", "tokens": ["Hin\u00b7aus", "!", "Schlag", "sie", "ent\u00b7zwei", ",", "aufs", "Pferd", "zu", "stei\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$.", "NN", "PPER", "PTKVZ", "$,", "APPRART", "NN", "PTKZU", "VVINF"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Beherzten Sprungs und festen Z\u00fcgelgriffs!", "tokens": ["Be\u00b7herz\u00b7ten", "Sprungs", "und", "fes\u00b7ten", "Z\u00fc\u00b7gel\u00b7griffs", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Hussa Galopp! Und mitten in die Welt!", "tokens": ["Hus\u00b7sa", "Ga\u00b7lopp", "!", "Und", "mit\u00b7ten", "in", "die", "Welt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "KON", "ADV", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Sei nur Sankt J\u00f6rg, und du findst Ungeheuer", "tokens": ["Sei", "nur", "Sankt", "J\u00f6rg", ",", "und", "du", "findst", "Un\u00b7ge\u00b7heu\u00b7er"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "VVFIN", "NE", "$,", "KON", "PPER", "VVFIN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und schreitst durch Teufelsflut und Drachenfeuer", "tokens": ["Und", "schreitst", "durch", "Teu\u00b7fels\u00b7flut", "und", "Dra\u00b7chen\u00b7feu\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zu einem sch\u00f6nen Kind, das dir gef\u00e4llt", "tokens": ["Zu", "ei\u00b7nem", "sch\u00f6\u00b7nen", "Kind", ",", "das", "dir", "ge\u00b7f\u00e4llt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und dir zum Lohn f\u00fcr deine Abenteuer", "tokens": ["Und", "dir", "zum", "Lohn", "f\u00fcr", "dei\u00b7ne", "A\u00b7bent\u00b7eu\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPRART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die holdste Frucht aus Gottes gro\u00dfer Scheuer,", "tokens": ["Die", "holds\u00b7te", "Frucht", "aus", "Got\u00b7tes", "gro\u00b7\u00dfer", "Scheu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Den vollen Mund zum Ku\u00df entgegenh\u00e4lt.", "tokens": ["Den", "vol\u00b7len", "Mund", "zum", "Ku\u00df", "ent\u00b7ge\u00b7gen\u00b7h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}