{"textgrid.poem.49150": {"metadata": {"author": {"name": "R\u00f6ling, Johann", "birth": "N.A.", "death": "N.A."}, "title": "Andacht", "genre": "verse", "period": "N.A.", "pub_year": 1656, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich lebe, doch nicht ich,", "tokens": ["Ich", "le\u00b7be", ",", "doch", "nicht", "ich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADV", "PTKNEG", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Nein, der belebet mich,", "tokens": ["Nein", ",", "der", "be\u00b7le\u00b7bet", "mich", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PRELS", "VVFIN", "PPER", "$,"], "meter": "++-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der alles Lebens Leben;", "tokens": ["Der", "al\u00b7les", "Le\u00b7bens", "Le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mein Leben war sein Tod,", "tokens": ["Mein", "Le\u00b7ben", "war", "sein", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und seine Todes-Noth", "tokens": ["Und", "sei\u00b7ne", "To\u00b7des\u00b7Noth"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Mu\u00df mir das Leben geben.", "tokens": ["Mu\u00df", "mir", "das", "Le\u00b7ben", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich lebe, doch nicht mir,", "tokens": ["Ich", "le\u00b7be", ",", "doch", "nicht", "mir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADV", "PTKNEG", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Mein Wandel ist nicht hier;", "tokens": ["Mein", "Wan\u00b7del", "ist", "nicht", "hier", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mich auff zu ihm zu heben", "tokens": ["Mich", "auff", "zu", "ihm", "zu", "he\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "APPR", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sterb' ich mir t\u00e4glich ab,", "tokens": ["Sterb'", "ich", "mir", "t\u00e4g\u00b7lich", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dies ist das heilge Grab,", "tokens": ["Dies", "ist", "das", "heil\u00b7ge", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Darinn er gern mag leben.", "tokens": ["Da\u00b7rinn", "er", "gern", "mag", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Was Erd' und Welt an mir,", "tokens": ["Was", "Erd'", "und", "Welt", "an", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "KON", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Das f\u00e4llt und faulet hier,", "tokens": ["Das", "f\u00e4llt", "und", "fau\u00b7let", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KON", "VVFIN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was sein, steht unbeweget", "tokens": ["Was", "sein", ",", "steht", "un\u00b7be\u00b7we\u00b7get"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PWS", "VAINF", "$,", "VVFIN", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und wird gantz neu verkl\u00e4rt,", "tokens": ["Und", "wird", "gantz", "neu", "ver\u00b7kl\u00e4rt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wenn sich die Welt und Erd'", "tokens": ["Wenn", "sich", "die", "Welt", "und", "Erd'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "ART", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Eins in die Asche leget.", "tokens": ["Eins", "in", "die", "A\u00b7sche", "le\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Denn zieh ich ihn wol an,", "tokens": ["Denn", "zieh", "ich", "ihn", "wol", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wenn ich mich abgethan,", "tokens": ["Wenn", "ich", "mich", "ab\u00b7ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sein Kleid und meines beyde,", "tokens": ["Sein", "Kleid", "und", "mei\u00b7nes", "bey\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "PIS", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die einigen sich nicht.", "tokens": ["Die", "ei\u00b7ni\u00b7gen", "sich", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "PRF", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wie f\u00fcgt sich Nacht und Licht,", "tokens": ["Wie", "f\u00fcgt", "sich", "Nacht", "und", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PRF", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Wie h\u00e4ren Tuch und Seide?", "tokens": ["Wie", "h\u00e4\u00b7ren", "Tuch", "und", "Sei\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "KON", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ach Jesu, ich wil dein,", "tokens": ["Ach", "Je\u00b7su", ",", "ich", "wil", "dein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "ITJ", "$,", "PPER", "VMFIN", "PPOSAT", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Nicht mein, gantz mein nicht seyn,", "tokens": ["Nicht", "mein", ",", "gantz", "mein", "nicht", "seyn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPOSAT", "$,", "ADV", "PPOSAT", "PTKNEG", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sey du auch mein nicht minder;", "tokens": ["Sey", "du", "auch", "mein", "nicht", "min\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PPOSAT", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich la\u00dfe mich gantz dir,", "tokens": ["Ich", "la\u00b7\u00dfe", "mich", "gantz", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nimm mich und gib dich mir,", "tokens": ["Nimm", "mich", "und", "gib", "dich", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "KON", "VVIMP", "PPER", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Tausch Gott mit einem S\u00fcnder.", "tokens": ["Tausch", "Gott", "mit", "ei\u00b7nem", "S\u00fcn\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}