{"textgrid.poem.37616": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Seit nun Gott die Welt durchschnitten", "genre": "verse", "period": "N.A.", "pub_year": 1807, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seit nun Gott die Welt durchschnitten", "tokens": ["Seit", "nun", "Gott", "die", "Welt", "durch\u00b7schnit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "NN", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit der Allmacht sausend Schwert,", "tokens": ["Mit", "der", "All\u00b7macht", "sau\u00b7send", "Schwert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Liegt in Tag und Nacht inmitten,", "tokens": ["Liegt", "in", "Tag", "und", "Nacht", "in\u00b7mit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wer des Weines Becher leert:", "tokens": ["Wer", "des", "Wei\u00b7nes", "Be\u00b7cher", "leert", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Tief und dunkel zieht der Becher,", "tokens": ["Tief", "und", "dun\u00b7kel", "zieht", "der", "Be\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Licht und strahlend singt der Zecher,", "tokens": ["Licht", "und", "strah\u00b7lend", "singt", "der", "Ze\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Schwingt den Huth und jubelnd singt,", "tokens": ["Schwingt", "den", "Huth", "und", "ju\u00b7belnd", "singt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Da\u00df der Becher schwirrend springt.", "tokens": ["Da\u00df", "der", "Be\u00b7cher", "schwir\u00b7rend", "springt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "So soll Wein die Welt verbinden,", "tokens": ["So", "soll", "Wein", "die", "Welt", "ver\u00b7bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "NN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die getrennt in Licht und Nacht,", "tokens": ["Die", "ge\u00b7trennt", "in", "Licht", "und", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie die Lichter mir verschwinden,", "tokens": ["Wie", "die", "Lich\u00b7ter", "mir", "ver\u00b7schwin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Scheinet licht, was ich gedacht,", "tokens": ["Schei\u00b7net", "licht", ",", "was", "ich", "ge\u00b7dacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "PWS", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df nun alle mit mir singen,", "tokens": ["Da\u00df", "nun", "al\u00b7le", "mit", "mir", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PIS", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mu\u00df mir Herz und Mund aufspringen,", "tokens": ["Mu\u00df", "mir", "Herz", "und", "Mund", "auf\u00b7sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "NN", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Ja des Paradieses Baum", "tokens": ["Ja", "des", "Pa\u00b7ra\u00b7die\u00b7ses", "Baum"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKANT", "ART", "NN", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Hat in diesem Keller Raum.", "tokens": ["Hat", "in", "die\u00b7sem", "Kel\u00b7ler", "Raum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PDAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Seht, es steigt aus mir hernieder", "tokens": ["Seht", ",", "es", "steigt", "aus", "mir", "her\u00b7nie\u00b7der"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PPER", "VVFIN", "APPR", "PPER", "PTKVZ"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lucifer, der lang verbannt,", "tokens": ["Lu\u00b7ci\u00b7fer", ",", "der", "lang", "ver\u00b7bannt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Er und Bachus sind zwei Br\u00fcder,", "tokens": ["Er", "und", "Ba\u00b7chus", "sind", "zwei", "Br\u00fc\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "NE", "VAFIN", "CARD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Es erscheint ein neues Land", "tokens": ["Es", "er\u00b7scheint", "ein", "neu\u00b7es", "Land"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Weingelaubt der J\u00fcnger Schaaren,", "tokens": ["Wein\u00b7ge\u00b7laubt", "der", "J\u00fcn\u00b7ger", "Schaa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Flammen in des Waldes Haaren", "tokens": ["Flam\u00b7men", "in", "des", "Wal\u00b7des", "Haa\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Leuchten durch die D\u00e4mmerung", "tokens": ["Leuch\u00b7ten", "durch", "die", "D\u00e4m\u00b7me\u00b7rung"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.8": {"text": "Alle in erhabnen Schwung.", "tokens": ["Al\u00b7le", "in", "er\u00b7hab\u00b7nen", "Schwung", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Panther, L\u00f6w' und blaue Schlangen", "tokens": ["Pan\u00b7ther", ",", "L\u00f6\u00b7w'", "und", "blau\u00b7e", "Schlan\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "KON", "ADJA", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Liegen auf dem R\u00fccken schon;", "tokens": ["Lie\u00b7gen", "auf", "dem", "R\u00fc\u00b7cken", "schon", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Faunenweibchen, ohne Bangen", "tokens": ["Fau\u00b7nen\u00b7weib\u00b7chen", ",", "oh\u00b7ne", "Ban\u00b7gen"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "KOUI", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "S\u00e4ugst du Tieger ohne Lohn?", "tokens": ["S\u00e4ugst", "du", "Tie\u00b7ger", "oh\u00b7ne", "Lohn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "K\u00f6nnen sie dich nicht mehr missen,", "tokens": ["K\u00f6n\u00b7nen", "sie", "dich", "nicht", "mehr", "mis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "PTKNEG", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Einen hab ich abgerissen,", "tokens": ["Ei\u00b7nen", "hab", "ich", "ab\u00b7ge\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Der hing fest an deiner Brust,", "tokens": ["Der", "hing", "fest", "an", "dei\u00b7ner", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Nimm mein S\u00f6hnlein dran zur Lust.", "tokens": ["Nimm", "mein", "S\u00f6hn\u00b7lein", "dran", "zur", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "PAV", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Was erblick ich, die Gesellen", "tokens": ["Was", "er\u00b7blick", "ich", ",", "die", "Ge\u00b7sel\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "$,", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Halten Kronen rings f\u00fcr mich,", "tokens": ["Hal\u00b7ten", "Kro\u00b7nen", "rings", "f\u00fcr", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ADV", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wollt ihr euch wie Menschen stellen,", "tokens": ["Wollt", "ihr", "euch", "wie", "Men\u00b7schen", "stel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "KOKOM", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Oder bin ein Gott auch ich?", "tokens": ["O\u00b7der", "bin", "ein", "Gott", "auch", "ich", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ADV", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nun so kann ich euch begl\u00fccken,", "tokens": ["Nun", "so", "kann", "ich", "euch", "be\u00b7gl\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Kann erschaffen mit Entz\u00fccken,", "tokens": ["Kann", "er\u00b7schaf\u00b7fen", "mit", "Ent\u00b7z\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVPP", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Heute schaff ich euch die Welt,", "tokens": ["Heu\u00b7te", "schaff", "ich", "euch", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Wie ein jeder sie bestellt.", "tokens": ["Wie", "ein", "je\u00b7der", "sie", "be\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "PIS", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Tanzet munter, tretet Leimen,", "tokens": ["Tan\u00b7zet", "mun\u00b7ter", ",", "tre\u00b7tet", "Lei\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tretet Rosenbl\u00e4tter drein,", "tokens": ["Tre\u00b7tet", "Ro\u00b7sen\u00b7bl\u00e4t\u00b7ter", "drein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ich will schon t\u00fcchtig reimen,", "tokens": ["Und", "ich", "will", "schon", "t\u00fcch\u00b7tig", "rei\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "ADV", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Feuchtet an den Stoff mit Wein,", "tokens": ["Feuch\u00b7tet", "an", "den", "Stoff", "mit", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "La\u00dft den Honig aus den Zellen,", "tokens": ["La\u00dft", "den", "Ho\u00b7nig", "aus", "den", "Zel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Seht wie schl\u00e4gt der Wein nun Wellen,", "tokens": ["Seht", "wie", "schl\u00e4gt", "der", "Wein", "nun", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "VVFIN", "ART", "NN", "ADV", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Macht den Kopf zur T\u00f6pferscheib,", "tokens": ["Macht", "den", "Kopf", "zur", "T\u00f6p\u00b7fer\u00b7scheib", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Menschen formt zum Zeitvertreib.", "tokens": ["Men\u00b7schen", "formt", "zum", "Zeit\u00b7ver\u00b7treib", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Lebe jeder, der's verlanget,", "tokens": ["Le\u00b7be", "je\u00b7der", ",", "der's", "ver\u00b7lan\u00b7get", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PIS", "$,", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sterbe, wer nicht leben mag,", "tokens": ["Ster\u00b7be", ",", "wer", "nicht", "le\u00b7ben", "mag", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was der Br\u00fcder Herz erlanget", "tokens": ["Was", "der", "Br\u00fc\u00b7der", "Herz", "er\u00b7lan\u00b7get"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "ART", "NN", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und verlanget, jeder sag,", "tokens": ["Und", "ver\u00b7lan\u00b7get", ",", "je\u00b7der", "sag", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Was der Wein jetzt offenbaret", "tokens": ["Was", "der", "Wein", "jetzt", "of\u00b7fen\u00b7ba\u00b7ret"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "ART", "NN", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sinkt in Nacht, wenn Tag uns klaret,", "tokens": ["Sinkt", "in", "Nacht", ",", "wenn", "Tag", "uns", "kla\u00b7ret", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$,", "KOUS", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Nur der Augenblick sei ganz", "tokens": ["Nur", "der", "Au\u00b7gen\u00b7blick", "sei", "ganz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VAFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Offner Herzen Flammenkranz.", "tokens": ["Off\u00b7ner", "Her\u00b7zen", "Flam\u00b7men\u00b7kranz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Ich, der Becher geh im Kreise,", "tokens": ["Ich", ",", "der", "Be\u00b7cher", "geh", "im", "Krei\u00b7se", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tausend Geister send' ich euch,", "tokens": ["Tau\u00b7send", "Geis\u00b7ter", "send'", "ich", "euch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PPER", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jeder bleib bei seiner Weise,", "tokens": ["Je\u00b7der", "bleib", "bei", "sei\u00b7ner", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bin ich doch f\u00fcr alle reich.", "tokens": ["Bin", "ich", "doch", "f\u00fcr", "al\u00b7le", "reich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "PIS", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie ein Meer ich kann euch fassen", "tokens": ["Wie", "ein", "Meer", "ich", "kann", "euch", "fas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "PPER", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und die Welt, sie liegt im Nassen,", "tokens": ["Und", "die", "Welt", ",", "sie", "liegt", "im", "Nas\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Jedem wird ein Schatz gezeigt,", "tokens": ["Je\u00b7dem", "wird", "ein", "Schatz", "ge\u00b7zeigt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Der sein Haupt recht tief mir neigt.", "tokens": ["Der", "sein", "Haupt", "recht", "tief", "mir", "neigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADV", "ADJD", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Kommt, ihr meine lust'gen B\u00f6cke,", "tokens": ["Kommt", ",", "ihr", "mei\u00b7ne", "lust'\u00b7gen", "B\u00f6\u00b7cke", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf die h\u00f6chste Felsenspitz',", "tokens": ["Auf", "die", "h\u00f6chs\u00b7te", "Fel\u00b7sen\u00b7spitz'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Pflanzt mir da die sch\u00f6nsten St\u00f6cke,", "tokens": ["Pflanzt", "mir", "da", "die", "sch\u00f6ns\u00b7ten", "St\u00f6\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df der Wein hochthronend sitz',", "tokens": ["Da\u00df", "der", "Wein", "hoch\u00b7thro\u00b7nend", "sitz'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Unter lichten Rebenlauben", "tokens": ["Un\u00b7ter", "lich\u00b7ten", "Re\u00b7ben\u00b7lau\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sto\u00dfen Ziegen sich um Trauben,", "tokens": ["Sto\u00b7\u00dfen", "Zie\u00b7gen", "sich", "um", "Trau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PRF", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Mir zum Munde spritzt der Saft,", "tokens": ["Mir", "zum", "Mun\u00b7de", "spritzt", "der", "Saft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Alle Welt ist voller Kraft.", "tokens": ["Al\u00b7le", "Welt", "ist", "vol\u00b7ler", "Kraft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}