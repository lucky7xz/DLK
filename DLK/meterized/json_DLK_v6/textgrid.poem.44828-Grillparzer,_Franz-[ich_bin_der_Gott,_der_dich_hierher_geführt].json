{"textgrid.poem.44828": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[ich bin der Gott, der dich hierher gef\u00fchrt]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin der Gott, der dich hierher gef\u00fchrt,", "tokens": ["Ich", "bin", "der", "Gott", ",", "der", "dich", "hier\u00b7her", "ge\u00b7f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "PRF", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nicht ", "tokens": ["Nicht"], "token_info": ["word"], "pos": ["PTKNEG"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Man nennt mich den Gedanken, nein, das Denken,", "tokens": ["Man", "nennt", "mich", "den", "Ge\u00b7dan\u00b7ken", ",", "nein", ",", "das", "Den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ART", "NN", "$,", "PTKANT", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des Denkens Denken, die Abwesenheit", "tokens": ["Des", "Den\u00b7kens", "Den\u00b7ken", ",", "die", "Ab\u00b7we\u00b7sen\u00b7heit"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN"], "meter": "-+-+--++-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Des Denkens, des Gedachten allzumal.", "tokens": ["Des", "Den\u00b7kens", ",", "des", "Ge\u00b7dach\u00b7ten", "all\u00b7zu\u00b7mal", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ich bin das Nichts, das sich im Nichts beschaut", "tokens": ["Ich", "bin", "das", "Nichts", ",", "das", "sich", "im", "Nichts", "be\u00b7schaut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "PIS", "$,", "PRELS", "PRF", "APPRART", "PIS", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und Nichts erzeugt, den eingebornen Sohn.", "tokens": ["Und", "Nichts", "er\u00b7zeugt", ",", "den", "ein\u00b7ge\u00b7bor\u00b7nen", "Sohn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVPP", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}