{"dta.poem.11129": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Die beregnete Doris.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der regen w\u00e4sserte die arme Doris ein,", "tokens": ["Der", "re\u00b7gen", "w\u00e4s\u00b7ser\u00b7te", "die", "ar\u00b7me", "Do\u00b7ris", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "ART", "ADJA", "NE", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als sie spatzieren gieng, und nicht ans wetter dachte.", "tokens": ["Als", "sie", "spat\u00b7zie\u00b7ren", "gieng", ",", "und", "nicht", "ans", "wet\u00b7ter", "dach\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "VVFIN", "$,", "KON", "PTKNEG", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Jhr leute, lachet doch! Sie will ein engel seyn,", "tokens": ["Ihr", "leu\u00b7te", ",", "la\u00b7chet", "doch", "!", "Sie", "will", "ein", "en\u00b7gel", "seyn", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "ADV", "$.", "PPER", "VMFIN", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und wuste dennoch nicht, was man im himmel machte.", "tokens": ["Und", "wus\u00b7te", "den\u00b7noch", "nicht", ",", "was", "man", "im", "him\u00b7mel", "mach\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PTKNEG", "$,", "PRELS", "PIS", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}