{"dta.poem.11750": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "36.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du bist ein Muttersohn, und von der Mutterbrust", "tokens": ["Du", "bist", "ein", "Mut\u00b7ter\u00b7sohn", ",", "und", "von", "der", "Mut\u00b7ter\u00b7brust"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "KON", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Noch nicht entw\u00f6hnt, sie ist noch immer deine Lust.", "tokens": ["Noch", "nicht", "ent\u00b7w\u00f6hnt", ",", "sie", "ist", "noch", "im\u00b7mer", "dei\u00b7ne", "Lust", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "VVPP", "$,", "PPER", "VAFIN", "ADV", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Du bist ein Muttersohn, doch an der Mutterbrust", "tokens": ["Du", "bist", "ein", "Mut\u00b7ter\u00b7sohn", ",", "doch", "an", "der", "Mut\u00b7ter\u00b7brust"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hast du den Vater selbst geahnt in stiller Lust.", "tokens": ["Hast", "du", "den", "Va\u00b7ter", "selbst", "ge\u00b7ahnt", "in", "stil\u00b7ler", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADV", "VVPP", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Du bist ein Muttersohn, doch auch des Vaters Kind,", "tokens": ["Du", "bist", "ein", "Mut\u00b7ter\u00b7sohn", ",", "doch", "auch", "des", "Va\u00b7ters", "Kind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "ADV", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der auch die Kinder liebt, die lieb der Mutter sind.", "tokens": ["Der", "auch", "die", "Kin\u00b7der", "liebt", ",", "die", "lieb", "der", "Mut\u00b7ter", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VVFIN", "$,", "PRELS", "ADJD", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}