{"textgrid.poem.62293": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "1L: Waldv\u00f6glein zwitschert im Walde allein:", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Waldv\u00f6glein zwitschert im Walde allein:", "tokens": ["Wald\u00b7v\u00f6\u00b7glein", "zwit\u00b7schert", "im", "Wal\u00b7de", "al\u00b7lein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "ADV", "$."], "meter": "+---+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Werden wir niemals gleichgestellt sein", "tokens": ["Wer\u00b7den", "wir", "nie\u00b7mals", "gleich\u00b7ge\u00b7stellt", "sein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "VVFIN", "PPOSAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Jenen, die wir durch Lieder erfreu'n,", "tokens": ["Je\u00b7nen", ",", "die", "wir", "durch", "Lie\u00b7der", "er\u00b7freu'n", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Und von all ihren Sorgen zerstreu'n?", "tokens": ["Und", "von", "all", "ih\u00b7ren", "Sor\u00b7gen", "zer\u00b7streu'n", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "PPOSAT", "NN", "VVINF", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}}, "stanza.2": {"line.1": {"text": "Horch, aus Geb\u00fcsch und Bl\u00fctenflor", "tokens": ["Horch", ",", "aus", "Ge\u00b7b\u00fcsch", "und", "Bl\u00fc\u00b7ten\u00b7flor"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "APPR", "NN", "KON", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "T\u00f6net hervor ein lustiger Chor:", "tokens": ["T\u00f6\u00b7net", "her\u00b7vor", "ein", "lus\u00b7ti\u00b7ger", "Chor", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "ART", "ADJA", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Meinest Du etwa das Menschengeschlecht,", "tokens": ["Mei\u00b7nest", "Du", "et\u00b7wa", "das", "Men\u00b7schen\u00b7ge\u00b7schlecht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Ewig unmenschlich und ungerecht? \u2013", "tokens": ["E\u00b7wig", "un\u00b7menschlich", "und", "un\u00b7ge\u00b7recht", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "$.", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.3": {"line.1": {"text": "Ach, nicht unsre Lieder bei Tag und Nacht,", "tokens": ["Ach", ",", "nicht", "uns\u00b7re", "Lie\u00b7der", "bei", "Tag", "und", "Nacht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PTKNEG", "PPOSAT", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Ach, nicht unsre Sch\u00f6nheitund Farbenpracht,", "tokens": ["Ach", ",", "nicht", "uns\u00b7re", "Sch\u00f6n\u00b7hei\u00b7tund", "Far\u00b7ben\u00b7pracht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PTKNEG", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Keinerlei bricht seinen Uebermut,", "tokens": ["Kei\u00b7ner\u00b7lei", "bricht", "sei\u00b7nen", "Ue\u00b7ber\u00b7mut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Lechzend immer nach Fleisch und nach Blut!", "tokens": ["Lech\u00b7zend", "im\u00b7mer", "nach", "Fleisch", "und", "nach", "Blut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Waldv\u00f6glein zittert, leise es weint,", "tokens": ["Wald\u00b7v\u00f6\u00b7glein", "zit\u00b7tert", ",", "lei\u00b7se", "es", "weint", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "ADJD", "PPER", "VVFIN", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Abendsonne es golden bescheint,", "tokens": ["A\u00b7bend\u00b7son\u00b7ne", "es", "gol\u00b7den", "be\u00b7scheint", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJD", "VVPP", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Pl\u00f6tzlich ruft es: Menschengeschlecht,", "tokens": ["Pl\u00f6tz\u00b7lich", "ruft", "es", ":", "Men\u00b7schen\u00b7ge\u00b7schlecht", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "$.", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Jedes Gesch\u00f6pf hat des Lebens Recht! \u2013", "tokens": ["Je\u00b7des", "Ge\u00b7sch\u00f6pf", "hat", "des", "Le\u00b7bens", "Recht", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ART", "NN", "NN", "$.", "$("], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}}}}