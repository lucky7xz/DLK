{"textgrid.poem.35971": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du V\u00f6glein, sei gegr\u00fc\u00dfet mir", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du V\u00f6glein, sei gegr\u00fc\u00dfet mir", "tokens": ["Du", "V\u00f6\u00b7glein", ",", "sei", "ge\u00b7gr\u00fc\u00b7\u00dfet", "mir"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "NN", "$,", "VAFIN", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In meinen Blumenkronen!", "tokens": ["In", "mei\u00b7nen", "Blu\u00b7men\u00b7kro\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du V\u00f6glein bunt, ist dir wie mir", "tokens": ["Du", "V\u00f6\u00b7glein", "bunt", ",", "ist", "dir", "wie", "mir"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "NN", "ADJD", "$,", "VAFIN", "PPER", "PWAV", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So wohl in Blumen wohnen?", "tokens": ["So", "wohl", "in", "Blu\u00b7men", "woh\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Dann wirst aus meinem Garten hier", "tokens": ["Dann", "wirst", "aus", "mei\u00b7nem", "Gar\u00b7ten", "hier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wohl nimmer wieder gehen,", "tokens": ["Wohl", "nim\u00b7mer", "wie\u00b7der", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du V\u00f6glein traut, ist dir wie mir", "tokens": ["Du", "V\u00f6\u00b7glein", "traut", ",", "ist", "dir", "wie", "mir"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "NN", "VVFIN", "$,", "VAFIN", "PPER", "PWAV", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So \u00fcbel schon geschehen?", "tokens": ["So", "\u00fc\u00b7bel", "schon", "ge\u00b7sche\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Hast sollen von Wald und Blumen fern", "tokens": ["Hast", "sol\u00b7len", "von", "Wald", "und", "Blu\u00b7men", "fern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "APPR", "NN", "KON", "NN", "PTKVZ"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Im goldnen K\u00e4fig leben?", "tokens": ["Im", "gold\u00b7nen", "K\u00e4\u00b7fig", "le\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wollten dir stolze Damen und Herrn", "tokens": ["Woll\u00b7ten", "dir", "stol\u00b7ze", "Da\u00b7men", "und", "Herrn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADJA", "NN", "KON", "NN"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Zucker f\u00fcr Freiheit geben?", "tokens": ["Zu\u00b7cker", "f\u00fcr", "Frei\u00b7heit", "ge\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Hast du die Leutlein ausgelacht", "tokens": ["Hast", "du", "die", "Leut\u00b7lein", "aus\u00b7ge\u00b7lacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit ihren goldnen Spangen?", "tokens": ["Mit", "ih\u00b7ren", "gold\u00b7nen", "Span\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Hast dich heimlich davongemacht,", "tokens": ["Hast", "dich", "heim\u00b7lich", "da\u00b7von\u00b7ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "L\u00e4\u00dft dich nicht wieder fangen?", "tokens": ["L\u00e4\u00dft", "dich", "nicht", "wie\u00b7der", "fan\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Hast du die sch\u00f6nste Rose gesehn", "tokens": ["Hast", "du", "die", "sch\u00f6ns\u00b7te", "Ro\u00b7se", "ge\u00b7sehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "VVPP"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Dort dr\u00fcben bei der Linden?", "tokens": ["Dort", "dr\u00fc\u00b7ben", "bei", "der", "Lin\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du kannst durch alle W\u00e4lder sp\u00e4hn,", "tokens": ["Du", "kannst", "durch", "al\u00b7le", "W\u00e4l\u00b7der", "sp\u00e4hn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wirst keine sch\u00f6n're finden.", "tokens": ["Wirst", "kei\u00b7ne", "sch\u00f6n'\u00b7re", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "ADJA", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Drum mach dir nur ein Nestchen fein", "tokens": ["Drum", "mach", "dir", "nur", "ein", "Nest\u00b7chen", "fein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In meinen Blumenkronen,", "tokens": ["In", "mei\u00b7nen", "Blu\u00b7men\u00b7kro\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wir wollen wie die Engelein", "tokens": ["Wir", "wol\u00b7len", "wie", "die", "En\u00b7ge\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In sch\u00f6nen Blumen wohnen.", "tokens": ["In", "sch\u00f6\u00b7nen", "Blu\u00b7men", "woh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "In Blumen singen den ganzen Tag,", "tokens": ["In", "Blu\u00b7men", "sin\u00b7gen", "den", "gan\u00b7zen", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "In Blumen die \u00c4uglein schlie\u00dfen.", "tokens": ["In", "Blu\u00b7men", "die", "\u00c4ug\u00b7lein", "schlie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Fliegst aber \u00fcber den M\u00fchlenbach,", "tokens": ["Fliegst", "a\u00b7ber", "\u00fc\u00b7ber", "den", "M\u00fch\u00b7len\u00b7bach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sollst du die Rose dort gr\u00fc\u00dfen.", "tokens": ["Sollst", "du", "die", "Ro\u00b7se", "dort", "gr\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "ADV", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}}}}