{"textgrid.poem.32753": {"metadata": {"author": {"name": "Miller, Johann Martin", "birth": "N.A.", "death": "N.A."}, "title": "Das M\u00fcndner Thal", "genre": "verse", "period": "N.A.", "pub_year": 1774, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich kenn' ein liebes, sch\u00f6nes Thal,", "tokens": ["Ich", "kenn'", "ein", "lie\u00b7bes", ",", "sch\u00f6\u00b7nes", "Thal", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das denk' ich Tages tausendmal,", "tokens": ["Das", "denk'", "ich", "Ta\u00b7ges", "tau\u00b7send\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wandl' auf seiner gr\u00fcnen Flur,", "tokens": ["Und", "wandl'", "auf", "sei\u00b7ner", "gr\u00fc\u00b7nen", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch, ach, in falschen Tr\u00e4umen nur.", "tokens": ["Doch", ",", "ach", ",", "in", "fal\u00b7schen", "Tr\u00e4u\u00b7men", "nur", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ITJ", "$,", "APPR", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Da kr\u00f6nen W\u00e4lder, sch\u00f6nbelaubt,", "tokens": ["Da", "kr\u00f6\u00b7nen", "W\u00e4l\u00b7der", ",", "sch\u00f6n\u00b7be\u00b7laubt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der milden Berge stolzes Haupt,", "tokens": ["Der", "mil\u00b7den", "Ber\u00b7ge", "stol\u00b7zes", "Haupt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und Quellen rieseln hell und frisch", "tokens": ["Und", "Quel\u00b7len", "rie\u00b7seln", "hell", "und", "frisch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Herab in niedres Schleegeb\u00fcsch.", "tokens": ["Her\u00b7ab", "in", "nie\u00b7dres", "Schlee\u00b7ge\u00b7b\u00fcsch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Da ist der Wiese gr\u00fcnes Kleid", "tokens": ["Da", "ist", "der", "Wie\u00b7se", "gr\u00fc\u00b7nes", "Kleid"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit bunten Blumen \u00fcberstreut;", "tokens": ["Mit", "bun\u00b7ten", "Blu\u00b7men", "\u00fc\u00b7bers\u00b7treut", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da schallet, wenn die Sonne flieht,", "tokens": ["Da", "schal\u00b7let", ",", "wenn", "die", "Son\u00b7ne", "flieht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des wohlbelohnten Flei\u00dfes Lied.", "tokens": ["Des", "wohl\u00b7be\u00b7lohn\u00b7ten", "Flei\u00b7\u00dfes", "Lied", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und im vergn\u00fcgten St\u00e4dtchen freut", "tokens": ["Und", "im", "ver\u00b7gn\u00fcg\u00b7ten", "St\u00e4dt\u00b7chen", "freut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich Unschuld und Vertraulichkeit,", "tokens": ["Sich", "Un\u00b7schuld", "und", "Ver\u00b7trau\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In seinen Thoren k\u00fcssen frei", "tokens": ["In", "sei\u00b7nen", "Tho\u00b7ren", "k\u00fcs\u00b7sen", "frei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich Redlichkeit und deutsche Treu.", "tokens": ["Sich", "Red\u00b7lich\u00b7keit", "und", "deut\u00b7sche", "Treu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Zween Str\u00f6me gr\u00fc\u00dfen br\u00fcderlich", "tokens": ["Zween", "Str\u00f6\u00b7me", "gr\u00fc\u00b7\u00dfen", "br\u00fc\u00b7der\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An seinen stillen Mauren sich;", "tokens": ["An", "sei\u00b7nen", "stil\u00b7len", "Mau\u00b7ren", "sich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Umarmen sich in Einer Bahn,", "tokens": ["Um\u00b7ar\u00b7men", "sich", "in", "Ei\u00b7ner", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und str\u00f6men freudiger heran.", "tokens": ["Und", "str\u00f6\u00b7men", "freu\u00b7di\u00b7ger", "he\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "So str\u00f6men in der Abendruh", "tokens": ["So", "str\u00f6\u00b7men", "in", "der", "A\u00b7ben\u00b7druh"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich hier verwandte Seelen zu;", "tokens": ["Sich", "hier", "ver\u00b7wand\u00b7te", "See\u00b7len", "zu", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So ward mein Herz mit einem Freund", "tokens": ["So", "ward", "mein", "Herz", "mit", "ei\u00b7nem", "Freund"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und einer Freundin hier vereint.", "tokens": ["Und", "ei\u00b7ner", "Freun\u00b7din", "hier", "ver\u00b7eint", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Was, von Begeisterung entgl\u00fcht,", "tokens": ["Was", ",", "von", "Be\u00b7geis\u00b7te\u00b7rung", "ent\u00b7gl\u00fcht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Dichter nur in Tr\u00e4umen sieht,", "tokens": ["Ein", "Dich\u00b7ter", "nur", "in", "Tr\u00e4u\u00b7men", "sieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Des goldnen Alters ganzes Gl\u00fcck", "tokens": ["Des", "gold\u00b7nen", "Al\u00b7ters", "gan\u00b7zes", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kam bei den Edeln mir zur\u00fcck.", "tokens": ["Kam", "bei", "den", "E\u00b7deln", "mir", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Ihr Herz, voll deutscher Redlichkeit,", "tokens": ["Ihr", "Herz", ",", "voll", "deut\u00b7scher", "Red\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist dir, o Tugend, nur geweiht,", "tokens": ["Ist", "dir", ",", "o", "Tu\u00b7gend", ",", "nur", "ge\u00b7weiht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "FM", "NN", "$,", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ist jedem braven deutschen Man", "tokens": ["Ist", "je\u00b7dem", "bra\u00b7ven", "deut\u00b7schen", "Man"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PIAT", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und allem Sch\u00f6nen zugethan.", "tokens": ["Und", "al\u00b7lem", "Sch\u00f6\u00b7nen", "zu\u00b7ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Sie hassen, stolz aufs Vaterland,", "tokens": ["Sie", "has\u00b7sen", ",", "stolz", "aufs", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der ** H\u00f6flichkeit und Tand;", "tokens": ["Der", "*", "*", "H\u00f6f\u00b7lich\u00b7keit", "und", "Tand", ";"], "token_info": ["word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ART", "XY", "XY", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sind offen, lieben Saitenklang,", "tokens": ["Sind", "of\u00b7fen", ",", "lie\u00b7ben", "Sai\u00b7ten\u00b7klang", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und ehren Vaterlandsgesang.", "tokens": ["Und", "eh\u00b7ren", "Va\u00b7ter\u00b7lands\u00b7ge\u00b7sang", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "O, wenn erblick' ich, liebes Thal,", "tokens": ["O", ",", "wenn", "er\u00b7blick'", "ich", ",", "lie\u00b7bes", "Thal", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In dir die Edeln noch einmal,", "tokens": ["In", "dir", "die", "E\u00b7deln", "noch", "ein\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df, frei von dieser W\u00fcstenei,", "tokens": ["Da\u00df", ",", "frei", "von", "die\u00b7ser", "W\u00fcs\u00b7te\u00b7nei", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "ADJD", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich wieder meine Seele freu'?", "tokens": ["Sich", "wie\u00b7der", "mei\u00b7ne", "See\u00b7le", "freu'", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}