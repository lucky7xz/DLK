{"textgrid.poem.29246": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "61.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lingus schilt den Nachbar sehr, da\u00df er ihm in seine Wand", "tokens": ["Lin\u00b7gus", "schilt", "den", "Nach\u00b7bar", "sehr", ",", "da\u00df", "er", "ihm", "in", "sei\u00b7ne", "Wand"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "NN", "ADV", "$,", "KOUS", "PPER", "PPER", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Heimlich einen Balcken schub, gleichwol dieses nie bekant;", "tokens": ["Heim\u00b7lich", "ei\u00b7nen", "Bal\u00b7cken", "schub", ",", "gleich\u00b7wol", "die\u00b7ses", "nie", "be\u00b7kant", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$,", "ADV", "PDAT", "ADV", "ADJD", "$."], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Wann er solte wissen das, was er mehr sonst eingeschoben,", "tokens": ["Wann", "er", "sol\u00b7te", "wis\u00b7sen", "das", ",", "was", "er", "mehr", "sonst", "ein\u00b7ge\u00b7scho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VMFIN", "VVINF", "PDS", "$,", "PWS", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "(seine Frau, die wei\u00df es wol,) w\u00fcrd er ihn noch minder loben.", "tokens": ["(", "sei\u00b7ne", "Frau", ",", "die", "wei\u00df", "es", "wol", ",", ")", "w\u00fcrd", "er", "ihn", "noch", "min\u00b7der", "lo\u00b7ben", "."], "token_info": [