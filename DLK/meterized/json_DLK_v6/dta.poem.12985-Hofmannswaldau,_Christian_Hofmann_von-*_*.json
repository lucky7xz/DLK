{"dta.poem.12985": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "* *", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Macht mir nicht so ein werck aus den verdorbnen Heyden,", "tokens": ["Macht", "mir", "nicht", "so", "ein", "werck", "aus", "den", "ver\u00b7dorb\u00b7nen", "Hey\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKNEG", "ADV", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "So ihnen ohne furcht das leben abgek\u00fcrtzt:", "tokens": ["So", "ih\u00b7nen", "oh\u00b7ne", "furcht", "das", "le\u00b7ben", "ab\u00b7ge\u00b7k\u00fcrtzt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "APPR", "NN", "PDS", "VVINF", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sie hatten, da sie sich selbst in den tod gest\u00fcrtzt,", "tokens": ["Sie", "hat\u00b7ten", ",", "da", "sie", "sich", "selbst", "in", "den", "tod", "ge\u00b7st\u00fcrtzt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "PPER", "PRF", "ADV", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "In diesem leben noch viel ungemach zu leiden.", "tokens": ["In", "die\u00b7sem", "le\u00b7ben", "noch", "viel", "un\u00b7ge\u00b7mach", "zu", "lei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "VVFIN", "ADV", "ADV", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Das leben hatte dann nichts s\u00fc\u00dfes mehr vor sie:", "tokens": ["Das", "le\u00b7ben", "hat\u00b7te", "dann", "nichts", "s\u00fc\u00b7\u00dfes", "mehr", "vor", "sie", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "VAFIN", "ADV", "PIS", "ADJA", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ein tod war leidlicher als tausend auszustehen.", "tokens": ["Ein", "tod", "war", "leid\u00b7li\u00b7cher", "als", "tau\u00b7send", "aus\u00b7zu\u00b7ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KOKOM", "CARD", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Jm ungel\u00fccke nicht aus ungeduld vergehen,", "tokens": ["Jm", "un\u00b7ge\u00b7l\u00fc\u00b7cke", "nicht", "aus", "un\u00b7ge\u00b7duld", "ver\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKNEG", "APPR", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ist schwerer, als der tod; der kostet schlechte m\u00fch.", "tokens": ["Ist", "schwe\u00b7rer", ",", "als", "der", "tod", ";", "der", "kos\u00b7tet", "schlech\u00b7te", "m\u00fch", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "KOUS", "ART", "NN", "$.", "ART", "VVFIN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}