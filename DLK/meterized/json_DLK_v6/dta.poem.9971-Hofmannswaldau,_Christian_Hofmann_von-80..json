{"dta.poem.9971": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "80.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Aller orten werd ich an- und zu gleicher zeit getroffen/", "tokens": ["Al\u00b7ler", "or\u00b7ten", "werd", "ich", "an", "und", "zu", "glei\u00b7cher", "zeit", "ge\u00b7trof\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "PPER", "TRUNC", "KON", "APPR", "ADJA", "NN", "VVPP", "$("], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Ich bin k\u00fchn und habe st\u00e4rcke/ vor mir zittert alle welt:", "tokens": ["Ich", "bin", "k\u00fchn", "und", "ha\u00b7be", "st\u00e4r\u00b7cke", "/", "vor", "mir", "zit\u00b7tert", "al\u00b7le", "welt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "KON", "VAFIN", "VVFIN", "$(", "APPR", "PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "F\u00fcrsten habe\u0303 nicht mehr gnad\u2019 als ihr volck bey mir zu hoffen/", "tokens": ["F\u00fcrs\u00b7ten", "hab\u1ebd", "nicht", "mehr", "gnad'", "als", "ihr", "volck", "bey", "mir", "zu", "hof\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PTKNEG", "ADV", "ADJD", "KOKOM", "PPOSAT", "NN", "APPR", "PPER", "PTKZU", "VVINF", "$("], "meter": "+-+--+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Komm ich bald zu dem und jenem wo ich will und mirs gef\u00e4llt.", "tokens": ["Komm", "ich", "bald", "zu", "dem", "und", "je\u00b7nem", "wo", "ich", "will", "und", "mirs", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "KON", "PDAT", "PWAV", "PPER", "VMFIN", "KON", "NE", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}