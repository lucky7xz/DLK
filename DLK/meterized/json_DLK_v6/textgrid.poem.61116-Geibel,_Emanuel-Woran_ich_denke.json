{"textgrid.poem.61116": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Woran ich denke", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Woran ich denk'? - An meines Lebens Morgen,", "tokens": ["Wo\u00b7ran", "ich", "denk'", "?", "An", "mei\u00b7nes", "Le\u00b7bens", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$.", "$(", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als noch so ungest\u00fcm, so frei von Sorgen", "tokens": ["Als", "noch", "so", "un\u00b7ge\u00b7st\u00fcm", ",", "so", "frei", "von", "Sor\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ADV", "ADJD", "$,", "ADV", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das jugendliche Herz mir schlug,", "tokens": ["Das", "ju\u00b7gend\u00b7li\u00b7che", "Herz", "mir", "schlug", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als vor mir, ein besonnter Meeresspiegel,", "tokens": ["Als", "vor", "mir", ",", "ein", "be\u00b7sonn\u00b7ter", "Mee\u00b7res\u00b7spie\u00b7gel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPER", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die Hoffnung lag, als der Gedanke Fl\u00fcgel,", "tokens": ["Die", "Hoff\u00b7nung", "lag", ",", "als", "der", "Ge\u00b7dan\u00b7ke", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KOUS", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und als die Liebe Rosen trug.", "tokens": ["Und", "als", "die", "Lie\u00b7be", "Ro\u00b7sen", "trug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Da weilt' ich abends, ohne zu ermatten,", "tokens": ["Da", "weilt'", "ich", "a\u00b7bends", ",", "oh\u00b7ne", "zu", "er\u00b7mat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "KOUI", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im Regen, nur um einen fl\u00fccht'gen Schatten", "tokens": ["Im", "Re\u00b7gen", ",", "nur", "um", "ei\u00b7nen", "fl\u00fccht'\u00b7gen", "Schat\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Am hellen Fenster zu ersp\u00e4hn;", "tokens": ["Am", "hel\u00b7len", "Fens\u00b7ter", "zu", "er\u00b7sp\u00e4hn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und selig war ich, durft' ich aus der Ferne", "tokens": ["Und", "se\u00b7lig", "war", "ich", ",", "durft'", "ich", "aus", "der", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VAFIN", "PPER", "$,", "VMFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Nach ihrem Auge wie nach einem Sterne", "tokens": ["Nach", "ih\u00b7rem", "Au\u00b7ge", "wie", "nach", "ei\u00b7nem", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "KOKOM", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Im tiefen Blau des Himmels sehn.", "tokens": ["Im", "tie\u00b7fen", "Blau", "des", "Him\u00b7mels", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich sah im Duft der Lilie, die mit Schweigen", "tokens": ["Ich", "sah", "im", "Duft", "der", "Li\u00b7lie", ",", "die", "mit", "Schwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sich auftat, ein Gebet zum Himmel steigen,", "tokens": ["Sich", "auf\u00b7tat", ",", "ein", "Ge\u00b7bet", "zum", "Him\u00b7mel", "stei\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVPP", "$,", "ART", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und meine Seele kniete mit;", "tokens": ["Und", "mei\u00b7ne", "See\u00b7le", "knie\u00b7te", "mit", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich h\u00f6rte Lieder im Ger\u00e4usch der Quellen,", "tokens": ["Ich", "h\u00f6r\u00b7te", "Lie\u00b7der", "im", "Ge\u00b7r\u00e4usch", "der", "Quel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die mir der Wind mit Sinken und mit Schwellen", "tokens": ["Die", "mir", "der", "Wind", "mit", "Sin\u00b7ken", "und", "mit", "Schwel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "NN", "APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "In ungewisse Strophen schnitt.", "tokens": ["In", "un\u00b7ge\u00b7wis\u00b7se", "Stro\u00b7phen", "schnitt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ja, ich war fromm und frei und rein. Ich glaubte", "tokens": ["Ja", ",", "ich", "war", "fromm", "und", "frei", "und", "rein", ".", "Ich", "glaub\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "ADJD", "KON", "ADJD", "KON", "ADJD", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "An jede Reinheit, und mit stolzem Haupte", "tokens": ["An", "je\u00b7de", "Rein\u00b7heit", ",", "und", "mit", "stol\u00b7zem", "Haup\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "$,", "KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sah ich hinab auf das Gew\u00fchl,", "tokens": ["Sah", "ich", "hin\u00b7ab", "auf", "das", "Ge\u00b7w\u00fchl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das unter mir im engen Horizonte", "tokens": ["Das", "un\u00b7ter", "mir", "im", "en\u00b7gen", "Ho\u00b7ri\u00b7zon\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "APPR", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Schaffen, sich freun, leben und sterben konnte,", "tokens": ["Schaf\u00b7fen", ",", "sich", "freun", ",", "le\u00b7ben", "und", "ster\u00b7ben", "konn\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRF", "VVINF", "$,", "VVFIN", "KON", "VVINF", "VMFIN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.6": {"text": "Des Windes und der Wellen Spiel.", "tokens": ["Des", "Win\u00b7des", "und", "der", "Wel\u00b7len", "Spiel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Nun hab' ich, ach, geschaut, erkannt, genossen;", "tokens": ["Nun", "hab'", "ich", ",", "ach", ",", "ge\u00b7schaut", ",", "er\u00b7kannt", ",", "ge\u00b7nos\u00b7sen", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "ITJ", "$,", "VVPP", "$,", "VVPP", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Bl\u00fct' ist hin, der Farben Schmelz zerflossen,", "tokens": ["Die", "Bl\u00fct'", "ist", "hin", ",", "der", "Far\u00b7ben", "Schmelz", "zer\u00b7flos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$,", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich bin erprobt in Lust und Schmerz.", "tokens": ["Ich", "bin", "er\u00b7probt", "in", "Lust", "und", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich ward ein Mann, doch konnt' ich nichts erlangen,", "tokens": ["Ich", "ward", "ein", "Mann", ",", "doch", "konnt'", "ich", "nichts", "er\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "ADV", "VMFIN", "PPER", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Als wen'ge Lieder, sonnverbrannte Wangen", "tokens": ["Als", "wen'\u00b7ge", "Lie\u00b7der", ",", "sonn\u00b7ver\u00b7brann\u00b7te", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PIAT", "NN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und dieses sehnsuchtsvolle Herz.", "tokens": ["Und", "die\u00b7ses", "sehn\u00b7suchts\u00b7vol\u00b7le", "Herz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Und jene Zeit, da mir so unvermessen", "tokens": ["Und", "je\u00b7ne", "Zeit", ",", "da", "mir", "so", "un\u00b7ver\u00b7mes\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PDAT", "NN", "$,", "KOUS", "PPER", "ADV", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Welt noch schien, fast hab' ich sie vergessen;", "tokens": ["Die", "Welt", "noch", "schien", ",", "fast", "hab'", "ich", "sie", "ver\u00b7ges\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$,", "ADV", "VAFIN", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nur manchmal, wenn der Feigenbaum", "tokens": ["Nur", "manch\u00b7mal", ",", "wenn", "der", "Fei\u00b7gen\u00b7baum"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An meinem offnen Fenster leise rauschet", "tokens": ["An", "mei\u00b7nem", "off\u00b7nen", "Fens\u00b7ter", "lei\u00b7se", "rau\u00b7schet"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und still durchs Laub des Mondes Sichel lauschet,", "tokens": ["Und", "still", "durchs", "Laub", "des", "Mon\u00b7des", "Si\u00b7chel", "lau\u00b7schet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "NN", "ART", "NN", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Blickt sie mich schmerzlich an im Traum.", "tokens": ["Blickt", "sie", "mich", "schmerz\u00b7lich", "an", "im", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADJD", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}