{"dta.poem.679": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "ViI.  \n DOMINE USQUE QUO?", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Ach wie lang/ O GOtt! mein GOtt wie lange/ ", "tokens": ["Ach", "wie", "lang", "/", "O", "Gott", "!", "mein", "Gott", "wie", "lan\u00b7ge", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ADJD", "$(", "NE", "NN", "$.", "PPOSAT", "NN", "KOKOM", "ADV", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wilst du dich von meinen threnen kehren/", "tokens": ["Wilst", "du", "dich", "von", "mei\u00b7nen", "thre\u00b7nen", "keh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Vnd keiner bitte mich gewehren!", "tokens": ["Vnd", "kei\u00b7ner", "bit\u00b7te", "mich", "ge\u00b7weh\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ach/ wie ist mir doch so hefftig bange!", "tokens": ["Ach", "/", "wie", "ist", "mir", "doch", "so", "heff\u00b7tig", "ban\u00b7ge", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$(", "KOKOM", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "ADJD", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Da\u00df du mich nun gantz au\u00df deinem hertzen/", "tokens": ["Da\u00df", "du", "mich", "nun", "gantz", "au\u00df", "dei\u00b7nem", "hert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "ADV", "APPR", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Schleust vnd in grundlosen schmertzen/", "tokens": ["Schleust", "vnd", "in", "grund\u00b7lo\u00b7sen", "schmert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "APPR", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Ohne trost versincken l\u00e4st.", "tokens": ["Oh\u00b7ne", "trost", "ver\u00b7sin\u00b7cken", "l\u00e4st", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Soll ich HERR/ dein antlitz nicht mehr schawen?", "tokens": ["Soll", "ich", "HeRR", "/", "dein", "ant\u00b7litz", "nicht", "mehr", "scha\u00b7wen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "NN", "$(", "PPOSAT", "NN", "PTKNEG", "ADV", "VVINF", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Hab ich nicht O Heylandt/) mein vertrawen", "tokens": ["Hab", "ich", "nicht", "O", "Hey\u00b7landt", "/", ")", "mein", "ver\u00b7tra\u00b7wen"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["NN", "PPER", "PTKNEG", "NE", "NE", "$(", "$(", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Stets auff dich gegr\u00fcndet fest? ", "tokens": ["Stets", "auff", "dich", "ge\u00b7gr\u00fcn\u00b7det", "fest", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "VVPP", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "2. Ach/ wie lange soll in tausendt plagen/", "tokens": ["Ach", "/", "wie", "lan\u00b7ge", "soll", "in", "tau\u00b7sendt", "pla\u00b7gen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$(", "PWAV", "ADV", "VMFIN", "APPR", "CARD", "VVFIN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Vnter deines grimmes donnerkeilen/", "tokens": ["Vn\u00b7ter", "dei\u00b7nes", "grim\u00b7mes", "don\u00b7ner\u00b7kei\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Vnd H\u00f6llenheissen Schwefel pfeilen", "tokens": ["Vnd", "H\u00f6l\u00b7len\u00b7heis\u00b7sen", "Schwe\u00b7fel", "pfei\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich mein jmmerwehrendt weh beklagen?", "tokens": ["Ich", "mein", "jm\u00b7mer\u00b7weh\u00b7rendt", "weh", "be\u00b7kla\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Ach wie m\u00fcd\u2019 ist mein gem\u00fct von sorgen!", "tokens": ["Ach", "wie", "m\u00fcd'", "ist", "mein", "ge\u00b7m\u00fct", "von", "sor\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ADJD", "VAFIN", "PPOSAT", "VVPP", "APPR", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Welches pl\u00f6tzlich alle morgen/", "tokens": ["Wel\u00b7ches", "pl\u00f6tz\u00b7lich", "al\u00b7le", "mor\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "PIS", "ADV", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Angst vnd ", "tokens": ["Angst", "vnd"], "token_info": ["word", "word"], "pos": ["NN", "KON"], "meter": "+-", "measure": "trochaic.single"}, "line.8": {"text": "Ist wol eine tr\u00fcbsal zu ergr\u00fcnden/", "tokens": ["Ist", "wol", "ei\u00b7ne", "tr\u00fcb\u00b7sal", "zu", "er\u00b7gr\u00fcn\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJD", "PTKZU", "VVINF", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Wirdt man auch ein vngl\u00fcck k\u00f6nnen finden.", "tokens": ["Wirdt", "man", "auch", "ein", "vn\u00b7gl\u00fcck", "k\u00f6n\u00b7nen", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADV", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Das mich nicht in klawen h\u00e4lt? ", "tokens": ["Das", "mich", "nicht", "in", "kla\u00b7wen", "h\u00e4lt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "PTKNEG", "APPR", "ADJA", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "3. ", "tokens": [], "token_info": [], "pos": []}, "line.2": {"text": "Das sich aber meine feind' erheben ", "tokens": ["Das", "sich", "a\u00b7ber", "mei\u00b7ne", "feind'", "er\u00b7he\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "PRF", "ADV", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Weil ich in h\u00f6chster qual mu\u00df leben/ ", "tokens": ["Weil", "ich", "in", "h\u00f6chs\u00b7ter", "qual", "mu\u00df", "le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df so fr\u00f6lich jauchtzen/ die mich neiden; ", "tokens": ["Da\u00df", "so", "fr\u00f6\u00b7lich", "jaucht\u00b7zen", "/", "die", "mich", "nei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADJD", "VVINF", "$(", "PRELS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Dis/ dis wil mir leib vnd Geist durchdringen/", "tokens": ["Dis", "/", "dis", "wil", "mir", "leib", "vnd", "Geist", "durch\u00b7drin\u00b7gen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$(", "PDS", "VMFIN", "PPER", "NN", "KON", "NN", "VVINF", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Vnd mich zum verzweifeln bringen.", "tokens": ["Vnd", "mich", "zum", "ver\u00b7zwei\u00b7feln", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPRART", "VVINF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Mein G", "tokens": ["Mein", "G"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.8": {"text": "Wofern dein gem\u00fcte zu erweichen:", "tokens": ["Wo\u00b7fern", "dein", "ge\u00b7m\u00fc\u00b7te", "zu", "er\u00b7wei\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Wofern eine gnade zu erreichen", "tokens": ["Wo\u00b7fern", "ei\u00b7ne", "gna\u00b7de", "zu", "er\u00b7rei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJD", "PTKZU", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Schawe doch auff meinen spott.", "tokens": ["Scha\u00b7we", "doch", "auff", "mei\u00b7nen", "spott", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "4. Welt ade es ist vmb mich geschehen!", "tokens": ["Welt", "a\u00b7de", "es", "ist", "vmb", "mich", "ge\u00b7sche\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Meine krafft weicht/ vnd die Augen brechen/ ", "tokens": ["Mei\u00b7ne", "krafft", "weicht", "/", "vnd", "die", "Au\u00b7gen", "bre\u00b7chen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$(", "KON", "ART", "NN", "VVINF", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Die Zunge kan kein wort mehr sprechen: ", "tokens": ["Die", "Zun\u00b7ge", "kan", "kein", "wort", "mehr", "spre\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PIAT", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Todt hat mich jhm zum raub' ersehen. ", "tokens": ["Der", "Todt", "hat", "mich", "jhm", "zum", "raub'", "er\u00b7se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Ach HERR! einen strall nur/ deiner g\u00fctte/", "tokens": ["Ach", "HeRR", "!", "ei\u00b7nen", "strall", "nur", "/", "dei\u00b7ner", "g\u00fct\u00b7te", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "NN", "$.", "ART", "NN", "ADV", "$(", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "W\u00fcndtscht mein sterbendes gem\u00fcte", "tokens": ["W\u00fcndt\u00b7scht", "mein", "ster\u00b7ben\u00b7des", "ge\u00b7m\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Brich doch an du lebens licht!", "tokens": ["Brich", "doch", "an", "du", "le\u00b7bens", "licht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Mein feind wird es seiner macht zu schreiben/", "tokens": ["Mein", "feind", "wird", "es", "sei\u00b7ner", "macht", "zu", "schrei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "PPOSAT", "VVFIN", "PTKZU", "VVINF", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Wo ich mu\u00df im staube liegen bleiben.", "tokens": ["Wo", "ich", "mu\u00df", "im", "stau\u00b7be", "lie\u00b7gen", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VMFIN", "APPRART", "ADJA", "VVINF", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Dulde do", "tokens": ["Dul\u00b7de", "do"], "token_info": ["word", "word"], "pos": ["NE", "NE"], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.6": {"line.1": {"text": "5. Nun ich weis/ du wirst mir nicht abschlagen/", "tokens": ["Nun", "ich", "weis", "/", "du", "wirst", "mir", "nicht", "ab\u00b7schla\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PTKVZ", "$(", "PPER", "VAFIN", "PPER", "PTKNEG", "VVINF", "$("], "meter": "+-+-+-+++-", "measure": "unknown.measure.hexa"}, "line.2": {"text": "Was ich jtzt/ mit halberstarter zungen/", "tokens": ["Was", "ich", "jtzt", "/", "mit", "hal\u00b7ber\u00b7star\u00b7ter", "zun\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "$(", "APPR", "ADJA", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Vnd pfn\u00fcchtzend habe vorgedrungen. ", "tokens": ["Vnd", "pf\u00b7n\u00fccht\u00b7zend", "ha\u00b7be", "vor\u00b7ge\u00b7drun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Deine gnade kennet kein versagen/", "tokens": ["Dei\u00b7ne", "gna\u00b7de", "ken\u00b7net", "kein", "ver\u00b7sa\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PIAT", "VVINF", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Alle welt weis deine trew zu preisen/", "tokens": ["Al\u00b7le", "welt", "weis", "dei\u00b7ne", "trew", "zu", "prei\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PTKVZ", "PPOSAT", "ADJD", "PTKZU", "VVINF", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Die du pflegest zu erweisen/", "tokens": ["Die", "du", "pfle\u00b7gest", "zu", "er\u00b7wei\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "PTKZU", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wenn kein mittel mehr zu sehn ", "tokens": ["Wenn", "kein", "mit\u00b7tel", "mehr", "zu", "sehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIAT", "NN", "ADV", "PTKZU", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "HeRR ich glaub\u2019 ich wil nach so viel schreyen/", "tokens": ["HeRR", "ich", "glaub'", "ich", "wil", "nach", "so", "viel", "schre\u00b7yen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "VVFIN", "PPER", "VMFIN", "APPR", "ADV", "ADV", "VVINF", "$("], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.9": {"text": "Vber deiner wolthat mich erfrewen!", "tokens": ["Vber", "dei\u00b7ner", "wolt\u00b7hat", "mich", "er\u00b7fre\u00b7wen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Ja ich weis es wirdt geschehn!", "tokens": ["Ja", "ich", "weis", "es", "wirdt", "ge\u00b7schehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "PTKVZ", "PPER", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}