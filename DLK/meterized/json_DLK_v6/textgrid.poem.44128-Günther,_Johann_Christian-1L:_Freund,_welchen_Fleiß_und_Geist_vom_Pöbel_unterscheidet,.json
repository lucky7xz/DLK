{"textgrid.poem.44128": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Freund, welchen Flei\u00df und Geist vom P\u00f6bel unterscheidet,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freund, welchen Flei\u00df und Geist vom P\u00f6bel unterscheidet,", "tokens": ["Freund", ",", "wel\u00b7chen", "Flei\u00df", "und", "Geist", "vom", "P\u00f6\u00b7bel", "un\u00b7ter\u00b7schei\u00b7det", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAT", "NN", "KON", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Freund, dem der Musen Gunst vor tausend andern lacht,", "tokens": ["Freund", ",", "dem", "der", "Mu\u00b7sen", "Gunst", "vor", "tau\u00b7send", "an\u00b7dern", "lacht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "NN", "APPR", "CARD", "ADJA", "VVFIN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Freund, de\u00dfen Tugend blos der Pr\u00fcfung wegen leidet", "tokens": ["Freund", ",", "de\u00b7\u00dfen", "Tu\u00b7gend", "blos", "der", "Pr\u00fc\u00b7fung", "we\u00b7gen", "lei\u00b7det"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELAT", "NN", "ADV", "ART", "NN", "APPR", "VVFIN"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Und de\u00dfen Redligkeit sich nie zur Larve macht,", "tokens": ["Und", "de\u00b7\u00dfen", "Red\u00b7lig\u00b7keit", "sich", "nie", "zur", "Lar\u00b7ve", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PRF", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Nimm, was mir selbst gebricht, Zufriedenheit und Gl\u00fccke", "tokens": ["Nimm", ",", "was", "mir", "selbst", "ge\u00b7bricht", ",", "Zu\u00b7frie\u00b7den\u00b7heit", "und", "Gl\u00fc\u00b7cke"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "PWS", "PPER", "ADV", "VVFIN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "In Reimen ohne Kunst und W\u00fcntschen treuer Hand.", "tokens": ["In", "Rei\u00b7men", "oh\u00b7ne", "Kunst", "und", "W\u00fcnt\u00b7schen", "treu\u00b7er", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "KON", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wen Noth und Schwermuth plagt, der singt kein Meisterst\u00fccke,", "tokens": ["Wen", "Noth", "und", "Schwer\u00b7muth", "plagt", ",", "der", "singt", "kein", "Meis\u00b7ter\u00b7st\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "NN", "VVFIN", "$,", "PRELS", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und wie es G\u00fcnthern geh, ist allemahl bekand.", "tokens": ["Und", "wie", "es", "G\u00fcn\u00b7thern", "geh", ",", "ist", "al\u00b7le\u00b7mahl", "be\u00b7kand", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "NE", "VVFIN", "$,", "VAFIN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Nichts sieht man in der Welt, es hat geme\u00dfne Gr\u00e4nzen;", "tokens": ["Nichts", "sieht", "man", "in", "der", "Welt", ",", "es", "hat", "ge\u00b7me\u00df\u00b7ne", "Gr\u00e4n\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PIS", "APPR", "ART", "NN", "$,", "PPER", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Der Morgen jagt die Nacht, der Mond das gro\u00dfe Licht,", "tokens": ["Der", "Mor\u00b7gen", "jagt", "die", "Nacht", ",", "der", "Mond", "das", "gro\u00b7\u00dfe", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Der Marmor weicht der Zeit, der Winter vor dem Lenzen,", "tokens": ["Der", "Mar\u00b7mor", "weicht", "der", "Zeit", ",", "der", "Win\u00b7ter", "vor", "dem", "Len\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Nur dies mein Elend bleibt und weicht und \u00e4ndert nicht.", "tokens": ["Nur", "dies", "mein", "E\u00b7lend", "bleibt", "und", "weicht", "und", "\u00e4n\u00b7dert", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "PPOSAT", "NN", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Vorwahr, ich Schmerzenskind, ich ungl\u00fccksvolle B\u00fcrde,", "tokens": ["Vor\u00b7wahr", ",", "ich", "Schmer\u00b7zens\u00b7kind", ",", "ich", "un\u00b7gl\u00fccks\u00b7vol\u00b7le", "B\u00fcr\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "NN", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Kam unter Angst und Blut der Mutter sauer an,", "tokens": ["Kam", "un\u00b7ter", "Angst", "und", "Blut", "der", "Mut\u00b7ter", "sau\u00b7er", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "KON", "NN", "ART", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Nur darum, da\u00df an mir der Welt gewiesen w\u00fcrde,", "tokens": ["Nur", "da\u00b7rum", ",", "da\u00df", "an", "mir", "der", "Welt", "ge\u00b7wie\u00b7sen", "w\u00fcr\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PAV", "$,", "KOUS", "APPR", "PPER", "ART", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+---+-+-+-", "measure": "unknown.measure.penta"}, "line.16": {"text": "Wie grausam und wie starck die Allmacht z\u00fcrnen kan.", "tokens": ["Wie", "grau\u00b7sam", "und", "wie", "starck", "die", "All\u00b7macht", "z\u00fcr\u00b7nen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "KON", "PWAV", "ADJD", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Doch geht mir nichts so nah, so viel ich sonst auch trage,", "tokens": ["Doch", "geht", "mir", "nichts", "so", "nah", ",", "so", "viel", "ich", "sonst", "auch", "tra\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIS", "ADV", "ADJD", "$,", "ADV", "ADV", "PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Als da\u00df mich Hohn und Neid der Strafe w\u00fcrdig nennt", "tokens": ["Als", "da\u00df", "mich", "Hohn", "und", "Neid", "der", "Stra\u00b7fe", "w\u00fcr\u00b7dig", "nennt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "PPER", "NN", "KON", "NN", "ART", "NN", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Und da\u00df kein einziger den Ursprung solcher Plage", "tokens": ["Und", "da\u00df", "kein", "ein\u00b7zi\u00b7ger", "den", "Ur\u00b7sprung", "sol\u00b7cher", "Pla\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PIAT", "ADJA", "ART", "NN", "PIAT", "NN"], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.20": {"text": "Noch de\u00dfen Heftigkeit noch jeden Umstand kennt.", "tokens": ["Noch", "de\u00b7\u00dfen", "Hef\u00b7tig\u00b7keit", "noch", "je\u00b7den", "Um\u00b7stand", "kennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.21": {"text": "Das kan ich leicht gestehn, ich bin ein Mensch wie alle", "tokens": ["Das", "kan", "ich", "leicht", "ge\u00b7stehn", ",", "ich", "bin", "ein", "Mensch", "wie", "al\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "PPER", "ADJD", "VVPP", "$,", "PPER", "VAFIN", "ART", "NN", "KOKOM", "PIAT"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Und hab aus Unverstand und Jugend viel versehn,", "tokens": ["Und", "hab", "aus", "Un\u00b7ver\u00b7stand", "und", "Ju\u00b7gend", "viel", "ver\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "NN", "KON", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Allein, wer zeigt mir wohl den Freybrief vor dem Falle?", "tokens": ["Al\u00b7lein", ",", "wer", "zeigt", "mir", "wohl", "den", "Frey\u00b7brief", "vor", "dem", "Fal\u00b7le", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWS", "VVFIN", "PPER", "ADV", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Mein Fehler ist und wird noch oftermahls geschehn.", "tokens": ["Mein", "Feh\u00b7ler", "ist", "und", "wird", "noch", "of\u00b7ter\u00b7mahls", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "KON", "VAFIN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Die Noth macht freylich tumm, und wen die Sorgen kr\u00e4ncken,", "tokens": ["Die", "Noth", "macht", "frey\u00b7lich", "tumm", ",", "und", "wen", "die", "Sor\u00b7gen", "kr\u00e4n\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "$,", "KON", "PWS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Der hat Gem\u00fcth und Hand von andrer Arbeit voll,", "tokens": ["Der", "hat", "Ge\u00b7m\u00fcth", "und", "Hand", "von", "an\u00b7drer", "Ar\u00b7beit", "voll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NN", "KON", "NN", "APPR", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Als da\u00df er mit Vernunft auf Rath und Rettung dencken", "tokens": ["Als", "da\u00df", "er", "mit", "Ver\u00b7nunft", "auf", "Rath", "und", "Ret\u00b7tung", "den\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "PPER", "APPR", "NN", "APPR", "NN", "KON", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und sonder falsche Wahl die Mittel trefen soll.", "tokens": ["Und", "son\u00b7der", "fal\u00b7sche", "Wahl", "die", "Mit\u00b7tel", "tre\u00b7fen", "soll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJA", "NN", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Ein Mensch, dem Hiz und Schmerz der Sinnen Kr\u00e4fte brechen,", "tokens": ["Ein", "Mensch", ",", "dem", "Hiz", "und", "Schmerz", "der", "Sin\u00b7nen", "Kr\u00e4f\u00b7te", "bre\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "KON", "NN", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Vergie\u00dft vor Thr\u00e4nen Blut, nimmt Gift vor Mithridat", "tokens": ["Ver\u00b7gie\u00dft", "vor", "Thr\u00e4\u00b7nen", "Blut", ",", "nimmt", "Gift", "vor", "Mith\u00b7ri\u00b7dat"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "NN", "NN", "$,", "VVFIN", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Und schadet sich oft selbst. Wer will es aber r\u00e4chen?", "tokens": ["Und", "scha\u00b7det", "sich", "oft", "selbst", ".", "Wer", "will", "es", "a\u00b7ber", "r\u00e4\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADV", "ADV", "$.", "PWS", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Die Kranckheit und nicht er begeht die Mi\u00dfethat.", "tokens": ["Die", "Kran\u00b7ck\u00b7heit", "und", "nicht", "er", "be\u00b7geht", "die", "Mi\u00b7\u00dfet\u00b7hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PTKNEG", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.33": {"text": "Man wird so wie ein Schif von St\u00fcrmen hingeri\u00dfen,", "tokens": ["Man", "wird", "so", "wie", "ein", "Schif", "von", "St\u00fcr\u00b7men", "hin\u00b7ge\u00b7ri\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ADV", "KOKOM", "ART", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Man ist den Tr\u00e4umenden im Fantasieren gleich,", "tokens": ["Man", "ist", "den", "Tr\u00e4u\u00b7men\u00b7den", "im", "Fan\u00b7ta\u00b7sie\u00b7ren", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ART", "NN", "APPRART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Man kriegt aus Ungedult ein schl\u00e4friges Gewi\u00dfen", "tokens": ["Man", "kriegt", "aus", "Un\u00b7ge\u00b7dult", "ein", "schl\u00e4f\u00b7ri\u00b7ges", "Ge\u00b7wi\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "APPR", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Und s\u00fcndigt durch den Fluch auf jeden Ungl\u00fccksstreich.", "tokens": ["Und", "s\u00fcn\u00b7digt", "durch", "den", "Fluch", "auf", "je\u00b7den", "Un\u00b7gl\u00fccks\u00b7streich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Denn da ich endlich seh, wie wenig mein Gem\u00fcthe,", "tokens": ["Denn", "da", "ich", "end\u00b7lich", "seh", ",", "wie", "we\u00b7nig", "mein", "Ge\u00b7m\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VVFIN", "$,", "PWAV", "PIS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Das andern gern vergiebt, Entschuldigung erh\u00e4lt,", "tokens": ["Das", "an\u00b7dern", "gern", "ver\u00b7giebt", ",", "Ent\u00b7schul\u00b7di\u00b7gung", "er\u00b7h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "VVFIN", "$,", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Da gar kein treuer Freund und keines G\u00f6nners G\u00fcte,", "tokens": ["Da", "gar", "kein", "treu\u00b7er", "Freund", "und", "kei\u00b7nes", "G\u00f6n\u00b7ners", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIAT", "ADJA", "NN", "KON", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "So gut er helfen kan, mein Herz zufrieden stellt,", "tokens": ["So", "gut", "er", "hel\u00b7fen", "kan", ",", "mein", "Herz", "zu\u00b7frie\u00b7den", "stellt", ","], "toke