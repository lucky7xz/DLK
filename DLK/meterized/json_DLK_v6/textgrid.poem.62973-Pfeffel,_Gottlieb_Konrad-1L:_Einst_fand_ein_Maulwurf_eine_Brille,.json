{"textgrid.poem.62973": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Einst fand ein Maulwurf eine Brille,", "genre": "verse", "period": "N.A.", "pub_year": 1785, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einst fand ein Maulwurf eine Brille,", "tokens": ["Einst", "fand", "ein", "Maul\u00b7wurf", "ei\u00b7ne", "Bril\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die eine fahrende Sibylle", "tokens": ["Die", "ei\u00b7ne", "fah\u00b7ren\u00b7de", "Si\u00b7byl\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Aus ihrem Zauberbuch verlor.", "tokens": ["Aus", "ih\u00b7rem", "Zau\u00b7ber\u00b7buch", "ver\u00b7lor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er pflanzt sie r\u00fcstig auf die Nase", "tokens": ["Er", "pflanzt", "sie", "r\u00fcs\u00b7tig", "auf", "die", "Na\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und sah gerade \u2013 was zuvor.", "tokens": ["Und", "sah", "ge\u00b7ra\u00b7de", "\u2013", "was", "zu\u00b7vor", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$(", "PWS", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Pfuy, sprach er, mit dem dummen Glase!", "tokens": ["Pfuy", ",", "sprach", "er", ",", "mit", "dem", "dum\u00b7men", "Gla\u00b7se", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "$,", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und warf es weg. Doch pl\u00f6tzlich gieng", "tokens": ["Und", "warf", "es", "weg", ".", "Doch", "pl\u00f6tz\u00b7lich", "gieng"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$.", "KON", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ein Licht ihm auf. Mit trunkner Seele", "tokens": ["Ein", "Licht", "ihm", "auf", ".", "Mit", "trunk\u00b7ner", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "PTKVZ", "$.", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Fuhr er damit in seine H\u00f6hle", "tokens": ["Fuhr", "er", "da\u00b7mit", "in", "sei\u00b7ne", "H\u00f6h\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PAV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Und wies es, als ein Wunderding,", "tokens": ["Und", "wies", "es", ",", "als", "ein", "Wun\u00b7der\u00b7ding", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "KOUS", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Der Colonie: \u00bbSeht, Br\u00fcder, sehet,", "tokens": ["Der", "Co\u00b7lo\u00b7nie", ":", "\u00bb", "Seht", ",", "Br\u00fc\u00b7der", ",", "se\u00b7het", ","], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$.", "$(", "VVFIN", "$,", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Rief er von Weisheit aufgebl\u00e4het,", "tokens": ["Rief", "er", "von", "Weis\u00b7heit", "auf\u00b7ge\u00b7bl\u00e4\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Was ich vom Trismegist empfieng!", "tokens": ["Was", "ich", "vom", "Tris\u00b7me\u00b7gist", "emp\u00b7fi\u00b7eng", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.14": {"text": "Ein Glas, wodurch ich Sterne, Maden,", "tokens": ["Ein", "Glas", ",", "wo\u00b7durch", "ich", "Ster\u00b7ne", ",", "Ma\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PPER", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "D\u00e4monen, G\u00f6tter, und Monaden", "tokens": ["D\u00e4\u00b7mo\u00b7nen", ",", "G\u00f6t\u00b7ter", ",", "und", "Mo\u00b7na\u00b7den"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Mit hellem Aug erblicken kann.\u00ab", "tokens": ["Mit", "hel\u00b7lem", "Aug", "er\u00b7bli\u00b7cken", "kann", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "VMFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Itzt f\u00e4ngt er an zu demonstrieren", "tokens": ["Itzt", "f\u00e4ngt", "er", "an", "zu", "de\u00b7monst\u00b7rie\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "PTKZU", "VVINF"], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.18": {"text": "Und von dem neuen Talisman", "tokens": ["Und", "von", "dem", "neu\u00b7en", "Ta\u00b7lis\u00b7man"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.19": {"text": "So salbungsvoll zu phantasieren,", "tokens": ["So", "sal\u00b7bungs\u00b7voll", "zu", "phan\u00b7ta\u00b7sie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Da\u00df jeder glaubt es sey was dran.", "tokens": ["Da\u00df", "je\u00b7der", "glaubt", "es", "sey", "was", "dran", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "PPER", "VAFIN", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Die Br\u00fcder wolltens auch probiren,", "tokens": ["Die", "Br\u00fc\u00b7der", "woll\u00b7tens", "auch", "pro\u00b7bi\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Und ob sie gleich nur D\u00fcnste sahn,", "tokens": ["Und", "ob", "sie", "gleich", "nur", "D\u00fcns\u00b7te", "sahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "So that es nichts. Die Br\u00fcder sprachen", "tokens": ["So", "that", "es", "nichts", ".", "Die", "Br\u00fc\u00b7der", "spra\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "$.", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Nur desto mehr vom Wunderglas", "tokens": ["Nur", "des\u00b7to", "mehr", "vom", "Wun\u00b7der\u00b7glas"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Und von dem Mann, der es besa\u00df.", "tokens": ["Und", "von", "dem", "Mann", ",", "der", "es", "be\u00b7sa\u00df", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Wir m\u00fc\u00dfen ihn zum Doktor machen,", "tokens": ["Wir", "m\u00fc\u00b7\u00dfen", "ihn", "zum", "Dok\u00b7tor", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Hie\u00df es, und flugs ward er gekr\u00f6nt.", "tokens": ["Hie\u00df", "es", ",", "und", "flugs", "ward", "er", "ge\u00b7kr\u00f6nt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KON", "ADV", "VAFIN", "PPER", "VVPP", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.28": {"text": "Sein Oheim nur, ein alter Sp\u00e4her,", "tokens": ["Sein", "O\u00b7heim", "nur", ",", "ein", "al\u00b7ter", "Sp\u00e4\u00b7her", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-++--+-+-", "measure": "iambic.tetra.relaxed"}, "line.29": {"text": "Kratzt sich den Scheitel und verh\u00f6hnt", "tokens": ["Kratzt", "sich", "den", "Schei\u00b7tel", "und", "ver\u00b7h\u00f6hnt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ART", "NN", "KON", "VVPP"], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Mit bittrem Spott den neuen Seher;", "tokens": ["Mit", "bitt\u00b7rem", "Spott", "den", "neu\u00b7en", "Se\u00b7her", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Allein man gab ihm Hohn f\u00fcr Hohn,", "tokens": ["Al\u00b7lein", "man", "gab", "ihm", "Hohn", "f\u00fcr", "Hohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "PPER", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Er ward verdammet und geflohn.", "tokens": ["Er", "ward", "ver\u00b7dam\u00b7met", "und", "ge\u00b7flohn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVFIN", "KON", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Das kr\u00e4nkt den Alten. In der Stille", "tokens": ["Das", "kr\u00e4nkt", "den", "Al\u00b7ten", ".", "In", "der", "Stil\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "NN", "$.", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "Der Nacht bricht er beym Docktor ein,", "tokens": ["Der", "Nacht", "bricht", "er", "beym", "Dock\u00b7tor", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Stiehlt beyde Gl\u00e4ser aus der Brille", "tokens": ["Stiehlt", "bey\u00b7de", "Gl\u00e4\u00b7ser", "aus", "der", "Bril\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIAT", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "Und deckt den Raub mit einem Stein.", "tokens": ["Und", "deckt", "den", "Raub", "mit", "ei\u00b7nem", "Stein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Des Morgens tritt der neue Weise,", "tokens": ["Des", "Mor\u00b7gens", "tritt", "der", "neu\u00b7e", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.38": {"text": "Mit dem entlehnten Augenpaar", "tokens": ["Mit", "dem", "ent\u00b7lehn\u00b7ten", "Au\u00b7gen\u00b7paar"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "In seiner Sch\u00fcler dichte Kreise", "tokens": ["In", "sei\u00b7ner", "Sch\u00fc\u00b7ler", "dich\u00b7te", "Krei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.40": {"text": "Und zeiget der entz\u00fcckten Schaar", "tokens": ["Und", "zei\u00b7get", "der", "ent\u00b7z\u00fcck\u00b7ten", "Schaar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "Trotz einem epischen Poeten,", "tokens": ["Trotz", "ei\u00b7nem", "e\u00b7pisc\u00b7hen", "Po\u00b7et\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.42": {"text": "Viel wundersch\u00f6ne Rarit\u00e4ten.", "tokens": ["Viel", "wun\u00b7der\u00b7sch\u00f6\u00b7ne", "Ra\u00b7ri\u00b7t\u00e4\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.43": {"text": "Er ward des Diebstahls nicht gewahr,", "tokens": ["Er", "ward", "des", "Dieb\u00b7stahls", "nicht", "ge\u00b7wahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.44": {"text": "Als aus dem Dunkel eines Winkels", "tokens": ["Als", "aus", "dem", "Dun\u00b7kel", "ei\u00b7nes", "Win\u00b7kels"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.45": {"text": "Der Oheim auf den Lehrstuhl sprang:", "tokens": ["Der", "O\u00b7heim", "auf", "den", "Lehr\u00b7stuhl", "sprang", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-++--+-+", "measure": "iambic.tetra.relaxed"}, "line.46": {"text": "\u00bbdas Fratzenspiel des Eigend\u00fcnkels", "tokens": ["\u00bb", "das", "Frat\u00b7zen\u00b7spiel", "des", "Ei\u00b7gen\u00b7d\u00fcn\u00b7kels"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.47": {"text": "Und des Betrugs w\u00e4hrt allzulang;", "tokens": ["Und", "des", "Be\u00b7trugs", "w\u00e4hrt", "all\u00b7zu\u00b7lang", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.48": {"text": "Ich mu\u00df des Hermes gro\u00dfem Sch\u00fcler", "tokens": ["Ich", "mu\u00df", "des", "Her\u00b7mes", "gro\u00b7\u00dfem", "Sch\u00fc\u00b7ler"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.49": {"text": "Die Maske von der Stirne ziehn!\u00ab", "tokens": ["Die", "Mas\u00b7ke", "von", "der", "Stir\u00b7ne", "ziehn", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.50": {"text": "Rief er und warf dem Schattenspieler", "tokens": ["Rief", "er", "und", "warf", "dem", "Schat\u00b7ten\u00b7spie\u00b7ler"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.51": {"text": "Die Gl\u00e4ser vor die F\u00fc\u00dfe hin.", "tokens": ["Die", "Gl\u00e4\u00b7ser", "vor", "die", "F\u00fc\u00b7\u00dfe", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.52": {"text": "Er schweigt. Tumult erf\u00fcllt die Grotte;", "tokens": ["Er", "schweigt", ".", "Tu\u00b7mult", "er\u00b7f\u00fcllt", "die", "Grot\u00b7te", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.53": {"text": "Der Doktor stutzt, die ganze Rotte", "tokens": ["Der", "Dok\u00b7tor", "stutzt", ",", "die", "gan\u00b7ze", "Rot\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.54": {"text": "F\u00e4llt racheschnaubend \u00fcber ihn;", "tokens": ["F\u00e4llt", "ra\u00b7ch\u00b7e\u00b7schnau\u00b7bend", "\u00fc\u00b7ber", "ihn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "PPER", "$."], "meter": "+-+-+-+