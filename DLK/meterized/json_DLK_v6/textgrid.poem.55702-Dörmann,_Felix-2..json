{"textgrid.poem.55702": {"metadata": {"author": {"name": "D\u00f6rmann, Felix", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Manches Mal, in stillen N\u00e4chten,", "tokens": ["Man\u00b7ches", "Mal", ",", "in", "stil\u00b7len", "N\u00e4ch\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Steigt mir noch Dein Bild empor", "tokens": ["Steigt", "mir", "noch", "Dein", "Bild", "em\u00b7por"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ich kann's nicht, kann's nicht fassen,", "tokens": ["Und", "ich", "kann's", "nicht", ",", "kann's", "nicht", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PTKNEG", "$,", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df ich Dich so ganz verlor.", "tokens": ["Da\u00df", "ich", "Dich", "so", "ganz", "ver\u00b7lor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Deine gro\u00dfen, braunen Augen,", "tokens": ["Dei\u00b7ne", "gro\u00b7\u00dfen", ",", "brau\u00b7nen", "Au\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit den Wimpern lang und schwer,", "tokens": ["Mit", "den", "Wim\u00b7pern", "lang", "und", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Blicken ganz noch wie vor Zeiten", "tokens": ["Bli\u00b7cken", "ganz", "noch", "wie", "vor", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "ADV", "KOKOM", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Warm und innig zu mir her.", "tokens": ["Warm", "und", "in\u00b7nig", "zu", "mir", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Als in jener dunklen Stunde", "tokens": ["Als", "in", "je\u00b7ner", "dunk\u00b7len", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PDAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In das fremde Land Du gingst", "tokens": ["In", "das", "frem\u00b7de", "Land", "Du", "gingst"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "PPER", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und zum allerletzten Male", "tokens": ["Und", "zum", "al\u00b7ler\u00b7letz\u00b7ten", "Ma\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weinend mir am Halse hingst,", "tokens": ["Wei\u00b7nend", "mir", "am", "Hal\u00b7se", "hingst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Damals hast Du mir versprochen:", "tokens": ["Da\u00b7mals", "hast", "Du", "mir", "ver\u00b7spro\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00bbh\u00f6ren wirst Du bald von mir\u00ab", "tokens": ["\u00bb", "h\u00f6\u00b7ren", "wirst", "Du", "bald", "von", "mir", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVINF", "VAFIN", "PPER", "ADV", "APPR", "PPER", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber niemals kam ein Zeichen,", "tokens": ["A\u00b7ber", "nie\u00b7mals", "kam", "ein", "Zei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Niemals nur ein Gru\u00df von Dir.", "tokens": ["Nie\u00b7mals", "nur", "ein", "Gru\u00df", "von", "Dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "\u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013", "tokens": ["\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013"], "token_info": ["punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct"], "pos": ["$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$("]}}, "stanza.5": {"line.1": {"text": "Wilder Schmerzen w\u00fcstes Toben", "tokens": ["Wil\u00b7der", "Schmer\u00b7zen", "w\u00fcs\u00b7tes", "To\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hat in Wehmut sich gewandt,", "tokens": ["Hat", "in", "Weh\u00b7mut", "sich", "ge\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "PRF", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und im raschen Lauf der Tage", "tokens": ["Und", "im", "ra\u00b7schen", "Lauf", "der", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Selbst Dein Bild dem Geist entschwand. \u2013", "tokens": ["Selbst", "Dein", "Bild", "dem", "Geist", "ent\u00b7schwand", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Manchmal nur in stillen N\u00e4chten", "tokens": ["Manch\u00b7mal", "nur", "in", "stil\u00b7len", "N\u00e4ch\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Steigt es mir noch hei\u00df empor \u2013", "tokens": ["Steigt", "es", "mir", "noch", "hei\u00df", "em\u00b7por", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "ADJD", "PTKVZ", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ich kann's nicht, kann's nicht fassen,", "tokens": ["Und", "ich", "kann's", "nicht", ",", "kann's", "nicht", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PTKNEG", "$,", "VMFIN", "PTKNEG", "VVINF", "$,"], "met