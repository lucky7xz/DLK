{"textgrid.poem.33830": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Wetterleuchten", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "L\u00e4ngs der dunklen Berge f\u00e4hrt", "tokens": ["L\u00e4ngs", "der", "dunk\u00b7len", "Ber\u00b7ge", "f\u00e4hrt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wetterleuchten durch die Nacht,", "tokens": ["Wet\u00b7ter\u00b7leuch\u00b7ten", "durch", "die", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ein gez\u00fccktes Racheschwert,", "tokens": ["Ein", "ge\u00b7z\u00fcck\u00b7tes", "Ra\u00b7ch\u00b7e\u00b7schwert", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Eine stumme Geisterschlacht.", "tokens": ["Ei\u00b7ne", "stum\u00b7me", "Geis\u00b7ter\u00b7schlacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Recht und Freiheit bl\u00fchen kaum", "tokens": ["Recht", "und", "Frei\u00b7heit", "bl\u00fc\u00b7hen", "kaum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VVFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf Momente nur der Welt,", "tokens": ["Auf", "Mo\u00b7men\u00b7te", "nur", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Von der Dichtung sch\u00f6nem Traum", "tokens": ["Von", "der", "Dich\u00b7tung", "sch\u00f6\u00b7nem", "Traum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird sie fl\u00fcchtig nur erhellt.", "tokens": ["Wird", "sie", "fl\u00fcch\u00b7tig", "nur", "er\u00b7hellt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Gutes m\u00fc\u00dfte siegreich sein,", "tokens": ["Gu\u00b7tes", "m\u00fc\u00df\u00b7te", "sieg\u00b7reich", "sein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ADJD", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hielt' Gerechtigkeit ihr Wort \u2013", "tokens": ["Hielt'", "Ge\u00b7rech\u00b7tig\u00b7keit", "ihr", "Wort", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "PPOSAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist sie nur ein Flammenschein,", "tokens": ["Ist", "sie", "nur", "ein", "Flam\u00b7men\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie das Wetterleuchten dort?", "tokens": ["Wie", "das", "Wet\u00b7ter\u00b7leuch\u00b7ten", "dort", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}