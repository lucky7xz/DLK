{"dta.poem.10727": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "ArISTARCHUS,  \n SIVE DE CONTEMPTU LIN-  \n GU\u00c6 TEUTONIC \u00c6 .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "In dieser Zeit nur der sterbliche dichtet/", "tokens": ["In", "die\u00b7ser", "Zeit", "nur", "der", "sterb\u00b7li\u00b7che", "dich\u00b7tet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADV", "ART", "ADJA", "VVFIN", "$("], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wie da\u00df sein schatz sey heufftig zugerichtet/", "tokens": ["Wie", "da\u00df", "sein", "schatz", "sey", "heuff\u00b7tig", "zu\u00b7ge\u00b7rich\u00b7tet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "PPOSAT", "NN", "VAFIN", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Gottes furcht in dessen (ach der Noth!)", "tokens": ["Die", "Got\u00b7tes", "furcht", "in", "des\u00b7sen", "(", "ach", "der", "Noth", "!", ")"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "NN", "APPR", "PRELAT", "$(", "ADV", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Entschlaffen ist/ so sie nicht gar ist Todt.", "tokens": ["Ent\u00b7schlaf\u00b7fen", "ist", "/", "so", "sie", "nicht", "gar", "ist", "Todt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$(", "ADV", "PPER", "PTKNEG", "ADV", "VAFIN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}