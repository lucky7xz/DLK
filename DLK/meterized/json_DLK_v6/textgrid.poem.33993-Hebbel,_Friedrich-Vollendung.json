{"textgrid.poem.33993": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Vollendung", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von einer Wunderblume la\u00dft mich tr\u00e4umen!", "tokens": ["Von", "ei\u00b7ner", "Wun\u00b7der\u00b7blu\u00b7me", "la\u00dft", "mich", "tr\u00e4u\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Tag verschwendet seine reichsten Stralen,", "tokens": ["Der", "Tag", "ver\u00b7schwen\u00b7det", "sei\u00b7ne", "reichs\u00b7ten", "Stra\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In aller Farben Glut sie auszumalen;", "tokens": ["In", "al\u00b7ler", "Far\u00b7ben", "Glut", "sie", "aus\u00b7zu\u00b7ma\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "NN", "PPER", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Nacht versucht, mit Perlen sie zu s\u00e4umen.", "tokens": ["Die", "Nacht", "ver\u00b7sucht", ",", "mit", "Per\u00b7len", "sie", "zu", "s\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "APPR", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Bald wird das Leben in ihr \u00fcbersch\u00e4umen,", "tokens": ["Bald", "wird", "das", "Le\u00b7ben", "in", "ihr", "\u00fc\u00b7bersc\u00b7h\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und brennend, die Gestirne zu bezahlen,", "tokens": ["Und", "bren\u00b7nend", ",", "die", "Ge\u00b7stir\u00b7ne", "zu", "be\u00b7zah\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Verstr\u00f6mt sie aus der Kelche Opferschaalen", "tokens": ["Ver\u00b7str\u00f6mt", "sie", "aus", "der", "Kel\u00b7che", "Op\u00b7fer\u00b7schaa\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den flammenhei\u00dfen Duft nach allen R\u00e4umen.", "tokens": ["Den", "flam\u00b7men\u00b7hei\u00b7\u00dfen", "Duft", "nach", "al\u00b7len", "R\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch, da\u00df einmal das Sch\u00f6nste sich vollende,", "tokens": ["Doch", ",", "da\u00df", "ein\u00b7mal", "das", "Sch\u00f6ns\u00b7te", "sich", "voll\u00b7en\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "ADV", "ART", "NN", "PRF", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verschlie\u00dft der Himmel seine durst'gen Lippen", "tokens": ["Ver\u00b7schlie\u00dft", "der", "Him\u00b7mel", "sei\u00b7ne", "dur\u00b7st'\u00b7gen", "Lip\u00b7pen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Vor ihrem Opfer, und es senkt sich wieder.", "tokens": ["Vor", "ih\u00b7rem", "Op\u00b7fer", ",", "und", "es", "senkt", "sich", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "KON", "PPER", "VVFIN", "PRF", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wie sie den Duft in jede Ferne sende,", "tokens": ["Wie", "sie", "den", "Duft", "in", "je\u00b7de", "Fer\u00b7ne", "sen\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nicht Mond, noch Sonne, nicht ein Stern darf nippen,", "tokens": ["Nicht", "Mond", ",", "noch", "Son\u00b7ne", ",", "nicht", "ein", "Stern", "darf", "nip\u00b7pen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "$,", "ADV", "NN", "$,", "PTKNEG", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Er wird zu Thau und sinkt auf sie hernieder.", "tokens": ["Er", "wird", "zu", "Thau", "und", "sinkt", "auf", "sie", "her\u00b7nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "KON", "VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}