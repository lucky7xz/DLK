{"textgrid.poem.47587": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[wi\u00dft, da\u00df ich alle Fesseln der Geduld] [1]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wi\u00dft, da\u00df ich alle Fesseln der Geduld", "tokens": ["Wi\u00dft", ",", "da\u00df", "ich", "al\u00b7le", "Fes\u00b7seln", "der", "Ge\u00b7duld"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "PIAT", "NN", "ART", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Hinweggeworfen", "tokens": ["Hin\u00b7weg\u00b7ge\u00b7wor\u00b7fen"], "token_info": ["word"], "pos": ["NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Wir haben jegliche Verstellung weit", "tokens": ["Wir", "ha\u00b7ben", "jeg\u00b7li\u00b7che", "Ver\u00b7stel\u00b7lung", "weit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Hinweggeworfen.", "tokens": ["Hin\u00b7weg\u00b7ge\u00b7wor\u00b7fen", "."], "token_info": ["word", "punct"], "pos": ["VVIZU", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Der hohen Schule Porticus und Saal", "tokens": ["Der", "ho\u00b7hen", "Schu\u00b7le", "Por\u00b7ti\u00b7cus", "und", "Saal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NE", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und Qu\u00e4stionen,", "tokens": ["Und", "Qu\u00e4s\u00b7tio\u00b7nen", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$,"], "meter": "--+-", "measure": "anapaest.init"}, "line.3": {"text": "Wir haben all gelehrten Zank und Streit", "tokens": ["Wir", "ha\u00b7ben", "all", "ge\u00b7lehr\u00b7ten", "Zank", "und", "Streit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PIAT", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Hinweggeworfen.", "tokens": ["Hin\u00b7weg\u00b7ge\u00b7wor\u00b7fen", "."], "token_info": ["word", "punct"], "pos": ["VVIZU", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Den guten Namen von so manchem Jahr,", "tokens": ["Den", "gu\u00b7ten", "Na\u00b7men", "von", "so", "man\u00b7chem", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Den schwer errungnen,", "tokens": ["Den", "schwer", "er\u00b7rung\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Wir haben ihn, wie eine Kleinigkeit,", "tokens": ["Wir", "ha\u00b7ben", "ihn", ",", "wie", "ei\u00b7ne", "Klei\u00b7nig\u00b7keit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Hinweggeworfen.", "tokens": ["Hin\u00b7weg\u00b7ge\u00b7wor\u00b7fen", "."], "token_info": ["word", "punct"], "pos": ["VVIZU", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Wir haben unser altes, ernstes Haupt,", "tokens": ["Wir", "ha\u00b7ben", "un\u00b7ser", "al\u00b7tes", ",", "erns\u00b7tes", "Haupt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Es vor die F\u00fc\u00dfe", "tokens": ["Es", "vor", "die", "F\u00fc\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Dir rollend in den Staub der Niedrigkeit", "tokens": ["Dir", "rol\u00b7lend", "in", "den", "Staub", "der", "Nied\u00b7rig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADJD", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Hinweggeworfen.", "tokens": ["Hin\u00b7weg\u00b7ge\u00b7wor\u00b7fen", "."], "token_info": ["word", "punct"], "pos": ["VVIZU", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Wir haben alles, was da l\u00f6blich ist,", "tokens": ["Wir", "ha\u00b7ben", "al\u00b7les", ",", "was", "da", "l\u00f6b\u00b7lich", "ist", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "$,", "PRELS", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wir haben Glauben,", "tokens": ["Wir", "ha\u00b7ben", "Glau\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Schaam, Sitte, Tugend und Bescheidenheit", "tokens": ["Schaam", ",", "Sit\u00b7te", ",", "Tu\u00b7gend", "und", "Be\u00b7schei\u00b7den\u00b7heit"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Hinweggeworfen.", "tokens": ["Hin\u00b7weg\u00b7ge\u00b7wor\u00b7fen", "."], "token_info": ["word", "punct"], "pos": ["VVIZU", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Wir haben, der Narcisse deines Augs", "tokens": ["Wir", "ha\u00b7ben", ",", "der", "Nar\u00b7cis\u00b7se", "dei\u00b7nes", "Augs"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Seel' empfehlend,", "tokens": ["Die", "Seel'", "emp\u00b7feh\u00b7lend", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "All andrer Hoffnung Trost und S\u00fc\u00dfigkeit", "tokens": ["All", "an\u00b7drer", "Hoff\u00b7nung", "Trost", "und", "S\u00fc\u00b7\u00dfig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Hinweggeworfen.", "tokens": ["Hin\u00b7weg\u00b7ge\u00b7wor\u00b7fen", "."], "token_info": ["word", "punct"], "pos": ["VVIZU", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}