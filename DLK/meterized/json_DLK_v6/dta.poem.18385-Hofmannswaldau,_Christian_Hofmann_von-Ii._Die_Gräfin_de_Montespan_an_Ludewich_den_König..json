{"dta.poem.18385": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Ii.  \n Die Gr\u00e4fin de Montespan an  \n Ludewich den K\u00f6nig.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein unverhoffter brieff von mehr als hohen h\u00e4nden/", "tokens": ["Ein", "un\u00b7ver\u00b7hoff\u00b7ter", "brieff", "von", "mehr", "als", "ho\u00b7hen", "h\u00e4n\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PIAT", "KOKOM", "ADJA", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hat sinnen und verstand mir gantz best\u00fcrtzt gemacht.", "tokens": ["Hat", "sin\u00b7nen", "und", "ver\u00b7stand", "mir", "gantz", "be\u00b7st\u00fcrtzt", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVINF", "KON", "VVFIN", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich wei\u00df nicht ob mein gl\u00fcck mich suchet zu verblenden/", "tokens": ["Ich", "wei\u00df", "nicht", "ob", "mein", "gl\u00fcck", "mich", "su\u00b7chet", "zu", "ver\u00b7blen\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "KOUS", "PPOSAT", "NN", "PPER", "VVFIN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und da\u00df die schmeichlerin mit falschen munde lacht.", "tokens": ["Und", "da\u00df", "die", "schmeich\u00b7le\u00b7rin", "mit", "fal\u00b7schen", "mun\u00b7de", "lacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es will mir iede reyh ja iedes wort beschreiben/", "tokens": ["Es", "will", "mir", "ie\u00b7de", "reyh", "ja", "ie\u00b7des", "wort", "be\u00b7schrei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PIAT", "NN", "ADV", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wie dein entflammtes hertz mir s\u00fcssen weyrauch brennt.", "tokens": ["Wie", "dein", "ent\u00b7flamm\u00b7tes", "hertz", "mir", "s\u00fcs\u00b7sen", "wey\u00b7rauch", "brennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "PPER", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ich soll gelindes \u00f6hl in deine wunden reiben/", "tokens": ["Ich", "soll", "ge\u00b7lin\u00b7des", "\u00f6hl", "in", "dei\u00b7ne", "wun\u00b7den", "rei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und deine kranckheit hat zur \u00e4rtztin mich ernennt.", "tokens": ["Und", "dei\u00b7ne", "kran\u00b7ck\u00b7heit", "hat", "zur", "\u00e4rtz\u00b7tin", "mich", "er\u00b7nennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "APPRART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Ach aber was ist dies! ein k\u00f6nig wil mich lieben/", "tokens": ["Ach", "a\u00b7ber", "was", "ist", "dies", "!", "ein", "k\u00f6\u00b7nig", "wil", "mich", "lie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "PWS", "VAFIN", "PDS", "$.", "ART", "ADJD", "VMFIN", "PRF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Der grosse Ludewich verlanget meine gunst?", "tokens": ["Der", "gros\u00b7se", "Lu\u00b7de\u00b7wich", "ver\u00b7lan\u00b7get", "mei\u00b7ne", "gunst", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Es scheint die schickung will ihr spiel an mir ver\u00fcben/", "tokens": ["Es", "scheint", "die", "schi\u00b7ckung", "will", "ihr", "spiel", "an", "mir", "ver\u00b7\u00fc\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VMFIN", "PPER", "VVFIN", "APPR", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und machen asch und staub aus dieser heissen brunst.", "tokens": ["Und", "ma\u00b7chen", "asch", "und", "staub", "aus", "die\u00b7ser", "heis\u00b7sen", "brunst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "KON", "VVFIN", "APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ach lasse mich/ mein f\u00fcrst/ in niedern th\u00e4lern bleiben/", "tokens": ["Ach", "las\u00b7se", "mich", "/", "mein", "f\u00fcrst", "/", "in", "nie\u00b7dern", "th\u00e4\u00b7lern", "blei\u00b7ben", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PPER", "$(", "PPOSAT", "VVFIN", "$(", "APPR", "PIS", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Auf bergen kommet man den blitze gar zu nah/", "tokens": ["Auf", "ber\u00b7gen", "kom\u00b7met", "man", "den", "blit\u00b7ze", "gar", "zu", "nah", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVINF", "VVFIN", "PIS", "ART", "VVFIN", "ADV", "PTKA", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Es kan mir Jearus ein bild f\u00fcr augen schreiben/", "tokens": ["Es", "kan", "mir", "Jea\u00b7rus", "ein", "bild", "f\u00fcr", "au\u00b7gen", "schrei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "NE", "ART", "NN", "APPR", "NN", "VVINF", "$("], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "Wie der den stoltzen muht so schrecklich fallen sah.", "tokens": ["Wie", "der", "den", "stolt\u00b7zen", "muht", "so", "schreck\u00b7lich", "fal\u00b7len", "sah", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ART", "ADJA", "VVFIN", "ADV", "ADJD", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ich mag/ gro\u00dfm\u00e4chtigster/ nicht mehr zu hoffe gehen/", "tokens": ["Ich", "mag", "/", "gro\u00df\u00b7m\u00e4ch\u00b7tigs\u00b7ter", "/", "nicht", "mehr", "zu", "hof\u00b7fe", "ge\u00b7hen", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$(", "ADJA", "$(", "PTKNEG", "ADV", "PTKZU", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Bey hoffe-lufft verwelckt der keuschheit blume bald.", "tokens": ["Bey", "hof\u00b7fe\u00b7lufft", "ver\u00b7welckt", "der", "keuschheit", "blu\u00b7me", "bald", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "ART", "NN", "VVFIN", "ADV", "$."], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.19": {"text": "Die sonnen k\u00f6nnen da nicht ohne wolcken stehen/", "tokens": ["Die", "son\u00b7nen", "k\u00f6n\u00b7nen", "da", "nicht", "oh\u00b7ne", "wol\u00b7cken", "ste\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VMFIN", "ADV", "PTKNEG", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "In einem augenblick ist glut und feuer kalt.", "tokens": ["In", "ei\u00b7nem", "au\u00b7gen\u00b7blick", "ist", "glut", "und", "feu\u00b7er", "kalt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "ADJD", "KON", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Wer ist wohl auff dem ei\u00df des hoffes nicht geglitten?", "tokens": ["Wer", "ist", "wohl", "auff", "dem", "ei\u00df", "des", "hof\u00b7fes", "nicht", "ge\u00b7glit\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "APPR", "ART", "NN", "ART", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Wen f\u00e4ngt das schlimme garn der s\u00fcssen worte nicht?", "tokens": ["Wen", "f\u00e4ngt", "das", "schlim\u00b7me", "garn", "der", "s\u00fcs\u00b7sen", "wor\u00b7te", "nicht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Wird nicht durch hofes griff die tugend auch bestritten?", "tokens": ["Wird", "nicht", "durch", "ho\u00b7fes", "griff", "die", "tu\u00b7gend", "auch", "be\u00b7strit\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "APPR", "ADJA", "VVFIN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Weil da die falschheit selbst die sch\u00f6nsten worte spricht.", "tokens": ["Weil", "da", "die", "falschheit", "selbst", "die", "sch\u00f6ns\u00b7ten", "wor\u00b7te", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "----+-+-+-+", "measure": "unknown.measure.tetra"}, "line.25": {"text": "Zu dem ist solcher glantz der sch\u00f6nheit nicht zu finden", "tokens": ["Zu", "dem", "ist", "sol\u00b7cher", "glantz", "der", "sch\u00f6n\u00b7heit", "nicht", "zu", "fin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "VAFIN", "PIAT", "NN", "ART", "NN", "PTKNEG", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "An deiner schlechten magd/ wie deine feder schreibt.", "tokens": ["An", "dei\u00b7ner", "schlech\u00b7ten", "magd", "/", "wie", "dei\u00b7ne", "fe\u00b7der", "schreibt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$(", "KOKOM", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Mein bl\u00f6des ange kan kein helden-hertze binden/", "tokens": ["Mein", "bl\u00f6\u00b7des", "an\u00b7ge", "kan", "kein", "hel\u00b7den\u00b7hert\u00b7ze", "bin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VMFIN", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und meinen lippen ist kein Purpur einverleibt.", "tokens": ["Und", "mei\u00b7nen", "lip\u00b7pen", "ist", "kein", "Pur\u00b7pur", "ein\u00b7ver\u00b7leibt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Es tr\u00e4gt ihr bleicher rand nichts als verdorrte Nelcken/", "tokens": ["Es", "tr\u00e4gt", "ihr", "blei\u00b7cher", "rand", "nichts", "als", "ver\u00b7dorr\u00b7te", "Nel\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "VVFIN", "PIS", "KOKOM", "VVFIN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Und auf den wangen sind die Rosen schon verbl\u00fcht.", "tokens": ["Und", "auf", "den", "wan\u00b7gen", "sind", "die", "Ro\u00b7sen", "schon", "ver\u00b7bl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VAFIN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Mein hal\u00df vergleichet sich Nareissen die verwelcken/", "tokens": ["Mein", "hal\u00df", "ver\u00b7glei\u00b7chet", "sich", "Na\u00b7reis\u00b7sen", "die", "ver\u00b7wel\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "NN", "ART", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Die br\u00fcste tragen schnee/ indem kein feuer gl\u00fcht/", "tokens": ["Die", "br\u00fcs\u00b7te", "tra\u00b7gen", "schnee", "/", "in\u00b7dem", "kein", "feu\u00b7er", "gl\u00fcht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$(", "KOUS", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Durch hohe reden kan mein geist sich nicht entdecken/", "tokens": ["Durch", "ho\u00b7he", "re\u00b7den", "kan", "mein", "geist", "sich", "nicht", "ent\u00b7de\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "VVINF", "VMFIN", "PPOSAT", "NN", "PRF", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "In meiner seelen wohnt ein niedriger verstand/", "tokens": ["In", "mei\u00b7ner", "see\u00b7len", "wohnt", "ein", "nied\u00b7ri\u00b7ger", "ver\u00b7stand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Ich kan die schalckheit nicht in bunte schaalen stecken/", "tokens": ["Ich", "kan", "die", "schalck\u00b7heit", "nicht", "in", "bun\u00b7te", "schaa\u00b7len", "ste\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PTKNEG", "APPR", "ADJA", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Und meinen sinnen ist die hoffart unbekandt.", "tokens": ["Und", "mei\u00b7nen", "sin\u00b7nen", "ist", "die", "hof\u00b7fart", "un\u00b7be\u00b7kandt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "VAFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Ja was ein mehres ist/ bin ich nicht schon gebunden?", "tokens": ["Ja", "was", "ein", "meh\u00b7res", "ist", "/", "bin", "ich", "nicht", "schon", "ge\u00b7bun\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PRELS", "ART", "PIS", "VAFIN", "$(", "VAFIN", "PPER", "PTKNEG", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Ich bin mit mund und hand an meinen mann getraut/", "tokens": ["Ich", "bin", "mit", "mund", "und", "hand", "an", "mei\u00b7nen", "mann", "ge\u00b7traut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "KON", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Es hat des ehstands netz die freyheit mir umbwunden.", "tokens": ["Es", "hat", "des", "eh\u00b7stands", "netz", "die", "frey\u00b7heit", "mir", "um\u00b7bwun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Der liebe Myrthen seyn mir ein verbotnes kraut.", "tokens": ["Der", "lie\u00b7be", "Myr\u00b7then", "seyn", "mir", "ein", "ver\u00b7bot\u00b7nes", "kraut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAINF", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Wie kan ich ohne schimpff aus diesem cirkel gehen?", "tokens": ["Wie", "kan", "ich", "oh\u00b7ne", "schimpff", "aus", "die\u00b7sem", "cir\u00b7kel", "ge\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "APPR", "NN", "APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Den priesters hand umb mich und meinen mann gemacht:", "tokens": ["Den", "pries\u00b7ters", "hand", "umb", "mich", "und", "mei\u00b7nen", "mann", "ge\u00b7macht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPER", "KON", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Es mu\u00df kein geiles aug in meinen spiegel sehen/", "tokens": ["Es", "mu\u00df", "kein", "gei\u00b7les", "aug", "in", "mei\u00b7nen", "spie\u00b7gel", "se\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Denn dadurch werden ihm nur flecken zugebracht.", "tokens": ["Denn", "da\u00b7durch", "wer\u00b7den", "ihm", "nur", "fle\u00b7cken", "zu\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VAFIN", "PPER", "ADV", "VVINF", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Du schreibest/ grosser F\u00fcrst! Ich soll des p\u00f6bels lachen;", "tokens": ["Du", "schrei\u00b7best", "/", "gros\u00b7ser", "F\u00fcrst", "!", "Ich", "soll", "des", "p\u00f6\u00b7bels", "la\u00b7chen", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "ADJA", "NN", "$.", "PPER", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Stopfft man dann auch dadurch der leute m\u00e4uler zu?", "tokens": ["Stopfft", "man", "dann", "auch", "da\u00b7durch", "der", "leu\u00b7te", "m\u00e4u\u00b7ler", "zu", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "ADV", "PAV", "ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Du darffst dir keine sorg umb andrer urtheil machen/", "tokens": ["Du", "darffst", "dir", "kei\u00b7ne", "sorg", "umb", "an\u00b7drer", "ur\u00b7theil", "ma\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "NN", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Mir aber wird dadurch verg\u00e4lt die s\u00fcsse ruh.", "tokens": ["Mir", "a\u00b7ber", "wird", "da\u00b7durch", "ver\u00b7g\u00e4lt", "die", "s\u00fcs\u00b7se", "ruh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "PAV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Allein wo denck ich hin? ich flehe tauben ohren/", "tokens": ["Al\u00b7lein", "wo", "denck", "ich", "hin", "?", "ich", "fle\u00b7he", "tau\u00b7ben", "oh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "VVIMP", "PPER", "PTKVZ", "$.", "PPER", "VVFIN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Auf k\u00f6nige befehl mu\u00df man gehorsam seyn.", "tokens": ["Auf", "k\u00f6\u00b7ni\u00b7ge", "be\u00b7fehl", "mu\u00df", "man", "ge\u00b7hor\u00b7sam", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VMFIN", "PIS", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Hat ungl\u00fcck oder gl\u00fcck zum balle mich erkohren/", "tokens": ["Hat", "un\u00b7gl\u00fcck", "o\u00b7der", "gl\u00fcck", "zum", "bal\u00b7le", "mich", "er\u00b7koh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KON", "ADJD", "APPRART", "NN", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "So gibt mein gantzes hertz gedultig sich darein.", "tokens": ["So", "gibt", "mein", "gant\u00b7zes", "hertz", "ge\u00b7dul\u00b7tig", "sich", "da\u00b7rein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "ADJD", "PRF", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "Wird sch\u00f6nheit und verstand der liebe zunder fehlen/", "tokens": ["Wird", "sch\u00f6n\u00b7heit", "und", "ver\u00b7stand", "der", "lie\u00b7be", "zun\u00b7der", "feh\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KON", "VVFIN", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "Geb ich mich h\u00f6her nicht als eine selavin an/", "tokens": ["Geb", "ich", "mich", "h\u00f6\u00b7her", "nicht", "als", "ei\u00b7ne", "se\u00b7la\u00b7vin", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADJD", "PTKNEG", "KOUS", "ART", "ADV", "PTKVZ", "$("], "meter": "+--+-+-+--+-+", "measure": "iambic.hexa.invert"}, "line.55": {"text": "Will deine g\u00fctigkeit nicht meine m\u00e4ngel zehlen/", "tokens": ["Will", "dei\u00b7ne", "g\u00fc\u00b7tig\u00b7keit", "nicht", "mei\u00b7ne", "m\u00e4n\u00b7gel", "zeh\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "PTKNEG", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.56": {"text": "Werd ich geflissen seyn/ wie ich dir dienen kan.", "tokens": ["Werd", "ich", "ge\u00b7flis\u00b7sen", "seyn", "/", "wie", "ich", "dir", "die\u00b7nen", "kan", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "VAINF", "$(", "PWAV", "PPER", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.57": {"text": "Hat meine wenigkeit dein hoher geist erkohren/", "tokens": ["Hat", "mei\u00b7ne", "we\u00b7nig\u00b7keit", "dein", "ho\u00b7her", "geist", "er\u00b7koh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "KOUS", "PPOSAT", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.58": {"text": "Mu\u00df ich ja billig nach verm\u00f6gen danckbahr seyn.", "tokens": ["Mu\u00df", "ich", "ja", "bil\u00b7lig", "nach", "ver\u00b7m\u00f6\u00b7gen", "dan\u00b7ck\u00b7bahr", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADJD", "APPR", "VVFIN", "ADV", "VAINF", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.59": {"text": "Zwar hab ich lieb und treu zu meinen mann geschworen/", "tokens": ["Zwar", "hab", "ich", "lieb", "und", "treu", "zu", "mei\u00b7nen", "mann", "ge\u00b7schwo\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.60": {"text": "Allein des k\u00f6nigs recht reist schwur und bindni\u00df ein!", "tokens": ["Al\u00b7lein", "des", "k\u00f6\u00b7nigs", "recht", "reist", "schwur", "und", "bind\u00b7ni\u00df", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADV", "ADJD", "VVFIN", "KON", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.61": {"text": "Bin ich den nun befreyt von dem/ dem ich versprochen/", "tokens": ["Bin", "ich", "den", "nun", "be\u00b7freyt", "von", "dem", "/", "dem", "ich", "ver\u00b7spro\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADV", "VVPP", "APPR", "ART", "$(", "PRELS", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.62": {"text": "So h\u00e4lt mich keine macht von deiner glut zur\u00fcck.", "tokens": ["So", "h\u00e4lt", "mich", "kei\u00b7ne", "macht", "von", "dei\u00b7ner", "glut", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "VVFIN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.63": {"text": "Durch diese liebe wird kein eyd von mir gebrochen/", "tokens": ["Durch", "die\u00b7se", "lie\u00b7be", "wird", "kein", "eyd", "von", "mir", "ge\u00b7bro\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "VAFIN", "PIAT", "NN", "APPR", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.64": {"text": "Und was dein scepter sch\u00fctzt/ ber\u00fchrt kein ungl\u00fcck.", "tokens": ["Und", "was", "dein", "scep\u00b7ter", "sch\u00fctzt", "/", "be\u00b7r\u00fchrt", "kein", "un\u00b7gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPOSAT", "ADJA", "VVFIN", "$(", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.65": {"text": "Wolan so seys gewagt/ ich gebe mich gefangen/", "tokens": ["Wo\u00b7lan", "so", "seys", "ge\u00b7wagt", "/", "ich", "ge\u00b7be", "mich", "ge\u00b7fan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "VVPP", "$(", "PPER", "VVFIN", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.66": {"text": "Mein hertze sp\u00fchret schon den kercker/ drinn es liegt.", "tokens": ["Mein", "hert\u00b7ze", "sp\u00fch\u00b7ret", "schon", "den", "ker\u00b7cker", "/", "drinn", "es", "liegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "ART", "NN", "$(", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.67": {"text": "Doch werd ich dieses lob zum wenigsten erlangen/", "tokens": ["Doch", "werd", "ich", "die\u00b7ses", "lob", "zum", "we\u00b7nigs\u00b7ten", "er\u00b7lan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PDAT", "NN", "APPRART", "PIS", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.68": {"text": "Da\u00df mich der gr\u00f6ste held der erden hat besiegt.", "tokens": ["Da\u00df", "mich", "der", "gr\u00f6s\u00b7te", "held", "der", "er\u00b7den", "hat", "be\u00b7siegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "VVFIN", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.69": {"text": "Und mu\u00df ich gleich daf\u00fcr in h\u00f6chstem grad ertragen", "tokens": ["Und", "mu\u00df", "ich", "gleich", "da\u00b7f\u00fcr", "in", "h\u00f6chs\u00b7tem", "grad", "er\u00b7tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "ADV", "PAV", "APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.70": {"text": "Des mannes eiffersucht/ den ha\u00df der k\u00f6nigin/", "tokens": ["Des", "man\u00b7nes", "eif\u00b7fer\u00b7sucht", "/", "den", "ha\u00df", "der", "k\u00f6\u00b7ni\u00b7gin", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.71": {"text": "Soll doch dies alles nie die liebe niederschlagen", "tokens": ["Soll", "doch", "dies", "al\u00b7les", "nie", "die", "lie\u00b7be", "nie\u00b7der\u00b7schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "PDS", "PIS", "ADV", "ART", "ADJA", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.72": {"text": "Zu welcher ich von dir/ o printz! bewogen bin.", "tokens": ["Zu", "wel\u00b7cher", "ich", "von", "dir", "/", "o", "printz", "!", "be\u00b7wo\u00b7gen", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "APPR", "PPER", "$