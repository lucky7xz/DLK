{"textgrid.poem.35671": {"metadata": {"author": {"name": "Vo\u00df, Johann Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "34. Die S\u00e4ngerin", "genre": "verse", "period": "N.A.", "pub_year": 1786, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Leiser scholl mein Gesang in des Klaviers schmachtenden Silberton", "tokens": ["Lei\u00b7ser", "scholl", "mein", "Ge\u00b7sang", "in", "des", "Kla\u00b7viers", "schmach\u00b7ten\u00b7den", "Sil\u00b7ber\u00b7ton"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "PPOSAT", "NN", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+--+--+-+--+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Denn das M\u00e4dchen erhub, \u00fcbergelehnt, hellere Melodie:", "tokens": ["Denn", "das", "M\u00e4d\u00b7chen", "er\u00b7hub", ",", "\u00fc\u00b7ber\u00b7ge\u00b7lehnt", ",", "hel\u00b7le\u00b7re", "Me\u00b7lo\u00b7die", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$,", "VVPP", "$,", "ADJA", "NN", "$."], "meter": "--+-+-+--++--+-+", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Da\u00df ihr Busen dem Flor sch\u00fcchtern entwallt', und mit der Rose Duft", "tokens": ["Da\u00df", "ihr", "Bu\u00b7sen", "dem", "Flor", "sch\u00fcch\u00b7tern", "ent\u00b7wallt'", ",", "und", "mit", "der", "Ro\u00b7se", "Duft"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "NN", "VVFIN", "PTKVZ", "$,", "KON", "APPR", "ART", "ADJA", "NN"], "meter": "--+--++--+-+-+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Warm ihr rosiger Mund gegen die Wang' \u00c4ther mir atmete.", "tokens": ["Warm", "ihr", "ro\u00b7si\u00b7ger", "Mund", "ge\u00b7gen", "die", "Wang'", "\u00c4\u00b7ther", "mir", "at\u00b7me\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "APPR", "ART", "NN", "NN", "PPER", "VVFIN", "$."], "meter": "+-+--+-+-++--+--", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "Glut durchstr\u00f6mte die Wang', und in der Brust pochte mein Herz empor;", "tokens": ["Glut", "durch\u00b7str\u00f6m\u00b7te", "die", "Wang'", ",", "und", "in", "der", "Brust", "poch\u00b7te", "mein", "Herz", "em\u00b7por", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,", "KON", "APPR", "ART", "NN", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+--+-+-+---+-+", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Und mir stockte die Stimm', unter der Hand stammelte Mi\u00dfget\u00f6n.", "tokens": ["Und", "mir", "stock\u00b7te", "die", "Stimm'", ",", "un\u00b7ter", "der", "Hand", "stam\u00b7mel\u00b7te", "Mi\u00df\u00b7ge\u00b7t\u00f6n", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$,", "APPR", "ART", "NN", "VVFIN", "NE", "$."], "meter": "--+--++--+-+-+-+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Auch des M\u00e4dchens Gesang stammelte hold. Trunken von Seligkeit", "tokens": ["Auch", "des", "M\u00e4d\u00b7chens", "Ge\u00b7sang", "stam\u00b7mel\u00b7te", "hold", ".", "Trun\u00b7ken", "von", "Se\u00b7lig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "NN", "VVFIN", "ADJD", "$.", "NN", "APPR", "NN"], "meter": "--+--+-+--+--+-+", "measure": "anapaest.di.plus"}, "line.8": {"text": "Bebt' ich n\u00e4her; und ach! unter dem Ku\u00df zuckte der Rosenmund.", "tokens": ["Bebt'", "ich", "n\u00e4\u00b7her", ";", "und", "ach", "!", "un\u00b7ter", "dem", "Ku\u00df", "zuck\u00b7te", "der", "Ro\u00b7sen\u00b7mund", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$.", "KON", "XY", "$.", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+--++--++--+-+", "measure": "trochaic.octa.plus.relaxed"}}}}}