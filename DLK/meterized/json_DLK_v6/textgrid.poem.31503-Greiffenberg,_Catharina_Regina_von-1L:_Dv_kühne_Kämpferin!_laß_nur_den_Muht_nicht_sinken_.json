{"textgrid.poem.31503": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dv k\u00fchne K\u00e4mpferin! la\u00df nur den Muht nicht sinken /", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dv k\u00fchne K\u00e4mpferin! la\u00df nur den Muht nicht sinken /", "tokens": ["Dv", "k\u00fch\u00b7ne", "K\u00e4mp\u00b7fe\u00b7rin", "!", "la\u00df", "nur", "den", "Muht", "nicht", "sin\u00b7ken", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$.", "VVIMP", "ADV", "ART", "NN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "halt bet- und n\u00f6tend' an! klopf hart an diesen Stein:", "tokens": ["halt", "bet", "und", "n\u00f6\u00b7tend'", "an", "!", "klopf", "hart", "an", "die\u00b7sen", "Stein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "TRUNC", "KON", "VVFIN", "PTKVZ", "$.", "VVFIN", "ADJD", "APPR", "PDAT", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "ein Gnaden-f\u00fcnklein wird unfehlbar seyn darein /", "tokens": ["ein", "Gna\u00b7den\u00b7f\u00fcnk\u00b7lein", "wird", "un\u00b7fehl\u00b7bar", "seyn", "da\u00b7rein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "VAINF", "PAV", "$("], "meter": "-+-+-+---+-+", "measure": "unknown.measure.penta"}, "line.4": {"text": "das wird / nach starken sto\u00df / mit Freuden aus ihm blinken.", "tokens": ["das", "wird", "/", "nach", "star\u00b7ken", "sto\u00df", "/", "mit", "Freu\u00b7den", "aus", "ihm", "blin\u00b7ken", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$(", "APPR", "ADJA", "NN", "$(", "APPR", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Du wirst nach Heiles-Safft aus diesem Felsen trinken.", "tokens": ["Du", "wirst", "nach", "Hei\u00b7les\u00b7\u00b7S\u00b7afft", "aus", "die\u00b7sem", "Fel\u00b7sen", "trin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Das Tieger / wird gar bald ein Pelican dir seyn;", "tokens": ["Das", "Tie\u00b7ger", "/", "wird", "gar", "bald", "ein", "Pe\u00b7li\u00b7can", "dir", "seyn", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "VAFIN", "ADV", "ADV", "ART", "NN", "PPER", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "der Mars / ein Venus Stern; Blitz-Donner / Sonnenschein.", "tokens": ["der", "Mars", "/", "ein", "Ve\u00b7nus", "Stern", ";", "Blitz\u00b7Don\u00b7ner", "/", "Son\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "NN", "NN", "$.", "NN", "$(", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Es kommt bereit die Zeit / H\u00fclf / Raht / und That zu winken.", "tokens": ["Es", "kommt", "be\u00b7reit", "die", "Zeit", "/", "H\u00fclf", "/", "Raht", "/", "und", "That", "zu", "win\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "NN", "$(", "NE", "$(", "NN", "$(", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "H\u00f6r / was er sagt; O Freud! er giebt / er giebt sich schon.", "tokens": ["H\u00f6r", "/", "was", "er", "sagt", ";", "O", "Freud", "!", "er", "giebt", "/", "er", "giebt", "sich", "schon", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "PWS", "PPER", "VVFIN", "$.", "NE", "NN", "$.", "PPER", "VVFIN", "$(", "PPER", "VVFIN", "PRF", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Vom unbesiegbaren / tr\u00e4gst du den Sieg davon.", "tokens": ["Vom", "un\u00b7be\u00b7sieg\u00b7ba\u00b7ren", "/", "tr\u00e4gst", "du", "den", "Sieg", "da\u00b7von", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$(", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "O Weib dein Glaub ist gro\u00df! es soll dir di\u00df geschehen /", "tokens": ["O", "Weib", "dein", "Glaub", "ist", "gro\u00df", "!", "es", "soll", "dir", "di\u00df", "ge\u00b7sche\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PPOSAT", "NN", "VAFIN", "ADJD", "$.", "PPER", "VMFIN", "PPER", "PDS", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "was du hoch-hoffend dich zu mir jetzt hast versehen.", "tokens": ["was", "du", "hoch\u00b7hof\u00b7fend", "dich", "zu", "mir", "jetzt", "hast", "ver\u00b7se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADJD", "PPER", "APPR", "PPER", "ADV", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ein Glaubens-Heldenstreich / erlegt die gr\u00f6sten zween:", "tokens": ["Ein", "Glau\u00b7bens\u00b7Hel\u00b7den\u00b7streich", "/", "er\u00b7legt", "die", "gr\u00f6s\u00b7ten", "zween", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "VVFIN", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Gott / zu erbarmen sich; den Teufel / auszugehn.", "tokens": ["Gott", "/", "zu", "er\u00b7bar\u00b7men", "sich", ";", "den", "Teu\u00b7fel", "/", "aus\u00b7zu\u00b7gehn", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$(", "PTKZU", "VVINF", "PRF", "$.", "ART", "NN", "$(", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}