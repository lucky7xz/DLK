{"textgrid.poem.33054": {"metadata": {"author": {"name": "B\u00fcchner, Luise", "birth": "N.A.", "death": "N.A."}, "title": "Stille Frage", "genre": "verse", "period": "N.A.", "pub_year": 1849, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es quillt des Abendsterns", "tokens": ["Es", "quillt", "des", "A\u00b7bends\u00b7terns"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Geheimni\u00dfvoller Schein,", "tokens": ["Ge\u00b7heim\u00b7ni\u00df\u00b7vol\u00b7ler", "Schein", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So nah' und auch so fern,", "tokens": ["So", "nah'", "und", "auch", "so", "fern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "KON", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Mir in das Herz hinein.", "tokens": ["Mir", "in", "das", "Herz", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Drin gl\u00fcht ein and'res Licht,", "tokens": ["Drin", "gl\u00fcht", "ein", "an\u00b7d'\u00b7res", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "So nah' und auch so fern,", "tokens": ["So", "nah'", "und", "auch", "so", "fern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "KON", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Herz umschlie\u00dft es dicht \u2013", "tokens": ["Das", "Herz", "um\u00b7schlie\u00dft", "es", "dicht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Doch weit ist's wie der Stern.", "tokens": ["Doch", "weit", "ist's", "wie", "der", "Stern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Du gold'ner Liebesstrahl,", "tokens": ["Du", "gold'\u00b7ner", "Lie\u00b7bes\u00b7strahl", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Geh', frage deinen Stern,", "tokens": ["Geh'", ",", "fra\u00b7ge", "dei\u00b7nen", "Stern", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bleibt er zu deiner Qual,", "tokens": ["Bleibt", "er", "zu", "dei\u00b7ner", "Qual", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Dir ewig, ewig fern?", "tokens": ["Dir", "e\u00b7wig", ",", "e\u00b7wig", "fern", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJD", "$,", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}