{"dta.poem.21130": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "An den Freyherrn von G \u2012 \u2012", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20676-2", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Klage nicht immer, o Freund, von einem feindlichen\nSchicksal,", "tokens": ["Kla\u00b7ge", "nicht", "im\u00b7mer", ",", "o", "Freund", ",", "von", "ei\u00b7nem", "feind\u00b7li\u00b7chen", "Schick\u00b7sal", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ADV", "$,", "FM", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Welches wir feindlicher noch in schwarzen Stunden uns", "tokens": ["Wel\u00b7ches", "wir", "feind\u00b7li\u00b7cher", "noch", "in", "schwar\u00b7zen", "Stun\u00b7den", "uns"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "ADJD", "ADV", "APPR", "ADJA", "NN", "PPER"], "meter": "+--+--+-+-+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Stelle die Welt dir nicht blos von ihrer traurigen Seite,", "tokens": ["Stel\u00b7le", "die", "Welt", "dir", "nicht", "blos", "von", "ih\u00b7rer", "trau\u00b7ri\u00b7gen", "Sei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PPER", "PTKNEG", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Stelle sie dir von der guten auch vor.", "tokens": ["Stel\u00b7le", "sie", "dir", "von", "der", "gu\u00b7ten", "auch", "vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "APPR", "ART", "ADJA", "ADV", "PTKVZ", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.2": {"line.1": {"text": "Soll ich den Vorhang einmal, der deine Freuden", "tokens": ["Soll", "ich", "den", "Vor\u00b7hang", "ein\u00b7mal", ",", "der", "dei\u00b7ne", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ART", "NN", "ADV", "$,", "PRELS", "PPOSAT", "NN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Aufziehn mit zaubernder Hand, und dir in heitern Pro-", "tokens": ["Auf\u00b7ziehn", "mit", "zau\u00b7bern\u00b7der", "Hand", ",", "und", "dir", "in", "hei\u00b7tern", "Pro"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN", "$,", "KON", "PPER", "APPR", "ADJA", "NN"], "meter": "+--+--+-+-+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Helle Gefilde voll Gl\u00fcck, und lachende Landschaften zeit", "tokens": ["Hel\u00b7le", "Ge\u00b7fil\u00b7de", "voll", "Gl\u00fcck", ",", "und", "la\u00b7chen\u00b7de", "Land\u00b7schaf\u00b7ten", "zeit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "ADJD", "NN", "$,", "KON", "ADJA", "NN", "NN"], "meter": "+--+--+-+--++-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Welche die Melancholie dir verbarg?", "tokens": ["Wel\u00b7che", "die", "Me\u00b7lan\u00b7cho\u00b7lie", "dir", "ver\u00b7barg", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+---+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.3": {"line.1": {"text": "Bist du nicht weise mein Freund? Gewi\u00df ein Ge-", "tokens": ["Bist", "du", "nicht", "wei\u00b7se", "mein", "Freund", "?", "Ge\u00b7wi\u00df", "ein", "Ge"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PTKNEG", "VVFIN", "PPOSAT", "NN", "$.", "ADV", "ART", "TRUNC"], "meter": "+--+--+-+--", "measure": "dactylic.di.plus"}, "line.2": {"text": "Nicht oft zu Ahnen gelegt, und zu westindischem Reich-", "tokens": ["Nicht", "oft", "zu", "Ah\u00b7nen", "ge\u00b7legt", ",", "und", "zu", "west\u00b7in\u00b7di\u00b7schem", "Reich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "APPR", "NN", "VVPP", "$,", "KON", "APPR", "ADJA", "TRUNC"], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Kan dir das tobende Meer, kan dir die w\u00fctende", "tokens": ["Kan", "dir", "das", "to\u00b7ben\u00b7de", "Meer", ",", "kan", "dir", "die", "w\u00fc\u00b7ten\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "NN", "$,", "VMFIN", "PPER", "ART", "ADJA"], "meter": "-+-+--+-+-+--", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Rauben das, was nur der Seele geh\u00f6rt?", "tokens": ["Rau\u00b7ben", "das", ",", "was", "nur", "der", "See\u00b7le", "ge\u00b7h\u00f6rt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "$,", "PRELS", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.4": {"line.1": {"text": "W\u00e4rest du nun ein Monarch, dem Millionen ge-", "tokens": ["W\u00e4\u00b7rest", "du", "nun", "ein", "Mon\u00b7arch", ",", "dem", "Mil\u00b7lion\u00b7en", "ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN", "$,", "ART", "NN", "TRUNC"], "meter": "+--+-+-+-+--", "measure": "iambic.penta.invert"}, "line.2": {"text": "Dessen gef\u00fcrchteten Ruhm un\u00fcberwindliche Flotten", "tokens": ["Des\u00b7sen", "ge\u00b7f\u00fcrch\u00b7te\u00b7ten", "Ruhm", "un\u00b7\u00fc\u00b7berw\u00b7ind\u00b7li\u00b7che", "Flot\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDAT", "ADJA", "NN", "ADJA", "NN"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.3": {"text": "Ueber das zagende Meer kleinm\u00fcthigen Inseln verk\u00fcn-", "tokens": ["Ue\u00b7ber", "das", "za\u00b7gen\u00b7de", "Meer", "klein\u00b7m\u00fct\u00b7hi\u00b7gen", "In\u00b7seln", "ver\u00b7k\u00fcn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJA", "NN", "TRUNC"], "meter": "+--+--+-+--+--+", "measure": "dactylic.di.plus"}, "line.4": {"text": "W\u00fcrdest du etwa gl\u00fcckseliger seyn?", "tokens": ["W\u00fcr\u00b7dest", "du", "et\u00b7wa", "gl\u00fcck\u00b7se\u00b7li\u00b7ger", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "VAINF", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.5": {"line.1": {"text": "W\u00fcrdest du, einsam und ernst, mit deiner erha-", "tokens": ["W\u00fcr\u00b7dest", "du", ",", "ein\u00b7sam", "und", "ernst", ",", "mit", "dei\u00b7ner", "er\u00b7ha"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PPER", "$,", "ADJD", "KON", "ADJD", "$,", "APPR", "PPOSAT", "TRUNC"], "meter": "+--+--+-+--+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Mehr noch bekannt seyn, als ietzt? und w\u00fcrden ver-", "tokens": ["Mehr", "noch", "be\u00b7kannt", "seyn", ",", "als", "ietzt", "?", "und", "w\u00fcr\u00b7den", "ver"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "ADJD", "VAINF", "$,", "KOUS", "ADV", "$.", "KON", "VAFIN", "TRUNC"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Dichter aus Rom und Athen zum Throne des K\u00f6nigs", "tokens": ["Dich\u00b7ter", "aus", "Rom", "und", "A\u00b7then", "zum", "Thro\u00b7ne", "des", "K\u00f6\u00b7nigs"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NE", "KON", "NE", "APPRART", "NN", "ART", "NN"], "meter": "+--+-+--+--+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Welcher nur blutiger Ehre gefolgt?", "tokens": ["Wel\u00b7cher", "nur", "blu\u00b7ti\u00b7ger", "Eh\u00b7re", "ge\u00b7folgt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "ADV", "ADJA", "NN", "VVPP", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.6": {"line.1": {"text": "Oder ged\u00e4chtest du denn, wenn du beladene Flot-", "tokens": ["O\u00b7der", "ge\u00b7d\u00e4ch\u00b7test", "du", "denn", ",", "wenn", "du", "be\u00b7la\u00b7de\u00b7ne", "Flot"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "ADJA", "TRUNC"], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Ueber die Meere geschickt, dich mit dem Golde zu tr\u00f6-", "tokens": ["Ue\u00b7ber", "die", "Mee\u00b7re", "ge\u00b7schickt", ",", "dich", "mit", "dem", "Gol\u00b7de", "zu", "tr\u00f6"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVPP", "$,", "PRF", "APPR", "ART", "NN", "APPR", "TRUNC"], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Oder vermeintest du wohl in Cyperns bunten Gefilden", "tokens": ["O\u00b7der", "ver\u00b7mein\u00b7test", "du", "wohl", "in", "Cy\u00b7perns", "bun\u00b7ten", "Ge\u00b7fil\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "NE", "ADJA", "NN"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Gl\u00fccklicher ohne die Schwermuth zu seyn?", "tokens": ["Gl\u00fcck\u00b7li\u00b7cher", "oh\u00b7ne", "die", "Schwer\u00b7muth", "zu", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PTKZU", "VAINF", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.7": {"line.1": {"text": "G \u2014, gl\u00fccklich bist du, da\u00df deine denkende Seele", "tokens": ["G", ",", "gl\u00fcck\u00b7lich", "bist", "du", ",", "da\u00df", "dei\u00b7ne", "den\u00b7ken\u00b7de", "See\u00b7le"], "token_info": ["word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$(", "$,", "ADJD", "VAFIN", "PPER", "$,", "KOUS", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Sich mit seraphischem Schwung zu h\u00f6hern Sph\u00e4ren", "tokens": ["Sich", "mit", "se\u00b7ra\u00b7phi\u00b7schem", "Schwung", "zu", "h\u00f6\u00b7hern", "Sph\u00e4\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PRF", "APPR", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Flie\u00dft auch dein Leben dahin, gleich B\u00e4chen in trauri-", "tokens": ["Flie\u00dft", "auch", "dein", "Le\u00b7ben", "da\u00b7hin", ",", "gleich", "B\u00e4\u00b7chen", "in", "trau\u00b7ri"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "PAV", "$,", "ADV", "NN", "APPR", "TRUNC"], "meter": "+--+--+-+--+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Ist denn dies Leben der Klage wohl werth?", "tokens": ["Ist", "denn", "dies", "Le\u00b7ben", "der", "Kla\u00b7ge", "wohl", "werth", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PDS", "NN", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Aber der Himmel hat ja dein philosophisches Le-", "tokens": ["A\u00b7ber", "der", "Him\u00b7mel", "hat", "ja", "dein", "phi\u00b7lo\u00b7so\u00b7phi\u00b7sches", "Le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VAFIN", "ADV", "PPOSAT", "ADJA", "TRUNC"], "meter": "+--+-+-+-+-+-+", "measure": "iambic.septa.invert"}, "line.2": {"text": "Auch mit dem Gl\u00fccke durchwebt, und mit der Freude", "tokens": ["Auch", "mit", "dem", "Gl\u00fc\u00b7cke", "durch\u00b7webt", ",", "und", "mit", "der", "Freu\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "VVPP", "$,", "KON", "APPR", "ART", "NN"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Sage, f\u00fcr was f\u00fcr ein Gl\u00fcck willst du die Stunden", "tokens": ["Sa\u00b7ge", ",", "f\u00fcr", "was", "f\u00fcr", "ein", "Gl\u00fcck", "willst", "du", "die", "Stun\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "APPR", "PRELS", "APPR", "ART", "NN", "VMFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Die du in einsamen N\u00e4chten durchdenkst?", "tokens": ["Die", "du", "in", "ein\u00b7sa\u00b7men", "N\u00e4ch\u00b7ten", "durch\u00b7denkst", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.9": {"line.1": {"text": "H\u00f6rest du nicht auch entz\u00fcckt der holden Pirkerin", "tokens": ["H\u00f6\u00b7rest", "du", "nicht", "auch", "ent\u00b7z\u00fcckt", "der", "hol\u00b7den", "Pir\u00b7ke\u00b7rin"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": "R\u00fchret dich nicht im Concert die Biankinische Geige?", "tokens": ["R\u00fch\u00b7ret", "dich", "nicht", "im", "Con\u00b7cert", "die", "Bi\u00b7an\u00b7ki\u00b7ni\u00b7sche", "Gei\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+-+---+-+-+-", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Sch\u00e4umet Champagner Wein nicht in deinem umkr\u00e4n-", "tokens": ["Sch\u00e4u\u00b7met", "Cham\u00b7pag\u00b7ner", "Wein", "nicht", "in", "dei\u00b7nem", "um\u00b7kr\u00e4n"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "NN", "PTKNEG", "APPR", "PPOSAT", "TRUNC"], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Singet die Hubersche Leyer nicht dir?", "tokens": ["Sin\u00b7get", "die", "Hu\u00b7ber\u00b7sche", "Le\u00b7yer", "nicht", "dir", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PTKNEG", "PPER", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.10": {"line.1": {"text": "Heitre die Stirne dann auf, die eremitische Run-", "tokens": ["Heit\u00b7re", "die", "Stir\u00b7ne", "dann", "auf", ",", "die", "e\u00b7re\u00b7mi\u00b7ti\u00b7sche", "Run"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ART", "NN", "ADV", "PTKVZ", "$,", "ART", "ADJA", "TRUNC"], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Lange mit Tiefsinn und Ernst und Unzufriedenheit fur-", "tokens": ["Lan\u00b7ge", "mit", "Tief\u00b7sinn", "und", "Ernst", "und", "Un\u00b7zu\u00b7frie\u00b7den\u00b7heit", "fur"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "KON", "NE", "KON", "NN", "TRUNC"], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Wende den Blick zum Olymp, und deine m\u00e4chtige Leyer", "tokens": ["Wen\u00b7de", "den", "Blick", "zum", "O\u00b7lymp", ",", "und", "dei\u00b7ne", "m\u00e4ch\u00b7ti\u00b7ge", "Le\u00b7yer"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "APPRART", "NN", "$,", "KON", "PPOSAT", "ADJA", "NN"], "meter": "+--+--+-+-+-+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Singe dir fr\u00f6liche Stunden herab!", "tokens": ["Sin\u00b7ge", "dir", "fr\u00f6\u00b7li\u00b7che", "Stun\u00b7den", "her\u00b7ab", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "ADV", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}}}}