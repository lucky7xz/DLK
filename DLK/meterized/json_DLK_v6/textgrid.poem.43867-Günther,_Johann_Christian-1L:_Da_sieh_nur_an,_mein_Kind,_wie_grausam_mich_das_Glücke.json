{"textgrid.poem.43867": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Da sieh nur an, mein Kind, wie grausam mich das Gl\u00fccke", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da sieh nur an, mein Kind, wie grausam mich das Gl\u00fccke", "tokens": ["Da", "sieh", "nur", "an", ",", "mein", "Kind", ",", "wie", "grau\u00b7sam", "mich", "das", "Gl\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "PTKVZ", "$,", "PPOSAT", "NN", "$,", "PWAV", "ADJD", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als keinen auf der Welt in allen Sachen dr\u00fccke;", "tokens": ["Als", "kei\u00b7nen", "auf", "der", "Welt", "in", "al\u00b7len", "Sa\u00b7chen", "dr\u00fc\u00b7cke", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "APPR", "ART", "NN", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es gab dich mir zu sehn, es gab mir deinen Ku\u00df,", "tokens": ["Es", "gab", "dich", "mir", "zu", "sehn", ",", "es", "gab", "mir", "dei\u00b7nen", "Ku\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPER", "PTKZU", "VVINF", "$,", "PPER", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und mitten in der Lust, im Anfang unsrer Flammen,", "tokens": ["Und", "mit\u00b7ten", "in", "der", "Lust", ",", "im", "An\u00b7fang", "uns\u00b7rer", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "$,", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Rei\u00dft uns sein harter Schlu\u00df", "tokens": ["Rei\u00dft", "uns", "sein", "har\u00b7ter", "Schlu\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Durch einen Streich vonsammen,", "tokens": ["Durch", "ei\u00b7nen", "Streich", "von\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Der dich in Unruh sezt und mich besch\u00e4men mu\u00df.", "tokens": ["Der", "dich", "in", "Un\u00b7ruh", "sezt", "und", "mich", "be\u00b7sch\u00e4\u00b7men", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "VVPP", "KON", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Es scheint zwar etwas viel, drei Tag einander kennen", "tokens": ["Es", "scheint", "zwar", "et\u00b7was", "viel", ",", "drei", "Tag", "ein\u00b7an\u00b7der", "ken\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADV", "$,", "CARD", "NN", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und in drey Tagen schon von gleichem Zunder brennen,", "tokens": ["Und", "in", "drey", "Ta\u00b7gen", "schon", "von", "glei\u00b7chem", "Zun\u00b7der", "bren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "CARD", "NN", "ADV", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dies scheint dem P\u00f6bel viel, doch wundert mich es nicht;", "tokens": ["Dies", "scheint", "dem", "P\u00f6\u00b7bel", "viel", ",", "doch", "wun\u00b7dert", "mich", "es", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "ADV", "$,", "ADV", "VVFIN", "PPER", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Denn Lieben ist ein Bund getreu- und edler Herzen,", "tokens": ["Denn", "Lie\u00b7ben", "ist", "ein", "Bund", "ge\u00b7treu", "und", "ed\u00b7ler", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "VAFIN", "ART", "NN", "TRUNC", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die durch der Augen Licht", "tokens": ["Die", "durch", "der", "Au\u00b7gen", "Licht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Sogleich verbindlich scherzen,", "tokens": ["Sog\u00b7leich", "ver\u00b7bind\u00b7lich", "scher\u00b7zen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Sobald die \u00c4hnligkeit der Geister ausw\u00e4rts bricht.", "tokens": ["So\u00b7bald", "die", "\u00c4hn\u00b7lig\u00b7keit", "der", "Geis\u00b7ter", "aus\u00b7w\u00e4rts", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Frag dich nur selber aus, so wirstu mich ergr\u00fcnden,", "tokens": ["Frag", "dich", "nur", "sel\u00b7ber", "aus", ",", "so", "wirs\u00b7tu", "mich", "er\u00b7gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ADV", "PTKVZ", "$,", "ADV", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Besuche dich genau, du wirst mein Herz schon finden,", "tokens": ["Be\u00b7su\u00b7che", "dich", "ge\u00b7nau", ",", "du", "wirst", "mein", "Herz", "schon", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$,", "PPER", "VAFIN", "PPOSAT", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da, wo die Ros und Schnee den vollen Busen deckt.", "tokens": ["Da", ",", "wo", "die", "Ros", "und", "Schnee", "den", "vol\u00b7len", "Bu\u00b7sen", "deckt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ART", "NN", "KON", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Auch dein Herz fing ich bald mit halb erstohlnen K\u00fc\u00dfen,", "tokens": ["Auch", "dein", "Herz", "fing", "ich", "bald", "mit", "halb", "er\u00b7stohl\u00b7nen", "K\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "PPER", "ADV", "APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "O z\u00e4rtliches Confect,", "tokens": ["O", "z\u00e4rt\u00b7li\u00b7ches", "Con\u00b7fect", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Davon du selbst wirst wi\u00dfen,", "tokens": ["Da\u00b7von", "du", "selbst", "wirst", "wi\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "ADV", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wie kr\u00e4ftig und wie gut es auch im Schlafe schmeckt.", "tokens": ["Wie", "kr\u00e4f\u00b7tig", "und", "wie", "gut", "es", "auch", "im", "Schla\u00b7fe", "schmeckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "KON", "PWAV", "ADJD", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Das Dr\u00fccken sch\u00f6ner Hand erg\u00f6zt mir noch die Sinnen;", "tokens": ["Das", "Dr\u00fc\u00b7cken", "sch\u00f6\u00b7ner", "Hand", "er\u00b7g\u00f6zt", "mir", "noch", "die", "Sin\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Vorwiz sa\u00df dabey und ward es doch nicht innen,", "tokens": ["Der", "Vor\u00b7wiz", "sa\u00df", "da\u00b7bey", "und", "ward", "es", "doch", "nicht", "in\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PAV", "KON", "VAFIN", "PPER", "ADV", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wenn unsrer Finger Scherz die stumme Sehnsucht wies.", "tokens": ["Wenn", "uns\u00b7rer", "Fin\u00b7ger", "Scherz", "die", "stum\u00b7me", "Sehn\u00b7sucht", "wies", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So sch\u00f6n entz\u00fcckt uns kaum der Morgenr\u00f6the Prangen,", "tokens": ["So", "sch\u00f6n", "ent\u00b7z\u00fcckt", "uns", "kaum", "der", "Mor\u00b7gen\u00b7r\u00f6\u00b7the", "Pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So sch\u00f6n kein Paradies", "tokens": ["So", "sch\u00f6n", "kein", "Pa\u00b7ra\u00b7dies"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJD", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Als damahls deine Wangen,", "tokens": ["Als", "da\u00b7mahls", "dei\u00b7ne", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Da sich mein fauler Geist dein M\u00e4ulchen wecken lies.", "tokens": ["Da", "sich", "mein", "fau\u00b7ler", "Geist", "dein", "M\u00e4ul\u00b7chen", "we\u00b7cken", "lies", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PPOSAT", "ADJA", "NN", "PPOSAT", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "O Lust voll Eitelkeit! So fl\u00fcchtig sind die Sachen,", "tokens": ["O", "Lust", "voll", "Ei\u00b7tel\u00b7keit", "!", "So", "fl\u00fcch\u00b7tig", "sind", "die", "Sa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADJD", "NN", "$.", "ADV", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Woraus wir Sterblichen ein himmlisch Gl\u00fccke machen;", "tokens": ["Wo\u00b7raus", "wir", "Sterb\u00b7li\u00b7chen", "ein", "himm\u00b7lisch", "Gl\u00fc\u00b7cke", "ma\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "ART", "ADJD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der vierte Mittag kommt, so heist es: Gute Nacht.", "tokens": ["Der", "vier\u00b7te", "Mit\u00b7tag", "kommt", ",", "so", "heist", "es", ":", "Gu\u00b7te", "Nacht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "ADV", "VAFIN", "PPER", "$.", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie mir zu Muthe sey, das wirstu selbst wohl f\u00fchlen.", "tokens": ["Wie", "mir", "zu", "Mu\u00b7the", "sey", ",", "das", "wirs\u00b7tu", "selbst", "wohl", "f\u00fch\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "NN", "VAFIN", "$,", "PRELS", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wer h\u00e4tte dies gedacht,", "tokens": ["Wer", "h\u00e4t\u00b7te", "dies", "ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PDS", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Da\u00df so ein kurzes Spielen", "tokens": ["Da\u00df", "so", "ein", "kur\u00b7zes", "Spie\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "So viele Seelenangst und bange Sehnsucht macht.", "tokens": ["So", "vie\u00b7le", "See\u00b7len\u00b7angst", "und", "ban\u00b7ge", "Sehn\u00b7sucht", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "KON", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Ach, k\u00f6nt ich dir mein Leid in Bildern \u00fcberschicken,", "tokens": ["Ach", ",", "k\u00f6nt", "ich", "dir", "mein", "Leid", "in", "Bil\u00b7dern", "\u00fc\u00b7bersc\u00b7hi\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VMFIN", "PPER", "PPER", "PPOSAT", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ach, h\u00e4tt ich deinen Ku\u00df, wie w\u00fcrd er mich erquicken,", "tokens": ["Ach", ",", "h\u00e4tt", "ich", "dei\u00b7nen", "Ku\u00df", ",", "wie", "w\u00fcrd", "er", "mich", "er\u00b7qui\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VAFIN", "PPER", "PPOSAT", "NN", "$,", "PWAV", "VAFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da Hize, Weg und Sand den m\u00fcden C\u00f6rper qu\u00e4lt!", "tokens": ["Da", "Hi\u00b7ze", ",", "Weg", "und", "Sand", "den", "m\u00fc\u00b7den", "C\u00f6r\u00b7per", "qu\u00e4lt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "NN", "KON", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vor Schwermuth hab ich schon in Wiesen, Thal und Heiden", "tokens": ["Vor", "Schwer\u00b7muth", "hab", "ich", "schon", "in", "Wie\u00b7sen", ",", "Thal", "und", "Hei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "VAFIN", "PPER", "ADV", "APPR", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Den rechten Weg verfehlt", "tokens": ["Den", "rech\u00b7ten", "Weg", "ver\u00b7fehlt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Und dies mein strenges Leiden", "tokens": ["Und", "dies", "mein", "stren\u00b7ges", "Lei\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Den Str\u00e4uchen und der Luft und mehr mir selbst erzehlt.", "tokens": ["Den", "Str\u00e4u\u00b7chen", "und", "der", "Luft", "und", "mehr", "mir", "selbst", "er\u00b7zehlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "KON", "ADV", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Bleib, Olorene, bleib, so wie ich dich gefunden,", "tokens": ["Bleib", ",", "O\u00b7lo\u00b7re\u00b7ne", ",", "bleib", ",", "so", "wie", "ich", "dich", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NE", "$,", "VVFIN", "$,", "ADV", "KOKOM", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich meine klug und treu, und rei\u00df die Abschiedswunden", "tokens": ["Ich", "mei\u00b7ne", "klug", "und", "treu", ",", "und", "rei\u00df", "di