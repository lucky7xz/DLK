{"dta.poem.4106": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Vereinigung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1821", "urn": "urn:nbn:de:kobv:b4-200905194209", "language": ["de:0.85", "no:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In deine klaren, treuen, frommen Sterne,", "tokens": ["In", "dei\u00b7ne", "kla\u00b7ren", ",", "treu\u00b7en", ",", "from\u00b7men", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "$,", "VVINF", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So f\u00fchl' ich weichen das geheime Grauen,", "tokens": ["So", "f\u00fchl'", "ich", "wei\u00b7chen", "das", "ge\u00b7hei\u00b7me", "Grau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das Lieb' und Liebe h\u00e4lt in stummer Ferne.", "tokens": ["Das", "Lieb'", "und", "Lie\u00b7be", "h\u00e4lt", "in", "stum\u00b7mer", "Fer\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und unsre Herzen wollen sich begegnen", "tokens": ["Und", "uns\u00b7re", "Her\u00b7zen", "wol\u00b7len", "sich", "be\u00b7geg\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VMFIN", "PRF", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In langen Blicken, die mit Thr\u00e4nen ringen,", "tokens": ["In", "lan\u00b7gen", "Bli\u00b7cken", ",", "die", "mit", "Thr\u00e4\u00b7nen", "rin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und unsre Liebe will ein Engel segnen:", "tokens": ["Und", "uns\u00b7re", "Lie\u00b7be", "will", "ein", "En\u00b7gel", "seg\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Er schl\u00e4gt um uns die weichen, warmen Schwingen.", "tokens": ["Er", "schl\u00e4gt", "um", "uns", "die", "wei\u00b7chen", ",", "war\u00b7men", "Schwin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nach seinem Namen wag' ich nicht zu fragen,", "tokens": ["Nach", "sei\u00b7nem", "Na\u00b7men", "wag'", "ich", "nicht", "zu", "fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Noch nach dem Namen dessen, der ihn sendet;", "tokens": ["Noch", "nach", "dem", "Na\u00b7men", "des\u00b7sen", ",", "der", "ihn", "sen\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PDS", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Ich darf ja wieder weinen, wieder klagen:", "tokens": ["Ich", "darf", "ja", "wie\u00b7der", "wei\u00b7nen", ",", "wie\u00b7der", "kla\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "VVINF", "$,", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "F\u00fcrwahr, mich hat kein eitler Wahn geblendet!", "tokens": ["F\u00fcr\u00b7wahr", ",", "mich", "hat", "kein", "eit\u00b7ler", "Wahn", "ge\u00b7blen\u00b7det", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "PIAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}