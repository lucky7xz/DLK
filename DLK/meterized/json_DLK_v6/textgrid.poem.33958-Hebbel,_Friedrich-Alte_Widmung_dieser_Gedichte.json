{"textgrid.poem.33958": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Alte Widmung dieser Gedichte", "genre": "verse", "period": "N.A.", "pub_year": 1841, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du starbst; mir war in meinem Grauen,", "tokens": ["Du", "starbst", ";", "mir", "war", "in", "mei\u00b7nem", "Grau\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VAFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nun h\u00e4tt' ich hier Nichts mehr zu thun,", "tokens": ["Nun", "h\u00e4tt'", "ich", "hier", "Nichts", "mehr", "zu", "thun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PIS", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als dir ein Denkmal noch zu bauen,", "tokens": ["Als", "dir", "ein", "Denk\u00b7mal", "noch", "zu", "bau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Damit du s\u00fc\u00dfer m\u00f6gtest ruh'n;", "tokens": ["Da\u00b7mit", "du", "s\u00fc\u00b7\u00dfer", "m\u00f6g\u00b7test", "ruh'n", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Welt durch meinen Schmerz zu zeigen,", "tokens": ["Der", "Welt", "durch", "mei\u00b7nen", "Schmerz", "zu", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Was du so jung gewesen bist,", "tokens": ["Was", "du", "so", "jung", "ge\u00b7we\u00b7sen", "bist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADJD", "VAPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dann selbst in's Grab hinab zu steigen,", "tokens": ["Dann", "selbst", "in's", "Grab", "hin\u00b7ab", "zu", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "NN", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Das deine ew'ge Wohnung ist.", "tokens": ["Das", "dei\u00b7ne", "ew'\u00b7ge", "Woh\u00b7nung", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Nun schaut' ich still und unverdrossen", "tokens": ["Nun", "schaut'", "ich", "still", "und", "un\u00b7ver\u00b7dros\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "KON", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In meines Herzens Nacht hinab,", "tokens": ["In", "mei\u00b7nes", "Her\u00b7zens", "Nacht", "hin\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Allein mein Schmerz war stumm, verschlossen,", "tokens": ["Al\u00b7lein", "mein", "Schmerz", "war", "stumm", ",", "ver\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VAFIN", "ADJD", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Unfruchtbar war er, wie dein Grab.", "tokens": ["Un\u00b7frucht\u00b7bar", "war", "er", ",", "wie", "dein", "Grab", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "$,", "PWAV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und wenn das Leben, das erstickte,", "tokens": ["Und", "wenn", "das", "Le\u00b7ben", ",", "das", "er\u00b7stick\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "$,", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Zuweilen eine Bl\u00fcte trieb,", "tokens": ["Zu\u00b7wei\u00b7len", "ei\u00b7ne", "Bl\u00fc\u00b7te", "trieb", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So war sie farbig, und ich knickte", "tokens": ["So", "war", "sie", "far\u00b7big", ",", "und", "ich", "knick\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$,", "KON", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Im Keim die meisten, dir zu lieb.", "tokens": ["Im", "Keim", "die", "meis\u00b7ten", ",", "dir", "zu", "lieb", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "VVFIN", "$,", "PPER", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Bald aber ward, ich f\u00fchlt' es schaudernd,", "tokens": ["Bald", "a\u00b7ber", "ward", ",", "ich", "f\u00fchlt'", "es", "schau\u00b7dernd", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "$,", "PPER", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dem stolzen Geist, der dich zu Gott", "tokens": ["Dem", "stol\u00b7zen", "Geist", ",", "der", "dich", "zu", "Gott"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PRF", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hinauf verfolgt, das Herz, zaudernd", "tokens": ["Hin\u00b7auf", "ver\u00b7folgt", ",", "das", "Herz", ",", "zau\u00b7dernd"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ADV", "VVPP", "$,", "ART", "NN", "$,", "VVPP"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Noch bei der Asche weilt, ein Spott.", "tokens": ["Noch", "bei", "der", "A\u00b7sche", "weilt", ",", "ein", "Spott", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er ist nur stumm f\u00fcr mich geworden \u2013", "tokens": ["Er", "ist", "nur", "stumm", "f\u00fcr", "mich", "ge\u00b7wor\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "APPR", "PPER", "VAPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So sprach er \u2013 ich nur blind f\u00fcr ihn,", "tokens": ["So", "sprach", "er", "\u2013", "ich", "nur", "blind", "f\u00fcr", "ihn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$(", "PPER", "ADV", "ADJD", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Doch steh'n wir noch im gleichen Orden,", "tokens": ["Doch", "steh'n", "wir", "noch", "im", "glei\u00b7chen", "Or\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und k\u00f6nnen gleiche Bahnen zieh'n.", "tokens": ["Und", "k\u00f6n\u00b7nen", "glei\u00b7che", "Bah\u00b7nen", "zieh'", "n."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["KON", "VMFIN", "ADJA", "NN", "VVFIN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Nun regten denn sich unaufhaltsam", "tokens": ["Nun", "reg\u00b7ten", "denn", "sich", "un\u00b7auf\u00b7halt\u00b7sam"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "KON", "PRF", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Kr\u00e4fte wieder, wie zuvor,", "tokens": ["Die", "Kr\u00e4f\u00b7te", "wie\u00b7der", ",", "wie", "zu\u00b7vor", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "PWAV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nun rangen st\u00fcrmisch und gewaltsam", "tokens": ["Nun", "ran\u00b7gen", "st\u00fcr\u00b7misch", "und", "ge\u00b7walt\u00b7sam"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sich neue Sch\u00f6pfungen empor.", "tokens": ["Sich", "neu\u00b7e", "Sch\u00f6p\u00b7fun\u00b7gen", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Und Friede ward's auch im Gem\u00fcthe,", "tokens": ["Und", "Frie\u00b7de", "ward's", "auch", "im", "Ge\u00b7m\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das all dies Leben erst bedr\u00e4ngt,", "tokens": ["Das", "all", "dies", "Le\u00b7ben", "erst", "be\u00b7dr\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "PDS", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich seh' ja, da\u00df an jeder Bl\u00fcte", "tokens": ["Ich", "seh'", "ja", ",", "da\u00df", "an", "je\u00b7der", "Bl\u00fc\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOUS", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der Thr\u00e4nenthau des Schmerzes h\u00e4ngt.", "tokens": ["Der", "Thr\u00e4\u00b7nent\u00b7hau", "des", "Schmer\u00b7zes", "h\u00e4ngt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Dir weih' ich Alles. Mag's vergehen,", "tokens": ["Dir", "weih'", "ich", "Al\u00b7les", ".", "Mag's", "ver\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIS", "$.", "NE", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mag's dauern, wie die Zeit erkennt!", "tokens": ["Mag's", "dau\u00b7ern", ",", "wie", "die", "Zeit", "er\u00b7kennt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVINF", "$,", "PWAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mir gilt es gleich. Kann es bestehen,", "tokens": ["Mir", "gilt", "es", "gleich", ".", "Kann", "es", "be\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$.", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So g\u00f6nn' ich's deinem Monument.", "tokens": ["So", "g\u00f6nn'", "ich's", "dei\u00b7nem", "Mo\u00b7nu\u00b7ment", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und wenn's zerst\u00e4ubt \u2013 in deinem Lenze", "tokens": ["Und", "wenn's", "zer\u00b7st\u00e4ubt", "\u2013", "in", "dei\u00b7nem", "Len\u00b7ze"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PIS", "VVPP", "$(", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sah ich dich selbst hinabgesandt,", "tokens": ["Sah", "ich", "dich", "selbst", "hin\u00b7ab\u00b7ge\u00b7sandt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mich kann's nicht schmerzen, wenn die Kr\u00e4nze", "tokens": ["Mich", "kann's", "nicht", "schmer\u00b7zen", ",", "wenn", "die", "Kr\u00e4n\u00b7ze"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Dir folgen, die dein Freund dir wand.", "tokens": ["Dir", "fol\u00b7gen", ",", "die", "dein", "Freund", "dir", "wand", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$,", "PRELS", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}