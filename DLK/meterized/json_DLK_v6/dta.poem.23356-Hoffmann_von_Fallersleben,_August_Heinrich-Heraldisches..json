{"dta.poem.23356": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Heraldisches.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die F\u00fcrsten voller G\u00fct' und Milde,", "tokens": ["Die", "F\u00fcrs\u00b7ten", "vol\u00b7ler", "G\u00fct'", "und", "Mil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was f\u00fchren sie in ihrem Schilde?", "tokens": ["Was", "f\u00fch\u00b7ren", "sie", "in", "ih\u00b7rem", "Schil\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Gemeiniglich ein wildes Thier,", "tokens": ["Ge\u00b7mei\u00b7nig\u00b7lich", "ein", "wil\u00b7des", "Thier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Thier voll Raub- und Mordbegier,", "tokens": ["Ein", "Thier", "voll", "Raub", "und", "Mord\u00b7be\u00b7gier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "TRUNC", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wovon gottlob nichts wei\u00df die Welt,", "tokens": ["Wo\u00b7von", "gott\u00b7lob", "nichts", "wei\u00df", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PIS", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Als da\u00df man es im K\u00e4fich h\u00e4lt.", "tokens": ["Als", "da\u00df", "man", "es", "im", "K\u00e4\u00b7fich", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PIS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch diese Thiere k\u00f6nnten leben,", "tokens": ["Doch", "die\u00b7se", "Thie\u00b7re", "k\u00f6nn\u00b7ten", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Lebendig jeden Thron umgeben \u2014", "tokens": ["Le\u00b7ben\u00b7dig", "je\u00b7den", "Thron", "um\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PIAT", "NN", "VVPP", "$("], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Uns br\u00e4chte weniger Gefahr", "tokens": ["Uns", "br\u00e4ch\u00b7te", "we\u00b7ni\u00b7ger", "Ge\u00b7fahr"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "B\u00e4r, Geier, L\u00f6we, Greif und Aar,", "tokens": ["B\u00e4r", ",", "Gei\u00b7er", ",", "L\u00f6\u00b7we", ",", "Greif", "und", "Aar", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NE", "$,", "NN", "KON", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Als jenes saubre Hofgeschmei\u00df,", "tokens": ["Als", "je\u00b7nes", "saub\u00b7re", "Hof\u00b7ge\u00b7schmei\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wovon die Welt zu viel nur wei\u00df.", "tokens": ["Wo\u00b7von", "die", "Welt", "zu", "viel", "nur", "wei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "PIS", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}