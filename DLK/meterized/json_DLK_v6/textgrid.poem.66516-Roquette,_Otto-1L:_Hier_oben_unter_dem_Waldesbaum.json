{"textgrid.poem.66516": {"metadata": {"author": {"name": "Roquette, Otto", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hier oben unter dem Waldesbaum", "genre": "verse", "period": "N.A.", "pub_year": 1860, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier oben unter dem Waldesbaum", "tokens": ["Hier", "o\u00b7ben", "un\u00b7ter", "dem", "Wal\u00b7des\u00b7baum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Zu Thale schaut man nieder,", "tokens": ["Zu", "Tha\u00b7le", "schaut", "man", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PIS", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und hochhin, wo im Himmelsraum", "tokens": ["Und", "hoch\u00b7hin", ",", "wo", "im", "Him\u00b7mels\u00b7raum"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PTKVZ", "$,", "PWAV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Adler wiegt das Gefieder.", "tokens": ["Der", "Ad\u00b7ler", "wiegt", "das", "Ge\u00b7fie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Du Sonne, gieb noch hellen Schein,", "tokens": ["Du", "Son\u00b7ne", ",", "gieb", "noch", "hel\u00b7len", "Schein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "VVIMP", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Welt will noch nicht schlafen!", "tokens": ["Die", "Welt", "will", "noch", "nicht", "schla\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Dem Schifflein drunten auf dem Rhein,", "tokens": ["Dem", "Schif\u00b7flein", "drun\u00b7ten", "auf", "dem", "Rhein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "ART", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Dem leuchte noch zum Hafen!", "tokens": ["Dem", "leuch\u00b7te", "noch", "zum", "Ha\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Hier oben zwischen Laub und Gras,", "tokens": ["Hier", "o\u00b7ben", "zwi\u00b7schen", "Laub", "und", "Gras", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Kr\u00e4nzen, frisch gewunden,", "tokens": ["Mit", "Kr\u00e4n\u00b7zen", ",", "frisch", "ge\u00b7wun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADJD", "VAPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wie leicht und schnell das Herz genas", "tokens": ["Wie", "leicht", "und", "schnell", "das", "Herz", "ge\u00b7nas"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "KON", "ADJD", "ART", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von schw\u00fclen Tagesstunden!", "tokens": ["Von", "schw\u00fc\u00b7len", "Ta\u00b7gess\u00b7tun\u00b7den", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Du Sonne, schau, dort schreitet im Thal", "tokens": ["Du", "Son\u00b7ne", ",", "schau", ",", "dort", "schrei\u00b7tet", "im", "Thal"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "NN", "$,", "PTKVZ", "$,", "ADV", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Ein m\u00fcder Wandergeselle,", "tokens": ["Ein", "m\u00fc\u00b7der", "Wan\u00b7der\u00b7ge\u00b7sel\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "O leucht' ihm noch mit einem Strahl", "tokens": ["O", "leucht'", "ihm", "noch", "mit", "ei\u00b7nem", "Strahl"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Zu seiner Ruhestelle!", "tokens": ["Zu", "sei\u00b7ner", "Ru\u00b7he\u00b7stel\u00b7le", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wir aber Zwei, wir haben's gut,", "tokens": ["Wir", "a\u00b7ber", "Zwei", ",", "wir", "ha\u00b7ben's", "gut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "CARD", "$,", "PPER", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wir m\u00f6gen fr\u00f6hlich singen,", "tokens": ["Wir", "m\u00f6\u00b7gen", "fr\u00f6h\u00b7lich", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dieweil die Welt bedeckt schon ruht", "tokens": ["Die\u00b7weil", "die", "Welt", "be\u00b7deckt", "schon", "ruht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "VVFIN", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von grauer D\u00e4mmrung Schwingen.", "tokens": ["Von", "grau\u00b7er", "D\u00e4mm\u00b7rung", "Schwin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "O Sonn', hast du dich still und sacht", "tokens": ["O", "Sonn'", ",", "hast", "du", "dich", "still", "und", "sacht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "VAFIN", "PPER", "PRF", "ADJD", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Verh\u00fcllt in blauen Fernen,", "tokens": ["Ver\u00b7h\u00fcllt", "in", "blau\u00b7en", "Fer\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "So singen wir noch der Fr\u00fchlingsnacht", "tokens": ["So", "sin\u00b7gen", "wir", "noch", "der", "Fr\u00fch\u00b7lings\u00b7nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Mit ihren tausend Sternen!", "tokens": ["Mit", "ih\u00b7ren", "tau\u00b7send", "Ster\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "CARD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}