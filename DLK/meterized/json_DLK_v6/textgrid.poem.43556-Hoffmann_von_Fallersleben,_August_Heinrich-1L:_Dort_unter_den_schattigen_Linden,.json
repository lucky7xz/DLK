{"textgrid.poem.43556": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dort unter den schattigen Linden,", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dort unter den schattigen Linden,", "tokens": ["Dort", "un\u00b7ter", "den", "schat\u00b7ti\u00b7gen", "Lin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NE", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Wo frische Blumen bl\u00fch'n,", "tokens": ["Wo", "fri\u00b7sche", "Blu\u00b7men", "bl\u00fch'n", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ruht aus eine gute Mutter", "tokens": ["Ruht", "aus", "ei\u00b7ne", "gu\u00b7te", "Mut\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Von ihres Lebens M\u00fch'n.", "tokens": ["Von", "ih\u00b7res", "Le\u00b7bens", "M\u00fch'", "n."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["APPR", "PPOSAT", "NN", "NN", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sie sieht nicht ihre Kinder,", "tokens": ["Sie", "sieht", "nicht", "ih\u00b7re", "Kin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sie ahnt nicht unsern Schmerz;", "tokens": ["Sie", "ahnt", "nicht", "un\u00b7sern", "Schmerz", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Geschlossen ist ihr Auge,", "tokens": ["Ge\u00b7schlos\u00b7sen", "ist", "ihr", "Au\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und ruhig bleibt ihr Herz.", "tokens": ["Und", "ru\u00b7hig", "bleibt", "ihr", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Mit jedem Fr\u00fchling werden", "tokens": ["Mit", "je\u00b7dem", "Fr\u00fch\u00b7ling", "wer\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VAINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Linden wieder gr\u00fcn,", "tokens": ["Die", "Lin\u00b7den", "wie\u00b7der", "gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und an dem Grabesh\u00fcgel", "tokens": ["Und", "an", "dem", "Gra\u00b7bes\u00b7h\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Blumen wieder bl\u00fch'n.", "tokens": ["Die", "Blu\u00b7men", "wie\u00b7der", "bl\u00fch'", "n."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["ART", "NN", "ADV", "VVFIN", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Dann blicken die Linden und Blumen", "tokens": ["Dann", "bli\u00b7cken", "die", "Lin\u00b7den", "und", "Blu\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NE", "KON", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Gar fr\u00f6hlich himmelw\u00e4rts,", "tokens": ["Gar", "fr\u00f6h\u00b7lich", "him\u00b7mel\u00b7w\u00e4rts", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "$,"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.3": {"text": "Doch unsern Blick beugt nieder", "tokens": ["Doch", "un\u00b7sern", "Blick", "beugt", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PTKVZ"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zum Grabe tief der Schmerz.", "tokens": ["Zum", "Gra\u00b7be", "tief", "der", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}