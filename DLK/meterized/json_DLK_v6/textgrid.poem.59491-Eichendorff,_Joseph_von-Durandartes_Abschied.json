{"textgrid.poem.59491": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Durandartes Abschied", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbdurandarte, Durandarte,", "tokens": ["\u00bb", "du\u00b7rand\u00b7ar\u00b7te", ",", "Du\u00b7rand\u00b7ar\u00b7te", ","], "token_info": ["punct", "word", "punct", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "NN", "$,"], "meter": "-++-+-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Ritterlich in Lust und Streit,", "tokens": ["Rit\u00b7ter\u00b7lich", "in", "Lust", "und", "Streit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bitt dich, la\u00df uns einmal plaudern", "tokens": ["Bitt", "dich", ",", "la\u00df", "uns", "ein\u00b7mal", "plau\u00b7dern"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "VVIMP", "PPER", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wieder von der alten Zeit.", "tokens": ["Wie\u00b7der", "von", "der", "al\u00b7ten", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Denkst du noch der sch\u00f6nen Tage,", "tokens": ["Denkst", "du", "noch", "der", "sch\u00f6\u00b7nen", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo du mir dein Herz geweiht,", "tokens": ["Wo", "du", "mir", "dein", "Herz", "ge\u00b7weiht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und in Sang und Ritterspielen", "tokens": ["Und", "in", "Sang", "und", "Rit\u00b7ter\u00b7spie\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Vor der Welt um mich gefreit?", "tokens": ["Vor", "der", "Welt", "um", "mich", "ge\u00b7freit", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wieviel Mohren warfst du nieder,", "tokens": ["Wie\u00b7viel", "Moh\u00b7ren", "warfst", "du", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Rief ich zum Turniere dich!", "tokens": ["Rief", "ich", "zum", "Tur\u00b7nie\u00b7re", "dich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PPER", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Fast kenn ich dich jetzt nicht wieder,", "tokens": ["Fast", "kenn", "ich", "dich", "jetzt", "nicht", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "PTKNEG", "ADV", "$,"], "meter": "++--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Sag, warum verga\u00dft du mich?\u00ab \u2013", "tokens": ["Sag", ",", "wa\u00b7rum", "ver\u00b7ga\u00dft", "du", "mich", "?", "\u00ab", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NN", "$,", "PWAV", "VVFIN", "PPER", "PRF", "$.", "$(", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "\u00bbschmeichelnd klingen solche Worte", "tokens": ["\u00bb", "schmei\u00b7chelnd", "klin\u00b7gen", "sol\u00b7che", "Wor\u00b7te"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "ADJD", "VVFIN", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und verlockend ist die Huld,", "tokens": ["Und", "ver\u00b7lo\u00b7ckend", "ist", "die", "Huld", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber wenn mein Herz sich wandte,", "tokens": ["A\u00b7ber", "wenn", "mein", "Herz", "sich", "wand\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Euer, Dame, ist die Schuld.", "tokens": ["Eu\u00b7er", ",", "Da\u00b7me", ",", "ist", "die", "Schuld", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "$,", "NN", "$,", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wohl wei\u00df ich's, f\u00fcr Gaiferos", "tokens": ["Wohl", "wei\u00df", "ich's", ",", "f\u00fcr", "Gai\u00b7fe\u00b7ros"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "$,", "APPR", "NE"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Waret Ihr in Lieb entbrannt,", "tokens": ["Wa\u00b7ret", "Ihr", "in", "Lieb", "ent\u00b7brannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Als ich trostlos und ge\u00e4chtet", "tokens": ["Als", "ich", "trost\u00b7los", "und", "ge\u00e4ch\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "KON", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Irrte fern im fremden Land.", "tokens": ["Irr\u00b7te", "fern", "im", "frem\u00b7den", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Drum, wenn Ihr von Lieb jetzt redet,", "tokens": ["Drum", ",", "wenn", "Ihr", "von", "Lieb", "jetzt", "re\u00b7det", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "KOUS", "PPER", "APPR", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Habt Ihr's weislich nicht bedacht,", "tokens": ["Habt", "Ih\u00b7r's", "weis\u00b7lich", "nicht", "be\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADV", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn um nicht die Schmach zu tragen,", "tokens": ["Denn", "um", "nicht", "die", "Schmach", "zu", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PTKNEG", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Wend ich mich in Todesnacht.\u00ab", "tokens": ["Wend", "ich", "mich", "in", "To\u00b7des\u00b7nacht", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}