{"dta.poem.11054": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Unrichtige  \n Anwendung der Vernunft.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Aus allen Dingen scheinet deutlich, es wolle Gott von uns,\nauf Erden,", "tokens": ["Aus", "al\u00b7len", "Din\u00b7gen", "schei\u00b7net", "deut\u00b7lich", ",", "es", "wol\u00b7le", "Gott", "von", "uns", ",", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "ADJD", "$,", "PPER", "VMFIN", "NN", "APPR", "PPER", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+--+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "In seinem Wesen ja so wenig, als seinem Werk, begriffen werden,", "tokens": ["In", "sei\u00b7nem", "We\u00b7sen", "ja", "so", "we\u00b7nig", ",", "als", "sei\u00b7nem", "Werk", ",", "be\u00b7grif\u00b7fen", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "ADV", "PIS", "$,", "KOUS", "PPOSAT", "NN", "$,", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+--+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.3": {"text": "Und da\u00df wir solch ein Ma\u00df von Geist, und nur so viel Ver-", "tokens": ["Und", "da\u00df", "wir", "solch", "ein", "Ma\u00df", "von", "Geist", ",", "und", "nur", "so", "viel", "Ver"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "PIAT", "ART", "NN", "APPR", "NN", "$,", "KON", "ADV", "ADV", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als wie, zum ", "tokens": ["Als", "wie", ",", "zum"], "token_info": ["word", "word", "punct", "word"], "pos": ["KOUS", "KOKOM", "$,", "APPRART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Ja, da\u00df vermuthlich das ", "tokens": ["Ja", ",", "da\u00df", "ver\u00b7muth\u00b7lich", "das"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "KOUS", "ADV", "ART"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "In einem andern Zustand uns, und in der selgen Ewigkeit,", "tokens": ["In", "ei\u00b7nem", "an\u00b7dern", "Zu\u00b7stand", "uns", ",", "und", "in", "der", "sel\u00b7gen", "E\u00b7wig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PPER", "$,", "KON", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.7": {"text": "Vermuthlich vorbehalten seyn. Weil wir nun aber hier allein,", "tokens": ["Ver\u00b7muth\u00b7lich", "vor\u00b7be\u00b7hal\u00b7ten", "seyn", ".", "Weil", "wir", "nun", "a\u00b7ber", "hier", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "VAINF", "$.", "KOUS", "PPER", "ADV", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.8": {"text": "Statt unsrer Pflicht, mit blossem Gr\u00fcbeln, am heftigsten be-", "tokens": ["Statt", "uns\u00b7rer", "Pflicht", ",", "mit", "blos\u00b7sem", "Gr\u00fc\u00b7beln", ",", "am", "hef\u00b7tigs\u00b7ten", "be"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "$,", "APPR", "ADJA", "NN", "$,", "APPRART", "ADJA", "TRUNC"], "meter": "-+-+-+-+--+---", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "So ist der wahre Grund ja klar: Woher, mit stetem Dispu-", "tokens": ["So", "ist", "der", "wah\u00b7re", "Grund", "ja", "klar", ":", "Wo\u00b7her", ",", "mit", "ste\u00b7tem", "Dis\u00b7pu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "ADV", "ADJD", "$.", "PWAV", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.10": {"text": "Durch blossen Stolz dazu verf\u00fchrt, wir Zeit, und uns und", "tokens": ["Durch", "blos\u00b7sen", "Stolz", "da\u00b7zu", "ver\u00b7f\u00fchrt", ",", "wir", "Zeit", ",", "und", "uns", "und"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "PAV", "VVPP", "$,", "PPER", "NN", "$,", "KON", "PPER", "KON"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Da wir, wenn wir von unsern Pflichten uns nicht so ungl\u00fcck-", "tokens": ["Da", "wir", ",", "wenn", "wir", "von", "un\u00b7sern", "Pflich\u00b7ten", "uns", "nicht", "so", "un\u00b7gl\u00fc\u00b7ck"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "KOUS", "PPER", "APPR", "PPOSAT", "NN", "PPER", "PTKNEG", "ADV", "TRUNC"], "meter": "-+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Wir unsre Zeit hier wohl verbringen, uns freuen und Gott", "tokens": ["Wir", "uns\u00b7re", "Zeit", "hier", "wohl", "ver\u00b7brin\u00b7gen", ",", "uns", "freu\u00b7en", "und", "Gott"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "PPOSAT", "NN", "ADV", "ADV", "VVINF", "$,", "PPER", "VVINF", "KON", "NN"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}}}}}