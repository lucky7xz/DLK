{"textgrid.poem.40435": {"metadata": {"author": {"name": "Scheffel, Joseph Viktor von", "birth": "N.A.", "death": "N.A."}, "title": "Der letzte Postillon", "genre": "verse", "period": "N.A.", "pub_year": 1856, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bald ist, soweit die Menschheit haust,", "tokens": ["Bald", "ist", ",", "so\u00b7weit", "die", "Menschheit", "haust", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Schienenweg gespannt;", "tokens": ["Der", "Schie\u00b7nen\u00b7weg", "ge\u00b7spannt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es keucht und schnaubt und stampft und saust", "tokens": ["Es", "keucht", "und", "schnaubt", "und", "stampft", "und", "saust"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Dampfro\u00df rings durchs Land.", "tokens": ["Das", "Dam\u00b7pfro\u00df", "rings", "durchs", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und wiedrum in f\u00fcnfhundert Jahr", "tokens": ["Und", "wie\u00b7drum", "in", "f\u00fcnf\u00b7hun\u00b7dert", "Jahr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "CARD", "NN"], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wei\u00df der Gelahrteste nicht", "tokens": ["Wei\u00df", "der", "Ge\u00b7lahr\u00b7tes\u00b7te", "nicht"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "PTKNEG"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Zu sagen, was ein Hauderer war,", "tokens": ["Zu", "sa\u00b7gen", ",", "was", "ein", "Hau\u00b7de\u00b7rer", "war", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PRELS", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Was Fuhrmanns Recht und Pflicht.", "tokens": ["Was", "Fuhr\u00b7manns", "Recht", "und", "Pflicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NE", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Nur in der Nacht der Sonnenwend',", "tokens": ["Nur", "in", "der", "Nacht", "der", "Son\u00b7nen\u00b7wend'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo dunkle Schemen gehn,", "tokens": ["Wo", "dunk\u00b7le", "Sche\u00b7men", "gehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wird zwischen Erd' und Firmament", "tokens": ["Wird", "zwi\u00b7schen", "Erd'", "und", "Fir\u00b7ma\u00b7ment"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein fremd Gespann gesehn.", "tokens": ["Ein", "fremd", "Ge\u00b7spann", "ge\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Der Schimmel trabt, die Peitsche schwirrt,", "tokens": ["Der", "Schim\u00b7mel", "trabt", ",", "die", "Peit\u00b7sche", "schwirrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Laut schmettert Posthornton,", "tokens": ["Laut", "schmet\u00b7tert", "Post\u00b7horn\u00b7ton", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als Geist kommt durch die Luft kutschiert", "tokens": ["Als", "Geist", "kommt", "durch", "die", "Luft", "kut\u00b7schiert"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "VVFIN", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein greiser Postillon.", "tokens": ["Ein", "grei\u00b7ser", "Pos\u00b7til\u00b7lon", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Fahl gl\u00e4nzt am gelben Sperlingsfrack", "tokens": ["Fahl", "gl\u00e4nzt", "am", "gel\u00b7ben", "Sper\u00b7lings\u00b7frack"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Thurn Taxis' Wappenknopf,", "tokens": ["Thurn", "Ta\u00b7xis'", "Wap\u00b7pen\u00b7knopf", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er raucht uralten Rauchtabak", "tokens": ["Er", "raucht", "ur\u00b7al\u00b7ten", "Rauch\u00b7ta\u00b7bak"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aus braunem Ulmerkopf.", "tokens": ["Aus", "brau\u00b7nem", "Ul\u00b7mer\u00b7kopf", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Er raucht und spricht: \u00bbO Erdenball,", "tokens": ["Er", "raucht", "und", "spricht", ":", "\u00bb", "O", "Er\u00b7den\u00b7ball", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$.", "$(", "NE", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie anders schaust du drein,", "tokens": ["Wie", "an\u00b7ders", "schaust", "du", "drein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Seit ich mit Sang und Peitschenknall", "tokens": ["Seit", "ich", "mit", "Sang", "und", "Peit\u00b7schen\u00b7knall"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Reichspostdienst tat am Rhein!", "tokens": ["Reichs\u00b7post\u00b7dienst", "tat", "am", "Rhein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NE", "$."], "meter": "---+-+", "measure": "unknown.measure.di"}}, "stanza.7": {"line.1": {"text": "O Zeit des Pa\u00dfgangs und des Trabs,", "tokens": ["O", "Zeit", "des", "Pa\u00df\u00b7gangs", "und", "des", "Trabs", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Trinkgelds und des Trunks,", "tokens": ["Des", "Trink\u00b7gelds", "und", "des", "Trunks", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Des Poststalls und des Wanderstabs,", "tokens": ["Des", "Post\u00b7stalls", "und", "des", "Wan\u00b7der\u00b7stabs", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des idealen Schwungs!", "tokens": ["Des", "i\u00b7dea\u00b7len", "Schwungs", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Jetzt geht die Welt aus Rand und Band,", "tokens": ["Jetzt", "geht", "die", "Welt", "aus", "Rand", "und", "Band", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Besten ziehn davon,", "tokens": ["Die", "Bes\u00b7ten", "ziehn", "da\u00b7von", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PAV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und mit dem letzten Hausknecht schwand", "tokens": ["Und", "mit", "dem", "letz\u00b7ten", "Haus\u00b7knecht", "schwand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der letzte Postillon.", "tokens": ["Der", "letz\u00b7te", "Pos\u00b7til\u00b7lon", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Jetzt rennt der Dampf, jetzt brennt der Wind,", "tokens": ["Jetzt", "rennt", "der", "Dampf", ",", "jetzt", "brennt", "der", "Wind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Jetzt gilt kein Fruh und Spat,", "tokens": ["Jetzt", "gilt", "kein", "Fruh", "und", "Spat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Sonne malt und blitzgeschwind", "tokens": ["Die", "Son\u00b7ne", "malt", "und", "blitz\u00b7ge\u00b7schwind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Briefschreibt der Kupferdraht.", "tokens": ["Brief\u00b7schreibt", "der", "Kup\u00b7fer\u00b7draht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.10": {"line.1": {"text": "O neues R\u00fcstzeug, alter Kampf!", "tokens": ["O", "neu\u00b7es", "R\u00fcst\u00b7zeug", ",", "al\u00b7ter", "Kampf", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo treff' ich Gl\u00fcck und Ruh'?...", "tokens": ["Wo", "treff'", "ich", "Gl\u00fcck", "und", "Ruh'", "?", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "O Erdenphosphor, Gas und Dampf!", "tokens": ["O", "Er\u00b7den\u00b7phos\u00b7phor", ",", "Gas", "und", "Dampf", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Fahr' zu, mein Schimmel, fahr' zu!\u00ab", "tokens": ["Fahr'", "zu", ",", "mein", "Schim\u00b7mel", ",", "fahr'", "zu", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["NE", "PTKVZ", "$,", "PPOSAT", "NN", "$,", "VVFIN", "PTKVZ", "$.", "$("], "meter": "+--+--+", "measure": "dactylic.tri"}}}}}