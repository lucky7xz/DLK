{"textgrid.poem.48002": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Genug der Qualen!", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich ging mit dir durch alles Elends Tiefen,", "tokens": ["Ich", "ging", "mit", "dir", "durch", "al\u00b7les", "E\u00b7lends", "Tie\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "APPR", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "geknechtet Volk, durch einen Pfuhl der Schmach;", "tokens": ["ge\u00b7knech\u00b7tet", "Volk", ",", "durch", "ei\u00b7nen", "Pfuhl", "der", "Schmach", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "NN", "$,", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "die Stimmen h\u00f6rt' ich, die nach Freiheit riefen,", "tokens": ["die", "Stim\u00b7men", "h\u00f6rt'", "ich", ",", "die", "nach", "Frei\u00b7heit", "rie\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "PRELS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "und meine Seele hallte zitternd nach.", "tokens": ["und", "mei\u00b7ne", "See\u00b7le", "hall\u00b7te", "zit\u00b7ternd", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ich schlief mit dir in deiner Armut H\u00fctten,", "tokens": ["Ich", "schlief", "mit", "dir", "in", "dei\u00b7ner", "Ar\u00b7mut", "H\u00fct\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "in die kein Mondlicht mild verkl\u00e4rend scheint,", "tokens": ["in", "die", "kein", "Mond\u00b7licht", "mild", "ver\u00b7kl\u00e4\u00b7rend", "scheint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIAT", "NN", "ADJD", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "all deinen Jammer hab' ich durchgelitten,", "tokens": ["all", "dei\u00b7nen", "Jam\u00b7mer", "hab'", "ich", "durch\u00b7ge\u00b7lit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "all deine Tr\u00e4nen hab' ich mitgeweint!", "tokens": ["all", "dei\u00b7ne", "Tr\u00e4\u00b7nen", "hab'", "ich", "mit\u00b7ge\u00b7weint", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich frohnt' wie du dem Sausen der Maschine", "tokens": ["Ich", "frohnt'", "wie", "du", "dem", "Sau\u00b7sen", "der", "Ma\u00b7schi\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOKOM", "PPER", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "im grauen Tagewerk voll Staub und Dunst;", "tokens": ["im", "grau\u00b7en", "Ta\u00b7ge\u00b7werk", "voll", "Staub", "und", "Dunst", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADJD", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "mit deinen T\u00f6chtern ging ich, da\u00df ich diene, \u2013", "tokens": ["mit", "dei\u00b7nen", "T\u00f6ch\u00b7tern", "ging", "ich", ",", "da\u00df", "ich", "die\u00b7ne", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PDS", "$,", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "um trocken Brot verkauft' ich Geist und Gunst!", "tokens": ["um", "tro\u00b7cken", "Brot", "ver\u00b7kauft'", "ich", "Geist", "und", "Gunst", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJD", "NN", "VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ich ballt' die Faust \u2013 und doch: das Joch zu tragen,", "tokens": ["Ich", "ballt'", "die", "Faust", "\u2013", "und", "doch", ":", "das", "Joch", "zu", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$(", "KON", "ADV", "$.", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "beugt' ich die Stirn vor des Gesetzes Fluch \u2013", "tokens": ["beugt'", "ich", "die", "Stirn", "vor", "des", "Ge\u00b7set\u00b7zes", "Fluch", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "und deine Z\u00e4hne h\u00f6rt' ich knirschend schlagen", "tokens": ["und", "dei\u00b7ne", "Z\u00e4h\u00b7ne", "h\u00f6rt'", "ich", "knir\u00b7schend", "schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "und knirscht mit dir ein trotziges: \u00bbGenug!\u00ab", "tokens": ["und", "knirscht", "mit", "dir", "ein", "trot\u00b7zi\u00b7ges", ":", "\u00bb", "Ge\u00b7nug", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "ART", "ADJA", "$.", "$(", "ADV", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Genug des Knechttums und genug der Qualen!", "tokens": ["Ge\u00b7nug", "des", "Knecht\u00b7tums", "und", "ge\u00b7nug", "der", "Qua\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "KON", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Gott des Zorns, den deine Sehnsucht tr\u00e4umt,", "tokens": ["Der", "Gott", "des", "Zorns", ",", "den", "dei\u00b7ne", "Sehn\u00b7sucht", "tr\u00e4umt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "geht durch die Welt. \u2013 Und wenn aus seinen Schalen", "tokens": ["geht", "durch", "die", "Welt", ".", "\u2013", "Und", "wenn", "aus", "sei\u00b7nen", "Scha\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "$.", "$(", "KON", "KOUS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "der erste Tropfen brausend \u00fcbersch\u00e4umt,", "tokens": ["der", "ers\u00b7te", "Trop\u00b7fen", "brau\u00b7send", "\u00fc\u00b7bersc\u00b7h\u00e4umt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "dann weh dem G\u00f6tzen, der auf ehrnen Achsen", "tokens": ["dann", "weh", "dem", "G\u00f6t\u00b7zen", ",", "der", "auf", "ehr\u00b7nen", "Ach\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "das Feld zerstampft, von deinem Schwei\u00df betr\u00e4uft:", "tokens": ["das", "Feld", "zer\u00b7stampft", ",", "von", "dei\u00b7nem", "Schwei\u00df", "be\u00b7tr\u00e4uft", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "aus deinen Tr\u00e4nen wird die Sturmflut wachsen,", "tokens": ["aus", "dei\u00b7nen", "Tr\u00e4\u00b7nen", "wird", "die", "Sturm\u00b7flut", "wach\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "die seine goldne Herrlichkeit ers\u00e4uft!", "tokens": ["die", "sei\u00b7ne", "gold\u00b7ne", "Herr\u00b7lich\u00b7keit", "er\u00b7s\u00e4uft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Dann aus den Himmeln f\u00e4llt der Wahrheit Feuer", "tokens": ["Dann", "aus", "den", "Him\u00b7meln", "f\u00e4llt", "der", "Wahr\u00b7heit", "Feu\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "in deine Nacht, das einst Prometheus stahl \u2013", "tokens": ["in", "dei\u00b7ne", "Nacht", ",", "das", "einst", "Pro\u00b7me\u00b7theus", "stahl", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PRELS", "ADV", "NE", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "an ihrem Brand entz\u00fcndet sich ein neuer:", "tokens": ["an", "ih\u00b7rem", "Brand", "ent\u00b7z\u00fcn\u00b7det", "sich", "ein", "neu\u00b7er", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PRF", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "der Welterl\u00f6sung leuchtend Flammenmal!", "tokens": ["der", "Wel\u00b7ter\u00b7l\u00f6\u00b7sung", "leuch\u00b7tend", "Flam\u00b7men\u00b7mal", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Lichttrunken will ich dann die Arme heben", "tokens": ["Licht\u00b7trun\u00b7ken", "will", "ich", "dann", "die", "Ar\u00b7me", "he\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "PPER", "ADV", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "und jauchzen in den gl\u00fchen Glanz hinein \u2013", "tokens": ["und", "jauch\u00b7zen", "in", "den", "gl\u00fc\u00b7hen", "Glanz", "hin\u00b7ein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "und wenn des Liedes Gabe mir gegeben,", "tokens": ["und", "wenn", "des", "Lie\u00b7des", "Ga\u00b7be", "mir", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "la\u00df mich die Stimme deiner Freiheit sein!", "tokens": ["la\u00df", "mich", "die", "Stim\u00b7me", "dei\u00b7ner", "Frei\u00b7heit", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "PPOSAT", "NN", "VAINF", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}}}}