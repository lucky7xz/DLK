{"textgrid.poem.60995": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Der arme Taugenichts", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich kann wahrhaftig doch nichts daf\u00fcr,", "tokens": ["Ich", "kann", "wahr\u00b7haf\u00b7tig", "doch", "nichts", "da\u00b7f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "ADV", "PIS", "PAV", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da\u00df schief mir die Nas' im Gesichte steht,", "tokens": ["Da\u00df", "schief", "mir", "die", "Nas'", "im", "Ge\u00b7sich\u00b7te", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Und da\u00df sich's leichter zur Schenkent\u00fcr", "tokens": ["Und", "da\u00df", "sich's", "leich\u00b7ter", "zur", "Schen\u00b7ken\u00b7t\u00fcr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PIS", "ADJD", "APPRART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Als hinter dem Pflug auf dem Felde geht,", "tokens": ["Als", "hin\u00b7ter", "dem", "Pflug", "auf", "dem", "Fel\u00b7de", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Und da\u00df mir besser des M\u00fcllers Kind", "tokens": ["Und", "da\u00df", "mir", "bes\u00b7ser", "des", "M\u00fcl\u00b7lers", "Kind"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADJD", "ART", "NN", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.6": {"text": "Als unser dicker Herr Pfarrer gef\u00e4llt -", "tokens": ["Als", "un\u00b7ser", "di\u00b7cker", "Herr", "Pfar\u00b7rer", "ge\u00b7f\u00e4llt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "NN", "VVPP", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Ich aber predige in den Wind;", "tokens": ["Ich", "a\u00b7ber", "pre\u00b7di\u00b7ge", "in", "den", "Wind", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Denn nimmer begreift mich die arge Welt.", "tokens": ["Denn", "nim\u00b7mer", "be\u00b7greift", "mich", "die", "ar\u00b7ge", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Der M\u00fcller, der ist euch ein grimmer Kumpan!", "tokens": ["Der", "M\u00fcl\u00b7ler", ",", "der", "ist", "euch", "ein", "grim\u00b7mer", "Kum\u00b7pan", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$,", "PRELS", "VAFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Er sagt, ich w\u00e4re ein Taugenichts,", "tokens": ["Er", "sagt", ",", "ich", "w\u00e4\u00b7re", "ein", "Tau\u00b7ge\u00b7nichts", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und die Leute im Dorfe glauben daran,", "tokens": ["Und", "die", "Leu\u00b7te", "im", "Dor\u00b7fe", "glau\u00b7ben", "da\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPRART", "NN", "VVFIN", "PAV", "$,"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Und auch sein rosiges T\u00f6chterlein spricht's.", "tokens": ["Und", "auch", "sein", "ro\u00b7si\u00b7ges", "T\u00f6ch\u00b7ter\u00b7lein", "spricht'", "s."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["KON", "ADV", "PPOSAT", "ADJA", "NN", "VVFIN", "NE"], "meter": "---+--+--+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Und wenn sie mich sieht am M\u00fchlbach stehn,", "tokens": ["Und", "wenn", "sie", "mich", "sieht", "am", "M\u00fchl\u00b7bach", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "VVFIN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Da r\u00fcmpft sie das N\u00e4schen und zieht ein Gesicht,", "tokens": ["Da", "r\u00fcmpft", "sie", "das", "N\u00e4\u00b7schen", "und", "zieht", "ein", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "Und wei\u00df doch so zierlich dabei sich zu drehn,", "tokens": ["Und", "wei\u00df", "doch", "so", "zier\u00b7lich", "da\u00b7bei", "sich", "zu", "drehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "ADJD", "PAV", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.8": {"text": "Da\u00df vor \u00c4rger und Liebe das Herz mir bricht.", "tokens": ["Da\u00df", "vor", "\u00c4r\u00b7ger", "und", "Lie\u00b7be", "das", "Herz", "mir", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "KON", "NN", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}}, "stanza.3": {"line.1": {"text": "Nun klag' ich mein Leid den B\u00e4umen da drau\u00df,", "tokens": ["Nun", "klag'", "ich", "mein", "Leid", "den", "B\u00e4u\u00b7men", "da", "drau\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "ART", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Doch sie bleiben so stumm, doch sie bleiben so starr,", "tokens": ["Doch", "sie", "blei\u00b7ben", "so", "stumm", ",", "doch", "sie", "blei\u00b7ben", "so", "starr", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ADJD", "$,", "KON", "PPER", "VVFIN", "ADV", "ADJD", "$,"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.3": {"text": "Und Kuckuck und Gimpel pfeifen mich aus,", "tokens": ["Und", "Ku\u00b7ckuck", "und", "Gim\u00b7pel", "pfei\u00b7fen", "mich", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und die K\u00e4fer summen: du Narr! du Narr!", "tokens": ["Und", "die", "K\u00e4\u00b7fer", "sum\u00b7men", ":", "du", "Narr", "!", "du", "Narr", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVINF", "$.", "NE", "NE", "$.", "NE", "NE", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und wird das nicht anders, und kommt's nicht bald,", "tokens": ["Und", "wird", "das", "nicht", "an\u00b7ders", ",", "und", "kommt's", "nicht", "bald", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PDS", "PTKNEG", "ADV", "$,", "KON", "VVFIN", "PTKNEG", "ADV", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "So halt' ich's im Dorfe nimmermehr aus;", "tokens": ["So", "halt'", "ich's", "im", "Dor\u00b7fe", "nim\u00b7mer\u00b7mehr", "aus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPRART", "NN", "ADV", "PTKVZ", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Da zieh' ich davon durch den gro\u00dfen Wald", "tokens": ["Da", "zieh'", "ich", "da\u00b7von", "durch", "den", "gro\u00b7\u00dfen", "Wald"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PAV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und streiche die Fiedel von Haus zu Haus.", "tokens": ["Und", "strei\u00b7che", "die", "Fie\u00b7del", "von", "Haus", "zu", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "NN", "APPR", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}}}}