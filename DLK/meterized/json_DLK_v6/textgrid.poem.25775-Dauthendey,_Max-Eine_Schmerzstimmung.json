{"textgrid.poem.25775": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Eine Schmerzstimmung", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist eine starre frostige Ebene,", "tokens": ["Es", "ist", "ei\u00b7ne", "star\u00b7re", "fros\u00b7ti\u00b7ge", "E\u00b7be\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+--+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Tiefes graues Gew\u00f6lk,", "tokens": ["Tie\u00b7fes", "grau\u00b7es", "Ge\u00b7w\u00f6lk", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Lautlose schwarze V\u00f6gel", "tokens": ["Laut\u00b7lo\u00b7se", "schwar\u00b7ze", "V\u00f6\u00b7gel"], "token_info": ["word", "word", "word"], "pos": ["NE", "ADJA", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "In flachem Flug.", "tokens": ["In", "fla\u00b7chem", "Flug", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Und zwischen dem Himmel", "tokens": ["Und", "zwi\u00b7schen", "dem", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Und zwischen dem Erdrand", "tokens": ["Und", "zwi\u00b7schen", "dem", "Er\u00b7drand"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "-+---+", "measure": "dactylic.init"}, "line.7": {"text": "Ein blasser hilfloser Strahl,", "tokens": ["Ein", "blas\u00b7ser", "hilf\u00b7lo\u00b7ser", "Strahl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Liegt einsam an der Erde,", "tokens": ["Liegt", "ein\u00b7sam", "an", "der", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Einsam am Himmel.", "tokens": ["Ein\u00b7sam", "am", "Him\u00b7mel", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Es ist eine blutleere Hand,", "tokens": ["Es", "ist", "ei\u00b7ne", "blut\u00b7lee\u00b7re", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Bla\u00df ausgestreckt,", "tokens": ["Bla\u00df", "aus\u00b7ge\u00b7streckt", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Mit d\u00fcnnem, mattgr\u00fcnem Ge\u00e4der,", "tokens": ["Mit", "d\u00fcn\u00b7nem", ",", "matt\u00b7gr\u00fc\u00b7nem", "Ge\u00b7\u00e4\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und zitternd gereckten, blauen kranken Adern.", "tokens": ["Und", "zit\u00b7ternd", "ge\u00b7reck\u00b7ten", ",", "blau\u00b7en", "kran\u00b7ken", "A\u00b7dern", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "$,", "ADJA", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Und die graue leere Hand", "tokens": ["Und", "die", "grau\u00b7e", "lee\u00b7re", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Liegt hungernd ge\u00f6ffnet.", "tokens": ["Liegt", "hun\u00b7gernd", "ge\u00b7\u00f6ff\u00b7net", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "VVPP", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Es ist das erstickte Auge einer Leiche,", "tokens": ["Es", "ist", "das", "er\u00b7stick\u00b7te", "Au\u00b7ge", "ei\u00b7ner", "Lei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Blauwei\u00df in stechender Steilheit,", "tokens": ["Blau\u00b7wei\u00df", "in", "ste\u00b7chen\u00b7der", "Steil\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Grell unter halbge\u00f6ffnetem Lid", "tokens": ["Grell", "un\u00b7ter", "halb\u00b7ge\u00b7\u00f6ff\u00b7ne\u00b7tem", "Lid"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Ein erw\u00fcrgter aufschreiender Blick.", "tokens": ["Ein", "er\u00b7w\u00fcrg\u00b7ter", "auf\u00b7schrei\u00b7en\u00b7der", "Blick", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Und es ist von der Leiche", "tokens": ["Und", "es", "ist", "von", "der", "Lei\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "APPR", "ART", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Noch der blaue gekrampfte Mund,", "tokens": ["Noch", "der", "blau\u00b7e", "ge\u00b7krampf\u00b7te", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Mit den schweren harten Lippen,", "tokens": ["Mit", "den", "schwe\u00b7ren", "har\u00b7ten", "Lip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und dem schweren harten Schweigen.", "tokens": ["Und", "dem", "schwe\u00b7ren", "har\u00b7ten", "Schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Aber von T\u00f6nen ist es kein Akkord, und kein Laut,", "tokens": ["A\u00b7ber", "von", "T\u00f6\u00b7nen", "ist", "es", "kein", "Ak\u00b7kord", ",", "und", "kein", "Laut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VAFIN", "PPER", "PIAT", "NN", "$,", "KON", "PIAT", "NN", "$,"], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Es ist die vibrierende Fieberstille zwischen zwei Lauten.", "tokens": ["Es", "ist", "die", "vi\u00b7brie\u00b7ren\u00b7de", "Fie\u00b7ber\u00b7stil\u00b7le", "zwi\u00b7schen", "zwei", "Lau\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "APPR", "CARD", "NN", "$."], "meter": "-+--+--+-+-+--+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Und von Ger\u00fcchen ist es", "tokens": ["Und", "von", "Ge\u00b7r\u00fc\u00b7chen", "ist", "es"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "VAFIN", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "der schluchzende Duft", "tokens": ["der", "schluch\u00b7zen\u00b7de", "Duft"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "nasser schwarzer Erde.", "tokens": ["nas\u00b7ser", "schwar\u00b7zer", "Er\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Und von Farben:", "tokens": ["Und", "von", "Far\u00b7ben", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Das geronnene Rot", "tokens": ["Das", "ge\u00b7ron\u00b7ne\u00b7ne", "Rot"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.8": {"text": "und das flehende Bla\u00df", "tokens": ["und", "das", "fle\u00b7hen\u00b7de", "Bla\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.9": {"text": "scharfer, verwester Rosen.", "tokens": ["schar\u00b7fer", ",", "ver\u00b7wes\u00b7ter", "Ro\u00b7sen", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJA", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}}}}