{"textgrid.poem.37521": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Erstes Kapitel", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie der Wind in Trauerweiden", "tokens": ["Wie", "der", "Wind", "in", "Trau\u00b7er\u00b7wei\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "T\u00f6nt des frommen S\u00e4ngers Lied,", "tokens": ["T\u00f6nt", "des", "from\u00b7men", "S\u00e4n\u00b7gers", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn er auf die Lasterfreuden", "tokens": ["Wenn", "er", "auf", "die", "Las\u00b7ter\u00b7freu\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In den gro\u00dfen St\u00e4dten sieht.", "tokens": ["In", "den", "gro\u00b7\u00dfen", "St\u00e4d\u00b7ten", "sieht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ach, die sittenlose Presse!", "tokens": ["Ach", ",", "die", "sit\u00b7ten\u00b7lo\u00b7se", "Pres\u00b7se", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tut sie nicht in fr\u00fcher Stund", "tokens": ["Tut", "sie", "nicht", "in", "fr\u00fc\u00b7her", "Stund"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "PTKNEG", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "All die s\u00fcndlichen Exzesse", "tokens": ["All", "die", "s\u00fcnd\u00b7li\u00b7chen", "Ex\u00b7zes\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.4": {"text": "Schon den B\u00fcrgersleuten kund?!", "tokens": ["Schon", "den", "B\u00fcr\u00b7gers\u00b7leu\u00b7ten", "kund", "?!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Offenbach ist im Thalia,", "tokens": ["Of\u00b7fen\u00b7bach", "ist", "im", "Tha\u00b7lia", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPRART", "NE", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Hier sind B\u00e4lle, da Konzerts.", "tokens": ["Hier", "sind", "B\u00e4l\u00b7le", ",", "da", "Kon\u00b7zerts", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "$,", "KOUS", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Annchen, Hannchen und Maria", "tokens": ["Ann\u00b7chen", ",", "Hann\u00b7chen", "und", "Ma\u00b7ria"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "NE", "KON", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00fcpft vor Freuden schon das Herz.", "tokens": ["H\u00fcpft", "vor", "Freu\u00b7den", "schon", "das", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Kaum trank man die letzte Tasse,", "tokens": ["Kaum", "trank", "man", "die", "letz\u00b7te", "Tas\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Putzt man schon den ird'schen Leib.", "tokens": ["Putzt", "man", "schon", "den", "ird'\u00b7schen", "Leib", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf dem Walle, auf der Gasse", "tokens": ["Auf", "dem", "Wal\u00b7le", ",", "auf", "der", "Gas\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wimmelt man zum Zeitvertreib.", "tokens": ["Wim\u00b7melt", "man", "zum", "Zeit\u00b7ver\u00b7treib", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Und der Jud mit krummer Ferse,", "tokens": ["Und", "der", "Jud", "mit", "krum\u00b7mer", "Fer\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Krummer Nas' und krummer Hos'", "tokens": ["Krum\u00b7mer", "Nas'", "und", "krum\u00b7mer", "Hos'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NE", "KON", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schl\u00e4ngelt sich zur hohen B\u00f6rse", "tokens": ["Schl\u00e4n\u00b7gelt", "sich", "zur", "ho\u00b7hen", "B\u00f6r\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tiefverderbt und seelenlos.", "tokens": ["Tief\u00b7ver\u00b7derbt", "und", "see\u00b7len\u00b7los", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Wie sie schauen, wie sie gr\u00fc\u00dfen!", "tokens": ["Wie", "sie", "schau\u00b7en", ",", "wie", "sie", "gr\u00fc\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVINF", "$,", "PWAV", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hier die zierlichen Mosj\u00f6s,", "tokens": ["Hier", "die", "zier\u00b7li\u00b7chen", "Mos\u00b7j\u00f6s", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dort die Damen mit den s\u00fc\u00dfen", "tokens": ["Dort", "die", "Da\u00b7men", "mit", "den", "s\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Himmlisch hohen Prachtpop\u00f6s.", "tokens": ["Himm\u00b7lisch", "ho\u00b7hen", "Pracht\u00b7po\u00b7p\u00f6s", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Schweigen will ich von Lokalen,", "tokens": ["Schwei\u00b7gen", "will", "ich", "von", "Lo\u00b7ka\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo der B\u00f6se n\u00e4chtlich pra\u00dft,", "tokens": ["Wo", "der", "B\u00f6\u00b7se", "n\u00e4cht\u00b7lich", "pra\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo im Kreis der Liberalen", "tokens": ["Wo", "im", "Kreis", "der", "Li\u00b7be\u00b7ra\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Man den Heil'gen Vater ha\u00dft.", "tokens": ["Man", "den", "Heil'\u00b7gen", "Va\u00b7ter", "ha\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Schweigen will ich von Konzerten,", "tokens": ["Schwei\u00b7gen", "will", "ich", "von", "Kon\u00b7zer\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo der Kenner hoch entz\u00fcckt", "tokens": ["Wo", "der", "Ken\u00b7ner", "hoch", "ent\u00b7z\u00fcckt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADJD", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit dem seelenvoll-verkl\u00e4rten", "tokens": ["Mit", "dem", "see\u00b7len\u00b7voll\u00b7ver\u00b7kl\u00e4r\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Opernglase um sich blickt;", "tokens": ["O\u00b7pern\u00b7gla\u00b7se", "um", "sich", "blickt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PRF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Wo mit weichem Wogebusen", "tokens": ["Wo", "mit", "wei\u00b7chem", "Wo\u00b7ge\u00b7bu\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Man sch\u00f6n warm beisammen sitzt,", "tokens": ["Man", "sch\u00f6n", "warm", "bei\u00b7sam\u00b7men", "sitzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADJD", "ADJD", "VVIZU", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo der hehre Chor der Musen,", "tokens": ["Wo", "der", "heh\u00b7re", "Chor", "der", "Mu\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo Apollo selber schwitzt.", "tokens": ["Wo", "A\u00b7pol\u00b7lo", "sel\u00b7ber", "schwitzt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Schweigen will ich vom Theater,", "tokens": ["Schwei\u00b7gen", "will", "ich", "vom", "The\u00b7a\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie von da, des Abends sp\u00e4t,", "tokens": ["Wie", "von", "da", ",", "des", "A\u00b7bends", "sp\u00e4t", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ADV", "$,", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sch\u00f6ne Mutter, alter Vater", "tokens": ["Sch\u00f6\u00b7ne", "Mut\u00b7ter", ",", "al\u00b7ter", "Va\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Arm in Arm nach Hause geht.", "tokens": ["Arm", "in", "Arm", "nach", "Hau\u00b7se", "geht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Zwar man zeuget viele Kinder,", "tokens": ["Zwar", "man", "zeu\u00b7get", "vie\u00b7le", "Kin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Doch man denket nichts dabei.", "tokens": ["Doch", "man", "den\u00b7ket", "nichts", "da\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PIS", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die Kinder werden S\u00fcnder,", "tokens": ["Und", "die", "Kin\u00b7der", "wer\u00b7den", "S\u00fcn\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn's den Eltern einerlei.", "tokens": ["Wenn's", "den", "El\u00b7tern", "ei\u00b7ner\u00b7lei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "\u00bbkomm Helenchen!\u00ab sprach der brave", "tokens": ["\u00bb", "komm", "He\u00b7len\u00b7chen", "!", "\u00ab", "sprach", "der", "bra\u00b7ve"], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["$(", "VVFIN", "NN", "$.", "$(", "VVFIN", "ART", "ADJA"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Vormund \u2013 \u00bbKomm, mein liebes Kind!", "tokens": ["Vor\u00b7mund", "\u2013", "\u00bb", "Komm", ",", "mein", "lie\u00b7bes", "Kind", "!"], "token_info": ["word", "punct", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$(", "$(", "VVFIN", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Komm aufs Land, wo sanfte Schafe", "tokens": ["Komm", "aufs", "Land", ",", "wo", "sanf\u00b7te", "Scha\u00b7fe"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "NN", "$,", "PWAV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die frommen L\u00e4mmer sind.", "tokens": ["Und", "die", "from\u00b7men", "L\u00e4m\u00b7mer", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Da ist Onkel, da ist Tante,", "tokens": ["Da", "ist", "On\u00b7kel", ",", "da", "ist", "Tan\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "$,", "ADV", "VAFIN", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Da ist Tugend und Verstand,", "tokens": ["Da", "ist", "Tu\u00b7gend", "und", "Ver\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da sind deine Anverwandte!\u00ab", "tokens": ["Da", "sind", "dei\u00b7ne", "An\u00b7ver\u00b7wand\u00b7te", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "So kam Lenchen auf das Land.", "tokens": ["So", "kam", "Len\u00b7chen", "auf", "das", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}