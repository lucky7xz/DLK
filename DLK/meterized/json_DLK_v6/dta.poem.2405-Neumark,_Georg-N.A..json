{"dta.poem.2405": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1652", "urn": "urn:nbn:de:kobv:b4-20428-0", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch- und Musikalisches Lustw\u00e4ldchen. Hamburg, 1652."}, "poem": {"stanza.1": {"line.1": {"text": "Ach mein Sohn mein liebster Sohn/ der du vormats", "tokens": ["Ach", "mein", "Sohn", "mein", "liebs\u00b7ter", "Sohn", "/", "der", "du", "vor\u00b7mats"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "PPOSAT", "NN", "PPOSAT", "ADJA", "NN", "$(", "ART", "NE", "NE"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "mein Verlangen", "tokens": ["mein", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Und gewisse Hoffnung warst  Ach was hastu doch be-", "tokens": ["Und", "ge\u00b7wis\u00b7se", "Hoff\u00b7nung", "warst", "Ach", "was", "has\u00b7tu", "doch", "be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VAFIN", "ITJ", "PWS", "VAFIN", "ADV", "TRUNC"], "meter": "--+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "gangen?", "tokens": ["gan\u00b7gen", "?"], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Ach des grossen Hertzeleidens! ach ach weh/ der bittren", "tokens": ["Ach", "des", "gros\u00b7sen", "Hert\u00b7ze\u00b7lei\u00b7dens", "!", "ach", "ach", "weh", "/", "der", "bit\u00b7tren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN", "$.", "XY", "XY", "PTKVZ", "$(", "ART", "ADJA"], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.6": {"text": "Noht!", "tokens": ["Noht", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Mein! wo waren die Gedankken/ da\u00df du doch das", "tokens": ["Mein", "!", "wo", "wa\u00b7ren", "die", "Ge\u00b7dank\u00b7ken", "/", "da\u00df", "du", "doch", "das"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "$.", "PWAV", "VAFIN", "ART", "NN", "$(", "KOUS", "PPER", "ADV", "ART"], "meter": "+-+-+-+-++-+", "measure": "unknown.measure.septa"}, "line.8": {"text": "scharff Geboht", "tokens": ["scharff", "Ge\u00b7boht"], "token_info": ["word", "word"], "pos": ["VVFIN", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "Welches dir dein Vater gab hast so sch\u00e4ndlich \u00fcber-", "tokens": ["Wel\u00b7ches", "dir", "dein", "Va\u00b7ter", "gab", "hast", "so", "sch\u00e4nd\u00b7lich", "\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "PPOSAT", "NN", "VVFIN", "VAFIN", "ADV", "ADJD", "TRUNC"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.10": {"text": "schritten/", "tokens": ["schrit\u00b7ten", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.11": {"text": "Welches Leib und Leben gilt? Nu hilfft dich/ mein Sohn/", "tokens": ["Wel\u00b7ches", "Leib", "und", "Le\u00b7ben", "gilt", "?", "Nu", "hilfft", "dich", "/", "mein", "Sohn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAT", "NN", "KON", "NN", "VVFIN", "$.", "ADV", "VVFIN", "PPER", "$(", "PPOSAT", "NN", "$("], "meter": "+-+-+---+--+", "measure": "iambic.penta.chol"}, "line.12": {"text": "kein Bitten/", "tokens": ["kein", "Bit\u00b7ten", "/"], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.13": {"text": "Du O frisches Blut der Helden! Du must ietzo doch", "tokens": ["Du", "O", "fri\u00b7sches", "Blut", "der", "Hel\u00b7den", "!", "Du", "must", "iet\u00b7zo", "doch"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "NE", "ADJA", "NN", "ART", "NN", "$.", "PPER", "VMFIN", "ADV", "ADV"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.14": {"text": "daran/", "tokens": ["da\u00b7ran", "/"], "token_info": ["word", "punct"], "pos": ["PAV", "$("], "meter": "-+", "measure": "iambic.single"}, "line.15": {"text": "Weil du wider die Gesetze dieser R\u00f6mer hast gethan.", "tokens": ["Weil", "du", "wi\u00b7der", "die", "Ge\u00b7set\u00b7ze", "die\u00b7ser", "R\u00f6\u00b7mer", "hast", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "PDAT", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.16": {"text": "Entweder ich mu\u00df ietzund meines v\u00e4terlichen Hertzen", "tokens": ["Ent\u00b7we\u00b7der", "ich", "mu\u00df", "ie\u00b7tzund", "mei\u00b7nes", "v\u00e4\u00b7ter\u00b7li\u00b7chen", "Hert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.17": {"text": "Gantz vergessen/ und dein Grab mit vergallter Angst", "tokens": ["Gantz", "ver\u00b7ges\u00b7sen", "/", "und", "dein", "Grab", "mit", "ver\u00b7gall\u00b7ter", "Angst"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVPP", "$(", "KON", "PPOSAT", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.18": {"text": "verschmertzen/", "tokens": ["ver\u00b7schmert\u00b7zen", "/"], "token_info": ["word", "punct"], "pos": ["VVINF", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.19": {"text": "Oder den gemeinen Nutzen/ und das liebe Vaterland", "tokens": ["O\u00b7der", "den", "ge\u00b7mei\u00b7nen", "Nut\u00b7zen", "/", "und", "das", "lie\u00b7be", "Va\u00b7ter\u00b7land"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "$(", "KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.20": {"text": "Sch\u00e4ndlich auf die Seite setzen. (O der unerh\u00f6rten", "tokens": ["Sch\u00e4nd\u00b7lich", "auf", "die", "Sei\u00b7te", "set\u00b7zen", ".", "(", "O", "der", "un\u00b7er\u00b7h\u00f6r\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "VVINF", "$.", "$(", "NE", "ART", "ADJA"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.21": {"text": "Schand!)", "tokens": ["Schand", "!", ")"], "token_info": ["word", "punct", "punct"], "pos": ["NN", "$.", "$("], "meter": "+", "measure": "single.up"}, "line.22": {"text": "Ach die wird man nimmermehr in Torquats Regierung", "tokens": ["Ach", "die", "wird", "man", "nim\u00b7mer\u00b7mehr", "in", "Tor\u00b7quats", "Re\u00b7gie\u00b7rung"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "PDS", "VAFIN", "PIS", "ADV", "APPR", "NE", "NN"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.23": {"text": "h\u00f6ren/", "tokens": ["h\u00f6\u00b7ren", "/"], "token_info": ["word", "punct"], "pos": ["VVINF", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.24": {"text": "Nimmermehr wird solch ein St\u00fckk diesen alten Kopff be-", "tokens": ["Nim\u00b7mer\u00b7mehr", "wird", "solch", "ein", "St\u00fckk", "die\u00b7sen", "al\u00b7ten", "Kopff", "be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PIAT", "ART", "NN", "PDAT", "ADJA", "NN", "TRUNC"], "meter": "+--+-++--+-+-", "measure": "iambic.hexa.invert"}, "line.25": {"text": "th\u00f6ren.", "tokens": ["th\u00f6\u00b7ren", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.26": {"text": "La\u00df ich/ liebstes Kind/ dich leben wo bleibt mein Gehor-", "tokens": ["La\u00df", "ich", "/", "liebs\u00b7tes", "Kind", "/", "dich", "le\u00b7ben", "wo", "bleibt", "mein", "Ge\u00b7hor"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "$(", "ADJA", "NN", "$(", "PPER", "VVINF", "PWAV", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-++--+", "measure": "iambic.septa.chol"}, "line.27": {"text": "sam dann?", "tokens": ["sam", "dann", "?"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "ADV", "$."], "meter": "--", "measure": "unknown.measure.zero"}, "line.28": {"text": "Und der andern Obrigkeiten? es wird bald ein ieder-", "tokens": ["Und", "der", "an\u00b7dern", "Ob\u00b7rig\u00b7kei\u00b7ten", "?", "es", "wird", "bald", "ein", "ie\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "$.", "PPER", "VAFIN", "ADV", "ART", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.29": {"text": "mann", "tokens": ["mann"], "token_info": ["word"], "pos": ["NN"], "meter": "-", "measure": "single.down"}, "line.30": {"text": "Der nur Lust zu s\u00fcndigen mir ins Angesichte sagen:", "tokens": ["Der", "nur", "Lust", "zu", "s\u00fcn\u00b7di\u00b7gen", "mir", "ins", "An\u00b7ge\u00b7sich\u00b7te", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "NN", "PTKZU", "VVFIN", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.31": {"text": "Hat doch selbst dein eigner Sohn Manlius vor wenig Ta-", "tokens": ["Hat", "doch", "selbst", "dein", "eig\u00b7ner", "Sohn", "Man\u00b7lius", "vor", "we\u00b7nig", "Ta"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ADV", "PPOSAT", "ADJA", "NN", "NE", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.32": {"text": "gen", "tokens": ["gen"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.33": {"text": "Wider die\u00df Gesetz gehandelt. Siehstu nun mein liebts", "tokens": ["Wi\u00b7der", "die\u00df", "Ge\u00b7setz", "ge\u00b7han\u00b7delt", ".", "Sieh\u00b7stu", "nun", "mein", "liebts"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PDS", "NN", "VVPP", "$.", "PPER", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.34": {"text": "Kind/", "tokens": ["Kind", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+", "measure": "single.up"}, "line.35": {"text": "Was man hier wird mit dir machen. Wo mein Bit-", "tokens": ["Was", "man", "hier", "wird", "mit", "dir", "ma\u00b7chen", ".", "Wo", "mein", "Bit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "PIS", "ADV", "VAFIN", "APPR", "PPER", "VVINF", "$.", "PWAV", "PPOSAT", "TRUNC"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.36": {"text": "ten Stelle find/", "tokens": ["ten", "Stel\u00b7le", "find", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.37": {"text": "Und bey dir verfangen kan/ ach so wollstu meinen Willen/", "tokens": ["Und", "bey", "dir", "ver\u00b7fan\u00b7gen", "kan", "/", "ach", "so", "woll\u00b7stu", "mei\u00b7nen", "Wil\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVINF", "VMFIN", "$(", "ITJ", "ADV", "VMFIN", "PPOSAT", "NN", "$("], "meter": "-+--+--+-+-+-+-", "measure": "amphibrach.tri.plus"}, "line.38": {"text": "(ach was willen/ wenn ich mu\u00df!) nun mit deinem Blut er-", "tokens": ["(", "ach", "was", "wil\u00b7len", "/", "wenn", "ich", "mu\u00df", "!", ")", "nun", "mit", "dei\u00b7nem", "Blut", "er"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ITJ", "PWS", "NN", "$(", "KOUS", "PPER", "VMFIN", "$.", "$(", "ADV", "APPR", "PPOSAT", "NN", "TRUNC"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.39": {"text": "f\u00fcllen/", "tokens": ["f\u00fcl\u00b7len", "/"], "token_info": ["word", "punct"], "pos": ["VVINF", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.40": {"text": "Sey hierzu mein Sohn ged\u00fcldig zu der bittren Todes-", "tokens": ["Sey", "hier\u00b7zu", "mein", "Sohn", "ge\u00b7d\u00fcl\u00b7dig", "zu", "der", "bit\u00b7tren", "To\u00b7des"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PAV", "PPOSAT", "NN", "ADJD", "APPR", "ART", "ADJA", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.41": {"text": "pein/", "tokens": ["pein", "/"], "token_info": ["word", "punct"], "pos": ["NE", "$("], "meter": "+", "measure": "single.up"}, "line.42": {"text": "Dieser Tod wird hier im Lande der Gesetze Geltung", "tokens": ["Die\u00b7ser", "Tod", "wird", "hier", "im", "Lan\u00b7de", "der", "Ge\u00b7set\u00b7ze", "Gel\u00b7tung"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "VAFIN", "ADV", "APPRART", "NN", "ART", "NN", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.43": {"text": "seyn.", "tokens": ["seyn", "."], "token_info": ["word", "punct"], "pos": ["VAINF", "$."], "meter": "+", "measure": "single.up"}, "line.44": {"text": "Dieser Tod wird mir und dir einen Ruhm zuwege bringe\u0303/", "tokens": ["Die\u00b7ser", "Tod", "wird", "mir", "und", "dir", "ei\u00b7nen", "Ruhm", "zu\u00b7we\u00b7ge", "bring\u1ebd", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VAFIN", "PPER", "KON", "PPER", "ART", "NN", "ADV", "VVFIN", "$("], "meter": "+--+-+-+-+-+-+", "measure": "iambic.septa.invert"}, "line.45": {"text": "Welcher nimmermehr vergeht/ Dieser Tod wird Rom be-", "tokens": ["Wel\u00b7cher", "nim\u00b7mer\u00b7mehr", "ver\u00b7geht", "/", "Die\u00b7ser", "Tod", "wird", "Rom", "be"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAT", "ADV", "VVFIN", "$(", "PDAT", "NN", "VAFIN", "NE", "TRUNC"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.46": {"text": "zwingen/", "tokens": ["zwin\u00b7gen", "/"], "token_info": ["word", "punct"], "pos": ["VVINF", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.47": {"text": "Da\u00df man wird gehorsam bleiben. B\u00f6tel bring das Richt-", "tokens": ["Da\u00df", "man", "wird", "ge\u00b7hor\u00b7sam", "blei\u00b7ben", ".", "B\u00f6\u00b7tel", "bring", "das", "Richt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "VAFIN", "ADJD", "VVINF", "$.", "NE", "VVFIN", "ART", "TRUNC"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.48": {"text": "beil her/", "tokens": ["beil", "her", "/"], "token_info": ["word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$("], "meter": "--", "measure": "unknown.measure.zero"}, "line.49": {"text": "Thu was dir ist anbefohlen. Nun mag sehen der und", "tokens": ["Thu", "was", "dir", "ist", "an\u00b7be\u00b7foh\u00b7len", ".", "Nun", "mag", "se\u00b7hen", "der", "und"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PWS", "PPER", "VAFIN", "VVPP", "$.", "ADV", "VMFIN", "VVINF", "ART", "KON"], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.50": {"text": "der", "tokens": ["der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.51": {"text": "Wer ein R\u00f6mscher Kriegsman ist/ wie er seinen Oberleute\u0303", "tokens": ["Wer", "ein", "R\u00f6m\u00b7scher", "Kriegs\u00b7man", "ist", "/", "wie", "er", "sei\u00b7nen", "O\u00b7ber\u00b7leut\u1ebd"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "ART", "ADJA", "NN", "VAFIN", "$(", "PWAV", "PPER", "PPOSAT", "NN"], "meter": "--+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.52": {"text": "Allezeit gehorchen soll/ und hernachmals tapffer streiten.", "tokens": ["Al\u00b7le\u00b7zeit", "ge\u00b7hor\u00b7chen", "soll", "/", "und", "her\u00b7nach\u00b7mals", "tapf\u00b7fer", "strei\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VMFIN", "$(", "KON", "ADV", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.53": {"text": "Wo es in ber\u00fchmten St\u00e4dten wol und gl\u00fcklich zu- soll-", "tokens": ["Wo", "es", "in", "be\u00b7r\u00fchm\u00b7ten", "St\u00e4d\u00b7ten", "wol", "und", "gl\u00fck\u00b7lich", "zu", "soll"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "ADJA", "NN", "ADV", "KON", "ADJD", "TRUNC", "TRUNC"], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.54": {"text": "gehn", "tokens": ["gehn"], "token_info": ["word"], "pos": ["VVINF"], "meter": "+", "measure": "single.up"}, "line.55": {"text": "Mu\u00df Gehorsam bey Gesetzen/ und bey Herren Wei\u00df-", "tokens": ["Mu\u00df", "Ge\u00b7hor\u00b7sam", "bey", "Ge\u00b7set\u00b7zen", "/", "und", "bey", "Her\u00b7ren", "Wei\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VMFIN", "NN", "APPR", "NN", "$(", "KON", "APPR", "NN", "TRUNC"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.56": {"text": "heit stehn.", "tokens": ["heit", "stehn", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$."], "meter": "-+", "measure": "iambic.single"}}}}}