{"textgrid.poem.30783": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "97.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir musten alle V\u00f6lcker zu Todtengr\u00e4bern haben,", "tokens": ["Wir", "mus\u00b7ten", "al\u00b7le", "V\u00f6l\u00b7cker", "zu", "Tod\u00b7ten\u00b7gr\u00e4\u00b7bern", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIAT", "NN", "APPR", "NN", "VAFIN", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Eh Deutschland in sich selbsten sie kunten recht vergraben.", "tokens": ["Eh", "Deutschland", "in", "sich", "selbs\u00b7ten", "sie", "kun\u00b7ten", "recht", "ver\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "APPR", "PRF", "VVFIN", "PPER", "VMFIN", "ADJD", "VVPP", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Noch sind sie mehr ietzt m\u00fchsam, den C\u00f6rper zu verwahren,", "tokens": ["Noch", "sind", "sie", "mehr", "ietzt", "m\u00fch\u00b7sam", ",", "den", "C\u00f6r\u00b7per", "zu", "ver\u00b7wah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Da\u00df in ihn neue Geister nicht etwa wieder fahren,", "tokens": ["Da\u00df", "in", "ihn", "neu\u00b7e", "Geis\u00b7ter", "nicht", "et\u00b7wa", "wie\u00b7der", "fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPER", "ADJA", "NN", "PTKNEG", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Da\u00df seine Todtengr\u00e4ber es nicht sey wieder willig", "tokens": ["Da\u00df", "sei\u00b7ne", "Tod\u00b7ten\u00b7gr\u00e4\u00b7ber", "es", "nicht", "sey", "wie\u00b7der", "wil\u00b7lig"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "PTKNEG", "VAFIN", "ADV", "ADJD"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Ingleichem zu bestatten, vielleicht auch mehr noch v\u00f6llig.", "tokens": ["In\u00b7glei\u00b7chem", "zu", "be\u00b7stat\u00b7ten", ",", "viel\u00b7leicht", "auch", "mehr", "noch", "v\u00f6l\u00b7lig", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$,", "ADV", "ADV", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}}}}}