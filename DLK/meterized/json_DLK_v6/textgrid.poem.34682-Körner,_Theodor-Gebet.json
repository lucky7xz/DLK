{"textgrid.poem.34682": {"metadata": {"author": {"name": "K\u00f6rner, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Gebet", "genre": "verse", "period": "N.A.", "pub_year": 1802, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Deine Sonne, Herr des Himmels,", "tokens": ["Dei\u00b7ne", "Son\u00b7ne", ",", "Herr", "des", "Him\u00b7mels", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schmilzt den Schnee von deinen Bergen,", "tokens": ["Schmilzt", "den", "Schnee", "von", "dei\u00b7nen", "Ber\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bricht mit rosiger Verkl\u00e4rung", "tokens": ["Bricht", "mit", "ro\u00b7si\u00b7ger", "Ver\u00b7kl\u00e4\u00b7rung"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Durch der Nebel d\u00fcstern Schleier,", "tokens": ["Durch", "der", "Ne\u00b7bel", "d\u00fcs\u00b7tern", "Schlei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Tr\u00e4gt den milden Hauch des Tages", "tokens": ["Tr\u00e4gt", "den", "mil\u00b7den", "Hauch", "des", "Ta\u00b7ges"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Siegend aus dem Kampf der Nacht.", "tokens": ["Sie\u00b7gend", "aus", "dem", "Kampf", "der", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Deine Sonne, g\u00fct'ger Vater,", "tokens": ["Dei\u00b7ne", "Son\u00b7ne", ",", "g\u00fct'\u00b7ger", "Va\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lockt die Knospe aus dem Kelche,", "tokens": ["Lockt", "die", "Knos\u00b7pe", "aus", "dem", "Kel\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Taucht die jungfr\u00e4ulichen Bl\u00e4tter", "tokens": ["Taucht", "die", "jung\u00b7fr\u00e4u\u00b7li\u00b7chen", "Bl\u00e4t\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "In das zarte Rot der Sehnsucht,", "tokens": ["In", "das", "zar\u00b7te", "Rot", "der", "Sehn\u00b7sucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+---", "measure": "unknown.measure.tri"}, "line.5": {"text": "K\u00fc\u00dft des Taues Thr\u00e4nenperle", "tokens": ["K\u00fc\u00dft", "des", "Tau\u00b7es", "Thr\u00e4\u00b7nen\u00b7per\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "L\u00e4chelnd aus dem Bl\u00fctenkelch.", "tokens": ["L\u00e4\u00b7chelnd", "aus", "dem", "Bl\u00fc\u00b7ten\u00b7kelch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "O, so f\u00fchre deine Liebe", "tokens": ["O", ",", "so", "f\u00fch\u00b7re", "dei\u00b7ne", "Lie\u00b7be"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "ADV", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus der Nebel d\u00fcst'rem Schatten", "tokens": ["Aus", "der", "Ne\u00b7bel", "d\u00fcst'\u00b7rem", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mir herauf den goldnen Morgen,", "tokens": ["Mir", "her\u00b7auf", "den", "gold\u00b7nen", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.4": {"text": "Locke meines Herzens Keime,", "tokens": ["Lo\u00b7cke", "mei\u00b7nes", "Her\u00b7zens", "Kei\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "K\u00fcsse mir vom matten Auge", "tokens": ["K\u00fcs\u00b7se", "mir", "vom", "mat\u00b7ten", "Au\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Meiner Sehnsucht Thr\u00e4ne ab!", "tokens": ["Mei\u00b7ner", "Sehn\u00b7sucht", "Thr\u00e4\u00b7ne", "ab", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Oder pflanze mir die Rosen", "tokens": ["O\u00b7der", "pflan\u00b7ze", "mir", "die", "Ro\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schattend \u00fcber meinem H\u00fcgel,", "tokens": ["Schat\u00b7tend", "\u00fc\u00b7ber", "mei\u00b7nem", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df der Bl\u00fctendorn der Liebe,", "tokens": ["Da\u00df", "der", "Bl\u00fc\u00b7ten\u00b7dorn", "der", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der den Lebenden verwundet,", "tokens": ["Der", "den", "Le\u00b7ben\u00b7den", "ver\u00b7wun\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Fest verzweigt mit der Cypresse,", "tokens": ["Fest", "ver\u00b7zweigt", "mit", "der", "Cyp\u00b7res\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.6": {"text": "Doch den Toten k\u00fchlen mu\u00df!", "tokens": ["Doch", "den", "To\u00b7ten", "k\u00fch\u00b7len", "mu\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}