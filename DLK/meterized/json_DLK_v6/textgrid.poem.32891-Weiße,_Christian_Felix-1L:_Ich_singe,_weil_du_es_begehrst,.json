{"textgrid.poem.32891": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich singe, weil du es begehrst,", "genre": "verse", "period": "N.A.", "pub_year": 1765, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich singe, weil du es begehrst,", "tokens": ["Ich", "sin\u00b7ge", ",", "weil", "du", "es", "be\u00b7gehrst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und singe gern, weil du es gerne h\u00f6rst:", "tokens": ["Und", "sin\u00b7ge", "gern", ",", "weil", "du", "es", "ger\u00b7ne", "h\u00f6rst", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$,", "KOUS", "PPER", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Denn dir w\u00fcnsch ich vor allen,", "tokens": ["Denn", "dir", "w\u00fcnsch", "ich", "vor", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "APPR", "PIAT", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Mein Thyrsis, zu gefallen.", "tokens": ["Mein", "Thyr\u00b7sis", ",", "zu", "ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Doch Thyrsis, sprich! was soll ich dir", "tokens": ["Doch", "Thyr\u00b7sis", ",", "sprich", "!", "was", "soll", "ich", "dir"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "NE", "$,", "ADJD", "$.", "PWS", "VMFIN", "PPER", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Stets singen? wie? hast du denn sonst bey mir", "tokens": ["Stets", "sin\u00b7gen", "?", "wie", "?", "hast", "du", "denn", "sonst", "bey", "mir"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVINF", "$.", "PWAV", "$.", "VAFIN", "PPER", "ADV", "ADV", "APPR", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nichts, gar nichts anzubringen?", "tokens": ["Nichts", ",", "gar", "nichts", "an\u00b7zu\u00b7brin\u00b7gen", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "ADV", "PIS", "VVIZU", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Kann ich nichts mehr, als singen?", "tokens": ["Kann", "ich", "nichts", "mehr", ",", "als", "sin\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PIS", "ADV", "$,", "KOUS", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}