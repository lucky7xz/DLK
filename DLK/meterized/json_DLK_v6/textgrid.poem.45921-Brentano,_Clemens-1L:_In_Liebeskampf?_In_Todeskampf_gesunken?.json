{"textgrid.poem.45921": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "1L: In Liebeskampf? In Todeskampf gesunken?", "genre": "verse", "period": "N.A.", "pub_year": 1808, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In Liebeskampf? In Todeskampf gesunken?", "tokens": ["In", "Lie\u00b7bes\u00b7kampf", "?", "In", "To\u00b7des\u00b7kampf", "ge\u00b7sun\u00b7ken", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$.", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ob Atem noch von ihren Lippen flie\u00dft?", "tokens": ["Ob", "A\u00b7tem", "noch", "von", "ih\u00b7ren", "Lip\u00b7pen", "flie\u00dft", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ob ihr der Krampf den kleinen Mund verschlie\u00dft?", "tokens": ["Ob", "ihr", "der", "Krampf", "den", "klei\u00b7nen", "Mund", "ver\u00b7schlie\u00dft", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Kein \u00d6l die Lampe? oder keinen Funken?", "tokens": ["Kein", "\u00d6l", "die", "Lam\u00b7pe", "?", "o\u00b7der", "kei\u00b7nen", "Fun\u00b7ken", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "$.", "KON", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der J\u00fcngling \u2013 betend? tot? in Liebe trunken?", "tokens": ["Der", "J\u00fcng\u00b7ling", "\u2013", "be\u00b7tend", "?", "tot", "?", "in", "Lie\u00b7be", "trun\u00b7ken", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "VVPP", "$.", "ADJD", "$.", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ob er der Jungfrau h\u00f6chste Gunst genie\u00dft?", "tokens": ["Ob", "er", "der", "Jung\u00b7frau", "h\u00f6chs\u00b7te", "Gunst", "ge\u00b7nie\u00dft", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Was ist's, das der gefallne Becher gie\u00dft?", "tokens": ["Was", "ist's", ",", "das", "der", "ge\u00b7fall\u00b7ne", "Be\u00b7cher", "gie\u00dft", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "$,", "PRELS", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Hat Gift, hat Wein, hat Balsam sie getrunken.", "tokens": ["Hat", "Gift", ",", "hat", "Wein", ",", "hat", "Bal\u00b7sam", "sie", "ge\u00b7trun\u00b7ken", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "VAFIN", "NN", "$,", "VAFIN", "NE", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Des J\u00fcnglings Arme, Engelsfl\u00fcgel werden \u2013", "tokens": ["Des", "J\u00fcng\u00b7lings", "Ar\u00b7me", ",", "En\u00b7gels\u00b7fl\u00fc\u00b7gel", "wer\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nein Mantelsfalten \u2013 Leichentuches Falten.", "tokens": ["Nein", "Man\u00b7tels\u00b7fal\u00b7ten", "\u2013", "Lei\u00b7chen\u00b7tu\u00b7ches", "Fal\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "NN", "$(", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Um sie strahlt Heil'genschein \u2013 zerraufte Haare.", "tokens": ["Um", "sie", "strahlt", "Heil'\u00b7gen\u00b7schein", "\u2013", "zer\u00b7rauf\u00b7te", "Haa\u00b7re", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUI", "PPER", "VVFIN", "NN", "$(", "ADJA", "NN", "$."], "meter": "--++-+-+-+-", "measure": "anapaest.init"}}, "stanza.4": {"line.1": {"text": "Strahl' Himmelslicht, flamm' H\u00f6lle zu der Erde", "tokens": ["Strahl'", "Him\u00b7mels\u00b7licht", ",", "flamm'", "H\u00f6l\u00b7le", "zu", "der", "Er\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "VVFIN", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Brich der Verzweiflung rasende Gewalten,", "tokens": ["Brich", "der", "Ver\u00b7zwei\u00b7flung", "ra\u00b7sen\u00b7de", "Ge\u00b7wal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+--+-+---+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Enth\u00fcll' \u2013 verh\u00fcll' \u2013 das Freudenbett \u2013 die Bahre.", "tokens": ["Ent\u00b7h\u00fcll'", "\u2013", "ver\u00b7h\u00fcll'", "\u2013", "das", "Freu\u00b7den\u00b7bett", "\u2013", "die", "Bah\u00b7re", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "$(", "VVIMP", "$(", "ART", "NN", "$(", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}