{"textgrid.poem.39161": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich will ja nur an Deiner Lippe sterben,", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich will ja nur an Deiner Lippe sterben,", "tokens": ["Ich", "will", "ja", "nur", "an", "Dei\u00b7ner", "Lip\u00b7pe", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als Sonnenstaub in Deinem Ku\u00df verfliegen,", "tokens": ["Als", "Son\u00b7nen\u00b7staub", "in", "Dei\u00b7nem", "Ku\u00df", "ver\u00b7flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Will nur den Schmerz, den tiefen, schweren, herben,", "tokens": ["Will", "nur", "den", "Schmerz", ",", "den", "tie\u00b7fen", ",", "schwe\u00b7ren", ",", "her\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "$,", "ART", "ADJA", "$,", "VVFIN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit Deines Mundes Lethetrank besiegen.", "tokens": ["Mit", "Dei\u00b7nes", "Mun\u00b7des", "Le\u00b7the\u00b7trank", "be\u00b7sie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich will ja nicht an Deinem Munde saugen,", "tokens": ["Ich", "will", "ja", "nicht", "an", "Dei\u00b7nem", "Mun\u00b7de", "sau\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PTKNEG", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nur fromm und gl\u00e4ubig in Dein Antlitz schauen", "tokens": ["Nur", "fromm", "und", "gl\u00e4u\u00b7big", "in", "Dein", "Ant\u00b7litz", "schau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KON", "ADJD", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und auf dem Strahle Deiner Wunderaugen", "tokens": ["Und", "auf", "dem", "Strah\u00b7le", "Dei\u00b7ner", "Wun\u00b7der\u00b7au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zum \u00c4ther hin demant'ne Br\u00fccken bauen.", "tokens": ["Zum", "\u00c4\u00b7ther", "hin", "de\u00b7mant'\u00b7ne", "Br\u00fc\u00b7cken", "bau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich will ja nicht in Deinem Aug' mich sonnen,", "tokens": ["Ich", "will", "ja", "nicht", "in", "Dei\u00b7nem", "Aug'", "mich", "son\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PTKNEG", "APPR", "PPOSAT", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nur Worte tauschen s\u00fc\u00dfer Minnefehde,", "tokens": ["Nur", "Wor\u00b7te", "tau\u00b7schen", "s\u00fc\u00b7\u00dfer", "Min\u00b7ne\u00b7feh\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nur rauschen h\u00f6ren Deiner Lippe Bronnen", "tokens": ["Nur", "rau\u00b7schen", "h\u00f6\u00b7ren", "Dei\u00b7ner", "Lip\u00b7pe", "Bron\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVINF", "VVFIN", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In sanften Wellen zarter Frauenrede.", "tokens": ["In", "sanf\u00b7ten", "Wel\u00b7len", "zar\u00b7ter", "Frau\u00b7en\u00b7re\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ich will ja nicht Dich sehen, k\u00fcssen, h\u00f6ren,", "tokens": ["Ich", "will", "ja", "nicht", "Dich", "se\u00b7hen", ",", "k\u00fcs\u00b7sen", ",", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PTKNEG", "PPER", "VVINF", "$,", "VVFIN", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich will ja nur Dein denken im geheimen", "tokens": ["Ich", "will", "ja", "nur", "Dein", "den\u00b7ken", "im", "ge\u00b7hei\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "PPOSAT", "VVFIN", "APPRART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und hoffnungslos der Saite Gold emp\u00f6ren", "tokens": ["Und", "hoff\u00b7nungs\u00b7los", "der", "Sai\u00b7te", "Gold", "em\u00b7p\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ART", "NN", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und mich ergehn in zarten Liebesreimen.", "tokens": ["Und", "mich", "er\u00b7gehn", "in", "zar\u00b7ten", "Lie\u00b7bes\u00b7rei\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}