{"dta.poem.11979": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nachrede b\u00f6se mag leicht Freundesbund vergiften,", "tokens": ["Nach\u00b7re\u00b7de", "b\u00f6\u00b7se", "mag", "leicht", "Freun\u00b7des\u00b7bund", "ver\u00b7gif\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VMFIN", "ADJD", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Zurede gute schwer Feindesvers\u00f6hnung stiften.", "tokens": ["Zu\u00b7re\u00b7de", "gu\u00b7te", "schwer", "Fein\u00b7des\u00b7ver\u00b7s\u00f6h\u00b7nung", "stif\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}}, "stanza.2": {"line.1": {"text": "Dort brauchst du einem nur vom andern zuzutragen,", "tokens": ["Dort", "brauchst", "du", "ei\u00b7nem", "nur", "vom", "an\u00b7dern", "zu\u00b7zu\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADV", "APPRART", "ADJA", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was er, wenn nicht gesagt, doch h\u00e4tte k\u00f6nnen sagen.", "tokens": ["Was", "er", ",", "wenn", "nicht", "ge\u00b7sagt", ",", "doch", "h\u00e4t\u00b7te", "k\u00f6n\u00b7nen", "sa\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "$,", "KOUS", "PTKNEG", "VVPP", "$,", "ADV", "VAFIN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Hier wechselweise mu\u00dft du jeden jedem zeigen", "tokens": ["Hier", "wech\u00b7sel\u00b7wei\u00b7se", "mu\u00dft", "du", "je\u00b7den", "je\u00b7dem", "zei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VMFIN", "PPER", "PIAT", "PIS", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Geneigt zum Frieden, um zum Frieden ihn zu neigen.", "tokens": ["Ge\u00b7neigt", "zum", "Frie\u00b7den", ",", "um", "zum", "Frie\u00b7den", "ihn", "zu", "nei\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "NN", "$,", "KOUI", "APPRART", "NN", "PPER", "PTKZU", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "So h\u00e4ufig jen's und leicht, so schwer ist dies und selten,", "tokens": ["So", "h\u00e4u\u00b7fig", "jen's", "und", "leicht", ",", "so", "schwer", "ist", "dies", "und", "sel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "NE", "KON", "ADJD", "$,", "ADV", "ADJD", "VAFIN", "PDS", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch auch verdienstlicher ist nichts in beiden Welten.", "tokens": ["Doch", "auch", "ver\u00b7dienst\u00b7li\u00b7cher", "ist", "nichts", "in", "bei\u00b7den", "Wel\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VAFIN", "PIS", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}