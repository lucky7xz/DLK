{"dta.poem.357": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Als sie ihn ihren Engel genennet.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich/ Lysette, soll dein lieber Engel seyn", "tokens": ["Wenn", "ich", "/", "Ly\u00b7set\u00b7te", ",", "soll", "dein", "lie\u00b7ber", "En\u00b7gel", "seyn"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "$(", "NE", "$,", "VMFIN", "PPOSAT", "ADJA", "NN", "VAINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So habe ich zwar nichts dagegen einzuwenden/", "tokens": ["So", "ha\u00b7be", "ich", "zwar", "nichts", "da\u00b7ge\u00b7gen", "ein\u00b7zu\u00b7wen\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PIS", "PAV", "VVIZU", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Allein! verzeihe doch/ mir f\u00e4lt noch etwas ein;", "tokens": ["Al\u00b7lein", "!", "ver\u00b7zei\u00b7he", "doch", "/", "mir", "f\u00e4lt", "noch", "et\u00b7was", "ein", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "VVFIN", "ADV", "$(", "PPER", "VVFIN", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Kan auch ein Engel wol an anderm Ort und Enden", "tokens": ["Kan", "auch", "ein", "En\u00b7gel", "wol", "an", "an\u00b7derm", "Ort", "und", "En\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "ART", "NN", "ADV", "APPR", "PIS", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Als in den Himmel seyn? ich h\u00f6r/ dein Hertze spricht/", "tokens": ["Als", "in", "den", "Him\u00b7mel", "seyn", "?", "ich", "h\u00f6r", "/", "dein", "Hert\u00b7ze", "spricht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "VAINF", "$.", "PPER", "VVFIN", "$(", "PPOSAT", "VVFIN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Nein! wer ein Engelist/ mu\u00df einen Himmel haben/", "tokens": ["Nein", "!", "wer", "ein", "En\u00b7ge\u00b7list", "/", "mu\u00df", "ei\u00b7nen", "Him\u00b7mel", "ha\u00b7ben", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PWS", "ART", "NN", "$(", "VMFIN", "ART", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wohlan! dein eigner Schlu\u00df ist wider dich gericht/", "tokens": ["Wo\u00b7hlan", "!", "dein", "eig\u00b7ner", "Schlu\u00df", "ist", "wi\u00b7der", "dich", "ge\u00b7richt", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PPOSAT", "ADJA", "NN", "VAFIN", "APPR", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Mir fehlt das Paradies/ mich will kein Himmel laben.", "tokens": ["Mir", "fehlt", "das", "Pa\u00b7ra\u00b7dies", "/", "mich", "will", "kein", "Him\u00b7mel", "la\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$(", "PPER", "VMFIN", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Doch du kanst/ so du wilt/ mir bald ein Paradies/", "tokens": ["Doch", "du", "kanst", "/", "so", "du", "wilt", "/", "mir", "bald", "ein", "Pa\u00b7ra\u00b7dies", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "$(", "ADV", "PPER", "VMFIN", "$(", "PPER", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Das meine Wohnung sey/ und lieblich ist gewehren.", "tokens": ["Das", "mei\u00b7ne", "Woh\u00b7nung", "sey", "/", "und", "lieb\u00b7lich", "ist", "ge\u00b7weh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "VAFIN", "$(", "KON", "ADJD", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Du bist der Ort/ und must/ glaub\u2019s nur mir gantz gewi\u00df/", "tokens": ["Du", "bist", "der", "Ort", "/", "und", "must", "/", "glaub's", "nur", "mir", "gantz", "ge\u00b7wi\u00df", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$(", "KON", "VMFIN", "$(", "NE", "ADV", "PPER", "ADV", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Soll ich dein Engel seyn/ als Himmel zugeh\u00f6ren.", "tokens": ["Soll", "ich", "dein", "En\u00b7gel", "seyn", "/", "als", "Him\u00b7mel", "zu\u00b7ge\u00b7h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "VAINF", "$(", "KOUS", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}