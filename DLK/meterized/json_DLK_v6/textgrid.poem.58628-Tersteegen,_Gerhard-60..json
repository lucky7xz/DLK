{"textgrid.poem.58628": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "60.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So ist es eben mir, mein Heiland, auch ergangen:", "tokens": ["So", "ist", "es", "e\u00b7ben", "mir", ",", "mein", "Hei\u00b7land", ",", "auch", "er\u00b7gan\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PPER", "$,", "PPOSAT", "NN", "$,", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich fragte nicht nach dir, ich nahm nichts Gut's zur Hand,", "tokens": ["Ich", "frag\u00b7te", "nicht", "nach", "dir", ",", "ich", "nahm", "nichts", "Gut's", "zur", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "PPER", "$,", "PPER", "VVFIN", "PIS", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch kamst du mir zuvor und gabst mir ein Verlangen,", "tokens": ["Doch", "kamst", "du", "mir", "zu\u00b7vor", "und", "gabst", "mir", "ein", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "ADV", "KON", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zu sterben der Natur, zu werden dir bekannt.", "tokens": ["Zu", "ster\u00b7ben", "der", "Na\u00b7tur", ",", "zu", "wer\u00b7den", "dir", "be\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ART", "NN", "$,", "PTKZU", "VAINF", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich suchte, doch noch nicht dich innerlich im Herzen,", "tokens": ["Ich", "such\u00b7te", ",", "doch", "noch", "nicht", "dich", "in\u00b7ner\u00b7lich", "im", "Her\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADV", "ADV", "PTKNEG", "PPER", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ich scheuete das Volk, das dich erwartet da,", "tokens": ["Ich", "scheu\u00b7e\u00b7te", "das", "Volk", ",", "das", "dich", "er\u00b7war\u00b7tet", "da", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ich lief in eigner Kraft mit M\u00fche, Furcht und Schmerzen,", "tokens": ["Ich", "lief", "in", "eig\u00b7ner", "Kraft", "mit", "M\u00fc\u00b7he", ",", "Furcht", "und", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Bis du wurd'st offenbar und sprachst: Hie bin ich nah.", "tokens": ["Bis", "du", "wurd'st", "of\u00b7fen\u00b7bar", "und", "sprachst", ":", "Hie", "bin", "ich", "nah", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "ADJD", "KON", "VVFIN", "$.", "ADV", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}