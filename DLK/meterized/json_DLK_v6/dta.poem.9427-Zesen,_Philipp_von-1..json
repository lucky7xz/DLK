{"dta.poem.9427": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mann pfleget den \u00e4pfelbaum h\u00f6her zu halten", "tokens": ["Mann", "pfle\u00b7get", "den", "\u00e4p\u00b7fel\u00b7baum", "h\u00f6\u00b7her", "zu", "hal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "ADV", "ADJD", "PTKZU", "VVINF"], "meter": "++--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Der \u00e4pfel und s\u00fc\u00dfes Obst bringet herf\u00fcr/", "tokens": ["Der", "\u00e4p\u00b7fel", "und", "s\u00fc\u00b7\u00dfes", "Obst", "brin\u00b7get", "her\u00b7f\u00fcr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ADJA", "NN", "VVFIN", "ADV", "$("], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Als einen/ der besser ins Feuer zu spalten/", "tokens": ["Als", "ei\u00b7nen", "/", "der", "bes\u00b7ser", "ins", "Feu\u00b7er", "zu", "spal\u00b7ten", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "$(", "ART", "ADJD", "APPRART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Tr\u00e4gt sauere Fr\u00fcchte/ so deuchtet auch mier/", "tokens": ["Tr\u00e4gt", "sau\u00b7e\u00b7re", "Fr\u00fcch\u00b7te", "/", "so", "deuch\u00b7tet", "auch", "mier", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$(", "ADV", "VVFIN", "ADV", "PPER", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Sey h\u00f6her zu achten/", "tokens": ["Sey", "h\u00f6\u00b7her", "zu", "ach\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "PTKZU", "VVINF", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Wenn wier es betrachten/", "tokens": ["Wenn", "wier", "es", "be\u00b7trach\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVINF", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Vor andern mein Freund;", "tokens": ["Vor", "an\u00b7dern", "mein", "Freund", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "PPOSAT", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Mein Liebster mich letzet/", "tokens": ["Mein", "Liebs\u00b7ter", "mich", "let\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VVFIN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Sein schatten erg\u00f6tzet/", "tokens": ["Sein", "schat\u00b7ten", "er\u00b7g\u00f6t\u00b7zet", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VVFIN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Der/ welchen mein Hertze so treulich gemeint.", "tokens": ["Der", "/", "wel\u00b7chen", "mein", "Hert\u00b7ze", "so", "treu\u00b7lich", "ge\u00b7meint", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "PWAT", "PPOSAT", "VVFIN", "ADV", "ADJD", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Er f\u00fchret mich sanffte zum Keller hinnunter/", "tokens": ["Er", "f\u00fch\u00b7ret", "mich", "sanff\u00b7te", "zum", "Kel\u00b7ler", "hin\u00b7nun\u00b7ter", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "APPRART", "NN", "PTKVZ", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Mit Liebes-Panieren und schencket mier ein/", "tokens": ["Mit", "Lie\u00b7bes\u00b7Pa\u00b7nie\u00b7ren", "und", "schen\u00b7cket", "mier", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "VVFIN", "PPER", "ART", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Er f\u00fchret mich freundlich und machet mich munter/", "tokens": ["Er", "f\u00fch\u00b7ret", "mich", "freund\u00b7lich", "und", "ma\u00b7chet", "mich", "mun\u00b7ter", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "KON", "VVFIN", "PPER", "ADJD", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Er giebet mier Blumen-safft/ \u00e4pfel und Wein;", "tokens": ["Er", "gie\u00b7bet", "mier", "Blu\u00b7men\u00b7\u00b7safft", "/", "\u00e4p\u00b7fel", "und", "Wein", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "$(", "ADJD", "KON", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Er st\u00e4rcket die kr\u00e4ffte", "tokens": ["Er", "st\u00e4r\u00b7cket", "die", "kr\u00e4ff\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Durch mancherley s\u00e4ffte;", "tokens": ["Durch", "man\u00b7cher\u00b7ley", "s\u00e4ff\u00b7te", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVFIN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Mich labet der tranck;", "tokens": ["Mich", "la\u00b7bet", "der", "tranck", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Dieweil ich im Hertzen", "tokens": ["Die\u00b7weil", "ich", "im", "Hert\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "Von Liebe mit schmertzen", "tokens": ["Von", "Lie\u00b7be", "mit", "schmert\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Beladen und gleichsam vor Liebe gar kranck.", "tokens": ["Be\u00b7la\u00b7den", "und", "gleich\u00b7sam", "vor", "Lie\u00b7be", "gar", "kranck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "APPR", "NN", "ADV", "ADJD", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "Es liebet mich hertzlich und treulich mein Leben/", "tokens": ["Es", "lie\u00b7bet", "mich", "hertz\u00b7lich", "und", "treu\u00b7lich", "mein", "Le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "PPOSAT", "NN", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Mein Liebster mich liebet/ das wei\u00df ich gewi\u00df/", "tokens": ["Mein", "Liebs\u00b7ter", "mich", "lie\u00b7bet", "/", "das", "wei\u00df", "ich", "ge\u00b7wi\u00df", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VVFIN", "$(", "PDS", "VVFIN", "PPER", "ADV", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Die Lincke/ so unter dem H\u00e4upte ligt eben/", "tokens": ["Die", "Lin\u00b7cke", "/", "so", "un\u00b7ter", "dem", "H\u00e4up\u00b7te", "ligt", "e\u00b7ben", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADV", "APPR", "ART", "NN", "VVFIN", "ADV", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Zeiget die Liebe/ der Falschheit gebi\u00df/", "tokens": ["Zei\u00b7get", "die", "Lie\u00b7be", "/", "der", "Falschheit", "ge\u00b7bi\u00df", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$(", "ART", "NN", "VVFIN", "$("], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Befestigt die Treue", "tokens": ["Be\u00b7fes\u00b7tigt", "die", "Treu\u00b7e"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Nun wider aufs neue/", "tokens": ["Nun", "wi\u00b7der", "aufs", "neu\u00b7e", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "APPRART", "ADJA", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Das herrliche pfand:", "tokens": ["Das", "herr\u00b7li\u00b7che", "pfand", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Die Rechte mich hertzet/", "tokens": ["Die", "Rech\u00b7te", "mich", "hert\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Die Lincke die schertzet/", "tokens": ["Die", "Lin\u00b7cke", "die", "schert\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "VVFIN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Nun hab\u2019 ich das hertze des Freundes erkant.", "tokens": ["Nun", "hab'", "ich", "das", "hert\u00b7ze", "des", "Freun\u00b7des", "er\u00b7kant", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+--+---+-+", "measure": "iambic.tetra.relaxed"}}}}}