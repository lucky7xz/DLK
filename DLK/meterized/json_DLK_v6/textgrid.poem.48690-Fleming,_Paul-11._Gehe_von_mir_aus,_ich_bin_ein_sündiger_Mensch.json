{"textgrid.poem.48690": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "11. Gehe von mir aus, ich bin ein s\u00fcndiger Mensch", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sollst du, Allwissender, nicht meinen Zustand wissen?", "tokens": ["Sollst", "du", ",", "All\u00b7wis\u00b7sen\u00b7der", ",", "nicht", "mei\u00b7nen", "Zu\u00b7stand", "wis\u00b7sen", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "NN", "$,", "PTKNEG", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mich hat der erste Tod dem andern zugef\u00fchrt.", "tokens": ["Mich", "hat", "der", "ers\u00b7te", "Tod", "dem", "an\u00b7dern", "zu\u00b7ge\u00b7f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "ART", "ADJA", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das sch\u00f6ne Bild ist weg, mit dem ich war geziert,", "tokens": ["Das", "sch\u00f6\u00b7ne", "Bild", "ist", "weg", ",", "mit", "dem", "ich", "war", "ge\u00b7ziert", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "$,", "APPR", "PRELS", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "der erste fremde Fall hat mich auch umgerissen,", "tokens": ["der", "ers\u00b7te", "frem\u00b7de", "Fall", "hat", "mich", "auch", "um\u00b7ge\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "der H\u00f6lle schwere Hand mich t\u00f6tlich wund geschmissen,", "tokens": ["der", "H\u00f6l\u00b7le", "schwe\u00b7re", "Hand", "mich", "t\u00f6t\u00b7lich", "wund", "ge\u00b7schmis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "PPER", "ADJD", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "so da\u00df mein schwacher Geist sich weder kennt, noch r\u00fchrt", "tokens": ["so", "da\u00df", "mein", "schwa\u00b7cher", "Geist", "sich", "we\u00b7der", "kennt", ",", "noch", "r\u00fchrt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "KOUS", "PPOSAT", "ADJA", "NN", "PRF", "KON", "VVFIN", "$,", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "aus sich und von sich selbst, ja t\u00e4glich noch gebiert,", "tokens": ["aus", "sich", "und", "von", "sich", "selbst", ",", "ja", "t\u00e4g\u00b7lich", "noch", "ge\u00b7biert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "KON", "APPR", "PRF", "ADV", "$,", "ADV", "ADJD", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "was ich beweinen mu\u00df mit starken Tr\u00e4neng\u00fcssen.", "tokens": ["was", "ich", "be\u00b7wei\u00b7nen", "mu\u00df", "mit", "star\u00b7ken", "Tr\u00e4\u00b7nen\u00b7g\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVINF", "VMFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wie k\u00f6mts denn, da\u00df du k\u00f6mst und kehrest zu mir ein,", "tokens": ["Wie", "k\u00f6mts", "denn", ",", "da\u00df", "du", "k\u00f6mst", "und", "keh\u00b7rest", "zu", "mir", "ein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "$,", "KOUS", "PPER", "VVFIN", "KON", "VVFIN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "o selge Heiligkeit, in mich verdamte S\u00fcnde,", "tokens": ["o", "sel\u00b7ge", "Hei\u00b7lig\u00b7keit", ",", "in", "mich", "ver\u00b7dam\u00b7te", "S\u00fcn\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM", "ADJA", "NN", "$,", "APPR", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "o Leben, in den Tod? Ach! da\u00df ich das verst\u00fcnde!", "tokens": ["o", "Le\u00b7ben", ",", "in", "den", "Tod", "?", "Ach", "!", "da\u00df", "ich", "das", "ver\u00b7st\u00fcn\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM", "NN", "$,", "APPR", "ART", "NN", "$.", "ITJ", "$.", "KOUS", "PPER", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Doch tu du, was du wilt, ich will dir willig sein.", "tokens": ["Doch", "tu", "du", ",", "was", "du", "wilt", ",", "ich", "will", "dir", "wil\u00b7lig", "sein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "PWS", "PPER", "VMFIN", "$,", "PPER", "VMFIN", "PPER", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sag, H\u00f6lle, was du wilst, es ist f\u00fcrwar erlogen,", "tokens": ["Sag", ",", "H\u00f6l\u00b7le", ",", "was", "du", "wilst", ",", "es", "ist", "f\u00fcr\u00b7war", "er\u00b7lo\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "PWS", "PPER", "VMFIN", "$,", "PPER", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "die Seligkeit selbselbst ist in mich eingezogen.", "tokens": ["die", "Se\u00b7lig\u00b7keit", "selb\u00b7selbst", "ist", "in", "mich", "ein\u00b7ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}