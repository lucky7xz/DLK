{"dta.poem.19074": {"metadata": {"author": {"name": "Zinzendorf, Nicolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "Lv.   Auf Herr   M.   Schwedlers m\u00e4chtige Pre-  \n digten in Bertelsdorf und Herrnhut.   An Mari\u00e4 Heimsuchung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20688-6", "language": ["de:0.99"], "booktitle": "Zinzendorf, Nicolaus Ludwig von: Teutscher Gedichte Erster Theil. Herrnhuth, 1735."}, "poem": {"stanza.1": {"line.1": {"text": "Die Glieder JEsu freun sich sehr,", "tokens": ["Die", "Glie\u00b7der", "Je\u00b7su", "freun", "sich", "sehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VVFIN", "PRF", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch ohne viel Ger\u00e4usche;", "tokens": ["Doch", "oh\u00b7ne", "viel", "Ge\u00b7r\u00e4u\u00b7sche", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie kennen JEsum selbst nicht mehr", "tokens": ["Sie", "ken\u00b7nen", "Je\u00b7sum", "selbst", "nicht", "mehr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NE", "ADV", "PTKNEG", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nach Augenschein und Fleische;", "tokens": ["Nach", "Au\u00b7gen\u00b7schein", "und", "Flei\u00b7sche", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sie dencken wenig oder nichts", "tokens": ["Sie", "den\u00b7cken", "we\u00b7nig", "o\u00b7der", "nichts"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIS", "KON", "PIS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "An V\u00e4ter und Regierer;", "tokens": ["An", "V\u00e4\u00b7ter", "und", "Re\u00b7gie\u00b7rer", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Das Ebenbild des ewgen Lichts", "tokens": ["Das", "E\u00b7ben\u00b7bild", "des", "ew\u00b7gen", "Lichts"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ist Vater und ist F\u00fchrer.", "tokens": ["Ist", "Va\u00b7ter", "und", "ist", "F\u00fch\u00b7rer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "VAFIN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Dann sucht und findt man keinen Rath", "tokens": ["Dann", "sucht", "und", "findt", "man", "kei\u00b7nen", "Rath"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "PIS", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bey ledigen Geschw\u00e4tze;", "tokens": ["Bey", "le\u00b7di\u00b7gen", "Ge\u00b7schw\u00e4t\u00b7ze", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Auch macht man nicht gewissen Staat", "tokens": ["Auch", "macht", "man", "nicht", "ge\u00b7wis\u00b7sen", "Staat"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "PTKNEG", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf v\u00e4terliche S\u00e4tze.", "tokens": ["Auf", "v\u00e4\u00b7ter\u00b7li\u00b7che", "S\u00e4t\u00b7ze", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So jucken uns die Ohren nicht", "tokens": ["So", "ju\u00b7cken", "uns", "die", "Oh\u00b7ren", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nach blossen Redner-Stimmen;", "tokens": ["Nach", "blos\u00b7sen", "Red\u00b7ner\u00b7Stim\u00b7men", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Das Wort, das aufgesteckte Licht", "tokens": ["Das", "Wort", ",", "das", "auf\u00b7ge\u00b7steck\u00b7te", "Licht"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Macht manchen Tocht entglimmen.", "tokens": ["Macht", "man\u00b7chen", "Tocht", "ent\u00b7glim\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "So wird der Weg zur Seligkeit", "tokens": ["So", "wird", "der", "Weg", "zur", "Se\u00b7lig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Jm Geiste ausposaunet;", "tokens": ["Jm", "Geis\u00b7te", "aus\u00b7po\u00b7sau\u00b7net", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der eine wird durchs Wort ", "tokens": ["Der", "ei\u00b7ne", "wird", "durchs", "Wort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "VAFIN", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der andre steht ", "tokens": ["Der", "and\u00b7re", "steht"], "token_info": ["word", "word", "word"], "pos": ["ART", "PIS", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Der dritte ", "tokens": ["Der", "drit\u00b7te"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Der vierdte wird ", "tokens": ["Der", "vierd\u00b7te", "wird"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "VAFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Der f\u00fcnfte ", "tokens": ["Der", "f\u00fcnf\u00b7te"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.8": {"text": "Der sechste wird ", "tokens": ["Der", "sechs\u00b7te", "wird"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "VAFIN"], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Doch dencken wir in Wahrheit nicht", "tokens": ["Doch", "den\u00b7cken", "wir", "in", "Wahr\u00b7heit", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gott sey bey uns alleine;", "tokens": ["Gott", "sey", "bey", "uns", "al\u00b7lei\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "PPER", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wir sehen: Wie so manches Licht", "tokens": ["Wir", "se\u00b7hen", ":", "Wie", "so", "man\u00b7ches", "Licht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVINF", "$.", "PWAV", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auch andrer Orten scheine;", "tokens": ["Auch", "an\u00b7drer", "Or\u00b7ten", "schei\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Da pflegen wir dann froh zu seyn,", "tokens": ["Da", "pfle\u00b7gen", "wir", "dann", "froh", "zu", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJD", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und uns nicht sehr zu sperren,", "tokens": ["Und", "uns", "nicht", "sehr", "zu", "sper\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PTKNEG", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und dienen einem HErren.", "tokens": ["Und", "die\u00b7nen", "ei\u00b7nem", "Her\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Herr JEsu! deines Hertzens Glut,", "tokens": ["Herr", "Je\u00b7su", "!", "dei\u00b7nes", "Hert\u00b7zens", "Glut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$.", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die vor den Vater eifert,", "tokens": ["Die", "vor", "den", "Va\u00b7ter", "ei\u00b7fert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wor\u00fcber Satan grimmig thut,", "tokens": ["Wo\u00b7r\u00fc\u00b7ber", "Sa\u00b7tan", "grim\u00b7mig", "thut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und seine Secte geifert,", "tokens": ["Und", "sei\u00b7ne", "Sec\u00b7te", "gei\u00b7fert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die hat uns Br\u00fcder lange schon", "tokens": ["Die", "hat", "uns", "Br\u00fc\u00b7der", "lan\u00b7ge", "schon"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PPER", "NN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zu einem Geist vereinigt,", "tokens": ["Zu", "ei\u00b7nem", "Geist", "ver\u00b7ei\u00b7nigt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und unsre Liebe hat der Sohn", "tokens": ["Und", "uns\u00b7re", "Lie\u00b7be", "hat", "der", "Sohn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der Liebe wohl gereinigt.", "tokens": ["Der", "Lie\u00b7be", "wohl", "ge\u00b7rei\u00b7nigt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Du guter Heyland bind uns doch", "tokens": ["Du", "gu\u00b7ter", "Hey\u00b7land", "bind", "uns", "doch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "VAFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Je mehr und mehr zusammen;", "tokens": ["Je", "mehr", "und", "mehr", "zu\u00b7sam\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KON", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "O spann uns an ein gleiches Joch,", "tokens": ["O", "spann", "uns", "an", "ein", "glei\u00b7ches", "Joch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Entz\u00fcnde gleiche Flammen;", "tokens": ["Ent\u00b7z\u00fcn\u00b7de", "glei\u00b7che", "Flam\u00b7men", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Erneure auch von Zeit zu Zeit", "tokens": ["Er\u00b7neu\u00b7re", "auch", "von", "Zeit", "zu", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "APPR", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den Eyd bey deinen Fahnen,", "tokens": ["Den", "Eyd", "bey", "dei\u00b7nen", "Fah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und mehr die Lieb insonderheit", "tokens": ["Und", "mehr", "die", "Lieb", "in\u00b7son\u00b7der\u00b7heit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Durch hertzliches Ermahnen.", "tokens": ["Durch", "hertz\u00b7li\u00b7ches", "Er\u00b7mah\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}