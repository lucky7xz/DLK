{"textgrid.poem.59458": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der Gefangene", "genre": "verse", "period": "N.A.", "pub_year": 1812, "urn": "N.A.", "language": ["de:0.71", "da:0.28"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In goldner Morgenstunde,", "tokens": ["In", "gold\u00b7ner", "Mor\u00b7gen\u00b7stun\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Weil alles freudig stand,", "tokens": ["Weil", "al\u00b7les", "freu\u00b7dig", "stand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da ritt im heitern Grunde", "tokens": ["Da", "ritt", "im", "hei\u00b7tern", "Grun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein Ritter \u00fcber Land.", "tokens": ["Ein", "Rit\u00b7ter", "\u00fc\u00b7ber", "Land", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Rings sangen auf das beste", "tokens": ["Rings", "san\u00b7gen", "auf", "das", "bes\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die V\u00f6glein mannigfalt,", "tokens": ["Die", "V\u00f6\u00b7glein", "man\u00b7nig\u00b7falt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es sch\u00fcttelte die \u00c4ste", "tokens": ["Es", "sch\u00fct\u00b7tel\u00b7te", "die", "\u00c4s\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vor Lust der gr\u00fcne Wald.", "tokens": ["Vor", "Lust", "der", "gr\u00fc\u00b7ne", "Wald", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Den Nacken, stolz gebogen,", "tokens": ["Den", "Na\u00b7cken", ",", "stolz", "ge\u00b7bo\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Klopft er dem R\u00f6sselein \u2013", "tokens": ["Klopft", "er", "dem", "R\u00f6s\u00b7sel\u00b7ein", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "So ist er hingezogen", "tokens": ["So", "ist", "er", "hin\u00b7ge\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Tief in den Wald hinein.", "tokens": ["Tief", "in", "den", "Wald", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Sein Ro\u00df hat er getrieben,", "tokens": ["Sein", "Ro\u00df", "hat", "er", "ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihn trieb der frische Mut:", "tokens": ["Ihn", "trieb", "der", "fri\u00b7sche", "Mut", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbist alles fern geblieben,", "tokens": ["\u00bb", "ist", "al\u00b7les", "fern", "ge\u00b7blie\u00b7ben", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PIS", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So ist mir wohl und gut!\u00ab", "tokens": ["So", "ist", "mir", "wohl", "und", "gut", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "KON", "ADJD", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Mit Freuden mu\u00dft er sehen", "tokens": ["Mit", "Freu\u00b7den", "mu\u00dft", "er", "se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im Wald ein' gr\u00fcne Au,", "tokens": ["Im", "Wald", "ein'", "gr\u00fc\u00b7ne", "Au", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo Br\u00fcnnlein k\u00fchle gehen,", "tokens": ["Wo", "Br\u00fcnn\u00b7lein", "k\u00fch\u00b7le", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von Blumen rot und blau.", "tokens": ["Von", "Blu\u00b7men", "rot", "und", "blau", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Vom Ro\u00df ist er gesprungen,", "tokens": ["Vom", "Ro\u00df", "ist", "er", "ge\u00b7sprun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Legt' sich zum k\u00fchlen Bach,", "tokens": ["Legt'", "sich", "zum", "k\u00fch\u00b7len", "Bach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Wellen lieblich klungen,", "tokens": ["Die", "Wel\u00b7len", "lieb\u00b7lich", "klun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das ganze Herz zog nach.", "tokens": ["Das", "gan\u00b7ze", "Herz", "zog", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "So gr\u00fcne war der Rasen,", "tokens": ["So", "gr\u00fc\u00b7ne", "war", "der", "Ra\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Es rauschte Bach und Baum,", "tokens": ["Es", "rauschte", "Bach", "und", "Baum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "KON", "NE", "$,"], "meter": "--+-+", "measure": "anapaest.init"}, "line.3": {"text": "Sein Ro\u00df t\u00e4t stille grasen,", "tokens": ["Sein", "Ro\u00df", "t\u00e4t", "stil\u00b7le", "gra\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und alles wie ein Traum.", "tokens": ["Und", "al\u00b7les", "wie", "ein", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Die Wolken sah er gehen,", "tokens": ["Die", "Wol\u00b7ken", "sah", "er", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die schifften immerzu,", "tokens": ["Die", "schiff\u00b7ten", "im\u00b7mer\u00b7zu", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er konnt nicht widerstehen \u2013", "tokens": ["Er", "konnt", "nicht", "wi\u00b7der\u00b7ste\u00b7hen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Augen sanken ihm zu.", "tokens": ["Die", "Au\u00b7gen", "san\u00b7ken", "ihm", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.9": {"line.1": {"text": "Nun h\u00f6rt' er Stimmen rinnen,", "tokens": ["Nun", "h\u00f6rt'", "er", "Stim\u00b7men", "rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Als wie der Liebsten Gru\u00df,", "tokens": ["Als", "wie", "der", "Liebs\u00b7ten", "Gru\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er konnt sich nicht besinnen \u2013", "tokens": ["Er", "konnt", "sich", "nicht", "be\u00b7sin\u00b7nen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bis ihn erweckt' ein Ku\u00df.", "tokens": ["Bis", "ihn", "er\u00b7weckt'", "ein", "Ku\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Wie pr\u00e4chtig gl\u00e4nzt' die Aue!", "tokens": ["Wie", "pr\u00e4ch\u00b7tig", "gl\u00e4nzt'", "die", "Au\u00b7e", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie Gold der Quell nun flo\u00df,", "tokens": ["Wie", "Gold", "der", "Quell", "nun", "flo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und einer s\u00fc\u00dfen Fraue", "tokens": ["Und", "ei\u00b7ner", "s\u00fc\u00b7\u00dfen", "Frau\u00b7e"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Lag er im weichen Scho\u00df.", "tokens": ["Lag", "er", "im", "wei\u00b7chen", "Scho\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "\u00bbherr Ritter! wollt Ihr wohnen", "tokens": ["\u00bb", "herr", "Rit\u00b7ter", "!", "wollt", "Ihr", "woh\u00b7nen"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "NN", "NN", "$.", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bei mir im gr\u00fcnen Haus:", "tokens": ["Bei", "mir", "im", "gr\u00fc\u00b7nen", "Haus", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Aus allen Blumenkronen", "tokens": ["Aus", "al\u00b7len", "Blu\u00b7men\u00b7kro\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wind ich Euch einen Strau\u00df!", "tokens": ["Wind", "ich", "Euch", "ei\u00b7nen", "Strau\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "ART", "NE", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.12": {"line.1": {"text": "Der Wald ringsum wird wachen,", "tokens": ["Der", "Wald", "ring\u00b7sum", "wird", "wa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie wir beisammen sein,", "tokens": ["Wie", "wir", "bei\u00b7sam\u00b7men", "sein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVPP", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Kuckuck schelmisch lachen,", "tokens": ["Der", "Ku\u00b7ckuck", "schel\u00b7misch", "la\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und alles fr\u00f6hlich sein.\u00ab", "tokens": ["Und", "al\u00b7les", "fr\u00f6h\u00b7lich", "sein", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PIS", "ADJD", "VAINF", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Es bog ihr Angesichte", "tokens": ["Es", "bog", "ihr", "An\u00b7ge\u00b7sich\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Auf ihn, den s\u00fc\u00dfen Leib,", "tokens": ["Auf", "ihn", ",", "den", "s\u00fc\u00b7\u00dfen", "Leib", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Schaut' mit den Augen lichte", "tokens": ["Schaut'", "mit", "den", "Au\u00b7gen", "lich\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Das wundersch\u00f6ne Weib.", "tokens": ["Das", "wun\u00b7der\u00b7sch\u00f6\u00b7ne", "Weib", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Sie nahm sein'n Helm herunter,", "tokens": ["Sie", "nahm", "sein'n", "Helm", "her\u00b7un\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "L\u00f6st' Krause ihm und Bund,", "tokens": ["L\u00f6st'", "Krau\u00b7se", "ihm", "und", "Bund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PPER", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Spielt' mit den Locken munter,", "tokens": ["Spielt'", "mit", "den", "Lo\u00b7cken", "mun\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "K\u00fc\u00dft' ihm den roten Mund.", "tokens": ["K\u00fc\u00dft'", "ihm", "den", "ro\u00b7ten", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Und spielt' viel s\u00fc\u00dfe Spiele", "tokens": ["Und", "spielt'", "viel", "s\u00fc\u00b7\u00dfe", "Spie\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wohl in geheimer Lust,", "tokens": ["Wohl", "in", "ge\u00b7hei\u00b7mer", "Lust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es flog so k\u00fchl und schw\u00fcle", "tokens": ["Es", "flog", "so", "k\u00fchl", "und", "schw\u00fc\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ihm um die offne Brust.", "tokens": ["Ihm", "um", "die", "off\u00b7ne", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Um ihn nun t\u00e4t sie schlagen", "tokens": ["Um", "ihn", "nun", "t\u00e4t", "sie", "schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "PPER", "ADV", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Arme weich und blo\u00df,", "tokens": ["Die", "Ar\u00b7me", "weich", "und", "blo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er konnte nichts mehr sagen,", "tokens": ["Er", "konn\u00b7te", "nichts", "mehr", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIS", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie lie\u00df ihn nicht mehr los.", "tokens": ["Sie", "lie\u00df", "ihn", "nicht", "mehr", "los", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.17": {"line.1": {"text": "Und diese Au zur Stunde", "tokens": ["Und", "die\u00b7se", "Au", "zur", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PDAT", "NN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ward ein kristallnes Schlo\u00df,", "tokens": ["Ward", "ein", "kris\u00b7tall\u00b7nes", "Schlo\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Bach ein Strom, gewunden", "tokens": ["Der", "Bach", "ein", "Strom", ",", "ge\u00b7wun\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "ART", "NN", "$,", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ringsum, gewaltig flo\u00df.", "tokens": ["Ring\u00b7sum", ",", "ge\u00b7wal\u00b7tig", "flo\u00df", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "ADJD", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.18": {"line.1": {"text": "Auf diesem Strome gingen", "tokens": ["Auf", "die\u00b7sem", "Stro\u00b7me", "gin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Viel Schiffe wohl vorbei,", "tokens": ["Viel", "Schif\u00b7fe", "wohl", "vor\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es konnt ihn keines bringen", "tokens": ["Es", "konnt", "ihn", "kei\u00b7nes", "brin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "PIS", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Aus b\u00f6ser Zauberei.", "tokens": ["Aus", "b\u00f6\u00b7ser", "Zau\u00b7be\u00b7rei", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}