{"textgrid.poem.65197": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Stets \u00f6der wird's auf meinem Pfade,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Stets \u00f6der wird's auf meinem Pfade,", "tokens": ["Stets", "\u00f6\u00b7der", "wird's", "auf", "mei\u00b7nem", "Pfa\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Am Herzen nagt mir dumpfe Pein.", "tokens": ["Am", "Her\u00b7zen", "nagt", "mir", "dump\u00b7fe", "Pein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O Hoffnung, du Scheherezade!", "tokens": ["O", "Hoff\u00b7nung", ",", "du", "Sche\u00b7he\u00b7re\u00b7za\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Wieg' mich mit deinen M\u00e4rchen ein!", "tokens": ["Wieg'", "mich", "mit", "dei\u00b7nen", "M\u00e4r\u00b7chen", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Nacht, der ich entgegenschreite,", "tokens": ["Die", "Nacht", ",", "der", "ich", "ent\u00b7ge\u00b7gen\u00b7schrei\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Verh\u00fclle mir mit ros'gem Flor,", "tokens": ["Ver\u00b7h\u00fcl\u00b7le", "mir", "mit", "ros'\u00b7gem", "Flor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und gib mir tr\u00f6stend zum Geleite", "tokens": ["Und", "gib", "mir", "tr\u00f6s\u00b7tend", "zum", "Ge\u00b7lei\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "PPER", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Holdsel'ger Zukunftstr\u00e4ume Chor.", "tokens": ["Hold\u00b7sel'\u00b7ger", "Zu\u00b7kunfts\u00b7tr\u00e4u\u00b7me", "Chor", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wird ihnen die Erf\u00fcllung nimmer", "tokens": ["Wird", "ih\u00b7nen", "die", "Er\u00b7f\u00fcl\u00b7lung", "nim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In diesem schwanken Erdenhaus,", "tokens": ["In", "die\u00b7sem", "schwan\u00b7ken", "Er\u00b7den\u00b7haus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So breite deinen milden Schimmer", "tokens": ["So", "brei\u00b7te", "dei\u00b7nen", "mil\u00b7den", "Schim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Weit \u00fcber's dunkle Grab hinaus.", "tokens": ["Weit", "\u00fc\u00b7ber's", "dunk\u00b7le", "Grab", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "O nahe mir wie Fr\u00fchlingsrauschen,", "tokens": ["O", "na\u00b7he", "mir", "wie", "Fr\u00fch\u00b7lings\u00b7rau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPER", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vor dem des Eises Rinde springt,", "tokens": ["Vor", "dem", "des", "Ei\u00b7ses", "Rin\u00b7de", "springt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und la\u00df mich deinen M\u00e4rchen lauschen,", "tokens": ["Und", "la\u00df", "mich", "dei\u00b7nen", "M\u00e4r\u00b7chen", "lau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bis froh mein Herz davon erklingt!", "tokens": ["Bis", "froh", "mein", "Herz", "da\u00b7von", "er\u00b7klingt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "PPOSAT", "NN", "PAV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Die Hoffnung drauf: \u00bbEin M\u00e4rchen nennst", "tokens": ["Die", "Hoff\u00b7nung", "drauf", ":", "\u00bb", "Ein", "M\u00e4r\u00b7chen", "nennst"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "$.", "$(", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du selbst, was ich zu k\u00fcnden wei\u00df,", "tokens": ["Du", "selbst", ",", "was", "ich", "zu", "k\u00fcn\u00b7den", "wei\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "PWS", "PPER", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und sagst damit, da\u00df du erkennst,", "tokens": ["Und", "sagst", "da\u00b7mit", ",", "da\u00df", "du", "er\u00b7kennst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie fern und fremd du meinem Kreis.", "tokens": ["Wie", "fern", "und", "fremd", "du", "mei\u00b7nem", "Kreis", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "KON", "ADJD", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Zu tief ist deiner Stirn das Zeichen", "tokens": ["Zu", "tief", "ist", "dei\u00b7ner", "Stirn", "das", "Zei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKA", "ADJD", "VAFIN", "PPOSAT", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ruchlosen Zweifels eingebrannt!", "tokens": ["Ruch\u00b7lo\u00b7sen", "Zwei\u00b7fels", "ein\u00b7ge\u00b7brannt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Nie wieder wird er von dir weichen,", "tokens": ["Nie", "wie\u00b7der", "wird", "er", "von", "dir", "wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du bist und bleibst an ihn gebannt.", "tokens": ["Du", "bist", "und", "bleibst", "an", "ihn", "ge\u00b7bannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KON", "VVFIN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Und so ist mir die Macht benommen,", "tokens": ["Und", "so", "ist", "mir", "die", "Macht", "be\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Dir vorzuspiegeln holden Wahn.", "tokens": ["Dir", "vor\u00b7zu\u00b7spie\u00b7geln", "hol\u00b7den", "Wahn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mein Zuspruch k\u00f6nnte dir nicht frommen,", "tokens": ["Mein", "Zu\u00b7spruch", "k\u00f6nn\u00b7te", "dir", "nicht", "from\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "PTKNEG", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Denn, ach! Du glaubtest nicht daran!\u00ab", "tokens": ["Denn", ",", "ach", "!", "Du", "glaub\u00b7test", "nicht", "da\u00b7ran", "!", "\u00ab"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "$,", "ITJ", "$.", "PPER", "VVFIN", "PTKNEG", "PAV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}