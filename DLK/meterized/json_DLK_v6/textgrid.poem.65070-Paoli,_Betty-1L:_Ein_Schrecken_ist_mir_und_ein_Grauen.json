{"textgrid.poem.65070": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Schrecken ist mir und ein Grauen", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Schrecken ist mir und ein Grauen", "tokens": ["Ein", "Schre\u00b7cken", "ist", "mir", "und", "ein", "Grau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPER", "KON", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Aehnlichkeit, von der man spricht,", "tokens": ["Die", "A\u00b7ehn\u00b7lich\u00b7keit", ",", "von", "der", "man", "spricht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PRELS", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Ich will nicht deine Z\u00fcge schauen", "tokens": ["Ich", "will", "nicht", "dei\u00b7ne", "Z\u00fc\u00b7ge", "schau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PTKNEG", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In einem fremden Angesicht! \u2013", "tokens": ["In", "ei\u00b7nem", "frem\u00b7den", "An\u00b7ge\u00b7sicht", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Vernehmen nicht aus fremdem Munde", "tokens": ["Ver\u00b7neh\u00b7men", "nicht", "aus", "frem\u00b7dem", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Will ich der theuern Stimme Klang,", "tokens": ["Will", "ich", "der", "theu\u00b7ern", "Stim\u00b7me", "Klang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die einst wie eine Himmelskunde", "tokens": ["Die", "einst", "wie", "ei\u00b7ne", "Him\u00b7mels\u00b7kun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Beseligend mein Herz durchdrang.", "tokens": ["Be\u00b7se\u00b7li\u00b7gend", "mein", "Herz", "durch\u00b7drang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Denn deine Sch\u00f6nheit ist der Schimmer,", "tokens": ["Denn", "dei\u00b7ne", "Sch\u00f6n\u00b7heit", "ist", "der", "Schim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der hell durch meine N\u00e4chte bricht;", "tokens": ["Der", "hell", "durch", "mei\u00b7ne", "N\u00e4ch\u00b7te", "bricht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Glanz von oben tr\u00fcbe nimmer", "tokens": ["Den", "Glanz", "von", "o\u00b7ben", "tr\u00fc\u00b7be", "nim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein falschem Schein erborgtes Licht.", "tokens": ["Ein", "fal\u00b7schem", "Schein", "er\u00b7borg\u00b7tes", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Mir ist von myst'schen Geisters\u00e4ngen", "tokens": ["Mir", "ist", "von", "myst'\u00b7schen", "Geis\u00b7ter\u00b7s\u00e4n\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein s\u00fc\u00dfer Wiederhall dein Wort,", "tokens": ["Ein", "s\u00fc\u00b7\u00dfer", "Wie\u00b7der\u00b7hall", "dein", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und unvermischt mit ird'schen Kl\u00e4ngen", "tokens": ["Und", "un\u00b7ver\u00b7mischt", "mit", "ird'\u00b7schen", "Kl\u00e4n\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hall' es in meiner Seele fort.", "tokens": ["Hall'", "es", "in", "mei\u00b7ner", "See\u00b7le", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}