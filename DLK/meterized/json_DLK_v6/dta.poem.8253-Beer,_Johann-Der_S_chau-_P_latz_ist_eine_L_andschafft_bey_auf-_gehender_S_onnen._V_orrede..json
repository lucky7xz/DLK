{"dta.poem.8253": {"metadata": {"author": {"name": "Beer, Johann", "birth": "N.A.", "death": "N.A."}, "title": "Der  S chau- P latz ist eine  L andschafft bey auf-  \n gehender  S onnen.  \n  V orrede.", "genre": "Lyrik, Libretto", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-25222-5", "language": ["de:0.99"], "booktitle": "Beer, Johann: Antonius R\u00f6mischer Triumvir. Wei\u00dfenfels, 1697."}, "poem": {"stanza.1": {"line.1": {"text": "Lohn und Strafen theil ich ein/", "tokens": ["Lohn", "und", "Stra\u00b7fen", "theil", "ich", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KOUS", "PPER", "ART", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie der Menschen Thaten seyn/", "tokens": ["Wie", "der", "Men\u00b7schen", "Tha\u00b7ten", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "VAINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wer mit reinen Dienst mich ehret/", "tokens": ["Wer", "mit", "rei\u00b7nen", "Dienst", "mich", "eh\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ADJA", "NN", "PPER", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dem wird alles di\u00df gew\u00e4hret/", "tokens": ["Dem", "wird", "al\u00b7les", "di\u00df", "ge\u00b7w\u00e4h\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PIS", "PDS", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Was er sich nur w\u00fcndschen kan/", "tokens": ["Was", "er", "sich", "nur", "w\u00fcnd\u00b7schen", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PRF", "ADV", "VVINF", "VMFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Denn ihm bleib ich zugethan.", "tokens": ["Denn", "ihm", "bleib", "ich", "zu\u00b7ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}