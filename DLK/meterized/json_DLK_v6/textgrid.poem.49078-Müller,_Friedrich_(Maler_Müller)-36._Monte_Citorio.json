{"textgrid.poem.49078": {"metadata": {"author": {"name": "M\u00fcller, Friedrich (Maler M\u00fcller)", "birth": "N.A.", "death": "N.A."}, "title": "36. Monte Citorio", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf der Stiege des Hauses, wo die r\u00f6mische Themis", "tokens": ["Auf", "der", "Stie\u00b7ge", "des", "Hau\u00b7ses", ",", "wo", "die", "r\u00f6\u00b7mi\u00b7sche", "The\u00b7mis"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,", "PWAV", "ART", "ADJA", "NN"], "meter": "+-+--+-+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "H\u00e4lt die richtende Waag', zeiget hoch sich ein Stein:", "tokens": ["H\u00e4lt", "die", "rich\u00b7ten\u00b7de", "Waag'", ",", "zei\u00b7get", "hoch", "sich", "ein", "Stein", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,", "VVFIN", "ADJD", "PRF", "ART", "NN", "$."], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Phoebus, wie er den Marsias schindet. B\u00fcrger, betrachtets,", "tokens": ["Phoe\u00b7bus", ",", "wie", "er", "den", "Mar\u00b7si\u00b7as", "schin\u00b7det", ".", "B\u00fcr\u00b7ger", ",", "be\u00b7trach\u00b7tets", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "PWAV", "PPER", "ART", "NN", "VVFIN", "$.", "NN", "$,", "VVFIN", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Lehrets der Eingang doch klar, wie man's mit B\u00fcrgern hier treibt!", "tokens": ["Leh\u00b7rets", "der", "Ein\u00b7gang", "doch", "klar", ",", "wie", "man's", "mit", "B\u00fcr\u00b7gern", "hier", "treibt", "!"], "token_info": ["word", "word", "w