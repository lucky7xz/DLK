{"dta.poem.20622": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "An  \n die Stadt Berlin  \n wegen  \n Sr. K\u00f6nigl. Hoheit  \n des                  \n  Prinzen und Feldherrn  Heinrichs .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Du der goldnen Zeit zu Dir gew\u00fcnschte Tage                 ", "tokens": ["Die", "Du", "der", "gold\u00b7nen", "Zeit", "zu", "Dir", "ge\u00b7w\u00fcnschte", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "ADJA", "NN", "APPR", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+----", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Mit Freudenspiel und Tanz empf\u00e4ngst,", "tokens": ["Mit", "Freu\u00b7den\u00b7spiel", "und", "Tanz", "emp\u00b7f\u00e4ngst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und oft mit wiederholter Frage", "tokens": ["Und", "oft", "mit", "wie\u00b7der\u00b7hol\u00b7ter", "Fra\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dich um des T\u00fcrken wei\u00dfbewundne Stirne", "tokens": ["Dich", "um", "des", "T\u00fcr\u00b7ken", "wei\u00df\u00b7be\u00b7wund\u00b7ne", "Stir\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "---+-+-+-+-", "measure": "unknown.measure.tetra"}, "line.5": {"text": "dr\u00e4ngst; ", "tokens": ["dr\u00e4ngst", ";"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-", "measure": "single.down"}}, "stanza.2": {"line.1": {"text": "Sieh Du, Neugierige! wie dort in Feuerstr\u00f6men", "tokens": ["Sieh", "Du", ",", "Neu\u00b7gie\u00b7ri\u00b7ge", "!", "wie", "dort", "in", "Feu\u00b7er\u00b7str\u00f6\u00b7men"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "PPER", "$,", "NN", "$.", "PWAV", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Sonne vor Gestirnen gl\u00e4nzt,", "tokens": ["Die", "Son\u00b7ne", "vor", "Ge\u00b7stir\u00b7nen", "gl\u00e4nzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So gl\u00e4nzt vor allen Diademen,", "tokens": ["So", "gl\u00e4nzt", "vor", "al\u00b7len", "Di\u00b7a\u00b7de\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Siegeslorbeerschmuck, der einen Helden", "tokens": ["Der", "Sie\u00b7ges\u00b7lor\u00b7beer\u00b7schmuck", ",", "der", "ei\u00b7nen", "Hel\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "kr\u00e4nzt,", "tokens": ["kr\u00e4nzt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}}, "stanza.3": {"line.1": {"text": "Den ein bezwungnes Volk mit Blumen warf, und neue", "tokens": ["Den", "ein", "be\u00b7zwung\u00b7nes", "Volk", "mit", "Blu\u00b7men", "warf", ",", "und", "neu\u00b7e"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ART", "ADJA", "NN", "APPR", "NN", "VVFIN", "$,", "KON", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gel\u00fcbde f\u00fcr Ihn ausgedacht;", "tokens": ["Ge\u00b7l\u00fcb\u00b7de", "f\u00fcr", "Ihn", "aus\u00b7ge\u00b7dacht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dem niemals Tadel oder Reue", "tokens": ["Dem", "nie\u00b7mals", "Ta\u00b7del", "o\u00b7der", "Reu\u00b7e"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nach einer k\u00fchnen That die Wange roth gemacht;", "tokens": ["Nach", "ei\u00b7ner", "k\u00fch\u00b7nen", "That", "die", "Wan\u00b7ge", "roth", "ge\u00b7macht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Der kein Lucullus war den tragenden Soldaten,", "tokens": ["Der", "kein", "Lu\u00b7cul\u00b7lus", "war", "den", "tra\u00b7gen\u00b7den", "Sol\u00b7da\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NE", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die ihrer Waffen Last gedr\u00fcckt;", "tokens": ["Die", "ih\u00b7rer", "Waf\u00b7fen", "Last", "ge\u00b7dr\u00fcckt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "NE", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wegen seiner gro\u00dfen Thaten", "tokens": ["Und", "we\u00b7gen", "sei\u00b7ner", "gro\u00b7\u00dfen", "Tha\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nie einen Kriegesknecht ver\u00e4chtlich angeblickt;", "tokens": ["Nie", "ei\u00b7nen", "Krie\u00b7ges\u00b7knecht", "ver\u00b7\u00e4cht\u00b7lich", "an\u00b7ge\u00b7blickt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Der alle Fabier mit den geschonten Heeren", "tokens": ["Der", "al\u00b7le", "Fa\u00b7bier", "mit", "den", "ge\u00b7schon\u00b7ten", "Hee\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Zur\u00fccke strahlet \u2014 O Berlin!", "tokens": ["Zu\u00b7r\u00fc\u00b7cke", "strah\u00b7let", "O", "Ber\u00b7lin", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$(", "NE", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sey dankbar, setze zu Alt\u00e4ren", "tokens": ["Sey", "dank\u00b7bar", ",", "set\u00b7ze", "zu", "Al\u00b7t\u00e4\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VAIMP", "ADJD", "$,", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sein Bild an ", "tokens": ["Sein", "Bild", "an"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "PTKVZ"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Flaminin", "tokens": ["Fla\u00b7mi\u00b7nin"], "token_info": ["word"], "pos": ["NN"], "meter": "+--", "measure": "dactylic.init"}}, "stanza.6": {"line.1": {"text": "War nicht so werth, als Er, zu gl\u00e4nzen wie die G\u00f6tter", "tokens": ["War", "nicht", "so", "werth", ",", "als", "Er", ",", "zu", "gl\u00e4n\u00b7zen", "wie", "die", "G\u00f6t\u00b7ter"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PTKNEG", "ADV", "ADJD", "$,", "KOUS", "PPER", "$,", "PTKZU", "VVINF", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In ihrer Tempel Ueberschrift;", "tokens": ["In", "ih\u00b7rer", "Tem\u00b7pel", "Ue\u00b7ber\u00b7schrift", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Rom hatte niemals einen Retter,", "tokens": ["Rom", "hat\u00b7te", "nie\u00b7mals", "ei\u00b7nen", "Ret\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den ", "tokens": ["Den"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "\u00fcbertrift.", "tokens": ["\u00fc\u00b7bert\u00b7rift", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}