{"textgrid.poem.62433": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "Beichte", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir haben alle schwer ges\u00fcndigt,", "tokens": ["Wir", "ha\u00b7ben", "al\u00b7le", "schwer", "ge\u00b7s\u00fcn\u00b7digt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wir mangeln allesammt an Ruhm,", "tokens": ["Wir", "man\u00b7geln", "al\u00b7le\u00b7sammt", "an", "Ruhm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Man hat, o Herr! uns oft verk\u00fcndigt", "tokens": ["Man", "hat", ",", "o", "Herr", "!", "uns", "oft", "ver\u00b7k\u00fcn\u00b7digt"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "VAFIN", "$,", "FM", "NN", "$.", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Freiheit Evangelium;", "tokens": ["Der", "Frei\u00b7heit", "E\u00b7van\u00b7ge\u00b7li\u00b7um", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wir aber hatten uns entm\u00fcndigt,", "tokens": ["Wir", "a\u00b7ber", "hat\u00b7ten", "uns", "ent\u00b7m\u00fcn\u00b7digt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das Salz der Erde wurde dumm;", "tokens": ["Das", "Salz", "der", "Er\u00b7de", "wur\u00b7de", "dumm", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So F\u00fcrst als B\u00fcrger, so der Adel,", "tokens": ["So", "F\u00fcrst", "als", "B\u00fcr\u00b7ger", ",", "so", "der", "A\u00b7del", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KOUS", "NN", "$,", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Hier ist nicht einer ohne Tadel.", "tokens": ["Hier", "ist", "nicht", "ei\u00b7ner", "oh\u00b7ne", "Ta\u00b7del", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PTKNEG", "ART", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wir haben an der bunten Wange", "tokens": ["Wir", "ha\u00b7ben", "an", "der", "bun\u00b7ten", "Wan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der alten Babel uns berauscht,", "tokens": ["Der", "al\u00b7ten", "Ba\u00b7bel", "uns", "be\u00b7rauscht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und ihrem frechen Lustgesange", "tokens": ["Und", "ih\u00b7rem", "fre\u00b7chen", "Lust\u00b7ge\u00b7san\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit keuschem deutschem Ohr gelauscht,", "tokens": ["Mit", "keu\u00b7schem", "deut\u00b7schem", "Ohr", "ge\u00b7lauscht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Kraft entschwand uns vor dem Klange,", "tokens": ["Die", "Kraft", "ent\u00b7schwand", "uns", "vor", "dem", "Klan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Im Taumel haben wir vertauscht", "tokens": ["Im", "Tau\u00b7mel", "ha\u00b7ben", "wir", "ver\u00b7tauscht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mit eklem Rothwelsch der Garonne", "tokens": ["Mit", "ek\u00b7lem", "Roth\u00b7welsch", "der", "Ga\u00b7ron\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die Sprache Teuts, der Helden Wonne.", "tokens": ["Die", "Spra\u00b7che", "Teuts", ",", "der", "Hel\u00b7den", "Won\u00b7ne", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Da kamen \u00fcber uns gezogen", "tokens": ["Da", "ka\u00b7men", "\u00fc\u00b7ber", "uns", "ge\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Schmach, die Gr\u00e4uel ohne Zahl,", "tokens": ["Die", "Schmach", ",", "die", "Gr\u00e4u\u00b7el", "oh\u00b7ne", "Zahl", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wir bauten mit am Siegesbogen,", "tokens": ["Wir", "bau\u00b7ten", "mit", "am", "Sie\u00b7ges\u00b7bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wir sa\u00dfen mit beim G\u00f6tzenmahl,", "tokens": ["Wir", "sa\u00b7\u00dfen", "mit", "beim", "G\u00f6t\u00b7zen\u00b7mahl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die nie das freie Haupt gebogen,", "tokens": ["Die", "nie", "das", "frei\u00b7e", "Haupt", "ge\u00b7bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die M\u00e4nner stolz und rein wie Stahl,", "tokens": ["Die", "M\u00e4n\u00b7ner", "stolz", "und", "rein", "wie", "Stahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sie webten mit am Sklavenbande,", "tokens": ["Sie", "web\u00b7ten", "mit", "am", "Skla\u00b7ven\u00b7ban\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Sie prunkten mit dem Schmuck der Schande.", "tokens": ["Sie", "prunk\u00b7ten", "mit", "dem", "Schmuck", "der", "Schan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Nun Herr! die Binden sind gefallen", "tokens": ["Nun", "Herr", "!", "die", "Bin\u00b7den", "sind", "ge\u00b7fal\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "NN", "$.", "ART", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von H\u00e4nden, wie von Blick und Ohr;", "tokens": ["Von", "H\u00e4n\u00b7den", ",", "wie", "von", "Blick", "und", "Ohr", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PWAV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "La\u00df uns dein gn\u00e4dig Wort erschallen,", "tokens": ["La\u00df", "uns", "dein", "gn\u00e4\u00b7dig", "Wort", "er\u00b7schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "ADJD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sei wieder mit uns wie zuvor.", "tokens": ["Sei", "wie\u00b7der", "mit", "uns", "wie", "zu\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PPER", "KOKOM", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wir nahen uns des Harzes Hallen,", "tokens": ["Wir", "na\u00b7hen", "uns", "des", "Har\u00b7zes", "Hal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wir ziehn durch Vater Hermanns Thor.", "tokens": ["Wir", "ziehn", "durch", "Va\u00b7ter", "Her\u00b7manns", "Thor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "NE", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "O gib, da\u00df unser Blut erkaufe", "tokens": ["O", "gib", ",", "da\u00df", "un\u00b7ser", "Blut", "er\u00b7kau\u00b7fe"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVIMP", "$,", "KOUS", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Des alten Namens Feuertaufe.", "tokens": ["Des", "al\u00b7ten", "Na\u00b7mens", "Feu\u00b7er\u00b7tau\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Orakel haben l\u00e4ngst geklungen,", "tokens": ["O\u00b7ra\u00b7kel", "ha\u00b7ben", "l\u00e4ngst", "ge\u00b7klun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie deuteten des Riesen Fall;", "tokens": ["Sie", "deu\u00b7te\u00b7ten", "des", "Rie\u00b7sen", "Fall", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vor'm heil'gen Lied der Niebelungen", "tokens": ["Vor'm", "heil'\u00b7gen", "Lied", "der", "Nie\u00b7be\u00b7lun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Verstummte schon der fremde Schall,", "tokens": ["Ver\u00b7stumm\u00b7te", "schon", "der", "frem\u00b7de", "Schall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Viel deutsche Schwerter sind geschwungen", "tokens": ["Viel", "deut\u00b7sche", "Schwer\u00b7ter", "sind", "ge\u00b7schwun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Bei Moskau wie bei Roncevall,", "tokens": ["Bei", "Mos\u00b7kau", "wie", "bei", "Ron\u00b7ce\u00b7vall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KOKOM", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Acht Monde f\u00fchrt nun schon die Fehde,", "tokens": ["Acht", "Mon\u00b7de", "f\u00fchrt", "nun", "schon", "die", "Feh\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NE", "VVFIN", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ein Volk von deutscher Art und Rede.", "tokens": ["Ein", "Volk", "von", "deut\u00b7scher", "Art", "und", "Re\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Du ziehst, o Herr! im Siegesfluge", "tokens": ["Du", "ziehst", ",", "o", "Herr", "!", "im", "Sie\u00b7ges\u00b7flu\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "FM", "NN", "$.", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vor deinen treuen Schaaren her;", "tokens": ["Vor", "dei\u00b7nen", "treu\u00b7en", "Schaa\u00b7ren", "her", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Man glaubt nicht mehr dem fremden Truge,", "tokens": ["Man", "glaubt", "nicht", "mehr", "dem", "frem\u00b7den", "Tru\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PTKNEG", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Man glaubt der guten alten M\u00e4r,", "tokens": ["Man", "glaubt", "der", "gu\u00b7ten", "al\u00b7ten", "M\u00e4r", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Donau braust's auf ihrem Zuge", "tokens": ["Die", "Do\u00b7nau", "braust's", "auf", "ih\u00b7rem", "Zu\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NE", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Von Schwaben bis ins schwarze Meer,", "tokens": ["Von", "Schwa\u00b7ben", "bis", "ins", "schwar\u00b7ze", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da\u00df Deutsche nur f\u00fcr Deutsche fechten", "tokens": ["Da\u00df", "Deut\u00b7sche", "nur", "f\u00fcr", "Deut\u00b7sche", "fech\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "ADV", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Nach alter Sitte, alten Rechten.", "tokens": ["Nach", "al\u00b7ter", "Sit\u00b7te", ",", "al\u00b7ten", "Rech\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Du hast uns, Herr! der Schuld entladen,", "tokens": ["Du", "hast", "uns", ",", "Herr", "!", "der", "Schuld", "ent\u00b7la\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$,", "NN", "$.", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Schmach entlud uns unser Schwert;", "tokens": ["Der", "Schmach", "ent\u00b7lud", "uns", "un\u00b7ser", "Schwert", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O flie\u00df uns ferner, Quell der Gnaden,", "tokens": ["O", "flie\u00df", "uns", "fer\u00b7ner", ",", "Quell", "der", "Gna\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "$,", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wir sammeln uns um freien Herd,", "tokens": ["Wir", "sam\u00b7meln", "uns", "um", "frei\u00b7en", "Herd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wir bergen tief in heil'ger Laden", "tokens": ["Wir", "ber\u00b7gen", "tief", "in", "heil'\u00b7ger", "La\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ADJA", "NN"], "meter": "---+-+-+-", "measure": "unknown.measure.tri"}, "line.6": {"text": "Die Bundes-Worte fromm und werth,", "tokens": ["Die", "Bun\u00b7des\u00b7Wor\u00b7te", "fromm", "und", "werth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der junge Bund voll Lust und Ehren,", "tokens": ["Der", "jun\u00b7ge", "Bund", "voll", "Lust", "und", "Eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der graue Bund soll ewig w\u00e4hren.", "tokens": ["Der", "grau\u00b7e", "Bund", "soll", "e\u00b7wig", "w\u00e4h\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}