{"textgrid.poem.56333": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "Der einst zum Grabstein Bl\u00fcchers bestimmte Granitblock am Zobten", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was dieser m\u00e4cht'ge Stein der k\u00fcnft'gen Zeit", "tokens": ["Was", "die\u00b7ser", "m\u00e4cht'\u00b7ge", "Stein", "der", "k\u00fcnft'\u00b7gen", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PDAT", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Von uns erz\u00e4hlen wird? ihr m\u00f6gt ihn fragen;", "tokens": ["Von", "uns", "er\u00b7z\u00e4h\u00b7len", "wird", "?", "ihr", "m\u00f6gt", "ihn", "fra\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVINF", "VAFIN", "$.", "PPER", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Er wird euch schroff und kalt die Antwort sagen:", "tokens": ["Er", "wird", "euch", "schroff", "und", "kalt", "die", "Ant\u00b7wort", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVFIN", "KON", "ADJD", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich bin der Denkstein der Vergessenheit.", "tokens": ["Ich", "bin", "der", "Denk\u00b7stein", "der", "Ver\u00b7ges\u00b7sen\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Um Freiheit ward und Unabh\u00e4ngigkeit", "tokens": ["Um", "Frei\u00b7heit", "ward", "und", "Un\u00b7ab\u00b7h\u00e4n\u00b7gig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "NN", "VAFIN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Begeistert manche V\u00f6lkerschlacht geschlagen,", "tokens": ["Be\u00b7geis\u00b7tert", "man\u00b7che", "V\u00f6l\u00b7ker\u00b7schlacht", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein Held war V\u00f6lkerf\u00fcrst in diesen Tagen", "tokens": ["Ein", "Held", "war", "V\u00f6l\u00b7ker\u00b7f\u00fcrst", "in", "die\u00b7sen", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Vorw\u00e4rtsf\u00fchrer in den heil'gen Streit.", "tokens": ["Und", "Vor\u00b7w\u00e4rts\u00b7f\u00fch\u00b7rer", "in", "den", "heil'\u00b7gen", "Streit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich ward bestimmt als Grabstein dieses Helden", "tokens": ["Ich", "ward", "be\u00b7stimmt", "als", "Grab\u00b7stein", "die\u00b7ses", "Hel\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "KOKOM", "NN", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der sp\u00e4ten Nachwelt die Begeisterung,", "tokens": ["Der", "sp\u00e4\u00b7ten", "Nach\u00b7welt", "die", "Be\u00b7geis\u00b7te\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-++--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die schnellverrauchende des Tags, zu melden.", "tokens": ["Die", "schnell\u00b7ver\u00b7rau\u00b7chen\u00b7de", "des", "Tags", ",", "zu", "mel\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "ART", "NN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch, als sie her mich zogen, war indessen", "tokens": ["Doch", ",", "als", "sie", "her", "mich", "zo\u00b7gen", ",", "war", "in\u00b7des\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "$,", "KOUS", "PPER", "ADV", "PPER", "VVFIN", "$,", "VAFIN", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Rad der Zeit gerollt in schnellem Schwung,", "tokens": ["Das", "Rad", "der", "Zeit", "ge\u00b7rollt", "in", "schnel\u00b7lem", "Schwung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und er und ich, wir waren schon vergessen.", "tokens": ["Und", "er", "und", "ich", ",", "wir", "wa\u00b7ren", "schon", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "KON", "PPER", "$,", "PPER", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}