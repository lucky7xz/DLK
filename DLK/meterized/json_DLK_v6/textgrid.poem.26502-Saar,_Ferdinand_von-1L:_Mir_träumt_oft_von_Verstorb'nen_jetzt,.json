{"textgrid.poem.26502": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mir tr\u00e4umt oft von Verstorb'nen jetzt,", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mir tr\u00e4umt oft von Verstorb'nen jetzt,", "tokens": ["Mir", "tr\u00e4umt", "oft", "von", "Ver\u00b7stor\u00b7b'\u00b7nen", "jetzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN", "ADV", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Von solchen, die ich l\u00e4ngst vergessen,", "tokens": ["Von", "sol\u00b7chen", ",", "die", "ich", "l\u00e4ngst", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "$,", "PRELS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die nie erfreut mich, nie verletzt", "tokens": ["Die", "nie", "er\u00b7freut", "mich", ",", "nie", "ver\u00b7letzt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADV", "VVFIN", "PPER", "$,", "ADV", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und meine Seele nie besessen.", "tokens": ["Und", "mei\u00b7ne", "See\u00b7le", "nie", "be\u00b7ses\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sie treten still und ernst heran \u2013", "tokens": ["Sie", "tre\u00b7ten", "still", "und", "ernst", "he\u00b7ran", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "ADJD", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich aber blicke nach den Leuten", "tokens": ["Ich", "a\u00b7ber", "bli\u00b7cke", "nach", "den", "Leu\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und frage mich im Traume dann", "tokens": ["Und", "fra\u00b7ge", "mich", "im", "Trau\u00b7me", "dann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verwundert selbst: was soll's bedeuten?", "tokens": ["Ver\u00b7wun\u00b7dert", "selbst", ":", "was", "soll's", "be\u00b7deu\u00b7ten", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$.", "PWS", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und beim Erwachen sinn' ich nach,", "tokens": ["Und", "beim", "Er\u00b7wa\u00b7chen", "sinn'", "ich", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie Der, wie Jener hingegangen \u2013", "tokens": ["Wie", "Der", ",", "wie", "Je\u00b7ner", "hin\u00b7ge\u00b7gan\u00b7gen", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "$,", "PWAV", "PDAT", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und f\u00fchle, da\u00df jetzt allgemach", "tokens": ["Und", "f\u00fch\u00b7le", ",", "da\u00df", "jetzt", "all\u00b7ge\u00b7mach"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "KOUS", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Reihe wird an mich gelangen.", "tokens": ["Die", "Rei\u00b7he", "wird", "an", "mich", "ge\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Geliebtere Vergangenheit", "tokens": ["Ge\u00b7lieb\u00b7te\u00b7re", "Ver\u00b7gan\u00b7gen\u00b7heit"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Will selbst im Traum nicht wieder sprie\u00dfen:", "tokens": ["Will", "selbst", "im", "Traum", "nicht", "wie\u00b7der", "sprie\u00b7\u00dfen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPRART", "NN", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Gleichgilt'ge Boten schickt die Zeit,", "tokens": ["Gleich\u00b7gilt'\u00b7ge", "Bo\u00b7ten", "schickt", "die", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Um ihre Rechnung abzuschlie\u00dfen.", "tokens": ["Um", "ih\u00b7re", "Rech\u00b7nung", "ab\u00b7zu\u00b7schlie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}