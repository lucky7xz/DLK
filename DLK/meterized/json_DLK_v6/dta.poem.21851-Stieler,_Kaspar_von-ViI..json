{"dta.poem.21851": {"metadata": {"author": {"name": "Stieler, Kaspar von", "birth": "N.A.", "death": "N.A."}, "title": "ViI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1660", "urn": "urn:nbn:de:kobv:b4-20586-2", "language": ["de:0.99"], "booktitle": "Filidor der Dorfferer [i. e. Stieler, Kaspar von]: Die Geharnschte Venus. Hamburg, 1660."}, "poem": {"stanza.1": {"line.1": {"text": "Ich la\u00df\u2019 ein gutes Pfand/", "tokens": ["Ich", "la\u00df'", "ein", "gu\u00b7tes", "Pfand", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "und mir ists unbekannt.", "tokens": ["und", "mir", "ists", "un\u00b7be\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ach! solt\u2019 ichs einst erkennen!", "tokens": ["Ach", "!", "solt'", "ichs", "einst", "er\u00b7ken\u00b7nen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VMFIN", "PIS", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mein Ungl\u00fckk aber treibt mich fort/", "tokens": ["Mein", "Un\u00b7gl\u00fckk", "a\u00b7ber", "treibt", "mich", "fort", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVFIN", "PPER", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "ich weiche von dir/ liebster Ort/", "tokens": ["ich", "wei\u00b7che", "von", "dir", "/", "liebs\u00b7ter", "Ort", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "APPR", "PPER", "$(", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "den ich ohne Schmerzen kan nimmermehr", "tokens": ["den", "ich", "oh\u00b7ne", "Schmer\u00b7zen", "kan", "nim\u00b7mer\u00b7mehr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "NN", "VMFIN", "ADV"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}}}}}