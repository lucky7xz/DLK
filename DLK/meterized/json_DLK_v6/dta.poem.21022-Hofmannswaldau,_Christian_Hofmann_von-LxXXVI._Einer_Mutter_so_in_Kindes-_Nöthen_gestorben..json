{"dta.poem.21022": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "LxXXVI.  \n Einer Mutter/ so in Kindes-  \n N\u00f6then gestorben.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Dem ich das Leben gab/ der hat es mir genommen/", "tokens": ["Dem", "ich", "das", "Le\u00b7ben", "gab", "/", "der", "hat", "es", "mir", "ge\u00b7nom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "$(", "ART", "VAFIN", "PPER", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich bin durch die Geburth an diese Stelle kommen/", "tokens": ["Ich", "bin", "durch", "die", "Ge\u00b7burth", "an", "die\u00b7se", "Stel\u00b7le", "kom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "APPR", "PDAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch wil ich diese Schmach nicht ungeklagt vertra-", "tokens": ["Doch", "wil", "ich", "die\u00b7se", "Schmach", "nicht", "un\u00b7ge\u00b7klagt", "ver\u00b7tra"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "PDAT", "NN", "PTKNEG", "ADJD", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "gen/", "tokens": ["gen", "/"], "token_info": ["word", "punct"], "pos": ["APPR", "$("], "meter": "-", "measure": "single.down"}, "line.5": {"text": "Ich wil es alsobald der grossen Mutter sagen.", "tokens": ["Ich", "wil", "es", "al\u00b7so\u00b7bald", "der", "gros\u00b7sen", "Mut\u00b7ter", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}