{"dta.poem.12053": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "73.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Demuth ist wol gut da\u00df sie ein Herz erringe;", "tokens": ["Die", "De\u00b7muth", "ist", "wol", "gut", "da\u00df", "sie", "ein", "Herz", "er\u00b7rin\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch h\u00fcte dich da\u00df dich dazu nicht Hochmuth bringe.", "tokens": ["Doch", "h\u00fc\u00b7te", "dich", "da\u00df", "dich", "da\u00b7zu", "nicht", "Hoch\u00b7muth", "brin\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "KOUS", "PPER", "PAV", "PTKNEG", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nicht falscher Demuth Schein ist es wovor ich warne,", "tokens": ["Nicht", "fal\u00b7scher", "De\u00b7muth", "Schein", "ist", "es", "wo\u00b7vor", "ich", "war\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJA", "NN", "NN", "VAFIN", "PPER", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den k\u00fcnstlich Hochmuth webt, da\u00df er die Welt umgarne;", "tokens": ["Den", "k\u00fcnst\u00b7lich", "Hoch\u00b7muth", "webt", ",", "da\u00df", "er", "die", "Welt", "um\u00b7gar\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wirkliche Demuth auch, die dir im Herzen sprie\u00dft,", "tokens": ["Wirk\u00b7li\u00b7che", "De\u00b7muth", "auch", ",", "die", "dir", "im", "Her\u00b7zen", "sprie\u00dft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "$,", "PRELS", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "+--+---+-+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Gib Acht ob sie in sich nicht wahren Hochmuth schlie\u00dft;", "tokens": ["Gib", "Acht", "ob", "sie", "in", "sich", "nicht", "wah\u00b7ren", "Hoch\u00b7muth", "schlie\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "CARD", "KOUS", "PPER", "APPR", "PRF", "PTKNEG", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Der, wenn er sich gel\u00e4hmt sieht au\u00dfen, und sich sch\u00e4mt", "tokens": ["Der", ",", "wenn", "er", "sich", "ge\u00b7l\u00e4hmt", "sieht", "au\u00b7\u00dfen", ",", "und", "sich", "sch\u00e4mt"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "$,", "KOUS", "PPER", "PRF", "ADJD", "VVFIN", "ADV", "$,", "KON", "PRF", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mislungenen Erfolgs, zur Demuth sich bequemt.", "tokens": ["Mis\u00b7lun\u00b7ge\u00b7nen", "Er\u00b7folgs", ",", "zur", "De\u00b7muth", "sich", "be\u00b7quemt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "APPRART", "NN", "PRF", "ADJD", "$."], "meter": "-+---+-+-+-+", "measure": "dactylic.init"}}, "stanza.5": {"line.1": {"text": "Wie die Begierde, die, verzweifelnd an Erjagung", "tokens": ["Wie", "die", "Be\u00b7gier\u00b7de", ",", "die", ",", "ver\u00b7zwei\u00b7felnd", "an", "Er\u00b7ja\u00b7gung"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "$,", "PRELS", "$,", "VVPP", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Begehrter G\u00fcter, sich zur\u00fcckzieht in Entsagung.", "tokens": ["Be\u00b7gehr\u00b7ter", "G\u00fc\u00b7ter", ",", "sich", "zu\u00b7r\u00fcck\u00b7zieht", "in", "Ent\u00b7sa\u00b7gung", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRF", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Doch ist es nicht genug, das Ziel erreicht zu haben,", "tokens": ["Doch", "ist", "es", "nicht", "ge\u00b7nug", ",", "das", "Ziel", "er\u00b7reicht", "zu", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PTKNEG", "ADV", "$,", "ART", "NN", "VVPP", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn, statt auf ebnem Weg, auch \u00fcber Stock und Graben?", "tokens": ["Wenn", ",", "statt", "auf", "eb\u00b7nem", "Weg", ",", "auch", "\u00fc\u00b7ber", "Stock", "und", "Gra\u00b7ben", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUI", "APPR", "ADJA", "NN", "$,", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Du danke Gott da\u00df doch die Feinde sind geschlagen,", "tokens": ["Du", "dan\u00b7ke", "Gott", "da\u00df", "doch", "die", "Fein\u00b7de", "sind", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KOUS", "ADV", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und herrsche so da\u00df sie ihr Joch geduldig tragen.", "tokens": ["Und", "herr\u00b7sche", "so", "da\u00df", "sie", "ihr", "Joch", "ge\u00b7dul\u00b7dig", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "KOUS", "PPER", "PPOSAT", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}