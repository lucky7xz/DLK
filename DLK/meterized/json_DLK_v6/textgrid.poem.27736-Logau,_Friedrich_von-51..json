{"textgrid.poem.27736": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "51.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Danckbarkeit, du theure Tugend,", "tokens": ["Dan\u00b7ck\u00b7bar\u00b7keit", ",", "du", "theu\u00b7re", "Tu\u00b7gend", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Alterst bald in deiner Jugend;", "tokens": ["Al\u00b7terst", "bald", "in", "dei\u00b7ner", "Ju\u00b7gend", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Drum macht deine kurtze Frist,", "tokens": ["Drum", "macht", "dei\u00b7ne", "kurt\u00b7ze", "Frist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Da\u00df du immer seltsam bist.", "tokens": ["Da\u00df", "du", "im\u00b7mer", "selt\u00b7sam", "bist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}