{"textgrid.poem.60627": {"metadata": {"author": {"name": "La Fontaine, Jean de", "birth": "N.A.", "death": "N.A."}, "title": "1L: In seinem Werk stellt sich der K\u00fcnstler dar.", "genre": "verse", "period": "N.A.", "pub_year": 1658, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In seinem Werk stellt sich der K\u00fcnstler dar.", "tokens": ["In", "sei\u00b7nem", "Werk", "stellt", "sich", "der", "K\u00fcnst\u00b7ler", "dar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PRF", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Hornissen stritten einst mit einer Bienenschar", "tokens": ["Hor\u00b7nis\u00b7sen", "strit\u00b7ten", "einst", "mit", "ei\u00b7ner", "Bie\u00b7nen\u00b7schar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ADV", "APPR", "ART", "NN"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Um ein paar herrenlose Honigwaben.", "tokens": ["Um", "ein", "paar", "her\u00b7ren\u00b7lo\u00b7se", "Ho\u00b7nig\u00b7wa\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein jeder meinte Recht zu haben,", "tokens": ["Ein", "je\u00b7der", "mein\u00b7te", "Recht", "zu", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "NN", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Waben in Besitz zu nehmen.", "tokens": ["Die", "Wa\u00b7ben", "in", "Be\u00b7sitz", "zu", "neh\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "So mu\u00df man endlich sich bequemen,", "tokens": ["So", "mu\u00df", "man", "end\u00b7lich", "sich", "be\u00b7que\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "ADV", "PRF", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Als Schiedsrichter die Wespe zu befragen.", "tokens": ["Als", "Schieds\u00b7rich\u00b7ter", "die", "Wes\u00b7pe", "zu", "be\u00b7fra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-++--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Ein Richterspruch indes war schwer:", "tokens": ["Ein", "Rich\u00b7ter\u00b7spruch", "in\u00b7des", "war", "schwer", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die Zeugen konnten nur das eine sagen,", "tokens": ["Die", "Zeu\u00b7gen", "konn\u00b7ten", "nur", "das", "ei\u00b7ne", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "ART", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Da\u00df l\u00e4ngliche befl\u00fcgelte Insekten", "tokens": ["Da\u00df", "l\u00e4ng\u00b7li\u00b7che", "be\u00b7fl\u00fc\u00b7gel\u00b7te", "In\u00b7sek\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Einst eifrig schaffend in den Waben steckten;", "tokens": ["Einst", "eif\u00b7rig", "schaf\u00b7fend", "in", "den", "Wa\u00b7ben", "steck\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Gewi\u00df, sie glichen wohl den Bienen sehr,", "tokens": ["Ge\u00b7wi\u00df", ",", "sie", "gli\u00b7chen", "wohl", "den", "Bie\u00b7nen", "sehr", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "ADV", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Doch ach, die vorgenannten Zeichen", "tokens": ["Doch", "ach", ",", "die", "vor\u00b7ge\u00b7nann\u00b7ten", "Zei\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "XY", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Sind bei Hornissen fast die gleichen.", "tokens": ["Sind", "bei", "Hor\u00b7nis\u00b7sen", "fast", "die", "glei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NE", "ADV", "ART", "ADJA", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.14": {"text": "Die Wespe nun, um in die Sache Licht zu tragen,", "tokens": ["Die", "Wes\u00b7pe", "nun", ",", "um", "in", "die", "Sa\u00b7che", "Licht", "zu", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "KOUI", "APPR", "ART", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Beschlo\u00df, ein Volk Ameisen zu befragen;", "tokens": ["Be\u00b7schlo\u00df", ",", "ein", "Volk", "A\u00b7mei\u00b7sen", "zu", "be\u00b7fra\u00b7gen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ART", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Umsonst: der Fall war nicht zu schlichten.", "tokens": ["Um\u00b7sonst", ":", "der", "Fall", "war", "nicht", "zu", "schlich\u00b7ten", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "ART", "NN", "VAFIN", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Da rief ein Bienchen in besorgtem Ton:", "tokens": ["Da", "rief", "ein", "Bien\u00b7chen", "in", "be\u00b7sorg\u00b7tem", "Ton", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "\u00bbsechs Monde h\u00e4ngt die Sache schon,", "tokens": ["\u00bb", "sechs", "Mon\u00b7de", "h\u00e4ngt", "die", "Sa\u00b7che", "schon", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "CARD", "NN", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Doch ist kein Fortschritt zu berichten.", "tokens": ["Doch", "ist", "kein", "Fort\u00b7schritt", "zu", "be\u00b7rich\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PIAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Der Honig wird uns noch verderben,", "tokens": ["Der", "Ho\u00b7nig", "wird", "uns", "noch", "ver\u00b7der\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Ein schneller Spruch tut not, sonst fri\u00dft", "tokens": ["Ein", "schnel\u00b7ler", "Spruch", "tut", "not", ",", "sonst", "fri\u00dft"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "NN", "$,", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Den s\u00fc\u00dfen Stoff der B\u00e4r mit seinen Erben;", "tokens": ["Den", "s\u00fc\u00b7\u00dfen", "Stoff", "der", "B\u00e4r", "mit", "sei\u00b7nen", "Er\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Drum la\u00dft das unn\u00fctz viele Fragen,", "tokens": ["Drum", "la\u00dft", "das", "un\u00b7n\u00fctz", "vie\u00b7le", "Fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Das Wortew\u00e4gen und das Hin und Her,", "tokens": ["Das", "Wor\u00b7te\u00b7w\u00e4\u00b7gen", "und", "das", "Hin", "und", "Her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Da ein viel be\u00dfres Mittel ist,", "tokens": ["Da", "ein", "viel", "be\u00df\u00b7res", "Mit\u00b7tel", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "PIAT", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Den Streitfall endlich auszutragen:", "tokens": ["Den", "Streit\u00b7fall", "end\u00b7lich", "aus\u00b7zu\u00b7tra\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "La\u00dft uns und auch den Gegner Waben bauen!", "tokens": ["La\u00dft", "uns", "und", "auch", "den", "Geg\u00b7ner", "Wa\u00b7ben", "bau\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "KON", "ADV", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Gewi\u00df ist die Entscheidung dann nicht schwer,", "tokens": ["Ge\u00b7wi\u00df", "ist", "die", "Ent\u00b7schei\u00b7dung", "dann", "nicht", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Denn wessen Zellen jenen \u00e4hnlich schauen,", "tokens": ["Denn", "wes\u00b7sen", "Zel\u00b7len", "je\u00b7nen", "\u00e4hn\u00b7lich", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PDAT", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Die hier in Frage stehn, der ist im Recht.\u00ab", "tokens": ["Die", "hier", "in", "Fra\u00b7ge", "stehn", ",", "der", "ist", "im", "Recht", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADV", "APPR", "NN", "VVINF", "$,", "PRELS", "VAFIN", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "Da wehrten die Hornissen sich nicht schlecht,", "tokens": ["Da", "wehr\u00b7ten", "die", "Hor\u00b7nis\u00b7sen", "sich", "nicht", "schlecht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PRF", "PTKNEG", "ADJD", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.32": {"text": "Und ihre Weigrung zeigte klar,", "tokens": ["Und", "ih\u00b7re", "Wei\u00b7grung", "zeig\u00b7te", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Welche Partei im Unrecht war.", "tokens": ["Wel\u00b7che", "Par\u00b7tei", "im", "Un\u00b7recht", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "APPRART", "NN", "VAFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.34": {"text": "Der Honig wurde unverweilt", "tokens": ["Der", "Ho\u00b7nig", "wur\u00b7de", "un\u00b7ver\u00b7weilt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Den klugen Bienen zugeteilt.", "tokens": ["Den", "klu\u00b7gen", "Bie\u00b7nen", "zu\u00b7ge\u00b7teilt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wollt Gott, ein jeder Streitfall w\u00fcrde so geschlichtet,", "tokens": ["Wollt", "Gott", ",", "ein", "je\u00b7der", "Streit\u00b7fall", "w\u00fcr\u00b7de", "so", "ge\u00b7schlich\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "$,", "ART", "PIAT", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df nicht ein Paragraph, nein, klare Einsicht richtet!", "tokens": ["Da\u00df", "nicht", "ein", "Pa\u00b7ra\u00b7gra\u00b7ph", ",", "nein", ",", "kla\u00b7re", "Ein\u00b7sicht", "rich\u00b7tet", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ART", "NN", "$,", "PTKANT", "$,", "ADJA", "NN", "VVFIN", "$."], "meter": "--+-+-+-+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Ja, folgten wir hierin den Orientalen,", "tokens": ["Ja", ",", "folg\u00b7ten", "wir", "hie\u00b7rin", "den", "O\u00b7rien\u00b7ta\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So brauchten wir nur halb so viel zu zahlen.", "tokens": ["So", "brauch\u00b7ten", "wir", "nur", "halb", "so", "viel", "zu", "zah\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJD", "ADV", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "So aber ist's der Richter, der gewinnt,", "tokens": ["So", "a\u00b7ber", "ist's", "der", "Rich\u00b7ter", ",", "der", "ge\u00b7winnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "ART", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "F\u00fcr den die sch\u00f6nen fetten Austern sind,", "tokens": ["F\u00fcr", "den", "die", "sch\u00f6\u00b7nen", "fet\u00b7ten", "Aus\u00b7tern", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "ADJA", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und f\u00fcr den Kl\u00e4ger bleiben nur die Schalen.", "tokens": ["Und", "f\u00fcr", "den", "Kl\u00e4\u00b7ger", "blei\u00b7ben", "nur", "die", "Scha\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}