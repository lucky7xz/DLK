{"dta.poem.6710": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Gesicht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In welcher Fern' und auf wie mancherley", "tokens": ["In", "wel\u00b7cher", "Fern'", "und", "auf", "wie", "man\u00b7cher\u00b7ley"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PWAT", "NN", "KON", "APPR", "PWAV", "PIS"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Erstrecket das Gesichte sich!", "tokens": ["Er\u00b7stre\u00b7cket", "das", "Ge\u00b7sich\u00b7te", "sich", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es ist der erste Sinn, der fast nicht c\u00f6rperlich,", "tokens": ["Es", "ist", "der", "ers\u00b7te", "Sinn", ",", "der", "fast", "nicht", "c\u00f6r\u00b7per\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,", "PRELS", "ADV", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der wunderbarlich ist, und der dabey", "tokens": ["Der", "wun\u00b7der\u00b7bar\u00b7lich", "ist", ",", "und", "der", "da\u00b7bey"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJD", "VAFIN", "$,", "KON", "ART", "PAV"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Am meisten allgemein, der an Geschwindigkeit", "tokens": ["Am", "meis\u00b7ten", "all\u00b7ge\u00b7mein", ",", "der", "an", "Ge\u00b7schwin\u00b7dig\u00b7keit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "PIS", "ADJD", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Gantz unbegreifflich ist. Des Geists Beschaffenheit", "tokens": ["Gantz", "un\u00b7be\u00b7greif\u00b7flich", "ist", ".", "Des", "Geists", "Be\u00b7schaf\u00b7fen\u00b7heit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "VAFIN", "$.", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Folgt dieser gantz allein;", "tokens": ["Folgt", "die\u00b7ser", "gantz", "al\u00b7lein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Ja er wird dem Verstand am meisten brauchbar seyn.", "tokens": ["Ja", "er", "wird", "dem", "Ver\u00b7stand", "am", "meis\u00b7ten", "brauch\u00b7bar", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "VAFIN", "ART", "NN", "APPRART", "PIS", "ADJD", "VAINF", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}}}}}