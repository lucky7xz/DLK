{"textgrid.poem.63870": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "1L: Willst du noch ein Liebeslied singen, meine Seele,", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Willst du noch ein Liebeslied singen, meine Seele,", "tokens": ["Willst", "du", "noch", "ein", "Lie\u00b7bes\u00b7lied", "sin\u00b7gen", ",", "mei\u00b7ne", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Ob auch l\u00e4ngst die Jugend schied, m\u00fcd' und rauh die Kehle?", "tokens": ["Ob", "auch", "l\u00e4ngst", "die", "Ju\u00b7gend", "schied", ",", "m\u00fcd'", "und", "rauh", "die", "Keh\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "ART", "NN", "VVFIN", "$,", "ADJD", "KON", "ADJD", "ART", "NN", "$."], "meter": "--+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "S\u00fc\u00dfer wohl und heller klang's in den Rosentagen,", "tokens": ["S\u00fc\u00b7\u00dfer", "wohl", "und", "hel\u00b7ler", "klang's", "in", "den", "Ro\u00b7sen\u00b7ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "KON", "ADJD", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Als noch rings im Lenzgebiet mitsang Philomele,", "tokens": ["Als", "noch", "rings", "im", "Lenz\u00b7ge\u00b7biet", "mit\u00b7sang", "Phi\u00b7lo\u00b7me\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "APPRART", "NN", "VVFIN", "NE", "$,"], "meter": "--+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Als des Baches Murmeln nur und des Waldes Rauschen", "tokens": ["Als", "des", "Ba\u00b7ches", "Mur\u00b7meln", "nur", "und", "des", "Wal\u00b7des", "Rau\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "ADV", "KON", "ART", "NN", "NN"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "Mich geheimnisvoll beriet, wie das Wort ich w\u00e4hle.", "tokens": ["Mich", "ge\u00b7heim\u00b7nis\u00b7voll", "be\u00b7riet", ",", "wie", "das", "Wort", "ich", "w\u00e4h\u00b7le", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVFIN", "$,", "PWAV", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "Jetzt im Lebenswinter ach, mu\u00df ich wohl erkennen,", "tokens": ["Jetzt", "im", "Le\u00b7bens\u00b7win\u00b7ter", "ach", ",", "mu\u00df", "ich", "wohl", "er\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "XY", "$,", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "--+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Wenn es zum Gesang mich zieht, da\u00df das Beste fehle.", "tokens": ["Wenn", "es", "zum", "Ge\u00b7sang", "mich", "zieht", ",", "da\u00df", "das", "Bes\u00b7te", "feh\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "PPER", "VVFIN", "$,", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Statt, was mir im Herzen lebt, m\u00fchlos auszustr\u00f6men,", "tokens": ["Statt", ",", "was", "mir", "im", "Her\u00b7zen", "lebt", ",", "m\u00fch\u00b7los", "aus\u00b7zu\u00b7str\u00f6\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "PPER", "APPRART", "NN", "VVFIN", "$,", "ADJD", "VVPP", "$,"], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.10": {"text": "Wie die Thr\u00e4n' entquillt dem Lid, reim' ich jetzt Ghasele.", "tokens": ["Wie", "die", "Thr\u00e4n'", "ent\u00b7quillt", "dem", "Lid", ",", "reim'", "ich", "jetzt", "Gha\u00b7se\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "ART", "NN", "$,", "VVFIN", "PPER", "ADV", "NN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.11": {"text": "Doch den Eisesblumen gleich, die vom warmen Hauche", "tokens": ["Doch", "den", "Ei\u00b7ses\u00b7blu\u00b7men", "gleich", ",", "die", "vom", "war\u00b7men", "Hau\u00b7che"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADV", "$,", "PRELS", "APPRART", "ADJA", "NN"], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.12": {"text": "Man erbl\u00fchn am Fenster sieht, zierlich sonder Fehle,", "tokens": ["Man", "er\u00b7bl\u00fchn", "am", "Fens\u00b7ter", "sieht", ",", "zier\u00b7lich", "son\u00b7der", "Feh\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPRART", "NN", "VVFIN", "$,", "ADJD", "KON", "NN", "$,"], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.13": {"text": "Ist auch dieser Winterflor warmer Brust entsprossen \u2013", "tokens": ["Ist", "auch", "die\u00b7ser", "Win\u00b7ter\u00b7flor", "war\u00b7mer", "Brust", "ent\u00b7spros\u00b7sen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PDAT", "NN", "ADJA", "NN", "VVPP", "$("], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.14": {"text": "Singe nur dein Liebeslied, sing es, meine Seele!", "tokens": ["Sin\u00b7ge", "nur", "dein", "Lie\u00b7bes\u00b7lied", ",", "sing", "es", ",", "mei\u00b7ne", "See\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}}}}}