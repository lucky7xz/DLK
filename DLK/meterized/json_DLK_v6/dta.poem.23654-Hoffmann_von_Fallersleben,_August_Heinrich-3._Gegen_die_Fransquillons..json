{"dta.poem.23654": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "3. Gegen die Fransquillons.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Einst wird auch eure Stunde schlagen", "tokens": ["Einst", "wird", "auch", "eu\u00b7re", "Stun\u00b7de", "schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und rufen wird euch Mann und Kind", "tokens": ["Und", "ru\u00b7fen", "wird", "euch", "Mann", "und", "Kind"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVINF", "VAFIN", "PPER", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Ruf aus jenen sch\u00f6nen Tagen:", "tokens": ["Den", "Ruf", "aus", "je\u00b7nen", "sch\u00f6\u00b7nen", "Ta\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schild en Vrind! \nSiehe Leo's 12. B&#252;cher niederl. Geschichten I, 179.", "tokens": ["Schild", "en", "Vrind", "!", "Sie\u00b7he", "Leo's", "12.", "B", "&#252;", "cher", "nie\u00b7der\u00b7l.", "Ge\u00b7schich\u00b7ten", "I", ",", "179."], "token_info": ["word", "word", "word", "punct", "word", "word", "ordinal", "word", "XML_entity", "word", "abbreviation", "word", "word", "punct", "ordinal"], "pos": ["NN", "FM", "FM", "$.", "VVIMP", "NE", "ADJA", "XY", "$(", "FM.la", "FM.la", "FM.la", "FM.la", "$,", "ADJA"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Und alle Herzen werden sagen:", "tokens": ["Und", "al\u00b7le", "Her\u00b7zen", "wer\u00b7den", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wohl uns, da\u00df wir es wieder sind,", "tokens": ["Wohl", "uns", ",", "da\u00df", "wir", "es", "wie\u00b7der", "sind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "KOUS", "PPER", "PPER", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Volk aus jenen sch\u00f6nen Tagen!", "tokens": ["Das", "Volk", "aus", "je\u00b7nen", "sch\u00f6\u00b7nen", "Ta\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch heute k\u00f6nnen wir nur klagen:", "tokens": ["Doch", "heu\u00b7te", "k\u00f6n\u00b7nen", "wir", "nur", "kla\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Kaum h\u00f6ren wir vor w\u00e4lschem Wind", "tokens": ["Kaum", "h\u00f6\u00b7ren", "wir", "vor", "w\u00e4l\u00b7schem", "Wind"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Ruf aus jenen sch\u00f6nen Tagen:", "tokens": ["Den", "Ruf", "aus", "je\u00b7nen", "sch\u00f6\u00b7nen", "Ta\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}