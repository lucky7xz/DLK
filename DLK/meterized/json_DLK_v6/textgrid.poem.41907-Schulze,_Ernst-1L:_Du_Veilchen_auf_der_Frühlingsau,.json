{"textgrid.poem.41907": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du Veilchen auf der Fr\u00fchlingsau,", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du Veilchen auf der Fr\u00fchlingsau,", "tokens": ["Du", "Veil\u00b7chen", "auf", "der", "Fr\u00fch\u00b7lin\u00b7gsau", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie stehst du tief in's Gr\u00fcn gebogen,", "tokens": ["Wie", "stehst", "du", "tief", "in's", "Gr\u00fcn", "ge\u00b7bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und hast von k\u00fchlem Morgenthau", "tokens": ["Und", "hast", "von", "k\u00fch\u00b7lem", "Mor\u00b7gent\u00b7hau"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den kleinen Kelch so voll gesogen!", "tokens": ["Den", "klei\u00b7nen", "Kelch", "so", "voll", "ge\u00b7so\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du f\u00fchlst wohl auch schon, kaum entbl\u00fchk,", "tokens": ["Du", "f\u00fchlst", "wohl", "auch", "schon", ",", "kaum", "ent\u00b7bl\u00fchk", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADV", "$,", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der bangen Liebe Sorg' und Sehnen,", "tokens": ["Der", "ban\u00b7gen", "Lie\u00b7be", "Sor\u00b7g'", "und", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die vor dem Blick der Menschen flieht", "tokens": ["Die", "vor", "dem", "Blick", "der", "Men\u00b7schen", "flieht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und dunkle Schatten liebt und Thr\u00e4nen.", "tokens": ["Und", "dunk\u00b7le", "Schat\u00b7ten", "liebt", "und", "Thr\u00e4\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Mag freundlich auch das Sonnenlicht", "tokens": ["Mag", "freund\u00b7lich", "auch", "das", "Son\u00b7nen\u00b7licht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADJD", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Um deine gr\u00fcne Wiege glimmen;", "tokens": ["Um", "dei\u00b7ne", "gr\u00fc\u00b7ne", "Wie\u00b7ge", "glim\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dich w\u00e4rmt sein lauer Schimmer nicht,", "tokens": ["Dich", "w\u00e4rmt", "sein", "lau\u00b7er", "Schim\u00b7mer", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "ADJA", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So lang die Perlen in dir schwimmen.", "tokens": ["So", "lang", "die", "Per\u00b7len", "in", "dir", "schwim\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch bist du gl\u00fccklicher als ich,", "tokens": ["Doch", "bist", "du", "gl\u00fcck\u00b7li\u00b7cher", "als", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "KOKOM", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Denn Keiner wehrt es dir, zu weinen,", "tokens": ["Denn", "Kei\u00b7ner", "wehrt", "es", "dir", ",", "zu", "wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPER", "PPER", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und Eine liebt und findet dich", "tokens": ["Und", "Ei\u00b7ne", "liebt", "und", "fin\u00b7det", "dich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "VVFIN", "KON", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wird es freundlich mit dir meinen.", "tokens": ["Und", "wird", "es", "freund\u00b7lich", "mit", "dir", "mei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Drum zittre nur im Morgenwehn", "tokens": ["Drum", "zitt\u00b7re", "nur", "im", "Mor\u00b7gen\u00b7wehn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und n\u00e4hre deine s\u00fc\u00dfen Schmerzen,", "tokens": ["Und", "n\u00e4h\u00b7re", "dei\u00b7ne", "s\u00fc\u00b7\u00dfen", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dann bl\u00fchst du doppelt frisch und sch\u00f6n", "tokens": ["Dann", "bl\u00fchst", "du", "dop\u00b7pelt", "frisch", "und", "sch\u00f6n"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und duftiger an ihrem Herzen.", "tokens": ["Und", "duf\u00b7ti\u00b7ger", "an", "ih\u00b7rem", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Sie liebt die Blumen auf der Flur,", "tokens": ["Sie", "liebt", "die", "Blu\u00b7men", "auf", "der", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie liebt das V\u00f6gelein am Bache;", "tokens": ["Sie", "liebt", "das", "V\u00f6\u00b7ge\u00b7lein", "am", "Ba\u00b7che", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und ach, mich Einen flieht sie nur,", "tokens": ["Und", "ach", ",", "mich", "Ei\u00b7nen", "flieht", "sie", "nur", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$,", "PRF", "ART", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und dennoch will sie, da\u00df ich lache!", "tokens": ["Und", "den\u00b7noch", "will", "sie", ",", "da\u00df", "ich", "la\u00b7che", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}