{"textgrid.poem.24226": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Gl\u00fcck", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin so voll von Liebe,", "tokens": ["Ich", "bin", "so", "voll", "von", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie die Traube ist voll von S\u00fc\u00dfe,", "tokens": ["Wie", "die", "Trau\u00b7be", "ist", "voll", "von", "S\u00fc\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VAFIN", "ADJD", "APPR", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Mein Herz ist wie im Sommer", "tokens": ["Mein", "Herz", "ist", "wie", "im", "Som\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "KOKOM", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der volle Apfelbaum.", "tokens": ["Der", "vol\u00b7le", "Ap\u00b7fel\u00b7baum", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich gehe stille Wege", "tokens": ["Ich", "ge\u00b7he", "stil\u00b7le", "We\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit ruhigem Gem\u00fcte,", "tokens": ["Mit", "ru\u00b7hi\u00b7gem", "Ge\u00b7m\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Der hohe blaue Himmel", "tokens": ["Der", "ho\u00b7he", "blau\u00b7e", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ist mir kein leerer Raum.", "tokens": ["Ist", "mir", "kein", "lee\u00b7rer", "Raum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich bin mit allem Leben", "tokens": ["Ich", "bin", "mit", "al\u00b7lem", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PIS", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Verwurzelt und verwachsen,", "tokens": ["Ver\u00b7wur\u00b7zelt", "und", "ver\u00b7wach\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Sonne ist meine Mutter,", "tokens": ["Die", "Son\u00b7ne", "ist", "mei\u00b7ne", "Mut\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Gott ist mein sch\u00f6nster Traum.", "tokens": ["Gott", "ist", "mein", "sch\u00f6ns\u00b7ter", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}