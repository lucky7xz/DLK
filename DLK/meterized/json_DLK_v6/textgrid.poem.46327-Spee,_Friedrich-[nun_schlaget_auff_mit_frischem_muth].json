{"textgrid.poem.46327": {"metadata": {"author": {"name": "Spee, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[nun schlaget auff mit frischem muth]", "genre": "verse", "period": "N.A.", "pub_year": 1613, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun schlaget auff mit frischem muth", "tokens": ["Nun", "schla\u00b7get", "auff", "mit", "fri\u00b7schem", "muth"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem frommen Gott zun ehren,", "tokens": ["Dem", "from\u00b7men", "Gott", "zun", "eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Auff trommlen vnd heerpaucken gut,", "tokens": ["Auff", "tromm\u00b7len", "vnd", "heer\u00b7pau\u00b7cken", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Last vns sein lob vermehren.", "tokens": ["Last", "vns", "sein", "lob", "ver\u00b7meh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Schlagt auff, da\u00df Erd vnd himel gleich", "tokens": ["Schlagt", "auff", ",", "da\u00df", "Erd", "vnd", "hi\u00b7mel", "gleich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKVZ", "$,", "KOUS", "NN", "KON", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vom widerschal sich wege", "tokens": ["Vom", "wi\u00b7der\u00b7schal", "sich", "we\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "PRF", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df auch das meer zu iedem streich", "tokens": ["Da\u00df", "auch", "das", "meer", "zu", "ie\u00b7dem", "streich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "All wasserwellen rege.", "tokens": ["All", "was\u00b7ser\u00b7wel\u00b7len", "re\u00b7ge", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Blast an die h\u00f6rner allzumahl,", "tokens": ["Blast", "an", "die", "h\u00f6r\u00b7ner", "all\u00b7zu\u00b7mahl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "All Cymbalen last leuten;", "tokens": ["All", "Cym\u00b7ba\u00b7len", "last", "leu\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Last h\u00f6ren der posaunen schall,", "tokens": ["Last", "h\u00f6\u00b7ren", "der", "po\u00b7sau\u00b7nen", "schall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "All Orgel, Zinck, vnd fleuten.", "tokens": ["All", "Or\u00b7gel", ",", "Zinck", ",", "vnd", "fleu\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "NN", "$,", "KON", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ja auch so vil: dergleichen spiel", "tokens": ["Ja", "auch", "so", "vil", ":", "derg\u00b7lei\u00b7chen", "spiel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PTKANT", "ADV", "ADV", "ADV", "$.", "PIS", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "All k\u00fcnstler inventiren,", "tokens": ["All", "k\u00fcnst\u00b7ler", "in\u00b7ven\u00b7ti\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.3": {"text": "Last alle gar zu selben ziel", "tokens": ["Last", "al\u00b7le", "gar", "zu", "sel\u00b7ben", "ziel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PIS", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Einhellig musicieren.", "tokens": ["Ein\u00b7hel\u00b7lig", "mu\u00b7si\u00b7cie\u00b7ren", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Mit harffen, vnd mit lautenklang", "tokens": ["Mit", "harf\u00b7fen", ",", "vnd", "mit", "lau\u00b7ten\u00b7klang"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "VVFIN", "$,", "KON", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Last ihm zun ehren singen,", "tokens": ["Last", "ihm", "zun", "eh\u00b7ren", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "VVINF", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Mit zincken, vnd trompetten sang", "tokens": ["Mit", "zin\u00b7cken", ",", "vnd", "trom\u00b7pet\u00b7ten", "sang"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "VVINF", "$,", "KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Last ihm zun ehren klingen:", "tokens": ["Last", "ihm", "zun", "eh\u00b7ren", "klin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "VVINF", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.6": {"line.1": {"text": "Vnd dan auch fort im selben gang", "tokens": ["Vnd", "dan", "auch", "fort", "im", "sel\u00b7ben", "gang"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "PTKVZ", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Last ihm zun ehren springen,", "tokens": ["Last", "ihm", "zun", "eh\u00b7ren", "sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "VVINF", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Last sagen danck vn\u00df lebenlang,", "tokens": ["Last", "sa\u00b7gen", "danck", "vn\u00df", "le\u00b7ben\u00b7lang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "NE", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Last allweg opffer bringen.", "tokens": ["Last", "all\u00b7weg", "opf\u00b7fer", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "VVINF", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}}}}