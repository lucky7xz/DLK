{"textgrid.poem.35577": {"metadata": {"author": {"name": "Trakl, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Abend in Lans", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wanderschaft durch d\u00e4mmernden Sommer", "tokens": ["Wan\u00b7der\u00b7schaft", "durch", "d\u00e4m\u00b7mern\u00b7den", "Som\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "An B\u00fcndeln vergilbten Korns vorbei. Unter get\u00fcnchten Bogen,", "tokens": ["An", "B\u00fcn\u00b7deln", "ver\u00b7gilb\u00b7ten", "Korns", "vor\u00b7bei", ".", "Un\u00b7ter", "ge\u00b7t\u00fcnch\u00b7ten", "Bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "PTKVZ", "$.", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+--+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Wo die Schwalbe aus und ein flog, tranken wir feurigen Wein.", "tokens": ["Wo", "die", "Schwal\u00b7be", "aus", "und", "ein", "flog", ",", "tran\u00b7ken", "wir", "feu\u00b7ri\u00b7gen", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PTKVZ", "KON", "ART", "VVFIN", "$,", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "--+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Sch\u00f6n: o Schwermut und purpurnes Lachen.", "tokens": ["Sch\u00f6n", ":", "o", "Schwer\u00b7mut", "und", "pur\u00b7pur\u00b7nes", "La\u00b7chen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "FM", "NN", "KON", "ADJA", "NN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Abend und die dunklen D\u00fcfte des Gr\u00fcns", "tokens": ["A\u00b7bend", "und", "die", "dunk\u00b7len", "D\u00fcf\u00b7te", "des", "Gr\u00fcns"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "K\u00fchlen mit Schauern die gl\u00fchende Stirne uns.", "tokens": ["K\u00fch\u00b7len", "mit", "Schau\u00b7ern", "die", "gl\u00fc\u00b7hen\u00b7de", "Stir\u00b7ne", "uns", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "ART", "ADJA", "NN", "PPER", "$."], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}}, "stanza.3": {"line.1": {"text": "Silberne Wasser rinnen \u00fcber die Stufen des Walds,", "tokens": ["Sil\u00b7ber\u00b7ne", "Was\u00b7ser", "rin\u00b7nen", "\u00fc\u00b7ber", "die", "Stu\u00b7fen", "des", "Walds", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Die Nacht und sprachlos ein vergessenes Leben.", "tokens": ["Die", "Nacht", "und", "sprach\u00b7los", "ein", "ver\u00b7ges\u00b7se\u00b7nes", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Freund; die belaubten Stege ins Dorf.", "tokens": ["Freund", ";", "die", "be\u00b7laub\u00b7ten", "Ste\u00b7ge", "ins", "Dorf", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}}}}}