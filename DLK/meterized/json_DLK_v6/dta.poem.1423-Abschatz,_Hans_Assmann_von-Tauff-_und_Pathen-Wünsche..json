{"dta.poem.1423": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Tauff- und Pathen-W\u00fcnsche.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Encklin/ auff die Welt kommst du befleckt mit", "tokens": ["Mein", "E\u00b7ncklin", "/", "auff", "die", "Welt", "kommst", "du", "be\u00b7fleckt", "mit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$(", "APPR", "ART", "NN", "VVFIN", "PPER", "VVFIN", "APPR"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Schuld/", "tokens": ["Schuld", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Das Tauff-Bad reinigt dich und bringt dir GOttes Huld.", "tokens": ["Das", "Tauff\u00b7Bad", "rei\u00b7nigt", "dich", "und", "bringt", "dir", "Got\u00b7tes", "Huld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "KON", "VVFIN", "PPER", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mein Wunsch ist: Bleibe stets in dir geschlossen ein/", "tokens": ["Mein", "Wunsch", "ist", ":", "Blei\u00b7be", "stets", "in", "dir", "ge\u00b7schlos\u00b7sen", "ein", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "$.", "NN", "ADV", "APPR", "PPER", "VVPP", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Gott lasse seine Ruh in deiner Seele seyn.", "tokens": ["Gott", "las\u00b7se", "sei\u00b7ne", "Ruh", "in", "dei\u00b7ner", "See\u00b7le", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}