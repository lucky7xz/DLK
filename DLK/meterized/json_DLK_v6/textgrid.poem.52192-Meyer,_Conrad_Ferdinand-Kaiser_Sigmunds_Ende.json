{"textgrid.poem.52192": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Kaiser Sigmunds Ende", "genre": "verse", "period": "N.A.", "pub_year": 1861, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bblicht und lauter Bl\u00e4ue! Recht ein Wandertag!", "tokens": ["\u00bb", "licht", "und", "lau\u00b7ter", "Bl\u00e4u\u00b7e", "!", "Recht", "ein", "Wan\u00b7der\u00b7tag", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "NN", "KON", "PIAT", "NN", "$.", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Weit hinaus ins Freie! Weg aus diesem Prag!", "tokens": ["Weit", "hin\u00b7aus", "ins", "Frei\u00b7e", "!", "Weg", "aus", "die\u00b7sem", "Prag", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APZR", "APPRART", "NN", "$.", "NN", "APPR", "PDAT", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Holt mir eine S\u00e4nfte, macht es mir zu Dank:", "tokens": ["Holt", "mir", "ei\u00b7ne", "S\u00e4nf\u00b7te", ",", "macht", "es", "mir", "zu", "Dank", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "VVFIN", "PPER", "PPER", "APPR", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Vorn ein R\u00f6\u00dflein, hinten eins, und beide blank!", "tokens": ["Vorn", "ein", "R\u00f6\u00df\u00b7lein", ",", "hin\u00b7ten", "eins", ",", "und", "bei\u00b7de", "blank", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ADV", "PIS", "$,", "KON", "PIS", "PTKVZ", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.2": {"line.1": {"text": "Fr\u00f6hlich will ich fahren tief ins Abendrot,", "tokens": ["Fr\u00f6h\u00b7lich", "will", "ich", "fah\u00b7ren", "tief", "ins", "A\u00b7ben\u00b7drot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "VVFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Sei mein schlanker L\u00e4ufer, spring, Gevatter Tod!", "tokens": ["Sei", "mein", "schlan\u00b7ker", "L\u00e4u\u00b7fer", ",", "spring", ",", "Ge\u00b7vat\u00b7ter", "Tod", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "$,", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Trabe, L\u00e4ufer, trabe! Flugs bestelle mir", "tokens": ["Tra\u00b7be", ",", "L\u00e4u\u00b7fer", ",", "tra\u00b7be", "!", "Flugs", "be\u00b7stel\u00b7le", "mir"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "VVFIN", "$.", "ADV", "VVFIN", "PPER"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Ein geruhig Bettlein und das Nachtquartier!\u00ab", "tokens": ["Ein", "ge\u00b7ru\u00b7hig", "Bet\u00b7tlein", "und", "das", "Nacht\u00b7quar\u00b7tier", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJD", "NN", "KON", "ART", "NN", "$.", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.3": {"line.1": {"text": "Durch die Gassen ging es, wo die Menge stand,", "tokens": ["Durch", "die", "Gas\u00b7sen", "ging", "es", ",", "wo", "die", "Men\u00b7ge", "stand", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Statt des Purpurs trug er schlichtes Reis'gewand,", "tokens": ["Statt", "des", "Pur\u00b7purs", "trug", "er", "schlich\u00b7tes", "Reis'\u00b7ge\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Von dem Lorbeerzweige das Gelock umlaubt,", "tokens": ["Von", "dem", "Lor\u00b7beer\u00b7zwei\u00b7ge", "das", "Ge\u00b7lock", "um\u00b7laubt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Nickt' ins Volk er freundlich, zitternd mit dem Haupt.", "tokens": ["Nickt'", "ins", "Volk", "er", "freund\u00b7lich", ",", "zit\u00b7ternd", "mit", "dem", "Haupt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "PPER", "ADJD", "$,", "VVPP", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.4": {"line.1": {"text": "Als er vor dem Tore blaches Feld gewann,", "tokens": ["Als", "er", "vor", "dem", "To\u00b7re", "bla\u00b7ches", "Feld", "ge\u00b7wann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Pries er Erd und Himmel: \u00bbO ich sel'ger Mann!", "tokens": ["Pries", "er", "Erd", "und", "Him\u00b7mel", ":", "\u00bb", "O", "ich", "sel'\u00b7ger", "Mann", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "NN", "KON", "NN", "$.", "$(", "NE", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Herden seh ich gerne, auch den Pfl\u00fcger gern:", "tokens": ["Her\u00b7den", "seh", "ich", "ger\u00b7ne", ",", "auch", "den", "Pfl\u00fc\u00b7ger", "gern", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "$,", "ADV", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Sei gesegnet, N\u00e4he! Sei gesegnet, Fern!\u00ab", "tokens": ["Sei", "ge\u00b7seg\u00b7net", ",", "N\u00e4\u00b7he", "!", "Sei", "ge\u00b7seg\u00b7net", ",", "Fern", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct", "punct"], "pos": ["VAFIN", "VVPP", "$,", "NN", "$.", "VAFIN", "VVPP", "$,", "NN", "$.", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.5": {"line.1": {"text": "Wie die wanderm\u00fcde Sonne niedersank,", "tokens": ["Wie", "die", "wan\u00b7der\u00b7m\u00fc\u00b7de", "Son\u00b7ne", "nie\u00b7der\u00b7sank", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "\u00d6ffnet' er die Lippen als zum Abendtrank,", "tokens": ["\u00d6ff\u00b7net'", "er", "die", "Lip\u00b7pen", "als", "zum", "A\u00b7bend\u00b7trank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "KOKOM", "APPRART", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Dann ist er entschlummert in der dunkeln Flur,", "tokens": ["Dann", "ist", "er", "ent\u00b7schlum\u00b7mert", "in", "der", "dun\u00b7keln", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Drauf mit wei\u00dfen R\u00f6\u00dflein seine S\u00e4nfte fuhr.", "tokens": ["Drauf", "mit", "wei\u00b7\u00dfen", "R\u00f6\u00df\u00b7lein", "sei\u00b7ne", "S\u00e4nf\u00b7te", "fuhr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "ADJA", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}}}}