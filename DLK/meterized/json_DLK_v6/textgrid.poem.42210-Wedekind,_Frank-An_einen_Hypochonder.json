{"textgrid.poem.42210": {"metadata": {"author": {"name": "Wedekind, Frank", "birth": "N.A.", "death": "N.A."}, "title": "An einen Hypochonder", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du runzelst die Stirne,", "tokens": ["Du", "run\u00b7zelst", "die", "Stir\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Du wetterst und schreist,", "tokens": ["Du", "wet\u00b7terst", "und", "schreist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Dieweil mit der Birne", "tokens": ["Die\u00b7weil", "mit", "der", "Bir\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Den Wurm du verspeist.", "tokens": ["Den", "Wurm", "du", "ver\u00b7speist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Was folgst du empfindlich", "tokens": ["Was", "folgst", "du", "emp\u00b7find\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "ADJD"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Der grausigen Spur?", "tokens": ["Der", "grau\u00b7si\u00b7gen", "Spur", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Erfreu dich doch kindlich", "tokens": ["Er\u00b7freu", "dich", "doch", "kind\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADJD"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Der reichen Natur.", "tokens": ["Der", "rei\u00b7chen", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Je herber dein Liebchen,", "tokens": ["Je", "her\u00b7ber", "dein", "Lieb\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Um so s\u00fc\u00dfer sein Ku\u00df,", "tokens": ["Um", "so", "s\u00fc\u00b7\u00dfer", "sein", "Ku\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "ADJD", "PPOSAT", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und je kleiner sein St\u00fcbchen", "tokens": ["Und", "je", "klei\u00b7ner", "sein", "St\u00fcb\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "PPOSAT", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Desto gr\u00f6\u00dfer dein Genu\u00df.", "tokens": ["Des\u00b7to", "gr\u00f6\u00b7\u00dfer", "dein", "Ge\u00b7nu\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}