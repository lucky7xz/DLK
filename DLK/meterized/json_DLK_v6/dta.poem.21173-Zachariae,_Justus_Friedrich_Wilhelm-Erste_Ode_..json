{"dta.poem.21173": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Erste Ode .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20676-2", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Denk ihn hinaus \u2014 den schrecklichen Gedanken,", "tokens": ["Denk", "ihn", "hin\u00b7aus", "den", "schreck\u00b7li\u00b7chen", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APZR", "$(", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Der m\u00e4chtig dich ergreift!", "tokens": ["Der", "m\u00e4ch\u00b7tig", "dich", "er\u00b7greift", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie schwarz! \u2014 Er liegt auf der gebeugten Seele,", "tokens": ["Wie", "schwarz", "!", "Er", "liegt", "auf", "der", "ge\u00b7beug\u00b7ten", "See\u00b7le", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$.", "$(", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Wie ein Gebirge liegt.", "tokens": ["Wie", "ein", "Ge\u00b7bir\u00b7ge", "liegt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sie liebt dich nicht! Tief im zerri\u00dfnen Herzen", "tokens": ["Sie", "liebt", "dich", "nicht", "!", "Tief", "im", "zer\u00b7ri\u00df\u00b7nen", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$.", "ADJD", "APPRART", "ADJA", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Sagts ein geheim Gef\u00fchl.", "tokens": ["Sagts", "ein", "ge\u00b7heim", "Ge\u00b7f\u00fchl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bald w\u00e4chst es auf, und mit dem lautsten Donner", "tokens": ["Bald", "w\u00e4chst", "es", "auf", ",", "und", "mit", "dem", "lauts\u00b7ten", "Don\u00b7ner"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ruft es: Sie liebt dich nicht!", "tokens": ["Ruft", "es", ":", "Sie", "liebt", "dich", "nicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "O Mitternacht, die dicken Finsternisse", "tokens": ["O", "Mit\u00b7ter\u00b7nacht", ",", "die", "di\u00b7cken", "Fins\u00b7ter\u00b7nis\u00b7se"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sind noch nicht finster gnug;", "tokens": ["Sind", "noch", "nicht", "fins\u00b7ter", "gnug", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PTKNEG", "ADJD", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Verh\u00fclle doch in zehnmal schw\u00e4rzre Schatten", "tokens": ["Ver\u00b7h\u00fcl\u00b7le", "doch", "in", "zehn\u00b7mal", "schw\u00e4rz\u00b7re", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den thr\u00e4nenvollen Blick!", "tokens": ["Den", "thr\u00e4\u00b7nen\u00b7vol\u00b7len", "Blick", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Sie liebt dich nicht! Ich kan dir nicht entfliehen,", "tokens": ["Sie", "liebt", "dich", "nicht", "!", "Ich", "kan", "dir", "nicht", "ent\u00b7flie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$.", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gedanke, voller Quaal!", "tokens": ["Ge\u00b7dan\u00b7ke", ",", "vol\u00b7ler", "Qua\u00b7al", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "La\u00df ab, la\u00df ab! Schon blutet dir das Opfer", "tokens": ["La\u00df", "ab", ",", "la\u00df", "ab", "!", "Schon", "blu\u00b7tet", "dir", "das", "Op\u00b7fer"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PTKVZ", "$,", "VVIMP", "PTKVZ", "$.", "ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Schon stirbt das kalte Herz.", "tokens": ["Schon", "stirbt", "das", "kal\u00b7te", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}