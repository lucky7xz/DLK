{"dta.poem.5792": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Eigenschafften der  \n Materie.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gleichwie es wahr, da\u00df nie ein C\u00f6rper sich", "tokens": ["Gleich\u00b7wie", "es", "wahr", ",", "da\u00df", "nie", "ein", "C\u00f6r\u00b7per", "sich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ADJD", "$,", "KOUS", "ADV", "ART", "NN", "PRF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Von seiner Stelle r\u00fchren k\u00f6nne,", "tokens": ["Von", "sei\u00b7ner", "Stel\u00b7le", "r\u00fch\u00b7ren", "k\u00f6n\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df nicht ein andrer gleich an dessen Stelle renne;", "tokens": ["Da\u00df", "nicht", "ein", "an\u00b7drer", "gleich", "an", "des\u00b7sen", "Stel\u00b7le", "ren\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ART", "ADJA", "ADV", "APPR", "PRELAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So macht sich circkelweis ein schneller Strich.", "tokens": ["So", "macht", "sich", "cir\u00b7ckel\u00b7weis", "ein", "schnel\u00b7ler", "Strich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der C\u00f6rper, der zuerst gedr\u00fcckt, dr\u00fcckt bis zum letzten,", "tokens": ["Der", "C\u00f6r\u00b7per", ",", "der", "zu\u00b7erst", "ge\u00b7dr\u00fcckt", ",", "dr\u00fcckt", "bis", "zum", "letz\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "VVPP", "$,", "VVFIN", "APPR", "APPRART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der sich nicht eher r\u00fchrt, als in dem Augenblick,", "tokens": ["Der", "sich", "nicht", "e\u00b7her", "r\u00fchrt", ",", "als", "in", "dem", "Au\u00b7gen\u00b7blick", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "PTKNEG", "ADV", "VVFIN", "$,", "KOUS", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wenn sich der erste zieht zur\u00fcck.", "tokens": ["Wenn", "sich", "der", "ers\u00b7te", "zieht", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "ADJA", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Auf diese Weise dr\u00fcckt sich alles, und wie weit", "tokens": ["Auf", "die\u00b7se", "Wei\u00b7se", "dr\u00fcckt", "sich", "al\u00b7les", ",", "und", "wie", "weit"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "PRF", "PIS", "$,", "KON", "PWAV", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der Raum auch ist, den die Bewegung f\u00fcllet;", "tokens": ["Der", "Raum", "auch", "ist", ",", "den", "die", "Be\u00b7we\u00b7gung", "f\u00fcl\u00b7let", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Entsteht das R\u00fchren doch in steter Richtigkeit", "tokens": ["Ent\u00b7steht", "das", "R\u00fch\u00b7ren", "doch", "in", "ste\u00b7ter", "Rich\u00b7tig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Jm Augenblick, vergeht auch in derselben Zeit.", "tokens": ["Jm", "Au\u00b7gen\u00b7blick", ",", "ver\u00b7geht", "auch", "in", "der\u00b7sel\u00b7ben", "Zeit", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "VVFIN", "ADV", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}