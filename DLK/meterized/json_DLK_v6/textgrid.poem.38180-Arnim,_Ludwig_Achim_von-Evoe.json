{"textgrid.poem.38180": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Evoe", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freut euch ihr lieben Knaben!", "tokens": ["Freut", "euch", "ihr", "lie\u00b7ben", "Kna\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Herbst erzeigt sich wohl,", "tokens": ["Der", "Herbst", "er\u00b7zeigt", "sich", "wohl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die lang getrauert haben,", "tokens": ["Die", "lang", "ge\u00b7trau\u00b7ert", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Heut wollen wir werden voll.", "tokens": ["Heut", "wol\u00b7len", "wir", "wer\u00b7den", "voll", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VAFIN", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Wir haben vormals den sauren Wein", "tokens": ["Wir", "ha\u00b7ben", "vor\u00b7mals", "den", "sau\u00b7ren", "Wein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Gar theuer genommen an,", "tokens": ["Gar", "theu\u00b7er", "ge\u00b7nom\u00b7men", "an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Das wollen wir heute bringen ein,", "tokens": ["Das", "wol\u00b7len", "wir", "heu\u00b7te", "brin\u00b7gen", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "VVINF", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Der s\u00fc\u00dfe Most, der neue Wein,", "tokens": ["Der", "s\u00fc\u00b7\u00dfe", "Most", ",", "der", "neu\u00b7e", "Wein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Wird uns gar gern eingahn.", "tokens": ["Wird", "uns", "gar", "gern", "ein\u00b7gahn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Was wir vers\u00e4umet haben,", "tokens": ["Was", "wir", "ver\u00b7s\u00e4u\u00b7met", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das machen wir nun gleich,", "tokens": ["Das", "ma\u00b7chen", "wir", "nun", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit Wein wollen wir uns laben", "tokens": ["Mit", "Wein", "wol\u00b7len", "wir", "uns", "la\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VMFIN", "PPER", "PRF", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Hier und in Oesterreich,", "tokens": ["Hier", "und", "in", "O\u00b7es\u00b7ter\u00b7reich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "APPR", "NE", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "In einer neuen Krausen", "tokens": ["In", "ei\u00b7ner", "neu\u00b7en", "Krau\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wollen wir ihn nehmen an,", "tokens": ["Wol\u00b7len", "wir", "ihn", "neh\u00b7men", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ob ihm soll uns nit grausen,", "tokens": ["Ob", "ihm", "soll", "uns", "nit", "grau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Bis uns der Kopf thut sausen,", "tokens": ["Bis", "uns", "der", "Kopf", "thut", "sau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Nit eh gehn wir davon.", "tokens": ["Nit", "eh", "gehn", "wir", "da\u00b7von", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVFIN", "PPER", "PAV", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "In einem Keller tiefe,", "tokens": ["In", "ei\u00b7nem", "Kel\u00b7ler", "tie\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wollen wir uns senken ein;", "tokens": ["Wol\u00b7len", "wir", "uns", "sen\u00b7ken", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Darnach dem Wirthsknecht rufen:", "tokens": ["Dar\u00b7nach", "dem", "Wirths\u00b7knecht", "ru\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Trag her ein k\u00fchlen Wein!", "tokens": ["Trag", "her", "ein", "k\u00fch\u00b7len", "Wein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APZR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Von dir wollen wir nit weichen,", "tokens": ["Von", "dir", "wol\u00b7len", "wir", "nit", "wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bis da\u00df wir werden voll,", "tokens": ["Bis", "da\u00df", "wir", "wer\u00b7den", "voll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "La\u00df uns nur Wein herreichen,", "tokens": ["La\u00df", "uns", "nur", "Wein", "her\u00b7rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Gesellen, ich will euch zeigen,", "tokens": ["Ge\u00b7sel\u00b7len", ",", "ich", "will", "euch", "zei\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Der Wein thut was er soll.", "tokens": ["Der", "Wein", "thut", "was", "er", "soll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PWS", "PPER", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wirthsknecht, nun merk uns eben,", "tokens": ["Wirths\u00b7knecht", ",", "nun", "merk", "uns", "e\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was unser Meinung sey,", "tokens": ["Was", "un\u00b7ser", "Mei\u00b7nung", "sey", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Kein Pfenning wir dir geben,", "tokens": ["Kein", "Pfen\u00b7ning", "wir", "dir", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du bringst uns dann herbey", "tokens": ["Du", "bringst", "uns", "dann", "her\u00b7bey"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ein guten feisten Braten,", "tokens": ["Ein", "gu\u00b7ten", "feis\u00b7ten", "Bra\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Den wollen wir gern haben,", "tokens": ["Den", "wol\u00b7len", "wir", "gern", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wir m\u00f6gen sein nicht gerathen,", "tokens": ["Wir", "m\u00f6\u00b7gen", "sein", "nicht", "ge\u00b7ra\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "PTKNEG", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Eine gute Henne gesotten,", "tokens": ["Ei\u00b7ne", "gu\u00b7te", "Hen\u00b7ne", "ge\u00b7sot\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.9": {"text": "Die f\u00fcgt wohl solchen Knaben.", "tokens": ["Die", "f\u00fcgt", "wohl", "sol\u00b7chen", "Kna\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ein Bergwerk haben wir funden", "tokens": ["Ein", "Berg\u00b7werk", "ha\u00b7ben", "wir", "fun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVFIN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Das macht uns heut noch reich,", "tokens": ["Das", "macht", "uns", "heut", "noch", "reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das bringt uns Freud und Wonne,", "tokens": ["Das", "bringt", "uns", "Freud", "und", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu Wien in Oesterreich,", "tokens": ["Zu", "Wi\u00b7en", "in", "O\u00b7es\u00b7ter\u00b7reich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da finden wir aufgeschlagen", "tokens": ["Da", "fin\u00b7den", "wir", "auf\u00b7ge\u00b7schla\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Gar manche Grube fein,", "tokens": ["Gar", "man\u00b7che", "Gru\u00b7be", "fein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Da f\u00fcllen wir unsern Kragen,", "tokens": ["Da", "f\u00fcl\u00b7len", "wir", "un\u00b7sern", "Kra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Den Bauch und auch den Magen", "tokens": ["Den", "Bauch", "und", "auch", "den", "Ma\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Wohl bei dem Osterwein.", "tokens": ["Wohl", "bei", "dem", "Os\u00b7ter\u00b7wein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Wohl auf ihr lieben Gesellen,", "tokens": ["Wohl", "auf", "ihr", "lie\u00b7ben", "Ge\u00b7sel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wohl in das Bergwerk ein,", "tokens": ["Wohl", "in", "das", "Berg\u00b7werk", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die alle Morgen w\u00f6llen", "tokens": ["Die", "al\u00b7le", "Mor\u00b7gen", "w\u00f6l\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Trinken gut Wermuthwein,", "tokens": ["Trin\u00b7ken", "gut", "Wer\u00b7muth\u00b7wein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Das sind die rechten Gesellen,", "tokens": ["Das", "sind", "die", "rech\u00b7ten", "Ge\u00b7sel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Die in das Bergwerk fahrn,", "tokens": ["Die", "in", "das", "Berg\u00b7werk", "fahrn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Es sind die rechten Knappen,", "tokens": ["Es", "sind", "die", "rech\u00b7ten", "Knap\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Sie sitzen in einer Kappen,", "tokens": ["Sie", "sit\u00b7zen", "in", "ei\u00b7ner", "Kap\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Gott woll sie all bewahrn.", "tokens": ["Gott", "woll", "sie", "all", "be\u00b7wahrn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PIAT", "ADJA", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Den Herren allen gleiche,", "tokens": ["Den", "Her\u00b7ren", "al\u00b7len", "glei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "W\u00fcnsch ich viel Gl\u00fcck und Heil,", "tokens": ["W\u00fcnsch", "ich", "viel", "Gl\u00fcck", "und", "Heil", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PIAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die heut von Oesterreiche", "tokens": ["Die", "heut", "von", "O\u00b7es\u00b7ter\u00b7rei\u00b7che"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Bringen ein rechtes Theil", "tokens": ["Brin\u00b7gen", "ein", "rech\u00b7tes", "Theil"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Des Erzes aus der Grube,", "tokens": ["Des", "Er\u00b7zes", "aus", "der", "Gru\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Noe funden hat,", "tokens": ["Die", "Noe", "fun\u00b7den", "hat", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VAFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Sie erfreut gar manchen Buben,", "tokens": ["Sie", "er\u00b7freut", "gar", "man\u00b7chen", "Bu\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Um Sorg g\u00e4b er kein Ruben,", "tokens": ["Um", "Sorg", "g\u00e4b", "er", "kein", "Ru\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+---+-", "measure": "dactylic.init"}, "line.9": {"text": "Sein Herz in Freuden staht.", "tokens": ["Sein", "Herz", "in", "Freu\u00b7den", "staht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Wir haben uns vermessen,", "tokens": ["Wir", "ha\u00b7ben", "uns", "ver\u00b7mes\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gut Gesellen allgemein,", "tokens": ["Gut", "Ge\u00b7sel\u00b7len", "all\u00b7ge\u00b7mein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wir sollen nit vergessen", "tokens": ["Wir", "sol\u00b7len", "nit", "ver\u00b7ges\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Fuhrleut gro\u00df und klein,", "tokens": ["Der", "Fuhr\u00b7leut", "gro\u00df", "und", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die in das Elsas fahren,", "tokens": ["Die", "in", "das", "El\u00b7sas", "fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NE", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und bringen rheinschen Wein,", "tokens": ["Und", "brin\u00b7gen", "rhein\u00b7schen", "Wein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ihr Lob wolln wir nit sparen,", "tokens": ["Ihr", "Lob", "wolln", "wir", "nit", "spa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Gott woll sie all bewahren,", "tokens": ["Gott", "woll", "sie", "all", "be\u00b7wah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PIAT", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Maria die K\u00f6nigein.", "tokens": ["Ma\u00b7ria", "die", "K\u00f6\u00b7ni\u00b7gein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Dazu die Franken alle,", "tokens": ["Da\u00b7zu", "die", "Fran\u00b7ken", "al\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "PIS", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die bauen guten Wein,", "tokens": ["Die", "bau\u00b7en", "gu\u00b7ten", "Wein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gott gr\u00fc\u00df sie mit reichem Schalle,", "tokens": ["Gott", "gr\u00fc\u00df", "sie", "mit", "rei\u00b7chem", "Schal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihr Lob das ist nit klein,", "tokens": ["Ihr", "Lob", "das", "ist", "nit", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PDS", "VAFIN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dazu die Fuhrleut gute,", "tokens": ["Da\u00b7zu", "die", "Fuhr\u00b7leut", "gu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "All die gen Frankfurt fahrn,", "tokens": ["All", "die", "gen", "Frank\u00b7furt", "fahrn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "APPR", "NE", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Gott habs in seiner Hute,", "tokens": ["Gott", "habs", "in", "sei\u00b7ner", "Hu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Maria die viel gute", "tokens": ["Ma\u00b7ria", "die", "viel", "gu\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ART", "PIAT", "ADJA"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Woll sie allzeit bewahrn.", "tokens": ["Woll", "sie", "all\u00b7zeit", "be\u00b7wahrn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Im Wirthshaus ist gut leben,", "tokens": ["Im", "Wirths\u00b7haus", "ist", "gut", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wenn k\u00f6mmt der heurig Wein,", "tokens": ["Wenn", "k\u00f6mmt", "der", "heu\u00b7rig", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da wollen wir dann streben,", "tokens": ["Da", "wol\u00b7len", "wir", "dann", "stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und wollen fr\u00f6hlich sein,", "tokens": ["Und", "wol\u00b7len", "fr\u00f6h\u00b7lich", "sein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADJD", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Bratw\u00fcrst, jung Schwein und Hahnen", "tokens": ["Brat\u00b7w\u00fcrst", ",", "jung", "Schwein", "und", "Hah\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADJD", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Soll man uns tragen her,", "tokens": ["Soll", "man", "uns", "tra\u00b7gen", "her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Und andre Gericht und Nahmen", "tokens": ["Und", "and\u00b7re", "Ge\u00b7richt", "und", "Nah\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "KON", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "So kommen wir zusammen", "tokens": ["So", "kom\u00b7men", "wir", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "All voll und selten leer.", "tokens": ["All", "voll", "und", "sel\u00b7ten", "leer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "ADJD", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Reich' W\u00fcrfel her und Karten,", "tokens": ["Reich'", "W\u00fcr\u00b7fel", "her", "und", "Kar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PTKVZ", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Bretspiel wolln wir han,", "tokens": ["Ein", "Bret\u00b7spiel", "wolln", "wir", "han", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So m\u00f6gen wir erwarten,", "tokens": ["So", "m\u00f6\u00b7gen", "wir", "er\u00b7war\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den n\u00fcchtern Morgenhahn,", "tokens": ["Den", "n\u00fcch\u00b7tern", "Mor\u00b7gen\u00b7hahn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dann wollen wir noch haben", "tokens": ["Dann", "wol\u00b7len", "wir", "noch", "ha\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VAFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ein guten Salvewein,", "tokens": ["Ein", "gu\u00b7ten", "Sal\u00b7ve\u00b7wein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Damit wir uns erlaben,", "tokens": ["Da\u00b7mit", "wir", "uns", "er\u00b7la\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Gott beh\u00fct die frommen Knaben,", "tokens": ["Gott", "be\u00b7h\u00fct", "die", "from\u00b7men", "Kna\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Die stets voll wollen seyn.", "tokens": ["Die", "stets", "voll", "wol\u00b7len", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "VMFIN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}