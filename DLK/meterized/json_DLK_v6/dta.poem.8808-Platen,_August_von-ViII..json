{"dta.poem.8808": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "ViII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Wir kennen dich, wir fassen dich, wiewohl du schweigst:", "tokens": ["Wir", "ken\u00b7nen", "dich", ",", "wir", "fas\u00b7sen", "dich", ",", "wie\u00b7wohl", "du", "schweigst", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der schelm'sche Zug um deinen Mund und um dein Aug'", "tokens": ["Der", "schel\u00b7m'\u00b7sche", "Zug", "um", "dei\u00b7nen", "Mund", "und", "um", "dein", "Aug'"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "KON", "APPR", "PPOSAT", "NN"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Verr\u00e4th auf allen Gassen dich, wiewohl du schweigst;", "tokens": ["Ver\u00b7r\u00e4\u00b7th", "auf", "al\u00b7len", "Gas\u00b7sen", "dich", ",", "wie\u00b7wohl", "du", "schweigst", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "NN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Verstellung irrt um deine Stirn so liebenswerth,", "tokens": ["Ver\u00b7stel\u00b7lung", "irrt", "um", "dei\u00b7ne", "Stirn", "so", "lie\u00b7bens\u00b7werth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPOSAT", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wie sollten wir verlassen dich, wiewohl du schweigst?", "tokens": ["Wie", "soll\u00b7ten", "wir", "ver\u00b7las\u00b7sen", "dich", ",", "wie\u00b7wohl", "du", "schweigst", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Es ist der Wein, den ", "tokens": ["Es", "ist", "der", "Wein", ",", "den"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "ART"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Doch Liebe macht erblassen dich, wiewohl du schweigst.", "tokens": ["Doch", "Lie\u00b7be", "macht", "er\u00b7blas\u00b7sen", "dich", ",", "wie\u00b7wohl", "du", "schweigst", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}