{"dta.poem.13019": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Siebendes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr Anblick fiele mir besonders ins Gesicht;", "tokens": ["Ihr", "An\u00b7blick", "fie\u00b7le", "mir", "be\u00b7son\u00b7ders", "ins", "Ge\u00b7sicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kein Wunder, dunckte mich, wann sie die Bahne bricht.", "tokens": ["Kein", "Wun\u00b7der", ",", "dunck\u00b7te", "mich", ",", "wann", "sie", "die", "Bah\u00b7ne", "bricht", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "VVFIN", "PPER", "$,", "PWAV", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Schwert, Harnisch, Helm und Schild erh\u00f6hten ihr Betragen,", "tokens": ["Schwert", ",", "Har\u00b7nisch", ",", "Helm", "und", "Schild", "er\u00b7h\u00f6h\u00b7ten", "ihr", "Be\u00b7tra\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "$,", "NN", "KON", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "100Als wollte sie mit Macht sich in das Mittel schlagen.", "tokens": ["woll\u00b7te", "sie", "mit", "Macht", "sich", "in", "das", "Mit\u00b7tel", "schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "NN", "PRF", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.5": {"text": "Mehr Oberherrlichkeit mit Herrsch-Begier gepaart", "tokens": ["Mehr", "O\u00b7ber\u00b7herr\u00b7lich\u00b7keit", "mit", "Herr\u00b7schBe\u00b7gier", "ge\u00b7paart"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "APPR", "NN", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Erkannte man in ihr als wahrer Hoheit Art;", "tokens": ["Er\u00b7kann\u00b7te", "man", "in", "ihr", "als", "wah\u00b7rer", "Ho\u00b7heit", "Art", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "PPER", "KOUS", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Jhr blau-gesticktes Kleid schien alles auszukunden,", "tokens": ["Ihr", "blau\u00b7ge\u00b7stick\u00b7tes", "Kleid", "schien", "al\u00b7les", "aus\u00b7zu\u00b7kun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Weil auf desselben Stoff viel hundert Ohren stunden.", "tokens": ["Weil", "auf", "des\u00b7sel\u00b7ben", "Stoff", "viel", "hun\u00b7dert", "Oh\u00b7ren", "stun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PDAT", "NN", "ADV", "CARD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "105Mich hielte diese Tracht befremdet und entz\u00fcckt;", "tokens": ["hiel\u00b7te", "die\u00b7se", "Tracht", "be\u00b7frem\u00b7det", "und", "ent\u00b7z\u00fcckt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "NN", "VVPP", "KON", "VVPP", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.10": {"text": "Die Falten waren auch mit Augen ausgeschm\u00fcckt,", "tokens": ["Die", "Fal\u00b7ten", "wa\u00b7ren", "auch", "mit", "Au\u00b7gen", "aus\u00b7ge\u00b7schm\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die Bliz-geschwind den Blick nach allen Seiten schossen,", "tokens": ["Die", "Bli\u00b7zge\u00b7schwind", "den", "Blick", "nach", "al\u00b7len", "Sei\u00b7ten", "schos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Als h\u00e4tte sie der Streit und das Get\u00f6\u00df verdrossen.", "tokens": ["Als", "h\u00e4t\u00b7te", "sie", "der", "Streit", "und", "das", "Ge\u00b7t\u00f6\u00df", "ver\u00b7dros\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "ART", "NN", "KON", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}