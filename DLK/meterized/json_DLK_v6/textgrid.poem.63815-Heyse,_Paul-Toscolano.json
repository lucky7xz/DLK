{"textgrid.poem.63815": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Toscolano", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ja, das sind die alten Gassen,", "tokens": ["Ja", ",", "das", "sind", "die", "al\u00b7ten", "Gas\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Mauerschluchten, schauerk\u00fchl,", "tokens": ["Mau\u00b7er\u00b7schluch\u00b7ten", ",", "schau\u00b7er\u00b7k\u00fchl", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie ich damals dich verlassen,", "tokens": ["Wie", "ich", "da\u00b7mals", "dich", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mein gesegnet Herbstasyl!", "tokens": ["Mein", "ge\u00b7seg\u00b7net", "Herbs\u00b7ta\u00b7syl", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "VVPP", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "\u00dcber jener dunklen T\u00fcre,", "tokens": ["\u00dc\u00b7ber", "je\u00b7ner", "dunk\u00b7len", "T\u00fc\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die sich gastlich mir erschlo\u00df,", "tokens": ["Die", "sich", "gast\u00b7lich", "mir", "er\u00b7schlo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADJD", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00e4ngt verwittert noch das fr\u00fch're", "tokens": ["H\u00e4ngt", "ver\u00b7wit\u00b7tert", "noch", "das", "fr\u00fch'\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "VVFIN", "ADV", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Herbergsschild, das \u00bbwei\u00dfe Ro\u00df\u00ab.", "tokens": ["Her\u00b7bergs\u00b7schild", ",", "das", "\u00bb", "wei\u00b7\u00dfe", "Ro\u00df", "\u00ab", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "PRELS", "$(", "ADJA", "NN", "$(", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Kahl und d\u00fcster war mein Zimmer,", "tokens": ["Kahl", "und", "d\u00fcs\u00b7ter", "war", "mein", "Zim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Doch das Bette breit und rein;", "tokens": ["Doch", "das", "Bet\u00b7te", "breit", "und", "rein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "E\u00dfbar wohl die Kost nicht immer,", "tokens": ["E\u00df\u00b7bar", "wohl", "die", "Kost", "nicht", "im\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "ART", "NN", "PTKNEG", "ADV", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Aber trinkbar stets der Wein.", "tokens": ["A\u00b7ber", "trink\u00b7bar", "stets", "der", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und wie herrlich all die Pfade", "tokens": ["Und", "wie", "herr\u00b7lich", "all", "die", "Pfa\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ADJD", "PIAT", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bei dem Kirchlein steil hinab,", "tokens": ["Bei", "dem", "Kirch\u00b7lein", "steil", "hin\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo am blauen Seegestade", "tokens": ["Wo", "am", "blau\u00b7en", "See\u00b7ge\u00b7sta\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mir der \u00d6lwald Schatten gab!", "tokens": ["Mir", "der", "\u00d6l\u00b7wald", "Schat\u00b7ten", "gab", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Steil hinauf zu den Zypressen", "tokens": ["Steil", "hin\u00b7auf", "zu", "den", "Zyp\u00b7res\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "ART", "NN"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.2": {"text": "Um Gainos Kl\u00f6sterlein,", "tokens": ["Um", "Gai\u00b7nos", "Kl\u00f6s\u00b7ter\u00b7lein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUI", "NE", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo ich sa\u00df und weltvergessen", "tokens": ["Wo", "ich", "sa\u00df", "und", "welt\u00b7ver\u00b7ges\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "VVFIN", "KON", "PWAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tr\u00e4umte weit ins Land hinein.", "tokens": ["Tr\u00e4um\u00b7te", "weit", "ins", "Land", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Wenn die Sonne dann versank, o", "tokens": ["Wenn", "die", "Son\u00b7ne", "dann", "ver\u00b7sank", ",", "o"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "ART", "NN", "ADV", "VVFIN", "$,", "FM"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie begl\u00fcckt ich heimw\u00e4rts ging,", "tokens": ["Wie", "be\u00b7gl\u00fcckt", "ich", "heim\u00b7w\u00e4rts", "ging", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo mich im ", "tokens": ["Wo", "mich", "im"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "PPER", "APPRART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Mein frugales Mahl empfing!", "tokens": ["Mein", "fru\u00b7ga\u00b7les", "Mahl", "emp\u00b7fing", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Bracht' ich doch im Skizzenbuche", "tokens": ["Bracht'", "ich", "doch", "im", "Skiz\u00b7zen\u00b7bu\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Manches Landsch\u00e4ftchen mit heim,", "tokens": ["Man\u00b7ches", "Land\u00b7sch\u00e4ft\u00b7chen", "mit", "heim", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "PTKVZ", "$,"], "meter": "+-++--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Sehr bescheidne Pfuschversuche", "tokens": ["Sehr", "be\u00b7scheid\u00b7ne", "Pfu\u00b7schver\u00b7su\u00b7che"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und dazwischen manchen Reim.", "tokens": ["Und", "da\u00b7zwi\u00b7schen", "man\u00b7chen", "Reim", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "PIAT", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.8": {"line.1": {"text": "Dann mit meinem Wirt vertraulich", "tokens": ["Dann", "mit", "mei\u00b7nem", "Wirt", "ver\u00b7trau\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schwatzt' ich noch im G\u00e4rtchen lang,", "tokens": ["Schwatzt'", "ich", "noch", "im", "G\u00e4rt\u00b7chen", "lang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn die Nachtluft weich und laulich", "tokens": ["Wenn", "die", "Nacht\u00b7luft", "weich", "und", "lau\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00dcberm See die Fl\u00fcgel schwang.", "tokens": ["\u00dc\u00b7berm", "See", "die", "Fl\u00fc\u00b7gel", "schwang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Liebste, und es nimmt dich wunder,", "tokens": ["Liebs\u00b7te", ",", "und", "es", "nimmt", "dich", "wun\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "PPER", "VVFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df ich gern hier blieb zu Gast,", "tokens": ["Da\u00df", "ich", "gern", "hier", "blieb", "zu", "Gast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da mir all der eitle Plunder", "tokens": ["Da", "mir", "all", "der", "eit\u00b7le", "Plun\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PIAT", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der Kulturwelt tief verha\u00dft?", "tokens": ["Der", "Kul\u00b7tur\u00b7welt", "tief", "ver\u00b7ha\u00dft", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Ihre Fratze st\u00f6rt mich minder", "tokens": ["Ih\u00b7re", "Frat\u00b7ze", "st\u00f6rt", "mich", "min\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hier am Busen der Natur,", "tokens": ["Hier", "am", "Bu\u00b7sen", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Unter Menschen, die wie Kinder", "tokens": ["Un\u00b7ter", "Men\u00b7schen", ",", "die", "wie", "Kin\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "PRELS", "KOKOM", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Harmlos gehn auf ihrer Spur.", "tokens": ["Harm\u00b7los", "gehn", "auf", "ih\u00b7rer", "Spur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Etwas Einsamkeit und Stille,", "tokens": ["Et\u00b7was", "Ein\u00b7sam\u00b7keit", "und", "Stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Etwas Sch\u00f6nheit ringsumher,", "tokens": ["Et\u00b7was", "Sch\u00f6n\u00b7heit", "rings\u00b7um\u00b7her", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Traum und Zauber der Idylle \u2013", "tokens": ["Traum", "und", "Zau\u00b7ber", "der", "I\u00b7dyl\u00b7le", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was bedarf ein Dichter mehr?", "tokens": ["Was", "be\u00b7darf", "ein", "Dich\u00b7ter", "mehr", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Da ich all dies hier besessen,", "tokens": ["Da", "ich", "all", "dies", "hier", "be\u00b7ses\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "PDS", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "D\u00fcnkt mich wie ein Zauberschlo\u00df", "tokens": ["D\u00fcnkt", "mich", "wie", "ein", "Zau\u00b7ber\u00b7schlo\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "KOKOM", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Trotz dem mangelhaften Essen", "tokens": ["Trotz", "dem", "man\u00b7gel\u00b7haf\u00b7ten", "Es\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Toscolanos \u00bbWei\u00dfes Ro\u00df\u00ab.", "tokens": ["Tos\u00b7co\u00b7la\u00b7nos", "\u00bb", "Wei\u00b7\u00dfes", "Ro\u00df", "\u00ab", "."], "token_info": ["word", "punct", "word", "word", "punct", "punct"], "pos": ["NE", "$(", "ADJA", "NN", "$(", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}