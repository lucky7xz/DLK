{"textgrid.poem.58595": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "27.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So arm, so blo\u00df, gebeugt und klein gleichwie ein Wurm der Erden", "tokens": ["So", "arm", ",", "so", "blo\u00df", ",", "ge\u00b7beugt", "und", "klein", "gleich\u00b7wie", "ein", "Wurm", "der", "Er\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADV", "$,", "VVPP", "KON", "ADJD", "KON", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Mu\u00dft du durch Kreuz und S\u00fcndennot in deinen Augen werden,", "tokens": ["Mu\u00dft", "du", "durch", "Kreuz", "und", "S\u00fcn\u00b7den\u00b7not", "in", "dei\u00b7nen", "Au\u00b7gen", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "NN", "KON", "NN", "APPR", "PPOSAT", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "So will der H\u00f6chste helfen dann", "tokens": ["So", "will", "der", "H\u00f6chs\u00b7te", "hel\u00b7fen", "dann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem, der sich selbst nicht helfen kann;", "tokens": ["Dem", ",", "der", "sich", "selbst", "nicht", "hel\u00b7fen", "kann", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "PRF", "ADV", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wer als ein Wurm in sich entbl\u00f6\u00dft,", "tokens": ["Wer", "als", "ein", "Wurm", "in", "sich", "ent\u00b7bl\u00f6\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "KOKOM", "ART", "NN", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der ist es, den der Herr erl\u00f6st.", "tokens": ["Der", "ist", "es", ",", "den", "der", "Herr", "er\u00b7l\u00f6st", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich armes W\u00fcrmlein will mich auch zu Gottes F\u00fc\u00dfen schmiegen;", "tokens": ["Ich", "ar\u00b7mes", "W\u00fcrm\u00b7lein", "will", "mich", "auch", "zu", "Got\u00b7tes", "F\u00fc\u00b7\u00dfen", "schmie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "VMFIN", "PPER", "ADV", "APPR", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.8": {"text": "La\u00df mich im Kot und Ohnmacht nicht, mein Herr und Heiland, liegen!", "tokens": ["La\u00df", "mich", "im", "Kot", "und", "Ohn\u00b7macht", "nicht", ",", "mein", "Herr", "und", "Hei\u00b7land", ",", "lie\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVIMP", "PPER", "APPRART", "NN", "KON", "NN", "PTKNEG", "$,", "PPOSAT", "NN", "KON", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.9": {"text": "Ich h\u00f6r', da\u00df dein Erbarmen spricht:", "tokens": ["Ich", "h\u00f6r'", ",", "da\u00df", "dein", "Er\u00b7bar\u00b7men", "spricht", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "\u00bbdu armer Haufe, f\u00fcrcht dich nicht!\u00ab", "tokens": ["\u00bb", "du", "ar\u00b7mer", "Hau\u00b7fe", ",", "f\u00fcrcht", "dich", "nicht", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "ADJA", "NN", "$,", "VVFIN", "PPER", "PTKNEG", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "St\u00e4rk meine schwache Glaubenshand,", "tokens": ["St\u00e4rk", "mei\u00b7ne", "schwa\u00b7che", "Glau\u00b7bens\u00b7hand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Du kennest meinen Jammerstand!", "tokens": ["Du", "ken\u00b7nest", "mei\u00b7nen", "Jam\u00b7mer\u00b7stand", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}