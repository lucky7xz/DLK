{"textgrid.poem.24234": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00dcber die Ferne hin,", "tokens": ["\u00dc\u00b7ber", "die", "Fer\u00b7ne", "hin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "T\u00e4ler hin, Berge hin,", "tokens": ["T\u00e4\u00b7ler", "hin", ",", "Ber\u00b7ge", "hin", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "NN", "PTKVZ", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Durch alle Tage und N\u00e4chte hin,", "tokens": ["Durch", "al\u00b7le", "Ta\u00b7ge", "und", "N\u00e4ch\u00b7te", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sing ich zu dir, o Geliebte.", "tokens": ["Sing", "ich", "zu", "dir", ",", "o", "Ge\u00b7lieb\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "$,", "FM", "NN", "$."], "meter": "++-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "H\u00f6rst du mich?", "tokens": ["H\u00f6rst", "du", "mich", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Lausche dem Rauschen der B\u00e4ume im Regen,", "tokens": ["Lau\u00b7sche", "dem", "Rau\u00b7schen", "der", "B\u00e4u\u00b7me", "im", "Re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Lausche dem Winde, der \u00fcber die Halme", "tokens": ["Lau\u00b7sche", "dem", "Win\u00b7de", ",", "der", "\u00fc\u00b7ber", "die", "Hal\u00b7me"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "Mit dem z\u00e4rtlichen Fittiche hinstreift,", "tokens": ["Mit", "dem", "z\u00e4rt\u00b7li\u00b7chen", "Fit\u00b7ti\u00b7che", "hins\u00b7treift", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Lausche dem holden Munde der Nacht;", "tokens": ["Lau\u00b7sche", "dem", "hol\u00b7den", "Mun\u00b7de", "der", "Nacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Lausche in dich.", "tokens": ["Lau\u00b7sche", "in", "dich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Lausche geschlossenen Auges, h\u00f6re,", "tokens": ["Lau\u00b7sche", "ge\u00b7schlos\u00b7se\u00b7nen", "Au\u00b7ges", ",", "h\u00f6\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "H\u00f6re dein Herz, das rauschende, h\u00f6re,", "tokens": ["H\u00f6\u00b7re", "dein", "Herz", ",", "das", "rau\u00b7schen\u00b7de", ",", "h\u00f6\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$,", "ART", "ADJA", "$,", "VVFIN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "H\u00f6re dein Blut: es tr\u00e4gt meine Stimme,", "tokens": ["H\u00f6\u00b7re", "dein", "Blut", ":", "es", "tr\u00e4gt", "mei\u00b7ne", "Stim\u00b7me", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Tr\u00e4gt meine Liebe durch all dein Leben:", "tokens": ["Tr\u00e4gt", "mei\u00b7ne", "Lie\u00b7be", "durch", "all", "dein", "Le\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "PIAT", "PPOSAT", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.5": {"text": "Zu dir, um dich", "tokens": ["Zu", "dir", ",", "um", "dich"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "PPER", "$,", "KOUI", "PRF"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "T\u00f6nt mein Rufen,", "tokens": ["T\u00f6nt", "mein", "Ru\u00b7fen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "T\u00f6nt meine sehnsuchtsvolle Seele,", "tokens": ["T\u00f6nt", "mei\u00b7ne", "sehn\u00b7suchts\u00b7vol\u00b7le", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die dich sucht.", "tokens": ["Die", "dich", "sucht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "\u00dcber die Ferne hin,", "tokens": ["\u00dc\u00b7ber", "die", "Fer\u00b7ne", "hin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Berge hin, T\u00e4ler hin,", "tokens": ["Ber\u00b7ge", "hin", ",", "T\u00e4\u00b7ler", "hin", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "NN", "PTKVZ", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Durch alle Tage und N\u00e4chte hin", "tokens": ["Durch", "al\u00b7le", "Ta\u00b7ge", "und", "N\u00e4ch\u00b7te", "hin"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "KON", "NN", "PTKVZ"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sing ich zu dir, o Geliebte,", "tokens": ["Sing", "ich", "zu", "dir", ",", "o", "Ge\u00b7lieb\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "$,", "FM", "NN", "$,"], "meter": "++-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Singt meine Seele zu dir.", "tokens": ["Singt", "mei\u00b7ne", "See\u00b7le", "zu", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "PPER", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}}}}