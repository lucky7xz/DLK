{"textgrid.poem.46480": {"metadata": {"author": {"name": "Engelke, Gerrit", "birth": "N.A.", "death": "N.A."}, "title": "1L: Einsam im Zimmer klopft ein Herz", "genre": "verse", "period": "N.A.", "pub_year": 1916, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einsam im Zimmer klopft ein Herz", "tokens": ["Ein\u00b7sam", "im", "Zim\u00b7mer", "klopft", "ein", "Herz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPRART", "NN", "VVFIN", "ART", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Immerzu.", "tokens": ["Im\u00b7mer\u00b7zu", "."], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "+OO", "measure": "unknown.measure.single"}, "line.3": {"text": "In einem Gartenbaum schwillt ein Herz", "tokens": ["In", "ei\u00b7nem", "Gar\u00b7ten\u00b7baum", "schwillt", "ein", "Herz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Immerzu.", "tokens": ["Im\u00b7mer\u00b7zu", "."], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "+OO", "measure": "unknown.measure.single"}, "line.5": {"text": "In allen H\u00e4usern schlagen Herzen", "tokens": ["In", "al\u00b7len", "H\u00e4u\u00b7sern", "schla\u00b7gen", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Immerzu.", "tokens": ["Im\u00b7mer\u00b7zu", "."], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "+OO", "measure": "unknown.measure.single"}, "line.7": {"text": "Durch tausend Stra\u00dfen wandern Herzen", "tokens": ["Durch", "tau\u00b7send", "Stra\u00b7\u00dfen", "wan\u00b7dern", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "CARD", "NN", "VVFIN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Immerzu, immerzu.", "tokens": ["Im\u00b7mer\u00b7zu", ",", "im\u00b7mer\u00b7zu", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADV", "$,", "ADV", "$."], "meter": "+-++-O", "measure": "unknown.measure.tri"}}, "stanza.2": {"line.1": {"text": "Von Halm zu Halm durch Felder,", "tokens": ["Von", "Halm", "zu", "Halm", "durch", "Fel\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von Baum zu Baum durch W\u00e4lder,", "tokens": ["Von", "Baum", "zu", "Baum", "durch", "W\u00e4l\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "NE", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In Schiffen, Bahnen, Saaten,", "tokens": ["In", "Schif\u00b7fen", ",", "Bah\u00b7nen", ",", "Saa\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In Stadt und St\u00e4dten, Staaten:", "tokens": ["In", "Stadt", "und", "St\u00e4d\u00b7ten", ",", "Staa\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Klopfen, tanzen, t\u00f6nen Herzen", "tokens": ["Klop\u00b7fen", ",", "tan\u00b7zen", ",", "t\u00f6\u00b7nen", "Her\u00b7zen"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "$,", "VVFIN", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Immerimmerzu!", "tokens": ["Im\u00b7me\u00b7rim\u00b7mer\u00b7zu", "!"], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Und Du, du Mutter-Erde-Sohn,", "tokens": ["Und", "Du", ",", "du", "Mut\u00b7ter\u00b7Er\u00b7de\u00b7Sohn", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "H\u00f6rst du deinen Herzens-Ton!", "tokens": ["H\u00f6rst", "du", "dei\u00b7nen", "Her\u00b7zens\u00b7Ton", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00f6rst aus Milliarden Dingen", "tokens": ["H\u00f6rst", "aus", "Mil\u00b7li\u00b7ar\u00b7den", "Din\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einen Herzklang um dich schwingen?:", "tokens": ["Ei\u00b7nen", "Herz\u00b7klang", "um", "dich", "schwin\u00b7gen", "?", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "VVFIN", "$.", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Horch! in Allen Herzen braust die Welt!", "tokens": ["Horch", "!", "in", "Al\u00b7len", "Her\u00b7zen", "braust", "die", "Welt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "APPR", "NE", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Immerzu!", "tokens": ["Im\u00b7mer\u00b7zu", "!"], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "+OO", "measure": "unknown.measure.single"}}}}}