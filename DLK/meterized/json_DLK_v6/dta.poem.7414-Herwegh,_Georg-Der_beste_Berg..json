{"dta.poem.7414": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Der beste Berg.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192430", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es ist ein Berg auf Erden,", "tokens": ["Es", "ist", "ein", "Berg", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Gutenberg genannt,", "tokens": ["Der", "Gu\u00b7ten\u00b7berg", "ge\u00b7nannt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der soll besungen werden", "tokens": ["Der", "soll", "be\u00b7sun\u00b7gen", "wer\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "VVINF", "VAFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wohl auf und ab im Land.", "tokens": ["Wohl", "auf", "und", "ab", "im", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "KON", "PTKVZ", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Er heget keine Veste,", "tokens": ["Er", "he\u00b7get", "kei\u00b7ne", "Ves\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Er pfleget keinen Wein,", "tokens": ["Er", "pfle\u00b7get", "kei\u00b7nen", "Wein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wird doch stets der beste", "tokens": ["Und", "wird", "doch", "stets", "der", "bes\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ADV", "ADV", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von allen Bergen sein.", "tokens": ["Von", "al\u00b7len", "Ber\u00b7gen", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Es ist ein Berg auf Erden,", "tokens": ["Es", "ist", "ein", "Berg", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der steht zu Mainz am Rhein,", "tokens": ["Der", "steht", "zu", "Mainz", "am", "Rhein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "NE", "APPRART", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit trutzigen Geberden", "tokens": ["Mit", "trut\u00b7zi\u00b7gen", "Ge\u00b7ber\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Schaut er in's Land hinein.", "tokens": ["Schaut", "er", "in's", "Land", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Da schaut er, was wir treiben", "tokens": ["Da", "schaut", "er", ",", "was", "wir", "trei\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PRELS", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vom Rheine bis an's Meer,", "tokens": ["Vom", "Rhei\u00b7ne", "bis", "an's", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NE", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da liest er, was wir schreiben", "tokens": ["Da", "liest", "er", ",", "was", "wir", "schrei\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PRELS", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im weiten Land umher. \u2014", "tokens": ["Im", "wei\u00b7ten", "Land", "um\u00b7her", "."], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "ADJA", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Zu lang war dem Kyffh\u00e4user", "tokens": ["Zu", "lang", "war", "dem", "Kyff\u00b7h\u00e4u\u00b7ser"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKA", "ADJD", "VAFIN", "ART", "NN"], "meter": "-++--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Des Rothbarts Todesnacht,", "tokens": ["Des", "Roth\u00b7barts", "To\u00b7des\u00b7nacht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da ist f\u00fcr seinen Kaiser", "tokens": ["Da", "ist", "f\u00fcr", "sei\u00b7nen", "Kai\u00b7ser"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der gute Berg erwacht.", "tokens": ["Der", "gu\u00b7te", "Berg", "er\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Zu Schanden hei\u00dft er werden", "tokens": ["Zu", "Schan\u00b7den", "hei\u00dft", "er", "wer\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PPER", "VAINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Raben schwarzes Werk,", "tokens": ["Der", "Ra\u00b7ben", "schwar\u00b7zes", "Werk", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der beste Berg auf Erden,", "tokens": ["Der", "bes\u00b7te", "Berg", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das ist der Gutenberg.", "tokens": ["Das", "ist", "der", "Gu\u00b7ten\u00b7berg", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}