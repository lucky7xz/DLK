{"textgrid.poem.24686": {"metadata": {"author": {"name": "Hofmannsthal, Hugo von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sch\u00f6n ist mein Garten mit den goldnen B\u00e4umen,", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6n ist mein Garten mit den goldnen B\u00e4umen,", "tokens": ["Sch\u00f6n", "ist", "mein", "Gar\u00b7ten", "mit", "den", "gold\u00b7nen", "B\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPOSAT", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Den Bl\u00e4ttern, die mit Silbers\u00e4useln zittern,", "tokens": ["Den", "Bl\u00e4t\u00b7tern", ",", "die", "mit", "Sil\u00b7ber\u00b7s\u00e4u\u00b7seln", "zit\u00b7tern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dem Diamantentau, den Wappengittern,", "tokens": ["Dem", "Di\u00b7a\u00b7man\u00b7ten\u00b7tau", ",", "den", "Wap\u00b7pen\u00b7git\u00b7tern", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dem Klang des Gong, bei dem die L\u00f6wen tr\u00e4umen,", "tokens": ["Dem", "Klang", "des", "Gong", ",", "bei", "dem", "die", "L\u00f6\u00b7wen", "tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "APPR", "PRELS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die ehernen, und den Topasm\u00e4andern", "tokens": ["Die", "e\u00b7her\u00b7nen", ",", "und", "den", "To\u00b7pas\u00b7m\u00e4\u00b7an\u00b7dern"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "$,", "KON", "ART", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Und der Voli\u00e8re, wo die Reiher blinken,", "tokens": ["Und", "der", "Vo\u00b7li\u00b7\u00e8\u00b7re", ",", "wo", "die", "Rei\u00b7her", "blin\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PWAV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.7": {"text": "Die niemals aus dem Silberbrunnen trinken ...", "tokens": ["Die", "nie\u00b7mals", "aus", "dem", "Sil\u00b7ber\u00b7brun\u00b7nen", "trin\u00b7ken", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "So sch\u00f6n, ich sehn mich kaum nach jenem andern,", "tokens": ["So", "sch\u00f6n", ",", "ich", "sehn", "mich", "kaum", "nach", "je\u00b7nem", "an\u00b7dern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "PPER", "VVFIN", "PPER", "ADV", "APPR", "PDAT", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Dem andern Garten, wo ich fr\u00fcher war.", "tokens": ["Dem", "an\u00b7dern", "Gar\u00b7ten", ",", "wo", "ich", "fr\u00fc\u00b7her", "war", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PWAV", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Ich wei\u00df nicht wo ... Ich rieche nur den Tau,", "tokens": ["Ich", "wei\u00df", "nicht", "wo", "...", "Ich", "rie\u00b7che", "nur", "den", "Tau", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PWAV", "$(", "PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Den Tau, der fr\u00fch an meinen Haaren hing,", "tokens": ["Den", "Tau", ",", "der", "fr\u00fch", "an", "mei\u00b7nen", "Haa\u00b7ren", "hing", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Den Duft der Erde wei\u00df ich, feucht und lau,", "tokens": ["Den", "Duft", "der", "Er\u00b7de", "wei\u00df", "ich", ",", "feucht", "und", "lau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "PPER", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Wenn ich die weichen Beeren suchen ging ...", "tokens": ["Wenn", "ich", "die", "wei\u00b7chen", "Bee\u00b7ren", "su\u00b7chen", "ging", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "In jenem Garten, wo ich fr\u00fcher war ...", "tokens": ["In", "je\u00b7nem", "Gar\u00b7ten", ",", "wo", "ich", "fr\u00fc\u00b7her", "war", "..."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "PWAV", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}