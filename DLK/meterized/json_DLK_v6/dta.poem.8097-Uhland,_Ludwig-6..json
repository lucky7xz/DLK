{"dta.poem.8097": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Im Walde l\u00e4uft ein wildes Pferd,", "tokens": ["Im", "Wal\u00b7de", "l\u00e4uft", "ein", "wil\u00b7des", "Pferd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hat nie den Zaum gelitten,", "tokens": ["Hat", "nie", "den", "Zaum", "ge\u00b7lit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Goldfalb, mit langer, dichter M\u00e4hn\u2019,", "tokens": ["Gold\u00b7falb", ",", "mit", "lan\u00b7ger", ",", "dich\u00b7ter", "M\u00e4hn'", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schl\u00e4gt Funken bei allen Tritten.", "tokens": ["Schl\u00e4gt", "Fun\u00b7ken", "bei", "al\u00b7len", "Trit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Der K\u00f6nigssohn, er f\u00e4ngt es ein,", "tokens": ["Der", "K\u00f6\u00b7nigs\u00b7sohn", ",", "er", "f\u00e4ngt", "es", "ein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hat sich hinauf geschwungen,", "tokens": ["Hat", "sich", "hin\u00b7auf", "ge\u00b7schwun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es bl\u00e4ht die Brust und schwingt den Schweif,", "tokens": ["Es", "bl\u00e4ht", "die", "Brust", "und", "schwingt", "den", "Schweif", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "K\u00f6mmt wiehernd hergesprungen.", "tokens": ["K\u00f6mmt", "wie\u00b7hernd", "her\u00b7ge\u00b7sprun\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Und Alle horchen staunend auf,", "tokens": ["Und", "Al\u00b7le", "hor\u00b7chen", "stau\u00b7nend", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die in den Th\u00e4lern hausen.", "tokens": ["Die", "in", "den", "Th\u00e4\u00b7lern", "hau\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie h\u00f6ren\u2019s vom Gebirge her", "tokens": ["Sie", "h\u00f6\u00b7ren's", "vom", "Ge\u00b7bir\u00b7ge", "her"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "APZR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie Sturm und Donner brausen.", "tokens": ["Wie", "Sturm", "und", "Don\u00b7ner", "brau\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Da sprengt herab der K\u00f6nigssohn,", "tokens": ["Da", "sprengt", "her\u00b7ab", "der", "K\u00f6\u00b7nigs\u00b7sohn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Umwallt vom Fell des Leuen;", "tokens": ["Um\u00b7wallt", "vom", "Fell", "des", "Leu\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Des wilden Rosses M\u00e4hne fleugt,", "tokens": ["Des", "wil\u00b7den", "Ros\u00b7ses", "M\u00e4h\u00b7ne", "fleugt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Hufe Feuer streuen.", "tokens": ["Die", "Hu\u00b7fe", "Feu\u00b7er", "streu\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Da dr\u00e4ngt sich alles Volk herzu", "tokens": ["Da", "dr\u00e4ngt", "sich", "al\u00b7les", "Volk", "her\u00b7zu"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "PIAT", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Jubel und Gesange:", "tokens": ["Mit", "Ju\u00b7bel", "und", "Ge\u00b7san\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u201eheil uns! er ist\u2019s, der K\u00f6nig ist\u2019s,", "tokens": ["\u201e", "heil", "uns", "!", "er", "ist's", ",", "der", "K\u00f6\u00b7nig", "ist's", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "$.", "PPER", "VAFIN", "$,", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den wir erharrt so lange!\u201c", "tokens": ["Den", "wir", "er\u00b7harrt", "so", "lan\u00b7ge", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "PPER", "VVFIN", "ADV", "ADV", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}