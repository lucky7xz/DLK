{"textgrid.poem.37206": {"metadata": {"author": {"name": "Boie, Heinrich Christian", "birth": "N.A.", "death": "N.A."}, "title": "Immer Sie", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Warest du nicht die Armide,", "tokens": ["Wa\u00b7rest", "du", "nicht", "die", "Ar\u00b7mi\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Die den wackern Reinhold zwang?", "tokens": ["Die", "den", "wa\u00b7ckern", "Rein\u00b7hold", "zwang", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "T\u00f6nte dir nicht der Ovide,", "tokens": ["T\u00f6n\u00b7te", "dir", "nicht", "der", "O\u00b7vi\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Nicht der Gleime Liebessang?", "tokens": ["Nicht", "der", "Glei\u00b7me", "Lie\u00b7bes\u00b7sang", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Legtest du in Rosenbande", "tokens": ["Leg\u00b7test", "du", "in", "Ro\u00b7sen\u00b7ban\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nicht des Zephyrs Wankelmut?", "tokens": ["Nicht", "des", "Ze\u00b7phyrs", "Wan\u00b7kel\u00b7mut", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Stiegst du nicht an Cyprus Strande", "tokens": ["Stiegst", "du", "nicht", "an", "Cyp\u00b7rus", "Stran\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "APPR", "NE", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wonnenschauernd aus der Flut?", "tokens": ["Won\u00b7nen\u00b7schau\u00b7ernd", "aus", "der", "Flut", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ein auch dir bekannter Kleiner,", "tokens": ["Ein", "auch", "dir", "be\u00b7kann\u00b7ter", "Klei\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sch\u00f6n wie du und wohl gelaunt,", "tokens": ["Sch\u00f6n", "wie", "du", "und", "wohl", "ge\u00b7launt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PWAV", "PPER", "KON", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hat mir l\u00e4ngst ins Ohr geraunt:", "tokens": ["Hat", "mir", "l\u00e4ngst", "ins", "Ohr", "ge\u00b7raunt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wo gesprochen wird von Einer", "tokens": ["Wo", "ge\u00b7spro\u00b7chen", "wird", "von", "Ei\u00b7ner"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVPP", "VAFIN", "APPR", "PIS"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die, was treflich ist, vereint,", "tokens": ["Die", ",", "was", "tref\u00b7lich", "ist", ",", "ver\u00b7eint", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "$,", "PRELS", "ADJD", "VAFIN", "$,", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wird Helena stets gemeint.", "tokens": ["Wird", "He\u00b7le\u00b7na", "stets", "ge\u00b7meint", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "ADV", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}