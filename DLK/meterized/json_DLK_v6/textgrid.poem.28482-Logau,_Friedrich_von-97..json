{"textgrid.poem.28482": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "97.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Waffen sind verkn\u00fcpfft in eine Friedens-Pflicht;", "tokens": ["Die", "Waf\u00b7fen", "sind", "ver\u00b7kn\u00fcpfft", "in", "ei\u00b7ne", "Frie\u00b7dens\u00b7Pflicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Schulden aber noch, die Steuer nimmer nicht.", "tokens": ["Die", "Schul\u00b7den", "a\u00b7ber", "noch", ",", "die", "Steu\u00b7er", "nim\u00b7mer", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "$,", "ART", "NN", "ADV", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}