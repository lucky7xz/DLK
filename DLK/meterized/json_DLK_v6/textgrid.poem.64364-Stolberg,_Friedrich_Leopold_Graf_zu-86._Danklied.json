, "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So zeigt uns Gott auf Erden schon,", "tokens": ["So", "zeigt", "uns", "Gott", "auf", "Er\u00b7den", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "APPR", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie seine Sterne kreisen.", "tokens": ["Wie", "sei\u00b7ne", "Ster\u00b7ne", "krei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wir schaun die Wunder seiner Hand", "tokens": ["Wir", "schaun", "die", "Wun\u00b7der", "sei\u00b7ner", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Aus unsern tiefen Fernen,", "tokens": ["Aus", "un\u00b7sern", "tie\u00b7fen", "Fer\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und wissen, unser Vaterland", "tokens": ["Und", "wis\u00b7sen", ",", "un\u00b7ser", "Va\u00b7ter\u00b7land"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KON", "VVINF", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sei \u00fcber jenen Sternen.", "tokens": ["Sei", "\u00fc\u00b7ber", "je\u00b7nen", "Ster\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Auf unserm Leben schwimmt wie Schaum", "tokens": ["Auf", "un\u00b7serm", "Le\u00b7ben", "schwimmt", "wie", "Schaum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "KOKOM", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein wenig M\u00fch und Kummer;", "tokens": ["Ein", "we\u00b7nig", "M\u00fch", "und", "Kum\u00b7mer", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "KON", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Das Leben ist ein Morgentraum,", "tokens": ["Das", "Le\u00b7ben", "ist", "ein", "Mor\u00b7gen\u00b7traum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Tod ein kurzer Schlummer.", "tokens": ["Der", "Tod", "ein", "kur\u00b7zer", "Schlum\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wir sinken freudig in den Staub,", "tokens": ["Wir", "sin\u00b7ken", "freu\u00b7dig", "in", "den", "Staub", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der unsre V\u00e4ter decket,", "tokens": ["Der", "uns\u00b7re", "V\u00e4\u00b7ter", "de\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und g\u00f6nnen W\u00fcrmern ihren Raub,", "tokens": ["Und", "g\u00f6n\u00b7nen", "W\u00fcr\u00b7mern", "ih\u00b7ren", "Raub", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Weil Gott uns auferwecket.", "tokens": ["Weil", "Gott", "uns", "au\u00b7fer\u00b7we\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Es t\u00f6ne zu der Saite Klang,", "tokens": ["Es", "t\u00f6\u00b7ne", "zu", "der", "Sai\u00b7te", "Klang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So lange wir hier wallen,", "tokens": ["So", "lan\u00b7ge", "wir", "hier", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sein Lobgesang; und Lobgesang", "tokens": ["Sein", "Lob\u00b7ge\u00b7sang", ";", "und", "Lob\u00b7ge\u00b7sang"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Soll schon das Kindlein lallen!", "tokens": ["Soll", "schon", "das", "Kin\u00b7dlein", "lal\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und wenn's nach Seinem Namen fragt,", "tokens": ["Und", "wenn's", "nach", "Sei\u00b7nem", "Na\u00b7men", "fragt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So dr\u00fcckt mit beiden Armen", "tokens": ["So", "dr\u00fcckt", "mit", "bei\u00b7den", "Ar\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.