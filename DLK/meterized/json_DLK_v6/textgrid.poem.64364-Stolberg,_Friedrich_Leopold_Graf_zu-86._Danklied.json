{"textgrid.poem.64364": {"metadata": {"author": {"name": "Stolberg, Friedrich Leopold Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "86. Danklied", "genre": "verse", "period": "N.A.", "pub_year": 1786, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df unser Gott uns Leben gab,", "tokens": ["Da\u00df", "un\u00b7ser", "Gott", "uns", "Le\u00b7ben", "gab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des wollen wir uns freuen,", "tokens": ["Des", "wol\u00b7len", "wir", "uns", "freu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und von der Wiege bis ans Grab", "tokens": ["Und", "von", "der", "Wie\u00b7ge", "bis", "ans", "Grab"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "APPR", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihm unsern Dank erneuen:", "tokens": ["Ihm", "un\u00b7sern", "Dank", "er\u00b7neu\u00b7en", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Denn auch zur Freude gab uns Gott", "tokens": ["Denn", "auch", "zur", "Freu\u00b7de", "gab", "uns", "Gott"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "NN", "VVFIN", "PPER", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Auf dieser Welt das Leben,", "tokens": ["Auf", "die\u00b7ser", "Welt", "das", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und hat verhei\u00dfen, nach dem Tod", "tokens": ["Und", "hat", "ver\u00b7hei\u00b7\u00dfen", ",", "nach", "dem", "Tod"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VAFIN", "VVPP", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der Wonne mehr zu geben.", "tokens": ["Der", "Won\u00b7ne", "mehr", "zu", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wie fromme Kinder k\u00f6nnen wir", "tokens": ["Wie", "from\u00b7me", "Kin\u00b7der", "k\u00f6n\u00b7nen", "wir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "VMFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In froher Einfalt leben;", "tokens": ["In", "fro\u00b7her", "Ein\u00b7falt", "le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Drum hat der Vater schon allhier", "tokens": ["Drum", "hat", "der", "Va\u00b7ter", "schon", "all\u00b7hier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "ART", "NN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Eden uns gegeben.", "tokens": ["Ein", "E\u00b7den", "uns", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Fr\u00fchlingsw\u00e4rme haucht sein Mund,", "tokens": ["Die", "Fr\u00fch\u00b7lings\u00b7w\u00e4r\u00b7me", "haucht", "sein", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und K\u00fchlung wehn die Wogen;", "tokens": ["Und", "K\u00fch\u00b7lung", "wehn", "die", "Wo\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Am Himmel zeugt von seinem Bund", "tokens": ["Am", "Him\u00b7mel", "zeugt", "von", "sei\u00b7nem", "Bund"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der sch\u00f6ne Regenbogen.", "tokens": ["Der", "sch\u00f6\u00b7ne", "Re\u00b7gen\u00b7bo\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und Auen, Felder, Berg und Wald", "tokens": ["Und", "Au\u00b7en", ",", "Fel\u00b7der", ",", "Berg", "und", "Wald"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KON", "NN", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verk\u00fcnden seine Gnade,", "tokens": ["Ver\u00b7k\u00fcn\u00b7den", "sei\u00b7ne", "Gna\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und seines Namens Gr\u00f6\u00dfe schallt", "tokens": ["Und", "sei\u00b7nes", "Na\u00b7mens", "Gr\u00f6\u00b7\u00dfe", "schallt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Am hallenden Gestade.", "tokens": ["Am", "hal\u00b7len\u00b7den", "Ge\u00b7sta\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ihn singt die kleine Nachtigall.", "tokens": ["Ihn", "singt", "die", "klei\u00b7ne", "Nach\u00b7ti\u00b7gall", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "O la\u00dft mit ihr uns singen!", "tokens": ["O", "la\u00dft", "mit", "ihr", "uns", "sin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "La\u00dft mit der frohen Lerche Schall", "tokens": ["La\u00dft", "mit", "der", "fro\u00b7hen", "Ler\u00b7che", "Schall"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "APPR", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Auch unser Lied erklingen!", "tokens": ["Auch", "un\u00b7ser", "Lied", "er\u00b7klin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die Felder waren hart und wei\u00df,", "tokens": ["Die", "Fel\u00b7der", "wa\u00b7ren", "hart", "und", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Erde Scho\u00df verschlossen.", "tokens": ["Der", "Er\u00b7de", "Scho\u00df", "ver\u00b7schlos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gott sah herab; es schmolz das Eis;", "tokens": ["Gott", "sah", "her\u00b7ab", ";", "es", "schmolz", "das", "Eis", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "$.", "PPER", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Seht, unsre \u00c4hren sprossen.", "tokens": ["Seht", ",", "uns\u00b7re", "\u00c4h\u00b7ren", "spros\u00b7sen", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Vom Bienenstocke trieft der Seim,", "tokens": ["Vom", "Bie\u00b7nen\u00b7sto\u00b7cke", "trieft", "der", "Seim", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das Lamm h\u00fcpft auf der Weide,", "tokens": ["Das", "Lamm", "h\u00fcpft", "auf", "der", "Wei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und an der Rebe schwillt im Keim", "tokens": ["Und", "an", "der", "Re\u00b7be", "schwillt", "im", "Keim"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Des guten Bechers Freude.", "tokens": ["Des", "gu\u00b7ten", "Be\u00b7chers", "Freu\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Von heitrer Stirne flie\u00dft der Schwei\u00df", "tokens": ["Von", "hei\u00b7trer", "Stir\u00b7ne", "flie\u00dft", "der", "Schwei\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf unser Feld und Garten,", "tokens": ["Auf", "un\u00b7ser", "Feld", "und", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wenn wir mit unverdro\u00dfnem Flei\u00df", "tokens": ["Wenn", "wir", "mit", "un\u00b7ver\u00b7dro\u00df\u00b7nem", "Flei\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Jahres F\u00fcll' erwarten,", "tokens": ["Des", "Jah\u00b7res", "F\u00fcll'", "er\u00b7war\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Nicht \u00e4ngstlich unsern Samen streun,", "tokens": ["Nicht", "\u00e4ngst\u00b7lich", "un\u00b7sern", "Sa\u00b7men", "streun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sein Korn dem Vogel g\u00f6nnen,", "tokens": ["Sein", "Korn", "dem", "Vo\u00b7gel", "g\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Uns auch des Nachbars Ernte freun,", "tokens": ["Uns", "auch", "des", "Nach\u00b7bars", "Ern\u00b7te", "freun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und wohlthun, wo wir k\u00f6nnen.", "tokens": ["Und", "wohl\u00b7thun", ",", "wo", "wir", "k\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "PPER", "VMFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Aus freier Gnade hie\u00df der Herr", "tokens": ["Aus", "frei\u00b7er", "Gna\u00b7de", "hie\u00df", "der", "Herr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So sch\u00f6n die Erde werden.", "tokens": ["So", "sch\u00f6n", "die", "Er\u00b7de", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bedarf zu seinem Wohlsein Er", "tokens": ["Be\u00b7darf", "zu", "sei\u00b7nem", "Wohl\u00b7sein", "Er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "NN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Fr\u00fcchte dieser Erden?", "tokens": ["Der", "Fr\u00fcch\u00b7te", "die\u00b7ser", "Er\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Drum wollen wir auch geben gern,", "tokens": ["Drum", "wol\u00b7len", "wir", "auch", "ge\u00b7ben", "gern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "ADV", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie wir von Ihm genommen,", "tokens": ["Wie", "wir", "von", "Ihm", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und \u00e4hnlich werden unserm Herrn,", "tokens": ["Und", "\u00e4hn\u00b7lich", "wer\u00b7den", "un\u00b7serm", "Herrn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}}, "stanza.7": {"line.1": {"text": "Wer k\u00e4rglich sich der Fr\u00fccht' allein,", "tokens": ["Wer", "k\u00e4rg\u00b7lich", "sich", "der", "Fr\u00fccht'", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "PRF", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht auch der Blumen freuet,", "tokens": ["Nicht", "auch", "der", "Blu\u00b7men", "freu\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Vergi\u00dft, da\u00df Gottes Sonnenschein", "tokens": ["Ver\u00b7gi\u00dft", ",", "da\u00df", "Got\u00b7tes", "Son\u00b7nen\u00b7schein"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "KOUS", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Blumen auch erneuet.", "tokens": ["Die", "Blu\u00b7men", "auch", "er\u00b7neu\u00b7et", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die blaue Blum' im Erntekranz", "tokens": ["Die", "blau\u00b7e", "Blum'", "im", "Ern\u00b7te\u00b7kranz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Hat Gottes Hand ges\u00e4et;", "tokens": ["Hat", "Got\u00b7tes", "Hand", "ge\u00b7s\u00e4et", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und Ihm gef\u00e4llt des Schnitters Tanz,", "tokens": ["Und", "Ihm", "ge\u00b7f\u00e4llt", "des", "Schnit\u00b7ters", "Tanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wenn freudig er gem\u00e4het.", "tokens": ["Wenn", "freu\u00b7dig", "er", "ge\u00b7m\u00e4\u00b7het", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Es ward die Freundschaft uns vom Herrn", "tokens": ["Es", "ward", "die", "Freund\u00b7schaft", "uns", "vom", "Herrn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ins warme Herz gegeben:", "tokens": ["Ins", "war\u00b7me", "Herz", "ge\u00b7ge\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der wahre Freund vergi\u00dft sich gern,", "tokens": ["Der", "wah\u00b7re", "Freund", "ver\u00b7gi\u00dft", "sich", "gern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Um seinem Freund zu leben.", "tokens": ["Um", "sei\u00b7nem", "Freund", "zu", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Gott segnet keuscher Ehe Zucht", "tokens": ["Gott", "seg\u00b7net", "keu\u00b7scher", "E\u00b7he", "Zucht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit wahrer Liebe S\u00fc\u00dfe:", "tokens": ["Mit", "wah\u00b7rer", "Lie\u00b7be", "S\u00fc\u00b7\u00dfe", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Die Mutter liebt des Schmerzens Frucht,", "tokens": ["Die", "Mut\u00b7ter", "liebt", "des", "Schmer\u00b7zens", "Frucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Belohnt durch seine K\u00fcsse.", "tokens": ["Be\u00b7lohnt", "durch", "sei\u00b7ne", "K\u00fcs\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Mit Wohlgefallen sieht der Herr", "tokens": ["Mit", "Wohl\u00b7ge\u00b7fal\u00b7len", "sieht", "der", "Herr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie Blumen, Kinder bl\u00fchen;", "tokens": ["Wie", "Blu\u00b7men", ",", "Kin\u00b7der", "bl\u00fc\u00b7hen", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mit Wohlgefallen sieht auch Er", "tokens": ["Mit", "Wohl\u00b7ge\u00b7fal\u00b7len", "sieht", "auch", "Er"], "token_info": ["word", "word", "word", "word", 