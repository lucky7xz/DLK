{"textgrid.poem.46818": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[ein Thr\u00e4nchen ist mir geflossen]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Thr\u00e4nchen ist mir geflossen", "tokens": ["Ein", "Thr\u00e4n\u00b7chen", "ist", "mir", "ge\u00b7flos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wohl bei manchem Gedicht,", "tokens": ["Wohl", "bei", "man\u00b7chem", "Ge\u00b7dicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIAT", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Doch hier ist eins vergossen,", "tokens": ["Doch", "hier", "ist", "eins", "ver\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Geweint hab' ich das nicht.", "tokens": ["Ge\u00b7weint", "hab'", "ich", "das", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "PDS", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Geweint ist es nicht wahrer,", "tokens": ["Ge\u00b7weint", "ist", "es", "nicht", "wah\u00b7rer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "PTKNEG", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Es ist nicht bittrer geweint,", "tokens": ["Es", "ist", "nicht", "bit\u00b7trer", "ge\u00b7weint", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "VVPP", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Aber geweint ist's klarer,", "tokens": ["A\u00b7ber", "ge\u00b7weint", "ist's", "kla\u00b7rer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "VAFIN", "ADJD", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Und voller, wie mir scheint.", "tokens": ["Und", "vol\u00b7ler", ",", "wie", "mir", "scheint", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die Mutter hat's geweinet,", "tokens": ["Die", "Mut\u00b7ter", "hat's", "ge\u00b7wei\u00b7net", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die heimlich las das Gedicht,", "tokens": ["Die", "heim\u00b7lich", "las", "das", "Ge\u00b7dicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Hat's nicht gemerkt, und meinet,", "tokens": ["Hat's", "nicht", "ge\u00b7merkt", ",", "und", "mei\u00b7net", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "VVPP", "$,", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich merk' es auch wohl nicht.", "tokens": ["Ich", "merk'", "es", "auch", "wohl", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ich hab' es wohl bemerket,", "tokens": ["Ich", "hab'", "es", "wohl", "be\u00b7mer\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Doch st\u00f6rt mich nicht die Spur,", "tokens": ["Doch", "st\u00f6rt", "mich", "nicht", "die", "Spur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Beifallszeichen st\u00e4rket", "tokens": ["Das", "Bei\u00b7falls\u00b7zei\u00b7chen", "st\u00e4r\u00b7ket"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mich zum Fortfahren nur.", "tokens": ["Mich", "zum", "Fort\u00b7fah\u00b7ren", "nur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "ADV", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}