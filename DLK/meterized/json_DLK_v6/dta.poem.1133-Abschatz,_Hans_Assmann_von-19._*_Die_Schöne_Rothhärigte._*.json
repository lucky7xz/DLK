{"dta.poem.1133": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "19. * Die Sch\u00f6ne Rothh\u00e4rigte. *", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Wiltu der Haare Brand mit Ziper-Asche decken? ", "tokens": ["Wil\u00b7tu", "der", "Haa\u00b7re", "Brand", "mit", "Zi\u00b7per\u00b7A\u00b7sche", "de\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "NN", "APPR", "NN", "VVINF", "$."], "meter": "+--+-+--+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Rubelle/ thu es nicht: Der Sch\u00f6nheit h\u00f6chsten Prei\u00df/", "tokens": ["Ru\u00b7bel\u00b7le", "/", "thu", "es", "nicht", ":", "Der", "Sch\u00f6n\u00b7heit", "h\u00f6chs\u00b7ten", "Prei\u00df", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "VVFIN", "PPER", "PTKNEG", "$.", "ART", "NN", "ADJA", "NN", "$("], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Nach Urtheil kluger Welt/ behalten roth und wei\u00df.", "tokens": ["Nach", "Ur\u00b7theil", "klu\u00b7ger", "Welt", "/", "be\u00b7hal\u00b7ten", "roth", "und", "wei\u00df", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "$(", "VVFIN", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Morgen-Welt schm\u00fcckt Pferd und sich mit rothen Fle-", "tokens": ["Die", "Mor\u00b7gen\u00b7Welt", "schm\u00fcckt", "Pferd", "und", "sich", "mit", "ro\u00b7then", "Fle"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "NN", "KON", "PRF", "APPR", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "cken. ", "tokens": ["cken", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "-", "measure": "single.down"}, "line.6": {"text": "Was in den Adern kocht/ mu\u00df Etnens Haubt entstecken.", "tokens": ["Was", "in", "den", "A\u00b7dern", "kocht", "/", "mu\u00df", "Et\u00b7nens", "Haubt", "ent\u00b7ste\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "VVFIN", "$(", "VMFIN", "ADV", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der Scheitel heisse Glutt bef\u00e4rbt der Wangen Ei\u00df/", "tokens": ["Der", "Schei\u00b7tel", "heis\u00b7se", "Glutt", "be\u00b7f\u00e4rbt", "der", "Wan\u00b7gen", "Ei\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der Berenizen Haar gl\u00e4nzt so am Himmels-Krei\u00df;", "tokens": ["Der", "Be\u00b7re\u00b7ni\u00b7zen", "Haar", "gl\u00e4nzt", "so", "am", "Him\u00b7mels\u00b7Krei\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Mit Stralen solcher Art pflegt Ph\u00f6bus uns zu wecken.", "tokens": ["Mit", "Stra\u00b7len", "sol\u00b7cher", "Art", "pflegt", "Ph\u00f6\u00b7bus", "uns", "zu", "we\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PIAT", "NN", "VVFIN", "NE", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Das weisse Schindel-Holtz deckt manch ver\u00e4chtlich Hau\u00df;", "tokens": ["Das", "weis\u00b7se", "Schin\u00b7del\u00b7Holtz", "deckt", "manch", "ver\u00b7\u00e4cht\u00b7lich", "Hau\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PIAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Man flicht mit gelbem Stroh die Bauer-H\u00fctten aus/", "tokens": ["Man", "flicht", "mit", "gel\u00b7bem", "Stroh", "die", "Bau\u00b7e\u00b7rH\u00fct\u00b7ten", "aus", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "ADJA", "NN", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Damit das Ziegel-Dach der Schl\u00f6sser Zierde sey.", "tokens": ["Da\u00b7mit", "das", "Zie\u00b7gel\u00b7Dach", "der", "Schl\u00f6s\u00b7ser", "Zier\u00b7de", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "ART", "NN", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "K\u00f6mmt Silber oder Bley der Pracht des Goldes gleich?", "tokens": ["K\u00f6mmt", "Sil\u00b7ber", "o\u00b7der", "Bley", "der", "Pracht", "des", "Gol\u00b7des", "gleich", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "ART", "NN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Carfunckel und Scarlat macht Stein und Farben bleich:", "tokens": ["Car\u00b7fun\u00b7ckel", "und", "Scar\u00b7lat", "macht", "Stein", "und", "Far\u00b7ben", "bleich", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NE", "VVFIN", "NN", "KON", "NN", "ADJD", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.15": {"text": "Der rothe Safran ist die theurste Specerey.", "tokens": ["Der", "ro\u00b7the", "Saf\u00b7ran", "ist", "die", "theurs\u00b7te", "Spe\u00b7ce\u00b7rey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}