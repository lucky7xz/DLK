{"dta.poem.11269": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Das  Magister-M.  \n C. H.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die wei\u00dfheit bringt uns itzt mit ihren ehren-gaben", "tokens": ["Die", "wei\u00df\u00b7heit", "bringt", "uns", "itzt", "mit", "ih\u00b7ren", "eh\u00b7ren\u00b7ga\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wohlfeile zeit ins land: Denn was sonst tausend galt,", "tokens": ["Wohl\u00b7fei\u00b7le", "zeit", "ins", "land", ":", "Denn", "was", "sonst", "tau\u00b7send", "galt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPRART", "NN", "$.", "KON", "PWS", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das kan itzt jedermann, gelehrt, reich jung und alt,", "tokens": ["Das", "kan", "itzt", "je\u00b7der\u00b7mann", ",", "ge\u00b7lehrt", ",", "reich", "jung", "und", "alt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "PIS", "$,", "VVPP", "$,", "ADJD", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vor funffzehn, ja wohl gar vor zehn, zw\u00f6lff thaler haben.", "tokens": ["Vor", "funff\u00b7zehn", ",", "ja", "wohl", "gar", "vor", "zehn", ",", "zw\u00f6lff", "tha\u00b7ler", "ha\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word",