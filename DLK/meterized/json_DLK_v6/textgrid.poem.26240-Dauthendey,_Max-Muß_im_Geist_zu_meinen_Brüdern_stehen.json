{"textgrid.poem.26240": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Mu\u00df im Geist zu meinen Br\u00fcdern stehen", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Immer gurren eingesperrte Tauben", "tokens": ["Im\u00b7mer", "gur\u00b7ren", "ein\u00b7ge\u00b7sperr\u00b7te", "Tau\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Dr\u00fcben in den H\u00fctten der Javanen,", "tokens": ["Dr\u00fc\u00b7ben", "in", "den", "H\u00fct\u00b7ten", "der", "Ja\u00b7va\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Aus den K\u00e4figen an den Altanen.", "tokens": ["Aus", "den", "K\u00e4\u00b7fi\u00b7gen", "an", "den", "Al\u00b7ta\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}}, "stanza.2": {"line.1": {"text": "Immer seh' ich \u00fcber gr\u00fcnen Lauben", "tokens": ["Im\u00b7mer", "seh'", "ich", "\u00fc\u00b7ber", "gr\u00fc\u00b7nen", "Lau\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Der Javanenkinder Drachen fliegen,", "tokens": ["Der", "Ja\u00b7va\u00b7nen\u00b7kin\u00b7der", "Dra\u00b7chen", "flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Die sich wie papierne V\u00f6gel wiegen.", "tokens": ["Die", "sich", "wie", "pa\u00b7pier\u00b7ne", "V\u00f6\u00b7gel", "wie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "KOKOM", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Immer rauscht's im Reisfeld von den B\u00e4chen,", "tokens": ["Im\u00b7mer", "rauscht's", "im", "Reis\u00b7feld", "von", "den", "B\u00e4\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Die da schl\u00e4frig vor der T\u00fcre sprechen,", "tokens": ["Die", "da", "schl\u00e4f\u00b7rig", "vor", "der", "T\u00fc\u00b7re", "spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und ich m\u00f6chte nur an Frieden glauben.", "tokens": ["Und", "ich", "m\u00f6ch\u00b7te", "nur", "an", "Frie\u00b7den", "glau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Immer mu\u00df ich mir den Frieden rauben,", "tokens": ["Im\u00b7mer", "mu\u00df", "ich", "mir", "den", "Frie\u00b7den", "rau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Mu\u00df im Geist zu meinen Br\u00fcdern stehen,", "tokens": ["Mu\u00df", "im", "Geist", "zu", "mei\u00b7nen", "Br\u00fc\u00b7dern", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPRART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Die mit Bajonett und Kugel m\u00e4hen.", "tokens": ["Die", "mit", "Ba\u00b7jo\u00b7nett", "und", "Ku\u00b7gel", "m\u00e4\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}