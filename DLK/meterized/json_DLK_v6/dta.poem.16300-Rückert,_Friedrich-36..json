{"dta.poem.16300": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "36.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Oft mahnt ein j\u00e4her Sto\u00df den sorgenlosen Gleiter", "tokens": ["Oft", "mahnt", "ein", "j\u00e4\u00b7her", "Sto\u00df", "den", "sor\u00b7gen\u00b7lo\u00b7sen", "Glei\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auf glatter Lebensflut an Tr\u00fcmmerung und Scheiter.", "tokens": ["Auf", "glat\u00b7ter", "Le\u00b7bens\u00b7flut", "an", "Tr\u00fcm\u00b7me\u00b7rung", "und", "Schei\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Du dank' ihm, da\u00df er aus Gedankenlosigkeit", "tokens": ["Du", "dank'", "ihm", ",", "da\u00df", "er", "aus", "Ge\u00b7dan\u00b7ken\u00b7lo\u00b7sig\u00b7keit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dich weckt, zu danken Gott f\u00fcr gn\u00e4diges Geleit;", "tokens": ["Dich", "weckt", ",", "zu", "dan\u00b7ken", "Gott", "f\u00fcr", "gn\u00e4\u00b7di\u00b7ges", "Ge\u00b7leit", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PTKZU", "VVINF", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "F\u00fcr gn\u00e4diges Geleit zu danken und zu flehn,", "tokens": ["F\u00fcr", "gn\u00e4\u00b7di\u00b7ges", "Ge\u00b7leit", "zu", "dan\u00b7ken", "und", "zu", "flehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df weiter sanft gewiegt dein Schifflein m\u00f6ge gehn.", "tokens": ["Da\u00df", "wei\u00b7ter", "sanft", "ge\u00b7wiegt", "dein", "Schif\u00b7flein", "m\u00f6\u00b7ge", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADJD", "VVPP", "PPOSAT", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}