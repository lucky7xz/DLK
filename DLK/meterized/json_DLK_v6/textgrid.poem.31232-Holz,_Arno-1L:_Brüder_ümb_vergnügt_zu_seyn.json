{"textgrid.poem.31232": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "1L: Br\u00fcder/ \u00fcmb vergn\u00fcgt zu seyn/", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Br\u00fcder/ \u00fcmb vergn\u00fcgt zu seyn/", "tokens": ["Br\u00fc\u00b7der", "/", "\u00fcmb", "ver\u00b7gn\u00fcgt", "zu", "seyn", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "ADV", "VVPP", "PTKZU", "VAINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "br\u00e4ucht es keiner Sch\u00e4zze!", "tokens": ["br\u00e4ucht", "es", "kei\u00b7ner", "Sch\u00e4z\u00b7ze", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Dihser junge Bircken-Hayn/", "tokens": ["Dih\u00b7ser", "jun\u00b7ge", "Bir\u00b7cken\u00b7Hayn", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "durch den froh beym Mayen-Schein", "tokens": ["durch", "den", "froh", "beym", "Mayen\u00b7Schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJD", "APPRART", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "wihder\u00fcmb die Amsteln schreyn/", "tokens": ["wih\u00b7de\u00b7r\u00fcmb", "die", "A\u00b7msteln", "schreyn", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "beut uns dausend Bl\u00e4zze.", "tokens": ["beut", "uns", "dau\u00b7send", "Bl\u00e4z\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Jeder ist von Bluhmen bundt:", "tokens": ["Je\u00b7der", "ist", "von", "Bluh\u00b7men", "bundt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "APPR", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Veilgen und Turkosen/", "tokens": ["Veil\u00b7gen", "und", "Tur\u00b7ko\u00b7sen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "$("], "meter": "+-+++-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Jungffern-Schuch und Frauen-Mund", "tokens": ["Jungffern\u00b7Schuch", "und", "Frau\u00b7en\u00b7Mund"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "dhut der Lentz uns leuchtend kund/", "tokens": ["dhut", "der", "Lentz", "uns", "leuch\u00b7tend", "kund", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "ADJD", "PTKVZ", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "g\u00f6ldner gl\u00e4ntzt al\u00df dihser Grund/", "tokens": ["g\u00f6ld\u00b7ner", "gl\u00e4ntzt", "al\u00df", "dih\u00b7ser", "Grund", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ADV", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "nicht das Gl\u00fckks-Land ", "tokens": ["nicht", "das", "Gl\u00fc\u00b7kks\u00b7Land"], "token_info": ["word", "word", "word"], "pos": ["PTKNEG", "ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Hihr \u00fcmb dihses B\u00e4chleins Rand", "tokens": ["Hihr", "\u00fcmb", "dih\u00b7ses", "B\u00e4ch\u00b7leins", "Rand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PDAT", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "werfft euch in den Wasen!", "tokens": ["werfft", "euch", "in", "den", "Wa\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "B\u00e4ssres ward noch nie bekandt \u2013", "tokens": ["B\u00e4ss\u00b7res", "ward", "noch", "nie", "be\u00b7kandt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "ADV", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "d\u00e4kkt uns einmahl erst der Sand/", "tokens": ["d\u00e4kkt", "uns", "ein\u00b7mahl", "erst", "der", "Sand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ART", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "keiner h\u00f6rt dan mehr durchs Land", "tokens": ["kei\u00b7ner", "h\u00f6rt", "dan", "mehr", "durchs", "Land"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ADV", "ADV", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "wa\u00df die Sch\u00e4ffer blasen!", "tokens": ["wa\u00df", "die", "Sch\u00e4f\u00b7fer", "bla\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Traumt und kukkt den W\u00f6lckgens zu/", "tokens": ["Traumt", "und", "kukkt", "den", "W\u00f6lck\u00b7gens", "zu", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "ART", "NN", "PTKZU", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "wie sie selig gleissen!", "tokens": ["wie", "sie", "se\u00b7lig", "gleis\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Unterde\u00df lokkt gukrukru", "tokens": ["Un\u00b7ter\u00b7de\u00df", "lokkt", "gu\u00b7kru\u00b7kru"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "hihr der D\u00f6bber sine Fru/", "tokens": ["hihr", "der", "D\u00f6b\u00b7ber", "si\u00b7ne", "Fru", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "\u00fcmb ihr schon im nechsten Nu", "tokens": ["\u00fcmb", "ihr", "schon", "im", "nechs\u00b7ten", "Nu"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ADV", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "in den Kamm zu beissen!", "tokens": ["in", "den", "Kamm", "zu", "beis\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Jedes Kummers durchaus bloh\u00df/", "tokens": ["Je\u00b7des", "Kum\u00b7mers", "durc\u00b7haus", "bloh\u00df", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "ADV", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "freut euch dihser Stunde!", "tokens": ["freut", "euch", "dih\u00b7ser", "Stun\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Kaum schon fand wer so famos/", "tokens": ["Kaum", "schon", "fand", "wer", "so", "fa\u00b7mos", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PWS", "ADV", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "s\u00e4lbst die Reden ", "tokens": ["s\u00e4lbst", "die", "Re\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "alle Wei\u00dfheit ", "tokens": ["al\u00b7le", "Wei\u00df\u00b7heit"], "token_info": ["word", "word"], "pos": ["PIAT", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "lacht au\u00df ihrem Munde!", "tokens": ["lacht", "au\u00df", "ih\u00b7rem", "Mun\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}