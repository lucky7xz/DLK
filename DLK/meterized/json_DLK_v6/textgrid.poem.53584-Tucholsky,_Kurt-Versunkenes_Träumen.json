{"textgrid.poem.53584": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Versunkenes Tr\u00e4umen", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lieblich ruht der Busen, auf dem Tisch,", "tokens": ["Lieb\u00b7lich", "ruht", "der", "Bu\u00b7sen", ",", "auf", "dem", "Tisch", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "jener Jungfrau, welche rosig ist und frisch.", "tokens": ["je\u00b7ner", "Jung\u00b7frau", ",", "wel\u00b7che", "ro\u00b7sig", "ist", "und", "frisch", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PRELS", "ADJD", "VAFIN", "KON", "ADJD", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.2": {"line.1": {"text": "Ach, er ist so kugelig und gerundet,", "tokens": ["Ach", ",", "er", "ist", "so", "ku\u00b7ge\u00b7lig", "und", "ge\u00b7run\u00b7det", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VAFIN", "ADV", "ADJD", "KON", "VVPP", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "da\u00df er mir schon in Gedanken mundet.", "tokens": ["da\u00df", "er", "mir", "schon", "in", "Ge\u00b7dan\u00b7ken", "mun\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Heil und Sieg dereinst dem feinen Knaben,", "tokens": ["Heil", "und", "Sieg", "de\u00b7reinst", "dem", "fei\u00b7nen", "Kna\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "dem es freisteht, sich daran zu laben.", "tokens": ["dem", "es", "freis\u00b7teht", ",", "sich", "da\u00b7ran", "zu", "la\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "PRF", "PAV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Jener wird erst st\u00f6hnen und sich recken;", "tokens": ["Je\u00b7ner", "wird", "erst", "st\u00f6h\u00b7nen", "und", "sich", "re\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "VAFIN", "ADV", "VVINF", "KON", "PRF", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "aber nachher bleibt er sicher stecken.", "tokens": ["a\u00b7ber", "nach\u00b7her", "bleibt", "er", "si\u00b7cher", "ste\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PRF", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Heirat, Kinder und ein h\u00e4usliches Frangss\u00e4h \u2013", "tokens": ["Hei\u00b7rat", ",", "Kin\u00b7der", "und", "ein", "h\u00e4us\u00b7li\u00b7ches", "Frangs\u00b7s\u00e4h", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "KON", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "nichts von Liebesnacht und jenem Kanapee . . .", "tokens": ["nichts", "von", "Lie\u00b7bes\u00b7nacht", "und", "je\u00b7nem", "Ka\u00b7na\u00b7pee", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PIS", "APPR", "NN", "KON", "PDAT", "NN", "$.", "$.", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.6": {"line.1": {"text": "Ich hingegen sitz bei ihren Br\u00fcsten,", "tokens": ["Ich", "hin\u00b7ge\u00b7gen", "sitz", "bei", "ih\u00b7ren", "Br\u00fcs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "und \u2013 gedanklich \u2013 dient sie meinen L\u00fcsten.", "tokens": ["und", "\u2013", "ge\u00b7dank\u00b7lich", "\u2013", "dient", "sie", "mei\u00b7nen", "L\u00fcs\u00b7ten", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "ADJD", "$(", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Doch dann steh ich auf und schlenkre froh mein Bein,", "tokens": ["Doch", "dann", "steh", "ich", "auf", "und", "schlen\u00b7kre", "froh", "mein", "Bein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "ADJD", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "schiebe ab,", "tokens": ["schie\u00b7be", "ab", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "bin frei \u2013", "tokens": ["bin", "frei", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$("], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "und lasse Jungfer Jungfer sein! \u2013", "tokens": ["und", "las\u00b7se", "Jung\u00b7fer", "Jung\u00b7fer", "sein", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "NE", "NE", "VAINF", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}