{"textgrid.poem.31500": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: GoTT schl\u00e4fft / und schl\u00e4fft doch nicht. Er schl\u00e4ffet / zu entdecken", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "GoTT schl\u00e4fft / und schl\u00e4fft doch nicht. Er schl\u00e4ffet / zu entdecken", "tokens": ["GoTT", "schl\u00e4fft", "/", "und", "schl\u00e4fft", "doch", "nicht", ".", "Er", "schl\u00e4f\u00b7fet", "/", "zu", "ent\u00b7de\u00b7cken"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NE", "VVFIN", "$(", "KON", "VVFIN", "ADV", "PTKNEG", "$.", "PPER", "VVFIN", "$(", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "der J\u00fcnger Glaubens- Schw\u00e4ch' im Wetter-Widerstand /", "tokens": ["der", "J\u00fcn\u00b7ger", "Glau\u00b7bens", "Schw\u00e4ch'", "im", "Wet\u00b7ter\u00b7Wi\u00b7der\u00b7stand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "TRUNC", "NN", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "wann er von ihnen zieht die St\u00fcrme-Schirmungs-Hand;", "tokens": ["wann", "er", "von", "ih\u00b7nen", "zieht", "die", "St\u00fcr\u00b7me\u00b7Schir\u00b7mungs\u00b7Hand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "will / nach dem schein / den Schein des Gnaden-Augs verstecken.", "tokens": ["will", "/", "nach", "dem", "schein", "/", "den", "Schein", "des", "Gna\u00b7den\u00b7Augs", "ver\u00b7ste\u00b7cken", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "$(", "APPR", "ART", "NN", "$(", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "---+-+-+-+-+-", "measure": "unknown.measure.penta"}, "line.5": {"text": "der strengen Noht Gebot / macht ihn behend erwecken.", "tokens": ["der", "stren\u00b7gen", "Noht", "Ge\u00b7bot", "/", "macht", "ihn", "be\u00b7hend", "er\u00b7we\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$(", "VVFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Er / der die Wind verbindt / hat an der Hand das Band /", "tokens": ["Er", "/", "der", "die", "Wind", "ver\u00b7bindt", "/", "hat", "an", "der", "Hand", "das", "Band", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "ART", "ART", "NN", "VVPP", "$(", "VAFIN", "APPR", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "kan wider ruffen bald / die so Er ausgesandt:", "tokens": ["kan", "wi\u00b7der", "ruf\u00b7fen", "bald", "/", "die", "so", "Er", "aus\u00b7ge\u00b7sandt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "VVFIN", "ADV", "$(", "ART", "ADV", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "sie stellen sich stracks ein aus allen Felsen-Ecken.", "tokens": ["sie", "stel\u00b7len", "sich", "stracks", "ein", "aus", "al\u00b7len", "Fel\u00b7sen\u00b7E\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "ART", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-++-", "measure": "unknown.measure.hexa"}, "line.9": {"text": "Die Welle / die sich mit der hohen Wolken wolt", "tokens": ["Die", "Wel\u00b7le", "/", "die", "sich", "mit", "der", "ho\u00b7hen", "Wol\u00b7ken", "wolt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$(", "PRELS", "PRF", "APPR", "ART", "ADJA", "NN", "VMFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "verm\u00e4hlen / welche sich herunter neigen solt /", "tokens": ["ver\u00b7m\u00e4h\u00b7len", "/", "wel\u00b7che", "sich", "her\u00b7un\u00b7ter", "nei\u00b7gen", "solt", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVINF", "$(", "PRELS", "PRF", "APZR", "VVFIN", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "ist wider in den Grund / zum Ordnungs Ort / gewichen.", "tokens": ["ist", "wi\u00b7der", "in", "den", "Grund", "/", "zum", "Ord\u00b7nungs", "Ort", "/", "ge\u00b7wi\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "APPR", "APPR", "ART", "NN", "$(", "APPRART", "NN", "NN", "$(", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Nun Meer und Wind wird still / ein hohe Frag geht an:", "tokens": ["Nun", "Meer", "und", "Wind", "wird", "still", "/", "ein", "ho\u00b7he", "Frag", "geht", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "VAFIN", "ADJD", "$(", "ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "was ist / dem Wind und Meer gehorchen / vor ein Mann?", "tokens": ["was", "ist", "/", "dem", "Wind", "und", "Meer", "ge\u00b7hor\u00b7chen", "/", "vor", "ein", "Mann", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "$(", "ART", "NN", "KON", "NN", "VVINF", "$(", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "ich antwort': eben der / der Gott und uns verglichen.", "tokens": ["ich", "ant\u00b7wort'", ":", "e\u00b7ben", "der", "/", "der", "Gott", "und", "uns", "ver\u00b7gli\u00b7chen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "ADV", "ART", "$(", "ART", "NN", "KON", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}