{"textgrid.poem.28470": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "85.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bey Hofe haben die gemein den besten Sold,", "tokens": ["Bey", "Ho\u00b7fe", "ha\u00b7ben", "die", "ge\u00b7mein", "den", "bes\u00b7ten", "Sold", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "ART", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die sonsten doch nichts thun, als fressen nur und sauffen.", "tokens": ["Die", "sons\u00b7ten", "doch", "nichts", "thun", ",", "als", "fres\u00b7sen", "nur", "und", "sauf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PIS", "VVINF", "$,", "KOUS", "ADJA", "ADV", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "F\u00fcrwahr, wer Seele soll und soll Gesund verkauffen,", "tokens": ["F\u00fcr\u00b7wahr", ",", "wer", "See\u00b7le", "soll", "und", "soll", "Ge\u00b7sund", "ver\u00b7kauf\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "NN", "VMFIN", "KON", "VMFIN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dem ist kein Silber nicht genug und auch kein Gold.", "tokens": ["Dem", "ist", "kein", "Sil\u00b7ber", "nicht", "ge\u00b7nug", "und", "auch", "kein", "Gold", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PIAT", "NN", "PTKNEG", "ADV", "KON", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}