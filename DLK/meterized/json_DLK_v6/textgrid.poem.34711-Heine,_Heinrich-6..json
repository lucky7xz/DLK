{"textgrid.poem.34711": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Leise zieht durch mein Gem\u00fct", "tokens": ["Lei\u00b7se", "zieht", "durch", "mein", "Ge\u00b7m\u00fct"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Liebliches Gel\u00e4ute.", "tokens": ["Lieb\u00b7li\u00b7ches", "Ge\u00b7l\u00e4u\u00b7te", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Klinge, kleines Fr\u00fchlingslied.", "tokens": ["Klin\u00b7ge", ",", "klei\u00b7nes", "Fr\u00fch\u00b7lings\u00b7lied", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Kling hinaus ins Weite.", "tokens": ["Kling", "hin\u00b7aus", "ins", "Wei\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APZR", "APPRART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Kling hinaus, bis an das Haus,", "tokens": ["Kling", "hin\u00b7aus", ",", "bis", "an", "das", "Haus", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "APZR", "$,", "KOUS", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo die Blumen sprie\u00dfen.", "tokens": ["Wo", "die", "Blu\u00b7men", "sprie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wenn du eine Rose schaust,", "tokens": ["Wenn", "du", "ei\u00b7ne", "Ro\u00b7se", "schaust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sag, ich la\u00df sie gr\u00fc\u00dfen.", "tokens": ["Sag", ",", "ich", "la\u00df", "sie", "gr\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}