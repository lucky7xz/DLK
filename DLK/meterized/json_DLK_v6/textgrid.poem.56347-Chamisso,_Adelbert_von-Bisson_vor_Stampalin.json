{"textgrid.poem.56347": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "Bisson vor Stampalin", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbzum Unheil hat uns nur der Sturm verschont,", "tokens": ["\u00bb", "zum", "Un\u00b7heil", "hat", "uns", "nur", "der", "Sturm", "ver\u00b7schont", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPRART", "NN", "VAFIN", "PPER", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der uns verschlagen hat vor Stampalin,", "tokens": ["Der", "uns", "ver\u00b7schla\u00b7gen", "hat", "vor", "Stam\u00b7pa\u00b7lin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "VAFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das Nest, wo dieses Raubgesindel wohnt.", "tokens": ["Das", "Nest", ",", "wo", "die\u00b7ses", "Raub\u00b7ge\u00b7sin\u00b7del", "wohnt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die zwei Gefangnen, welche sich vorhin", "tokens": ["Die", "zwei", "Ge\u00b7fang\u00b7nen", ",", "wel\u00b7che", "sich", "vor\u00b7hin"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "CARD", "NN", "$,", "PRELS", "PRF", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Befreiten, schwimmend an das Land begaben \u2013", "tokens": ["Be\u00b7frei\u00b7ten", ",", "schwim\u00b7mend", "an", "das", "Land", "be\u00b7ga\u00b7ben", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJD", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "O diese zwei \u2013! Versteh mich, Trementin:", "tokens": ["O", "die\u00b7se", "zwei", "\u2013", "!", "Ver\u00b7steh", "mich", ",", "Tre\u00b7men\u00b7tin", ":"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NE", "PDAT", "CARD", "$(", "$.", "VVFIN", "PPER", "$,", "NN", "$."], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.7": {"text": "Zu ihrem Neste flogen diese Raben,", "tokens": ["Zu", "ih\u00b7rem", "Nes\u00b7te", "flo\u00b7gen", "die\u00b7se", "Ra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und einem Kampfe sehen wir entgegen,", "tokens": ["Und", "ei\u00b7nem", "Kamp\u00b7fe", "se\u00b7hen", "wir", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Wo nicht zu siegen wir die Hoffnung haben.", "tokens": ["Wo", "nicht", "zu", "sie\u00b7gen", "wir", "die", "Hoff\u00b7nung", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "PTKZU", "VVFIN", "PPER", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Doch, sind uns schon die R\u00e4uber \u00fcberlegen,", "tokens": ["Doch", ",", "sind", "uns", "schon", "die", "R\u00e4u\u00b7ber", "\u00fc\u00b7berl\u00b7e\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VAFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Noch steht uns, nicht besiegt zu werden, frei;", "tokens": ["Noch", "steht", "uns", ",", "nicht", "be\u00b7siegt", "zu", "wer\u00b7den", ",", "frei", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PTKNEG", "VVPP", "PTKZU", "VAINF", "$,", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Wir k\u00f6nnten tun, wie wackre Leute pflegen.", "tokens": ["Wir", "k\u00f6nn\u00b7ten", "tun", ",", "wie", "wack\u00b7re", "Leu\u00b7te", "pfle\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$,", "PWAV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Lebt einer noch von beiden, wer es sei, \u2013", "tokens": ["Lebt", "ei\u00b7ner", "noch", "von", "bei\u00b7den", ",", "wer", "es", "sei", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ART", "ADV", "APPR", "PIAT", "$,", "PWS", "PPER", "VAFIN", "$,", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Zur Pulverkammer \u2013 schnell! \u2013 Du bist ein Mann \u2013", "tokens": ["Zur", "Pul\u00b7ver\u00b7kam\u00b7mer", "\u2013", "schnell", "!", "\u2013", "Du", "bist", "ein", "Mann", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$(", "ADJD", "$.", "$(", "PPER", "VAFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Vorsorglich brennt die Lunte schon dabei!\u00ab \u2013", "tokens": ["Vor\u00b7sorg\u00b7lich", "brennt", "die", "Lun\u00b7te", "schon", "da\u00b7bei", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "PAV", "$.", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Drauf Trementin: \u00bbIch dachte so daran:", "tokens": ["Drauf", "Tre\u00b7men\u00b7tin", ":", "\u00bb", "Ich", "dach\u00b7te", "so", "da\u00b7ran", ":"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "NN", "$.", "$(", "PPER", "VVFIN", "ADV", "PAV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Du, Bisson, oder ich \u2013 es fliegt in Rauch", "tokens": ["Du", ",", "Bis\u00b7son", ",", "o\u00b7der", "ich", "\u2013", "es", "fliegt", "in", "Rauch"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "NE", "$,", "KON", "PPER", "$(", "PPER", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Die Brigg auf, eh der Feind sich freuen kann!\u00ab \u2013", "tokens": ["Die", "Brigg", "auf", ",", "eh", "der", "Feind", "sich", "freu\u00b7en", "kann", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "KOUS", "ART", "NN", "PRF", "VVINF", "VMFIN", "$.", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Sie dr\u00fcckten sich die Hand. Kein Wind, kein Hauch", "tokens": ["Sie", "dr\u00fcck\u00b7ten", "sich", "die", "Hand", ".", "Kein", "Wind", ",", "kein", "Hauch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "$.", "PIAT", "NN", "$,", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Durchschwirrt das schlaffe Tauwerk. Stumm die Nacht.", "tokens": ["Durch\u00b7schwirrt", "das", "schlaf\u00b7fe", "Tau\u00b7werk", ".", "Stumm", "die", "Nacht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$.", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Schlagfertig liegt das Schiff nach gutem Brauch.", "tokens": ["Schlag\u00b7fer\u00b7tig", "liegt", "das", "Schiff", "nach", "gu\u00b7tem", "Brauch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.22": {"text": "Nur funfzehn Tapfre sind der Franken Macht;", "tokens": ["Nur", "funf\u00b7zehn", "Tapf\u00b7re", "sind", "der", "Fran\u00b7ken", "Macht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "NN", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Auf zweien Misticks neun Mal funfzehn kommen,", "tokens": ["Auf", "zwei\u00b7en", "Mis\u00b7ticks", "neun", "Mal", "funf\u00b7zehn", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "CARD", "NN", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Die Gegenwehr zu finden kaum gedacht.", "tokens": ["Die", "Ge\u00b7gen\u00b7wehr", "zu", "fin\u00b7den", "kaum", "ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Sie rudern her; \u2013 der Kampf ist schon entglommen.", "tokens": ["Sie", "ru\u00b7dern", "her", ";", "\u2013", "der", "Kampf", "ist", "schon", "ent\u00b7glom\u00b7men", "."], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$.", "$(", "ART", "NN", "VAFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Gesch\u00fctzesdonner, Kriegesstimmen hallen,", "tokens": ["Ge\u00b7sch\u00fct\u00b7zes\u00b7don\u00b7ner", ",", "Krie\u00b7ges\u00b7stim\u00b7men", "hal\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Sie entern, der Verdeck ist eingenommen.", "tokens": ["Sie", "en\u00b7tern", ",", "der", "Ver\u00b7deck", "ist", "ein\u00b7ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Es sind von funfzehn neune schon gefallen,", "tokens": ["Es", "sind", "von", "funf\u00b7zehn", "neu\u00b7ne", "schon", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "CARD", "CARD", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Und Bisson blutet selbst aus schweren Wunden;", "tokens": ["Und", "Bis\u00b7son", "blu\u00b7tet", "selbst", "aus", "schwe\u00b7ren", "Wun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Er rafft sich auf und l\u00e4\u00dft den Ruf erschallen:", "tokens": ["Er", "rafft", "sich", "auf", "und", "l\u00e4\u00dft", "den", "Ruf", "er\u00b7schal\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PTKVZ", "KON", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "\u00bbauf! \u00fcber Bord, wer nicht den Tod gefunden!\u00ab", "tokens": ["\u00bb", "auf", "!", "\u00fc\u00b7ber", "Bord", ",", "wer", "nicht", "den", "Tod", "ge\u00b7fun\u00b7den", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PTKVZ", "$.", "APPR", "NN", "$,", "PWS", "PTKNEG", "ART", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "Es springen die Gef\u00e4hrten in die Flut,", "tokens": ["Es", "sprin\u00b7gen", "die", "Ge\u00b7f\u00e4hr\u00b7ten", "in", "die", "Flut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.33": {"text": "Er selbst ist in den Schiffsraum schnell verschwunden.", "tokens": ["Er", "selbst", "ist", "in", "den", "Schiffs\u00b7raum", "schnell", "ver\u00b7schwun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "APPR", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.34": {"text": "Und der Pirat, der nun vom Streite ruht,", "tokens": ["Und", "der", "Pi\u00b7rat", ",", "der", "nun", "vom", "Strei\u00b7te", "ruht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.35": {"text": "Der nicht zu morden findet einen mehr,", "tokens": ["Der", "nicht", "zu", "mor\u00b7den", "fin\u00b7det", "ei\u00b7nen", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "PTKZU", "VVINF", "VVFIN", "ART", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.36": {"text": "Beschauet sich den Raum in \u00dcbermut.", "tokens": ["Be\u00b7schau\u00b7et", "sich", "den", "Raum", "in", "\u00dc\u00b7ber\u00b7mut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.37": {"text": "Da flieget donnernd auf das Schiff, das Meer", "tokens": ["Da", "flie\u00b7get", "don\u00b7nernd", "auf", "das", "Schiff", ",", "das", "Meer"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD", "APPR", "ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.38": {"text": "Mischt gischend sich mit Tr\u00fcmmern und mit Leichen,", "tokens": ["Mischt", "gi\u00b7schend", "sich", "mit", "Tr\u00fcm\u00b7mern", "und", "mit", "Lei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "PRF", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.39": {"text": "Ein Dampfgew\u00f6lk bedeckt es stumm und schwer,", "tokens": ["Ein", "Dampf\u00b7ge\u00b7w\u00f6lk", "be\u00b7deckt", "es", "stumm", "und", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.40": {"text": "Und Bissons Name strahlet sonder Gleichen.", "tokens": ["Und", "Bis\u00b7sons", "Na\u00b7me", "strah\u00b7let", "son\u00b7der", "Glei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NN", "VVFIN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}