{"textgrid.poem.59639": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Die Sprache Teuts", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von hohen Zungen,", "tokens": ["Von", "ho\u00b7hen", "Zun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Die t\u00f6nten wie der Silberklang aus Schw\u00e4nen,", "tokens": ["Die", "t\u00f6n\u00b7ten", "wie", "der", "Sil\u00b7ber\u00b7klang", "aus", "Schw\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KOKOM", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ist mir erklungen", "tokens": ["Ist", "mir", "er\u00b7klun\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "PPER", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Ein s\u00fc\u00dfer Klang voll Lieben und voll Sehnen;", "tokens": ["Ein", "s\u00fc\u00b7\u00dfer", "Klang", "voll", "Lie\u00b7ben", "und", "voll", "Seh\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "ADJA", "KON", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Der Knabe lauschte", "tokens": ["Der", "Kna\u00b7be", "lauschte"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Tief staunend, was von ferne,", "tokens": ["Tief", "stau\u00b7nend", ",", "was", "von", "fer\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,", "PRELS", "APPR", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Gleichwie ein Jubellied der Sterne,", "tokens": ["Gleich\u00b7wie", "ein", "Ju\u00b7bel\u00b7lied", "der", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Gewaltig rauschte.", "tokens": ["Ge\u00b7wal\u00b7tig", "rauschte", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Das war dein Brausen,", "tokens": ["Das", "war", "dein", "Brau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Du stolze Sprache Teuts, die Blitz und Wetter", "tokens": ["Du", "stol\u00b7ze", "Spra\u00b7che", "Teuts", ",", "die", "Blitz", "und", "Wet\u00b7ter"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "NN", "$,", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und Sturmessausen", "tokens": ["Und", "Stur\u00b7mess\u00b7au\u00b7sen"], "token_info": ["word", "word"], "pos": ["KON", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Und der Kanonen schallendes Geschmetter", "tokens": ["Und", "der", "Ka\u00b7no\u00b7nen", "schal\u00b7len\u00b7des", "Ge\u00b7schmet\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Verm\u00e4hlt dem S\u00e4useln", "tokens": ["Ver\u00b7m\u00e4hlt", "dem", "S\u00e4u\u00b7seln"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Der Wellen, die im Lenze", "tokens": ["Der", "Wel\u00b7len", ",", "die", "im", "Len\u00b7ze"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Leicht angehaucht die Ringelt\u00e4nze", "tokens": ["Leicht", "an\u00b7ge\u00b7haucht", "die", "Rin\u00b7gel\u00b7t\u00e4n\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVPP", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Auf B\u00e4chen kr\u00e4useln.", "tokens": ["Auf", "B\u00e4\u00b7chen", "kr\u00e4u\u00b7seln", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "O Donnerfrohe!", "tokens": ["O", "Don\u00b7ner\u00b7fro\u00b7he", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "O tapfre Heldenbraut der schnellen Blitze,", "tokens": ["O", "tapf\u00b7re", "Hel\u00b7den\u00b7braut", "der", "schnel\u00b7len", "Blit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie jede Lohe", "tokens": ["Wie", "je\u00b7de", "Lo\u00b7he"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "PIAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Des Feuers aufzuckt zum gestirnten Sitze,", "tokens": ["Des", "Feu\u00b7ers", "auf\u00b7zuckt", "zum", "ge\u00b7stirn\u00b7ten", "Sit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Fliegt stolze Minne", "tokens": ["Fliegt", "stol\u00b7ze", "Min\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "In T\u00f6nen und in Worten,", "tokens": ["In", "T\u00f6\u00b7nen", "und", "in", "Wor\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und st\u00fcrmend zu den Himmelspforten", "tokens": ["Und", "st\u00fcr\u00b7mend", "zu", "den", "Him\u00b7mel\u00b7spfor\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Spornst du die Sinne.", "tokens": ["Spornst", "du", "die", "Sin\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Drob w\u00e4hnen viele,", "tokens": ["Drob", "w\u00e4h\u00b7nen", "vie\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Sie k\u00f6nnen auch den Sonnenreigen fliegen", "tokens": ["Sie", "k\u00f6n\u00b7nen", "auch", "den", "Son\u00b7nen\u00b7rei\u00b7gen", "flie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Im Heldenspiele,", "tokens": ["Im", "Hel\u00b7den\u00b7spie\u00b7le", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Wo Adler kaum im k\u00fchnsten Fluge siegen.", "tokens": ["Wo", "Ad\u00b7ler", "kaum", "im", "k\u00fchns\u00b7ten", "Flu\u00b7ge", "sie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADV", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Der Sonnenw\u00e4chter", "tokens": ["Der", "Son\u00b7nen\u00b7w\u00e4ch\u00b7ter"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Sieht traurig, wie sie fallen,", "tokens": ["Sieht", "trau\u00b7rig", ",", "wie", "sie", "fal\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,", "PWAV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und h\u00f6rt, wie hier von unten schallen", "tokens": ["Und", "h\u00f6rt", ",", "wie", "hier", "von", "un\u00b7ten", "schal\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "PWAV", "ADV", "APPR", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Spott und Gel\u00e4chter.", "tokens": ["Spott", "und", "Ge\u00b7l\u00e4ch\u00b7ter", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "Doch schw\u00e4chern Fl\u00fcgeln,", "tokens": ["Doch", "schw\u00e4\u00b7chern", "Fl\u00fc\u00b7geln", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Die auch in s\u00fc\u00dfer T\u00f6ne Lust erbeben,", "tokens": ["Die", "auch", "in", "s\u00fc\u00b7\u00dfer", "T\u00f6\u00b7ne", "Lust", "er\u00b7be\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ADJA", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Auf Blumenh\u00fcgeln", "tokens": ["Auf", "Blu\u00b7men\u00b7h\u00fc\u00b7geln"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Ward ihnen holdes Schwingen, Klingen, Schweben,", "tokens": ["Ward", "ih\u00b7nen", "hol\u00b7des", "Schwin\u00b7gen", ",", "Klin\u00b7gen", ",", "Schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJA", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "In leichten Scherzen,", "tokens": ["In", "leich\u00b7ten", "Scher\u00b7zen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "In trauter Erdenn\u00e4he", "tokens": ["In", "trau\u00b7ter", "Er\u00b7den\u00b7n\u00e4\u00b7he"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Spielt zarte Lust und zartes Wehe", "tokens": ["Spielt", "zar\u00b7te", "Lust", "und", "zar\u00b7tes", "We\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJA", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Sich lieb vom Herzen.", "tokens": ["Sich", "lieb", "vom", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Drum la\u00df sie klingen", "tokens": ["Drum", "la\u00df", "sie", "klin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Zu ihrem Sonnenglanz die Sonnenaare,", "tokens": ["Zu", "ih\u00b7rem", "Son\u00b7nen\u00b7glanz", "die", "Son\u00b7ne\u00b7naa\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Auf leisen Schwingen", "tokens": ["Auf", "lei\u00b7sen", "Schwin\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Durchflattre du der Birken gr\u00fcne Haare,", "tokens": ["Durch\u00b7flatt\u00b7re", "du", "der", "Bir\u00b7ken", "gr\u00fc\u00b7ne", "Haa\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "In stillen Hainen", "tokens": ["In", "stil\u00b7len", "Hai\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Mit s\u00fc\u00dfen Philomelen", "tokens": ["Mit", "s\u00fc\u00b7\u00dfen", "Phi\u00b7lo\u00b7me\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Sollst du dein s\u00fc\u00dfes Leid erz\u00e4hlen", "tokens": ["Sollst", "du", "dein", "s\u00fc\u00b7\u00dfes", "Leid", "er\u00b7z\u00e4h\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PPOSAT", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und still beweinen.", "tokens": ["Und", "still", "be\u00b7wei\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}