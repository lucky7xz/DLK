{"textgrid.poem.47689": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[ich ging wohl \u00fcber den Flu\u00df]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich ging wohl \u00fcber den Flu\u00df,", "tokens": ["Ich", "ging", "wohl", "\u00fc\u00b7ber", "den", "Flu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Ich schritt wohl \u00fcber die Br\u00fccke,", "tokens": ["Ich", "schritt", "wohl", "\u00fc\u00b7ber", "die", "Br\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich haftete mit dem Blicke,", "tokens": ["Ich", "haf\u00b7te\u00b7te", "mit", "dem", "Bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ich stockte mit dem Tritte,", "tokens": ["Ich", "stock\u00b7te", "mit", "dem", "Trit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Da, wo an ihrem Fenster", "tokens": ["Da", ",", "wo", "an", "ih\u00b7rem", "Fens\u00b7ter"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Kathinka sa\u00df und stickte,", "tokens": ["Ka\u00b7thin\u00b7ka", "sa\u00df", "und", "stick\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.7": {"text": "Benetzend ihren Rahmen", "tokens": ["Be\u00b7net\u00b7zend", "ih\u00b7ren", "Rah\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["VVPP", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mit einem Thr\u00e4nengu\u00df.", "tokens": ["Mit", "ei\u00b7nem", "Thr\u00e4\u00b7nen\u00b7gu\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "\u00bbo sage mir, Kathinka,", "tokens": ["\u00bb", "o", "sa\u00b7ge", "mir", ",", "Ka\u00b7thin\u00b7ka", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "FM", "VVFIN", "PPER", "$,", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was dr\u00e4nget dich, zu weinen,", "tokens": ["Was", "dr\u00e4n\u00b7get", "dich", ",", "zu", "wei\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Was tr\u00fcbet dein Gem\u00fcthe", "tokens": ["Was", "tr\u00fc\u00b7bet", "dein", "Ge\u00b7m\u00fc\u00b7the"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "F\u00fcr ein geheimer Gram?\u00ab \u2013", "tokens": ["F\u00fcr", "ein", "ge\u00b7hei\u00b7mer", "Gram", "?", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$.", "$(", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "\u00bbich weine, Freund, um einen,", "tokens": ["\u00bb", "ich", "wei\u00b7ne", ",", "Freund", ",", "um", "ei\u00b7nen", ","], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "$,", "NN", "$,", "KOUI", "ART", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der meinem Herzen theuer,", "tokens": ["Der", "mei\u00b7nem", "Her\u00b7zen", "theu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der mir die Ruhe nahm.\u00ab \u2013", "tokens": ["Der", "mir", "die", "Ru\u00b7he", "nahm", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "$.", "$(", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "\u00bbo nenne seinen Namen!\u00ab \u2013", "tokens": ["\u00bb", "o", "nen\u00b7ne", "sei\u00b7nen", "Na\u00b7men", "!", "\u00ab", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "FM", "VVFIN", "PPOSAT", "NN", "$.", "$(", "$("], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.5": {"line.1": {"text": "\u00bbnein, nein, das ist unm\u00f6glich;", "tokens": ["\u00bb", "nein", ",", "nein", ",", "das", "ist", "un\u00b7m\u00f6g\u00b7lich", ";"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PTKANT", "$,", "PTKANT", "$,", "PDS", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Denn wenn ich ihn verriethe,", "tokens": ["Denn", "wenn", "ich", "ihn", "ver\u00b7rie\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es t\u00f6dtete mich die Schaam.\u00ab", "tokens": ["Es", "t\u00f6d\u00b7te\u00b7te", "mich", "die", "Schaam", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}