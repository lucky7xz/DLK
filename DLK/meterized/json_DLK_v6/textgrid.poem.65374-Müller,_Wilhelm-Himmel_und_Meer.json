{"textgrid.poem.65374": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Himmel und Meer", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie sich im Meere jede Wolke malt,", "tokens": ["Wie", "sich", "im", "Mee\u00b7re", "je\u00b7de", "Wol\u00b7ke", "malt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "APPRART", "NN", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie's alle Sonnenstrahlen wiederstrahlt,", "tokens": ["Wie's", "al\u00b7le", "Son\u00b7nen\u00b7strah\u00b7len", "wie\u00b7der\u00b7strahlt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie es bei jedem leisen Hauche bebt,", "tokens": ["Wie", "es", "bei", "je\u00b7dem", "lei\u00b7sen", "Hau\u00b7che", "bebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "PIAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der aus der fernen H\u00f6h' herniederschwebt,", "tokens": ["Der", "aus", "der", "fer\u00b7nen", "H\u00f6h'", "her\u00b7nie\u00b7der\u00b7schwebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "So ist mein Herz dein Meer, mein Himmel du!", "tokens": ["So", "ist", "mein", "Herz", "dein", "Meer", ",", "mein", "Him\u00b7mel", "du", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Wann g\u00f6nnest du den Wogen endlich Ruh'?", "tokens": ["Wann", "g\u00f6n\u00b7nest", "du", "den", "Wo\u00b7gen", "end\u00b7lich", "Ruh'", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "NN", "ADV", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}