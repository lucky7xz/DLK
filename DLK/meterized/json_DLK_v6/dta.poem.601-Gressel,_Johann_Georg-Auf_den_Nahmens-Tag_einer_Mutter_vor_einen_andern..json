{"dta.poem.601": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Auf den Nahmens-Tag einer Mutter/  \n vor einen andern.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Auf! Himmel auf! verkl\u00e4hre dich!", "tokens": ["Auf", "!", "Him\u00b7mel", "auf", "!", "ver\u00b7kl\u00e4h\u00b7re", "dich", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "$.", "NN", "PTKVZ", "$.", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Jhr schwartzen Wolcken weicht zur\u00fccke", "tokens": ["Ihr", "schwart\u00b7zen", "Wol\u00b7cken", "weicht", "zu\u00b7r\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Was Ungl\u00fcck macht verberge sich/", "tokens": ["Was", "Un\u00b7gl\u00fcck", "macht", "ver\u00b7ber\u00b7ge", "sich", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVFIN", "VVFIN", "PRF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und du o! wandelbahres Gl\u00fccke", "tokens": ["Und", "du", "o", "!", "wan\u00b7del\u00b7bah\u00b7res", "Gl\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "FM", "$.", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Misch heute uns nichts tr\u00fcbes ein", "tokens": ["Misch", "heu\u00b7te", "uns", "nichts", "tr\u00fc\u00b7bes", "ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "PPER", "PIS", "ADJA", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "La\u00df deinen Glantz stets g\u00fclden seyn!", "tokens": ["La\u00df", "dei\u00b7nen", "Glantz", "stets", "g\u00fcl\u00b7den", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Auf Himmel auf verkl\u00e4hre dich!", "tokens": ["Auf", "Him\u00b7mel", "auf", "ver\u00b7kl\u00e4h\u00b7re", "dich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}