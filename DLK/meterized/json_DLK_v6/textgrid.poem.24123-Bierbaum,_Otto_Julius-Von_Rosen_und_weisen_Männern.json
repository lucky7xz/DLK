{"textgrid.poem.24123": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Von Rosen und weisen M\u00e4nnern", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Leute giebt's, mit langen grauen B\u00e4rten,", "tokens": ["Leu\u00b7te", "giebt's", ",", "mit", "lan\u00b7gen", "grau\u00b7en", "B\u00e4r\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Dicke Brillen auf den breiten Nasen;", "tokens": ["Di\u00b7cke", "Bril\u00b7len", "auf", "den", "brei\u00b7ten", "Na\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Feierlich, mit ungemeiner W\u00fcrde,", "tokens": ["Fei\u00b7er\u00b7lich", ",", "mit", "un\u00b7ge\u00b7mei\u00b7ner", "W\u00fcr\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Klagen sie, die Erde sei vom Uebel.", "tokens": ["Kla\u00b7gen", "sie", ",", "die", "Er\u00b7de", "sei", "vom", "Ue\u00b7bel", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "ART", "NN", "VAFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Glaube nicht sothanen Klagem\u00e4nnern!", "tokens": ["Glau\u00b7be", "nicht", "sot\u00b7ha\u00b7nen", "Kla\u00b7ge\u00b7m\u00e4n\u00b7nern", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Allerdings, nicht immer bl\u00fchen Rosen,", "tokens": ["Al\u00b7ler\u00b7dings", ",", "nicht", "im\u00b7mer", "bl\u00fc\u00b7hen", "Ro\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PTKNEG", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und zuweilen stechen dich die Dornen.", "tokens": ["Und", "zu\u00b7wei\u00b7len", "ste\u00b7chen", "dich", "die", "Dor\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Aber, und dies Aber sei gepriesen,", "tokens": ["A\u00b7ber", ",", "und", "dies", "A\u00b7ber", "sei", "ge\u00b7prie\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KON", "PDS", "KON", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wo ein Dorn dich sticht, da darfst du hoffen:", "tokens": ["Wo", "ein", "Dorn", "dich", "sticht", ",", "da", "darfst", "du", "hof\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Bald schwebt eine Rose hier im Winde.", "tokens": ["Bald", "schwebt", "ei\u00b7ne", "Ro\u00b7se", "hier", "im", "Win\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "APPRART", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Eine Rose, hundert, tausend Rosen,", "tokens": ["Ei\u00b7ne", "Ro\u00b7se", ",", "hun\u00b7dert", ",", "tau\u00b7send", "Ro\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "CARD", "$,", "CARD", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und die harten Dornen sind vergessen:", "tokens": ["Und", "die", "har\u00b7ten", "Dor\u00b7nen", "sind", "ver\u00b7ges\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Kleine M\u00e4dchen tanzen um die B\u00fcsche,", "tokens": ["Klei\u00b7ne", "M\u00e4d\u00b7chen", "tan\u00b7zen", "um", "die", "B\u00fc\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ihre Seelen wissen nichts von Dornen.", "tokens": ["Ih\u00b7re", "See\u00b7len", "wis\u00b7sen", "nichts", "von", "Dor\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PIS", "APPR", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Dumm sind diese lieben kleinen M\u00e4dchen,", "tokens": ["Dumm", "sind", "die\u00b7se", "lie\u00b7ben", "klei\u00b7nen", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PDAT", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und du Griesebart bist viel gescheidter;", "tokens": ["Und", "du", "Grie\u00b7se\u00b7bart", "bist", "viel", "ge\u00b7scheid\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "NE", "VAFIN", "ADV", "ADJD", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Tief mu\u00df meinen Hut ich vor dir ziehen,", "tokens": ["Tief", "mu\u00df", "mei\u00b7nen", "Hut", "ich", "vor", "dir", "zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "PPOSAT", "NN", "PPER", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Denn du bist in Dornen sehr beschlagen.", "tokens": ["Denn", "du", "bist", "in", "Dor\u00b7nen", "sehr", "be\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "APPR", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Aber wenn im Wind die Rosen schweben", "tokens": ["A\u00b7ber", "wenn", "im", "Wind", "die", "Ro\u00b7sen", "schwe\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "APPRART", "NN", "ART", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und im Tanz die lieben kleinen M\u00e4dchen,", "tokens": ["Und", "im", "Tanz", "die", "lie\u00b7ben", "klei\u00b7nen", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Dann, mein sehr gescheidter Mann im Barte,", "tokens": ["Dann", ",", "mein", "sehr", "ge\u00b7scheid\u00b7ter", "Mann", "im", "Bar\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPOSAT", "ADV", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Dr\u00fccke dich, geh, mach dich in die B\u00fcsche.", "tokens": ["Dr\u00fc\u00b7cke", "dich", ",", "geh", ",", "mach", "dich", "in", "die", "B\u00fc\u00b7sche", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VVFIN", "$,", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Denn, verzeihe: Wenn die Rosen bl\u00fchen", "tokens": ["Denn", ",", "ver\u00b7zei\u00b7he", ":", "Wenn", "die", "Ro\u00b7sen", "bl\u00fc\u00b7hen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$,", "VVFIN", "$.", "KOUS", "ART", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und die lieben kleinen M\u00e4dchen tanzen,", "tokens": ["Und", "die", "lie\u00b7ben", "klei\u00b7nen", "M\u00e4d\u00b7chen", "tan\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Ist die Dornenweisheit \u00fcberfl\u00fcssig.", "tokens": ["Ist", "die", "Dor\u00b7nen\u00b7weis\u00b7heit", "\u00fc\u00b7berf\u00b7l\u00fcs\u00b7sig", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wenigstens f\u00fcr uns. Du selber kannst ja", "tokens": ["We\u00b7nigs\u00b7tens", "f\u00fcr", "uns", ".", "Du", "sel\u00b7ber", "kannst", "ja"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPER", "$.", "PPER", "ADV", "VMFIN", "ADV"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "Eine Dissertation im Busche", "tokens": ["Ei\u00b7ne", "Dis\u00b7ser\u00b7ta\u00b7ti\u00b7on", "im", "Bu\u00b7sche"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Oder meinetwegen zweie schreiben.", "tokens": ["O\u00b7der", "mei\u00b7net\u00b7we\u00b7gen", "zwei\u00b7e", "schrei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}