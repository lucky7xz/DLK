{"textgrid.poem.60148": {"metadata": {"author": {"name": "Karsch, Anna Louisa", "birth": "N.A.", "death": "N.A."}, "title": "1L: Den Obersch\u00e4fer Friederich", "genre": "verse", "period": "N.A.", "pub_year": 1762, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Den Obersch\u00e4fer Friederich", "tokens": ["Den", "O\u00b7ber\u00b7sch\u00e4\u00b7fer", "Frie\u00b7de\u00b7rich"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Thyrsis, hoffen wir!", "tokens": ["Mein", "Thyr\u00b7sis", ",", "hof\u00b7fen", "wir", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zu seinen F\u00fcssen kr\u00fcmmet sich", "tokens": ["Zu", "sei\u00b7nen", "F\u00fcs\u00b7sen", "kr\u00fcm\u00b7met", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nun bald das b\u00f6se Thier,", "tokens": ["Nun", "bald", "das", "b\u00f6\u00b7se", "Thier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Das oft in unsre Heerden f\u00e4llt,", "tokens": ["Das", "oft", "in", "uns\u00b7re", "Heer\u00b7den", "f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die besten L\u00e4mmer w\u00fcrgt,", "tokens": ["Die", "bes\u00b7ten", "L\u00e4m\u00b7mer", "w\u00fcrgt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sich auf die h\u00f6chsten Berge stellt,", "tokens": ["Sich", "auf", "die", "h\u00f6chs\u00b7ten", "Ber\u00b7ge", "stellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und seinen Raub verbirgt.", "tokens": ["Und", "sei\u00b7nen", "Raub", "ver\u00b7birgt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "In tiefer H\u00f6hle schlau versteckt,", "tokens": ["In", "tie\u00b7fer", "H\u00f6h\u00b7le", "schlau", "ver\u00b7steckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Lauscht es, und d\u00fcrstet Blut,", "tokens": ["Lauscht", "es", ",", "und", "d\u00fcrs\u00b7tet", "Blut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KON", "VMFIN", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Und springt, wann es ein Schaf entdeckt,", "tokens": ["Und", "springt", ",", "wann", "es", "ein", "Schaf", "ent\u00b7deckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hervor mit Tyger-Wuth.", "tokens": ["Her\u00b7vor", "mit", "Ty\u00b7ger\u00b7Wuth", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die grossen Hunde werden scheu,", "tokens": ["Die", "gros\u00b7sen", "Hun\u00b7de", "wer\u00b7den", "scheu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Thier hascht sie mit List;", "tokens": ["Das", "Thier", "hascht", "sie", "mit", "List", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bald aber steurt die R\u00e4uberey", "tokens": ["Bald", "a\u00b7ber", "steurt", "die", "R\u00e4u\u00b7be\u00b7rey"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Pan, der mit Friedrich ist!", "tokens": ["Pan", ",", "der", "mit", "Fried\u00b7rich", "ist", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "APPR", "NE", "VAFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.5": {"line.1": {"text": "Schon seinem Herzen zugelenkt", "tokens": ["Schon", "sei\u00b7nem", "Her\u00b7zen", "zu\u00b7ge\u00b7lenkt"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ward ihm ein fremder Hirt,", "tokens": ["Ward", "ihm", "ein", "frem\u00b7der", "Hirt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der zornig an das Thier gedenkt,", "tokens": ["Der", "zor\u00b7nig", "an", "das", "Thier", "ge\u00b7denkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und treu ihm helfen wird.", "tokens": ["Und", "treu", "ihm", "hel\u00b7fen", "wird", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPER", "VVINF", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Wir h\u00f6rten dies, und angef\u00fcllt", "tokens": ["Wir", "h\u00f6r\u00b7ten", "dies", ",", "und", "an\u00b7ge\u00b7f\u00fcllt"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PDS", "$,", "KON", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von Freuden, wie entz\u00fcckt,", "tokens": ["Von", "Freu\u00b7den", ",", "wie", "ent\u00b7z\u00fcckt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PWAV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ward hergetanzt, um Friedrichs Bild,", "tokens": ["Ward", "her\u00b7ge\u00b7tanzt", ",", "um", "Fried\u00b7richs", "Bild", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$,", "KOUI", "NE", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Lorber rund umschm\u00fcckt.", "tokens": ["Mit", "Lor\u00b7ber", "rund", "um\u00b7schm\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Hoch aufgeh\u00fcpft mit Herzenstanz", "tokens": ["Hoch", "auf\u00b7ge\u00b7h\u00fcpft", "mit", "Her\u00b7zen\u00b7stanz"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVPP", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist vor uns her Welin!", "tokens": ["Ist", "vor", "uns", "her", "We\u00b7lin", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PRF", "APZR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er flochte selbst den grossen Cranz", "tokens": ["Er", "floch\u00b7te", "selbst", "den", "gros\u00b7sen", "Cranz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von Zweigen frisch und gr\u00fcn!", "tokens": ["Von", "Zwei\u00b7gen", "frisch", "und", "gr\u00fcn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Und hergetragen bracht er froh", "tokens": ["Und", "her\u00b7ge\u00b7tra\u00b7gen", "bracht", "er", "froh"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVPP", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das theure Bild, und sprach:", "tokens": ["Das", "theu\u00b7re", "Bild", ",", "und", "sprach", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zur\u00fccke kommt der Sch\u00e4fer so", "tokens": ["Zu\u00b7r\u00fc\u00b7cke", "kommt", "der", "Sch\u00e4\u00b7fer", "so"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Lorber, den er brach!", "tokens": ["Mit", "Lor\u00b7ber", ",", "den", "er", "brach", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Wir f\u00fchlten in der Seele tief,", "tokens": ["Wir", "f\u00fchl\u00b7ten", "in", "der", "See\u00b7le", "tief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wir jauchzten laut, wie er!", "tokens": ["Wir", "jauchz\u00b7ten", "laut", ",", "wie", "er", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "PWAV", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und Faunen, die der Jubel rief,", "tokens": ["Und", "Fau\u00b7nen", ",", "die", "der", "Ju\u00b7bel", "rief", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die h\u00fcpften um uns her!", "tokens": ["Die", "h\u00fcpf\u00b7ten", "um", "uns", "her", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Auf tausend Saiten spielte sich", "tokens": ["Auf", "tau\u00b7send", "Sai\u00b7ten", "spiel\u00b7te", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "CARD", "NN", "VVFIN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Herz; ich h\u00fcpfte mit,", "tokens": ["Mein", "Herz", ";", "ich", "h\u00fcpf\u00b7te", "mit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Warf Freudenvolle Blick' auf dich,", "tokens": ["Warf", "Freu\u00b7den\u00b7vol\u00b7le", "Blick'", "auf", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und dachte nicht den Trit.", "tokens": ["Und", "dach\u00b7te", "nicht", "den", "Trit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Und wenn des Oberhirten Hand", "tokens": ["Und", "wenn", "des", "O\u00b7berh\u00b7ir\u00b7ten", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Ungeheur erlegt,", "tokens": ["Das", "Un\u00b7ge\u00b7heur", "er\u00b7legt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn er wie Hercul zum Gewand", "tokens": ["Wenn", "er", "wie", "Her\u00b7cul", "zum", "Ge\u00b7wand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "KOKOM", "NE", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Haut des Thieres tr\u00e4gt;", "tokens": ["Die", "Haut", "des", "Thie\u00b7res", "tr\u00e4gt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Dann komme Thyrsis hin mit mir", "tokens": ["Dann", "kom\u00b7me", "Thyr\u00b7sis", "hin", "mit", "mir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NE", "ADV", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu danken hoch dem Pan!", "tokens": ["Zu", "dan\u00b7ken", "hoch", "dem", "Pan", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nachsingen will ich Lieder dir,", "tokens": ["Nach\u00b7sin\u00b7gen", "will", "ich", "Lie\u00b7der", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf gr\u00fcner Siegesbahn!", "tokens": ["Auf", "gr\u00fc\u00b7ner", "Sie\u00b7ges\u00b7bahn", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Im breiten Schatten an der Spree", "tokens": ["Im", "brei\u00b7ten", "Schat\u00b7ten", "an", "der", "Spree"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Versammlen Hirten sich,", "tokens": ["Ver\u00b7samm\u00b7len", "Hir\u00b7ten", "sich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PRF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Behorchet werden aus der H\u00f6h", "tokens": ["Be\u00b7hor\u00b7chet", "wer\u00b7den", "aus", "der", "H\u00f6h"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "VAFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von G\u00f6ttern du und ich!", "tokens": ["Von", "G\u00f6t\u00b7tern", "du", "und", "ich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "KON", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}