{"textgrid.poem.46687": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[wer mir von euch Beiden]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer mir von euch Beiden", "tokens": ["Wer", "mir", "von", "euch", "Bei\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "APPR", "PPER", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Theurer sei, entscheiden", "tokens": ["Theu\u00b7rer", "sei", ",", "ent\u00b7schei\u00b7den"], "token_info": ["word", "word", "punct", "word"], "pos": ["ADJD", "VAFIN", "$,", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Konnt' ich nie den Streit", "tokens": ["Konnt'", "ich", "nie", "den", "Streit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "In dem Mutterherzen;", "tokens": ["In", "dem", "Mut\u00b7ter\u00b7her\u00b7zen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Meine Liebeskerzen,", "tokens": ["Mei\u00b7ne", "Lie\u00b7bes\u00b7ker\u00b7zen", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Meine Lebensflammen!", "tokens": ["Mei\u00b7ne", "Le\u00b7bens\u00b7flam\u00b7men", "!"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Gingt ihr nun zusammen", "tokens": ["Gingt", "ihr", "nun", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Aus zu gleicher Zeit?", "tokens": ["Aus", "zu", "glei\u00b7cher", "Zeit", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "Ach, der Tod, der k\u00fchle,", "tokens": ["Ach", ",", "der", "Tod", ",", "der", "k\u00fch\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.10": {"text": "Mit wie kalten Scherzen", "tokens": ["Mit", "wie", "kal\u00b7ten", "Scher\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "KOKOM", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.11": {"text": "Er die Streitigkeit", "tokens": ["Er", "die", "Strei\u00b7tig\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["PPER", "ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.12": {"text": "Schlichtet der Gef\u00fchle,", "tokens": ["Schlich\u00b7tet", "der", "Ge\u00b7f\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.13": {"text": "Die um euch verschwammen", "tokens": ["Die", "um", "euch", "ver\u00b7schwam\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "PPER", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.14": {"text": "In die Ewigkeit", "tokens": ["In", "die", "E\u00b7wig\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.15": {"text": "Ungetheilter Schmerzen;", "tokens": ["Un\u00b7ge\u00b7theil\u00b7ter", "Schmer\u00b7zen", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.16": {"text": "Meine Liebeskerzen,", "tokens": ["Mei\u00b7ne", "Lie\u00b7bes\u00b7ker\u00b7zen", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.17": {"text": "Meine Lebensflammen,", "tokens": ["Mei\u00b7ne", "Le\u00b7bens\u00b7flam\u00b7men", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.18": {"text": "Die ihr aus zusammen", "tokens": ["Die", "ihr", "aus", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.19": {"text": "Gingt zu gleicher Zeit!", "tokens": ["Gingt", "zu", "glei\u00b7cher", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.20": {"text": "Und im Mutterherzen", "tokens": ["Und", "im", "Mut\u00b7ter\u00b7her\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["KON", "APPRART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.21": {"text": "Kann ich nie den Streit,", "tokens": ["Kann", "ich", "nie", "den", "Streit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.22": {"text": "Wer mir von euch Beiden", "tokens": ["Wer", "mir", "von", "euch", "Bei\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "APPR", "PPER", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.23": {"text": "Theurer sei, entscheiden,", "tokens": ["Theu\u00b7rer", "sei", ",", "ent\u00b7schei\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJD", "VAFIN", "$,", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.24": {"text": "Meine Liebeskerzen,", "tokens": ["Mei\u00b7ne", "Lie\u00b7be