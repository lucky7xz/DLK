{"textgrid.poem.66852": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Auf aus deinem winzigen Jammer,", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf aus deinem winzigen Jammer,", "tokens": ["Auf", "aus", "dei\u00b7nem", "win\u00b7zi\u00b7gen", "Jam\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Denke der gewaltigen Zeiten!", "tokens": ["Den\u00b7ke", "der", "ge\u00b7wal\u00b7ti\u00b7gen", "Zei\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Fa\u00df den Sturmhut, wirf den Hammer,", "tokens": ["Fa\u00df", "den", "Sturm\u00b7hut", ",", "wirf", "den", "Ham\u00b7mer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "VVIMP", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Reite durch der Erde Weiten!", "tokens": ["Rei\u00b7te", "durch", "der", "Er\u00b7de", "Wei\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Was f\u00fcr j\u00e4mmerliche Tr\u00e4nen", "tokens": ["Was", "f\u00fcr", "j\u00e4m\u00b7mer\u00b7li\u00b7che", "Tr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "L\u00e4\u00dft du deinem Aug' enttropfen?", "tokens": ["L\u00e4\u00dft", "du", "dei\u00b7nem", "Aug'", "ent\u00b7trop\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Streiche deines Rosses M\u00e4hnen,", "tokens": ["Strei\u00b7che", "dei\u00b7nes", "Ros\u00b7ses", "M\u00e4h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00f6r das Herz der Erde klopfen!", "tokens": ["H\u00f6r", "das", "Herz", "der", "Er\u00b7de", "klop\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Leg dein Herz ans Herz der Erde", "tokens": ["Leg", "dein", "Herz", "ans", "Herz", "der", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und der Menschheit Brust umschlie\u00dfe!", "tokens": ["Und", "der", "Menschheit", "Brust", "um\u00b7schlie\u00b7\u00dfe", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Deinen Eimer voll Beschwerde", "tokens": ["Dei\u00b7nen", "Ei\u00b7mer", "voll", "Be\u00b7schwer\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In das Meer des Elends gie\u00dfe.", "tokens": ["In", "das", "Meer", "des", "E\u00b7lends", "gie\u00b7\u00dfe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Fa\u00df den Sturmhut, wirf den Hammer,", "tokens": ["Fa\u00df", "den", "Sturm\u00b7hut", ",", "wirf", "den", "Ham\u00b7mer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "VVIMP", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Reite durch der Erde Weiten!", "tokens": ["Rei\u00b7te", "durch", "der", "Er\u00b7de", "Wei\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf aus deinem winzigen Jammer,", "tokens": ["Auf", "aus", "dei\u00b7nem", "win\u00b7zi\u00b7gen", "Jam\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Denke der gewaltigen Zeiten!", "tokens": ["Den\u00b7ke", "der", "ge\u00b7wal\u00b7ti\u00b7gen", "Zei\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}