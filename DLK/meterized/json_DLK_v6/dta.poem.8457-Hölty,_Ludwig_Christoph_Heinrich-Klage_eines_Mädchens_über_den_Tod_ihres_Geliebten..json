{"dta.poem.8457": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Klage eines M\u00e4dchens \u00fcber den Tod  \n ihres Geliebten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1783", "urn": "urn:nbn:de:kobv:b4-200905192568", "language": ["de:0.99"], "booktitle": "H\u00f6lty, Ludwig Christoph Heinrich: Gedichte. Hamburg, 1783."}, "poem": {"stanza.1": {"line.1": {"text": "O w\u00fcrd' er nie erf\u00fcllt!", "tokens": ["O", "w\u00fcrd'", "er", "nie", "er\u00b7f\u00fcllt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Sobald der Schlummer mich beschlich,", "tokens": ["So\u00b7bald", "der", "Schlum\u00b7mer", "mich", "be\u00b7schlich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Erschien mir Wilhelms Bild.", "tokens": ["Er\u00b7schien", "mir", "Wil\u00b7helms", "Bild", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NE", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ein Nachtgespenst, das auf der Gruft", "tokens": ["Ein", "Nacht\u00b7ge\u00b7spenst", ",", "das", "auf", "der", "Gruft"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Im Todtenhemde sizt!", "tokens": ["Im", "Tod\u00b7ten\u00b7hem\u00b7de", "sizt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Sein Haar flog blutig in die Luft;", "tokens": ["Sein", "Haar", "flog", "blu\u00b7tig", "in", "die", "Luft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die Brust war aufgeschlizt.", "tokens": ["Die", "Brust", "war", "auf\u00b7ge\u00b7schlizt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Blut floss ihm durch das Grabgewand,", "tokens": ["Blut", "floss", "ihm", "durch", "das", "Grab\u00b7ge\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie eine Purpurflut;", "tokens": ["Wie", "ei\u00b7ne", "Pur\u00b7pur\u00b7flut", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er nahm des Blutes in die Hand,", "tokens": ["Er", "nahm", "des", "Blu\u00b7tes", "in", "die", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und zeigte mir das Blut.", "tokens": ["Und", "zeig\u00b7te", "mir", "das", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sein blutend Herz, als sucht' es mich,", "tokens": ["Sein", "blu\u00b7tend", "Herz", ",", "als", "sucht'", "es", "mich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "$,", "KOUS", "VVFIN", "PPER", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Schlug dreimal hoch empor,", "tokens": ["Schlug", "drei\u00b7mal", "hoch", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und dreimal flog es sichtbarlich", "tokens": ["Und", "drei\u00b7mal", "flog", "es", "sicht\u00b7bar\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Aus seiner Wund' hervor.", "tokens": ["Aus", "sei\u00b7ner", "Wund'", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Doch pl\u00f6zlich floss ein L\u00e4cheln ihm", "tokens": ["Doch", "pl\u00f6z\u00b7lich", "floss", "ein", "L\u00e4\u00b7cheln", "ihm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ins traurige Gesicht;", "tokens": ["Ins", "trau\u00b7ri\u00b7ge", "Ge\u00b7sicht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er sprach, als spr\u00e4chen Serafim:", "tokens": ["Er", "sprach", ",", "als", "spr\u00e4\u00b7chen", "Se\u00b7ra\u00b7fim", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Geliebte, weine nicht!", "tokens": ["Ge\u00b7lieb\u00b7te", ",", "wei\u00b7ne", "nicht", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es war kein leeres Nachtgebild,", "tokens": ["Es", "war", "kein", "lee\u00b7res", "Nacht\u00b7ge\u00b7bild", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Was mir im Traum erschien.", "tokens": ["Was", "mir", "im", "Traum", "er\u00b7schien", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die Sarazenen, k\u00fchn und wild,", "tokens": ["Die", "Sa\u00b7ra\u00b7ze\u00b7nen", ",", "k\u00fchn", "und", "wild", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die, die zerfleischten ihn!", "tokens": ["Die", ",", "die", "zer\u00b7fleischten", "ihn", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PRELS", "VVFIN", "PPER", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Wo Jesus Christus uns vers\u00fchnt,", "tokens": ["Wo", "Je\u00b7sus", "Chris\u00b7tus", "uns", "ver\u00b7s\u00fchnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "NE", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da modert sein Gebein.", "tokens": ["Da", "mo\u00b7dert", "sein", "Ge\u00b7bein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Rausch sanfter, wo sein H\u00fcgel gr\u00fcnt,", "tokens": ["Rausch", "sanf\u00b7ter", ",", "wo", "sein", "H\u00fc\u00b7gel", "gr\u00fcnt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "PWAV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Rausch sanfter, Palmenhain.", "tokens": ["Rausch", "sanf\u00b7ter", ",", "Pal\u00b7men\u00b7hain", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NN", "ADJD", "$,", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Seele ruht in Christus Hand,", "tokens": ["Die", "See\u00b7le", "ruht", "in", "Chris\u00b7tus", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In dessen Dienst er fiel.", "tokens": ["In", "des\u00b7sen", "Dienst", "er", "fiel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Er starb in des Erl\u00f6sers Land,", "tokens": ["Er", "starb", "in", "des", "Er\u00b7l\u00f6\u00b7sers", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und Sterben war ihm Spiel.", "tokens": ["Und", "Ster\u00b7ben", "war", "ihm", "Spiel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "PPER", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Drum lohne dich der Palmenkranz,", "tokens": ["Drum", "loh\u00b7ne", "dich", "der", "Pal\u00b7men\u00b7kranz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Jesus dir verhiess;", "tokens": ["Den", "Je\u00b7sus", "dir", "ver\u00b7hiess", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Drum tanze mit den Engeln Tanz", "tokens": ["Drum", "tan\u00b7ze", "mit", "den", "En\u00b7geln", "Tanz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In seinem Paradies.", "tokens": ["In", "sei\u00b7nem", "Pa\u00b7ra\u00b7dies", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Bald folget dir in Gottes Ruh", "tokens": ["Bald", "fol\u00b7get", "dir", "in", "Got\u00b7tes", "Ruh"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dein armes M\u00e4dchen nach,", "tokens": ["Dein", "ar\u00b7mes", "M\u00e4d\u00b7chen", "nach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und schlummert s\u00fcssen Schlaf, wie du,", "tokens": ["Und", "schlum\u00b7mert", "s\u00fcs\u00b7sen", "Schlaf", ",", "wie", "du", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJA", "NN", "$,", "PWAV", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Bis an den j\u00fcngsten Tag.", "tokens": ["Bis", "an", "den", "j\u00fcng\u00b7sten", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}