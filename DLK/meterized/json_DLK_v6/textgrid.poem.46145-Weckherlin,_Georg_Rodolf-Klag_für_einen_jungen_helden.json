{"textgrid.poem.46145": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Klag f\u00fcr einen jungen helden", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ade hofnung, freud und mut,", "tokens": ["A\u00b7de", "hof\u00b7nung", ",", "freud", "und", "mut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "VVFIN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "ade was uns kan erlaben:", "tokens": ["a\u00b7de", "was", "uns", "kan", "er\u00b7la\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "PPER", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "nichts dan leid, verzweiflung, wut,", "tokens": ["nichts", "dan", "leid", ",", "ver\u00b7zwei\u00b7flung", ",", "wut", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PIS", "ADV", "ADJD", "$,", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "nichts dan ungl\u00fcck will ich haben.", "tokens": ["nichts", "dan", "un\u00b7gl\u00fcck", "will", "ich", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "ADJD", "VMFIN", "PPER", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Einmal mein herz nur auf lieb,", "tokens": ["Ein\u00b7mal", "mein", "herz", "nur", "auf", "lieb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "ADV", "APPR", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "einmal nur auf krieg bestehet,", "tokens": ["ein\u00b7mal", "nur", "auf", "krieg", "be\u00b7ste\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.3": {"text": "lieb nu f\u00fcr mich ist stets tr\u00fcb,", "tokens": ["lieb", "nu", "f\u00fcr", "mich", "ist", "stets", "tr\u00fcb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "APPR", "PPER", "VAFIN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "leider der krieg nicht fortgehet.", "tokens": ["lei\u00b7der", "der", "krieg", "nicht", "fort\u00b7ge\u00b7het", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PTKNEG", "VVFIN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "Amor und Mars, was hab ich", "tokens": ["A\u00b7mor", "und", "Mars", ",", "was", "hab", "ich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "KON", "NN", "$,", "PWS", "VAFIN", "PPER"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "allein euch so vil geehret?", "tokens": ["al\u00b7lein", "euch", "so", "vil", "ge\u00b7eh\u00b7ret", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "da doch euer keiner mich", "tokens": ["da", "doch", "eu\u00b7er", "kei\u00b7ner", "mich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPOSAT", "PIS", "PRF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "dankbar seines solds gewehret.", "tokens": ["dank\u00b7bar", "sei\u00b7nes", "solds", "ge\u00b7weh\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Gl\u00fcck ohn ungl\u00fcck ist ohn ehr,", "tokens": ["Gl\u00fcck", "ohn", "un\u00b7gl\u00fcck", "ist", "ohn", "ehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VAFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "gl\u00fcck ohn ungl\u00fcck ist nicht tugend:", "tokens": ["gl\u00fcck", "ohn", "un\u00b7gl\u00fcck", "ist", "nicht", "tu\u00b7gend", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "VAFIN", "PTKNEG", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "also will ich auch nicht mehr", "tokens": ["al\u00b7so", "will", "ich", "auch", "nicht", "mehr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "PTKNEG", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "als nur dienst in meiner jugend.", "tokens": ["als", "nur", "dienst", "in", "mei\u00b7ner", "ju\u00b7gend", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Mit der lieb f\u00fchr ich den streit,", "tokens": ["Mit", "der", "lieb", "f\u00fchr", "ich", "den", "streit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "meiner treu solt ich genie\u00dfen,", "tokens": ["mei\u00b7ner", "treu", "solt", "ich", "ge\u00b7nie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "aber f\u00fcr ein solche beut,", "tokens": ["a\u00b7ber", "f\u00fcr", "ein", "sol\u00b7che", "beut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "ach, mu\u00df ich mein herz einb\u00fc\u00dfen!", "tokens": ["ach", ",", "mu\u00df", "ich", "mein", "herz", "ein\u00b7b\u00fc\u00b7\u00dfen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["XY", "$,", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Ist es dan der g\u00f6tter will,", "tokens": ["Ist", "es", "dan", "der", "g\u00f6t\u00b7ter", "will", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "ADJA", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "jemehr mir zu widerstreben:", "tokens": ["je\u00b7mehr", "mir", "zu", "wi\u00b7der\u00b7stre\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "rei\u00df aus mein herz, ich halt still,", "tokens": ["rei\u00df", "aus", "mein", "herz", ",", "ich", "halt", "still", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "rei\u00df aus, bleib ohn lieb, ohn leben!", "tokens": ["rei\u00df", "aus", ",", "bleib", "ohn", "lieb", ",", "ohn", "le\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "VVFIN", "APPR", "ADJD", "$,", "KOUI", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Aber was thut doch der schmerz,", "tokens": ["A\u00b7ber", "was", "thut", "doch", "der", "schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "ach, schatz, kom, mich zu entleiden!", "tokens": ["ach", ",", "schatz", ",", "kom", ",", "mich", "zu", "ent\u00b7lei\u00b7den", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["XY", "$,", "ADJD", "$,", "VVFIN", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "mein geist will nicht, und mein herz", "tokens": ["mein", "geist", "will", "nicht", ",", "und", "mein", "herz"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VMFIN", "PTKNEG", "$,", "KON", "PPOSAT", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Mag Aus Gunst Noch Ungern Scheiden.", "tokens": ["Mag", "Aus", "Gunst", "Noch", "Un\u00b7gern", "Schei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "NN", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}