{"textgrid.poem.43520": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Mein L\u00e4mmchen", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich hab' ein L\u00e4mmchen wei\u00df wie Schnee,", "tokens": ["Ich", "hab'", "ein", "L\u00e4mm\u00b7chen", "wei\u00df", "wie", "Schnee", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVFIN", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das geht auf gr\u00fcner Weide,", "tokens": ["Das", "geht", "auf", "gr\u00fc\u00b7ner", "Wei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das ist so fromm, das ist so gut,", "tokens": ["Das", "ist", "so", "fromm", ",", "das", "ist", "so", "gut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADJD", "$,", "PDS", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Thut Keinem was zu Leide.", "tokens": ["Thut", "Kei\u00b7nem", "was", "zu", "Lei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PRELS", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es suchet sich die Bl\u00fcmchen aus,", "tokens": ["Es", "su\u00b7chet", "sich", "die", "Bl\u00fcm\u00b7chen", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die gelben und die wei\u00dfen,", "tokens": ["Die", "gel\u00b7ben", "und", "die", "wei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KON", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Den Quendel und den Thymian,", "tokens": ["Den", "Quen\u00b7del", "und", "den", "Thy\u00b7mi\u00b7an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wie die Kr\u00e4uter hei\u00dfen.", "tokens": ["Und", "wie", "die", "Kr\u00e4u\u00b7ter", "hei\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und wenn's genug gefressen hat", "tokens": ["Und", "wenn's", "ge\u00b7nug", "ge\u00b7fres\u00b7sen", "hat"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ADV", "VVPP", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und will nicht weiter grasen,", "tokens": ["Und", "will", "nicht", "wei\u00b7ter", "gra\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So lagert's sich am Erlenstrauch", "tokens": ["So", "la\u00b7gert's", "sich", "am", "Er\u00b7len\u00b7strauch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wohl auf dem k\u00fchlen Rasen.", "tokens": ["Wohl", "auf", "dem", "k\u00fch\u00b7len", "Ra\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und wenn der Hirt nach Hause treibt,", "tokens": ["Und", "wenn", "der", "Hirt", "nach", "Hau\u00b7se", "treibt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kommt auch mein L\u00e4mmchen wieder,", "tokens": ["Kommt", "auch", "mein", "L\u00e4mm\u00b7chen", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dann h\u00fcpft es in den Stall hinein,", "tokens": ["Dann", "h\u00fcpft", "es", "in", "den", "Stall", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und bl\u00f6kt und legt sich nieder.", "tokens": ["Und", "bl\u00f6kt", "und", "legt", "sich", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Dem L\u00e4mmchen bin ich gar zu gut,", "tokens": ["Dem", "L\u00e4mm\u00b7chen", "bin", "ich", "gar", "zu", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem L\u00e4mmchen auf der Weide,", "tokens": ["Dem", "L\u00e4mm\u00b7chen", "auf", "der", "Wei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und wer ihm was zu Leide thut,", "tokens": ["Und", "wer", "ihm", "was", "zu", "Lei\u00b7de", "thut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "PRELS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Thut mir auch was zu Leide.", "tokens": ["Thut", "mir", "auch", "was", "zu", "Lei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PIS", "APPR", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}}}}