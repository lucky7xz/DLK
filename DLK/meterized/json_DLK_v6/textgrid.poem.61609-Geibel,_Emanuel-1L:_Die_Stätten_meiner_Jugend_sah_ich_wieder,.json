{"textgrid.poem.61609": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die St\u00e4tten meiner Jugend sah ich wieder,", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die St\u00e4tten meiner Jugend sah ich wieder,", "tokens": ["Die", "St\u00e4t\u00b7ten", "mei\u00b7ner", "Ju\u00b7gend", "sah", "ich", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch zeigen sie mir fast ein fremd Gesicht;", "tokens": ["Doch", "zei\u00b7gen", "sie", "mir", "fast", "ein", "fremd", "Ge\u00b7sicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "ADV", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Rings wuchsen Giebel, sanken Wipfel nieder,", "tokens": ["Rings", "wuch\u00b7sen", "Gie\u00b7bel", ",", "san\u00b7ken", "Wip\u00b7fel", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "$,", "VVFIN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und selbst das Flu\u00dfbett ist das alte nicht;", "tokens": ["Und", "selbst", "das", "Flu\u00df\u00b7bett", "ist", "das", "al\u00b7te", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VAFIN", "ART", "ADJA", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ja, Freund, den Hauch, der unterm Schlag der Glocken", "tokens": ["Ja", ",", "Freund", ",", "den", "Hauch", ",", "der", "un\u00b7term", "Schlag", "der", "Glo\u00b7cken"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "NN", "$,", "ART", "NN", "$,", "PRELS", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die Welt durchschauert, sp\u00fcr' ich doppelt hier;", "tokens": ["Die", "Welt", "durch\u00b7schau\u00b7ert", ",", "sp\u00fcr'", "ich", "dop\u00b7pelt", "hier", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "VVFIN", "PPER", "ADJD", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Er blies nicht blo\u00df das Braun aus unsern Locken,", "tokens": ["Er", "blies", "nicht", "blo\u00df", "das", "Braun", "aus", "un\u00b7sern", "Lo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Verwandelt ward die Zeit, und wir mit ihr.", "tokens": ["Ver\u00b7wan\u00b7delt", "ward", "die", "Zeit", ",", "und", "wir", "mit", "ihr", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$,", "KON", "PPER", "APPR", "PPOSAT", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wie lag im goldnen M\u00e4rchenduft die Ferne,", "tokens": ["Wie", "lag", "im", "gold\u00b7nen", "M\u00e4r\u00b7chen\u00b7duft", "die", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da uns noch eng der Heimat Bann umgab!", "tokens": ["Da", "uns", "noch", "eng", "der", "Hei\u00b7mat", "Bann", "um\u00b7gab", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Vom ersten Berg schon sahn wir andre Sterne,", "tokens": ["Vom", "ers\u00b7ten", "Berg", "schon", "sahn", "wir", "and\u00b7re", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADV", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Zaubergerte schien der Wanderstab.", "tokens": ["Und", "Zau\u00b7ber\u00b7ger\u00b7te", "schien", "der", "Wan\u00b7der\u00b7stab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Sehns\u00fcchtig wuchs das Herz, wenn seine Weisen", "tokens": ["Sehn\u00b7s\u00fcch\u00b7tig", "wuchs", "das", "Herz", ",", "wenn", "sei\u00b7ne", "Wei\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$,", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Das Posthorn sang im n\u00e4cht'gen Waldrevier \u2013", "tokens": ["Das", "Post\u00b7horn", "sang", "im", "n\u00e4cht'\u00b7gen", "Wald\u00b7re\u00b7vier", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Jetzt pfeift der Dampf und l\u00e4\u00dft im Sturm uns reisen;", "tokens": ["Jetzt", "pfeift", "der", "Dampf", "und", "l\u00e4\u00dft", "im", "Sturm", "uns", "rei\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "KON", "VVFIN", "APPRART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Verwandelt ward die Zeit, und wir mit ihr.", "tokens": ["Ver\u00b7wan\u00b7delt", "ward", "die", "Zeit", ",", "und", "wir", "mit", "ihr", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$,", "KON", "PPER", "APPR", "PPOSAT", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Von Ort zu Ort die traute Liebeskunde,", "tokens": ["Von", "Ort", "zu", "Ort", "die", "trau\u00b7te", "Lie\u00b7bes\u00b7kun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Gr\u00fc\u00dfe, die der Freund dem Freunde rief,", "tokens": ["Die", "Gr\u00fc\u00b7\u00dfe", ",", "die", "der", "Freund", "dem", "Freun\u00b7de", "rief", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie bang erharrten wir sie Stund' um Stunde,", "tokens": ["Wie", "bang", "er\u00b7harr\u00b7ten", "wir", "sie", "Stund'", "um", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "PPER", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und zum Ereignis ward der sp\u00e4te Brief.", "tokens": ["Und", "zum", "Er\u00b7eig\u00b7nis", "ward", "der", "sp\u00e4\u00b7te", "Brief", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Verhallend selbst, als Echo nur, empfingen", "tokens": ["Ver\u00b7hal\u00b7lend", "selbst", ",", "als", "E\u00b7cho", "nur", ",", "emp\u00b7fin\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["VVPP", "ADV", "$,", "KOUS", "NN", "ADV", "$,", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der Weltgeschichte Donnerbotschaft wir \u2013", "tokens": ["Der", "Welt\u00b7ge\u00b7schich\u00b7te", "Don\u00b7ner\u00b7bot\u00b7schaft", "wir", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PPER", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Jetzt tr\u00e4gt der Blitz das Wort auf Feuerschwingen,", "tokens": ["Jetzt", "tr\u00e4gt", "der", "Blitz", "das", "Wort", "auf", "Feu\u00b7er\u00b7schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Verwandelt ward die Zeit, und wir mit ihr.", "tokens": ["Ver\u00b7wan\u00b7delt", "ward", "die", "Zeit", ",", "und", "wir", "mit", "ihr", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$,", "KON", "PPER", "APPR", "PPOSAT", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Vom Zauberduft der blauen Blume trunken,", "tokens": ["Vom", "Zau\u00b7ber\u00b7duft", "der", "blau\u00b7en", "Blu\u00b7me", "trun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Herzens R\u00e4tseln sann der Dichter nach;", "tokens": ["Des", "Her\u00b7zens", "R\u00e4t\u00b7seln", "sann", "der", "Dich\u00b7ter", "nach", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Er klagt' um Sonnen, die hinabgesunken,", "tokens": ["Er", "klagt'", "um", "Son\u00b7nen", ",", "die", "hin\u00b7ab\u00b7ge\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,", "PRELS", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und rief der Vorwelt m\u00e4cht'ge Schatten wach.", "tokens": ["Und", "rief", "der", "Vor\u00b7welt", "m\u00e4cht'\u00b7ge", "Schat\u00b7ten", "wach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der Freiheit Muse schlich nur auf den Zehen", "tokens": ["Der", "Frei\u00b7heit", "Mu\u00b7se", "schlich", "nur", "auf", "den", "Ze\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "ADJD", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Bei Nacht zu ihm, als w\u00e4r's Verbrechen schier \u2013", "tokens": ["Bei", "Nacht", "zu", "ihm", ",", "als", "w\u00e4r's", "Ver\u00b7bre\u00b7chen", "schier", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "PPER", "$,", "KOKOM", "VAFIN", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Heut l\u00e4\u00dft sie auf dem Markt ihr Banner wehen,", "tokens": ["Heut", "l\u00e4\u00dft", "sie", "auf", "dem", "Markt", "ihr", "Ban\u00b7ner", "we\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Verwandelt ward die Zeit und wir mit ihr.", "tokens": ["Ver\u00b7wan\u00b7delt", "ward", "die", "Zeit", "und", "wir", "mit", "ihr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "KON", "PPER", "APPR", "PPOSAT", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Gru\u00df euch, ihr M\u00fcnster mit den hohen Schiffen,", "tokens": ["Gru\u00df", "euch", ",", "ihr", "M\u00fcns\u00b7ter", "mit", "den", "ho\u00b7hen", "Schif\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PPOSAT", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Gebraus der Orgel, dunkles Chorgest\u00fchl,", "tokens": ["Ge\u00b7braus", "der", "Or\u00b7gel", ",", "dunk\u00b7les", "Chor\u00b7ge\u00b7st\u00fchl", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wo ein Geheimnis, ewig unbegriffen,", "tokens": ["Wo", "ein", "Ge\u00b7heim\u00b7nis", ",", "e\u00b7wig", "un\u00b7be\u00b7grif\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Uns Wahrheit ward durch unser wahr Gef\u00fchl!", "tokens": ["Uns", "Wahr\u00b7heit", "ward", "durch", "un\u00b7ser", "wahr", "Ge\u00b7f\u00fchl", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VAFIN", "APPR", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Auf seinen Fl\u00fcgeln jedes Zweifels Schranke", "tokens": ["Auf", "sei\u00b7nen", "Fl\u00fc\u00b7geln", "je\u00b7des", "Zwei\u00b7fels", "Schran\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "PIAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Hoch \u00fcberfliegend, kampflos glaubten wir \u2013", "tokens": ["Hoch", "\u00fc\u00b7berf\u00b7lie\u00b7gend", ",", "kampf\u00b7los", "glaub\u00b7ten", "wir", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,", "ADJD", "VVFIN", "PPER", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Jetzt heischt sein Recht am Glauben der Gedanke;", "tokens": ["Jetzt", "heischt", "sein", "Recht", "am", "Glau\u00b7ben", "der", "Ge\u00b7dan\u00b7ke", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Verwandelt ward die Zeit, und wir mit ihr.", "tokens": ["Ver\u00b7wan\u00b7delt", "ward", "die", "Zeit", ",", "und", "wir", "mit", "ihr", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$,", "KON", "PPER", "APPR", "PPOSAT", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Wohl trugen wir das Vaterland im Herzen,", "tokens": ["Wohl", "tru\u00b7gen", "wir", "das", "Va\u00b7ter\u00b7land", "im", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch liebten wir wie Knaben, stumm und zart;", "tokens": ["Doch", "lieb\u00b7ten", "wir", "wie", "Kna\u00b7ben", ",", "stumm", "und", "zart", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "KOKOM", "NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zum Freund nur sprach der Freund von seinen Schmerzen", "tokens": ["Zum", "Freund", "nur", "sprach", "der", "Freund", "von", "sei\u00b7nen", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und von dem Kaiser mit dem Flammenbart.", "tokens": ["Und", "von", "dem", "Kai\u00b7ser", "mit", "dem", "Flam\u00b7men\u00b7bart", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Das Wort vom Reich, ob niemals ganz verklungen,", "tokens": ["Das", "Wort", "vom", "Reich", ",", "ob", "nie\u00b7mals", "ganz", "ver\u00b7klun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,", "KOUS", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Doch scheu nur ward's gefl\u00fcstert dort und hier \u2013", "tokens": ["Doch", "scheu", "nur", "ward's", "ge\u00b7fl\u00fcs\u00b7tert", "dort", "und", "hier", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["K