{"textgrid.poem.28124": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "39.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Krieg und Hunger, Kriegs Geno\u00df,", "tokens": ["Krieg", "und", "Hun\u00b7ger", ",", "Kriegs", "Ge\u00b7no\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sind zwey ungezogne Br\u00fcder,", "tokens": ["Sind", "zwey", "un\u00b7ge\u00b7zog\u00b7ne", "Br\u00fc\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "CARD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die durch ihres Fusses Sto\u00df", "tokens": ["Die", "durch", "ih\u00b7res", "Fus\u00b7ses", "Sto\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Treten, was nur stehet, nieder.", "tokens": ["Tre\u00b7ten", ",", "was", "nur", "ste\u00b7het", ",", "nie\u00b7der", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADV", "VVFIN", "$,", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Jener f\u00fchret diesen an;", "tokens": ["Je\u00b7ner", "f\u00fch\u00b7ret", "die\u00b7sen", "an", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "VVFIN", "PDS", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wann mit morden, rauben, brennen,", "tokens": ["Wann", "mit", "mor\u00b7den", ",", "rau\u00b7ben", ",", "bren\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWAV", "APPR", "VVINF", "$,", "VVINF", "$,", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Iener hat genug gethan,", "tokens": ["Ie\u00b7ner", "hat", "ge\u00b7nug", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Lernt man diesen recht erst kennen;", "tokens": ["Lernt", "man", "die\u00b7sen", "recht", "erst", "ken\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PDAT", "ADJD", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Dann er ist so rasend k\u00fchn,", "tokens": ["Dann", "er", "ist", "so", "ra\u00b7send", "k\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "ADV", "ADJD", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "So ergrimmet und vermessen,", "tokens": ["So", "er\u00b7grim\u00b7met", "und", "ver\u00b7mes\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Da\u00df er, wann sonst alles hin,", "tokens": ["Da\u00df", "er", ",", "wann", "sonst", "al\u00b7les", "hin", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PWAV", "ADV", "PIS", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Auch den Bruder pflegt zu fressen.", "tokens": ["Auch", "den", "Bru\u00b7der", "pflegt", "zu", "fres\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}