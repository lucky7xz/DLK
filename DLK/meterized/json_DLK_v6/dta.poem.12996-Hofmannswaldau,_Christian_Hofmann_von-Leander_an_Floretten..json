{"dta.poem.12996": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Leander an Floretten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich schreibe nicht zu viel, prei\u00dfw\u00fcrdige Florette!", "tokens": ["Ich", "schrei\u00b7be", "nicht", "zu", "viel", ",", "prei\u00df\u00b7w\u00fcr\u00b7di\u00b7ge", "Flo\u00b7ret\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PTKA", "PIS", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dein auserlesner brief f\u00e4llt meiner feder bey.", "tokens": ["Dein", "au\u00b7ser\u00b7les\u00b7ner", "brief", "f\u00e4llt", "mei\u00b7ner", "fe\u00b7der", "bey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Apollo selber spricht: da\u00df nichts so sch\u00f6n und nette", "tokens": ["A\u00b7pol\u00b7lo", "sel\u00b7ber", "spricht", ":", "da\u00df", "nichts", "so", "sch\u00f6n", "und", "net\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "VVFIN", "$.", "KOUS", "PIS", "ADV", "ADJD", "KON", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als dein beliebter vers, o andre Sapho! sey.", "tokens": ["Als", "dein", "be\u00b7lieb\u00b7ter", "vers", ",", "o", "and\u00b7re", "Sa\u00b7pho", "!", "sey", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "$,", "FM", "ADJA", "NN", "$.", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und darum darffst du dich des dichtens gar nicht sch\u00e4men.", "tokens": ["Und", "da\u00b7rum", "darffst", "du", "dich", "des", "dich\u00b7tens", "gar", "nicht", "sch\u00e4\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VVFIN", "PPER", "PRF", "ART", "ADV", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die sonne sch\u00e4met sich der goldnen strahlen nicht.", "tokens": ["Die", "son\u00b7ne", "sch\u00e4\u00b7met", "sich", "der", "gold\u00b7nen", "strah\u00b7len", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ART", "ADJA", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ich aber m\u00f6chte wol vom Pindus abschied nehmen,", "tokens": ["Ich", "a\u00b7ber", "m\u00f6ch\u00b7te", "wol", "vom", "Pin\u00b7dus", "ab\u00b7schied", "neh\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "ADV", "APPRART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Weil meine schlechte faust doch keine lorbeer bricht.", "tokens": ["Weil", "mei\u00b7ne", "schlech\u00b7te", "faust", "doch", "kei\u00b7ne", "lor\u00b7beer", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wenn auch ein adler mir schon seine federn schenckte,", "tokens": ["Wenn", "auch", "ein", "ad\u00b7ler", "mir", "schon", "sei\u00b7ne", "fe\u00b7dern", "schenck\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "PPER", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "So w\u00fcst\u2019 ich dennoch nicht, wie man sie f\u00fchren soll,", "tokens": ["So", "w\u00fcst'", "ich", "den\u00b7noch", "nicht", ",", "wie", "man", "sie", "f\u00fch\u00b7ren", "soll", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKNEG", "$,", "PWAV", "PIS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Weil sie kein hoher geist, als wie der deine, lenckte.", "tokens": ["Weil", "sie", "kein", "ho\u00b7her", "geist", ",", "als", "wie", "der", "dei\u00b7ne", ",", "lenck\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "ADJA", "NN", "$,", "KOUS", "KOKOM", "ART", "PPOSAT", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Allein Florette schreibt mit ieder feder wohl.", "tokens": ["Al\u00b7lein", "Flo\u00b7ret\u00b7te", "schreibt", "mit", "ie\u00b7der", "fe\u00b7der", "wohl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "APPR", "PIAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Drum ist es nichts als schertz, wenn deine Muse klaget;", "tokens": ["Drum", "ist", "es", "nichts", "als", "schertz", ",", "wenn", "dei\u00b7ne", "Mu\u00b7se", "kla\u00b7get", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "PIS", "KOKOM", "ADJD", "$,", "KOUS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Da\u00df ihr Leander nicht die hand im dichten f\u00fchrt.", "tokens": ["Da\u00df", "ihr", "Le\u00b7an\u00b7der", "nicht", "die", "hand", "im", "dich\u00b7ten", "f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PTKNEG", "ART", "NN", "APPRART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Denn was sie von der krafft der s\u00fc\u00dfen worte saget,", "tokens": ["Denn", "was", "sie", "von", "der", "krafft", "der", "s\u00fc\u00b7\u00dfen", "wor\u00b7te", "sa\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Ist, glaub\u2019 es nur, ein lob, so dir allein geb\u00fchrt.", "tokens": ["Ist", ",", "glaub'", "es", "nur", ",", "ein", "lob", ",", "so", "dir", "al\u00b7lein", "ge\u00b7b\u00fchrt", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "VVFIN", "PPER", "ADV", "$,", "ART", "NN", "$,", "ADV", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ich wei\u00df die steine nicht, wie Orpheus, zu bewegen,", "tokens": ["Ich", "wei\u00df", "die", "stei\u00b7ne", "nicht", ",", "wie", "Or\u00b7pheus", ",", "zu", "be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "PTKNEG", "$,", "PWAV", "NE", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Man sieht sie noch, wie vor, an ihrer stelle stehn.", "tokens": ["Man", "sieht", "sie", "noch", ",", "wie", "vor", ",", "an", "ih\u00b7rer", "stel\u00b7le", "stehn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "$,", "PWAV", "PTKVZ", "$,", "APPR", "PPOSAT", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Und k\u00f6nten sie sich auch, bey meinem singen, regen,", "tokens": ["Und", "k\u00f6n\u00b7ten", "sie", "sich", "auch", ",", "bey", "mei\u00b7nem", "sin\u00b7gen", ",", "re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PRF", "ADV", "$,", "APPR", "PPOSAT", "VVFIN", "$,", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Sie w\u00fcrden vor verdru\u00df mir aus dem wege gehn.", "tokens": ["Sie", "w\u00fcr\u00b7den", "vor", "ver\u00b7dru\u00df", "mir", "aus", "dem", "we\u00b7ge", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Indessen wilst du doch, da\u00df ich noch ferner dichte:", "tokens": ["In\u00b7des\u00b7sen", "wilst", "du", "doch", ",", "da\u00df", "ich", "noch", "fer\u00b7ner", "dich\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Und weil du so befiehlst, so dicht ich, wie ich kan.", "tokens": ["Und", "weil", "du", "so", "be\u00b7fiehlst", ",", "so", "dicht", "ich", ",", "wie", "ich", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VVFIN", "$,", "ADV", "ADJD", "PPER", "$,", "PWAV", "PPER", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Denn sch\u00e4mt sich meine nacht schon vor dem hellen lichte,", "tokens": ["Denn", "sch\u00e4mt", "sich", "mei\u00b7ne", "nacht", "schon", "vor", "dem", "hel\u00b7len", "lich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "PPOSAT", "NN", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "So steht mir dennoch auch kein ungehorsam an.", "tokens": ["So", "steht", "mir", "den\u00b7noch", "auch", "kein", "un\u00b7ge\u00b7hor\u00b7sam", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "PIAT", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Wiewol ich kan in dem dir nicht gehorsam leben,", "tokens": ["Wie\u00b7wol", "ich", "kan", "in", "dem", "dir", "nicht", "ge\u00b7hor\u00b7sam", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "APPR", "PRELS", "PPER", "PTKNEG", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Was deines briefes schlu\u00df von meiner hand begehrt.", "tokens": ["Was", "dei\u00b7nes", "brie\u00b7fes", "schlu\u00df", "von", "mei\u00b7ner", "hand", "be\u00b7gehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Denn deine schreiben sind nur w\u00fcrdig aufzuheben,", "tokens": ["Denn", "dei\u00b7ne", "schrei\u00b7ben", "sind", "nur", "w\u00fcr\u00b7dig", "auf\u00b7zu\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "VAFIN", "ADV", "ADJD", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Leanders aber nichts als heisser flammen werth.", "tokens": ["Le\u00b7an\u00b7ders", "a\u00b7ber", "nichts", "als", "heis\u00b7ser", "flam\u00b7men", "werth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PIS", "KOKOM", "ADJD", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}