{"textgrid.poem.37114": {"metadata": {"author": {"name": "Boie, Heinrich Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Des Morgens wache K\u00f6nigin", "genre": "verse", "period": "N.A.", "pub_year": 1764, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Des Morgens wache K\u00f6nigin", "tokens": ["Des", "Mor\u00b7gens", "wa\u00b7che", "K\u00f6\u00b7ni\u00b7gin"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "H\u00f6rt schon mein fr\u00fches Lied;", "tokens": ["H\u00f6rt", "schon", "mein", "fr\u00fc\u00b7hes", "Lied", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie wei\u00df wie liebevoll ich bin", "tokens": ["Sie", "wei\u00df", "wie", "lie\u00b7be\u00b7voll", "ich", "bin"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOKOM", "ADJD", "PPER", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wie die Spr\u00f6de flieht.", "tokens": ["Und", "wie", "die", "Spr\u00f6\u00b7de", "flieht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und Phoebus von dem ersten Strahl", "tokens": ["Und", "Phoe\u00b7bus", "von", "dem", "ers\u00b7ten", "Strahl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NE", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bis er zur Ruhe geht", "tokens": ["Bis", "er", "zur", "Ru\u00b7he", "geht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sieht nichts als Thr\u00e4nen ohne Zahl,", "tokens": ["Sieht", "nichts", "als", "Thr\u00e4\u00b7nen", "oh\u00b7ne", "Zahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "KOKOM", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die doch ihr Stolz verschm\u00e4ht.", "tokens": ["Die", "doch", "ihr", "Stolz", "ver\u00b7schm\u00e4ht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "O! werd ich keinen Morgen sehn,", "tokens": ["O", "!", "werd", "ich", "kei\u00b7nen", "Mor\u00b7gen", "sehn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VAFIN", "PPER", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der mir gequ\u00e4ltem lacht?", "tokens": ["Der", "mir", "ge\u00b7qu\u00e4l\u00b7tem", "lacht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJA", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ist mir kein Tag, kein Abend sch\u00f6n,", "tokens": ["Ist", "mir", "kein", "Tag", ",", "kein", "A\u00b7bend", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PIAT", "NN", "$,", "PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und heiter keine Nacht?", "tokens": ["Und", "hei\u00b7ter", "kei\u00b7ne", "Nacht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Es ruft in meinen Trauerklang", "tokens": ["Es", "ruft", "in", "mei\u00b7nen", "Trau\u00b7er\u00b7klang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Wiederhall betr\u00fcbt.", "tokens": ["Der", "Wie\u00b7der\u00b7hall", "be\u00b7tr\u00fcbt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wann sagt mein freudiger Gesang,", "tokens": ["Wann", "sagt", "mein", "freu\u00b7di\u00b7ger", "Ge\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df Lalage mich liebt?", "tokens": ["Da\u00df", "La\u00b7la\u00b7ge", "mich", "liebt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}