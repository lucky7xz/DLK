{"textgrid.poem.42005": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Still l\u00f6ste sie, die G\u00f6ttin meiner Lieder,", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.85", "af:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Still l\u00f6ste sie, die G\u00f6ttin meiner Lieder,", "tokens": ["Still", "l\u00f6s\u00b7te", "sie", ",", "die", "G\u00f6t\u00b7tin", "mei\u00b7ner", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "$,", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Fessel, die das weiche Haar umschlang,", "tokens": ["Die", "Fes\u00b7sel", ",", "die", "das", "wei\u00b7che", "Haar", "um\u00b7schlang", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und sieh, der Locken seid'ne F\u00fclle sank", "tokens": ["Und", "sieh", ",", "der", "Lo\u00b7cken", "sei\u00b7d'\u00b7ne", "F\u00fcl\u00b7le", "sank"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "ART", "NN", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "In leichtem Tanz auf Hals und Busen nieder.", "tokens": ["In", "leich\u00b7tem", "Tanz", "auf", "Hals", "und", "Bu\u00b7sen", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und lodernd hob die Sehnsucht ihr Gefieder", "tokens": ["Und", "lo\u00b7dernd", "hob", "die", "Sehn\u00b7sucht", "ihr", "Ge\u00b7fie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und regte sich im Innern hei\u00df und bang;", "tokens": ["Und", "reg\u00b7te", "sich", "im", "In\u00b7nern", "hei\u00df", "und", "bang", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPRART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Schon folgt' ich k\u00fchn des Herzens s\u00fc\u00dfem Drang:", "tokens": ["Schon", "folgt'", "ich", "k\u00fchn", "des", "Her\u00b7zens", "s\u00fc\u00b7\u00dfem", "Drang", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da fa\u00dfte schnell mich leises Zagen wieder.", "tokens": ["Da", "fa\u00df\u00b7te", "schnell", "mich", "lei\u00b7ses", "Za\u00b7gen", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "PPER", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ein Heiligthum ward Mund und Busen mir,", "tokens": ["Ein", "Hei\u00b7lig\u00b7thum", "ward", "Mund", "und", "Bu\u00b7sen", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "KON", "NN", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und um sie her schien den geweihten Schleier", "tokens": ["Und", "um", "sie", "her", "schien", "den", "ge\u00b7weih\u00b7ten", "Schlei\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "APZR", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Geheimni\u00dfvoll der Locken Fluth zu weben,", "tokens": ["Ge\u00b7heim\u00b7ni\u00df\u00b7voll", "der", "Lo\u00b7cken", "Fluth", "zu", "we\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und zagend schwieg im Herzen die Begier;", "tokens": ["Und", "za\u00b7gend", "schwieg", "im", "Her\u00b7zen", "die", "Be\u00b7gier", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mein Geist versank in stiller Andacht Feier", "tokens": ["Mein", "Geist", "ver\u00b7sank", "in", "stil\u00b7ler", "An\u00b7dacht", "Fei\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und sah Madonna l\u00e4chelnd vor sich schweben.", "tokens": ["Und", "sah", "Ma\u00b7don\u00b7na", "l\u00e4\u00b7chelnd", "vor", "sich", "schwe\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NE", "ADJD", "APPR", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}