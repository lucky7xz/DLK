{"textgrid.poem.37310": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn die Tante Adelheide", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn die Tante Adelheide", "tokens": ["Wenn", "die", "Tan\u00b7te", "A\u00b7del\u00b7hei\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Als Logierbesuch erschien,", "tokens": ["Als", "Lo\u00b7gier\u00b7be\u00b7such", "er\u00b7schien", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "F\u00fchlte Fritzchen gro\u00dfe Freude,", "tokens": ["F\u00fchl\u00b7te", "Fritz\u00b7chen", "gro\u00b7\u00dfe", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn dann gab es was f\u00fcr ihn.", "tokens": ["Denn", "dann", "gab", "es", "was", "f\u00fcr", "ihn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PIS", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Immer hat die liebe Gute", "tokens": ["Im\u00b7mer", "hat", "die", "lie\u00b7be", "Gu\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tief im Reisekorb versteckt", "tokens": ["Tief", "im", "Rei\u00b7se\u00b7korb", "ver\u00b7steckt"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPRART", "NN", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Eine angenehme Tute,", "tokens": ["Ei\u00b7ne", "an\u00b7ge\u00b7neh\u00b7me", "Tu\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deren Inhalt k\u00f6stlich schmeckt.", "tokens": ["De\u00b7ren", "In\u00b7halt", "k\u00f6st\u00b7lich", "schmeckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "T\u00e4glich wird dem braven Knaben", "tokens": ["T\u00e4g\u00b7lich", "wird", "dem", "bra\u00b7ven", "Kna\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Draus ein h\u00fcbsches St\u00fcck beschert,", "tokens": ["Draus", "ein", "h\u00fcb\u00b7sches", "St\u00fcck", "be\u00b7schert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bis wir schlie\u00dflich nichts mehr haben", "tokens": ["Bis", "wir", "schlie\u00df\u00b7lich", "nichts", "mehr", "ha\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "PIS", "ADV", "VAFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die Tante weiterf\u00e4hrt.", "tokens": ["Und", "die", "Tan\u00b7te", "wei\u00b7ter\u00b7f\u00e4hrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Mit der Post fuhr sie von hinnen.", "tokens": ["Mit", "der", "Post", "fuhr", "sie", "von", "hin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "APPR", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fritzchens Trauer ist nur schwach.", "tokens": ["Fritz\u00b7chens", "Trau\u00b7er", "ist", "nur", "schwach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Einer Tute, wo nichts drinnen,", "tokens": ["Ei\u00b7ner", "Tu\u00b7te", ",", "wo", "nichts", "drin\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PIS", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weint man keine Tr\u00e4ne nach.", "tokens": ["Weint", "man", "kei\u00b7ne", "Tr\u00e4\u00b7ne", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PIAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}