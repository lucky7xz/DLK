{"dta.poem.11551": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Lied vom Winde.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Sausewind! Brausewind!", "tokens": ["Sau\u00b7se\u00b7wind", "!", "Brau\u00b7se\u00b7wind", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Dort und hier,", "tokens": ["Dort", "und", "hier", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "KON", "ADV", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Deine Heimath sage mir!", "tokens": ["Dei\u00b7ne", "Hei\u00b7math", "sa\u00b7ge", "mir", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "\u201ekindlein, wir fahren", "tokens": ["\u201e", "kin\u00b7dlein", ",", "wir", "fah\u00b7ren"], "token_info": ["punct", "word", "punct", "word", "word"], "pos": ["$(", "PTKVZ", "$,", "PPER", "VVINF"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Seit viel vielen Jahren", "tokens": ["Seit", "viel", "vie\u00b7len", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADV", "PIAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Durch die weit weite Welt,", "tokens": ["Durch", "die", "weit", "wei\u00b7te", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Und m\u00f6chten's erfragen,", "tokens": ["Und", "m\u00f6ch\u00b7ten's", "er\u00b7fra\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Die Antwort erjagen,", "tokens": ["Die", "Ant\u00b7wort", "er\u00b7ja\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Bei den Bergen, den Meeren,", "tokens": ["Bei", "den", "Ber\u00b7gen", ",", "den", "Mee\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.7": {"text": "Bei des Himmels klingenden Heeren,", "tokens": ["Bei", "des", "Him\u00b7mels", "klin\u00b7gen\u00b7den", "Hee\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Die wissen es nie.", "tokens": ["Die", "wis\u00b7sen", "es", "nie", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Bist du kl\u00fcger als sie,", "tokens": ["Bist", "du", "kl\u00fc\u00b7ger", "als", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "KOKOM", "PPER", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.10": {"text": "Magst du es sagen.", "tokens": ["Magst", "du", "es", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "\u2014 Fort, wohlauf!", "tokens": ["Fort", ",", "wohl\u00b7auf", "!"], "token_info": ["punct", "word", "punct", "word", "punct"], "pos": ["$(", "NN", "$,", "PWAV", "$."], "meter": "-+", "measure": "iambic.single"}, "line.12": {"text": "Halt' uns nicht auf!", "tokens": ["Halt'", "uns", "nicht", "auf", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "PTKVZ", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.13": {"text": "Kommen Andre nach, unsre Br\u00fcder,", "tokens": ["Kom\u00b7men", "And\u00b7re", "nach", ",", "uns\u00b7re", "Br\u00fc\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PIS", "PTKVZ", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.14": {"text": "Da frag' wieder.\u201c", "tokens": ["Da", "frag'", "wie\u00b7der", ".", "\u201c"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$.", "$("], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Halt an! Gemach,", "tokens": ["Halt", "an", "!", "Ge\u00b7mach", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$.", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Eine kleine Frist!", "tokens": ["Ei\u00b7ne", "klei\u00b7ne", "Frist", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Sagt, wo der Liebe Heimath ist,", "tokens": ["Sagt", ",", "wo", "der", "Lie\u00b7be", "Hei\u00b7math", "ist", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "ART", "NN", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr Anfang, ihr Ende?", "tokens": ["Ihr", "An\u00b7fang", ",", "ihr", "En\u00b7de", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "\u201ewer's nennen k\u00f6nnte!", "tokens": ["\u201e", "wer's", "nen\u00b7nen", "k\u00f6nn\u00b7te", "!"], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "VVINF", "VMFIN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Schelmisches Kind,", "tokens": ["Schel\u00b7mi\u00b7sches", "Kind", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Lieb ist wie Wind,", "tokens": ["Lieb", "ist", "wie", "Wind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "KOKOM", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Rasch und lebendig,", "tokens": ["Rasch", "und", "le\u00b7ben\u00b7dig", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$,"], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.5": {"text": "Ruhet nie,", "tokens": ["Ru\u00b7het", "nie", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Ewig ist sie,", "tokens": ["E\u00b7wig", "ist", "sie", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Aber dein Schatz nicht best\u00e4ndig.", "tokens": ["A\u00b7ber", "dein", "Schatz", "nicht", "be\u00b7st\u00e4n\u00b7dig", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PTKNEG", "ADJD", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.8": {"text": "\u2014 Frisch, wohlauf!", "tokens": ["Frisch", ",", "wohl\u00b7auf", "!"], "token_info": ["punct", "word", "punct", "word", "punct"], "pos": ["$(", "NE", "$,", "PWAV", "$."], "meter": "-+", "measure": "iambic.single"}, "line.9": {"text": "Halt uns nicht auf!", "tokens": ["Halt", "uns", "nicht", "auf", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "PTKVZ", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.10": {"text": "Fort \u00fcber Stoppel und W\u00e4lder und Wiesen!", "tokens": ["Fort", "\u00fc\u00b7ber", "Stop\u00b7pel", "und", "W\u00e4l\u00b7der", "und", "Wie\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.11": {"text": "Wenn ich dein Sch\u00e4tzchen seh',", "tokens": ["Wenn", "ich", "dein", "Sch\u00e4tz\u00b7chen", "seh'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Will ich es gr\u00fc\u00dfen;", "tokens": ["Will", "ich", "es", "gr\u00fc\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.13": {"text": "Kindlein \u2014 Ade!", "tokens": ["Kin\u00b7dlein", "A\u00b7de", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$(", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}