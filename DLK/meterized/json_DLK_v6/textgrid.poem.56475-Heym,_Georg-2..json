{"textgrid.poem.56475": {"metadata": {"author": {"name": "Heym, Georg", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1899, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dich gr\u00fc\u00dft der Dichter, d\u00fcsteres Phantom,", "tokens": ["Dich", "gr\u00fc\u00dft", "der", "Dich\u00b7ter", ",", "d\u00fcs\u00b7te\u00b7res", "Phan\u00b7tom", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Den durch die Nacht der Liebe Schatten f\u00fchrt,", "tokens": ["Den", "durch", "die", "Nacht", "der", "Lie\u00b7be", "Schat\u00b7ten", "f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Im unterirdisch ungeheuern Dom,", "tokens": ["Im", "un\u00b7ter\u00b7ir\u00b7disch", "un\u00b7ge\u00b7heu\u00b7ern", "Dom", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wo schwarzer Sturm die Kirchenlampe sch\u00fcrt,", "tokens": ["Wo", "schwar\u00b7zer", "Sturm", "die", "Kir\u00b7chen\u00b7lam\u00b7pe", "sch\u00fcrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die lautlos flackert, ein zerst\u00f6rtes Herz,", "tokens": ["Die", "laut\u00b7los", "fla\u00b7ckert", ",", "ein", "zer\u00b7st\u00f6r\u00b7tes", "Herz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Von Qual durchl\u00f6chert, und die Trauer krankt", "tokens": ["Von", "Qual", "durch\u00b7l\u00f6\u00b7chert", ",", "und", "die", "Trau\u00b7er", "krankt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVPP", "$,", "KON", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Im Tode noch in seinem schwarzen Erz.", "tokens": ["Im", "To\u00b7de", "noch", "in", "sei\u00b7nem", "schwar\u00b7zen", "Erz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "An langen Ketten zittert es und schwankt.", "tokens": ["An", "lan\u00b7gen", "Ket\u00b7ten", "zit\u00b7tert", "es", "und", "schwankt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Sein roter Schein flammt \u00fcber Gr\u00e4ber hin.", "tokens": ["Sein", "ro\u00b7ter", "Schein", "flammt", "\u00fc\u00b7ber", "Gr\u00e4\u00b7ber", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "An dem Altare kniet ein Ministrant,", "tokens": ["An", "dem", "Al\u00b7ta\u00b7re", "kniet", "ein", "Mi\u00b7nis\u00b7trant", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zwei Dolche in der offnen Brust. Darin", "tokens": ["Zwei", "Dol\u00b7che", "in", "der", "off\u00b7nen", "Brust", ".", "Da\u00b7rin"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["CARD", "NN", "APPR", "ART", "ADJA", "NN", "$.", "PAV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Noch schwelt und steigt trostloser Liebe Brand.", "tokens": ["Noch", "schwelt", "und", "steigt", "trost\u00b7lo\u00b7ser", "Lie\u00b7be", "Brand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Durch schwarze Stollen flattert das Gespenst.", "tokens": ["Durch", "schwar\u00b7ze", "Stol\u00b7len", "flat\u00b7tert", "das", "Ge\u00b7spenst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Er folgt ihm blind, wo schwarze Schatten fliehn,", "tokens": ["Er", "folgt", "ihm", "blind", ",", "wo", "schwar\u00b7ze", "Schat\u00b7ten", "fliehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$,", "PWAV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Den Mond an seiner Stirn, der tr\u00fcbe gl\u00e4nzt,", "tokens": ["Den", "Mond", "an", "sei\u00b7ner", "Stirn", ",", "der", "tr\u00fc\u00b7be", "gl\u00e4nzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und Stimmen h\u00f6rt er, die vor\u00fcberziehn", "tokens": ["Und", "Stim\u00b7men", "h\u00f6rt", "er", ",", "die", "vor\u00b7\u00fc\u00b7ber\u00b7ziehn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "NN", "VVFIN", "PPER", "$,", "PRELS", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Im hohlen Grund, der von den Qualen schwillt,", "tokens": ["Im", "hoh\u00b7len", "Grund", ",", "der", "von", "den", "Qua\u00b7len", "schwillt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit dumpfem Laut. Ein ferner Wasserfall", "tokens": ["Mit", "dum\u00b7pfem", "Laut", ".", "Ein", "fer\u00b7ner", "Was\u00b7ser\u00b7fall"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Pocht an der Wand, und bittre Trauer f\u00fcllt", "tokens": ["Pocht", "an", "der", "Wand", ",", "und", "bitt\u00b7re", "Trau\u00b7er", "f\u00fcllt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "$,", "KON", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wie ein Orkan der langen Treppen Fall.", "tokens": ["Wie", "ein", "Or\u00b7kan", "der", "lan\u00b7gen", "Trep\u00b7pen", "Fall", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "ADJA", "NN", "NN", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}}, "stanza.6": {"line.1": {"text": "Fern kommt ein Zug von Fackeln durch ein Tor,", "tokens": ["Fern", "kommt", "ein", "Zug", "von", "Fa\u00b7ckeln", "durch", "ein", "Tor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "APPR", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein Sarg, der auf der Tr\u00e4ger Schultern bebt", "tokens": ["Ein", "Sarg", ",", "der", "auf", "der", "Tr\u00e4\u00b7ger", "Schul\u00b7tern", "bebt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und langsam durch den langen Korridor", "tokens": ["Und", "lang\u00b7sam", "durch", "den", "lan\u00b7gen", "Kor\u00b7ri\u00b7dor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In trauriger Musik vor\u00fcberschwebt.", "tokens": ["In", "trau\u00b7ri\u00b7ger", "Mu\u00b7sik", "vor\u00b7\u00fc\u00b7bersc\u00b7hwebt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Wer ruht darin? Wer starb? Der matte Ton", "tokens": ["Wer", "ruht", "da\u00b7rin", "?", "Wer", "starb", "?", "Der", "mat\u00b7te", "Ton"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PAV", "$.", "PWS", "VVFIN", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Fl\u00f6ten wandert durch die G\u00e4nge fort.", "tokens": ["Der", "Fl\u00f6\u00b7ten", "wan\u00b7dert", "durch", "die", "G\u00e4n\u00b7ge", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein dunkles Echo ruft er noch, wo schon", "tokens": ["Ein", "dunk\u00b7les", "E\u00b7cho", "ruft", "er", "noch", ",", "wo", "schon"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADV", "$,", "PWAV", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Stille hockt an dem versunknen Ort.", "tokens": ["Die", "Stil\u00b7le", "hockt", "an", "dem", "ver\u00b7sun\u00b7knen", "Ort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Das Grau der Mitternacht wird kaum bedeckt", "tokens": ["Das", "Grau", "der", "Mit\u00b7ter\u00b7nacht", "wird", "kaum", "be\u00b7deckt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "ADV", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Von einer gelben Kerze, und es saust", "tokens": ["Von", "ei\u00b7ner", "gel\u00b7ben", "Ker\u00b7ze", ",", "und", "es", "saust"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "KON", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Wind die G\u00e4nge fort, der bellend schreckt", "tokens": ["Der", "Wind", "die", "G\u00e4n\u00b7ge", "fort", ",", "der", "bel\u00b7lend", "schreckt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "PTKVZ", "$,", "PRELS", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Den Staub der Gr\u00fcfte auf, der unten haust.", "tokens": ["Den", "Staub", "der", "Gr\u00fcf\u00b7te", "auf", ",", "der", "un\u00b7ten", "haust", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKVZ", "$,", "PRELS", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Ma\u00dflose Traurigkeit. In Nacht allein", "tokens": ["Ma\u00df\u00b7lo\u00b7se", "Trau\u00b7rig\u00b7keit", ".", "In", "Nacht", "al\u00b7lein"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "APPR", "NN", "ADV"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Verirrt der Wandrer durch den hohen Flur,", "tokens": ["Ver\u00b7irrt", "der", "Wand\u00b7rer", "durch", "den", "ho\u00b7hen", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wo oben in der dunklen W\u00f6lbung Stein", "tokens": ["Wo", "o\u00b7ben", "in", "der", "dunk\u00b7len", "W\u00f6l\u00b7bung", "Stein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "APPR", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Gestirne fliehn in magischer Figur.", "tokens": ["Ge\u00b7stir\u00b7ne", "fliehn", "in", "ma\u00b7gi\u00b7scher", "Fi\u00b7gur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}