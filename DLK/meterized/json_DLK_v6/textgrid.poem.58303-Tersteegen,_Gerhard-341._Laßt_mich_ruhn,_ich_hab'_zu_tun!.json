{"textgrid.poem.58303": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "341. La\u00dft mich ruhn, ich hab' zu tun!", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was geht mich's an, was Welt und andre Menschen tun,", "tokens": ["Was", "geht", "mich's", "an", ",", "was", "Welt", "und", "and\u00b7re", "Men\u00b7schen", "tun", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PIS", "PTKVZ", "$,", "PRELS", "NN", "KON", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da ich im Grund mit Gott kann im Verborgnen ruhn!", "tokens": ["Da", "ich", "im", "Grund", "mit", "Gott", "kann", "im", "Ver\u00b7borg\u00b7nen", "ruhn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "APPR", "NN", "VMFIN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mein neu' und altes Werk ist, da\u00df ich stets im Geist", "tokens": ["Mein", "neu'", "und", "al\u00b7tes", "Werk", "ist", ",", "da\u00df", "ich", "stets", "im", "Geist"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "KON", "ADJA", "NN", "VAFIN", "$,", "KOUS", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der h\u00f6chsten Majest\u00e4t, Gott selbst, Gesellschaft leist'.", "tokens": ["Der", "h\u00f6chs\u00b7ten", "Ma\u00b7jes\u00b7t\u00e4t", ",", "Gott", "selbst", ",", "Ge\u00b7sell\u00b7schaft", "leist'", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "NN", "ADV", "$,", "NN", "VVFIN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}}}}