{"dta.poem.18394": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Fleh-schrifft an ihre unempfindlich-  \n keit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Arminde deine brust mu\u00df kaltes ey\u00df nur hegen/", "tokens": ["Ar\u00b7min\u00b7de", "dei\u00b7ne", "brust", "mu\u00df", "kal\u00b7tes", "ey\u00df", "nur", "he\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "VMFIN", "ADJA", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dein hertz mu\u00df h\u00e4rter noch als stahl und marmorstein/", "tokens": ["Dein", "hertz", "mu\u00df", "h\u00e4r\u00b7ter", "noch", "als", "stahl", "und", "mar\u00b7mor\u00b7stein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "ADJD", "ADV", "KOKOM", "VVFIN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ja selbst dein blut erstarrt und gantz erfrohren seyn/", "tokens": ["Ja", "selbst", "dein", "blut", "er\u00b7starrt", "und", "gantz", "er\u00b7froh\u00b7ren", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "PPOSAT", "NN", "VVPP", "KON", "ADV", "VVPP", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil dich mein jammer nicht zur wehmut kan bewegen,", "tokens": ["Weil", "dich", "mein", "jam\u00b7mer", "nicht", "zur", "weh\u00b7mut", "kan", "be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "PTKNEG", "APPRART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ach! geh doch in dich selbst/ betrachte deine gaben/", "tokens": ["Ach", "!", "geh", "doch", "in", "dich", "selbst", "/", "be\u00b7trach\u00b7te", "dei\u00b7ne", "ga\u00b7ben", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "ADV", "APPR", "PPER", "ADV", "$(", "VVFIN", "PPOSAT", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die himmel und natur auff deinen leib gelegt;", "tokens": ["Die", "him\u00b7mel", "und", "na\u00b7tur", "auff", "dei\u00b7nen", "leib", "ge\u00b7legt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Es wird durch deine pracht ein steinern hertz bewegt/", "tokens": ["Es", "wird", "durch", "dei\u00b7ne", "pracht", "ein", "stei\u00b7nern", "hertz", "be\u00b7wegt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und ich soll keine glut in meiner seelen haben?", "tokens": ["Und", "ich", "soll", "kei\u00b7ne", "glut", "in", "mei\u00b7ner", "see\u00b7len", "ha\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PIAT", "NN", "APPR", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Bedencke was du thust: dein blick reitzt mich zum lieben/", "tokens": ["Be\u00b7den\u00b7cke", "was", "du", "thust", ":", "dein", "blick", "reitzt", "mich", "zum", "lie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PWS", "PPER", "VVFIN", "$.", "PPOSAT", "NN", "VVFIN", "PPER", "APPRART", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Er hat mich ohnversehns in diesen brand gesetzt/", "tokens": ["Er", "hat", "mich", "ohn\u00b7ver\u00b7sehns", "in", "die\u00b7sen", "brand", "ge\u00b7setzt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "APPR", "PDAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Dein mund hergegen schilt/ was meine seel erg\u00f6tzt/", "tokens": ["Dein", "mund", "her\u00b7ge\u00b7gen", "schilt", "/", "was", "mei\u00b7ne", "seel", "er\u00b7g\u00f6tzt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVFIN", "$(", "PWS", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und st\u00fcrtzt mich jederzeit nur tiffer ins betr\u00fcben.", "tokens": ["Und", "st\u00fcrtzt", "mich", "je\u00b7der\u00b7zeit", "nur", "tif\u00b7fer", "ins", "be\u00b7tr\u00fc\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "ADJD", "APPRART", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Du sprichst: ich soll dich nicht hinf\u00fchro mehr ber\u00fchren/", "tokens": ["Du", "sprichst", ":", "ich", "soll", "dich", "nicht", "hin\u00b7f\u00fch\u00b7ro", "mehr", "be\u00b7r\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VMFIN", "PPER", "PTKNEG", "ADV", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Es soll kein liebes-wort aus meinem munde gehn/", "tokens": ["Es", "soll", "kein", "lie\u00b7bes\u00b7wort", "aus", "mei\u00b7nem", "mun\u00b7de", "gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Wie lange soll ich dir so zu gebote stehn?", "tokens": ["Wie", "lan\u00b7ge", "soll", "ich", "dir", "so", "zu", "ge\u00b7bo\u00b7te", "stehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VMFIN", "PPER", "PPER", "ADV", "APPR", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Wann wird mich deine gunst aus diesen banden f\u00fchren?", "tokens": ["Wann", "wird", "mich", "dei\u00b7ne", "gunst", "aus", "die\u00b7sen", "ban\u00b7den", "f\u00fch\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PPOSAT", "NN", "APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ich kan mich warlich nicht in deine weise finden/", "tokens": ["Ich", "kan", "mich", "war\u00b7lich", "nicht", "in", "dei\u00b7ne", "wei\u00b7se", "fin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "PTKNEG", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Die sch\u00f6nheit hat sich sonst mit g\u00fctigkeit verm\u00e4hlt/", "tokens": ["Die", "sch\u00f6n\u00b7heit", "hat", "sich", "sonst", "mit", "g\u00fc\u00b7tig\u00b7keit", "ver\u00b7m\u00e4hlt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PRF", "ADV", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Doch wird in deiner brust nur tyranney verh\u00e4hlt/", "tokens": ["Doch", "wird", "in", "dei\u00b7ner", "brust", "nur", "ty\u00b7ran\u00b7ney", "ver\u00b7h\u00e4hlt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "PPOSAT", "NN", "ADV", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Du tadelst auch an mir die kleinsten liehes-s\u00fcnden/", "tokens": ["Du", "ta\u00b7delst", "auch", "an", "mir", "die", "kleins\u00b7ten", "lie\u00b7hes\u00b7s\u00fcn\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPER", "ART", "ADJA", "NN", "$("], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.21": {"text": "Du m\u00f6chtst zuletzt wohl gar die augen mir verblenden/", "tokens": ["Du", "m\u00f6chtst", "zu\u00b7letzt", "wohl", "gar", "die", "au\u00b7gen", "mir", "ver\u00b7blen\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "ADV", "ART", "NN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Da\u00df ich nicht ferner d\u00f6rfft dein himmlisch wesen schaun/", "tokens": ["Da\u00df", "ich", "nicht", "fer\u00b7ner", "d\u00f6rfft", "dein", "himm\u00b7lisch", "we\u00b7sen", "schaun", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "VVFIN", "PPOSAT", "ADJD", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Was aber kanstu wohl auf diesen grundstein baun/", "tokens": ["Was", "a\u00b7ber", "kans\u00b7tu", "wohl", "auf", "die\u00b7sen", "grund\u00b7stein", "baun", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VMFIN", "ADV", "APPR", "PDAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Der selbst zerrissen wird von deinen zarten h\u00e4nden?", "tokens": ["Der", "selbst", "zer\u00b7ris\u00b7sen", "wird", "von", "dei\u00b7nen", "zar\u00b7ten", "h\u00e4n\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVPP", "VAFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Arminde nicht zu scharff! Ich brenn in deinen flammen/", "tokens": ["Ar\u00b7min\u00b7de", "nicht", "zu", "scharff", "!", "Ich", "brenn", "in", "dei\u00b7nen", "flam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "PTKA", "ADJD", "$.", "PPER", "VVFIN", "APPR", "PPOSAT", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Die Salamandren auf fast unertr\u00e4glich seyn/", "tokens": ["Die", "Sa\u00b7la\u00b7man\u00b7dren", "auf", "fast", "un\u00b7er\u00b7tr\u00e4g\u00b7lich", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADV", "ADJD", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Mein feuer r\u00fchrt ja her von deinem sonnen schein/", "tokens": ["Mein", "feu\u00b7er", "r\u00fchrt", "ja", "her", "von", "dei\u00b7nem", "son\u00b7nen", "schein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Wie kanstu mich dann nun und meine glut verdammen.", "tokens": ["Wie", "kans\u00b7tu", "mich", "dann", "nun", "und", "mei\u00b7ne", "glut", "ver\u00b7dam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "ADV", "KON", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Verwirffstu meine pein und mein getreues lieben/", "tokens": ["Ver\u00b7wirffs\u00b7tu", "mei\u00b7ne", "pein", "und", "mein", "ge\u00b7treu\u00b7es", "lie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "KON", "PPOSAT", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "So tadle erst zuvor dein helles augen-licht/", "tokens": ["So", "tad\u00b7le", "erst", "zu\u00b7vor", "dein", "hel\u00b7les", "au\u00b7gen\u00b7licht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Bestraffe deine macht/ die tausend hertzen bricht/", "tokens": ["Be\u00b7straf\u00b7fe", "dei\u00b7ne", "macht", "/", "die", "tau\u00b7send", "hert\u00b7zen", "bricht", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "VVFIN", "$(", "ART", "CARD", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Denn du hast selbst di\u00df feur in meine brust getrieben,", "tokens": ["Denn", "du", "hast", "selbst", "di\u00df", "feur", "in", "mei\u00b7ne", "brust", "ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "PDS", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Arminde h\u00f6r doch auf mit meiner noth zu schertzen;", "tokens": ["Ar\u00b7min\u00b7de", "h\u00f6r", "doch", "auf", "mit", "mei\u00b7ner", "noth", "zu", "schert\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "APPR", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Gewi\u00df/ dein laulicht seyn vertilgt nicht meine glut/", "tokens": ["Ge\u00b7wi\u00df", "/", "dein", "lau\u00b7licht", "seyn", "ver\u00b7tilgt", "nicht", "mei\u00b7ne", "glut", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$(", "PPOSAT", "ADJD", "VAINF", "VVFIN", "PTKNEG", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Je k\u00e4lter du dich stellst/ je heisser wird mein blut/", "tokens": ["Je", "k\u00e4l\u00b7ter", "du", "dich", "stellst", "/", "je", "heis\u00b7ser", "wird", "mein", "blut", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "PRF", "VVFIN", "$(", "ADV", "ADJD", "VAFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Ach! la\u00df dir meine qual doch endlich gehn zu hertzen.", "tokens": ["Ach", "!", "la\u00df", "dir", "mei\u00b7ne", "qual", "doch", "end\u00b7lich", "gehn", "zu", "hert\u00b7zen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVIMP", "PPER", "PPOSAT", "NN", "ADV", "ADV", "VVINF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Ich mu\u00df ja lang genug in furcht und hoffnung schweben/", "tokens": ["Ich", "mu\u00df", "ja", "lang", "ge\u00b7nug", "in", "furcht", "und", "hoff\u00b7nung", "schwe\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADJD", "ADV", "APPR", "NN", "KON", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Soll ich ich nun ferner noch in liebes-banden stehn/", "tokens": ["Soll", "ich", "ich", "nun", "fer\u00b7ner", "noch", "in", "lie\u00b7bes\u00b7ban\u00b7den", "stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ADV", "ADV", "ADV", "APPR", "ADJA", "VVINF", "$("], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.39": {"text": "Und ohne allen trost und labsal untergehn?", "tokens": ["Und", "oh\u00b7ne", "al\u00b7len", "trost", "und", "lab\u00b7sal", "un\u00b7ter\u00b7gehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "NN", "KON", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Soll nichts als bittres weh den matten geist umgeben?", "tokens": ["Soll", "nichts", "als", "bitt\u00b7res", "weh", "den", "mat\u00b7ten", "geist", "um\u00b7ge\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "KOKOM", "ADJA", "NN", "ART", "ADJA", "NN", "VVPP", "$."]