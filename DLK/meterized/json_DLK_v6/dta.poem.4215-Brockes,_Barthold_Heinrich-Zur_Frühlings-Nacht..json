{"dta.poem.4215": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Zur Fr\u00fchlings-Nacht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein solcher Schatz von vieler Anmuht erf\u00fcllt mit Wundern", "tokens": ["Ein", "sol\u00b7cher", "Schatz", "von", "vie\u00b7ler", "An\u00b7muht", "er\u00b7f\u00fcllt", "mit", "Wun\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "APPR", "PIAT", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "unsre Welt,", "tokens": ["uns\u00b7re", "Welt", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Wenn ein entw\u00f6lkter Fr\u00fchlings-Tag allm\u00e4hlich in den", "tokens": ["Wenn", "ein", "ent\u00b7w\u00f6lk\u00b7ter", "Fr\u00fch\u00b7lings\u00b7Tag", "all\u00b7m\u00e4h\u00b7lich", "in", "den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "ADJD", "APPR", "ART"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Abend f\u00e4llt.", "tokens": ["A\u00b7bend", "f\u00e4llt", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Doch h\u00f6rt die g\u00fctige Natur, auch noch nicht einst im Dun-", "tokens": ["Doch", "h\u00f6rt", "die", "g\u00fc\u00b7ti\u00b7ge", "Na\u00b7tur", ",", "auch", "noch", "nicht", "einst", "im", "Dun"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$,", "ADV", "ADV", "PTKNEG", "ADV", "APPRART", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.6": {"text": "keln, auf,", "tokens": ["keln", ",", "auf", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "PTKVZ", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.7": {"text": "Uns neue Sch\u00e4tze zu verschwenden, mit neuer Lust uns", "tokens": ["Uns", "neu\u00b7e", "Sch\u00e4t\u00b7ze", "zu", "ver\u00b7schwen\u00b7den", ",", "mit", "neu\u00b7er", "Lust", "uns"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "PTKZU", "VVINF", "$,", "APPR", "ADJA", "NN", "PPER"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "zu begl\u00fccken,", "tokens": ["zu", "be\u00b7gl\u00fc\u00b7cken", ","], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Und, da sie nicht die Erde kann, f\u00fcr uns den Himmel,", "tokens": ["Und", ",", "da", "sie", "nicht", "die", "Er\u00b7de", "kann", ",", "f\u00fcr", "uns", "den", "Him\u00b7mel", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "PTKNEG", "ART", "NN", "VMFIN", "$,", "APPR", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "auszuschm\u00fccken.", "tokens": ["aus\u00b7zu\u00b7schm\u00fc\u00b7cken", "."], "token_info": ["word", "punct"], "pos": ["VVIZU", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.11": {"text": "Des Mondes Silber-weisser Kreis beschleunigt seinen hel-", "tokens": ["Des", "Mon\u00b7des", "Sil\u00b7ber\u00b7weis\u00b7ser", "Kreis", "be\u00b7schleu\u00b7nigt", "sei\u00b7nen", "hel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.12": {"text": "len Lauf", "tokens": ["len", "Lauf"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.13": {"text": "In reiner Luft noch einst so rein. Es sind an den sapphir-", "tokens": ["In", "rei\u00b7ner", "Luft", "noch", "einst", "so", "rein", ".", "Es", "sind", "an", "den", "sap\u00b7phir"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADV", "ADV", "ADV", "ADJD", "$.", "PPER", "VAFIN", "APPR", "ART", "TRUNC"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "nen H\u00f6hen", "tokens": ["nen", "H\u00f6\u00b7hen"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.15": {"text": "Viel tausend sch\u00fctternde Brillanten im hellen Sternen-", "tokens": ["Viel", "tau\u00b7send", "sch\u00fct\u00b7tern\u00b7de", "Bril\u00b7lan\u00b7ten", "im", "hel\u00b7len", "Ster\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ADJA", "NN", "APPRART", "ADJA", "TRUNC"], "meter": "-+-+--++--+-+-", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "Heer zu sehen.", "tokens": ["Heer", "zu", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Ist es nun m\u00f6glich und begreiflich, in solcher Anmuht", "tokens": ["Ist", "es", "nun", "m\u00f6g\u00b7lich", "und", "be\u00b7greif\u00b7lich", ",", "in", "sol\u00b7cher", "An\u00b7muht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "KON", "ADJD", "$,", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Fluht zu schwimmen,", "tokens": ["Fluht", "zu", "schwim\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "In einer mit so sch\u00f6nen Wundern ganz angef\u00fcllten Welt", "tokens": ["In", "ei\u00b7ner", "mit", "so", "sch\u00f6\u00b7nen", "Wun\u00b7dern", "ganz", "an\u00b7ge\u00b7f\u00fcll\u00b7ten", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "APPR", "ADV", "ADJA", "NN", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "zu leben,", "tokens": ["zu", "le\u00b7ben", ","], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Wo so viel tausend Herrlichkeiten an allen Orten uns um-", "tokens": ["Wo", "so", "viel", "tau\u00b7send", "Herr\u00b7lich\u00b7kei\u00b7ten", "an", "al\u00b7len", "Or\u00b7ten", "uns", "um"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ADV", "CARD", "NN", "APPR", "PIAT", "NN", "PPER", "TRUNC"], "meter": "-+-+-+-+--+-+--+", "measure": "iambic.septa.relaxed"}, "line.6": {"text": "geben,", "tokens": ["ge\u00b7ben", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Und doch dieselben nicht bemerken, um Dem ein Loblied", "tokens": ["Und", "doch", "die\u00b7sel\u00b7ben", "nicht", "be\u00b7mer\u00b7ken", ",", "um", "Dem", "ein", "Lob\u00b7lied"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PDS", "PTKNEG", "VVINF", "$,", "KOUI", "ART", "ART", "NN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "anzustimmen,", "tokens": ["an\u00b7zu\u00b7stim\u00b7men", ","], "token_info": ["word", "punct"], "pos": ["VVIZU", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Der sie so \u00fcberschwenglich schenkt! Kann wohl ein Mensch", "tokens": ["Der", "sie", "so", "\u00fc\u00b7bersc\u00b7hweng\u00b7lich", "schenkt", "!", "Kann", "wohl", "ein", "Mensch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "ADJD", "VVFIN", "$.", "VMFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "mit Recht verlangen,", "tokens": ["mit", "Recht", "ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "Er habe einen weisen Geist und eine kluge Seel\u2019 empfangen,", "tokens": ["Er", "ha\u00b7be", "ei\u00b7nen", "wei\u00b7sen", "Geist", "und", "ei\u00b7ne", "klu\u00b7ge", "Seel'", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "KON", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.12": {"text": "Da er doch in dem gr\u00f6\u00dften Vorwurf, in welchem man", "tokens": ["Da", "er", "doch", "in", "dem", "gr\u00f6\u00df\u00b7ten", "Vor\u00b7wurf", ",", "in", "wel\u00b7chem", "man"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ART", "ADJA", "NN", "$,", "APPR", "PRELS", "PIS"], "meter": "--+--+-+--+-+", "measure": "anapaest.di.plus"}, "line.13": {"text": "den Sch\u00f6pfer ehrt,", "tokens": ["den", "Sch\u00f6p\u00b7fer", "ehrt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Nicht besser, als das dummste Vieh, mit GOttes Creatur", "tokens": ["Nicht", "bes\u00b7ser", ",", "als", "das", "dumms\u00b7te", "Vieh", ",", "mit", "Got\u00b7tes", "Crea\u00b7tur"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "ADJD", "$,", "KOUS", "ART", "ADJA", "NN", "$,", "APPR", "NN", "NN"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.2": {"text": "verf\u00e4hrt,", "tokens": ["ver\u00b7f\u00e4hrt", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "In welcher Gott sich ihm doch zeiget, und welche meh-", "tokens": ["In", "wel\u00b7cher", "Gott", "sich", "ihm", "doch", "zei\u00b7get", ",", "und", "wel\u00b7che", "meh"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PWAT", "NN", "PRF", "PPER", "ADV", "VVFIN", "$,", "KON", "PWAT", "TRUNC"], "meter": "-+-+---+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "rentheils allein", "tokens": ["ren\u00b7theils", "al\u00b7lein"], "token_info": ["word", "word"], "pos": ["ADV", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Die allerdeutlichsten Beweise von Seinem wahren Da-", "tokens": ["Die", "al\u00b7ler\u00b7deut\u00b7lichs\u00b7ten", "Be\u00b7wei\u00b7se", "von", "Sei\u00b7nem", "wah\u00b7ren", "Da"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "TRUNC"], "meter": "-+-+---+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "seyn seyn!", "tokens": ["seyn", "seyn", "!"], "token_info": ["word", "word", "punct"], "pos": ["VAINF", "VAINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Von welchen, ohne Widerspruch, wir alle die\u00df bekennen", "tokens": ["Von", "wel\u00b7chen", ",", "oh\u00b7ne", "Wi\u00b7der\u00b7spruch", ",", "wir", "al\u00b7le", "die\u00df", "be\u00b7ken\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PWAT", "$,", "KOUI", "NN", "$,", "PPER", "PIAT", "PDS", "VVINF"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.8": {"text": "m\u00fcssen:", "tokens": ["m\u00fcs\u00b7sen", ":"], "token_info": ["word", "punct"], "pos": ["VMFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "Da\u00df, ohne g\u00f6ttliche Gesch\u00f6pfe, wir nichts von einem", "tokens": ["Da\u00df", ",", "oh\u00b7ne", "g\u00f6tt\u00b7li\u00b7che", "Ge\u00b7sch\u00f6p\u00b7fe", ",", "wir", "nichts", "von", "ei\u00b7nem"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "$,", "KOUI", "ADJA", "NN", "$,", "PPER", "PIS", "APPR", "ART"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Sch\u00f6pfer wissen.", "tokens": ["Sch\u00f6p\u00b7fer", "wis\u00b7sen", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}