{"textgrid.poem.64308": {"metadata": {"author": {"name": "Stolberg, Friedrich Leopold Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sie ist da! die Begeistrung, da!", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie ist da! die Begeistrung, da!", "tokens": ["Sie", "ist", "da", "!", "die", "Be\u00b7geis\u00b7trung", ",", "da", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$.", "ART", "NN", "$,", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Heil mir! und reden kann die trunkne Lippe!", "tokens": ["Heil", "mir", "!", "und", "re\u00b7den", "kann", "die", "trunk\u00b7ne", "Lip\u00b7pe", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "KON", "VVINF", "VMFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Von schneeichten Alpen", "tokens": ["Von", "schne\u00b7eich\u00b7ten", "Al\u00b7pen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Schwebt auf der Abendr\u00f6te Fl\u00fcgel sie zu mir herab,", "tokens": ["Schwebt", "auf", "der", "A\u00b7bend\u00b7r\u00f6\u00b7te", "Fl\u00fc\u00b7gel", "sie", "zu", "mir", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN", "PPER", "APPR", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.5": {"text": "Weilet nicht, fleugt auf,", "tokens": ["Wei\u00b7let", "nicht", ",", "fleugt", "auf", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Atmet, ihr blendendes Gewand", "tokens": ["At\u00b7met", ",", "ihr", "blen\u00b7den\u00b7des", "Ge\u00b7wand"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "PPOSAT", "ADJA", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Geg\u00fcrtet mit Regenbogen,", "tokens": ["Ge\u00b7g\u00fcr\u00b7tet", "mit", "Re\u00b7gen\u00b7bo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Umwunden ihr Haar mit gestirntem Diadem,", "tokens": ["Um\u00b7wun\u00b7den", "ihr", "Haar", "mit", "ge\u00b7stirn\u00b7tem", "Di\u00b7a\u00b7dem", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.9": {"text": "Atmet feinere L\u00fcfte,", "tokens": ["At\u00b7met", "fei\u00b7ne\u00b7re", "L\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.10": {"text": "Himmelsl\u00fcfte!", "tokens": ["Him\u00b7mel\u00b7sl\u00fcf\u00b7te", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.11": {"text": "Zieht mich ihr nach,", "tokens": ["Zieht", "mich", "ihr", "nach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPER", "PTKVZ", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.12": {"text": "Tr\u00e4nket mit Tau des n\u00e4heren Himmels mich!", "tokens": ["Tr\u00e4n\u00b7ket", "mit", "Tau", "des", "n\u00e4\u00b7he\u00b7ren", "Him\u00b7mels", "mich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "ART", "ADJA", "NN", "PPER", "$."], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}}, "stanza.2": {"line.1": {"text": "Heil mir, da\u00df ich kenne", "tokens": ["Heil", "mir", ",", "da\u00df", "ich", "ken\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPER", "$,", "KOUS", "PPER", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Die Strahlende!", "tokens": ["Die", "Strah\u00b7len\u00b7de", "!"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+--", "measure": "dactylic.init"}, "line.3": {"text": "Heil mir, da\u00df sie w\u00fcrdiget", "tokens": ["Heil", "mir", ",", "da\u00df", "sie", "w\u00fcr\u00b7di\u00b7get"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPER", "$,", "KOUS", "PPER", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihres Fluges mich!", "tokens": ["Ih\u00b7res", "Flu\u00b7ges", "mich", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "G\u00f6ttin, so du mich f\u00fchrst,", "tokens": ["G\u00f6t\u00b7tin", ",", "so", "du", "mich", "f\u00fchrst", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "PPER", "PRF", "VVFIN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "Flieget, nichtiges Gest\u00e4ub,", "tokens": ["Flie\u00b7get", ",", "nich\u00b7ti\u00b7ges", "Ge\u00b7st\u00e4ub", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Unter dem Fl\u00fcgelschlag meiner Phantasei,", "tokens": ["Un\u00b7ter", "dem", "Fl\u00fc\u00b7gel\u00b7schlag", "mei\u00b7ner", "Phan\u00b7ta\u00b7sei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.8": {"text": "Sonne dahin und Stern! Milchstra\u00dfe dahin!", "tokens": ["Son\u00b7ne", "da\u00b7hin", "und", "Stern", "!", "Milch\u00b7stra\u00b7\u00dfe", "da\u00b7hin", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PAV", "KON", "NN", "$.", "NN", "PAV", "$."], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.9": {"text": "Heil mir, da\u00df ich kenne", "tokens": ["Heil", "mir", ",", "da\u00df", "ich", "ken\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPER", "$,", "KOUS", "PPER", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.10": {"text": "Die Flammende!", "tokens": ["Die", "Flam\u00b7men\u00b7de", "!"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+--", "measure": "dactylic.init"}, "line.11": {"text": "Da\u00df k\u00fchn ihr folget der Fl\u00fcgelschlag meiner Phantasei", "tokens": ["Da\u00df", "k\u00fchn", "ihr", "fol\u00b7get", "der", "Fl\u00fc\u00b7gel\u00b7schlag", "mei\u00b7ner", "Phan\u00b7ta\u00b7sei"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "PPER", "VVFIN", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Durch die Nacht hindurch und der Erde Bauch!", "tokens": ["Durch", "die", "Nacht", "hin\u00b7durch", "und", "der", "Er\u00b7de", "Bauch", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PAV", "KON", "ART", "NN", "NN", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.13": {"text": "So die G\u00f6ttin gebeut,", "tokens": ["So", "die", "G\u00f6t\u00b7tin", "ge\u00b7beut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.14": {"text": "\u00d6ffnet ihr sich der schwarze Scho\u00df", "tokens": ["\u00d6ff\u00b7net", "ihr", "sich", "der", "schwar\u00b7ze", "Scho\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Ewiger Finsternis,", "tokens": ["E\u00b7wi\u00b7ger", "Fins\u00b7ter\u00b7nis", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.16": {"text": "Es umrauschet ihre Glieder das Gewand der Nacht!", "tokens": ["Es", "um\u00b7rau\u00b7schet", "ih\u00b7re", "Glie\u00b7der", "das", "Ge\u00b7wand", "der", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ART", "NN", "ART", "NN", "$."], "meter": "--+-+-+-+-+-+", "measure": "anapaest.init"}, "line.17": {"text": "Flammenatmend erhellst du Abgr\u00fcnde vor mir her,", "tokens": ["Flam\u00b7me\u00b7nat\u00b7mend", "er\u00b7hellst", "du", "Ab\u00b7gr\u00fcn\u00b7de", "vor", "mir", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "+-+--+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.18": {"text": "Deine wehende Fackel zeigt und gebeut mir Flug!", "tokens": ["Dei\u00b7ne", "we\u00b7hen\u00b7de", "Fa\u00b7ckel", "zeigt", "und", "ge\u00b7beut", "mir", "Flug", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "KON", "VVFIN", "PPER", "NN", "$."], "meter": "+-+--+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.19": {"text": "Ha! wie den Fremdling staunet an", "tokens": ["Ha", "!", "wie", "den", "Fremd\u00b7ling", "stau\u00b7net", "an"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "PWAV", "ART", "NN", "VVFIN", "PTKVZ"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.20": {"text": "Der Unterirdischen sch\u00fcchternes Geschlecht!", "tokens": ["Der", "Un\u00b7ter\u00b7ir\u00b7di\u00b7schen", "sch\u00fcch\u00b7ter\u00b7nes", "Ge\u00b7schlecht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.21": {"text": "So staunt an der Maulwurf das gezeigte Licht;", "tokens": ["So", "staunt", "an", "der", "Maul\u00b7wurf", "das", "ge\u00b7zeig\u00b7te", "Licht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.22": {"text": "So staunt an der P\u00f6bel,", "tokens": ["So", "staunt", "an", "der", "P\u00f6\u00b7bel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.23": {"text": "P\u00f6bel in Purpur und geh\u00fcllt in Schulstaub,", "tokens": ["P\u00f6\u00b7bel", "in", "Pur\u00b7pur", "und", "ge\u00b7h\u00fcllt", "in", "Schul\u00b7staub", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "VVPP", "APPR", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.24": {"text": "Den erdeh\u00f6hnenden Gesang", "tokens": ["Den", "er\u00b7de\u00b7h\u00f6h\u00b7nen\u00b7den", "Ge\u00b7sang"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Der Begeistrung, und des Dichters, den nur sie gebar!", "tokens": ["Der", "Be\u00b7geis\u00b7trung", ",", "und", "des", "Dich\u00b7ters", ",", "den", "nur", "sie", "ge\u00b7bar", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "ART", "NN", "$,", "PRELS", "ADV", "PPER", "ADJD", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}}}}}