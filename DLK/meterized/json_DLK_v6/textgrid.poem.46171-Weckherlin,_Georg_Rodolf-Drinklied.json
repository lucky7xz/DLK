{"textgrid.poem.46171": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Drinklied", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer ist doch immer so geschossen,", "tokens": ["Wer", "ist", "doch", "im\u00b7mer", "so", "ge\u00b7schos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "da\u00df ab dem lieben rebensaft,", "tokens": ["da\u00df", "ab", "dem", "lie\u00b7ben", "re\u00b7ben\u00b7saft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "der unsers herzens trost und kraft,", "tokens": ["der", "un\u00b7sers", "her\u00b7zens", "trost", "und", "kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "er unw\u00fcrsch sein solt und verdrossen?", "tokens": ["er", "un\u00b7w\u00fcrsch", "sein", "solt", "und", "ver\u00b7dros\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VAINF", "VMFIN", "KON", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dan was kan doch ohn drinken wehren?", "tokens": ["Dan", "was", "kan", "doch", "ohn", "drin\u00b7ken", "weh\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "VMFIN", "ADV", "APPR", "VVINF", "VVINF", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "und ist nicht unter dem gedrank", "tokens": ["und", "ist", "nicht", "un\u00b7ter", "dem", "ge\u00b7drank"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PTKNEG", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "der wein das best, mit lob und dank", "tokens": ["der", "wein", "das", "best", ",", "mit", "lob", "und", "dank"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "PDS", "VVFIN", "$,", "APPR", "NN", "KON", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "vor allem, was na\u00df, hoch zu ehren?", "tokens": ["vor", "al\u00b7lem", ",", "was", "na\u00df", ",", "hoch", "zu", "eh\u00b7ren", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "PRELS", "ADJD", "$,", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Besehet doch, freind, wan es regnet,", "tokens": ["Be\u00b7se\u00b7het", "doch", ",", "freind", ",", "wan", "es", "reg\u00b7net", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "ADJD", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "wie durch den starken regengu\u00df,", "tokens": ["wie", "durch", "den", "star\u00b7ken", "re\u00b7gen\u00b7gu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "bisweilen auch durch einen flu\u00df", "tokens": ["bis\u00b7wei\u00b7len", "auch", "durch", "ei\u00b7nen", "flu\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "das erdreich sich voll saufend segnet.", "tokens": ["das", "er\u00b7dreich", "sich", "voll", "sau\u00b7fend", "seg\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PRF", "ADJD", "ADJD", "VVFIN", "$."], "meter": "-++--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Die kr\u00e4uter und gew\u00e4chs der erden,", "tokens": ["Die", "kr\u00e4u\u00b7ter", "und", "ge\u00b7w\u00e4chs", "der", "er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KON", "ADJA", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "ja alle b\u00e4um auch, klein und gro\u00df,", "tokens": ["ja", "al\u00b7le", "b\u00e4um", "auch", ",", "klein", "und", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "ADV", "ADV", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "verschmachten trostlos und fruchtlos,", "tokens": ["ver\u00b7schmach\u00b7ten", "trost\u00b7los", "und", "frucht\u00b7los", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "wan sie nicht oft bezechet werden.", "tokens": ["wan", "sie", "nicht", "oft", "be\u00b7ze\u00b7chet", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PTKNEG", "ADV", "VVPP", "VAINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.5": {"line.1": {"text": "Den durst die thier und v\u00f6gel stillen", "tokens": ["Den", "durst", "die", "thier", "und", "v\u00f6\u00b7gel", "stil\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ART", "NN", "KON", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "nach lust mit wollust, und die fisch", "tokens": ["nach", "lust", "mit", "wol\u00b7lust", ",", "und", "die", "fisch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "VVFIN", "APPR", "NN", "$,", "KON", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "die suchen stets was na\u00df und frisch,", "tokens": ["die", "su\u00b7chen", "stets", "was", "na\u00df", "und", "frisch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "PWS", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "damit begirig sie sich f\u00fcllen.", "tokens": ["da\u00b7mit", "be\u00b7gi\u00b7rig", "sie", "sich", "f\u00fcl\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADJD", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Das meer will auch den rausch nicht fliehen,", "tokens": ["Das", "meer", "will", "auch", "den", "rausch", "nicht", "flie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "sondern es pfleget ohn abla\u00df", "tokens": ["son\u00b7dern", "es", "pfle\u00b7get", "ohn", "ab\u00b7la\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "breit tiefe fl\u00fc\u00df und b\u00e4ch ohn ma\u00df", "tokens": ["breit", "tie\u00b7fe", "fl\u00fc\u00df", "und", "b\u00e4ch", "ohn", "ma\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "ADJA", "NN", "KON", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "garau\u00dfend in den wanst zu ziehen.", "tokens": ["gar\u00b7au\u00b7\u00dfend", "in", "den", "wanst", "zu", "zie\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Ist es dan durch den drunk getroffen,", "tokens": ["Ist", "es", "dan", "durch", "den", "drunk", "ge\u00b7trof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "so fanget es ein wesen an,", "tokens": ["so", "fan\u00b7get", "es", "ein", "we\u00b7sen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "als ob es auch wolt jederman", "tokens": ["als", "ob", "es", "auch", "wolt", "je\u00b7der\u00b7man"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOKOM", "KOUS", "PPER", "ADV", "VMFIN", "PIS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "ers\u00e4ufen, weil es selbs besoffen.", "tokens": ["er\u00b7s\u00e4u\u00b7fen", ",", "weil", "es", "selbs", "be\u00b7sof\u00b7fen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KOUS", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Und warum fallen oft zu haufen", "tokens": ["Und", "wa\u00b7rum", "fal\u00b7len", "oft", "zu", "hau\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "VVFIN", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "die tobend-brausend-laute wind?", "tokens": ["die", "to\u00b7ben\u00b7dbrau\u00b7sen\u00b7dlau\u00b7te", "wind", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "weil sie, zu bausen sehr geschwind,", "tokens": ["weil", "sie", ",", "zu", "bau\u00b7sen", "sehr", "ge\u00b7schwind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PTKZU", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "das meer gern wolten gar aussaufen.", "tokens": ["das", "meer", "gern", "wol\u00b7ten", "gar", "aus\u00b7sau\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VMFIN", "ADV", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "In dem meer und in allen bronnen", "tokens": ["In", "dem", "meer", "und", "in", "al\u00b7len", "bron\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "KON", "APPR", "PIAT", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "die sonn selbs l\u00f6schet ihren durst,", "tokens": ["die", "sonn", "selbs", "l\u00f6\u00b7schet", "ih\u00b7ren", "durst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "VVFIN", "PPOSAT", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "und der mon w\u00e4r schon ein bratwurst,", "tokens": ["und", "der", "mon", "w\u00e4r", "schon", "ein", "brat\u00b7wurst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NE", "VAFIN", "ADV", "ART", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "wan er nicht yoll w\u00fcrd von der sonnen.", "tokens": ["wan", "er", "nicht", "yoll", "w\u00fcrd", "von", "der", "son\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PTKNEG", "ADJD", "VAFIN", "APPR", "ART", "ADJA", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.10": {"line.1": {"text": "Drum soll uns f\u00fcrhin niemand wehren,", "tokens": ["Drum", "soll", "uns", "f\u00fcr\u00b7hin", "nie\u00b7mand", "weh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "ADV", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "wan nichts will unbesoffen sein,", "tokens": ["wan", "nichts", "will", "un\u00b7be\u00b7sof\u00b7fen", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VMFIN", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "auch mit einander bei dem wein", "tokens": ["auch", "mit", "ein\u00b7an\u00b7der", "bei", "dem", "wein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "frolockend tag und nacht zu zehren.", "tokens": ["fro\u00b7lo\u00b7ckend", "tag", "und", "nacht", "zu", "zeh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Dan wer unw\u00fcrsch ist und verdrossen", "tokens": ["Dan", "wer", "un\u00b7w\u00fcrsch", "ist", "und", "ver\u00b7dros\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PWS", "ADJD", "VAFIN", "KON", "VVINF"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "ab diesem guten rebensaft,", "tokens": ["ab", "die\u00b7sem", "gu\u00b7ten", "re\u00b7ben\u00b7saft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "der unsers herzens trost und kraft,", "tokens": ["der", "un\u00b7sers", "her\u00b7zens", "trost", "und", "kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "der ist (zwar n\u00fcchtern, doch) geschossen.", "tokens": ["der", "ist", "(", "zwar", "n\u00fcch\u00b7tern", ",", "doch", ")", "ge\u00b7schos\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "VAFIN", "$(", "ADV", "ADJD", "$,", "ADV", "$(", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}