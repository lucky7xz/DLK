{"textgrid.poem.56023": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Eine Welke", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Leicht, wie nach ihrem Tode", "tokens": ["Leicht", ",", "wie", "nach", "ih\u00b7rem", "To\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "PWAV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "tr\u00e4gt sie die Handschuh, das Tuch.", "tokens": ["tr\u00e4gt", "sie", "die", "Hand\u00b7schuh", ",", "das", "Tuch", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Ein Duft aus ihrer Kommode", "tokens": ["Ein", "Duft", "aus", "ih\u00b7rer", "Kom\u00b7mo\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "verdr\u00e4ngte den lieben Geruch,", "tokens": ["ver\u00b7dr\u00e4ng\u00b7te", "den", "lie\u00b7ben", "Ge\u00b7ruch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "an dem sie sich fr\u00fcher erkannte.", "tokens": ["an", "dem", "sie", "sich", "fr\u00fc\u00b7her", "er\u00b7kann\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PRF", "ADJD", "VVFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Jetzt fragte sie lange nicht, wer", "tokens": ["Jetzt", "frag\u00b7te", "sie", "lan\u00b7ge", "nicht", ",", "wer"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKNEG", "$,", "PWS"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "sie sei (: eine ferne Verwandte),", "tokens": ["sie", "sei", "(", ":", "ei\u00b7ne", "fer\u00b7ne", "Ver\u00b7wand\u00b7te", ")", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "$(", "$.", "ART", "ADJA", "NN", "$(", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "und geht in Gedanken umher", "tokens": ["und", "geht", "in", "Ge\u00b7dan\u00b7ken", "um\u00b7her"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN", "PTKVZ"], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "und sorgt f\u00fcr ein \u00e4ngstliches Zimmer,", "tokens": ["und", "sorgt", "f\u00fcr", "ein", "\u00e4ngst\u00b7li\u00b7ches", "Zim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "das sie ordnet und schont,", "tokens": ["das", "sie", "ord\u00b7net", "und", "schont", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "weil es vielleicht noch immer", "tokens": ["weil", "es", "viel\u00b7leicht", "noch", "im\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADV"], "meter": "+-++-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "dasselbe M\u00e4dchen bewohnt.", "tokens": ["das\u00b7sel\u00b7be", "M\u00e4d\u00b7chen", "be\u00b7wohnt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}