{"dta.poem.2622": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Unverbl\u00fchte Lilie/  \n Bey Beerdigung Fr. S. Z. g. O. den 2.  \n Julii 1669.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "WoIEdler/ wo sein Geist mit Ach und Weh umbschren-\ncket/", "tokens": ["WoIEd\u00b7ler", "/", "wo", "sein", "Geist", "mit", "Ach", "und", "Weh", "umb\u00b7schren", "cket", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "PWAV", "PPOSAT", "NN", "APPR", "NN", "KON", "NN", "TRUNC", "VVFIN", "$("], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Von Seufftzen angef\u00fcllt/ von Thr\u00e4nen \u00fcberschwem\u0303t", "tokens": ["Von", "Seufft\u00b7zen", "an\u00b7ge\u00b7f\u00fcllt", "/", "von", "Thr\u00e4\u00b7nen", "\u00fc\u00b7ber\u00b7schwem\u0303t"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "VVPP", "$(", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "An seine Garten-Lust noch einst zur\u00fccke dencket/", "tokens": ["An", "sei\u00b7ne", "Gar\u00b7ten\u00b7Lust", "noch", "einst", "zu\u00b7r\u00fc\u00b7cke", "den\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "ADV", "VVFIN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und nicht des Hertzens Blut der Augen Stralen hemmt;", "tokens": ["Und", "nicht", "des", "Hert\u00b7zens", "Blut", "der", "Au\u00b7gen", "Stra\u00b7len", "hemmt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ART", "NN", "NN", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So wird er unverbl\u00fcht die edle Blumeschauen", "tokens": ["So", "wird", "er", "un\u00b7ver\u00b7bl\u00fcht", "die", "ed\u00b7le", "Blu\u00b7me\u00b7schau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die Name/ Stand und Zucht zu einer Lilge macht/", "tokens": ["Die", "Na\u00b7me", "/", "Stand", "und", "Zucht", "zu", "ei\u00b7ner", "Lil\u00b7ge", "macht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "NN", "KON", "NN", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und da\u00df/ ob in der Hitz ietzt schmachten Feld und Auen/", "tokens": ["Und", "da\u00df", "/", "ob", "in", "der", "Hitz", "ietzt", "schmach\u00b7ten", "Feld", "und", "Au\u00b7en", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "$(", "KOUS", "APPR", "ART", "NN", "ADV", "ADJA", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Jhr Glantz im h\u00f6chsten Grad der Sch\u00f6nheit sey gebracht.", "tokens": ["Ihr", "Glantz", "im", "h\u00f6chs\u00b7ten", "Grad", "der", "Sch\u00f6n\u00b7heit", "sey", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "ADJA", "NN", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Es mag des Sommers Kleid mit Nelcken sich besternen/", "tokens": ["Es", "mag", "des", "Som\u00b7mers", "Kleid", "mit", "Nel\u00b7cken", "sich", "bes\u00b7ter\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "NN", "APPR", "NN", "PRF", "VVINF", "$("], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Die Lilie bleibt doch der Blumen K\u00f6nigin/", "tokens": ["Die", "Li\u00b7lie", "bleibt", "doch", "der", "Blu\u00b7men", "K\u00f6\u00b7ni\u00b7gin", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN", "NN", "$("], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Der G\u00e4rten bester Schmuck/ der Balsam der von fernen", "tokens": ["Der", "G\u00e4r\u00b7ten", "bes\u00b7ter", "Schmuck", "/", "der", "Bal\u00b7sam", "der", "von", "fer\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "$(", "ART", "NN", "ART", "APPR", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Uns Hertz und Seelerfreut/ und sticht den Amber hin.", "tokens": ["Uns", "Hertz", "und", "See\u00b7ler\u00b7freut", "/", "und", "sticht", "den", "Am\u00b7ber", "hin", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "KON", "NN", "$(", "KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Es legte Jupiter an seiner Juno Br\u00fcste", "tokens": ["Es", "leg\u00b7te", "Ju\u00b7pi\u00b7ter", "an", "sei\u00b7ner", "Ju\u00b7no", "Br\u00fcs\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Den Helden Hercules/ als einen S\u00e4ugling/ an/", "tokens": ["Den", "Hel\u00b7den", "Her\u00b7cu\u00b7les", "/", "als", "ei\u00b7nen", "S\u00e4ug\u00b7ling", "/", "an", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "NN", "$(", "KOUS", "ART", "NN", "$(", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ob so der Himmel ihm zu theile werden m\u00fcste.", "tokens": ["Ob", "so", "der", "Him\u00b7mel", "ihm", "zu", "thei\u00b7le", "wer\u00b7den", "m\u00fcs\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "PPER", "PTKZU", "VVFIN", "VAINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Wie das gestillte Kind nicht weiter trincken kan/", "tokens": ["Wie", "das", "ge\u00b7still\u00b7te", "Kind", "nicht", "wei\u00b7ter", "trin\u00b7cken", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "PTKNEG", "ADV", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ist doch der G\u00f6ttin Milch im doppel Strom geflossen/", "tokens": ["Ist", "doch", "der", "G\u00f6t\u00b7tin", "Milch", "im", "dop\u00b7pel", "Strom", "ge\u00b7flos\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "NN", "APPRART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Der zarte Perlen-Safft/ so auff die Erde sprang/", "tokens": ["Der", "zar\u00b7te", "Per\u00b7len\u00b7\u00b7S\u00b7afft", "/", "so", "auff", "die", "Er\u00b7de", "sprang", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ADV", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.19": {"text": "War Zeug aus welchem erst die Lilien entsprossen/", "tokens": ["War", "Zeug", "aus", "wel\u00b7chem", "erst", "die", "Li\u00b7li\u00b7en", "ent\u00b7spros\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "APPR", "PRELS", "ADV", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und da\u00df ein Schnee Geruch auff dero Bl\u00e4tter sanck:", "tokens": ["Und", "da\u00df", "ein", "Schnee", "Ge\u00b7ruch", "auff", "de\u00b7ro", "Bl\u00e4t\u00b7ter", "san\u00b7ck", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "NN", "APPR", "PRELAT", "NN", "VVFIN", "$."], "meter": "-+-+-+---+-+-", "measure": "unknown.measure.penta"}, "line.21": {"text": "Was in die h\u00f6h gespritzt das ist die Milchbahn worden/", "tokens": ["Was", "in", "die", "h\u00f6h", "ge\u00b7spritzt", "das", "ist", "die", "Milch\u00b7bahn", "wor\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "ADJD", "VVPP", "PDS", "VAFIN", "ART", "NN", "VAPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Die sich bey heller Nacht in tausend Sternen zeigt.", "tokens": ["Die", "sich", "bey", "hel\u00b7ler", "Nacht", "in", "tau\u00b7send", "Ster\u00b7nen", "zeigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "ADJA", "NN", "APPR", "CARD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Nun dieser Uhrsprung setzt die Lilg\u2019 in Hoheits Orden/", "tokens": ["Nun", "die\u00b7ser", "Uhrs\u00b7prung", "setzt", "die", "Lil\u00b7g'", "in", "Ho\u00b7heits", "Or\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "NN", "VVFIN", "ART", "NN", "APPR", "NE", "NN", "$("], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.24": {"text": "Da\u00df sich f\u00fcr ihrem Stab der Blumen P\u00f6fel neigt.", "tokens": ["Da\u00df", "sich", "f\u00fcr", "ih\u00b7rem", "Stab", "der", "Blu\u00b7men", "P\u00f6\u00b7fel", "neigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "APPR", "PPOSAT", "NN", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Alleine Fantasey und toller Wahn der Alten:", "tokens": ["Al\u00b7lei\u00b7ne", "Fan\u00b7ta\u00b7sey", "und", "tol\u00b7ler", "Wahn", "der", "Al\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Dem Fabelwercke mist die Warheit niemand bey/", "tokens": ["Dem", "Fa\u00b7bel\u00b7wer\u00b7cke", "mist", "die", "War\u00b7heit", "nie\u00b7mand", "bey", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PIS", "APPR", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Nein/ unser Lilie kan diesen Ruhm behalten/", "tokens": ["Nein", "/", "un\u00b7ser", "Li\u00b7lie", "kan", "die\u00b7sen", "Ruhm", "be\u00b7hal\u00b7ten", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$(", "PPOSAT", "NN", "VMFIN", "PDAT", "NN", "VVINF", "$("], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.28": {"text": "Da\u00df sie der Ankunfft nach des Himmels Pflantze sey/", "tokens": ["Da\u00df", "sie", "der", "An\u00b7kunfft", "nach", "des", "Him\u00b7mels", "Pflant\u00b7ze", "sey", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "ART", "NN", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Von GOttes Hand erbaut/ von solchem Stamm geboren/", "tokens": ["Von", "Got\u00b7tes", "Hand", "er\u00b7baut", "/", "von", "sol\u00b7chem", "Stamm", "ge\u00b7bo\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "VVPP", "$(", "APPR", "PIAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Der durch der Ahnen Ruhm und Thaten ist bekand/", "tokens": ["Der", "durch", "der", "Ah\u00b7nen", "Ruhm", "und", "Tha\u00b7ten", "ist", "be\u00b7kand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "NN", "KON", "NN", "VAFIN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Zu einem Tugend-Bild auff Erden auserkohren/", "tokens": ["Zu", "ei\u00b7nem", "Tu\u00b7gen\u00b7dBild", "auff", "Er\u00b7den", "au\u00b7ser\u00b7koh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Zu einer Augen-Lust dem Liebsten zugesand.", "tokens": ["Zu", "ei\u00b7ner", "Au\u00b7gen\u00b7Lust", "dem", "Liebs\u00b7ten", "zu\u00b7ge\u00b7sand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Und jetzt ob es wol scheint/ da\u00df sie von uns gewichen", "tokens": ["Und", "jetzt", "ob", "es", "wol", "scheint", "/", "da\u00df", "sie", "von", "uns", "ge\u00b7wi\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "KOUS", "PPER", "ADV", "VVFIN", "$(", "KOUS", "PPER", "APPR", "PPER", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "In jenem Paradi\u00df ein unverwelckte Blum/", "tokens": ["In", "je\u00b7nem", "Pa\u00b7ra\u00b7di\u00df", "ein", "un\u00b7ver\u00b7welck\u00b7te", "Blum", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ART", "ADJA", "NE", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "So mehr den Milch-Weg kr\u00f6nt/ als was der Mund der Grichen/", "tokens": ["So", "mehr", "den", "Milch\u00b7Weg", "kr\u00f6nt", "/", "als", "was", "der", "Mund", "der", "Gri\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVFIN", "$(", "KOUS", "PIS", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Der irrdschen Lilien vermeldet hat zum Ruhm:", "tokens": ["Der", "irrd\u00b7schen", "Li\u00b7li\u00b7en", "ver\u00b7mel\u00b7det", "hat", "zum", "Ruhm", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "VAFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Die werden hochgesch\u00e4tzt von angenehmen Hauchen", "tokens": ["Die", "wer\u00b7den", "hoch\u00b7ge\u00b7sch\u00e4tzt", "von", "an\u00b7ge\u00b7neh\u00b7men", "Hau\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Und kr\u00e4fftigem Geruch/ der jederman beliebt/", "tokens": ["Und", "kr\u00e4ff\u00b7ti\u00b7gem", "Ge\u00b7ruch", "/", "der", "je\u00b7der\u00b7man", "be\u00b7liebt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$(", "ART", "PIS", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Da\u00df ihre Bl\u00e4tter vor die Schlangen-Stich zu brauchen/", "tokens": ["Da\u00df", "ih\u00b7re", "Bl\u00e4t\u00b7ter", "vor", "die", "Schlan\u00b7gen\u00b7Stich", "zu", "brau\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Und da\u00df ihr lindes Oel den Wunden Artzney gibt:", "tokens": ["Und", "da\u00df", "ihr", "lin\u00b7des", "O\u00b7el", "den", "Wun\u00b7den", "Artz\u00b7ney", "gibt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "ADJA", "NN", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.41": {"text": "Der gute Tugend-Ruch von der erblasten Frauen/", "tokens": ["Der", "gu\u00b7te", "Tu\u00b7gen\u00b7dRuch", "von", "der", "er\u00b7blas\u00b7ten", "Frau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Jhr herrliches Ger\u00fccht ergetzet noch die Stad