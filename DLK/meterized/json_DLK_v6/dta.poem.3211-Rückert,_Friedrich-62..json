{"dta.poem.3211": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "62.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mag doch aus Neubegier und Lust am Wechsel reisen", "tokens": ["Mag", "doch", "aus", "Neu\u00b7be\u00b7gier", "und", "Lust", "am", "Wech\u00b7sel", "rei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "APPR", "NN", "KON", "NN", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Jugend, treu bleibt gern das Alter seinen Kreisen.", "tokens": ["Die", "Ju\u00b7gend", ",", "treu", "bleibt", "gern", "das", "Al\u00b7ter", "sei\u00b7nen", "Krei\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "VVFIN", "ADV", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nach fernem Sch\u00f6nen la\u00df dich locken nicht das Sehnen;", "tokens": ["Nach", "fer\u00b7nem", "Sch\u00f6\u00b7nen", "la\u00df", "dich", "lo\u00b7cken", "nicht", "das", "Seh\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVIMP", "PPER", "VVFIN", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zieh es im Geist heran, und schm\u00fccke deine Scenen.", "tokens": ["Zieh", "es", "im", "Geist", "he\u00b7ran", ",", "und", "schm\u00fc\u00b7cke", "dei\u00b7ne", "Sce\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$,", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Dann aber, wann dich nah ein Unertr\u00e4gliches", "tokens": ["Dann", "a\u00b7ber", ",", "wann", "dich", "nah", "ein", "Un\u00b7er\u00b7tr\u00e4g\u00b7li\u00b7ches"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "PWAV", "PPER", "ADJD", "ART", "ADJA"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Umdr\u00e4ngen will, ein w\u00fcst und tr\u00fcb Allt\u00e4gliches;", "tokens": ["Um\u00b7dr\u00e4n\u00b7gen", "will", ",", "ein", "w\u00fcst", "und", "tr\u00fcb", "All\u00b7t\u00e4g\u00b7li\u00b7ches", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "$,", "ART", "ADJD", "KON", "ADJD", "ADJA", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Dann, eh' den hellen Sinn der Tr\u00fcbsinn dir umgraut,", "tokens": ["Dann", ",", "eh'", "den", "hel\u00b7len", "Sinn", "der", "Tr\u00fcb\u00b7sinn", "dir", "um\u00b7graut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "ART", "ADJA", "NN", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Wahnsinn, auf und fort, soweit der Himmel blaut!", "tokens": ["Der", "Wahn\u00b7sinn", ",", "auf", "und", "fort", ",", "so\u00b7weit", "der", "Him\u00b7mel", "blaut", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "KON", "PTKVZ", "$,", "KOUS", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Und schaue dich nach dem nicht um, dem du entrennst,", "tokens": ["Und", "schau\u00b7e", "dich", "nach", "dem", "nicht", "um", ",", "dem", "du", "en\u00b7trennst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "PTKNEG", "PTKVZ", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du m\u00f6chtest sonst dir nach beschw\u00f6ren das Gespenst.", "tokens": ["Du", "m\u00f6ch\u00b7test", "sonst", "dir", "nach", "be\u00b7schw\u00f6\u00b7ren", "das", "Ge\u00b7spenst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PPER", "APPR", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Nicht stille steh, bis du bist weit genug davon,", "tokens": ["Nicht", "stil\u00b7le", "steh", ",", "bis", "du", "bist", "weit", "ge\u00b7nug", "da\u00b7von", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVFIN", "$,", "KOUS", "PPER", "VAFIN", "ADJD", "ADV", "PAV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dann steh, und athme nur, und f\u00fchle dich entflohn.", "tokens": ["Dann", "steh", ",", "und", "ath\u00b7me", "nur", ",", "und", "f\u00fch\u00b7le", "dich", "ent\u00b7flohn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "KON", "VVFIN", "ADV", "$,", "KON", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Blick um! wie hinter dir in blau Ged\u00fcft die Berge", "tokens": ["Blick", "um", "!", "wie", "hin\u00b7ter", "dir", "in", "blau", "Ge\u00b7d\u00fcft", "die", "Ber\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PTKVZ", "$.", "PWAV", "APPR", "PPER", "APPR", "ADJD", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sich h\u00fcllen, so verh\u00fcllt die Ferne Gr\u00fcft' und S\u00e4rge.", "tokens": ["Sich", "h\u00fcl\u00b7len", ",", "so", "ver\u00b7h\u00fcllt", "die", "Fer\u00b7ne", "Gr\u00fcft'", "und", "S\u00e4r\u00b7ge", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVINF", "$,", "ADV", "VVFIN", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Und kehrst du wieder ein, so ist der Dunstkreis rein,", "tokens": ["Und", "kehrst", "du", "wie\u00b7der", "ein", ",", "so", "ist", "der", "Dunst\u00b7kreis", "rein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PTKVZ", "$,", "ADV", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und \u00fcber'm Moder wird das Gras gewachsen seyn.", "tokens": ["Und", "\u00fc\u00b7ber'm", "Mo\u00b7der", "wird", "das", "Gras", "ge\u00b7wach\u00b7sen", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VAFIN", "ART", "NN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}