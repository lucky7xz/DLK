{"textgrid.poem.24282": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Ausz\u00e4hlvers", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es war einmal ein Mann;", "tokens": ["Es", "war", "ein\u00b7mal", "ein", "Mann", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Der hie\u00df Pack-an;", "tokens": ["Der", "hie\u00df", "Pack\u00b7an", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NE", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Pack-an hie\u00df er,", "tokens": ["Pack\u00b7an", "hie\u00df", "er", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Aus den H\u00e4nden lie\u00df er", "tokens": ["Aus", "den", "H\u00e4n\u00b7den", "lie\u00df", "er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Nichts, was er gepackt;", "tokens": ["Nichts", ",", "was", "er", "ge\u00b7packt", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PWS", "PPER", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Hat viel nach Haus gesackt.", "tokens": ["Hat", "viel", "nach", "Haus", "ge\u00b7sackt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und schlo\u00df sodann", "tokens": ["Und", "schlo\u00df", "so\u00b7dann"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Der Herr Pack-an,", "tokens": ["Der", "Herr", "Pack\u00b7an", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Krach, hinter sich die T\u00fcre zu,", "tokens": ["Krach", ",", "hin\u00b7ter", "sich", "die", "T\u00fc\u00b7re", "zu", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "PRF", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Verzehrte seinen Raub in Ruh.", "tokens": ["Ver\u00b7zehr\u00b7te", "sei\u00b7nen", "Raub", "in", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Wer ist der Mann: Ich oder du?", "tokens": ["Wer", "ist", "der", "Mann", ":", "Ich", "o\u00b7der", "du", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "$.", "PPER", "KON", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}