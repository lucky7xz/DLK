{"textgrid.poem.45863": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[ein Fischer sa\u00df im Kahne]", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Fischer sa\u00df im Kahne,", "tokens": ["Ein", "Fi\u00b7scher", "sa\u00df", "im", "Kah\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihm war das Herz so schwer,", "tokens": ["Ihm", "war", "das", "Herz", "so", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sein Liebchen war gestorben,", "tokens": ["Sein", "Lieb\u00b7chen", "war", "ge\u00b7stor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das glaubt' er nimmermehr.", "tokens": ["Das", "glaubt'", "er", "nim\u00b7mer\u00b7mehr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und bis die Sternlein blinken,", "tokens": ["Und", "bis", "die", "Stern\u00b7lein", "blin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und bis zum Mondenschein,", "tokens": ["Und", "bis", "zum", "Mon\u00b7den\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Harrt er sein Lieb zu fahren", "tokens": ["Harrt", "er", "sein", "Lieb", "zu", "fah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wohl auf dem tiefen Rhein.", "tokens": ["Wohl", "auf", "dem", "tie\u00b7fen", "Rhein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Da k\u00f6mmt sie hergegangen", "tokens": ["Da", "k\u00f6mmt", "sie", "her\u00b7ge\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und steiget in den Kahn,", "tokens": ["Und", "stei\u00b7get", "in", "den", "Kahn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie schwanket in den Knien,", "tokens": ["Sie", "schwan\u00b7ket", "in", "den", "Kni\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hat nur ein Hemdlein an.", "tokens": ["Hat", "nur", "ein", "Hemd\u00b7lein", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Sie schwimmen auf den Wellen", "tokens": ["Sie", "schwim\u00b7men", "auf", "den", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hinab in tiefer Ruh',", "tokens": ["Hin\u00b7ab", "in", "tie\u00b7fer", "Ruh'", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da zittert sie und wanket,", "tokens": ["Da", "zit\u00b7tert", "sie", "und", "wan\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "O Liebchen frierest Du?", "tokens": ["O", "Lieb\u00b7chen", "frie\u00b7rest", "Du", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Dein Hemdlein spielt im Winde,", "tokens": ["Dein", "Hemd\u00b7lein", "spielt", "im", "Win\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das Schifflein treibt so schnell;", "tokens": ["Das", "Schif\u00b7flein", "treibt", "so", "schnell", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "H\u00fcll' dich in meinen Mantel,", "tokens": ["H\u00fcll'", "dich", "in", "mei\u00b7nen", "Man\u00b7tel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Nacht ist k\u00fchl und hell.", "tokens": ["Die", "Nacht", "ist", "k\u00fchl", "und", "hell", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Sie strecket nach den Bergen", "tokens": ["Sie", "stre\u00b7cket", "nach", "den", "Ber\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die wei\u00dfen Arme aus,", "tokens": ["Die", "wei\u00b7\u00dfen", "Ar\u00b7me", "aus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und freut sich, wie der Vollmond", "tokens": ["Und", "freut", "sich", ",", "wie", "der", "Voll\u00b7mond"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "$,", "PWAV", "ART", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Aus Wolken sieht heraus.", "tokens": ["Aus", "Wol\u00b7ken", "sieht", "he\u00b7raus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Und gr\u00fc\u00dft die alten T\u00fcrme,", "tokens": ["Und", "gr\u00fc\u00dft", "die", "al\u00b7ten", "T\u00fcr\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und will den hellen Schein,", "tokens": ["Und", "will", "den", "hel\u00b7len", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit ihren zarten Armen,", "tokens": ["Mit", "ih\u00b7ren", "zar\u00b7ten", "Ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Erfassen in dem Rhein.", "tokens": ["Er\u00b7fas\u00b7sen", "in", "dem", "Rhein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "O setze dich doch nieder", "tokens": ["O", "set\u00b7ze", "dich", "doch", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "ADV", "PTKVZ"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Herzallerliebste mein!", "tokens": ["Her\u00b7zal\u00b7ler\u00b7liebs\u00b7te", "mein", "!"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Wasser treibt so schnelle", "tokens": ["Das", "Was\u00b7ser", "treibt", "so", "schnel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "O fall nicht in den Rhein.", "tokens": ["O", "fall", "nicht", "in", "den", "Rhein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PTKNEG", "APPR", "ART", "NE", "$."], "meter": "+-++-+", "measure": "unknown.measure.tetra"}}, "stanza.9": {"line.1": {"text": "Und gro\u00dfe St\u00e4dte fliegen", "tokens": ["Und", "gro\u00b7\u00dfe", "St\u00e4d\u00b7te", "flie\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "An ihrem Kahn vorbei,", "tokens": ["An", "ih\u00b7rem", "Kahn", "vor\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und in den St\u00e4dten klingen", "tokens": ["Und", "in", "den", "St\u00e4d\u00b7ten", "klin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Glocken mancherlei.", "tokens": ["Der", "Glo\u00b7cken", "man\u00b7cher\u00b7lei", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PIS", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Da kniet das M\u00e4dchen nieder", "tokens": ["Da", "kniet", "das", "M\u00e4d\u00b7chen", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und faltet seine H\u00e4nd'", "tokens": ["Und", "fal\u00b7tet", "sei\u00b7ne", "H\u00e4nd'"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und seine hellen Augen", "tokens": ["Und", "sei\u00b7ne", "hel\u00b7len", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Es zu dem Himmel wendt.", "tokens": ["Es", "zu", "dem", "Him\u00b7mel", "wendt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Lieb M\u00e4dchen bete stille,", "tokens": ["Lieb", "M\u00e4d\u00b7chen", "be\u00b7te", "stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Schwank' nicht so hin und her,", "tokens": ["Schwank'", "nicht", "so", "hin", "und", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ADV", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Kahn, er m\u00f6chte sinken,", "tokens": ["Der", "Kahn", ",", "er", "m\u00f6ch\u00b7te", "sin\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das Wasser treibt so sehr.", "tokens": ["Das", "Was\u00b7ser", "treibt", "so", "sehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "In einem Nonnenkloster", "tokens": ["In", "ei\u00b7nem", "Non\u00b7nen\u00b7klos\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da singen Stimmen fein", "tokens": ["Da", "sin\u00b7gen", "Stim\u00b7men", "fein"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und in dem Kirchenfenster", "tokens": ["Und", "in", "dem", "Kir\u00b7chen\u00b7fens\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sieht man den Kerzenschein.", "tokens": ["Sieht", "man", "den", "Ker\u00b7zen\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.13": {"line.1": {"text": "Da singt das M\u00e4dchen helle", "tokens": ["Da", "singt", "das", "M\u00e4d\u00b7chen", "hel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Metten in dem Kahn,", "tokens": ["Die", "Met\u00b7ten", "in", "dem", "Kahn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und sieht dabei mit Tr\u00e4nen", "tokens": ["Und", "sieht", "da\u00b7bei", "mit", "Tr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PAV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den Fischerknaben an.", "tokens": ["Den", "Fi\u00b7scher\u00b7kna\u00b7ben", "an", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Der Knabe singt mit Tr\u00e4nen", "tokens": ["Der", "Kna\u00b7be", "singt", "mit", "Tr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Metten in dem Kahn,", "tokens": ["Die", "Met\u00b7ten", "in", "dem", "Kahn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und sieht dabei sein M\u00e4dchen", "tokens": ["Und", "sieht", "da\u00b7bei", "sein", "M\u00e4d\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PAV", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit stummen Blicken an.", "tokens": ["Mit", "stum\u00b7men", "Bli\u00b7cken", "an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "So rot und immer r\u00f6ter", "tokens": ["So", "rot", "und", "im\u00b7mer", "r\u00f6\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KON", "ADV", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wird nun die tiefe Flut,", "tokens": ["Wird", "nun", "die", "tie\u00b7fe", "Flut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wei\u00df und immer wei\u00dfer", "tokens": ["Und", "wei\u00df", "und", "im\u00b7mer", "wei\u00b7\u00dfer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KON", "ADV", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das M\u00e4dchen werden tut.", "tokens": ["Das", "M\u00e4d\u00b7chen", "wer\u00b7den", "tut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Der Mond ist schon zerronnen,", "tokens": ["Der", "Mond", "ist", "schon", "zer\u00b7ron\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Kein Sternlein mehr zu sehn,", "tokens": ["Kein", "Stern\u00b7lein", "mehr", "zu", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und auch dem lieben M\u00e4dchen", "tokens": ["Und", "auch", "dem", "lie\u00b7ben", "M\u00e4d\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Augen schon vergehn.", "tokens": ["Die", "Au\u00b7gen", "schon", "ver\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.17": {"line.1": {"text": "Lieb M\u00e4dchen guten Morgen!", "tokens": ["Lieb", "M\u00