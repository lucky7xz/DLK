{"dta.poem.7046": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Entwurff einiger Betrachtungen,  \n \u00fcber den  \n  Zustand der Seeligen.  \n Beym Anfang des 1727sten Jahres.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es stellten sich nicht nur sehr viele Heyden,", "tokens": ["Es", "stell\u00b7ten", "sich", "nicht", "nur", "sehr", "vie\u00b7le", "Hey\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PTKNEG", "ADV", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Viel Juden noch anitzt, viel T\u00fcrcken eben auch,", "tokens": ["Viel", "Ju\u00b7den", "noch", "a\u00b7nitzt", ",", "viel", "T\u00fcr\u00b7cken", "e\u00b7ben", "auch", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "ADV", "$,", "PIAT", "NN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Recht l\u00e4cherliche Ding\u2019 und seltsam s\u00fcsse Freuden", "tokens": ["Recht", "l\u00e4\u00b7cher\u00b7li\u00b7che", "Ding'", "und", "selt\u00b7sam", "s\u00fcs\u00b7se", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADJA", "NN", "KON", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Von jenem Leben vor. Es ist so gar der Brauch", "tokens": ["Von", "je\u00b7nem", "Le\u00b7ben", "vor", ".", "Es", "ist", "so", "gar", "der", "Brauch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "PTKVZ", "$.", "PPER", "VAFIN", "ADV", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Auch in der Christenheit, da\u00df, leider! offtermal", "tokens": ["Auch", "in", "der", "Chris\u00b7ten\u00b7heit", ",", "da\u00df", ",", "lei\u00b7der", "!", "off\u00b7ter\u00b7mal"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "KOUS", "$,", "ADV", "$.", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Recht l\u00e4ppische Gedancken, Fratzen, Grillen,", "tokens": ["Recht", "l\u00e4p\u00b7pi\u00b7sche", "Ge\u00b7dan\u00b7cken", ",", "Frat\u00b7zen", ",", "Gril\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Von jener Welt, die meisten K\u00f6pffe f\u00fcllen.", "tokens": ["Von", "je\u00b7ner", "Welt", ",", "die", "meis\u00b7ten", "K\u00f6pf\u00b7fe", "f\u00fcl\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "PRELS", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Da stellt man mehrentheils sich einen sch\u00f6nen Saal,", "tokens": ["Da", "stellt", "man", "meh\u00b7ren\u00b7theils", "sich", "ei\u00b7nen", "sch\u00f6\u00b7nen", "Saal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Vortrefflich ausgeputzt, voll Paucken und Trompeten,", "tokens": ["Vor\u00b7treff\u00b7lich", "aus\u00b7ge\u00b7putzt", ",", "voll", "Pau\u00b7cken", "und", "Trom\u00b7pe\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+--++-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Schallmeyen, Orgelwerck, Chytarren, Lauten, Fl\u00f6hten,", "tokens": ["Schall\u00b7me\u00b7yen", ",", "Or\u00b7gel\u00b7werck", ",", "Chy\u00b7tar\u00b7ren", ",", "Lau\u00b7ten", ",", "Fl\u00f6h\u00b7ten", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.11": {"text": "Rebst einem reichen Tisch, voll niedlicher Gerichte,", "tokens": ["Rebst", "ei\u00b7nem", "rei\u00b7chen", "Tisch", ",", "voll", "nied\u00b7li\u00b7cher", "Ge\u00b7rich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Voll aller Schleckerey, voll auserlesner Fr\u00fcchte,", "tokens": ["Voll", "al\u00b7ler", "Schle\u00b7cke\u00b7rey", ",", "voll", "au\u00b7ser\u00b7les\u00b7ner", "Fr\u00fcch\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "PIAT", "NN", "$,", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Jm Sinne so, wie in den Worten, dar,", "tokens": ["Jm", "Sin\u00b7ne", "so", ",", "wie", "in", "den", "Wor\u00b7ten", ",", "dar", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "$,", "PWAV", "APPR", "ART", "NN", "$,", "PTKVZ", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.14": {"text": "Und was der Freuden mehr. Ich sch\u00e4me mich f\u00fcrwahr,", "tokens": ["Und", "was", "der", "Freu\u00b7den", "mehr", ".", "Ich", "sch\u00e4\u00b7me", "mich", "f\u00fcr\u00b7wahr", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "ADV", "$.", "PPER", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Da\u00df man sich nicht entsieht,", "tokens": ["Da\u00df", "man", "sich", "nicht", "ent\u00b7sieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PRF", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.16": {"text": "So fleischlichen Begriff, von so erhabnen Dingen,", "tokens": ["So", "fleischli\u00b7chen", "Be\u00b7griff", ",", "von", "so", "er\u00b7hab\u00b7nen", "Din\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.17": {"text": "Die GOTT verkleinerlich, sich selber beyzubringen.", "tokens": ["Die", "GoTT", "ver\u00b7klei\u00b7ner\u00b7lich", ",", "sich", "sel\u00b7ber", "bey\u00b7zu\u00b7brin\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "PRF", "ADV", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}