{"textgrid.poem.65691": {"metadata": {"author": {"name": "Drollinger, Carl Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sch\u00f6pfer! der du alles kennest,", "genre": "verse", "period": "N.A.", "pub_year": 1715, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6pfer! der du alles kennest,", "tokens": ["Sch\u00f6p\u00b7fer", "!", "der", "du", "al\u00b7les", "ken\u00b7nest", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "PPER", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was in meinem Herzen ruht:", "tokens": ["Was", "in", "mei\u00b7nem", "Her\u00b7zen", "ruht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der du es mit Namen nennest,", "tokens": ["Der", "du", "es", "mit", "Na\u00b7men", "nen\u00b7nest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.4": {"text": "Ob es irrig, ob es gut:", "tokens": ["Ob", "es", "ir\u00b7rig", ",", "ob", "es", "gut", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "$,", "KOUS", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Schaue, was f\u00fcr Dunkelheiten", "tokens": ["Schau\u00b7e", ",", "was", "f\u00fcr", "Dun\u00b7kel\u00b7hei\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "PRELS", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sich durch meine Seele breiten;", "tokens": ["Sich", "durch", "mei\u00b7ne", "See\u00b7le", "brei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und gebiete deinem Licht,", "tokens": ["Und", "ge\u00b7bie\u00b7te", "dei\u00b7nem", "Licht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Da\u00df es meine N\u00e4chte bricht!", "tokens": ["Da\u00df", "es", "mei\u00b7ne", "N\u00e4ch\u00b7te", "bricht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Herr! ich bin mir selbst verborgen.", "tokens": ["Herr", "!", "ich", "bin", "mir", "selbst", "ver\u00b7bor\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wem ist wol sein Herze kund?", "tokens": ["Wem", "ist", "wol", "sein", "Her\u00b7ze", "kund", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "VAINF", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Tausendmal in Einem Morgen", "tokens": ["Tau\u00b7send\u00b7mal", "in", "Ei\u00b7nem", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aendert sich sein innrer Grund.", "tokens": ["A\u00b7en\u00b7dert", "sich", "sein", "inn\u00b7rer", "Grund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.5": {"text": "Tr\u00e4ume, die kein Wesen halten,", "tokens": ["Tr\u00e4u\u00b7me", ",", "die", "kein", "We\u00b7sen", "hal\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ungewisse Scheingestalten,", "tokens": ["Un\u00b7ge\u00b7wis\u00b7se", "Schein\u00b7ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Licht und Schatten, Wahn und Witz", "tokens": ["Licht", "und", "Schat\u00b7ten", ",", "Wahn", "und", "Witz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "KON", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Wechseln stets in seinem Sitz.", "tokens": ["Wech\u00b7seln", "stets", "in", "sei\u00b7nem", "Sitz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Dennoch richt ich mein Beginnen", "tokens": ["Den\u00b7noch", "richt", "ich", "mein", "Be\u00b7gin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nur nach eignem D\u00fcnkel ein,", "tokens": ["Nur", "nach", "eig\u00b7nem", "D\u00fcn\u00b7kel", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und es wollen meine Sinnen", "tokens": ["Und", "es", "wol\u00b7len", "mei\u00b7ne", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kl\u00fcger, als du selbsten, seyn.", "tokens": ["Kl\u00fc\u00b7ger", ",", "als", "du", "selbs\u00b7ten", ",", "seyn", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "VVFIN", "$,", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "O welch d\u00f6hrichtes Gesch\u00f6pfe!", "tokens": ["O", "welch", "d\u00f6h\u00b7rich\u00b7tes", "Ge\u00b7sch\u00f6p\u00b7fe", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Haben denn die schn\u00f6den T\u00f6pfe,", "tokens": ["Ha\u00b7ben", "denn", "die", "schn\u00f6\u00b7den", "T\u00f6p\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Die ein leichter Streich zerst\u00f6rt,", "tokens": ["Die", "ein", "leich\u00b7ter", "Streich", "zer\u00b7st\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Ihren Sch\u00f6pfer je gelehrt?", "tokens": ["Ih\u00b7ren", "Sch\u00f6p\u00b7fer", "je", "ge\u00b7lehrt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Nun! ich wei\u00df durch Tausend Proben,", "tokens": ["Nun", "!", "ich", "wei\u00df", "durch", "Tau\u00b7send", "Pro\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PPER", "VVFIN", "APPR", "CARD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df mein Tuhn mich stets betriegt,", "tokens": ["Da\u00df", "mein", "Tuhn", "mich", "stets", "be\u00b7triegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bis die hohe Macht von oben", "tokens": ["Bis", "die", "ho\u00b7he", "Macht", "von", "o\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich zu meiner Schwachheit f\u00fcgt.", "tokens": ["Sich", "zu", "mei\u00b7ner", "Schwach\u00b7heit", "f\u00fcgt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Drum, o einig-weyses Wesen,", "tokens": ["Drum", ",", "o", "ei\u00b7nig\u00b7wey\u00b7ses", "We\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "FM", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sorge du f\u00fcr mein Genesen;", "tokens": ["Sor\u00b7ge", "du", "f\u00fcr", "mein", "Ge\u00b7ne\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Und er\u00f6ffne deinem Knecht,", "tokens": ["Und", "er\u00b7\u00f6ff\u00b7ne", "dei\u00b7nem", "Knecht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Was verboten, was gerecht!", "tokens": ["Was", "ver\u00b7bo\u00b7ten", ",", "was", "ge\u00b7recht", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "PRELS", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Schau herab von deinen H\u00f6hen,", "tokens": ["Schau", "her\u00b7ab", "von", "dei\u00b7nen", "H\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und erforsche meinen Gang!", "tokens": ["Und", "er\u00b7for\u00b7sche", "mei\u00b7nen", "Gang", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sihest du mich irre gehen,", "tokens": ["Si\u00b7hest", "du", "mich", "ir\u00b7re", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "O so warte nicht zu lang!", "tokens": ["O", "so", "war\u00b7te", "nicht", "zu", "lang", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "PTKNEG", "PTKA", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Reisse mich mit starken Armen", "tokens": ["Reis\u00b7se", "mich", "mit", "star\u00b7ken", "Ar\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PRF", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Durch dein heiliges Erbarmen", "tokens": ["Durch", "dein", "hei\u00b7li\u00b7ges", "Er\u00b7bar\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Von dem Weg, den ich ber\u00fchrt,", "tokens": ["Von", "dem", "Weg", ",", "den", "ich", "be\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Eh er mich zur H\u00f6llen f\u00fchrt!", "tokens": ["Eh", "er", "mich", "zur", "H\u00f6l\u00b7len", "f\u00fchrt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Herr! ich f\u00fchle deine St\u00e4rke,", "tokens": ["Herr", "!", "ich", "f\u00fch\u00b7le", "dei\u00b7ne", "St\u00e4r\u00b7ke", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die mir Licht und Trost verhei\u00dft.", "tokens": ["Die", "mir", "Licht", "und", "Trost", "ver\u00b7hei\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Deiner Gnade Wunderwerke", "tokens": ["Dei\u00b7ner", "Gna\u00b7de", "Wun\u00b7der\u00b7wer\u00b7ke"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wirken schon in meinem Geist.", "tokens": ["Wir\u00b7ken", "schon", "in", "mei\u00b7nem", "Geist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ja du willt, du willt mich f\u00fchren,", "tokens": ["Ja", "du", "willt", ",", "du", "willt", "mich", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "VMFIN", "$,", "PPER", "VMFIN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wenn sich Bahn und Tag verlieren.", "tokens": ["Wenn", "sich", "Bahn", "und", "Tag", "ver\u00b7lie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Deine Rechte leitet mich;", "tokens": ["Dei\u00b7ne", "Rech\u00b7te", "lei\u00b7tet", "mich", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Und ich gehe sicherlich.", "tokens": ["Und", "ich", "ge\u00b7he", "si\u00b7cher\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "O wie selig, welcher immer", "tokens": ["O", "wie", "se\u00b7lig", ",", "wel\u00b7cher", "im\u00b7mer"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NE", "KOKOM", "ADJD", "$,", "PRELS", "ADV"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Unter deiner F\u00fchrung steht!", "tokens": ["Un\u00b7ter", "dei\u00b7ner", "F\u00fch\u00b7rung", "steht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00f6chster F\u00fchrer, weiche nimmer,", "tokens": ["H\u00f6chs\u00b7ter", "F\u00fch\u00b7rer", ",", "wei\u00b7che", "nim\u00b7mer", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bis mein Lauff zum Ende geht!", "tokens": ["Bis", "mein", "Lauff", "zum", "En\u00b7de", "geht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Le\u00fcchte meinem Angesichte,", "tokens": ["Le\u00fcch\u00b7te", "mei\u00b7nem", "An\u00b7ge\u00b7sich\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bis ich einst in vollem Lichte,", "tokens": ["Bis", "ich", "einst", "in", "vol\u00b7lem", "Lich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Frey von irrtumsreichem Wahn", "tokens": ["Frey", "von", "irr\u00b7tums\u00b7rei\u00b7chem", "Wahn"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Deine Gottheit schauen kan!", "tokens": ["Dei\u00b7ne", "Got\u00b7theit", "schau\u00b7en", "kan", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}