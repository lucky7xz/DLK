{"textgrid.poem.41556": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Am siebenten Sonntage nach Pfingsten", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo bist du, der noch unvers\u00f6hnt mit mir?", "tokens": ["Wo", "bist", "du", ",", "der", "noch", "un\u00b7ver\u00b7s\u00f6hnt", "mit", "mir", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "$,", "PRELS", "ADV", "ADJD", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Gern will ich, freudig, meine Hand dir reichen.", "tokens": ["Gern", "will", "ich", ",", "freu\u00b7dig", ",", "mei\u00b7ne", "Hand", "dir", "rei\u00b7chen", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "$,", "ADJD", "$,", "PPOSAT", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nicht wei\u00df ich es, was ich verbrach an dir;", "tokens": ["Nicht", "wei\u00df", "ich", "es", ",", "was", "ich", "ver\u00b7brach", "an", "dir", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPER", "PPER", "$,", "PWS", "PPER", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Verschwunden alte Zeiten, alte Zeichen.", "tokens": ["Ver\u00b7schwun\u00b7den", "al\u00b7te", "Zei\u00b7ten", ",", "al\u00b7te", "Zei\u00b7chen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Zerronnen sind mir Jahre wie ein Traum", "tokens": ["Zer\u00b7ron\u00b7nen", "sind", "mir", "Jah\u00b7re", "wie", "ein", "Traum"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "NN", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und r\u00fcckw\u00e4rts wend' ich die Gedanken kaum", "tokens": ["Und", "r\u00fcck\u00b7w\u00e4rts", "wend'", "ich", "die", "Ge\u00b7dan\u00b7ken", "kaum"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Zu Bildern, die wie Wolkenschatten bleichen.", "tokens": ["Zu", "Bil\u00b7dern", ",", "die", "wie", "Wol\u00b7ken\u00b7schat\u00b7ten", "blei\u00b7chen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "KOKOM", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Aus harter Not und manchem bittern Kampf", "tokens": ["Aus", "har\u00b7ter", "Not", "und", "man\u00b7chem", "bit\u00b7tern", "Kampf"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ist mir ein neues Leben aufgegangen.", "tokens": ["Ist", "mir", "ein", "neu\u00b7es", "Le\u00b7ben", "auf\u00b7ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Kein freudiges; der hei\u00dfe innre Krampf", "tokens": ["Kein", "freu\u00b7di\u00b7ges", ";", "der", "hei\u00b7\u00dfe", "inn\u00b7re", "Krampf"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "$.", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Entz\u00fcndet sich von au\u00dfen nicht befangen;", "tokens": ["Ent\u00b7z\u00fcn\u00b7det", "sich", "von", "au\u00b7\u00dfen", "nicht", "be\u00b7fan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Der Blick nach innen bohrend mit Gewalt", "tokens": ["Der", "Blick", "nach", "in\u00b7nen", "boh\u00b7rend", "mit", "Ge\u00b7walt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADV", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Kann tiefer tiefer in den dunkeln Spalt", "tokens": ["Kann", "tie\u00b7fer", "tie\u00b7fer", "in", "den", "dun\u00b7keln", "Spalt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADJD", "ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Der lang verharschten Wunden nun gelangen.", "tokens": ["Der", "lang", "ver\u00b7harschten", "Wun\u00b7den", "nun", "ge\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "ADV", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Was mich bewegt, es ist dahin, verweht;", "tokens": ["Was", "mich", "be\u00b7wegt", ",", "es", "ist", "da\u00b7hin", ",", "ver\u00b7weht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "PPER", "VAFIN", "PAV", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Geschieden l\u00e4ngst, die einst zusammentrafen.", "tokens": ["Ge\u00b7schie\u00b7den", "l\u00e4ngst", ",", "die", "einst", "zu\u00b7sam\u00b7ment\u00b7ra\u00b7fen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "PRELS", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und wie ein Schiff, das \u00fcberm Meere steht,", "tokens": ["Und", "wie", "ein", "Schiff", ",", "das", "\u00fc\u00b7berm", "Mee\u00b7re", "steht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Vergessend ganz den einst verla\u00dfnen Hafen,", "tokens": ["Ver\u00b7ges\u00b7send", "ganz", "den", "einst", "ver\u00b7la\u00df\u00b7nen", "Ha\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "ADV", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "La\u00df ich das Senkblei zitternd auf den Grund", "tokens": ["La\u00df", "ich", "das", "Senk\u00b7blei", "zit\u00b7ternd", "auf", "den", "Grund"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ART", "NN", "VVPP", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Zu forschen, wo die Seele krank und wund,", "tokens": ["Zu", "for\u00b7schen", ",", "wo", "die", "See\u00b7le", "krank", "und", "wund", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PWAV", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wo, wehe, die verborgnen Klippen schlafen.", "tokens": ["Wo", ",", "we\u00b7he", ",", "die", "ver\u00b7borg\u00b7nen", "Klip\u00b7pen", "schla\u00b7fen", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "ADJD", "$,", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ach, kann ich denn vollbrachte Dinge so", "tokens": ["Ach", ",", "kann", "ich", "denn", "voll\u00b7brach\u00b7te", "Din\u00b7ge", "so"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "VMFIN", "PPER", "ADV", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Gleich dem verbrauchten Mantel von mir streifen?", "tokens": ["Gleich", "dem", "ver\u00b7brauch\u00b7ten", "Man\u00b7tel", "von", "mir", "strei\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wird einer selbst nur seiner Trauer froh,", "tokens": ["Wird", "ei\u00b7ner", "selbst", "nur", "sei\u00b7ner", "Trau\u00b7er", "froh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADV", "ADV", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wo tausend kleine Fasern nach ihm greifen", "tokens": ["Wo", "tau\u00b7send", "klei\u00b7ne", "Fa\u00b7sern", "nach", "ihm", "grei\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "CARD", "ADJA", "NN", "APPR", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Der Wucherpflanze, so er ausges\u00e4t,", "tokens": ["Der", "Wu\u00b7cher\u00b7pflan\u00b7ze", ",", "so", "er", "aus\u00b7ge\u00b7s\u00e4t", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Wenn \u00fcberall des Fluches Ernte steht,", "tokens": ["Wenn", "\u00fc\u00b7be\u00b7rall", "des", "Flu\u00b7ches", "Ern\u00b7te", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "All\u00fcberall die irren Seufzer schweifen?", "tokens": ["Al\u00b7l\u00fc\u00b7be\u00b7rall", "die", "ir\u00b7ren", "Seuf\u00b7zer", "schwei\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "O r\u00fcttle dich, schlag deine Augen auf!", "tokens": ["O", "r\u00fctt\u00b7le", "dich", ",", "schlag", "dei\u00b7ne", "Au\u00b7gen", "auf", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$,", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Noch einmal mu\u00dft du sie nach au\u00dfen wenden,", "tokens": ["Noch", "ein\u00b7mal", "mu\u00dft", "du", "sie", "nach", "au\u00b7\u00dfen", "wen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "PPER", "APPR", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mu\u00dft sehn den Quell als wilden Stromes Lauf,", "tokens": ["Mu\u00dft", "sehn", "den", "Quell", "als", "wil\u00b7den", "Stro\u00b7mes", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "ART", "NN", "KOUS", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Den aufgegraben du mit deinen H\u00e4nden,", "tokens": ["Den", "auf\u00b7ge\u00b7gra\u00b7ben", "du", "mit", "dei\u00b7nen", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und wo er ward ged\u00e4mmt durch Gottes Huld,", "tokens": ["Und", "wo", "er", "ward", "ge\u00b7d\u00e4mmt", "durch", "Got\u00b7tes", "Huld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VAFIN", "VVPP", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Da schlag an deine Brust in deiner Schuld", "tokens": ["Da", "schlag", "an", "dei\u00b7ne", "Brust", "in", "dei\u00b7ner", "Schuld"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und w\u00e4hne nicht du k\u00f6nntest was vollenden.", "tokens": ["Und", "w\u00e4h\u00b7ne", "nicht", "du", "k\u00f6nn\u00b7test", "was", "voll\u00b7en\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "PPER", "VMFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Ja wend' ich meine Blicke nur zur\u00fcck,", "tokens": ["Ja", "wend'", "ich", "mei\u00b7ne", "Bli\u00b7cke", "nur", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "VVFIN", "PPER", "PPOSAT", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dann wei\u00df ich, wo ich mu\u00df um Gnade flehen,", "tokens": ["Dann", "wei\u00df", "ich", ",", "wo", "ich", "mu\u00df", "um", "Gna\u00b7de", "fle\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "PPER", "VMFIN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wo schuldig ich das eigne Lebensgl\u00fcck", "tokens": ["Wo", "schul\u00b7dig", "ich", "das", "eig\u00b7ne", "Le\u00b7bens\u00b7gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Zu tauschen gegen fremder Seele Wehen;", "tokens": ["Zu", "tau\u00b7schen", "ge\u00b7gen", "frem\u00b7der", "See\u00b7le", "We\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Dann wei\u00df ich wohl, wer mir noch unvers\u00f6hnt", "tokens": ["Dann", "wei\u00df", "ich", "wohl", ",", "wer", "mir", "noch", "un\u00b7ver\u00b7s\u00f6hnt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "PWS", "PPER", "ADV", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Vielleicht die dargebotne Rechte h\u00f6hnt,", "tokens": ["Viel\u00b7leicht", "die", "dar\u00b7ge\u00b7bot\u00b7ne", "Rech\u00b7te", "h\u00f6hnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Mich nach Verdienst l\u00e4\u00dft ungetr\u00f6stet gehen.", "tokens": ["Mich", "nach", "Ver\u00b7dienst", "l\u00e4\u00dft", "un\u00b7ge\u00b7tr\u00f6s\u00b7tet", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "VVFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Wo ich get\u00e4uscht in Leichtsinn, \u00dcbermut,", "tokens": ["Wo", "ich", "ge\u00b7t\u00e4uscht", "in", "Leicht\u00b7sinn", ",", "\u00dc\u00b7ber\u00b7mut", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "PPER", "VVPP", "APPR", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dort mag man mir vielleicht zuerst vergeben;", "tokens": ["Dort", "mag", "man", "mir", "viel\u00b7leicht", "zu\u00b7erst", "ver\u00b7ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch wo vergiftet ward ein reines Blut", "tokens": ["Doch", "wo", "ver\u00b7gif\u00b7tet", "ward", "ein", "rei\u00b7nes", "Blut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "VVPP", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein fremdem Beispiel hingegebnes Leben:", "tokens": ["Ein", "frem\u00b7dem", "Bei\u00b7spiel", "hin\u00b7ge\u00b7geb\u00b7nes", "Le\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Da liegt der Stein, den meine s\u00fcnd'ge Hand", "tokens": ["Da", "liegt", "der", "Stein", ",", "den", "mei\u00b7ne", "s\u00fcn\u00b7d'\u00b7ge", "Hand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PRELS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.6": {"text": "In Schwung zu setzen, ach, nur zu gewandt,", "tokens": ["In", "Schwung", "zu", "set\u00b7zen", 