{"textgrid.poem.60053": {"metadata": {"author": {"name": "Jacobi, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ihr Sch\u00e4ferinnen alle! seht,", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr Sch\u00e4ferinnen alle! seht,", "tokens": ["Ihr", "Sch\u00e4\u00b7fe\u00b7rin\u00b7nen", "al\u00b7le", "!", "seht", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "PIAT", "$.", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie da mein liebes M\u00e4dchen geht!", "tokens": ["Wie", "da", "mein", "lie\u00b7bes", "M\u00e4d\u00b7chen", "geht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie so von ganzer Seele", "tokens": ["Wie", "so", "von", "gan\u00b7zer", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dem Himmel und der Erde gut!", "tokens": ["Dem", "Him\u00b7mel", "und", "der", "Er\u00b7de", "gut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mit Rosen kr\u00e4nzt es seinen Hut,", "tokens": ["Mit", "Ro\u00b7sen", "kr\u00e4nzt", "es", "sei\u00b7nen", "Hut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und singt, wie Philomele.", "tokens": ["Und", "singt", ",", "wie", "Phi\u00b7lo\u00b7me\u00b7le", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "So geht das M\u00e4dchen allezeit,", "tokens": ["So", "geht", "das", "M\u00e4d\u00b7chen", "al\u00b7le\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vergn\u00fcgt mit Wenigem, bereit,", "tokens": ["Ver\u00b7gn\u00fcgt", "mit", "We\u00b7ni\u00b7gem", ",", "be\u00b7reit", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "APPR", "PIS", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auch dieses noch zu missen,", "tokens": ["Auch", "die\u00b7ses", "noch", "zu", "mis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wenn, irgend in der Gottes-Welt,", "tokens": ["Wenn", ",", "ir\u00b7gend", "in", "der", "Got\u00b7tes\u00b7Welt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es nur ein Pl\u00e4tzchen frey beh\u00e4lt,", "tokens": ["Es", "nur", "ein", "Pl\u00e4tz\u00b7chen", "frey", "be\u00b7h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zum Tanzen und zum K\u00fcssen.", "tokens": ["Zum", "Tan\u00b7zen", "und", "zum", "K\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}