{"textgrid.poem.24138": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Des Zweifels m\u00fcde und von Mi\u00dftraun matt", "tokens": ["Des", "Zwei\u00b7fels", "m\u00fc\u00b7de", "und", "von", "Mi\u00df\u00b7traun", "matt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "KON", "APPR", "NN", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sehn ich mich tief nach Glauben, wie der Mann,", "tokens": ["Sehn", "ich", "mich", "tief", "nach", "Glau\u00b7ben", ",", "wie", "der", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADJD", "APPR", "NN", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der schwer den ganzen Tag die Arme r\u00fchrte, sich", "tokens": ["Der", "schwer", "den", "gan\u00b7zen", "Tag", "die", "Ar\u00b7me", "r\u00fchr\u00b7te", ",", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "ADJD", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$,", "PRF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nach Ruhe sehnt.", "tokens": ["Nach", "Ru\u00b7he", "sehnt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Doch soll es wohl nicht sein.", "tokens": ["Doch", "soll", "es", "wohl", "nicht", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "PTKNEG", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Drum hab ich nur den Augenblick des Gl\u00fccks,", "tokens": ["Drum", "hab", "ich", "nur", "den", "Au\u00b7gen\u00b7blick", "des", "Gl\u00fccks", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Nicht seine Dauer und Beruhigung.", "tokens": ["Nicht", "sei\u00b7ne", "Dau\u00b7er", "und", "Be\u00b7ru\u00b7hi\u00b7gung", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Und alles Holden Grund ist mir verg\u00e4llt.", "tokens": ["Und", "al\u00b7les", "Hol\u00b7den", "Grund", "ist", "mir", "ver\u00b7g\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "So will ich an der Oberfl\u00e4che nur", "tokens": ["So", "will", "ich", "an", "der", "O\u00b7berf\u00b7l\u00e4\u00b7che", "nur"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Vom Quell des Sch\u00f6nen sch\u00f6pfen. Griff ich tiefer, ach,", "tokens": ["Vom", "Quell", "des", "Sch\u00f6\u00b7nen", "sch\u00f6p\u00b7fen", ".", "Griff", "ich", "tie\u00b7fer", ",", "ach", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVINF", "$.", "NN", "PPER", "ADJD", "$,", "ITJ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Es k\u00e4me wieder Schlamm mir in das Glas.", "tokens": ["Es", "k\u00e4\u00b7me", "wie\u00b7der", "Schlamm", "mir", "in", "das", "Glas", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}