{"textgrid.poem.24509": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Uber ein Spinn-Hau\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Diebe machen selbst das Seil/", "tokens": ["Die", "Die\u00b7be", "ma\u00b7chen", "selbst", "das", "Seil", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Huren wird das Spinnen hier zu Theil.", "tokens": ["Den", "Hu\u00b7ren", "wird", "das", "Spin\u00b7nen", "hier", "zu", "Theil", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die ersten/ weil sie stets dem Stricke nachgegangen/", "tokens": ["Die", "ers\u00b7ten", "/", "weil", "sie", "stets", "dem", "Stri\u00b7cke", "nach\u00b7ge\u00b7gan\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$(", "KOUS", "PPER", "ADV", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die andern/ weil sie sich in ihrem Garn gefangen.", "tokens": ["Die", "an\u00b7dern", "/", "weil", "sie", "sich", "in", "ih\u00b7rem", "Garn", "ge\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$(", "KOUS", "PPER", "PRF", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}