{"textgrid.poem.45054": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Fabius Cunctator", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kein Mittel wollte sich f\u00fcgen,", "tokens": ["Kein", "Mit\u00b7tel", "woll\u00b7te", "sich", "f\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "PRF", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Napoleons Macht zu besiegen,", "tokens": ["Na\u00b7po\u00b7le\u00b7ons", "Macht", "zu", "be\u00b7sie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mit List, mit Verrat, mit Macht, mit Geld,", "tokens": ["Mit", "List", ",", "mit", "Ver\u00b7rat", ",", "mit", "Macht", ",", "mit", "Geld", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "$,", "APPR", "NN", "$,", "APPR", "NN", "$,"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Vergebens! er blieb der Meister der Welt.", "tokens": ["Ver\u00b7ge\u00b7bens", "!", "er", "blieb", "der", "Meis\u00b7ter", "der", "Welt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PPER", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Nur Wellington ward einer Gabe froh,", "tokens": ["Nur", "Wel\u00b7ling\u00b7ton", "ward", "ei\u00b7ner", "Ga\u00b7be", "froh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Worin er mit keinem den Rang teilt,", "tokens": ["Wo\u00b7rin", "er", "mit", "kei\u00b7nem", "den", "Rang", "teilt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "PIS", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Und hat mit Erfolg bei Waterloo", "tokens": ["Und", "hat", "mit", "Er\u00b7folg", "bei", "Wa\u00b7ter\u00b7loo"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPR", "NN", "APPR", "NE"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Den Helden zu Tode gelangweilt.", "tokens": ["Den", "Hel\u00b7den", "zu", "To\u00b7de", "ge\u00b7lang\u00b7weilt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}