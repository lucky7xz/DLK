{"textgrid.poem.57545": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Margrete, Sch\u00f6nste der Sch\u00f6nen du,", "tokens": ["Mar\u00b7gre\u00b7te", ",", "Sch\u00f6ns\u00b7te", "der", "Sch\u00f6\u00b7nen", "du", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "ART", "NN", "PPER", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Die jemals mein Sinn begehrt,", "tokens": ["Die", "je\u00b7mals", "mein", "Sinn", "be\u00b7gehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Warum hast du mit kaltem Blick", "tokens": ["Wa\u00b7rum", "hast", "du", "mit", "kal\u00b7tem", "Blick"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.4": {"text": "Mein hei\u00dfes Herz emp\u00f6rt?", "tokens": ["Mein", "hei\u00b7\u00dfes", "Herz", "em\u00b7p\u00f6rt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Was blickt dein Auge so kalt und stolz,", "tokens": ["Was", "blickt", "dein", "Au\u00b7ge", "so", "kalt", "und", "stolz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPOSAT", "NN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wenn meins dir Liebe droht,", "tokens": ["Wenn", "meins", "dir", "Lie\u00b7be", "droht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was bleibt dein Herz so winterlich,", "tokens": ["Was", "bleibt", "dein", "Herz", "so", "win\u00b7ter\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPOSAT", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn mein Herz kocht und loht?", "tokens": ["Wenn", "mein", "Herz", "kocht", "und", "loht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "--++-+", "measure": "anapaest.init"}}, "stanza.3": {"line.1": {"text": "Ich habe nicht Gott noch Menschen gescheut,", "tokens": ["Ich", "ha\u00b7be", "nicht", "Gott", "noch", "Men\u00b7schen", "ge\u00b7scheut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "NN", "ADV", "NN", "VVPP", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mir hat schon als Kind nicht gegraut,", "tokens": ["Mir", "hat", "schon", "als", "Kind", "nicht", "ge\u00b7graut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "KOUS", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Doch Angst umkrallt mein freies Herz,", "tokens": ["Doch", "Angst", "um\u00b7krallt", "mein", "frei\u00b7es", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn du mich angeschaut.", "tokens": ["Wenn", "du", "mich", "an\u00b7ge\u00b7schaut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Du hast mich gebunden, zum Sklaven gemacht,", "tokens": ["Du", "hast", "mich", "ge\u00b7bun\u00b7den", ",", "zum", "Skla\u00b7ven", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVPP", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Die Seele und auch den Leib \u2013", "tokens": ["Die", "See\u00b7le", "und", "auch", "den", "Leib", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ADV", "ART", "NN", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Doch h\u00fct' dich, Margret, da\u00df der Strick nicht zerrei\u00dft,", "tokens": ["Doch", "h\u00fct'", "dich", ",", "Marg\u00b7ret", ",", "da\u00df", "der", "Strick", "nicht", "zer\u00b7rei\u00dft", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "NE", "$,", "KOUS", "ART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Margrete, du bist nur ein Weib!", "tokens": ["Mar\u00b7gre\u00b7te", ",", "du", "bist", "nur", "ein", "Weib", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}