{"dta.poem.124": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Auf den Gebuhrts-Tag seiner verlobten  \n  Julien.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "La\u00df Julia dis Blat zu deinen H\u00e4nden dringen", "tokens": ["La\u00df", "Ju\u00b7lia", "dis", "Blat", "zu", "dei\u00b7nen", "H\u00e4n\u00b7den", "drin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "NE", "PDS", "NN", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "+---+-+-+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Weil heute meine Lust und dein Vergn\u00fcgen lacht:", "tokens": ["Weil", "heu\u00b7te", "mei\u00b7ne", "Lust", "und", "dein", "Ver\u00b7gn\u00fc\u00b7gen", "lacht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dem Lichte mu\u00df mein Geist ein frohes Opffer bringen/", "tokens": ["Dem", "Lich\u00b7te", "mu\u00df", "mein", "Geist", "ein", "fro\u00b7hes", "Opf\u00b7fer", "brin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "An welchen die Natur dich auf die Welt gebracht.", "tokens": ["An", "wel\u00b7chen", "die", "Na\u00b7tur", "dich", "auf", "die", "Welt", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "ART", "NN", "PRF", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "O h\u00f6chstbegl\u00fcckter Tag! du hast ein Meister-St\u00fccke", "tokens": ["O", "h\u00f6chst\u00b7be\u00b7gl\u00fcck\u00b7ter", "Tag", "!", "du", "hast", "ein", "Meis\u00b7ter\u00b7St\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "$.", "PPER", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und meiner W\u00fcnsche-Ziel zum ersten angestrahlt/", "tokens": ["Und", "mei\u00b7ner", "W\u00fcn\u00b7sche\u00b7Ziel", "zum", "ers\u00b7ten", "an\u00b7ge\u00b7strahlt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPRART", "ADJA", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der Himmel schickt durch dich mir seine Gnaden-Blicke/", "tokens": ["Der", "Him\u00b7mel", "schickt", "durch", "dich", "mir", "sei\u00b7ne", "Gna\u00b7den\u00b7Bli\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "PPER", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da ", "tokens": ["Da"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.9": {"text": "Heut\u2019 ist der frohe Tag! es lachet mein Vergn\u00fcgen/", "tokens": ["Heut'", "ist", "der", "fro\u00b7he", "Tag", "!", "es", "la\u00b7chet", "mein", "Ver\u00b7gn\u00fc\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "$.", "PPER", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Heut\u2019 ist dein zartes Bild von der Natur gepr\u00e4gt", "tokens": ["Heut'", "ist", "dein", "zar\u00b7tes", "Bild", "von", "der", "Na\u00b7tur", "ge\u00b7pr\u00e4gt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "APPR", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die holden ", "tokens": ["Die", "hol\u00b7den"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.12": {"text": "So bald ", "tokens": ["So", "bald"], "token_info": ["word", "word"], "pos": ["ADV", "ADV"], "meter": "-+", "measure": "iambic.single"}, "line.13": {"text": "So offte nun der Tag/ dis heute Wiederkommen/", "tokens": ["So", "off\u00b7te", "nun", "der", "Tag", "/", "dis", "heu\u00b7te", "Wie\u00b7der\u00b7kom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "$(", "PDS", "ADV", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "So vielmahls sah man dich recht mit Erstaunen an/", "tokens": ["So", "viel\u00b7mahls", "sah", "man", "dich", "recht", "mit", "Er\u00b7stau\u00b7nen", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PIS", "PRF", "ADJD", "APPR", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Wie deines Leibes-Schmuck so h\u00e4uffig zugenommen", "tokens": ["Wie", "dei\u00b7nes", "Lei\u00b7bes\u00b7Schmuck", "so", "h\u00e4uf\u00b7fig", "zu\u00b7ge\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "ADV", "ADJD", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und wie sich der Verstand so braff hervorgethan.", "tokens": ["Und", "wie", "sich", "der", "Ver\u00b7stand", "so", "braff", "her\u00b7vor\u00b7ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PRF", "ART", "NN", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ich selber wei\u00df die Lust nicht gnugsahm auszusprechen/", "tokens": ["Ich", "sel\u00b7ber", "wei\u00df", "die", "Lust", "nicht", "gnug\u00b7sahm", "aus\u00b7zu\u00b7spre\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ART", "NN", "PTKNEG", "VVFIN", "VVIZU", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Die mir der s\u00fcsse Tag verschiedne mahl geschenckt", "tokens": ["Die", "mir", "der", "s\u00fcs\u00b7se", "Tag", "ver\u00b7schied\u00b7ne", "mahl", "ge\u00b7schenckt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "ADJA", "NN", "VVFIN", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Die frohe Zunge will aus ihren Schrancken brechen", "tokens": ["Die", "fro\u00b7he", "Zun\u00b7ge", "will", "aus", "ih\u00b7ren", "Schran\u00b7cken", "bre\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Sie zeigt die Freude an womit das Hertz umschr\u00e4nckt.", "tokens": ["Sie", "zeigt", "die", "Freu\u00b7de", "an", "wo\u00b7mit", "das", "Hertz", "um\u00b7schr\u00e4nckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PWAV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Kein Balsam findet sich/ der so das Hertz erquicket/", "tokens": ["Kein", "Bal\u00b7sam", "fin\u00b7det", "sich", "/", "der", "so", "das", "Hertz", "er\u00b7quic\u00b7ket", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PRF", "$(", "ART", "ADV", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Kein weisser Perlen-Thau labt mehr das d\u00fcrre Feld/", "tokens": ["Kein", "weis\u00b7ser", "Per\u00b7len\u00b7\u00b7T\u00b7hau", "labt", "mehr", "das", "d\u00fcr\u00b7re", "Feld", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.23": {"text": "Als mich/ mein Engels-Kind/ dein Lippen-Most begl\u00fccket/", "tokens": ["Als", "mich", "/", "mein", "En\u00b7gels\u00b7Kind", "/", "dein", "Lip\u00b7pen\u00b7Most", "be\u00b7gl\u00fc\u00b7cket", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$(", "PPOSAT", "NN", "$(", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Wenn in erw\u00fcnschter Lust mein Geist da Tafel h\u00e4lt.", "tokens": ["Wenn", "in", "er\u00b7w\u00fcnschter", "Lust", "mein", "Geist", "da", "Ta\u00b7fel", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ADJA", "NN", "PPOSAT", "NN", "ADV", "NN", "VVFIN", "$."], "meter": "--+-+-+-+-+", "measure": "anapaest.init"}, "line.25": {"text": "Hat schon der Sch\u00f6nheit-Pracht das Hertze mir ger\u00fchret", "tokens": ["Hat", "schon", "der", "Sch\u00f6n\u00b7heit\u00b7Pracht", "das", "Hert\u00b7ze", "mir", "ge\u00b7r\u00fch\u00b7ret"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "NN", "PDS", "VVFIN", "PPER", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Und ists der erste Zweck/ da\u00df ich dich auserkie\u00dft!", "tokens": ["Und", "ists", "der", "ers\u00b7te", "Zweck", "/", "da\u00df", "ich", "dich", "aus\u00b7er\u00b7kie\u00dft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "ADJA", "NN", "$(", "KOUS", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Doch zeigt das Tugend Gold/ so deine Seele zieret/", "tokens": ["Doch", "zeigt", "das", "Tu\u00b7gend", "Gold", "/", "so", "dei\u00b7ne", "See\u00b7le", "zie\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "$(", "ADV", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Da\u00df du mein Engels-Bild recht \u00fcber irrdisch bist.", "tokens": ["Da\u00df", "du", "mein", "En\u00b7gels\u00b7Bild", "recht", "\u00fc\u00b7ber", "irr\u00b7disch", "bist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "ADJD", "APPR", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Und wenn die Tugend erst den Zunder angeleget/", "tokens": ["Und", "wenn", "die", "Tu\u00b7gend", "erst", "den", "Zun\u00b7der", "an\u00b7ge\u00b7le\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADV", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Da deine Rose sich in ersten Knospen wie\u00df/", "tokens": ["Da", "dei\u00b7ne", "Ro\u00b7se", "sich", "in", "ers\u00b7ten", "Knos\u00b7pen", "wie\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PRF", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "So ist doch diese Gluht in mir noch mehr erreget", "tokens": ["So", "ist", "doch", "die\u00b7se", "Gluht", "in", "mir", "noch", "mehr", "er\u00b7re\u00b7get"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "PDAT", "NN", "APPR", "PPER", "ADV", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Als sich mit voller Pracht ihr Sammtes Haupt auslie\u00df.", "tokens": ["Als", "sich", "mit", "vol\u00b7ler", "Pracht", "ihr", "Samm\u00b7tes", "Haupt", "aus\u00b7lie\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "APPR", "ADJA", "NN", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Das Sehnen qu\u00e4hlet mich die L\u00fcste zugeniessen/", "tokens": ["Das", "Seh\u00b7nen", "qu\u00e4h\u00b7let", "mich", "die", "L\u00fcs\u00b7te", "zu\u00b7ge\u00b7nies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Die mir das falsche Gl\u00fcck noch unvollkommen schenckt/", "tokens": ["Die", "mir", "das", "fal\u00b7sche", "Gl\u00fcck", "noch", "un\u00b7voll\u00b7kom\u00b7men", "schenckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "ADJA", "NN", "ADV", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Ich w\u00fcnsche etwas mehr als den Corall zu k\u00fcssen", "tokens": ["Ich", "w\u00fcn\u00b7sche", "et\u00b7was", "mehr", "als", "den", "Co\u00b7rall", "zu", "k\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PIAT", "KOKOM", "ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Wenn sich mein heisser Mund zu deinen Lippen lenckt.", "tokens": ["Wenn", "sich", "mein", "heis\u00b7ser", "Mund", "zu", "dei\u00b7nen", "Lip\u00b7pen", "lenckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Gar offte pflegt mein Sinn das Schicksahl anzuklagen/", "tokens": ["Gar", "off\u00b7te", "pflegt", "mein", "Sinn", "das", "Schick\u00b7sahl", "an\u00b7zu\u00b7kla\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPOSAT", "NN", "ART", "NN", "VVIZU", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Da\u00df ich um deinen Schatz so lange seuffzen mu\u00df.", "tokens": ["Da\u00df", "ich", "um", "dei\u00b7nen", "Schatz", "so", "lan\u00b7ge", "seuff\u00b7zen", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "ADV", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Die keusche Liebes-Brunst vermehret meine Plagen/", "tokens": ["Die", "keu\u00b7sche", "Lie\u00b7bes\u00b7Brunst", "ver\u00b7meh\u00b7ret", "mei\u00b7ne", "Pla\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Das s\u00fcsse Hoffen zeugt mir schmertzlichen Verdru\u00df.", "tokens": ["Das", "s\u00fcs\u00b7se", "Hof\u00b7fen", "zeugt", "mir", "schmertz\u00b7li\u00b7chen", "Ver\u00b7dru\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Wie aber soll das Licht/ so mir das Leben giebet/", "tokens": ["Wie", "a\u00b7ber", "soll", "das", "Licht", "/", "so", "mir", "das", "Le\u00b7ben", "gie\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VMFIN", "ART", "NN", "$(", "ADV", "PPER", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "In meinen Hertzen seyn ein trauriger ", "tokens": ["In", "mei\u00b7nen", "Hert\u00b7zen", "seyn", "ein", "trau\u00b7ri\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "PPOSAT", "ART", "ADJA"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.43": {"text": "Ach nein! der Himmel selbst/ der reine Liebe liebet/", "tokens": ["Ach", "nein", "!", "der", "Him\u00b7mel", "selbst", "/", "der", "rei\u00b7ne", "Lie\u00b7be", "lie\u00b7bet", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKANT", "$.", "ART", "NN", "ADV", "$(