{"dta.poem.23635": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Dichterklage.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was soll Dichten, was soll Singen,", "tokens": ["Was", "soll", "Dich\u00b7ten", ",", "was", "soll", "Sin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "NN", "$,", "PRELS", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seit es niemand h\u00f6ren mag?", "tokens": ["Seit", "es", "nie\u00b7mand", "h\u00f6\u00b7ren", "mag", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Niemand will nach Freuden ringen,", "tokens": ["Nie\u00b7mand", "will", "nach", "Freu\u00b7den", "rin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Niemand will uns Freude bringen,", "tokens": ["Nie\u00b7mand", "will", "uns", "Freu\u00b7de", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie der Maienbl\u00fcthen-Tag.", "tokens": ["Wie", "der", "Mai\u00b7en\u00b7bl\u00fcthen\u00b7Tag", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Wehe, wehe jedem Herzen,", "tokens": ["We\u00b7he", ",", "we\u00b7he", "je\u00b7dem", "Her\u00b7zen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weil's den Fr\u00fchling so vergisst!", "tokens": ["Weil's", "den", "Fr\u00fch\u00b7ling", "so", "ver\u00b7gisst", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo ist heitre Lust und Scherzen,", "tokens": ["Wo", "ist", "heit\u00b7re", "Lust", "und", "Scher\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Seit die Jugend wie vor Schmerzen", "tokens": ["Seit", "die", "Ju\u00b7gend", "wie", "vor", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "KOKOM", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Stumm und eingewintert ist?", "tokens": ["Stumm", "und", "ein\u00b7ge\u00b7win\u00b7tert", "ist", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Junge Welt, nun tauch dich unter", "tokens": ["Jun\u00b7ge", "Welt", ",", "nun", "tauch", "dich", "un\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "ADV", "VVFIN", "PRF", "APPR"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In den Fr\u00fchlingssonnenschein!", "tokens": ["In", "den", "Fr\u00fch\u00b7lings\u00b7son\u00b7nen\u00b7schein", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sieh' die V\u00f6gel werden munter,", "tokens": ["Sieh'", "die", "V\u00f6\u00b7gel", "wer\u00b7den", "mun\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die Au wird bunt und bunter \u2014", "tokens": ["Und", "die", "Au", "wird", "bunt", "und", "bun\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Soll's f\u00fcr dich nicht Fr\u00fchling sein?", "tokens": ["Soll's", "f\u00fcr", "dich", "nicht", "Fr\u00fch\u00b7ling", "sein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPER", "PTKNEG", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}