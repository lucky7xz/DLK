{"dta.poem.19606": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Das M\u00fcnster.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Des Meisters hohle Wange brennt,", "tokens": ["Des", "Meis\u00b7ters", "hoh\u00b7le", "Wan\u00b7ge", "brennt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie bringen ihm das Sacrament,", "tokens": ["Sie", "brin\u00b7gen", "ihm", "das", "Sa\u00b7cra\u00b7ment", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er i\u00dft des ew'gen Lebens Brot,", "tokens": ["Er", "i\u00dft", "des", "ew'\u00b7gen", "Le\u00b7bens", "Brot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Stubenwinkel grinst der Tod.", "tokens": ["Im", "Stu\u00b7ben\u00b7win\u00b7kel", "grinst", "der", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Fort tr\u00e4gt der Pfaffe die Monstranz.", "tokens": ["Fort", "tr\u00e4gt", "der", "Pfaf\u00b7fe", "die", "Mons\u00b7tranz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "Mit Augen scharf von Fieberglanz", "tokens": ["Mit", "Au\u00b7gen", "scharf", "von", "Fie\u00b7ber\u00b7glanz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Winkt weg der Meister seinem Weibe,", "tokens": ["Winkt", "weg", "der", "Meis\u00b7ter", "sei\u00b7nem", "Wei\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Dem Sohn, dem einz'gen, winkt er: Bleibe!", "tokens": ["Dem", "Sohn", ",", "dem", "einz'\u00b7gen", ",", "winkt", "er", ":", "Blei\u00b7be", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVIZU", "$,", "VVFIN", "PPER", "$.", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Und deutet auf den Eichenschrein:", "tokens": ["Und", "deu\u00b7tet", "auf", "den", "Ei\u00b7chen\u00b7schrein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Was mag da K\u00f6stlich's drinnen sein?", "tokens": ["Was", "mag", "da", "K\u00f6st\u00b7lich's", "drin\u00b7nen", "sein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "NE", "ADV", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Der J\u00fcngling hebt ein Pergament", "tokens": ["Der", "J\u00fcng\u00b7ling", "hebt", "ein", "Per\u00b7ga\u00b7ment"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Aus einer Lade die er kennt", "tokens": ["Aus", "ei\u00b7ner", "La\u00b7de", "die", "er", "kennt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Und breitet auf die Lagerstatt", "tokens": ["Und", "brei\u00b7tet", "auf", "die", "La\u00b7ger\u00b7statt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Ein langsam aufgerolltes Blatt.", "tokens": ["Ein", "lang\u00b7sam", "auf\u00b7ge\u00b7roll\u00b7tes", "Blatt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Da dehnt sich feierlich gewaltig", "tokens": ["Da", "dehnt", "sich", "fei\u00b7er\u00b7lich", "ge\u00b7wal\u00b7tig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ADJD", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Ein M\u00fcnster eins und mannigfaltig", "tokens": ["Ein", "M\u00fcns\u00b7ter", "eins", "und", "man\u00b7nig\u00b7fal\u00b7tig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "CARD", "KON", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Vom obern bis zum untern Rand \u2014", "tokens": ["Vom", "o\u00b7bern", "bis", "zum", "un\u00b7tern", "Rand"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "APPR", "APPRART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Ein Ri\u00df von jugendk\u00fchner Hand.", "tokens": ["Ein", "Ri\u00df", "von", "ju\u00b7gend\u00b7k\u00fch\u00b7ner", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Der Meister sieht am Bret sich stehn", "tokens": ["Der", "Meis\u00b7ter", "sieht", "am", "Bret", "sich", "stehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "PRF", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Und seine Zeichenkohle gehn,", "tokens": ["Und", "sei\u00b7ne", "Zei\u00b7chen\u00b7koh\u00b7le", "gehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Sieht \u00fcber bl\u00fchendfrische Wangen", "tokens": ["Sieht", "\u00fc\u00b7ber", "bl\u00fc\u00b7hend\u00b7fri\u00b7sche", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Verworrne Haare niederhangen \u2014", "tokens": ["Ver\u00b7worr\u00b7ne", "Haa\u00b7re", "nie\u00b7der\u00b7han\u00b7gen"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Und vor dem ersten seiner Pl\u00e4ne", "tokens": ["Und", "vor", "dem", "ers\u00b7ten", "sei\u00b7ner", "Pl\u00e4\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Erstaunt er und zerdr\u00fcckt die Thr\u00e4ne.", "tokens": ["Er\u00b7staunt", "er", "und", "zer\u00b7dr\u00fcckt", "die", "Thr\u00e4\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Auflodern seine Lebensgeister,", "tokens": ["Auf\u00b7lo\u00b7dern", "sei\u00b7ne", "Le\u00b7bens\u00b7geis\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Mit raschen Pulsen spricht der Meister:", "tokens": ["Mit", "ra\u00b7schen", "Pul\u00b7sen", "spricht", "der", "Meis\u00b7ter", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "\u201edies Blatt erweckt den Tag mir wieder,", "tokens": ["\u201e", "dies", "Blatt", "er\u00b7weckt", "den", "Tag", "mir", "wie\u00b7der", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "NN", "VVFIN", "ART", "NN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wo in der Vaterstadt ich nieder", "tokens": ["Wo", "in", "der", "Va\u00b7ter\u00b7stadt", "ich", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "PPER", "PTKVZ"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Gelegt den Stab der Wanderschaft \u2014", "tokens": ["Ge\u00b7legt", "den", "Stab", "der", "Wan\u00b7der\u00b7schaft"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich schritt in voller Jugendkraft.", "tokens": ["Ich", "schritt", "in", "vol\u00b7ler", "Ju\u00b7gend\u00b7kraft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Daheim war ein begeistert Leben,", "tokens": ["Da\u00b7heim", "war", "ein", "be\u00b7geis\u00b7tert", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein M\u00fcnster wollten sie erheben", "tokens": ["Ein", "M\u00fcns\u00b7ter", "woll\u00b7ten", "sie", "er\u00b7he\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Mit andern L\u00e4ndern um die Wette", "tokens": ["Mit", "an\u00b7dern", "L\u00e4n\u00b7dern", "um", "die", "Wet\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und h\u00f6her noch als andre St\u00e4dte,", "tokens": ["Und", "h\u00f6\u00b7her", "noch", "als", "and\u00b7re", "St\u00e4d\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "KOUS", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Gott und den Heil'gen all zum Ruhm,", "tokens": ["Gott", "und", "den", "Heil'\u00b7gen", "all", "zum", "Ruhm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ART", "NN", "PIAT", "APPRART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.10": {"text": "Zur Ehre deutschem B\u00fcrgerthum.", "tokens": ["Zur", "Eh\u00b7re", "deut\u00b7schem", "B\u00fcr\u00b7ger\u00b7thum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Mich lie\u00df auf seine Stube kommen", "tokens": ["Mich", "lie\u00df", "auf", "sei\u00b7ne", "Stu\u00b7be", "kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Der Rath. \u201eLa\u00df, junger Meister, frommen,", "tokens": ["Der", "Rath", ".", "\u201e", "La\u00df", ",", "jun\u00b7ger", "Meis\u00b7ter", ",", "from\u00b7men", ","], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$.", "$(", "VVIMP", "$,", "ADJA", "NN", "$,", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Was du erwandert hast! Wohlan!", "tokens": ["Was", "du", "er\u00b7wan\u00b7dert", "hast", "!", "Wo\u00b7hlan", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "VAFIN", "$.", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Entwirf uns eines M\u00fcnsters Plan!\u201c", "tokens": ["Ent\u00b7wirf", "uns", "ei\u00b7nes", "M\u00fcns\u00b7ters", "Plan", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVIMP", "PPER", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Da sa\u00df ich auf in langen N\u00e4chten,", "tokens": ["Da", "sa\u00df", "ich", "auf", "in", "lan\u00b7gen", "N\u00e4ch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zur Linken standen mir und Rechten,", "tokens": ["Zur", "Lin\u00b7ken", "stan\u00b7den", "mir", "und", "Rech\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Christ mit seiner M\u00e4rtrerschaar,", "tokens": ["Der", "Christ", "mit", "sei\u00b7ner", "M\u00e4r\u00b7trer\u00b7schaar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Kaiser mit den Kronen gar,", "tokens": ["Die", "Kai\u00b7ser", "mit", "den", "Kro\u00b7nen", "gar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Viel reine Fraun und Helden gut,", "tokens": ["Viel", "rei\u00b7ne", "Fraun", "und", "Hel\u00b7den", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "KON", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die nahmen mich in Zucht und Hut,", "tokens": ["Die", "nah\u00b7men", "mich", "in", "Zucht", "und", "Hut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wollt' ich in schwelgendes Verzieren,", "tokens": ["Wollt'", "ich", "in", "schwel\u00b7gen\u00b7des", "Ver\u00b7zie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "In \u00fcppig Blattwerk mich verlieren,", "tokens": ["In", "\u00fcp\u00b7pig", "Blatt\u00b7werk", "mich", "ver\u00b7lie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Und opfert's nicht mit keuschem Sinn", "tokens": ["Und", "op\u00b7fert's", "nicht", "mit", "keu\u00b7schem", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKNEG", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Dem Ganzen streng ich zu Gewinn,", "tokens": ["Dem", "Gan\u00b7zen", "streng", "ich", "zu", "Ge\u00b7winn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Gleich schlug ein altes Heldenbild", "tokens": ["Gleich", "schlug", "ein", "al\u00b7tes", "Hel\u00b7den\u00b7bild"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Erz\u00fcrnt an seinen ehrnen Schild,", "tokens": ["Er\u00b7z\u00fcrnt", "an", "sei\u00b7nen", "ehr\u00b7nen", "Schild", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Den Finger hob (das Haupt von Licht", "tokens": ["Den", "Fin\u00b7ger", "hob", "(", "das", "Haupt", "von", "Licht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$(", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Umrahmt) ein Heil'ger: T\u00e4ndle nicht!", "tokens": ["Um\u00b7rahmt", ")", "ein", "Heil'\u00b7ger", ":", "T\u00e4nd\u00b7le", "nicht", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVPP", "$(", "ART", "NN", "$.", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Das Amt, das dir zu Lehen fiel,", "tokens": ["Das", "Amt", ",", "das", "dir", "zu", "Le\u00b7hen", "fiel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Das ist ein Werk 