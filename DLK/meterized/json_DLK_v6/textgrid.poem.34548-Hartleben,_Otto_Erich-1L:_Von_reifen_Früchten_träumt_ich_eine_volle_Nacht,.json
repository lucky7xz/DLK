{"textgrid.poem.34548": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Von reifen Fr\u00fcchten tr\u00e4umt ich eine volle Nacht,", "genre": "verse", "period": "N.A.", "pub_year": 1897, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von reifen Fr\u00fcchten tr\u00e4umt ich eine volle Nacht,", "tokens": ["Von", "rei\u00b7fen", "Fr\u00fcch\u00b7ten", "tr\u00e4umt", "ich", "ei\u00b7ne", "vol\u00b7le", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "von goldigen im dunkel \u00fcppigen Geb\u00fcsch.", "tokens": ["von", "gol\u00b7di\u00b7gen", "im", "dun\u00b7kel", "\u00fcp\u00b7pi\u00b7gen", "Ge\u00b7b\u00fcsch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "APPRART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Am Berge war es, unter altem Mauerwerk,", "tokens": ["Am", "Ber\u00b7ge", "war", "es", ",", "un\u00b7ter", "al\u00b7tem", "Mau\u00b7er\u00b7werk", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "und Duft und Sonne gl\u00fchten da in Heimlichkeit.", "tokens": ["und", "Duft", "und", "Son\u00b7ne", "gl\u00fch\u00b7ten", "da", "in", "Heim\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVFIN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Von gelben Marmorschwellen rieselte m\u00fcd ein Quell", "tokens": ["Von", "gel\u00b7ben", "Mar\u00b7morsc\u00b7hwel\u00b7len", "rie\u00b7sel\u00b7te", "m\u00fcd", "ein", "Quell"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ADJD", "ART", "NN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "und eine Nymphe lauschte dem leisen Tropfenfall", "tokens": ["und", "ei\u00b7ne", "Nym\u00b7phe", "lauschte", "dem", "lei\u00b7sen", "Tro\u00b7pfen\u00b7fall"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "und fing die k\u00fchlen Perlen mit der offnen Hand ...", "tokens": ["und", "fing", "die", "k\u00fch\u00b7len", "Per\u00b7len", "mit", "der", "off\u00b7nen", "Hand", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Von reifen Fr\u00fcchten tr\u00e4umt ich eine volle Nacht.", "tokens": ["Von", "rei\u00b7fen", "Fr\u00fcch\u00b7ten", "tr\u00e4umt", "ich", "ei\u00b7ne", "vol\u00b7le", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}