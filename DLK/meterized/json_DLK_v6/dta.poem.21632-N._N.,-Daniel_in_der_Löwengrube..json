{"dta.poem.21632": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Daniel in der L\u00f6wengrube.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Und sieh, die Gewaltigen beugen das Haupt,", "tokens": ["Und", "sieh", ",", "die", "Ge\u00b7wal\u00b7ti\u00b7gen", "beu\u00b7gen", "das", "Haupt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ART", "NN", "VVINF", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Es schleifet im Sande die lockige M\u00e4hne,", "tokens": ["Es", "schlei\u00b7fet", "im", "San\u00b7de", "die", "lo\u00b7cki\u00b7ge", "M\u00e4h\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Sie w\u00e4lzen die trotzigen Leiber im Staub \u2014", "tokens": ["Sie", "w\u00e4l\u00b7zen", "die", "trot\u00b7zi\u00b7gen", "Lei\u00b7ber", "im", "Staub"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPRART", "NN", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Ihr Gebr\u00fcll verstummt und horch \u2014 es wird still.", "tokens": ["Ihr", "Ge\u00b7br\u00fcll", "ver\u00b7stummt", "und", "horch", "es", "wird", "still", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "KON", "VVIMP", "$(", "PPER", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Ich f\u00fchle dich Odem des Herrn, du umflie\u00df\u2019t mich.", "tokens": ["Ich", "f\u00fch\u00b7le", "dich", "O\u00b7dem", "des", "Herrn", ",", "du", "um\u00b7flie\u00df't", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "ART", "NN", "$,", "PPER", "VVFIN", "PPER", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.6": {"text": "Erf\u00fcllst diese Schl\u00fcnde, ich sp\u00fcre dein Weh\u2019n.", "tokens": ["Er\u00b7f\u00fcllst", "die\u00b7se", "Schl\u00fcn\u00b7de", ",", "ich", "sp\u00fc\u00b7re", "dein", "Weh'", "n."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "abbreviation"], "pos": ["VVFIN", "PDAT", "NN", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "NE"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "Gn\u00e4diger Vater, o Du mein Gott,", "tokens": ["Gn\u00e4\u00b7di\u00b7ger", "Va\u00b7ter", ",", "o", "Du", "mein", "Gott", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "FM", "PPER", "PPOSAT", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.8": {"text": "Der Du h\u00f6rtest den Schrei des Kindes,", "tokens": ["Der", "Du", "h\u00f6r\u00b7test", "den", "Schrei", "des", "Kin\u00b7des", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.9": {"text": "Mich befreitest von Tod und Verderben", "tokens": ["Mich", "be\u00b7frei\u00b7test", "von", "Tod", "und", "Ver\u00b7der\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.10": {"text": "Danken m\u00f6cht\u2019 ich, wie dank\u2019 ich Dir?", "tokens": ["Dan\u00b7ken", "m\u00f6cht'", "ich", ",", "wie", "dank'", "ich", "Dir", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "$,", "PWAV", "VVFIN", "PPER", "PPER", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.11": {"text": "Ach, wie fa\u00dft\u2019 ich in d\u00fcrftiges Wort", "tokens": ["Ach", ",", "wie", "fa\u00dft'", "ich", "in", "d\u00fcrf\u00b7ti\u00b7ges", "Wort"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PWAV", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.12": {"text": "Meines Herzens br\u00fcnstige F\u00fclle?", "tokens": ["Mei\u00b7nes", "Her\u00b7zens", "br\u00fcns\u00b7ti\u00b7ge", "F\u00fcl\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.13": {"text": "Stumme Zeugen des Menschen-Innern,", "tokens": ["Stum\u00b7me", "Zeu\u00b7gen", "des", "Men\u00b7schen\u00b7In\u00b7nern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ART", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.14": {"text": "Flie\u00dfet Thr\u00e4nen, redet f\u00fcr mich,", "tokens": ["Flie\u00b7\u00dfet", "Thr\u00e4\u00b7nen", ",", "re\u00b7det", "f\u00fcr", "mich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "VVFIN", "APPR", "PPER", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.15": {"text": "Gott-gespendeter, friedlicher Schlaf,", "tokens": ["Got\u00b7tge\u00b7spen\u00b7de\u00b7ter", ",", "fried\u00b7li\u00b7cher", "Schlaf", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.16": {"text": "Schl\u00e4gst du die Fl\u00fcgel um meine Schl\u00e4fen?", "tokens": ["Schl\u00e4gst", "du", "die", "Fl\u00fc\u00b7gel", "um", "mei\u00b7ne", "Schl\u00e4\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.17": {"text": "Gerne sink\u2019 ich in deine Arme \u2014", "tokens": ["Ger\u00b7ne", "sink'", "ich", "in", "dei\u00b7ne", "Ar\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$("], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.18": {"text": "Unheil entschlief, so ruhe auch Du.", "tokens": ["Un\u00b7heil", "ent\u00b7schlief", ",", "so", "ru\u00b7he", "auch", "Du", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "ADV", "VVFIN", "ADV", "PPER", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}}}}}