{"textgrid.poem.60876": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Aufmunterung", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6n ist's, Gro\u00dfes zu tun und Unsterbliches. F\u00fchl es, o J\u00fcngling!", "tokens": ["Sch\u00f6n", "ist's", ",", "Gro\u00b7\u00dfes", "zu", "tun", "und", "U\u00b7nsterb\u00b7li\u00b7ches", ".", "F\u00fchl", "es", ",", "o", "J\u00fcng\u00b7ling", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "NN", "PTKZU", "VVINF", "KON", "NN", "$.", "NN", "PPER", "$,", "FM", "NN", "$."], "meter": "+-+--+-+---+--+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Fr\u00fch von der Stirn m\u00fchvoll rinne der m\u00e4nnliche Schwei\u00df!", "tokens": ["Fr\u00fch", "von", "der", "Stirn", "m\u00fch\u00b7voll", "rin\u00b7ne", "der", "m\u00e4nn\u00b7li\u00b7che", "Schwei\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.3": {"text": "Aber vergi\u00df niemals, da\u00df stets die geschw\u00e4tzige Tr\u00e4gheit,", "tokens": ["A\u00b7ber", "ver\u00b7gi\u00df", "nie\u00b7mals", ",", "da\u00df", "stets", "die", "ge\u00b7schw\u00e4t\u00b7zi\u00b7ge", "Tr\u00e4g\u00b7heit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ADV", "$,", "KOUS", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Wertlos, ohne Verdienst, gro\u00dfe Verdienste beschmutzt!", "tokens": ["Wert\u00b7los", ",", "oh\u00b7ne", "Ver\u00b7dienst", ",", "gro\u00b7\u00dfe", "Ver\u00b7diens\u00b7te", "be\u00b7schmutzt", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUI", "NN", "$,", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}}}}}