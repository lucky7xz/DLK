{"textgrid.poem.63566": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "[von den Halden herab]", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von den Halden herab", "tokens": ["Von", "den", "Hal\u00b7den", "her\u00b7ab"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADV"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Rinnen Str\u00f6me von Licht,", "tokens": ["Rin\u00b7nen", "Str\u00f6\u00b7me", "von", "Licht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Durch die Wellen am Weiher", "tokens": ["Durch", "die", "Wel\u00b7len", "am", "Wei\u00b7her"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPRART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Der Goldschein bricht.", "tokens": ["Der", "Gold\u00b7schein", "bricht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Es brennen die Rosen,", "tokens": ["Es", "bren\u00b7nen", "die", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Es funkelt der Bach,", "tokens": ["Es", "fun\u00b7kelt", "der", "Bach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Es blitzt wie Silber", "tokens": ["Es", "blitzt", "wie", "Sil\u00b7ber"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOKOM", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Das Kirchendach.", "tokens": ["Das", "Kir\u00b7chen\u00b7dach", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Die Augen der Menschen", "tokens": ["Die", "Au\u00b7gen", "der", "Men\u00b7schen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Leuchten so grell \u2013", "tokens": ["Leuch\u00b7ten", "so", "grell", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJD", "$("], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Wohin dich fl\u00fcchten,", "tokens": ["Wo\u00b7hin", "dich", "fl\u00fcch\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Kranker Gesell?", "tokens": ["Kran\u00b7ker", "Ge\u00b7sell", "?"], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "La\u00df deine Liebste", "tokens": ["La\u00df", "dei\u00b7ne", "Liebs\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["VVIMP", "PPOSAT", "NN"], "meter": "++-+-", "measure": "iambic.di"}, "line.2": {"text": "L\u00f6sen ihr Haar,", "tokens": ["L\u00f6\u00b7sen", "ihr", "Haar", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Birg ihr am Busen", "tokens": ["Birg", "ihr", "am", "Bu\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "PPER", "APPRART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Dein Augenpaar.", "tokens": ["Dein", "Au\u00b7gen\u00b7paar", "."], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Ward es von Wachen", "tokens": ["Ward", "es", "von", "Wa\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "NN"], "meter": "++-+-", "measure": "iambic.di"}, "line.2": {"text": "Und Weinen wund,", "tokens": ["Und", "Wei\u00b7nen", "wund", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Im Lockenschatten", "tokens": ["Im", "Lo\u00b7cken\u00b7schat\u00b7ten"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Schl\u00e4ft sich's gesund.", "tokens": ["Schl\u00e4ft", "sich's", "ge\u00b7sund", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "VVPP", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}}}}