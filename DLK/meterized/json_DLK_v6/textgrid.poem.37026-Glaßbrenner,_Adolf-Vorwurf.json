{"textgrid.poem.37026": {"metadata": {"author": {"name": "Gla\u00dfbrenner, Adolf", "birth": "N.A.", "death": "N.A."}, "title": "Vorwurf", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mehr, Freund, als Du aufgestellt,", "tokens": ["Mehr", ",", "Freund", ",", "als", "Du", "auf\u00b7ge\u00b7stellt", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "NN", "$,", "KOUS", "PPER", "VVPP", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Uebersahst Du, wir vermissen", "tokens": ["Ue\u00b7ber\u00b7sahst", "Du", ",", "wir", "ver\u00b7mis\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Vieles Wicht'ge, Deine Welt", "tokens": ["Vie\u00b7les", "Wicht'\u00b7ge", ",", "Dei\u00b7ne", "Welt"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PIAT", "NN", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist nichts Ganzes, ist zerrissen!", "tokens": ["Ist", "nichts", "Gan\u00b7zes", ",", "ist", "zer\u00b7ris\u00b7sen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "NN", "$,", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}