{"dta.poem.21300": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput II.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hier hab\u2019 ich Spitzen, die feiner sind", "tokens": ["Hier", "hab'", "ich", "Spit\u00b7zen", ",", "die", "fei\u00b7ner", "sind"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "NN", "$,", "PRELS", "ADJD", "VAFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Als die von Br\u00fcssel und Mecheln,", "tokens": ["Als", "die", "von", "Br\u00fcs\u00b7sel", "und", "Me\u00b7cheln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "APPR", "NN", "KON", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Und pack\u2019 ich einst meine Spitzen aus,", "tokens": ["Und", "pack'", "ich", "einst", "mei\u00b7ne", "Spit\u00b7zen", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "ADV", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sie werden Euch sticheln und hecheln.", "tokens": ["Sie", "wer\u00b7den", "Euch", "sti\u00b7cheln", "und", "he\u00b7cheln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVINF", "KON", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Im Kopfe trage ich Bijouterien,", "tokens": ["Im", "Kop\u00b7fe", "tra\u00b7ge", "ich", "Bi\u00b7jou\u00b7te\u00b7ri\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Zukunft Krondiamanten,", "tokens": ["Der", "Zu\u00b7kunft", "Kron\u00b7di\u00b7a\u00b7man\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die Tempelkleinodien des neuen Gotts,", "tokens": ["Die", "Tem\u00b7pel\u00b7klein\u00b7o\u00b7di\u00b7en", "des", "neu\u00b7en", "Gotts", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Des gro\u00dfen Unbekannten.", "tokens": ["Des", "gro\u00b7\u00dfen", "Un\u00b7be\u00b7kann\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und viele B\u00fccher trag\u2019 ich im Kopf!", "tokens": ["Und", "vie\u00b7le", "B\u00fc\u00b7cher", "trag'", "ich", "im", "Kopf", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Ich darf es Euch versichern,", "tokens": ["Ich", "darf", "es", "Euch", "ver\u00b7si\u00b7chern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mein Kopf ist ein zwitscherndes Vogelnest", "tokens": ["Mein", "Kopf", "ist", "ein", "zwit\u00b7schern\u00b7des", "Vo\u00b7gel\u00b7nest"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Von konfiszirlichen B\u00fcchern.", "tokens": ["Von", "kon\u00b7fis\u00b7zir\u00b7li\u00b7chen", "B\u00fc\u00b7chern", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Glaubt mir, in Satans Bibliothek", "tokens": ["Glaubt", "mir", ",", "in", "Sa\u00b7tans", "Bib\u00b7lio\u00b7thek"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kann es nicht schlimmere geben;", "tokens": ["Kann", "es", "nicht", "schlim\u00b7me\u00b7re", "ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "ADJA", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Sie sind gef\u00e4hrlicher noch als die", "tokens": ["Sie", "sind", "ge\u00b7f\u00e4hr\u00b7li\u00b7cher", "noch", "als", "die"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "ADV", "KOUS", "ART"], "meter": "-+-+--++-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Von Hoffmann von Fallersleben! \u2013", "tokens": ["Von", "Hoff\u00b7mann", "von", "Fal\u00b7lers\u00b7le\u00b7ben", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NE", "APPR", "NN", "$.", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}