{"textgrid.poem.31433": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gleich wie der Wolken last in tropfen sich verlieret:", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gleich wie der Wolken last in tropfen sich verlieret:", "tokens": ["Gleich", "wie", "der", "Wol\u00b7ken", "last", "in", "trop\u00b7fen", "sich", "ver\u00b7lie\u00b7ret", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "VVFIN", "APPR", "VVFIN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "also mein Vngl\u00fcck auch durch Thr\u00e4nen Regenf\u00e4llt.", "tokens": ["al\u00b7so", "mein", "Vn\u00b7gl\u00fcck", "auch", "durch", "Thr\u00e4\u00b7nen", "Re\u00b7gen\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "als Haubt Plejaden / sie zu feuchten sind bestellt /", "tokens": ["als", "Haubt", "Ple\u00b7ja\u00b7den", "/", "sie", "zu", "feuch\u00b7ten", "sind", "be\u00b7stellt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "NN", "$(", "PPER", "PTKZU", "VVINF", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "der Gottes G\u00fcte Land das h\u00fclff-bl\u00fch dann gebteret.", "tokens": ["der", "Got\u00b7tes", "G\u00fc\u00b7te", "Land", "das", "h\u00fclff\u00b7bl\u00fch", "dann", "geb\u00b7te\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "NN", "ART", "ADJD", "ADV", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Di\u00df qu\u00e4lend Wellen-Meer an wunsches Port offt f\u00fchret.", "tokens": ["Di\u00df", "qu\u00e4\u00b7lend", "Wel\u00b7len\u00b7Meer", "an", "wun\u00b7sches", "Port", "offt", "f\u00fch\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVPP", "NN", "APPR", "ADJA", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der Bu\u00dfe Muschel Perl in seinem scho\u00df es h\u00e4lt /", "tokens": ["Der", "Bu\u00b7\u00dfe", "Mu\u00b7schel", "Perl", "in", "sei\u00b7nem", "scho\u00df", "es", "h\u00e4lt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "NE", "APPR", "PPOSAT", "VVFIN", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "zu dem die Amber sich / das Ruf-Gebet / gesellt.", "tokens": ["zu", "dem", "die", "Am\u00b7ber", "sich", "/", "das", "Ruf\u00b7Ge\u00b7bet", "/", "ge\u00b7sellt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "NN", "PRF", "$(", "ART", "NN", "$(", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Offt man darinnen mich / gleich als im Felsen / sp\u00fcret:", "tokens": ["Offt", "man", "da\u00b7rin\u00b7nen", "mich", "/", "gleich", "als", "im", "Fel\u00b7sen", "/", "sp\u00fc\u00b7ret", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "PIS", "ADV", "PPER", "$(", "ADV", "KOKOM", "APPRART", "NN", "$(", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Sonst treibt die Wasserkunst offt grosses R\u00e4derwerck.", "tokens": ["Sonst", "treibt", "die", "Was\u00b7ser\u00b7kunst", "offt", "gros\u00b7ses", "R\u00e4\u00b7der\u00b7werck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ach da\u00df mein weinen doch auch Gottes Raths-Rad triebe /", "tokens": ["Ach", "da\u00df", "mein", "wei\u00b7nen", "doch", "auch", "Got\u00b7tes", "Raths\u00b7Rad", "trie\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "KOUS", "PPOSAT", "VVINF", "KON", "ADV", "NN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "da\u00df er dem Sternen gang ein gut geschick vorschriebe!", "tokens": ["da\u00df", "er", "dem", "Ster\u00b7nen", "gang", "ein", "gut", "ge\u00b7schick", "vor\u00b7schrie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "ART", "ADJD", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ach Thr\u00e4nen! h\u00e4ttet ihr doch zuverbrennen st\u00e4rk", "tokens": ["Ach", "Thr\u00e4\u00b7nen", "!", "h\u00e4t\u00b7tet", "ihr", "doch", "zu\u00b7ver\u00b7bren\u00b7nen", "st\u00e4rk"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "NN", "$.", "VAFIN", "PPER", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "die starken Vngl\u00fccks band. Seyd ihr doch siedend hei\u00df:", "tokens": ["die", "star\u00b7ken", "Vn\u00b7gl\u00fccks", "band", ".", "Seyd", "ihr", "doch", "sie\u00b7dend", "hei\u00df", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$.", "VAIMP", "PPER", "ADV", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "k\u00fchlt auf das wenigst nur die hitz / weil ihr auch Ey\u00df!", "tokens": ["k\u00fchlt", "auf", "das", "we\u00b7nigst", "nur", "die", "hitz", "/", "weil", "ihr", "auch", "Ey\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADV", "ADV", "ART", "NN", "$(", "KOUS", "PPER", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}