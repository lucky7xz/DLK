{"textgrid.poem.62034": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "1L: Unter Felsen wandelst Du,", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Unter Felsen wandelst Du,", "tokens": ["Un\u00b7ter", "Fel\u00b7sen", "wan\u00b7delst", "Du", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Unter Tr\u00e4umen wirkest Du,", "tokens": ["Un\u00b7ter", "Tr\u00e4u\u00b7men", "wir\u00b7kest", "Du", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wandle, wirke immer zu,", "tokens": ["Wand\u00b7le", ",", "wir\u00b7ke", "im\u00b7mer", "zu", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch das Herz find't keine Ruh.", "tokens": ["Doch", "das", "Herz", "find't", "kei\u00b7ne", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Nicht die W\u00fcste, nicht der Strand,", "tokens": ["Nicht", "die", "W\u00fcs\u00b7te", ",", "nicht", "der", "Strand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "$,", "PTKNEG", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Nicht die K\u00fcste, nicht das Land", "tokens": ["Nicht", "die", "K\u00fcs\u00b7te", ",", "nicht", "das", "Land"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "ART", "NN", "$,", "PTKNEG", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bringen es zum Schweigen,", "tokens": ["Brin\u00b7gen", "es", "zum", "Schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Wenn der Schmerz sein eigen.", "tokens": ["Wenn", "der", "Schmerz", "sein", "ei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPOSAT", "ADJD", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}