{"textgrid.poem.31723": {"metadata": {"author": {"name": "Weerth, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Die Schenke", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Herz, des Sanges schier entw\u00f6hnet,", "tokens": ["Mein", "Herz", ",", "des", "San\u00b7ges", "schier", "ent\u00b7w\u00f6h\u00b7net", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Schl\u00e4gt jetzt von neuem wild und hei\u00df.", "tokens": ["Schl\u00e4gt", "jetzt", "von", "neu\u00b7em", "wild", "und", "hei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ADJA", "NN", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Drum auf, ihr Saiten, klingt und t\u00f6net!", "tokens": ["Drum", "auf", ",", "ihr", "Sai\u00b7ten", ",", "klingt", "und", "t\u00f6\u00b7net", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "PTKVZ", "$,", "PPOSAT", "NN", "$,", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich singe einer Schenke Preis!", "tokens": ["Ich", "sin\u00b7ge", "ei\u00b7ner", "Schen\u00b7ke", "Preis", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dort ragt sie aus den Waldestannen", "tokens": ["Dort", "ragt", "sie", "aus", "den", "Wal\u00b7des\u00b7tan\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und zeigt den Leu auf rundem Schild,", "tokens": ["Und", "zeigt", "den", "Leu", "auf", "run\u00b7dem", "Schild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dazu drei Kr\u00fcge und drei Kannen", "tokens": ["Da\u00b7zu", "drei", "Kr\u00fc\u00b7ge", "und", "drei", "Kan\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "CARD", "NN", "KON", "CARD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und auch ein Spr\u00fcchlein fromm und mild.", "tokens": ["Und", "auch", "ein", "Spr\u00fcch\u00b7lein", "fromm", "und", "mild", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Es ist der Schenken allerbeste!", "tokens": ["Es", "ist", "der", "Schen\u00b7ken", "al\u00b7ler\u00b7bes\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und L\u00f6wenburg wird sie genannt.", "tokens": ["Und", "L\u00f6\u00b7wen\u00b7burg", "wird", "sie", "ge\u00b7nannt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Kommt, tretet ein, seid meine G\u00e4ste,", "tokens": ["Kommt", ",", "tre\u00b7tet", "ein", ",", "seid", "mei\u00b7ne", "G\u00e4s\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PTKVZ", "$,", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hier ist der beste Wein im Land!", "tokens": ["Hier", "ist", "der", "bes\u00b7te", "Wein", "im", "Land", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Seht dort die Wirtin \u2013 schon von ferne", "tokens": ["Seht", "dort", "die", "Wir\u00b7tin", "\u2013", "schon", "von", "fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN", "$(", "ADV", "APPR", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Winkt sie mit einem vollen Glas;", "tokens": ["Winkt", "sie", "mit", "ei\u00b7nem", "vol\u00b7len", "Glas", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein lieblich Mahl bringt sie uns gerne", "tokens": ["Ein", "lieb\u00b7lich", "Mahl", "bringt", "sie", "uns", "ger\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "VVFIN", "PPER", "PPER", "ADV"], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.4": {"text": "Und zapft aus einem neuen Fa\u00df!", "tokens": ["Und", "zapft", "aus", "ei\u00b7nem", "neu\u00b7en", "Fa\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Sie ist mir \u00e4u\u00dferst wohlgewogen,", "tokens": ["Sie", "ist", "mir", "\u00e4u\u00b7\u00dferst", "wohl\u00b7ge\u00b7wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dieweil ich neulich klug und schlau", "tokens": ["Die\u00b7weil", "ich", "neu\u00b7lich", "klug", "und", "schlau"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr in das Angesicht gelogen,", "tokens": ["Ihr", "in", "das", "An\u00b7ge\u00b7sicht", "ge\u00b7lo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie sei die wundersch\u00f6nste Frau!", "tokens": ["Sie", "sei", "die", "wun\u00b7der\u00b7sch\u00f6ns\u00b7te", "Frau", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Geht ihr nun jubelnd durch die T\u00fcre,", "tokens": ["Geht", "ihr", "nun", "ju\u00b7belnd", "durch", "die", "T\u00fc\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Seht, da\u00df der T\u00f6chter keine flieh;", "tokens": ["Seht", ",", "da\u00df", "der", "T\u00f6ch\u00b7ter", "kei\u00b7ne", "flieh", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "ART", "NN", "PIAT", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Meintwegen k\u00fc\u00dft sie \u2013 ich erk\u00fcre", "tokens": ["Meint\u00b7we\u00b7gen", "k\u00fc\u00dft", "sie", "\u2013", "ich", "er\u00b7k\u00fc\u00b7re"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "$(", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mir stets die liebliche Marie!", "tokens": ["Mir", "stets", "die", "lieb\u00b7li\u00b7che", "Ma\u00b7rie", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Denn Augen hat sie wie zwei Trauben", "tokens": ["Denn", "Au\u00b7gen", "hat", "sie", "wie", "zwei", "Trau\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VAFIN", "PPER", "KOKOM", "CARD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So dunkel \u2013 ist so sch\u00fcchtern noch;", "tokens": ["So", "dun\u00b7kel", "\u2013", "ist", "so", "sch\u00fcch\u00b7tern", "noch", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$(", "VAFIN", "ADV", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Ku\u00df will nimmer sie erlauben \u2013", "tokens": ["Den", "Ku\u00df", "will", "nim\u00b7mer", "sie", "er\u00b7lau\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was tut's? Man kommt und k\u00fc\u00dft sie doch!", "tokens": ["Was", "tut's", "?", "Man", "kommt", "und", "k\u00fc\u00dft", "sie", "doch", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NE", "$.", "PIS", "VVFIN", "KON", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Wer widerst\u00e4nde auch der Sch\u00f6nen,", "tokens": ["Wer", "wi\u00b7der\u00b7st\u00e4n\u00b7de", "auch", "der", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn sie den vollen Becher bringt,", "tokens": ["Wenn", "sie", "den", "vol\u00b7len", "Be\u00b7cher", "bringt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn sie zu ihrer Harfe T\u00f6nen", "tokens": ["Wenn", "sie", "zu", "ih\u00b7rer", "Har\u00b7fe", "T\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Lied mit heller Stimme singt?", "tokens": ["Ein", "Lied", "mit", "hel\u00b7ler", "Stim\u00b7me", "singt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Die G\u00e4ste lauschen in die Runde;", "tokens": ["Die", "G\u00e4s\u00b7te", "lau\u00b7schen", "in", "die", "Run\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Denn alle Herzen singt sie wach!", "tokens": ["Denn", "al\u00b7le", "Her\u00b7zen", "singt", "sie", "wach", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Jetzt schweigt sie \u2013 und von Mund zu Munde", "tokens": ["Jetzt", "schweigt", "sie", "\u2013", "und", "von", "Mund", "zu", "Mun\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$(", "KON", "APPR", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schallt wild die letzte Strophe nach!", "tokens": ["Schallt", "wild", "die", "letz\u00b7te", "Stro\u00b7phe", "nach", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Doch nun, o Lied, mit frischem Tone,", "tokens": ["Doch", "nun", ",", "o", "Lied", ",", "mit", "fri\u00b7schem", "To\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "FM", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Erkling aufs neu! Ihr Saiten, schwirrt!", "tokens": ["Er\u00b7kling", "aufs", "neu", "!", "Ihr", "Sai\u00b7ten", ",", "schwirrt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJD", "$.", "PPOSAT", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es kommt des Hauses Zier und Krone,", "tokens": ["Es", "kommt", "des", "Hau\u00b7ses", "Zier", "und", "Kro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der unvergleichlich dicke Wirt!", "tokens": ["Der", "un\u00b7ver\u00b7gleich\u00b7lich", "di\u00b7cke", "Wirt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Er kommt! er kommt! mit prallen Lenden,", "tokens": ["Er", "kommt", "!", "er", "kommt", "!", "mit", "pral\u00b7len", "Len\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "$.", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Er hat ein B\u00e4uchlein wie ein Fa\u00df,", "tokens": ["Er", "hat", "ein", "B\u00e4uch\u00b7lein", "wie", "ein", "Fa\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sein Weib und sieben Kinder f\u00e4nden,", "tokens": ["Sein", "Weib", "und", "sie\u00b7ben", "Kin\u00b7der", "f\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "CARD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "T\u00e4t's Not, mitsamt darin Gela\u00df!", "tokens": ["T\u00e4t's", "Not", ",", "mit\u00b7samt", "da\u00b7rin", "Ge\u00b7la\u00df", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "PAV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Sein rotes Antlitz scheint zu sagen:", "tokens": ["Sein", "ro\u00b7tes", "Ant\u00b7litz", "scheint", "zu", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbmir war so mancher Wein geg\u00f6nnt,", "tokens": ["\u00bb", "mir", "war", "so", "man\u00b7cher", "Wein", "ge\u00b7g\u00f6nnt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ADV", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df er zusammen mit Behagen", "tokens": ["Da\u00df", "er", "zu\u00b7sam\u00b7men", "mit", "Be\u00b7ha\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Drei K\u00f6nigreich ers\u00e4ufen k\u00f6nnt!\u00ab", "tokens": ["Drei", "K\u00f6\u00b7nig\u00b7reich", "er\u00b7s\u00e4u\u00b7fen", "k\u00f6nnt", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["CARD", "NN", "VVPP", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Stolz ist er drauf, sein Weib zu f\u00fchren", "tokens": ["Stolz", "ist", "er", "drauf", ",", "sein", "Weib", "zu", "f\u00fch\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "PTKVZ", "$,", "PPOSAT", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Am Sonntag in die Kirch hinein,", "tokens": ["Am", "Sonn\u00b7tag", "in", "die", "Kirch", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch bleibt er selber vor der T\u00fcren \u2013", "tokens": ["Doch", "bleibt", "er", "sel\u00b7ber", "vor", "der", "T\u00fc\u00b7ren", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr seinen Bauch ist sie zu klein!", "tokens": ["F\u00fcr", "sei\u00b7nen", "Bauch", "ist", "sie", "zu", "klein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "PPER", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Tut ihm der Tod dereinst mal winken,", "tokens": ["Tut", "ihm", "der", "Tod", "de\u00b7reinst", "mal", "win\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Glaubt mir, er f\u00e4hrt gen Himmel nicht!", "tokens": ["Glaubt", "mir", ",", "er", "f\u00e4hrt", "gen", "Him\u00b7mel", "nicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPER", "VVFIN", "APPR", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er wird zur\u00fcck zur Erde sinken \u2013", "tokens": ["Er", "wird", "zu\u00b7r\u00fcck", "zur", "Er\u00b7de", "sin\u00b7ken", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPRART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dieweil er ein zu gro\u00df Gewicht!", "tokens": ["Die\u00b7weil", "er", "ein", "zu", "gro\u00df", "Ge\u00b7wicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "PTKA", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.15": {"line.1": {"text": "So kennt ihr nun die ganze Schenke,", "tokens": ["So", "kennt", "ihr", "nun", "die", "gan\u00b7ze", "Schen\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr kennt den Wirt mit Weib und Kind \u2013", "tokens": ["Ihr", "kennt", "den", "Wirt", "mit", "Weib", "und", "Kind", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und Pfalzwein ist ein gut Getr\u00e4nke,", "tokens": ["Und", "Pfal\u00b7zwein", "ist", "ein", "gut", "Ge\u00b7tr\u00e4n\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "ART", "ADJD", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Streicht \u00fcbern Rhein der Morgenwind!", "tokens": ["Streicht", "\u00fc\u00b7bern", "Rhein", "der", "Mor\u00b7gen\u00b7wind", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NE", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}