{"dta.poem.3399": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Vinval und Vinvela.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193292", "language": ["de:0.85", "af:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Unter ging die Sonne Gottes", "tokens": ["Un\u00b7ter", "ging", "die", "Son\u00b7ne", "Got\u00b7tes"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "VVFIN", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Thauged\u00fcft entstieg der Flur.", "tokens": ["Thau\u00b7ge\u00b7d\u00fcft", "ent\u00b7stieg", "der", "Flur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf die Abendlispel Gottes", "tokens": ["Auf", "die", "A\u00b7bend\u00b7lis\u00b7pel", "Got\u00b7tes"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Horchte feyrend die Natur.", "tokens": ["Horch\u00b7te", "fey\u00b7rend", "die", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Grillen zirpten, M\u00fccken summten,", "tokens": ["Gril\u00b7len", "zirp\u00b7ten", ",", "M\u00fc\u00b7cken", "summ\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Fr\u00f6sche gurgelten im Sumpf;", "tokens": ["Fr\u00f6\u00b7sche", "gur\u00b7gel\u00b7ten", "im", "Sumpf", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+---+", "measure": "unknown.measure.tri"}, "line.7": {"text": "Und aus stillen D\u00f6rfern bummten", "tokens": ["Und", "aus", "stil\u00b7len", "D\u00f6r\u00b7fern", "bumm\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Abendglocken fern und dumpf.", "tokens": ["A\u00b7bend\u00b7glo\u00b7cken", "fern", "und", "dumpf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Aus der Mauern Eng' und Schw\u00fcle,", "tokens": ["Aus", "der", "Mau\u00b7ern", "Eng'", "und", "Schw\u00fc\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus der St\u00e4dter lautem Schwarm", "tokens": ["Aus", "der", "St\u00e4d\u00b7ter", "lau\u00b7tem", "Schwarm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Fl\u00fcchtet' in die freye K\u00fchle", "tokens": ["Fl\u00fcch\u00b7tet'", "in", "die", "frey\u00b7e", "K\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Vinval an Vinvelens Arm.", "tokens": ["Vin\u00b7val", "an", "Vin\u00b7ve\u00b7lens", "Arm", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NE", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "Warmen Herzens, reiner Seele", "tokens": ["War\u00b7men", "Her\u00b7zens", ",", "rei\u00b7ner", "See\u00b7le"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "War das liebetrunkne Paar,", "tokens": ["War", "das", "lie\u00b7be\u00b7trunk\u00b7ne", "Paar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Sonder T\u00fccke, sonder Fehle,", "tokens": ["Son\u00b7der", "T\u00fc\u00b7cke", ",", "son\u00b7der", "Feh\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Aller Schuld und Makel baar.", "tokens": ["Al\u00b7ler", "Schuld", "und", "Ma\u00b7kel", "baar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Gern vom gr\u00fcnen H\u00fcgel schauten", "tokens": ["Gern", "vom", "gr\u00fc\u00b7nen", "H\u00fc\u00b7gel", "schau\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sie der Sonne letzten Strahl;", "tokens": ["Sie", "der", "Son\u00b7ne", "letz\u00b7ten", "Strahl", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Horchten gern der wonnelauten", "tokens": ["Horch\u00b7ten", "gern", "der", "won\u00b7ne\u00b7lau\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wachtel Schlag im Wiesenthal;", "tokens": ["Wach\u00b7tel", "Schlag", "im", "Wie\u00b7sent\u00b7hal", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Schwelgten l\u00fcstern in den D\u00fcften", "tokens": ["Schwelg\u00b7ten", "l\u00fcs\u00b7tern", "in", "den", "D\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Der erfrischten Weizenflur;", "tokens": ["Der", "er\u00b7frischten", "Wei\u00b7zen\u00b7flur", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Freuten sich des Rauchs der Triften,", "tokens": ["Freu\u00b7ten", "sich", "des", "Rauchs", "der", "Trif\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und des Friedens der Natur.", "tokens": ["Und", "des", "Frie\u00b7dens", "der", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Itzt im bl\u00fcthenweissen Laube,", "tokens": ["Itzt", "im", "bl\u00fc\u00b7then\u00b7weis\u00b7sen", "Lau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Itzt am blaubebl\u00fcmten Bach,", "tokens": ["Itzt", "am", "blau\u00b7be\u00b7bl\u00fcm\u00b7ten", "Bach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Itzt im W\u00fcrmchen, itzt im Staube,", "tokens": ["Itzt", "im", "W\u00fcrm\u00b7chen", ",", "itzt", "im", "Stau\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sp\u00fcrten sie dem Ewgen nach;", "tokens": ["Sp\u00fcr\u00b7ten", "sie", "dem", "Ew\u00b7gen", "nach", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "H\u00f6rten seiner Stimme Hallen,", "tokens": ["H\u00f6r\u00b7ten", "sei\u00b7ner", "Stim\u00b7me", "Hal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "F\u00fchlten seiner Fl\u00fcgel Wehn,", "tokens": ["F\u00fchl\u00b7ten", "sei\u00b7ner", "Fl\u00fc\u00b7gel", "Wehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Sahn in jeder Wolke Wallen", "tokens": ["Sahn", "in", "je\u00b7der", "Wol\u00b7ke", "Wal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PIAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Seines Wagens Rosse gehn.", "tokens": ["Sei\u00b7nes", "Wa\u00b7gens", "Ros\u00b7se", "gehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Harmlos wandelten die Beyden", "tokens": ["Harm\u00b7los", "wan\u00b7del\u00b7ten", "die", "Bey\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In das volle \u00c4hrenfeld,", "tokens": ["In", "das", "vol\u00b7le", "\u00c4h\u00b7ren\u00b7feld", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und mit s\u00fcssen Seelenfreuden", "tokens": ["Und", "mit", "s\u00fcs\u00b7sen", "See\u00b7len\u00b7freu\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tr\u00e4nkte sie die D\u00e4mmerwelt.", "tokens": ["Tr\u00e4nk\u00b7te", "sie", "die", "D\u00e4m\u00b7mer\u00b7welt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Laulich war die Luft und milde,", "tokens": ["Lau\u00b7lich", "war", "die", "Luft", "und", "mil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Rein der Aether, klar und blau,", "tokens": ["Rein", "der", "A\u00b7e\u00b7ther", ",", "klar", "und", "blau", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Und die athmenden Gefilde", "tokens": ["Und", "die", "ath\u00b7men\u00b7den", "Ge\u00b7fil\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Hauchten Frisch' und F\u00fcll' und Thau", "tokens": ["Hauch\u00b7ten", "Frisch'", "und", "F\u00fcll'", "und", "Thau"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "NE", "KON", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Vinvals und Vinvelens Seelen", "tokens": ["Vin\u00b7vals", "und", "Vin\u00b7ve\u00b7lens", "See\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "KON", "NN", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Schmelzte R\u00fchrung und Gef\u00fchl.", "tokens": ["Schmelz\u00b7te", "R\u00fch\u00b7rung", "und", "Ge\u00b7f\u00fchl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Vinval lispelte Vinvelen:", "tokens": ["Vin\u00b7val", "lis\u00b7pel\u00b7te", "Vin\u00b7ve\u00b7len", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.4": {"text": "\u201ek\u00f6stlich ist das Abendk\u00fchl.", "tokens": ["\u201e", "k\u00f6st\u00b7lich", "ist", "das", "A\u00b7bend\u00b7k\u00fchl", "."], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "\u201eherrlich goldet deine Wange", "tokens": ["\u201e", "herr\u00b7lich", "gol\u00b7det", "dei\u00b7ne", "Wan\u00b7ge"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "ADJD", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "\u201ejener Wolke rother Schein.", "tokens": ["\u201e", "je\u00b7ner", "Wol\u00b7ke", "ro\u00b7ther", "Schein", "."], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "\u201elass uns, Traute, mit Gesange", "tokens": ["\u201e", "lass", "uns", ",", "Trau\u00b7te", ",", "mit", "Ge\u00b7san\u00b7ge"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["$(", "VVFIN", "PPER", "$,", "NN", "$,", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "\u201ediesen sch\u00f6nen Abend weihn.\u201c", "tokens": ["\u201e", "die\u00b7sen", "sch\u00f6\u00b7nen", "A\u00b7bend", "weihn", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PDAT", "ADJA", "NN", "VVINF", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Und Vinvela, leiseschauernd,", "tokens": ["Und", "Vin\u00b7ve\u00b7la", ",", "lei\u00b7se\u00b7schau\u00b7ernd", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["KON", "NE", "$,", "ADJD", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sang den holden Klaggesang.", "tokens": ["Sang", "den", "hol\u00b7den", "Klag\u00b7ge\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bangeahnend, leisetrauernd", "tokens": ["Ban\u00b7ge\u00b7ah\u00b7nend", ",", "lei\u00b7se\u00b7trau\u00b7ernd"], "token_info": ["word", "punct", "word"], "pos": ["VVPP", "$,", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Klang des Liedes s\u00fcsser Klang.", "tokens": ["Klang", "des", "Lie\u00b7des", "s\u00fcs\u00b7ser", "Klang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mit den stillen Abendl\u00fcften", "tokens": ["Mit", "den", "stil\u00b7len", "A\u00b7ben\u00b7dl\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "S\u00e4uselte der sanfte Hall", "tokens": ["S\u00e4u\u00b7sel\u00b7te", "der", "sanf\u00b7te", "Hall"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.7": {"text": "\u00dcber Feld und \u00fcber Triften.", "tokens": ["\u00dc\u00b7ber", "Feld", "und", "\u00fc\u00b7ber", "Trif\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wandrer lauschten seinem Schall.", "tokens": ["Wand\u00b7rer", "lauschten", "sei\u00b7nem", "Schall", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.8": {"line.1": {"text": "\u201elieblich ist des Abends Milde,", "tokens": ["\u201e", "lieb\u00b7lich", "ist", "des", "A\u00b7bends", "Mil\u00b7de", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u201esch\u00f6n des Himmels Angesicht.", "tokens": ["\u201e", "sch\u00f6n", "des", "Him\u00b7mels", "An\u00b7ge\u00b7sicht", "."], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u201efreundlich d\u00e4mmern die Gefilde,", "tokens": ["\u201e", "freund\u00b7lich", "d\u00e4m\u00b7mern", "die", "Ge\u00b7fil\u00b7de", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u201ehalb in Schatten, halb in Licht.", "tokens": ["\u201e", "halb", "in", "Schat\u00b7ten", ",", "halb", "in", "Licht", "."], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "APPR", 