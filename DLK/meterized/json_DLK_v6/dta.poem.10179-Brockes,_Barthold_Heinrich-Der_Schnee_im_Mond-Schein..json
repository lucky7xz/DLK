{"dta.poem.10179": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Schnee im Mond-Schein.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Allmacht, die dem gr\u00fcnen Klee", "tokens": ["Die", "All\u00b7macht", ",", "die", "dem", "gr\u00fc\u00b7nen", "Klee"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die sch\u00f6nen Farben eingepr\u00e4get,", "tokens": ["Die", "sch\u00f6\u00b7nen", "Far\u00b7ben", "ein\u00b7ge\u00b7pr\u00e4\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Hat in den Silber-weissen Schnee", "tokens": ["Hat", "in", "den", "Sil\u00b7ber\u00b7weis\u00b7sen", "Schnee"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auch einen lichten Schein geleget:", "tokens": ["Auch", "ei\u00b7nen", "lich\u00b7ten", "Schein", "ge\u00b7le\u00b7get", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wenn ich, im Frost, ihn gl\u00e4ntzen seh,", "tokens": ["Wenn", "ich", ",", "im", "Frost", ",", "ihn", "gl\u00e4nt\u00b7zen", "seh", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPRART", "NN", "$,", "PPER", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wird mein Gem\u00fcth zum Lob erreget.", "tokens": ["Wird", "mein", "Ge\u00b7m\u00fcth", "zum", "Lob", "er\u00b7re\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Je mehr er Glantz und Schimmer heget,", "tokens": ["Je", "mehr", "er", "Glantz", "und", "Schim\u00b7mer", "he\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Je mehr werd ich zum Ruhm beweget", "tokens": ["Je", "mehr", "werd", "ich", "zum", "Ruhm", "be\u00b7we\u00b7get"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Des Sch\u00f6pfers, der ihn in der H\u00f6h", "tokens": ["Des", "Sch\u00f6p\u00b7fers", ",", "der", "ihn", "in", "der", "H\u00f6h"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "APPR", "ART", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "So wunderbar zu bilden pfleget.", "tokens": ["So", "wun\u00b7der\u00b7bar", "zu", "bil\u00b7den", "pfle\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die\u00df war es ungefehr, was ich bey mir gedacht,", "tokens": ["Die\u00df", "war", "es", "un\u00b7ge\u00b7fehr", ",", "was", "ich", "bey", "mir", "ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADJD", "$,", "PWS", "PPER", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Als ich, in einer hellen Nacht,", "tokens": ["Als", "ich", ",", "in", "ei\u00b7ner", "hel\u00b7len", "Nacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Beym Mond-Schein, ein beschneytes Feld,", "tokens": ["Beym", "Mon\u00b7dSchein", ",", "ein", "be\u00b7schney\u00b7tes", "Feld", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In einer ungemeinen Pracht,", "tokens": ["In", "ei\u00b7ner", "un\u00b7ge\u00b7mei\u00b7nen", "Pracht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "In einem Silber reinen Schimmer,", "tokens": ["In", "ei\u00b7nem", "Sil\u00b7ber", "rei\u00b7nen", "Schim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Aus einem hoch erhabnen Zimmer", "tokens": ["Aus", "ei\u00b7nem", "hoch", "er\u00b7hab\u00b7nen", "Zim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Bewundernd \u00fcbersah. Das weisse Licht", "tokens": ["Be\u00b7wun\u00b7dernd", "\u00fc\u00b7ber\u00b7sah", ".", "Das", "weis\u00b7se", "Licht"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVPP", "VVFIN", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Durchdrang mein halb geblendetes Gesicht,", "tokens": ["Durch\u00b7drang", "mein", "halb", "ge\u00b7blen\u00b7de\u00b7tes", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Und fiel, mit seinem weissen Schein,", "tokens": ["Und", "fiel", ",", "mit", "sei\u00b7nem", "weis\u00b7sen", "Schein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Mir in die Seele selbst hinein;", "tokens": ["Mir", "in", "die", "See\u00b7le", "selbst", "hin\u00b7ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Erleuchtete, was finster war,", "tokens": ["Er\u00b7leuch\u00b7te\u00b7te", ",", "was", "fins\u00b7ter", "war", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PRELS", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Und machte,", "tokens": ["Und", "mach\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.13": {"text": "Da\u00df ich noch ferner also dachte:", "tokens": ["Da\u00df", "ich", "noch", "fer\u00b7ner", "al\u00b7so", "dach\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Was wirckt des Silbers weisser Glantz", "tokens": ["Was", "wirckt", "des", "Sil\u00b7bers", "weis\u00b7ser", "Glantz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In eines geitzgen Aug\u2019 und Brust", "tokens": ["In", "ei\u00b7nes", "geitz\u00b7gen", "Aug'", "und", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nicht f\u00fcr Vergn\u00fcgen, Freud und Lust?", "tokens": ["Nicht", "f\u00fcr", "Ver\u00b7gn\u00fc\u00b7gen", ",", "Freud", "und", "Lust", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der doch, beym Himmels-Silber, gantz", "tokens": ["Der", "doch", ",", "beym", "Him\u00b7mels\u00b7Sil\u00b7ber", ",", "gantz"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ART", "ADV", "$,", "APPRART", "NN", "$,", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Verblendet, Sinn-und f\u00fchl-los ist,", "tokens": ["Ver\u00b7blen\u00b7det", ",", "Sinn\u00b7\u00b7und", "f\u00fchl\u00b7los", "ist", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "NN", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und, weder des so weissen Mondes Schein,", "tokens": ["Und", ",", "we\u00b7der", "des", "so", "weis\u00b7sen", "Mon\u00b7des", "Schein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KON", "ART", "ADV", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Noch die durch ihn bestrahlte Welt,", "tokens": ["Noch", "die", "durch", "ihn", "be\u00b7strahl\u00b7te", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "APPR", "PPER", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Auch nicht das wei\u00df beschneyte Feld", "tokens": ["Auch", "nicht", "das", "wei\u00df", "be\u00b7schney\u00b7te", "Feld"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PTKNEG", "PDS", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Mit keinem Blick, mit keiner Lust, ermisst.", "tokens": ["Mit", "kei\u00b7nem", "Blick", ",", "mit", "kei\u00b7ner", "Lust", ",", "er\u00b7misst", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "APPR", "PIAT", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ach! n\u00e4hm uns doch der Schimmer, der so rein,", "tokens": ["Ach", "!", "n\u00e4hm", "uns", "doch", "der", "Schim\u00b7mer", ",", "der", "so", "rein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "PPER", "ADV", "ART", "NN", "$,", "PRELS", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Zum Ruhm des grossen Sch\u00f6pfers, ein!", "tokens": ["Zum", "Ruhm", "des", "gros\u00b7sen", "Sch\u00f6p\u00b7fers", ",", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ach m\u00f6gte man doch Sein,", "tokens": ["Ach", "m\u00f6g\u00b7te", "man", "doch", "Sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PIS", "ADV", "PPOSAT", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Bey diesem Silber-Glantz, gedencken!", "tokens": ["Bey", "die\u00b7sem", "Sil\u00b7ber\u00b7Glantz", ",", "ge\u00b7den\u00b7cken", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "So w\u00fcrd\u2019 Er uns nicht nur das sch\u00f6ne Morgen-Gold,", "tokens": ["So", "w\u00fcrd'", "Er", "uns", "nicht", "nur", "das", "sch\u00f6\u00b7ne", "Mor\u00b7gen\u00b7Gold", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "PTKNEG", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Das Gold des Segens auch, unfehlbar schencken.", "tokens": ["Das", "Gold", "des", "Se\u00b7gens", "auch", ",", "un\u00b7fehl\u00b7bar", "schen\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADV", "$,", "ADJD", "VVINF", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Bey dem Glantz des Silber-Lichts,", "tokens": ["Bey", "dem", "Glantz", "des", "Sil\u00b7ber\u00b7Lichts", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und der Anmuth, die ich habe,", "tokens": ["Und", "der", "An\u00b7muth", ",", "die", "ich", "ha\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "PPER", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Denck ich an die Wunder-Gabe", "tokens": ["Denck", "ich", "an", "die", "Wun\u00b7der\u00b7Ga\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Des betrachtenden Gesichts,", "tokens": ["Des", "be\u00b7trach\u00b7ten\u00b7den", "Ge\u00b7sichts", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Die der Sch\u00f6pfer mir gegeben:", "tokens": ["Die", "der", "Sch\u00f6p\u00b7fer", "mir", "ge\u00b7ge\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Opffr\u2019 Jhm billig diese Lust", "tokens": ["Opf\u00b7fr'", "Jhm", "bil\u00b7lig", "die\u00b7se", "Lust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ADJD", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Meiner recht vergn\u00fcgten Brust.", "tokens": ["Mei\u00b7ner", "recht", "ver\u00b7gn\u00fcg\u00b7ten", "Brust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Und, da ich auf Seine Wercke,", "tokens": ["Und", ",", "da", "ich", "auf", "Sei\u00b7ne", "Wer\u00b7cke", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Mit ger\u00fchrter Seele mercke,", "tokens": ["Mit", "ge\u00b7r\u00fchr\u00b7ter", "See\u00b7le", "mer\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Danck\u2019 ich Jhm, in stiller Freude,", "tokens": ["Danck", "ich", "Jhm", ",", "in", "stil\u00b7ler", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Bey so s\u00fcsser Augen-Weide.", "tokens": ["Bey", "so", "s\u00fcs\u00b7ser", "Au\u00b7gen\u00b7Wei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}