{"dta.poem.7121": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtung des Schlafs,  \n als  \n eine G\u00f6ttliche Wohlthat,  \n bey dem 1728sten Jahres-Wechsel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Bewunderns-w\u00fcrd\u2019ges Werck der weisen Lieb\u2019 und Macht", "tokens": ["Be\u00b7wun\u00b7derns\u00b7w\u00fcrd'\u00b7ges", "Werck", "der", "wei\u00b7sen", "Lieb'", "und", "Macht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "ART", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des Sch\u00f6pffers dieser Welt! Wer sieht hier nicht die Spur", "tokens": ["Des", "Sch\u00f6pf\u00b7fers", "die\u00b7ser", "Welt", "!", "Wer", "sieht", "hier", "nicht", "die", "Spur"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PDAT", "NN", "$.", "PWS", "VVFIN", "ADV", "PTKNEG", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Allmacht, in dem Schoo\u00df der \u00e4msigen Natur?", "tokens": ["Der", "All\u00b7macht", ",", "in", "dem", "Schoo\u00df", "der", "\u00e4m\u00b7si\u00b7gen", "Na\u00b7tur", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und alles dieses wird doch, leider! schlecht bedacht,", "tokens": ["Und", "al\u00b7les", "die\u00b7ses", "wird", "doch", ",", "lei\u00b7der", "!", "schlecht", "be\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PIS", "PDS", "VAFIN", "ADV", "$,", "ADV", "$.", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ja mancher sieht es gar mit scheelen Augen an:", "tokens": ["Ja", "man\u00b7cher", "sieht", "es", "gar", "mit", "schee\u00b7len", "Au\u00b7gen", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PIS", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201ees ist, spricht Trax, der Schlaff ein r\u00e4ub\u2019rischer Tyrann,", "tokens": ["\u201e", "es", "ist", ",", "spricht", "Trax", ",", "der", "Schlaff", "ein", "r\u00e4ub'\u00b7ri\u00b7scher", "Ty\u00b7rann", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "$,", "VVFIN", "NE", "$,", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201eder uns des gr\u00f6ssten Theils von unserm Leben,", "tokens": ["\u201e", "der", "uns", "des", "gr\u00f6ss\u00b7ten", "Theils", "von", "un\u00b7serm", "Le\u00b7ben", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PRELS", "PPER", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "\u201edas in so kleiner Maa\u00df uns ohne das gegeben,", "tokens": ["\u201e", "das", "in", "so", "klei\u00b7ner", "Maa\u00df", "uns", "oh\u00b7ne", "das", "ge\u00b7ge\u00b7ben", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "APPR", "ADV", "ADJA", "NN", "PPER", "APPR", "ART", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "\u201egewaltfamlich beraubt: Es heist, da\u00df er uns st\u00e4rcke,", "tokens": ["\u201e", "ge\u00b7walt\u00b7fam\u00b7lich", "be\u00b7raubt", ":", "Es", "heist", ",", "da\u00df", "er", "uns", "st\u00e4r\u00b7cke", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "VVPP", "$.", "PPER", "VVFIN", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "\u201eund unser Leben uns verl\u00e4ngre: aber mercke,", "tokens": ["\u201e", "und", "un\u00b7ser", "Le\u00b7ben", "uns", "ver\u00b7l\u00e4ng\u00b7re", ":", "a\u00b7ber", "mer\u00b7cke", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "KON", "PPOSAT", "NN", "PPER", "VVFIN", "$.", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "\u201eda\u00df er davor die Helfft\u2019, als einen Lohn f\u00fcr sich,", "tokens": ["\u201e", "da\u00df", "er", "da\u00b7vor", "die", "Helfft'", ",", "als", "ei\u00b7nen", "Lohn", "f\u00fcr", "sich", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "PAV", "ART", "NN", "$,", "KOUS", "ART", "NN", "APPR", "PRF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "\u201ef\u00fcr seine M\u00fche nimmt. Ist es nicht sonderlich,", "tokens": ["\u201e", "f\u00fcr", "sei\u00b7ne", "M\u00fc\u00b7he", "nimmt", ".", "Ist", "es", "nicht", "son\u00b7der\u00b7lich", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PPOSAT", "NN", "VVFIN", "$.", "VAFIN", "PPER", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "\u201eda\u00df, um das Leben zu erwerben,", "tokens": ["\u201e", "da\u00df", ",", "um", "das", "Le\u00b7ben", "zu", "er\u00b7wer\u00b7ben", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "$,", "KOUI", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "\u201eman gleichsam t\u00e4glich m\u00fcsse sterben,", "tokens": ["\u201e", "man", "gleich\u00b7sam", "t\u00e4g\u00b7lich", "m\u00fcs\u00b7se", "ster\u00b7ben", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PIS", "ADJD", "ADJD", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "\u201ehat nicht ein Schlafender dieselbige Gestalt", "tokens": ["\u201e", "hat", "nicht", "ein", "Schla\u00b7fen\u00b7der", "die\u00b7sel\u00b7bi\u00b7ge", "Ge\u00b7stalt"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "VAFIN", "PTKNEG", "ART", "NN", "PDAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "\u201evon Todten, welcher warm, und gleicht ein Todter nicht", "tokens": ["\u201e", "von", "Tod\u00b7ten", ",", "wel\u00b7cher", "warm", ",", "und", "gleicht", "ein", "Tod\u00b7ter", "nicht"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "APPR", "NN", "$,", "PRELS", "ADJD", "$,", "KON", "VVFIN", "ART", "NN", "PTKNEG"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "\u201efast einem Schlafenden, der kalt?", "tokens": ["\u201e", "fast", "ei\u00b7nem", "Schla\u00b7fen\u00b7den", ",", "der", "kalt", "?"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ADV", "ART", "NN", "$,", "PRELS", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Ach lieber Mensch! wer also spricht,", "tokens": ["Ach", "lie\u00b7ber", "Mensch", "!", "wer", "al\u00b7so", "spricht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "NN", "$.", "PWS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Zeigt fast nichts menschliches, und spricht mit Unbedacht.", "tokens": ["Zeigt", "fast", "nichts", "menschli\u00b7ches", ",", "und", "spricht", "mit", "Un\u00b7be\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PIS", "ADJA", "$,", "KON", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.20": {"text": "Wo wir den s\u00fcssen Schlaf in Andacht nicht betrachten,", "tokens": ["Wo", "wir", "den", "s\u00fcs\u00b7sen", "Schlaf", "in", "An\u00b7dacht", "nicht", "be\u00b7trach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN", "APPR", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Und ihn nicht, als gew\u00fcrckt von GOTTES Finger achten;", "tokens": ["Und", "ihn", "nicht", ",", "als", "ge\u00b7w\u00fcrckt", "von", "GoT\u00b7TES", "Fin\u00b7ger", "ach\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PTKNEG", "$,", "KOUS", "VVPP", "APPR", "NE", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "So haben wir das Leben zugebracht", "tokens": ["So", "ha\u00b7ben", "wir", "das", "Le\u00b7ben", "zu\u00b7ge\u00b7bracht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Noch \u00e4rger, als im Schlaf, ja g\u00e4ntzlich wie ein Vieh.", "tokens": ["Noch", "\u00e4r\u00b7ger", ",", "als", "im", "Schlaf", ",", "ja", "g\u00e4ntz\u00b7lich", "wie", "ein", "Vieh", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "APPRART", "NN", "$,", "ADV", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}