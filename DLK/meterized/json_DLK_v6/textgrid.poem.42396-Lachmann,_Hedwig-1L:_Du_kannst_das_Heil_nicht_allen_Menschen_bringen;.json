{"textgrid.poem.42396": {"metadata": {"author": {"name": "Lachmann, Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du kannst das Heil nicht allen Menschen bringen;", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du kannst das Heil nicht allen Menschen bringen;", "tokens": ["Du", "kannst", "das", "Heil", "nicht", "al\u00b7len", "Men\u00b7schen", "brin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PTKNEG", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch werden Hunderte sich um dich scharen,", "tokens": ["Doch", "wer\u00b7den", "Hun\u00b7der\u00b7te", "sich", "um", "dich", "scha\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "NN", "PRF", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die willig sind zu dienen und willfahren", "tokens": ["Die", "wil\u00b7lig", "sind", "zu", "die\u00b7nen", "und", "will\u00b7fah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "VAFIN", "PTKZU", "VVINF", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und stark in ihrem Glauben ans Gelingen.", "tokens": ["Und", "stark", "in", "ih\u00b7rem", "Glau\u00b7ben", "ans", "Ge\u00b7lin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nur um die wenigen gilt es zu ringen,", "tokens": ["Nur", "um", "die", "we\u00b7ni\u00b7gen", "gilt", "es", "zu", "rin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PRELS", "PIS", "VVFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dass diese ihren Br\u00fcdern offenbaren,", "tokens": ["Dass", "die\u00b7se", "ih\u00b7ren", "Br\u00fc\u00b7dern", "of\u00b7fen\u00b7ba\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was ihnen selbst im Innern widerfahren,", "tokens": ["Was", "ih\u00b7nen", "selbst", "im", "In\u00b7nern", "wi\u00b7der\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit Gl\u00e4ubigkeit einander zu durchdringen.", "tokens": ["Mit", "Gl\u00e4u\u00b7big\u00b7keit", "ein\u00b7an\u00b7der", "zu", "durch\u00b7drin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Millionen sind es, die vor Gott sich beugen,", "tokens": ["Mil\u00b7lion\u00b7en", "sind", "es", ",", "die", "vor", "Gott", "sich", "beu\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "$,", "PRELS", "APPR", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch nur ein H\u00e4uflein k\u00fcndet den Gesandten,", "tokens": ["Doch", "nur", "ein", "H\u00e4uf\u00b7lein", "k\u00fcn\u00b7det", "den", "Ge\u00b7sand\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der in ihr Reich zur\u00fcckf\u00fchrt die Verbannten.", "tokens": ["Der", "in", "ihr", "Reich", "zu\u00b7r\u00fcck\u00b7f\u00fchrt", "die", "Ver\u00b7bann\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "So sollst auch du ins Volk die Botschaft tragen", "tokens": ["So", "sollst", "auch", "du", "ins", "Volk", "die", "Bot\u00b7schaft", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ADV", "PPER", "APPRART", "NN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von der Erneuerung in unsern Tagen \u2013", "tokens": ["Von", "der", "Er\u00b7neu\u00b7e\u00b7rung", "in", "un\u00b7sern", "Ta\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und da und dort wird einer f\u00fcr dich zeugen.", "tokens": ["Und", "da", "und", "dort", "wird", "ei\u00b7ner", "f\u00fcr", "dich", "zeu\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KON", "ADV", "VAFIN", "ART", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}