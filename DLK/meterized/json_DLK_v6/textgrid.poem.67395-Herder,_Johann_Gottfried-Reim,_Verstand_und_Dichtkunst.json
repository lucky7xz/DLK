{"textgrid.poem.67395": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Reim, Verstand und Dichtkunst", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verschwunden war die Dichtkunst von der Erde,", "tokens": ["Ver\u00b7schwun\u00b7den", "war", "die", "Dicht\u00b7kunst", "von", "der", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ver\u00f6det lag ihr sch\u00f6nes Vaterland;", "tokens": ["Ver\u00b7\u00f6\u00b7det", "lag", "ihr", "sch\u00f6\u00b7nes", "Va\u00b7ter\u00b7land", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da traten auf den Platz mit Ritterthums Geberde", "tokens": ["Da", "tra\u00b7ten", "auf", "den", "Platz", "mit", "Rit\u00b7ter\u00b7thums", "Ge\u00b7ber\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "APPR", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein Araber, der Reim, ein Normann, der Verstand.", "tokens": ["Ein", "A\u00b7ra\u00b7ber", ",", "der", "Reim", ",", "ein", "Nor\u00b7mann", ",", "der", "Ver\u00b7stand", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+---+-+-+-+", "measure": "dactylic.init"}, "line.5": {"text": "Sie k\u00e4mpften lang' mit wechselnder Beschwerde", "tokens": ["Sie", "k\u00e4mpf\u00b7ten", "lang'", "mit", "wech\u00b7seln\u00b7der", "Be\u00b7schwer\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und wurden dann im Streit vertraulich und galant.", "tokens": ["Und", "wur\u00b7den", "dann", "im", "Streit", "ver\u00b7trau\u00b7lich", "und", "ga\u00b7lant", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "APPRART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Dichtkunst kam. Wem wird der Preis geb\u00fchren?", "tokens": ["Die", "Dicht\u00b7kunst", "kam", ".", "Wem", "wird", "der", "Preis", "ge\u00b7b\u00fch\u00b7ren", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "PWS", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "\u00bbthut Eure Kappen ab! Wie hei\u00dfest Du?\u00ab \u00bbVerstand.\u00ab", "tokens": ["\u00bb", "thut", "Eu\u00b7re", "Kap\u00b7pen", "ab", "!", "Wie", "hei\u00b7\u00dfest", "Du", "?", "\u00ab", "\u00bb", "Ver\u00b7stand", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "punct", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$.", "PWAV", "VVFIN", "PPER", "$.", "$(", "$(", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u00bbund Du?\u00ab \u00bbDer Reim.\u00ab \u00bbIhr Herrn, Ihr m\u00fc\u00dft nicht Kriege f\u00fchren;", "tokens": ["\u00bb", "und", "Du", "?", "\u00ab", "\u00bb", "Der", "Reim", ".", "\u00ab", "\u00bb", "Ihr", "Herrn", ",", "Ihr", "m\u00fc\u00dft", "nicht", "Krie\u00b7ge", "f\u00fch\u00b7ren", ";"], "token_info": ["punct", "word", "word", "punct", "punct", "punct", "word", "word", "punct", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PPER", "$.", "$(", "$(", "ART", "NN", "$.", "$(", "$(", "PPOSAT", "NN", "$,", "PPER", "VMFIN", "PTKNEG", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Gebt Euch, der Reim zuerst, einander treu die Hand!", "tokens": ["Gebt", "Euch", ",", "der", "Reim", "zu\u00b7erst", ",", "ein\u00b7an\u00b7der", "treu", "die", "Hand", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "ART", "NN", "ADV", "$,", "PRF", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wollt Ihr mir dienen, so mu\u00df ich regieren;", "tokens": ["Wollt", "Ihr", "mir", "die\u00b7nen", ",", "so", "mu\u00df", "ich", "re\u00b7gie\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "VVINF", "$,", "ADV", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Du reite hinten, Reim, Du vor mir her, Verstand!\u00ab", "tokens": ["Du", "rei\u00b7te", "hin\u00b7ten", ",", "Reim", ",", "Du", "vor", "mir", "her", ",", "Ver\u00b7stand", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "NN", "$,", "PPER", "APPR", "PPER", "PTKVZ", "$,", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Sie zogen. Doch der k\u00fchne Normannreiter", "tokens": ["Sie", "zo\u00b7gen", ".", "Doch", "der", "k\u00fch\u00b7ne", "Nor\u00b7mann\u00b7rei\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Durchstrich so wild und kreuz und quer das Land!", "tokens": ["Durch\u00b7strich", "so", "wild", "und", "kreuz", "und", "quer", "das", "Land", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "KON", "NN", "KON", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Dichtkunst rief. Umsonst! \u00bbDort folg' ich ihm nicht weiter,\u00ab", "tokens": ["Die", "Dicht\u00b7kunst", "rief", ".", "Um\u00b7sonst", "!", "\u00bb", "Dort", "fol\u00b7g'", "ich", "ihm", "nicht", "wei\u00b7ter", ",", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "NN", "$.", "$(", "ADV", "VVFIN", "PPER", "PPER", "PTKNEG", "PTKVZ", "$,", "$("], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Sprach sie und neigte sich anmuthig und verschwand.", "tokens": ["Sprach", "sie", "und", "neig\u00b7te", "sich", "an\u00b7mut\u00b7hig", "und", "ver\u00b7schwand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "KON", "VVFIN", "PRF", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "\u00bbso bin ", "tokens": ["\u00bb", "so", "bin"], "token_info": ["punct", "word", "word"], "pos": ["$(", "ADV", "VAFIN"], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "\u00bbund treff' ich ihn, ergreif' ich hurtig den Verstand.\u00ab", "tokens": ["\u00bb", "und", "treff'", "ich", "ihn", ",", "er\u00b7greif'", "ich", "hur\u00b7tig", "den", "Ver\u00b7stand", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KON", "VVFIN", "PPER", "PPER", "$,", "VVFIN", "PPER", "ADJD", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}