{"dta.poem.8488": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Im Grase .", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1860", "urn": "urn:nbn:de:kobv:b4-200905191007", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "S&#252;\u00dfe Ruh, s&#252;\u00dfer Taumel im Gras,", "tokens": ["S", "&#252;", "\u00dfe", "Ruh", ",", "s", "&#252;", "\u00dfer", "Tau\u00b7mel", "im", "Gras", ","], "token_info": ["word", "XML_entity", "word", "word", "punct", "word", "XML_entity", "word", "word", "word", "word", "punct"], "pos": ["XY", "$(", "ADJA", "NN", "$,", "NE", "$(", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Von des Krautes Arom umhaucht,", "tokens": ["Von", "des", "Krau\u00b7tes", "A\u00b7rom", "um\u00b7haucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Tiefe Fluth, tief, tieftrunkne Fluth,", "tokens": ["Tie\u00b7fe", "Fluth", ",", "tief", ",", "tief\u00b7trunk\u00b7ne", "Fluth", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJD", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Wenn die Wolk\u2019 am Azure verraucht,", "tokens": ["Wenn", "die", "Wol\u00b7k'", "am", "A\u00b7zu\u00b7re", "ver\u00b7raucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "--+--+---+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Wenn auf\u2019s m\u00fcde, schwimmende Haupt", "tokens": ["Wenn", "auf's", "m\u00fc\u00b7de", ",", "schwim\u00b7men\u00b7de", "Haupt"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "APPRART", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "S\u00fc\u00dfes Lachen gaukelt herab,", "tokens": ["S\u00fc\u00b7\u00dfes", "La\u00b7chen", "gau\u00b7kelt", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ADV", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Liebe Stimme s\u00e4uselt, und tr\u00e4uft", "tokens": ["Lie\u00b7be", "Stim\u00b7me", "s\u00e4u\u00b7selt", ",", "und", "tr\u00e4uft"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "$,", "KON", "VVFIN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Wie die Lindenbl\u00fcth\u2019 auf ein Grab.", "tokens": ["Wie", "die", "Lin\u00b7den\u00b7bl\u00fcth'", "auf", "ein", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Wenn im Busen die Todten dann,", "tokens": ["Wenn", "im", "Bu\u00b7sen", "die", "Tod\u00b7ten", "dann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "ART", "NN", "ADV", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Jede Leiche sich streckt und regt,", "tokens": ["Je\u00b7de", "Lei\u00b7che", "sich", "streckt", "und", "regt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PRF", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Leise, leise den Odem zieht,", "tokens": ["Lei\u00b7se", ",", "lei\u00b7se", "den", "O\u00b7dem", "zieht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Die geschlossne Wimper bewegt,", "tokens": ["Die", "ge\u00b7schloss\u00b7ne", "Wim\u00b7per", "be\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Todte Lieb\u2019, todte Lust, todte Zeit,", "tokens": ["Tod\u00b7te", "Lieb'", ",", "tod\u00b7te", "Lust", ",", "tod\u00b7te", "Zeit", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "All\u2019 die Sch\u00e4tze, im Schutt verw\u00fchlt,", "tokens": ["All'", "die", "Sch\u00e4t\u00b7ze", ",", "im", "Schutt", "ver\u00b7w\u00fchlt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "NN", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.7": {"text": "Sich ber\u00fchren mit sch\u00fcchternem Klang", "tokens": ["Sich", "be\u00b7r\u00fch\u00b7ren", "mit", "sch\u00fcch\u00b7ter\u00b7nem", "Klang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PRF", "VVINF", "APPR", "ADJA", "NN"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Gleich den Gl\u00f6ckchen, vom Winde umspielt.", "tokens": ["Gleich", "den", "Gl\u00f6ck\u00b7chen", ",", "vom", "Win\u00b7de", "um\u00b7spielt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "APPRART", "NN", "VVPP", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}}, "stanza.3": {"line.1": {"text": "Stunden, fl\u00fcchtiger ihr als der Ku\u00df", "tokens": ["Stun\u00b7den", ",", "fl\u00fcch\u00b7ti\u00b7ger", "ihr", "als", "der", "Ku\u00df"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADJA", "PPER", "KOUS", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Eines Strahls auf den trauernden See,", "tokens": ["Ei\u00b7nes", "Strahls", "auf", "den", "trau\u00b7ern\u00b7den", "See", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Als des ziehenden Vogels Lied,", "tokens": ["Als", "des", "zie\u00b7hen\u00b7den", "Vo\u00b7gels", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Das mir niederperlt aus der H\u00f6h\u2019,", "tokens": ["Das", "mir", "nie\u00b7der\u00b7perlt", "aus", "der", "H\u00f6h'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Als des schillernden K\u00e4fers Blitz,", "tokens": ["Als", "des", "schil\u00b7lern\u00b7den", "K\u00e4\u00b7fers", "Blitz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "Wenn den Sonnenpfad er durcheilt,", "tokens": ["Wenn", "den", "Son\u00b7nen\u00b7pfad", "er", "durch\u00b7eilt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Als der fl\u00fccht\u2019ge Druck einer Hand,", "tokens": ["Als", "der", "fl\u00fccht'\u00b7ge", "Druck", "ei\u00b7ner", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "Die zum letzten Male verweilt.", "tokens": ["Die", "zum", "letz\u00b7ten", "Ma\u00b7le", "ver\u00b7weilt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.4": {"line.1": {"text": "Dennoch, Himmel, immer mir nur,", "tokens": ["Den\u00b7noch", ",", "Him\u00b7mel", ",", "im\u00b7mer", "mir", "nur", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "$,", "ADV", "PPER", "ADV", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Dieses Eine nur: f\u00fcr das Lied", "tokens": ["Die\u00b7ses", "Ei\u00b7ne", "nur", ":", "f\u00fcr", "das", "Lied"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "ART", "ADV", "$.", "APPR", "ART", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Jedes freien Vogels im Blau", "tokens": ["Je\u00b7des", "frei\u00b7en", "Vo\u00b7gels", "im", "Blau"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "APPRART", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Eine Seele, die mit ihm zieht,", "tokens": ["Ei\u00b7ne", "See\u00b7le", ",", "die", "mit", "ihm", "zieht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "Nur f\u00fcr jeden k\u00e4rglichen Strahl", "tokens": ["Nur", "f\u00fcr", "je\u00b7den", "k\u00e4rg\u00b7li\u00b7chen", "Strahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PIAT", "ADJA", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Meinen farbigschillernden Saum,", "tokens": ["Mei\u00b7nen", "far\u00b7big\u00b7schil\u00b7lern\u00b7den", "Saum", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Jeder warmen Hand meinen Druck,", "tokens": ["Je\u00b7der", "war\u00b7men", "Hand", "mei\u00b7nen", "Druck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Und f\u00fcr jedes Gl\u00fcck einen Traum.", "tokens": ["Und", "f\u00fcr", "je\u00b7des", "Gl\u00fcck", "ei\u00b7nen", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "NN", "ART", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}}}}