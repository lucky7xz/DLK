 "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "das blutige Mal, den roten Streif? \u2013", "tokens": ["das", "blu\u00b7ti\u00b7ge", "Mal", ",", "den", "ro\u00b7ten", "Streif", "?", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Er dr\u00fcckte weh und wund mein Hirn,", "tokens": ["Er", "dr\u00fcck\u00b7te", "weh", "und", "wund", "mein", "Hirn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und ich zerbrach den Kettenreif.", "tokens": ["und", "ich", "zer\u00b7brach", "den", "Ket\u00b7ten\u00b7reif", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.25": {"line.1": {"text": "Des frommen Spieles ward ich m\u00fcd,", "tokens": ["Des", "from\u00b7men", "Spie\u00b7les", "ward", "ich", "m\u00fcd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "aus meinem Herzen bricht ein Schrei:", "tokens": ["aus", "mei\u00b7nem", "Her\u00b7zen", "bricht", "ein", "Schrei", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "es wogt die Nacht \u2013 die Lippe gl\u00fcht \u2013", "tokens": ["es", "wogt", "die", "Nacht", "\u2013", "die", "Lip\u00b7pe", "gl\u00fcht", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "pun