rd", "word", "word", "punct"], "pos": ["VVPP", "PPOSAT", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "auf deiner stolzen Stirne", "tokens": ["auf", "dei\u00b7ner", "stol\u00b7zen", "Stir\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "ein Wort geschrieben stand.", "tokens": ["ein", "Wort", "ge\u00b7schrie\u00b7ben", "stand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.19": {"line.1": {"text": "In schweren dunklen Z\u00fcgen", "tokens": ["In", "schwe\u00b7ren", "dunk\u00b7len", "Z\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "ein r\u00e4tselwirres Wort, \u2013", "tokens": ["ein", "r\u00e4t\u00b7sel\u00b7wir\u00b7res", "Wort", ",", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "ich seh' vor meinen Augen", "tokens": ["ich", "seh'", "vor", "mei\u00b7nen", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "es leuchten immerfort.", "tokens": ["es", "leuch\u00b7ten", "im\u00b7mer\u00b7fort", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.20": {"line.1": {"text": "Es gl\u00fcht in meinem Herzen", "tokens": ["Es", "gl\u00fcht", "in", "mei\u00b7nem", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"t