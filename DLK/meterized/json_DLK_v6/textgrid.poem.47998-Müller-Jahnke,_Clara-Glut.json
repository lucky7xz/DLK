: "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "ja, hass' ich dich aus meines Herzens Grunde!", "tokens": ["ja", ",", "hass'", "ich", "dich", "aus", "mei\u00b7nes", "Her\u00b7zens", "Grun\u00b7de", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVIMP", "PPER", "PRF", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.22": {"line.1": {"text": "Ich r\u00fcttle wild das eiserne Geflecht,", "tokens": ["Ich", "r\u00fctt\u00b7le", "wild", "das", "ei\u00b7ser\u00b7ne", "Ge\u00b7flecht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "das ich mir selber habe schmieden m\u00fcssen;", "tokens": ["das", "ich", "mir", "sel\u00b7ber", "ha\u00b7be", "schmie\u00b7den", "m\u00fcs\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRELS", "PPER", "PPER", "ADV", "VAFIN", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "in deinen Armen hass' ich dich erst recht \u2013", "tokens": ["in", "dei\u00b7nen", "Ar\u00b7men", "hass'", "ich", "dich", "erst", "recht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "PRF", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "und t\u00f6ten m\u00f6cht' ich dich mit meinen K\u00fcssen!", "tokens": ["und", "t\u00f6\u00b7ten", "m\u00f6cht'", "ich", "dich", "mit", "mei\u00b7nen", "K\u00fcs\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "ia