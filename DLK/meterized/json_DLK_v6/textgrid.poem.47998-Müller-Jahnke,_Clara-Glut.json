{"textgrid.poem.47998": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "Glut", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit roten Kressen hatt' ich mich geschm\u00fcckt \u2013", "tokens": ["Mit", "ro\u00b7ten", "Kres\u00b7sen", "hatt'", "ich", "mich", "ge\u00b7schm\u00fcckt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "PPER", "PRF", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "du hast sie j\u00e4h an deiner Brust zerdr\u00fcckt.", "tokens": ["du", "hast", "sie", "j\u00e4h", "an", "dei\u00b7ner", "Brust", "zer\u00b7dr\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Mit bleichen Wangen bot ich dir den Gru\u00df \u2013", "tokens": ["Mit", "blei\u00b7chen", "Wan\u00b7gen", "bot", "ich", "dir", "den", "Gru\u00df", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "PPER", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "in Flammenwogen tauchte sie dein Ku\u00df.", "tokens": ["in", "Flam\u00b7men\u00b7wo\u00b7gen", "tauch\u00b7te", "sie", "dein", "Ku\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Mit ruhigem Herzschlag trat ich zu dir her, \u2013", "tokens": ["Mit", "ru\u00b7hi\u00b7gem", "Herz\u00b7schlag", "trat", "ich", "zu", "dir", "her", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "APPR", "PPER", "PTKVZ", "$,", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "und nun, und nun: ich kenne mich nicht mehr....", "tokens": ["und", "nun", ",", "und", "nun", ":", "ich", "ken\u00b7ne", "mich", "nicht", "mehr", "...."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "KON", "ADV", "$.", "PPER", "VVFIN", "PPER", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Nun lachst du mich verstohlen an", "tokens": ["Nun", "lachst", "du", "mich", "ver\u00b7stoh\u00b7len", "an"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "VVINF", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "mit dunklem Auge, du fremder Mann;", "tokens": ["mit", "dunk\u00b7lem", "Au\u00b7ge", ",", "du", "frem\u00b7der", "Mann", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "mit brennender Lippe streifst du mich \u2013", "tokens": ["mit", "bren\u00b7nen\u00b7der", "Lip\u00b7pe", "streifst", "du", "mich", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "PRF", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "hei\u00df pocht mein Herz: ich kenne dich!", "tokens": ["hei\u00df", "pocht", "mein", "Herz", ":", "ich", "ken\u00b7ne", "dich", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Aus schw\u00fcler Tr\u00e4ume Zauberspuk,", "tokens": ["Aus", "schw\u00fc\u00b7ler", "Tr\u00e4u\u00b7me", "Zau\u00b7bers\u00b7puk", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "aus W\u00fcstenschemen voll Lug und Trug,", "tokens": ["aus", "W\u00fcs\u00b7ten\u00b7sche\u00b7men", "voll", "Lug", "und", "Trug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "aus Fr\u00fchlingsn\u00e4chten voll Windeswehn", "tokens": ["aus", "Fr\u00fch\u00b7lings\u00b7n\u00e4ch\u00b7ten", "voll", "Win\u00b7des\u00b7wehn"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "ADJD", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "hab ich dein Bild mir winken sehn!", "tokens": ["hab", "ich", "dein", "Bild", "mir", "win\u00b7ken", "sehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN", "PPER", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Aus d\u00fcster flammendem Morgenrot,", "tokens": ["Aus", "d\u00fcs\u00b7ter", "flam\u00b7men\u00b7dem", "Mor\u00b7gen\u00b7rot", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "das Hagelschauer den Saaten droht,", "tokens": ["das", "Ha\u00b7gel\u00b7schau\u00b7er", "den", "Saa\u00b7ten", "droht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "aus lohendem Blitz, wenn ein Wetter braut,", "tokens": ["aus", "lo\u00b7hen\u00b7dem", "Blitz", ",", "wenn", "ein", "Wet\u00b7ter", "braut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "hat schon dein Auge mich angeschaut . . .", "tokens": ["hat", "schon", "dein", "Au\u00b7ge", "mich", "an\u00b7ge\u00b7schaut", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "PPER", "VVPP", "$.", "$.", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "Nun trittst du selbst in meinen Pfad:", "tokens": ["Nun", "trittst", "du", "selbst", "in", "mei\u00b7nen", "Pfad", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "ich wei\u00df, da\u00df mein Verh\u00e4ngnis naht;", "tokens": ["ich", "wei\u00df", ",", "da\u00df", "mein", "Ver\u00b7h\u00e4ng\u00b7nis", "naht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "mit brennender Lippe streifst du mich \u2013", "tokens": ["mit", "bren\u00b7nen\u00b7der", "Lip\u00b7pe", "streifst", "du", "mich", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "PRF", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "wild rast mein Blut \u2013 ich gr\u00fc\u00dfe dich!", "tokens": ["wild", "rast", "mein", "Blut", "\u2013", "ich", "gr\u00fc\u00b7\u00dfe", "dich", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "$(", "PPER", "VVFIN", "PPER", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.8": {"line.1": {"text": "Und als ich aus dem liebebangen,", "tokens": ["Und", "als", "ich", "aus", "dem", "lie\u00b7be\u00b7ban\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "dem Kindertraum emporgeschreckt,", "tokens": ["dem", "Kin\u00b7der\u00b7traum", "em\u00b7por\u00b7ge\u00b7schreckt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "hieltest du meine Hand umfangen", "tokens": ["hiel\u00b7test", "du", "mei\u00b7ne", "Hand", "um\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "VVINF"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "und hast mit K\u00fcssen sie bedeckt.", "tokens": ["und", "hast", "mit", "K\u00fcs\u00b7sen", "sie", "be\u00b7deckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Ich hab im Blick dir lodern sehen", "tokens": ["Ich", "hab", "im", "Blick", "dir", "lo\u00b7dern", "se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "PPER", "VVINF", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "der Sehnsucht zwingende Gewalt \u2013 \u2013", "tokens": ["der", "Sehn\u00b7sucht", "zwin\u00b7gen\u00b7de", "Ge\u00b7walt", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$(", "$("], "meter": "-+-+---+", "measure": "unknown.measure.tri"}, "line.3": {"text": "ich sah die Fieberschauer gehen", "tokens": ["ich", "sah", "die", "Fie\u00b7ber\u00b7schau\u00b7er", "ge\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "durch deine trotzige Gestalt.", "tokens": ["durch", "dei\u00b7ne", "trot\u00b7zi\u00b7ge", "Ge\u00b7stalt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Umsonst! umsonst nun Kampf und Beben:", "tokens": ["Um\u00b7sonst", "!", "um\u00b7sonst", "nun", "Kampf", "und", "Be\u00b7ben", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "ADV", "ADV", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "du hast gewu\u00dft, was dir gefrommt . . . .", "tokens": ["du", "hast", "ge\u00b7wu\u00dft", ",", "was", "dir", "ge\u00b7frommt", ".", ".", ".", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "PWS", "PPER", "VVPP", "$.", "$.", "$.", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "ein Bl\u00fctenopfer war dein Leben,", "tokens": ["ein", "Bl\u00fc\u00b7ten\u00b7op\u00b7fer", "war", "dein", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "neige dein Haupt \u2013 der Herbststurm kommt!", "tokens": ["nei\u00b7ge", "dein", "Haupt", "\u2013", "der", "Herbst\u00b7sturm", "kommt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$(", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.11": {"line.1": {"text": "Auf meinen Lippen brennt dein Ku\u00df,", "tokens": ["Auf", "mei\u00b7nen", "Lip\u00b7pen", "brennt", "dein", "Ku\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "er brennt wie Feuer und S\u00fcnde,", "tokens": ["er", "brennt", "wie", "Feu\u00b7er", "und", "S\u00fcn\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "er brennt wie himmlischer Hochgenu\u00df", "tokens": ["er", "brennt", "wie", "himm\u00b7li\u00b7scher", "Hoch\u00b7ge\u00b7nu\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOKOM", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "und macht mich zum schwachen Kinde.", "tokens": ["und", "macht", "mich", "zum", "schwa\u00b7chen", "Kin\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.12": {"line.1": {"text": "Viel wilde Rosen erbl\u00fchn und gl\u00fchn", "tokens": ["Viel", "wil\u00b7de", "Ro\u00b7sen", "er\u00b7bl\u00fchn", "und", "gl\u00fchn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "VVINF", "KON", "VVINF"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "und gl\u00fchn und verwelken am Hage \u2013", "tokens": ["und", "gl\u00fchn", "und", "ver\u00b7wel\u00b7ken", "am", "Ha\u00b7ge", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "VVFIN", "APPRART", "NN", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "und der Wald ist duftig, der Wald ist gr\u00fcn", "tokens": ["und", "der", "Wald", "ist", "duf\u00b7tig", ",", "der", "Wald", "ist", "gr\u00fcn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "VAFIN", "ADJD"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "am leuchtenden Julitage . . . . .", "tokens": ["am", "leuch\u00b7ten\u00b7den", "Ju\u00b7li\u00b7ta\u00b7ge", ".", ".", ".", ".", "."], "token_info": ["word", "word", "word", "punct", "punct", "punct", "punct", "punct"], "pos": ["APPRART", "ADJA", "NN", "$.", "$.", "$.", "$.", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.13": {"line.1": {"text": "Vom Meer herauf die Sonne gr\u00fc\u00dft,", "tokens": ["Vom", "Meer", "her\u00b7auf", "die", "Son\u00b7ne", "gr\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Tautropfen am Riedgras beben: \u2013 \u2013", "tokens": ["Tau\u00b7trop\u00b7fen", "am", "Ried\u00b7gras", "be\u00b7ben", ":", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NN", "APPRART", "NN", "VVINF", "$.", "$(", "$("], "meter": "++--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "wir haben uns kaum Willkommen gek\u00fc\u00dft", "tokens": ["wir", "ha\u00b7ben", "uns", "kaum", "Will\u00b7kom\u00b7men", "ge\u00b7k\u00fc\u00dft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "NN", "VVPP"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "und sollen uns Abschied geben!", "tokens": ["und", "sol\u00b7len", "uns", "Ab\u00b7schied", "ge\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.14": {"line.1": {"text": "Und gehen sollst du, geliebter Mann,", "tokens": ["Und", "ge\u00b7hen", "sollst", "du", ",", "ge\u00b7lieb\u00b7ter", "Mann", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "mit all' dem zitternden Bangen,", "tokens": ["mit", "all'", "dem", "zit\u00b7tern\u00b7den", "Ban\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "mit der ungel\u00f6schten Glut hindann \u2013", "tokens": ["mit", "der", "un\u00b7ge\u00b7l\u00f6schten", "Glut", "hin\u00b7dann", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADV", "$("], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "und durften uns kaum umfangen.", "tokens": ["und", "durf\u00b7ten", "uns", "kaum", "um\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.15": {"line.1": {"text": "Wie lange w\u00e4hrt es, so schwillt der Wein,", "tokens": ["Wie", "lan\u00b7ge", "w\u00e4hrt", "es", ",", "so", "schwillt", "der", "Wein", ","], "token_info": ["word", "word", "word", "w