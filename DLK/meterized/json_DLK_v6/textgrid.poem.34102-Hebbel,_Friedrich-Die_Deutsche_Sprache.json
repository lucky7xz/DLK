{"textgrid.poem.34102": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Deutsche Sprache", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6n erscheint sie mir nicht, die Deutsche Sprache, doch sch\u00f6n ist", "tokens": ["Sch\u00f6n", "er\u00b7scheint", "sie", "mir", "nicht", ",", "die", "Deut\u00b7sche", "Spra\u00b7che", ",", "doch", "sch\u00f6n", "ist"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "PPER", "PTKNEG", "$,", "ART", "ADJA", "NN", "$,", "ADV", "ADJD", "VAFIN"], "meter": "+-+--+-+-+-+++", "measure": "trochaic.octa.plus.relaxed"}, "line.2": {"text": "Auch die franz\u00f6sische nicht, nur die italische klingt.", "tokens": ["Auch", "die", "fran\u00b7z\u00f6\u00b7si\u00b7sche", "nicht", ",", "nur", "die", "i\u00b7ta\u00b7li\u00b7sche", "klingt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "PTKNEG", "$,", "ADV", "ART", "ADJA", "VVFIN", "$."], "meter": "--+-+--+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Aber ich finde sie reich, wie irgend eine der V\u00f6lker,", "tokens": ["A\u00b7ber", "ich", "fin\u00b7de", "sie", "reich", ",", "wie", "ir\u00b7gend", "ei\u00b7ne", "der", "V\u00f6l\u00b7ker", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADJD", "$,", "PWAV", "ADV", "ART", "ART", "NN", "$,"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Finde den k\u00f6stlichsten Schatz treffender W\u00f6rter geh\u00e4uft,", "tokens": ["Fin\u00b7de", "den", "k\u00f6st\u00b7lichs\u00b7ten", "Schatz", "tref\u00b7fen\u00b7der", "W\u00f6r\u00b7ter", "ge\u00b7h\u00e4uft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "ADJA", "NN", "VVPP", "$,"], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Finde unendliche Freiheit, sie so und anders zu stellen,", "tokens": ["Fin\u00b7de", "un\u00b7end\u00b7li\u00b7che", "Frei\u00b7heit", ",", "sie", "so", "und", "an\u00b7ders", "zu", "stel\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,", "PPER", "ADV", "KON", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.6": {"text": "Bis der Gedanke die Form, bis er die F\u00e4rbung erlangt", "tokens": ["Bis", "der", "Ge\u00b7dan\u00b7ke", "die", "Form", ",", "bis", "er", "die", "F\u00e4r\u00b7bung", "er\u00b7langt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,", "KOUS", "PPER", "ART", "NN", "VVPP"], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.7": {"text": "Bis er sich leicht verwebt mit fremden Gedanken, und dennoch", "tokens": ["Bis", "er", "sich", "leicht", "ver\u00b7webt", "mit", "frem\u00b7den", "Ge\u00b7dan\u00b7ken", ",", "und", "den\u00b7noch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "VVFIN", "APPR", "ADJA", "NN", "$,", "KON", "ADV"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.8": {"text": "Das Gepr\u00e4ge des Ichs, dem er entsprang, nicht verliert.", "tokens": ["Das", "Ge\u00b7pr\u00e4\u00b7ge", "des", "Ichs", ",", "dem", "er", "ent\u00b7sprang", ",", "nicht", "ver\u00b7liert", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "PTKNEG", "VVFIN", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.9": {"text": "Denn der Genius, welcher im Ganzen und Gro\u00dfen hier waltet,", "tokens": ["Denn", "der", "Ge\u00b7nius", ",", "wel\u00b7cher", "im", "Gan\u00b7zen", "und", "Gro\u00b7\u00dfen", "hier", "wal\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "APPRART", "NN", "KON", "NN", "ADV", "VVFIN", "$,"], "meter": "+--+---+--+--+-", "measure": "dactylic.di.plus"}, "line.10": {"text": "Fesselt den schaffenden Geist nicht durch ein strenges Gesetz,", "tokens": ["Fes\u00b7selt", "den", "schaf\u00b7fen\u00b7den", "Geist", "nicht", "durch", "ein", "stren\u00b7ges", "Ge\u00b7setz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "PTKNEG", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.11": {"text": "Ueberl\u00e4\u00dft ihn sich selbst, verg\u00f6nnt ihm die frei'ste Bewegung", "tokens": ["Ue\u00b7berl\u00b7\u00e4\u00dft", "ihn", "sich", "selbst", ",", "ver\u00b7g\u00f6nnt", "ihm", "die", "frei'\u00b7ste", "Be\u00b7we\u00b7gung"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "$,", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.12": {"text": "Und bewahrt sich dadurch ewig lebendigen Reiz.", "tokens": ["Und", "be\u00b7wahrt", "sich", "da\u00b7durch", "e\u00b7wig", "le\u00b7ben\u00b7di\u00b7gen", "Reiz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "PAV", "ADJD", "ADJA", "NN", "$."], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "H\u00fctet euch nur, ihr Dichter, in dieser edlen Verl\u00e4ugnung", "tokens": ["H\u00fc\u00b7tet", "euch", "nur", ",", "ihr", "Dich\u00b7ter", ",", "in", "die\u00b7ser", "ed\u00b7len", "Ver\u00b7l\u00e4ug\u00b7nung"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "$,", "PPOSAT", "NN", "$,", "APPR", "PDAT", "ADJA", "NN"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.14": {"text": "Ihn zu kr\u00e4nken, zerbrecht nicht mit dem Joche das Maa\u00df,", "tokens": ["Ihn", "zu", "kr\u00e4n\u00b7ken", ",", "zer\u00b7brecht", "nicht", "mit", "dem", "Jo\u00b7che", "das", "Maa\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "VVFIN", "PTKNEG", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.15": {"text": "Glaubt nicht, zu gewinnen, wenn kindisch zerstochen, die D\u00e4mme", "tokens": ["Glaubt", "nicht", ",", "zu", "ge\u00b7win\u00b7nen", ",", "wenn", "kin\u00b7disch", "zer\u00b7sto\u00b7chen", ",", "die", "D\u00e4m\u00b7me"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PTKNEG", "$,", "PTKZU", "VVINF", "$,", "KOUS", "ADJD", "VVINF", "$,", "ART", "NN"], "meter": "----+--+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.16": {"text": "Bersten und rei\u00dfen; es f\u00fchrt wieder nach Babel zur\u00fcck,", "tokens": ["Bers\u00b7ten", "und", "rei\u00b7\u00dfen", ";", "es", "f\u00fchrt", "wie\u00b7der", "nach", "Ba\u00b7bel", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "$.", "PPER", "VVFIN", "ADV", "APPR", "NE", "PTKVZ", "$,"], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}, "line.17": {"text": "Oder wer setzte Barbaren im Ungebund'nen die Gr\u00e4nze?", "tokens": ["O\u00b7der", "wer", "setz\u00b7te", "Bar\u00b7ba\u00b7ren", "im", "Un\u00b7ge\u00b7bun\u00b7d'\u00b7nen", "die", "Gr\u00e4n\u00b7ze", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "NN", "APPRART", "NN", "ART", "NN", "$."], "meter": "+--+-+-+-+--+--+-", "measure": "iambic.septa.invert"}, "line.18": {"text": "Pa\u00dft doch am Ende: er ha\u00dft! f\u00fcr das gewohnte: er liebt!", "tokens": ["Pa\u00dft", "doch", "am", "En\u00b7de", ":", "er", "ha\u00dft", "!", "f\u00fcr", "das", "ge\u00b7wohn\u00b7te", ":", "er", "liebt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "NN", "$.", "PPER", "VVFIN", "$.", "APPR", "ART", "ADJA", "$.", "PPER", "VVFIN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.19": {"text": "Viel sind der Sprachen auf Erden, schon dieses sollte uns lehren,", "tokens": ["Viel", "sind", "der", "Spra\u00b7chen", "auf", "Er\u00b7den", ",", "schon", "die\u00b7ses", "soll\u00b7te", "uns", "leh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "NN", "$,", "ADV", "PDS", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+--+-", "measure": "iambic.septa.relaxed"}, "line.20": {"text": "Da\u00df kein inneres Band Dinge und Zeichen verkn\u00fcpft;", "tokens": ["Da\u00df", "kein", "in\u00b7ne\u00b7res", "Band", "Din\u00b7ge", "und", "Zei\u00b7chen", "ver\u00b7kn\u00fcpft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "ADJA", "NN", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+---++--+--+", "measure": "iambic.penta.relaxed"}, "line.21": {"text": "Darf sich aber darum ein Jeder die eigene bilden?", "tokens": ["Darf", "sich", "a\u00b7ber", "da\u00b7rum", "ein", "Je\u00b7der", "die", "ei\u00b7ge\u00b7ne", "bil\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "ADV", "PAV", "ART", "PIS", "ART", "ADJA", "VVINF", "$."], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.22": {"text": "Besser w\u00e4re der Mensch stumm, wie die Fische im Meer!", "tokens": ["Bes\u00b7ser", "w\u00e4\u00b7re", "der", "Mensch", "stumm", ",", "wie", "die", "Fi\u00b7sche", "im", "Meer", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "ADJD", "$,", "PWAV", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.23": {"text": "Seien die Stempel uns heilig, die alle Jahrhunderte brauchten,", "tokens": ["Sei\u00b7en", "die", "Stem\u00b7pel", "uns", "hei\u00b7lig", ",", "die", "al\u00b7le", "Jahr\u00b7hun\u00b7der\u00b7te", "brauch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "ADJD", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$,"], "meter": "+--+--+--+-+-+-+-", "measure": "dactylic.tri.plus"}, "line.24": {"text": "Sie es die Weise sogar, die sie bed\u00e4chtigt gew\u00e4hlt;", "tokens": ["Sie", "es", "die", "Wei\u00b7se", "so\u00b7gar", ",", "die", "sie", "be\u00b7d\u00e4ch\u00b7tigt", "ge\u00b7w\u00e4hlt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "ART", "NN", "ADV", "$,", "PRELS", "PPER", "ADJD", "VVPP", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.25": {"text": "Fand ein Goethe doch Raum in diesen gemessenen Schranken,", "tokens": ["Fand", "ein", "Goe\u00b7the", "doch", "Raum", "in", "die\u00b7sen", "ge\u00b7mes\u00b7se\u00b7nen", "Schran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NE", "KON", "NN", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.26": {"text": "W\u00e4ren sie pl\u00f6tzlich zu eng f\u00fcr die Heroen von heut'?", "tokens": ["W\u00e4\u00b7ren", "sie", "pl\u00f6tz\u00b7lich", "zu", "eng", "f\u00fcr", "die", "He\u00b7roen", "von", "heut'", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "PTKA", "ADJD", "APPR", "ART", "NN", "APPR", "ADV", "$."], "meter": "+--+--+-+-+-+", "measure": "dactylic.di.plus"}, "line.27": {"text": "Gleichen wir der Natur, die nie das Wunder der Sch\u00f6pfung", "tokens": ["Glei\u00b7chen", "wir", "der", "Na\u00b7tur", ",", "die", "nie", "das", "Wun\u00b7der", "der", "Sch\u00f6p\u00b7fung"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ART", "NN", "$,", "PRELS", "ADV", "ART", "NN", "ART", "NN"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.28": {"text": "Wiederholt und doch j\u00e4hrlich im Lenz sich erneut:", "tokens": ["Wie\u00b7der\u00b7holt", "und", "doch", "j\u00e4hr\u00b7lich", "im", "Lenz", "sich", "er\u00b7neut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADV", "ADJD", "APPRART", "NN", "PRF", "ADJD", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.29": {"text": "Alt sind die Formen, es kehren die Lilien wieder und Rosen,", "tokens": ["Alt", "sind", "die", "For\u00b7men", ",", "es", "keh\u00b7ren", "die", "Li\u00b7li\u00b7en", "wie\u00b7der", "und", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "ADV", "KON", "NN", "$,"], "meter": "+--+--+--+-+-+-+-", "measure": "dactylic.tri.plus"}, "line.30": {"text": "Frisch ist der Duft, und im Kranz thut sich der Meister hervor!", "tokens": ["Frisch", "ist", "der", "Duft", ",", "und", "im", "Kranz", "thut", "sich", "der", "Meis\u00b7ter", "her\u00b7vor", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$,", "KON", "APPRART", "NN", "VVFIN", "PRF", "ART", "NN", "PTKVZ", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}}}}}