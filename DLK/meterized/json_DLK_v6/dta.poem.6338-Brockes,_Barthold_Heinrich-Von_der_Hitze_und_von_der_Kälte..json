{"dta.poem.6338": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Hitze und von der K\u00e4lte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zumal das Oel und Wachs, in welchen, gleich den Zweigen,", "tokens": ["Zu\u00b7mal", "das", "O\u00b7el", "und", "Wachs", ",", "in", "wel\u00b7chen", ",", "gleich", "den", "Zwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "KON", "NN", "$,", "APPR", "PWAT", "$,", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "Sich eine Menge Theilchen zeigen,", "tokens": ["Sich", "ei\u00b7ne", "Men\u00b7ge", "Theil\u00b7chen", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wodurch, da sich gemach die krumme St\u00e4ubgen trennen,", "tokens": ["Wo\u00b7durch", ",", "da", "sich", "ge\u00b7mach", "die", "krum\u00b7me", "St\u00e4ub\u00b7gen", "tren\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "KOUS", "PRF", "ADV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Flammen lange Zeit, durch sie gen\u00e4hret, brennen.", "tokens": ["Die", "Flam\u00b7men", "lan\u00b7ge", "Zeit", ",", "durch", "sie", "ge\u00b7n\u00e4h\u00b7ret", ",", "bren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "APPR", "PPER", "VVPP", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Jhr dauerhafft und reines Licht,", "tokens": ["Ihr", "dau\u00b7er\u00b7hafft", "und", "rei\u00b7nes", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Macht Spiele, Fest, und B\u00e4lle,", "tokens": ["Macht", "Spie\u00b7le", ",", "Fest", ",", "und", "B\u00e4l\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "NN", "$,", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "In ausgezierten Zimmern helle,", "tokens": ["In", "aus\u00b7ge\u00b7zier\u00b7ten", "Zim\u00b7mern", "hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und stellet in der duncklen Nacht", "tokens": ["Und", "stel\u00b7let", "in", "der", "dunck\u00b7len", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Den Augen gleichsam vor des hellen Tages Pracht.", "tokens": ["Den", "Au\u00b7gen", "gleich\u00b7sam", "vor", "des", "hel\u00b7len", "Ta\u00b7ges", "Pracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}