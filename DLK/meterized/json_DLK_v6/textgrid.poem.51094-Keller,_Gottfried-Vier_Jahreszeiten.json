{"textgrid.poem.51094": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Vier Jahreszeiten", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und wieder gr\u00fcnt' der sch\u00f6ne Mai,", "tokens": ["Und", "wie\u00b7der", "gr\u00fcnt'", "der", "sch\u00f6\u00b7ne", "Mai", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O dreimal selige Zeit!", "tokens": ["O", "drei\u00b7mal", "se\u00b7li\u00b7ge", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Wie flog die Schwalbe froh herbei!", "tokens": ["Wie", "flog", "die", "Schwal\u00b7be", "froh", "her\u00b7bei", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als ob ich mitgeflogen sei,", "tokens": ["Als", "ob", "ich", "mit\u00b7ge\u00b7flo\u00b7gen", "sei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "War mir das Herz so weit!", "tokens": ["War", "mir", "das", "Herz", "so", "weit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "O linde Luft im fremden Land,", "tokens": ["O", "lin\u00b7de", "Luft", "im", "frem\u00b7den", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf Bergen und Gefild!", "tokens": ["Auf", "Ber\u00b7gen", "und", "Ge\u00b7fild", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie reizend fand ich diesen Strand,", "tokens": ["Wie", "rei\u00b7zend", "fand", "ich", "die\u00b7sen", "Strand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "VVFIN", "PPER", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Allwo mein suchend Auge fand", "tokens": ["All\u00b7wo", "mein", "su\u00b7chend", "Au\u00b7ge", "fand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "ADJD", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ihr leicht hinwandelnd Bild!", "tokens": ["Ihr", "leicht", "hin\u00b7wan\u00b7delnd", "Bild", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVPP", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich sah des Sommers helle Glut", "tokens": ["Ich", "sah", "des", "Som\u00b7mers", "hel\u00b7le", "Glut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Emp\u00f6rtes Land durchziehn;", "tokens": ["Em\u00b7p\u00f6r\u00b7tes", "Land", "durch\u00b7ziehn", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie stritten um das h\u00f6chste Gut,", "tokens": ["Sie", "strit\u00b7ten", "um", "das", "h\u00f6chs\u00b7te", "Gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Geschlagen mu\u00dft das freiste Blut", "tokens": ["Ge\u00b7schla\u00b7gen", "mu\u00dft", "das", "freis\u00b7te", "Blut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Aus hundert Wunden fliehn.", "tokens": ["Aus", "hun\u00b7dert", "Wun\u00b7den", "fliehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Kaum h\u00f6rt ich in verliebter Ruh", "tokens": ["Kaum", "h\u00f6rt", "ich", "in", "ver\u00b7lieb\u00b7ter", "Ruh"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der schw\u00fclen St\u00fcrme Wehn;", "tokens": ["Der", "schw\u00fc\u00b7len", "St\u00fcr\u00b7me", "Wehn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich wandte mich den Blumen zu", "tokens": ["Ich", "wand\u00b7te", "mich", "den", "Blu\u00b7men", "zu"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "PTKZU"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und sprach: \u00bbVielleicht, mein Herz, wirst du", "tokens": ["Und", "sprach", ":", "\u00bb", "Viel\u00b7leicht", ",", "mein", "Herz", ",", "wirst", "du"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "$.", "$(", "ADV", "$,", "PPOSAT", "NN", "$,", "VAFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein andres Herz erstehn!\u00ab", "tokens": ["Ein", "and\u00b7res", "Herz", "er\u00b7stehn", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Die Traube schwoll so frisch und blank,", "tokens": ["Die", "Trau\u00b7be", "schwoll", "so", "frisch", "und", "blank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADV", "ADJD", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und ich nahm beiderlei:", "tokens": ["Und", "ich", "nahm", "bei\u00b7der\u00b7lei", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+---+", "measure": "dactylic.init"}, "line.3": {"text": "Mit ihrem Gru\u00df den jungen Trank \u2013", "tokens": ["Mit", "ih\u00b7rem", "Gru\u00df", "den", "jun\u00b7gen", "Trank", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und als die letzte Traube sank,", "tokens": ["Und", "als", "die", "letz\u00b7te", "Trau\u00b7be", "sank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da war der Traum vorbei.", "tokens": ["Da", "war", "der", "Traum", "vor\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Doch jene, die zur Sommerzeit", "tokens": ["Doch", "je\u00b7ne", ",", "die", "zur", "Som\u00b7mer\u00b7zeit"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PDS", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Freiheit nachgejagt,", "tokens": ["Der", "Frei\u00b7heit", "nach\u00b7ge\u00b7jagt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie schwanden mit der Schwalbe weit,", "tokens": ["Sie", "schwan\u00b7den", "mit", "der", "Schwal\u00b7be", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie liegen im Friedhof eingeschneit,", "tokens": ["Sie", "lie\u00b7gen", "im", "Fried\u00b7hof", "ein\u00b7ge\u00b7schneit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Wo tr\u00fcb der Nachtwind klagt.", "tokens": ["Wo", "tr\u00fcb", "der", "Nacht\u00b7wind", "klagt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}