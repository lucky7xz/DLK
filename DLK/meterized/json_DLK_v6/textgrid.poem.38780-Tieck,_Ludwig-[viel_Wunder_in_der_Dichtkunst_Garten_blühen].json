{"textgrid.poem.38780": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "[viel Wunder in der Dichtkunst Garten bl\u00fchen]", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Viel Wunder in der Dichtkunst Garten bl\u00fchen.", "tokens": ["Viel", "Wun\u00b7der", "in", "der", "Dicht\u00b7kunst", "Gar\u00b7ten", "bl\u00fc\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es drohet als verschlingend Ungeheuer", "tokens": ["Es", "dro\u00b7het", "als", "ver\u00b7schlin\u00b7gend", "Un\u00b7ge\u00b7heu\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOKOM", "VVPP", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Allem, was lebt, das hunger-grimme Feuer,", "tokens": ["Al\u00b7lem", ",", "was", "lebt", ",", "das", "hun\u00b7ger\u00b7grim\u00b7me", "Feu\u00b7er", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PWS", "VVFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit seinem Raub dem Abgrund zuzufliehen:", "tokens": ["Mit", "sei\u00b7nem", "Raub", "dem", "Ab\u00b7grund", "zu\u00b7zu\u00b7flie\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nur einer Kreatur dr\u00e4ut nicht sein Gl\u00fchen,", "tokens": ["Nur", "ei\u00b7ner", "Kre\u00b7a\u00b7tur", "dr\u00e4ut", "nicht", "sein", "Gl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PTKNEG", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dem Salamander zeigt es sich getreuer,", "tokens": ["Dem", "Sa\u00b7la\u00b7man\u00b7der", "zeigt", "es", "sich", "ge\u00b7treu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PRF", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der f\u00fchlt sich in der Heimath, hold und theuer", "tokens": ["Der", "f\u00fchlt", "sich", "in", "der", "Hei\u00b7math", ",", "hold", "und", "theu\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PRF", "APPR", "ART", "NN", "$,", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ist ihm rundum der Flammen rothes Spr\u00fchen.", "tokens": ["Ist", "ihm", "run\u00b7dum", "der", "Flam\u00b7men", "ro\u00b7thes", "Spr\u00fc\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN", "ADJA", "NN", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Dies ist ein Bildni\u00df treuer Liebesherzen:", "tokens": ["Dies", "ist", "ein", "Bild\u00b7ni\u00df", "treu\u00b7er", "Lie\u00b7bes\u00b7her\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bist du mir nah, bin ich umweht von Flammen,", "tokens": ["Bist", "du", "mir", "nah", ",", "bin", "ich", "um\u00b7weht", "von", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ADJD", "$,", "VAFIN", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und jeder Blick saugt hei\u00df an meinem Blute,", "tokens": ["Und", "je\u00b7der", "Blick", "saugt", "hei\u00df", "an", "mei\u00b7nem", "Blu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch lebt das Herz so mehr im Liebes-Muthe,", "tokens": ["Doch", "lebt", "das", "Herz", "so", "mehr", "im", "Lie\u00b7bes\u00b7Mu\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als um mich n\u00e4her schl\u00e4gt der Brand zusammen,", "tokens": ["Als", "um", "mich", "n\u00e4\u00b7her", "schl\u00e4gt", "der", "Brand", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPER", "ADJD", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Erlischt er, t\u00f6dten mich der Sehnsucht Schmerzen.", "tokens": ["Er\u00b7lischt", "er", ",", "t\u00f6d\u00b7ten", "mich", "der", "Sehn\u00b7sucht", "Schmer\u00b7zen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}