{"dta.poem.16098": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "152.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du bleibst in deiner Klaus' und gehst nicht aus dem Haus,", "tokens": ["Du", "bleibst", "in", "dei\u00b7ner", "Klaus'", "und", "gehst", "nicht", "aus", "dem", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "KON", "VVFIN", "PTKNEG", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So blicke manchmal doch zum Fenster nur hinaus.", "tokens": ["So", "bli\u00b7cke", "manch\u00b7mal", "doch", "zum", "Fens\u00b7ter", "nur", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "APPRART", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und wenn zu deiner W\u00fcrd' auch das sich nicht will schicken,", "tokens": ["Und", "wenn", "zu", "dei\u00b7ner", "W\u00fcrd'", "auch", "das", "sich", "nicht", "will", "schi\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPR", "PPOSAT", "NN", "ADV", "PRELS", "PRF", "PTKNEG", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So la\u00df die Welt zu dir manchmal durchs Fenster blicken.", "tokens": ["So", "la\u00df", "die", "Welt", "zu", "dir", "manch\u00b7mal", "durchs", "Fens\u00b7ter", "bli\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "ART", "NN", "APPR", "PPER", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Dein Fenster liegt so hoch, nichts Niedres schaut herein,", "tokens": ["Dein", "Fens\u00b7ter", "liegt", "so", "hoch", ",", "nichts", "Nie\u00b7dres", "schaut", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "ADJD", "$,", "PIS", "PIS", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Am Tage nur die Sonn', und Nachts der Sterne Schein.", "tokens": ["Am", "Ta\u00b7ge", "nur", "die", "Sonn'", ",", "und", "Nachts", "der", "Ster\u00b7ne", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ART", "NN", "$,", "KON", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Was nicht die Sonne sieht, das werden Sterne sehn;", "tokens": ["Was", "nicht", "die", "Son\u00b7ne", "sieht", ",", "das", "wer\u00b7den", "Ster\u00b7ne", "sehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "ART", "NN", "VVFIN", "$,", "PDS", "VAFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und theilen sie dirs mit, so wird dir nichts entgehn.", "tokens": ["Und", "thei\u00b7len", "sie", "dirs", "mit", ",", "so", "wird", "dir", "nichts", "ent\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIS", "PTKVZ", "$,", "ADV", "VAFIN", "PPER", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}