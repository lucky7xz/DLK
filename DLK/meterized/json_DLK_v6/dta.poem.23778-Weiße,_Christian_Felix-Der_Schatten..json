{"dta.poem.23778": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "Der Schatten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1767", "urn": "urn:nbn:de:kobv:b4-20643-5", "language": ["de:0.99"], "booktitle": "Wei\u00dfe, Christian Felix: Lieder f\u00fcr Kinder. Leipzig, 1767."}, "poem": {"stanza.1": {"line.1": {"text": "Da l\u00e4uft mein Schatten vor mir hin:", "tokens": ["Da", "l\u00e4uft", "mein", "Schat\u00b7ten", "vor", "mir", "hin", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O seht doch! seht! wie gro\u00df ich bin!", "tokens": ["O", "seht", "doch", "!", "seht", "!", "wie", "gro\u00df", "ich", "bin", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "$.", "VVFIN", "$.", "PWAV", "ADJD", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mich wagt man Klein zu nennen? \u2014", "tokens": ["Mich", "wagt", "man", "Klein", "zu", "nen\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PIS", "NN", "PTKZU", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Doch ach! weg war ich! seh ichs nicht,", "tokens": ["Doch", "ach", "!", "weg", "war", "ich", "!", "seh", "ichs", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$.", "ADV", "VAFIN", "PPER", "$.", "VVFIN", "PIS", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein W\u00f6lkchen deckt der Sonne Licht?", "tokens": ["Ein", "W\u00f6lk\u00b7chen", "deckt", "der", "Son\u00b7ne", "Licht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So kann man sich verkennen!", "tokens": ["So", "kann", "man", "sich", "ver\u00b7ken\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der Herr dort, der sich vornehm", "tokens": ["Der", "Herr", "dort", ",", "der", "sich", "vor\u00b7nehm"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "$,", "PRELS", "PRF", "ADJD"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "bl\u00e4ht,", "tokens": ["bl\u00e4ht", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Lacht: doch wer wei\u00df, wies ihm ergeht,", "tokens": ["Lacht", ":", "doch", "wer", "wei\u00df", ",", "wies", "ihm", "er\u00b7geht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "KON", "PWS", "VVFIN", "$,", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "So gro\u00df wir ihn ietzt nennen.", "tokens": ["So", "gro\u00df", "wir", "ihn", "ietzt", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "PPER", "ADV", "VVINF", "$."], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.5": {"text": "Es nehme nur ein neidisch Gl\u00fcck", "tokens": ["Es", "neh\u00b7me", "nur", "ein", "nei\u00b7disch", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den g\u00fcldnen Sonnenschein zur\u00fcck:", "tokens": ["Den", "g\u00fcld\u00b7nen", "Son\u00b7nen\u00b7schein", "zu\u00b7r\u00fcck", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So wird man ihn nicht kennen!", "tokens": ["So", "wird", "man", "ihn", "nicht", "ken\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}