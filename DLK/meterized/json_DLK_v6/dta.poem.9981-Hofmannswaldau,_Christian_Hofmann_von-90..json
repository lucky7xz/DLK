{"dta.poem.9981": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "90.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich geb\u00e4hr mich aus mir selber/ mein geschlecht ist zweyerley/", "tokens": ["Ich", "ge\u00b7b\u00e4hr", "mich", "aus", "mir", "sel\u00b7ber", "/", "mein", "ge\u00b7schlecht", "ist", "zwey\u00b7er\u00b7ley", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPER", "ADV", "$(", "PPOSAT", "VVPP", "VAFIN", "ADV", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Zieh drey und mehr kleider an/ es mag/ wen es will/ verdr\u00fcssen;", "tokens": ["Zieh", "drey", "und", "mehr", "klei\u00b7der", "an", "/", "es", "mag", "/", "wen", "es", "will", "/", "ver\u00b7dr\u00fcs\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "CARD", "KON", "ADV", "ADJD", "PTKVZ", "$(", "PPER", "VMFIN", "$(", "PWS", "PPER", "VMFIN", "$(", "VVPP", "$."], "meter": "-+--+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Eher kan man meiner nicht/ bi\u00df ich nackend bin/ geniessen/", "tokens": ["E\u00b7her", "kan", "man", "mei\u00b7ner", "nicht", "/", "bi\u00df", "ich", "na\u00b7ckend", "bin", "/", "ge\u00b7nies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "PPOSAT", "PTKNEG", "$(", "KOUS", "PPER", "VVPP", "VAFIN", "$(", "VVPP", "$("], "meter": "-+--+--+-+-+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Weil man mich vorher verwundet/ so geschichts da\u00df ich nicht", "tokens": ["Weil", "man", "mich", "vor\u00b7her", "ver\u00b7wun\u00b7det", "/", "so", "ge\u00b7schichts", "da\u00df", "ich", "nicht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "PRF", "ADV", "VVPP", "$(", "ADV", "ADV", "KOUS", "PPER", "PTKNEG"], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.5": {"text": "schrey.", "tokens": ["schrey", "."], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "+", "measure": "single.up"}}}}}