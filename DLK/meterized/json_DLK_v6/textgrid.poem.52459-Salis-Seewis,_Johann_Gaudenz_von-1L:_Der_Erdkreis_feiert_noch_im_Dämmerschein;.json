{"textgrid.poem.52459": {"metadata": {"author": {"name": "Salis-Seewis, Johann Gaudenz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Erdkreis feiert noch im D\u00e4mmerschein;", "genre": "verse", "period": "N.A.", "pub_year": 1798, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Erdkreis feiert noch im D\u00e4mmerschein;", "tokens": ["Der", "Erd\u00b7kreis", "fei\u00b7ert", "noch", "im", "D\u00e4m\u00b7mer\u00b7schein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Still, wie die Lamp' in Tempelhallen, h\u00e4ngt", "tokens": ["Still", ",", "wie", "die", "Lamp'", "in", "Tem\u00b7pel\u00b7hal\u00b7len", ",", "h\u00e4ngt"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADJD", "$,", "PWAV", "ART", "NN", "APPR", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Morgenstern; es dampft vom Buchenhain,", "tokens": ["Der", "Mor\u00b7gens\u00b7tern", ";", "es", "dampft", "vom", "Bu\u00b7chen\u00b7hain", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der, Kuppeln gleich, empor die Wipfel dr\u00e4ngt.", "tokens": ["Der", ",", "Kup\u00b7peln", "gleich", ",", "em\u00b7por", "die", "Wip\u00b7fel", "dr\u00e4ngt", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "NN", "ADV", "$,", "VVFIN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Sieh, naher Felsen d\u00fcstre Zinn' entgl\u00fcht", "tokens": ["Sieh", ",", "na\u00b7her", "Fel\u00b7sen", "d\u00fcst\u00b7re", "Zinn'", "ent\u00b7gl\u00fcht"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "ADJA", "NN", "VVFIN", "NE", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Der Rose gleich, die \u00fcber Tr\u00fcmmern bl\u00fcht.", "tokens": ["Der", "Ro\u00b7se", "gleich", ",", "die", "\u00fc\u00b7ber", "Tr\u00fcm\u00b7mern", "bl\u00fcht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "PRELS", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wem dampft das Opfer der betauten Flur?", "tokens": ["Wem", "dampft", "das", "Op\u00b7fer", "der", "be\u00b7tau\u00b7ten", "Flur", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ihr Duft, der hoch in Silbernebeln dringt,", "tokens": ["Ihr", "Duft", ",", "der", "hoch", "in", "Sil\u00b7ber\u00b7ne\u00b7beln", "dringt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "ADJD", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ist Weihrauch, den die l\u00e4ndliche Natur", "tokens": ["Ist", "Weih\u00b7rauch", ",", "den", "die", "l\u00e4nd\u00b7li\u00b7che", "Na\u00b7tur"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "NN", "$,", "PRELS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dem Herrn auf niedern Rasenstufen bringt.", "tokens": ["Dem", "Herrn", "auf", "nie\u00b7dern", "Ra\u00b7sen\u00b7stu\u00b7fen", "bringt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die Himmel sind ein Hochaltar des Herrn,", "tokens": ["Die", "Him\u00b7mel", "sind", "ein", "Hoc\u00b7hal\u00b7tar", "des", "Herrn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ein Opferfunken nur der Morgenstern.", "tokens": ["Ein", "Op\u00b7fer\u00b7fun\u00b7ken", "nur", "der", "Mor\u00b7gens\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}}, "stanza.3": {"line.1": {"text": "Im Morgenrot, das naher Gletscher Reih'n", "tokens": ["Im", "Mor\u00b7gen\u00b7rot", ",", "das", "na\u00b7her", "Glet\u00b7scher", "Reih'n"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und ferner Meere Grenzkreis glorreich hellt,", "tokens": ["Und", "fer\u00b7ner", "Mee\u00b7re", "Grenz\u00b7kreis", "glor\u00b7reich", "hellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJA", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Verd\u00e4mmert seines Thrones Wiederschein,", "tokens": ["Ver\u00b7d\u00e4m\u00b7mert", "sei\u00b7nes", "Thro\u00b7nes", "Wie\u00b7der\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der mild auf Menschen, hell auf Gr\u00e4ber f\u00e4llt.", "tokens": ["Der", "mild", "auf", "Men\u00b7schen", ",", "hell", "auf", "Gr\u00e4\u00b7ber", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "NN", "$,", "ADJD", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Er leuchtet Huld auf redliches Vertrau'n", "tokens": ["Er", "leuch\u00b7tet", "Huld", "auf", "red\u00b7li\u00b7ches", "Ver\u00b7trau'n"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und Licht der Ewigkeit durch Todesgrau'n.", "tokens": ["Und", "Licht", "der", "E\u00b7wig\u00b7keit", "durch", "To\u00b7des\u00b7grau'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["KON", "NN", "ART", "NN", "APPR", "NN", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Noch wandeln wir, wo kaum der Aufgang tagt,", "tokens": ["Noch", "wan\u00b7deln", "wir", ",", "wo", "kaum", "der", "Auf\u00b7gang", "tagt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Im ersten Fr\u00fchschein der Unsterblichkeit.", "tokens": ["Im", "ers\u00b7ten", "Fr\u00fch\u00b7schein", "der", "U\u00b7nsterb\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Der Tag, wo Unschuld nimmer irrt, noch klagt,", "tokens": ["Der", "Tag", ",", "wo", "Un\u00b7schuld", "nim\u00b7mer", "irrt", ",", "noch", "klagt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "NN", "ADV", "VVFIN", "$,", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Gl\u00e4nzt hinter Gr\u00e4bern auf und ist nicht weit.", "tokens": ["Gl\u00e4nzt", "hin\u00b7ter", "Gr\u00e4\u00b7bern", "auf", "und", "ist", "nicht", "weit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "PTKVZ", "KON", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Des Wahnes Dunst, des Todes Nacht zerfleu\u00dft,", "tokens": ["Des", "Wah\u00b7nes", "Dunst", ",", "des", "To\u00b7des", "Nacht", "zer\u00b7fleu\u00dft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "O Allmacht, dir, die mir Erl\u00f6ser hei\u00dft!", "tokens": ["O", "All\u00b7macht", ",", "dir", ",", "die", "mir", "Er\u00b7l\u00f6\u00b7ser", "hei\u00dft", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "$,", "PRELS", "PPER", "NN", "VVFIN", "$."], "meter": "-+---+-+-+", "measure": "dactylic.init"}}}}}