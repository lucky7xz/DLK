{"dta.poem.11516": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Der Knabe und das Immlein.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Im Weinberg auf der H\u00f6he", "tokens": ["Im", "Wein\u00b7berg", "auf", "der", "H\u00f6\u00b7he"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein H\u00e4uslein steht so windebang,", "tokens": ["Ein", "H\u00e4us\u00b7lein", "steht", "so", "win\u00b7de\u00b7bang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hat weder Th\u00fcr noch Fenster,", "tokens": ["Hat", "we\u00b7der", "Th\u00fcr", "noch", "Fens\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KON", "NN", "ADV", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Weile wird ihm lang.", "tokens": ["Die", "Wei\u00b7le", "wird", "ihm", "lang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und ist der Tag so schw\u00fcle,", "tokens": ["Und", "ist", "der", "Tag", "so", "schw\u00fc\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sind all' verstummt die V\u00f6gelein;", "tokens": ["Sind", "all'", "ver\u00b7stummt", "die", "V\u00f6\u00b7ge\u00b7lein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Summt an der Sonnenblume", "tokens": ["Summt", "an", "der", "Son\u00b7nen\u00b7blu\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Ein Immlein ganz allein.", "tokens": ["Ein", "I\u00b7mmlein", "ganz", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Mein Lieb hat einen Garten,", "tokens": ["Mein", "Lieb", "hat", "ei\u00b7nen", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da steht ein h\u00fcbsches Immenhaus:", "tokens": ["Da", "steht", "ein", "h\u00fcb\u00b7sches", "Im\u00b7men\u00b7haus", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kommst du daher geflogen?", "tokens": ["Kommst", "du", "da\u00b7her", "ge\u00b7flo\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PAV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Schickt sie dich nach mir aus?", "tokens": ["Schickt", "sie", "dich", "nach", "mir", "aus", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "\u201eo nein, du feiner Knabe,", "tokens": ["\u201e", "o", "nein", ",", "du", "fei\u00b7ner", "Kna\u00b7be", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "FM", "PTKANT", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Es hie\u00df mich Niemand Boten gehn;", "tokens": ["Es", "hie\u00df", "mich", "Nie\u00b7mand", "Bo\u00b7ten", "gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIS", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dies Kind wei\u00df nichts von Lieben,", "tokens": ["Dies", "Kind", "wei\u00df", "nichts", "von", "Lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VVFIN", "PIS", "APPR", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hat dich noch kaum gesehn.", "tokens": ["Hat", "dich", "noch", "kaum", "ge\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Was w\u00fc\u00dften auch die M\u00e4dchen,", "tokens": ["Was", "w\u00fc\u00df\u00b7ten", "auch", "die", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wenn sie kaum aus der Schule sind!", "tokens": ["Wenn", "sie", "kaum", "aus", "der", "Schu\u00b7le", "sind", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ART", "NN", "VAFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Dein herzallerliebstes Sch\u00e4tzchen", "tokens": ["Dein", "her\u00b7zal\u00b7ler\u00b7liebs\u00b7tes", "Sch\u00e4tz\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist noch ein Mutterkind.", "tokens": ["Ist", "noch", "ein", "Mut\u00b7ter\u00b7kind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ich bring' ihm Wachs und Honig;", "tokens": ["Ich", "bring'", "ihm", "Wachs", "und", "Ho\u00b7nig", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ade! \u2014 ich hab' ein ganzes Pfund;", "tokens": ["A\u00b7de", "!", "ich", "hab'", "ein", "gan\u00b7zes", "Pfund", ";"], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "$(", "PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie wird das Sch\u00e4tzchen lachen,", "tokens": ["Wie", "wird", "das", "Sch\u00e4tz\u00b7chen", "la\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ihm w\u00e4ssert schon der Mund.\u201c", "tokens": ["Ihm", "w\u00e4s\u00b7sert", "schon", "der", "Mund", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Ach, wolltest du ihr sagen:", "tokens": ["Ach", ",", "woll\u00b7test", "du", "ihr", "sa\u00b7gen", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich w\u00fc\u00dfte, was viel s\u00fc\u00dfer ist:", "tokens": ["Ich", "w\u00fc\u00df\u00b7te", ",", "was", "viel", "s\u00fc\u00b7\u00dfer", "ist", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nichts Lieblichers auf Erden", "tokens": ["Nichts", "Lieb\u00b7li\u00b7chers", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Als wenn man herzt und k\u00fc\u00dft!", "tokens": ["Als", "wenn", "man", "herzt", "und", "k\u00fc\u00dft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PIS", "VVPP", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}