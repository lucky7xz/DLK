{"textgrid.poem.55083": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Der K\u00f6nig in Thule", "genre": "verse", "period": "N.A.", "pub_year": 1774, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es war ein K\u00f6nig in Thule", "tokens": ["Es", "war", "ein", "K\u00f6\u00b7nig", "in", "Thu\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "NE"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Gar treu bis an das Grab,", "tokens": ["Gar", "treu", "bis", "an", "das", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dem sterbend seine Buhle", "tokens": ["Dem", "ster\u00b7bend", "sei\u00b7ne", "Buh\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJD", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Einen goldnen Becher gab.", "tokens": ["Ei\u00b7nen", "gold\u00b7nen", "Be\u00b7cher", "gab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Es ging ihm nichts dar\u00fcber,", "tokens": ["Es", "ging", "ihm", "nichts", "da\u00b7r\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIS", "PAV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Er leert' ihn jeden Schmaus;", "tokens": ["Er", "leert'", "ihn", "je\u00b7den", "Schmaus", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Augen gingen ihm \u00fcber,", "tokens": ["Die", "Au\u00b7gen", "gin\u00b7gen", "ihm", "\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Sooft er trank daraus.", "tokens": ["Sooft", "er", "trank", "da\u00b7raus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PAV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Und als er kam zu sterben,", "tokens": ["Und", "als", "er", "kam", "zu", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Z\u00e4hlt' er seine St\u00e4dt im Reich,", "tokens": ["Z\u00e4hlt'", "er", "sei\u00b7ne", "St\u00e4dt", "im", "Reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "G\u00f6nnt' alles seinem Erben,", "tokens": ["G\u00f6nnt'", "al\u00b7les", "sei\u00b7nem", "Er\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den Becher nicht zugleich.", "tokens": ["Den", "Be\u00b7cher", "nicht", "zu\u00b7gleich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Er sa\u00df beim K\u00f6nigsmahle,", "tokens": ["Er", "sa\u00df", "beim", "K\u00f6\u00b7nigs\u00b7mah\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Ritter um ihn her,", "tokens": ["Die", "Rit\u00b7ter", "um", "ihn", "her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auf hohem V\u00e4tersaale,", "tokens": ["Auf", "ho\u00b7hem", "V\u00e4\u00b7ter\u00b7saa\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dort auf dem Schlo\u00df am Meer.", "tokens": ["Dort", "auf", "dem", "Schlo\u00df", "am", "Meer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Dort stand der alte Zecher,", "tokens": ["Dort", "stand", "der", "al\u00b7te", "Ze\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Trank letzte Lebensglut,", "tokens": ["Trank", "letz\u00b7te", "Le\u00b7bens\u00b7glut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und warf den heil'gen Becher", "tokens": ["Und", "warf", "den", "heil'\u00b7gen", "Be\u00b7cher"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hinunter in die Flut.", "tokens": ["Hin\u00b7un\u00b7ter", "in", "die", "Flut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Er sah ihn st\u00fcrzen, trinken", "tokens": ["Er", "sah", "ihn", "st\u00fcr\u00b7zen", ",", "trin\u00b7ken"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "PPER", "VVINF", "$,", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und sinken tief ins Meer.", "tokens": ["Und", "sin\u00b7ken", "tief", "ins", "Meer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Augen t\u00e4ten ihm sinken;", "tokens": ["Die", "Au\u00b7gen", "t\u00e4\u00b7ten", "ihm", "sin\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Trank nie einen Tropfen mehr.", "tokens": ["Trank", "nie", "ei\u00b7nen", "Trop\u00b7fen", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}