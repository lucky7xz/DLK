{"textgrid.poem.64734": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "1L: Da\u00df er neun Jahr Professor hie\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df er neun Jahr Professor hie\u00df", "tokens": ["Da\u00df", "er", "neun", "Jahr", "Pro\u00b7fes\u00b7sor", "hie\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "CARD", "NN", "NN", "VVFIN"], "meter": "--++-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Und nie sich als Gelehrter wies,", "tokens": ["Und", "nie", "sich", "als", "Ge\u00b7lehr\u00b7ter", "wies", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PRF", "KOUS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ist seiner Ohnmacht zu verzeihn;", "tokens": ["Ist", "sei\u00b7ner", "Ohn\u00b7macht", "zu", "ver\u00b7zeihn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Doch, was auch Menschenliebe spricht,", "tokens": ["Doch", ",", "was", "auch", "Men\u00b7schen\u00b7lie\u00b7be", "spricht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PRELS", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So m\u00fc\u00dfte doch die Ohnmacht nicht", "tokens": ["So", "m\u00fc\u00df\u00b7te", "doch", "die", "Ohn\u00b7macht", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ADV", "ART", "NN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Acht lange Jahr' besoldet seyn.", "tokens": ["Acht", "lan\u00b7ge", "Jahr'", "be\u00b7sol\u00b7det", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}