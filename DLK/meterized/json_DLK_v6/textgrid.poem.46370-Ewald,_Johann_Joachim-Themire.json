{"textgrid.poem.46370": {"metadata": {"author": {"name": "Ewald, Johann Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Themire", "genre": "verse", "period": "N.A.", "pub_year": 1744, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du siehst auf diesem Elfenbein Themirens himmlisches Gesicht,", "tokens": ["Du", "siehst", "auf", "die\u00b7sem", "El\u00b7fen\u00b7bein", "The\u00b7mi\u00b7rens", "himm\u00b7li\u00b7sches", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PDAT", "NN", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Die blendend wei\u00dfe Brust die kleine runde Hand", "tokens": ["Die", "blen\u00b7dend", "wei\u00b7\u00dfe", "Brust", "die", "klei\u00b7ne", "run\u00b7de", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADJA", "NN", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und bist in sie schon itzt entbrannt?", "tokens": ["Und", "bist", "in", "sie", "schon", "itzt", "ent\u00b7brannt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du siehst noch ihren Geist und ihre Tugend nicht!", "tokens": ["Du", "siehst", "noch", "ih\u00b7ren", "Geist", "und", "ih\u00b7re", "Tu\u00b7gend", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}