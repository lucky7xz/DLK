{"textgrid.poem.39279": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "[das tiefe Weh, das mir die Brust gepeinigt]", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das tiefe Weh, das mir die Brust gepeinigt,", "tokens": ["Das", "tie\u00b7fe", "Weh", ",", "das", "mir", "die", "Brust", "ge\u00b7pei\u00b7nigt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das str\u00f6mt' ich aus in hei\u00dfen S\u00fcdlandsliedern,", "tokens": ["Das", "str\u00f6mt'", "ich", "aus", "in", "hei\u00b7\u00dfen", "S\u00fcd\u00b7lands\u00b7lie\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Denn ewig mag das feige Tun mich widern,", "tokens": ["Denn", "e\u00b7wig", "mag", "das", "fei\u00b7ge", "Tun", "mich", "wi\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VMFIN", "ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das nicht die Worte mit dem Herzen einigt.", "tokens": ["Das", "nicht", "die", "Wor\u00b7te", "mit", "dem", "Her\u00b7zen", "ei\u00b7nigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKNEG", "ART", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ihr! die ich keck mit diesem Wort gesteinigt,", "tokens": ["Ihr", "!", "die", "ich", "keck", "mit", "die\u00b7sem", "Wort", "ge\u00b7stei\u00b7nigt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$.", "PRELS", "PPER", "ADJD", "APPR", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihr k\u00f6nnt das Herz mir nimmermehr erniedern,", "tokens": ["Ihr", "k\u00f6nnt", "das", "Herz", "mir", "nim\u00b7mer\u00b7mehr", "er\u00b7nie\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und Fehde ruf' ich eures Haufens Gliedern,", "tokens": ["Und", "Feh\u00b7de", "ruf'", "ich", "eu\u00b7res", "Hau\u00b7fens", "Glie\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bis deutsche Kraft den deutschen Sinn gereinigt.", "tokens": ["Bis", "deut\u00b7sche", "Kraft", "den", "deut\u00b7schen", "Sinn", "ge\u00b7rei\u00b7nigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "\u00bbwas spricht der Knabe von der V\u00f6lkerss Walten?", "tokens": ["\u00bb", "was", "spricht", "der", "Kna\u00b7be", "von", "der", "V\u00f6l\u00b7kerss", "Wal\u00b7ten", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was will er Land und Staat und Lieder meistern?\u00ab", "tokens": ["Was", "will", "er", "Land", "und", "Staat", "und", "Lie\u00b7der", "meis\u00b7tern", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VMFIN", "PPER", "NN", "KON", "NN", "KON", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "M\u00f6gt ihr ausrufen in des Zornes Toben. \u2013", "tokens": ["M\u00f6gt", "ihr", "aus\u00b7ru\u00b7fen", "in", "des", "Zor\u00b7nes", "To\u00b7ben", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ihr echten S\u00e4nger, mit den Flammengeistern,", "tokens": ["Ihr", "ech\u00b7ten", "S\u00e4n\u00b7ger", ",", "mit", "den", "Flam\u00b7men\u00b7geis\u00b7tern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihr m\u00f6gt das junge Herz mir aufrecht halten,", "tokens": ["Ihr", "m\u00f6gt", "das", "jun\u00b7ge", "Herz", "mir", "auf\u00b7recht", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Denn Sang und Wahrheit kommt vom Vater droben.", "tokens": ["Denn", "Sang", "und", "Wahr\u00b7heit", "kommt", "vom", "Va\u00b7ter", "dro\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVFIN", "APPRART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}