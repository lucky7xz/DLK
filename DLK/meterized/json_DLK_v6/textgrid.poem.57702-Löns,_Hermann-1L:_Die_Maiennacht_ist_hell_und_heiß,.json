{"textgrid.poem.57702": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Maiennacht ist hell und hei\u00df,", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Maiennacht ist hell und hei\u00df,", "tokens": ["Die", "Mai\u00b7en\u00b7nacht", "ist", "hell", "und", "hei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Flammen steht der heilige Kreis,", "tokens": ["In", "Flam\u00b7men", "steht", "der", "hei\u00b7li\u00b7ge", "Kreis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Ein Dreieck hin, ein Dreieck her,", "tokens": ["Ein", "Drei\u00b7eck", "hin", ",", "ein", "Drei\u00b7eck", "her", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die liegen \u00fcber Kreuz und Quer.", "tokens": ["Die", "lie\u00b7gen", "\u00fc\u00b7ber", "Kreuz", "und", "Quer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Es bebt das Laub am Lindenbaum,", "tokens": ["Es", "bebt", "das", "Laub", "am", "Lin\u00b7den\u00b7baum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es tr\u00e4umt der V\u00e4ter hohen Traum;", "tokens": ["Es", "tr\u00e4umt", "der", "V\u00e4\u00b7ter", "ho\u00b7hen", "Traum", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das ist die Nacht, die heilige Nacht,", "tokens": ["Das", "ist", "die", "Nacht", ",", "die", "hei\u00b7li\u00b7ge", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "In der das neue Reich erwacht.", "tokens": ["In", "der", "das", "neu\u00b7e", "Reich", "er\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Es neigt sich jedes Lindenreis,", "tokens": ["Es", "neigt", "sich", "je\u00b7des", "Lin\u00b7den\u00b7reis", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der starke Gott tritt in den Kreis;", "tokens": ["Der", "star\u00b7ke", "Gott", "tritt", "in", "den", "Kreis", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Sterne geben hellern Schein,", "tokens": ["Die", "Ster\u00b7ne", "ge\u00b7ben", "hel\u00b7lern", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die gute Fraue tritt herein.", "tokens": ["Die", "gu\u00b7te", "Frau\u00b7e", "tritt", "her\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Da hebt der Gott die Schwerthand auf", "tokens": ["Da", "hebt", "der", "Gott", "die", "Schwert\u00b7hand", "auf"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und hemmt der Wolkenk\u00fche Lauf;", "tokens": ["Und", "hemmt", "der", "Wol\u00b7ken\u00b7k\u00fc\u00b7he", "Lauf", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie eilen in den heiligen Kreis,", "tokens": ["Sie", "ei\u00b7len", "in", "den", "hei\u00b7li\u00b7gen", "Kreis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Wo jedes seine Stelle wei\u00df.", "tokens": ["Wo", "je\u00b7des", "sei\u00b7ne", "Stel\u00b7le", "wei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Die Euter h\u00e4ngen tief und schwer,", "tokens": ["Die", "Eu\u00b7ter", "h\u00e4n\u00b7gen", "tief", "und", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und Feld und Wiesen d\u00fcrsten sehr;", "tokens": ["Und", "Feld", "und", "Wie\u00b7sen", "d\u00fcrs\u00b7ten", "sehr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VMFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die G\u00f6ttin regt die wei\u00dfe Hand,", "tokens": ["Die", "G\u00f6t\u00b7tin", "regt", "die", "wei\u00b7\u00dfe", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Regen rieselt auf das Land.", "tokens": ["Der", "Re\u00b7gen", "rie\u00b7selt", "auf", "das", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Es sprie\u00dft das Gras, es schie\u00dft das Korn,", "tokens": ["Es", "sprie\u00dft", "das", "Gras", ",", "es", "schie\u00dft", "das", "Korn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es singt sein Lied der Hungerborn;", "tokens": ["Es", "singt", "sein", "Lied", "der", "Hun\u00b7ger\u00b7born", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O Maiennacht, o Weihenacht,", "tokens": ["O", "Mai\u00b7en\u00b7nacht", ",", "o", "Wei\u00b7he\u00b7nacht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es steht das Land in Hochzeitspracht!", "tokens": ["Es", "steht", "das", "Land", "in", "Hoch\u00b7zeit\u00b7spracht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}