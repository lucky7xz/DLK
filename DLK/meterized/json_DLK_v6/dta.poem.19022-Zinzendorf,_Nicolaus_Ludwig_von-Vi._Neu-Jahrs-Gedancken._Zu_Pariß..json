{"dta.poem.19022": {"metadata": {"author": {"name": "Zinzendorf, Nicolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "Vi.   Neu-Jahrs-Gedancken. Zu Pari\u00df.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20688-6", "language": ["de:0.99"], "booktitle": "Zinzendorf, Nicolaus Ludwig von: Teutscher Gedichte Erster Theil. Herrnhuth, 1735."}, "poem": {"stanza.1": {"line.1": {"text": "Owachsamer Geist,", "tokens": ["O\u00b7wach\u00b7sa\u00b7mer", "Geist", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Der Wunder beweist,", "tokens": ["Der", "Wun\u00b7der", "be\u00b7weist", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Erscheine der Seele,", "tokens": ["Er\u00b7schei\u00b7ne", "der", "See\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Dein g\u00f6ttliches Oele", "tokens": ["Dein", "g\u00f6tt\u00b7li\u00b7ches", "O\u00b7e\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Durchstr\u00f6me den Sinn!", "tokens": ["Durch\u00b7str\u00f6\u00b7me", "den", "Sinn", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Es m\u00fc\u00df ihm gelingen", "tokens": ["Es", "m\u00fc\u00df", "ihm", "ge\u00b7lin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Dein Reich zu erringen,", "tokens": ["Dein", "Reich", "zu", "er\u00b7rin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Er sehnt sich dahin;", "tokens": ["Er", "sehnt", "sich", "da\u00b7hin", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PAV", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Der dornigte Steg", "tokens": ["Der", "dor\u00b7nig\u00b7te", "Steg"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.10": {"text": "Kan Helden erschrecken,", "tokens": ["Kan", "Hel\u00b7den", "er\u00b7schre\u00b7cken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Und Tapffere strecken;", "tokens": ["Und", "Tapf\u00b7fe\u00b7re", "stre\u00b7cken", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.12": {"text": "Der sandigte Weg", "tokens": ["Der", "san\u00b7dig\u00b7te", "Weg"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.13": {"text": "Macht m\u00fcde und matt;", "tokens": ["Macht", "m\u00fc\u00b7de", "und", "matt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.14": {"text": "Wer aber dich hat,", "tokens": ["Wer", "a\u00b7ber", "dich", "hat", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PPER", "VAFIN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.15": {"text": "Den machen die Beulen", "tokens": ["Den", "ma\u00b7chen", "die", "Beu\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.16": {"text": "Nur hurtiger eilen,", "tokens": ["Nur", "hur\u00b7ti\u00b7ger", "ei\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.17": {"text": "Zur bleibenden Stadt.", "tokens": ["Zur", "blei\u00b7ben\u00b7den", "Stadt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.18": {"text": "Drum Streiter Hertz auf!", "tokens": ["Drum", "Strei\u00b7ter", "Hertz", "auf", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.19": {"text": "Auf! ohne Verweilen,", "tokens": ["Auf", "!", "oh\u00b7ne", "Ver\u00b7wei\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["APPR", "$.", "APPR", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.20": {"text": "Vollf\u00fchre den Lauff!", "tokens": ["Voll\u00b7f\u00fch\u00b7re", "den", "Lauff", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}