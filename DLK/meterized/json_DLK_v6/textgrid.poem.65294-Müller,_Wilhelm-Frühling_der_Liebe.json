{"textgrid.poem.65294": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fchling der Liebe", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Drau\u00dfen tobt der b\u00f6se Winter,", "tokens": ["Drau\u00b7\u00dfen", "tobt", "der", "b\u00f6\u00b7se", "Win\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die Blumen, die er knickte,", "tokens": ["Und", "die", "Blu\u00b7men", ",", "die", "er", "knick\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Malt er h\u00f6hnisch an die Fenster", "tokens": ["Malt", "er", "h\u00f6h\u00b7nisch", "an", "die", "Fens\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ADJD", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mir in bleichen, starren Bildern.", "tokens": ["Mir", "in", "blei\u00b7chen", ",", "star\u00b7ren", "Bil\u00b7dern", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "APPR", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Winter, st\u00fcrme nur und brause!", "tokens": ["Win\u00b7ter", ",", "st\u00fcr\u00b7me", "nur", "und", "brau\u00b7se", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ADV", "KON", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Machst mich doch nicht mehr erzittern.", "tokens": ["Machst", "mich", "doch", "nicht", "mehr", "er\u00b7zit\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKNEG", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Denn aus meines Herzens Grunde", "tokens": ["Denn", "aus", "mei\u00b7nes", "Her\u00b7zens", "Grun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Lass' ich einen Fr\u00fchling sprie\u00dfen,", "tokens": ["Lass'", "ich", "ei\u00b7nen", "Fr\u00fch\u00b7ling", "sprie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Den der Schnee nicht kann bedecken,", "tokens": ["Den", "der", "Schnee", "nicht", "kann", "be\u00b7de\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PTKNEG", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Den das Eis nicht macht gefrieren,", "tokens": ["Den", "das", "Eis", "nicht", "macht", "ge\u00b7frie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PTKNEG", "VVFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Einen Fr\u00fchling, dessen Sonne", "tokens": ["Ei\u00b7nen", "Fr\u00fch\u00b7ling", ",", "des\u00b7sen", "Son\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "PRELAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Ist das Auge meiner Liebsten,", "tokens": ["Ist", "das", "Au\u00b7ge", "mei\u00b7ner", "Liebs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Dessen Luft und Duft ihr Odem,", "tokens": ["Des\u00b7sen", "Luft", "und", "Duft", "ihr", "O\u00b7dem", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "KON", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Dessen Rosen ihre Lippen,", "tokens": ["Des\u00b7sen", "Ro\u00b7sen", "ih\u00b7re", "Lip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Und ich schweb' als junge Lerche", "tokens": ["Und", "ich", "schweb'", "als", "jun\u00b7ge", "Ler\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "KOKOM", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Dr\u00fcber hin mit meinen Liedern.", "tokens": ["Dr\u00fc\u00b7ber", "hin", "mit", "mei\u00b7nen", "Lie\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}