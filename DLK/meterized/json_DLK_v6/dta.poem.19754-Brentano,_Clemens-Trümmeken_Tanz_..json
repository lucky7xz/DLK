{"dta.poem.19754": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Tr\u00fcmmeken Tanz .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519168", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Herr Hinrich und siene Br\u00f6der alle dree, voll grone,               ", "tokens": ["Herr", "Hin\u00b7rich", "und", "sie\u00b7ne", "Br\u00f6\u00b7der", "al\u00b7le", "dree", ",", "voll", "gro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NE", "KON", "ADJA", "NN", "PIAT", "CARD", "$,", "ADJD", "ADJA", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "Se buuden een Schepken tor See, um de adlige Rosen-", "tokens": ["Se", "buu\u00b7den", "e\u00b7en", "Schep\u00b7ken", "tor", "See", ",", "um", "de", "ad\u00b7li\u00b7ge", "Ro\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NE", "ART", "NN", "VVFIN", "NN", "$,", "KOUI", "NE", "ADJA", "TRUNC"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.3": {"text": "blome,", "tokens": ["blo\u00b7me", ","], "token_info": ["word", "punct"], "pos": ["ADJA", "$,"], "meter": "+-", "measure": "trochaic.single"}}, "stanza.2": {"line.1": {"text": "Do dat Schepken rede was, voll grone,", "tokens": ["Do", "dat", "Schep\u00b7ken", "re\u00b7de", "was", ",", "voll", "gro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PIS", "$,", "ADJD", "ADJA", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Se setten sick darin, se f\u00f6hrde alle daher, um de adliche", "tokens": ["Se", "set\u00b7ten", "sick", "da\u00b7rin", ",", "se", "f\u00f6hr\u00b7de", "al\u00b7le", "da\u00b7her", ",", "um", "de", "ad\u00b7li\u00b7che"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "PRF", "PAV", "$,", "FM", "FM", "PIS", "PAV", "$,", "KOUI", "NE", "NE"], "meter": "-+-+-+-+-+-+++-+--", "measure": "unknown.measure.octa.plus"}, "line.3": {"text": "Rosenblome", "tokens": ["Ro\u00b7sen\u00b7blo\u00b7me"], "token_info": ["word"], "pos": ["NE"], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Do se Westwerts averkemen, voll grone,", "tokens": ["Do", "se", "West\u00b7werts", "av\u00b7er\u00b7ke\u00b7men", ",", "voll", "gro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVINF", "$,", "ADJD", "ADJA", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Do stond dar een Goldschmits S\u00f6hne vor de D\u00f6hr, mit", "tokens": ["Do", "stond", "dar", "e\u00b7en", "Gold\u00b7schmits", "S\u00f6h\u00b7ne", "vor", "de", "D\u00f6hr", ",", "mit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "ADJD", "PTKVZ", "ART", "NN", "NN", "APPR", "NE", "NE", "$,", "APPR"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "de adlige Rosenblome,", "tokens": ["de", "ad\u00b7li\u00b7ge", "Ro\u00b7sen\u00b7blo\u00b7me", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}