{"textgrid.poem.43480": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Frisch auf! frisch auf mit Sang und Klang,", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Frisch auf! frisch auf mit Sang und Klang,", "tokens": ["Frisch", "auf", "!", "frisch", "auf", "mit", "Sang", "und", "Klang", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$.", "ADJD", "APPR", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df Herz und Sinn erwache!", "tokens": ["Da\u00df", "Herz", "und", "Sinn", "er\u00b7wa\u00b7che", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein freudig Hoch! ein dreifach Hoch!", "tokens": ["Ein", "freu\u00b7dig", "Hoch", "!", "ein", "drei\u00b7fach", "Hoch", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJD", "$.", "ART", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es gilt der guten Sache.", "tokens": ["Es", "gilt", "der", "gu\u00b7ten", "Sa\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wir sind dieselben immer noch,", "tokens": ["Wir", "sind", "die\u00b7sel\u00b7ben", "im\u00b7mer", "noch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PDAT", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wir wollen es auch bleiben,", "tokens": ["Wir", "wol\u00b7len", "es", "auch", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mag auch Verrath, mag Lug und Trug", "tokens": ["Mag", "auch", "Ver\u00b7rath", ",", "mag", "Lug", "und", "Trug"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "NN", "$,", "VMFIN", "NE", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Stets seine K\u00fcnste treiben.", "tokens": ["Stets", "sei\u00b7ne", "K\u00fcns\u00b7te", "trei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Gott steht uns bei, nie k\u00f6nnen wir", "tokens": ["Gott", "steht", "uns", "bei", ",", "nie", "k\u00f6n\u00b7nen", "wir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "PTKVZ", "$,", "ADV", "VMFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In diesem Kampf erschlaffen:", "tokens": ["In", "die\u00b7sem", "Kampf", "er\u00b7schlaf\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das Recht ist unser Feldgeschrei,", "tokens": ["Das", "Recht", "ist", "un\u00b7ser", "Feld\u00b7ge\u00b7schrei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Recht weiht unsre Waffen.", "tokens": ["Das", "Recht", "weiht", "uns\u00b7re", "Waf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-++--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Wir stehen fest in Freud' und Leid", "tokens": ["Wir", "ste\u00b7hen", "fest", "in", "Freud'", "und", "Leid"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sind br\u00fcderlich vereinet,", "tokens": ["Sind", "br\u00fc\u00b7der\u00b7lich", "ver\u00b7ei\u00b7net", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So lang' ein Gott im Himmel lebt", "tokens": ["So", "lang'", "ein", "Gott", "im", "Him\u00b7mel", "lebt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und seine Sonne scheinet.", "tokens": ["Und", "sei\u00b7ne", "Son\u00b7ne", "schei\u00b7net", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Frisch auf! frisch auf mit Sang und Klang,", "tokens": ["Frisch", "auf", "!", "frisch", "auf", "mit", "Sang", "und", "Klang", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$.", "ADJD", "APPR", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df Herz und Sinn erwache!", "tokens": ["Da\u00df", "Herz", "und", "Sinn", "er\u00b7wa\u00b7che", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein freudig Hoch! ein dreifach Hoch!", "tokens": ["Ein", "freu\u00b7dig", "Hoch", "!", "ein", "drei\u00b7fach", "Hoch", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJD", "$.", "ART", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es gilt der guten Sache.", "tokens": ["Es", "gilt", "der", "gu\u00b7ten", "Sa\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}