{"textgrid.poem.45289": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[ein Trauerspiel, gra\u00df wie aus Hebbels Mund]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Trauerspiel, gra\u00df wie aus Hebbels Mund,", "tokens": ["Ein", "Trau\u00b7er\u00b7spiel", ",", "gra\u00df", "wie", "aus", "Heb\u00b7bels", "Mund", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "KOKOM", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Begibt sich in den Wellendrang des Todes,", "tokens": ["Be\u00b7gibt", "sich", "in", "den", "Wel\u00b7len\u00b7drang", "des", "To\u00b7des", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die arme Marianne geht zu Grund,", "tokens": ["Die", "ar\u00b7me", "Ma\u00b7ri\u00b7an\u00b7ne", "geht", "zu", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und \u00fcbrig bleibt \u2013 wer wei\u00df? \u2013 vielleicht Herodes.", "tokens": ["Und", "\u00fcb\u00b7rig", "bleibt", "\u2013", "wer", "wei\u00df", "?", "\u2013", "viel\u00b7leicht", "He\u00b7ro\u00b7des", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "$(", "PWS", "VVFIN", "$.", "$(", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}