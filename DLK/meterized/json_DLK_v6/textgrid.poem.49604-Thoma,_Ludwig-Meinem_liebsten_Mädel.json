{"textgrid.poem.49604": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Meinem liebsten M\u00e4del", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Herz mu\u00dft in die Irre geh'n,", "tokens": ["Mein", "Herz", "mu\u00dft", "in", "die", "Ir\u00b7re", "geh'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es mu\u00dft ihm alles Leid gescheh'n,", "tokens": ["Es", "mu\u00dft", "ihm", "al\u00b7les", "Leid", "ge\u00b7scheh'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nun nimm's in beide H\u00e4nde!", "tokens": ["Nun", "nim\u00b7m's", "in", "bei\u00b7de", "H\u00e4n\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und halt es fest und schlie\u00df es ein!", "tokens": ["Und", "halt", "es", "fest", "und", "schlie\u00df", "es", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dann solls noch einmal gl\u00fccklich sein", "tokens": ["Dann", "solls", "noch", "ein\u00b7mal", "gl\u00fcck\u00b7lich", "sein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ADV", "ADV", "ADJD", "VAINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und fr\u00f6hlich ohne Ende.", "tokens": ["Und", "fr\u00f6h\u00b7lich", "oh\u00b7ne", "En\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Das Gl\u00fcck, das klopfte bei mir an,", "tokens": ["Das", "Gl\u00fcck", ",", "das", "klopf\u00b7te", "bei", "mir", "an", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PDS", "VVFIN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Stand vor der T\u00fcr und wollt herein;", "tokens": ["Stand", "vor", "der", "T\u00fcr", "und", "wollt", "her\u00b7ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "KON", "VMFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich hab ihm doch nicht aufgetan,", "tokens": ["Ich", "hab", "ihm", "doch", "nicht", "auf\u00b7ge\u00b7tan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da mocht's nicht l\u00e4nger drau\u00dfen sein.", "tokens": ["Da", "mocht's", "nicht", "l\u00e4n\u00b7ger", "drau\u00b7\u00dfen", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PTKNEG", "ADJD", "ADV", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Es ging so leise, wie es kam.", "tokens": ["Es", "ging", "so", "lei\u00b7se", ",", "wie", "es", "kam", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich h\u00f6rt es nicht, ich sah es nicht,", "tokens": ["Ich", "h\u00f6rt", "es", "nicht", ",", "ich", "sah", "es", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch f\u00fchlt ich, wie es Abschied nahm.", "tokens": ["Doch", "f\u00fchlt", "ich", ",", "wie", "es", "Ab\u00b7schied", "nahm", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "PWAV", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In meiner Brust erlosch ein Licht.", "tokens": ["In", "mei\u00b7ner", "Brust", "er\u00b7losch", "ein", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}