{"dta.poem.3876": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "1) An seinen ehmahligen Lehrer.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Lehrer, dessen Huld und Treue ich zu lobe\u0303,", "tokens": ["Mein", "Leh\u00b7rer", ",", "des\u00b7sen", "Huld", "und", "Treu\u00b7e", "ich", "zu", "lob\u1ebd", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELAT", "NN", "KON", "NN", "PPER", "APPR", "NE", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hier bringt die Danckbarkeit gantz schlecht,", "tokens": ["Hier", "bringt", "die", "Dan\u00b7ck\u00b7bar\u00b7keit", "gantz", "schlecht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "doch \u00e4chte Proben,", "tokens": ["doch", "\u00e4ch\u00b7te", "Pro\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Denn sie erkennet wohl, wie viel du mir gethan,", "tokens": ["Denn", "sie", "er\u00b7ken\u00b7net", "wohl", ",", "wie", "viel", "du", "mir", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "$,", "PWAV", "PIS", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Drum nimm di\u00df schlechte Blatt, statt ihres Opf-", "tokens": ["Drum", "nimm", "di\u00df", "schlech\u00b7te", "Blatt", ",", "statt", "ih\u00b7res", "Opf"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PDS", "VVFIN", "NN", "$,", "KOUI", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "fers an.", "tokens": ["fers", "an", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$."], "meter": "--", "measure": "unknown.measure.zero"}, "line.7": {"text": "Durch dich fand ich mein Gl\u00fcck in ", "tokens": ["Durch", "dich", "fand", "ich", "mein", "Gl\u00fcck", "in"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "PPOSAT", "NN", "APPR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Fluhren,", "tokens": ["Fluh\u00b7ren", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "Du mein Gamaliel, du zeigtest mir die Spuhren", "tokens": ["Du", "mein", "Ga\u00b7ma\u00b7liel", ",", "du", "zeig\u00b7test", "mir", "die", "Spuh\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.10": {"text": "Von meinem Wohl und Gl\u00fcck, du nahmst dich", "tokens": ["Von", "mei\u00b7nem", "Wohl", "und", "Gl\u00fcck", ",", "du", "nahmst", "dich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "$,", "PPER", "VVFIN", "PPER"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "meiner an,", "tokens": ["mei\u00b7ner", "an", ","], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "PTKVZ", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.12": {"text": "Da\u00df Ehrfurcht, Danck und Treu nicht gnug ver-", "tokens": ["Da\u00df", "Ehr\u00b7furcht", ",", "Danck", "und", "Treu", "nicht", "gnug", "ver"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "$,", "NN", "KON", "NN", "PTKNEG", "ADV", "TRUNC"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "dancken kan.", "tokens": ["dan\u00b7cken", "kan", "."], "token_info": ["word", "word", "punct"], "pos": ["VVINF", "VMFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.14": {"text": "Den Grund, so du in mir durch deinen Flei\u00df ge-", "tokens": ["Den", "Grund", ",", "so", "du", "in", "mir", "durch", "dei\u00b7nen", "Flei\u00df", "ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ADV", "PPER", "APPR", "PPER", "APPR", "PPOSAT", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "bauet,", "tokens": ["bau\u00b7et", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.16": {"text": "Den habe ich nunmehr Herr ", "tokens": ["Den", "ha\u00b7be", "ich", "nun\u00b7mehr", "Herr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "PPER", "ADV", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.17": {"text": "Verzeihe noch ein Wort, die Zeit setzt mir das Ziel,", "tokens": ["Ver\u00b7zei\u00b7he", "noch", "ein", "Wort", ",", "die", "Zeit", "setzt", "mir", "das", "Ziel", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,", "ART", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Ein eintzigs w\u00e4re noch, so Spindler wissen will:", "tokens": ["Ein", "eint\u00b7zigs", "w\u00e4\u00b7re", "noch", ",", "so", "Spind\u00b7ler", "wis\u00b7sen", "will", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VAFIN", "ADV", "$,", "ADV", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Wie? lodern nicht bereits in deinem werthen Hertze\u0303,", "tokens": ["Wie", "?", "lo\u00b7dern", "nicht", "be\u00b7reits", "in", "dei\u00b7nem", "wert\u00b7hen", "Hertz\u1ebd", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "VVFIN", "PTKNEG", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "(wie man geschrieben hat) die frohen Hochzeit-", "tokens": ["(", "wie", "man", "ge\u00b7schrie\u00b7ben", "hat", ")", "die", "fro\u00b7hen", "Hoch\u00b7zeit"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "PWAV", "PIS", "VVPP", "VAFIN", "$(", "ART", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Kertzen?", "tokens": ["Kert\u00b7zen", "?"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.22": {"text": "Wen\u0303 nun der frohe Schall in unsre Mauren dringt,", "tokens": ["We\u00f1", "nun", "der", "fro\u00b7he", "Schall", "in", "uns\u00b7re", "Mau\u00b7ren", "dringt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Denn bin ich eine Gans, so unter Schwanen singt.", "tokens": ["Denn", "bin", "ich", "ei\u00b7ne", "Gans", ",", "so", "un\u00b7ter", "Schwa\u00b7nen", "singt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ART", "NN", "$,", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Indessen lebe wohl, di\u00df sey der frohe Schlu\u00df,", "tokens": ["In\u00b7des\u00b7sen", "le\u00b7be", "wohl", ",", "di\u00df", "sey", "der", "fro\u00b7he", "Schlu\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "$,", "PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Ich aber bleibe stets dein treuer ", "tokens": ["Ich", "a\u00b7ber", "blei\u00b7be", "stets", "dein", "treu\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "ADV", "PPOSAT", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}