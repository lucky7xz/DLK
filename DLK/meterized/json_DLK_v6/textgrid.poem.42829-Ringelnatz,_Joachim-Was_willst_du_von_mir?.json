{"textgrid.poem.42829": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Was willst du von mir?", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "M\u00f6chtest du meine Frau werden,", "tokens": ["M\u00f6ch\u00b7test", "du", "mei\u00b7ne", "Frau", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "VAINF", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.2": {"text": "Da meine Haare schon grau werden,", "tokens": ["Da", "mei\u00b7ne", "Haa\u00b7re", "schon", "grau", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "ADJD", "VAINF", "$,"], "meter": "-+-+--++-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Schon gr\u00f6\u00dftenteils sind?", "tokens": ["Schon", "gr\u00f6\u00df\u00b7ten\u00b7teils", "sind", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "M\u00f6chtest du \u00fcber mich lachen?", "tokens": ["M\u00f6ch\u00b7test", "du", "\u00fc\u00b7ber", "mich", "la\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PPER", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.5": {"text": "Soll ich dir Freude machen?", "tokens": ["Soll", "ich", "dir", "Freu\u00b7de", "ma\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Oder ein Kind?", "tokens": ["O\u00b7der", "ein", "Kind", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Willst du die Peitsche sp\u00fcren?", "tokens": ["Willst", "du", "die", "Peit\u00b7sche", "sp\u00fc\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Soll ich dich ausfuhren?", "tokens": ["Soll", "ich", "dich", "aus\u00b7fuh\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Brauchst du Geld oder einen Rat?", "tokens": ["Brauchst", "du", "Geld", "o\u00b7der", "ei\u00b7nen", "Rat", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "KON", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Willst du nur mit mir spielen?", "tokens": ["Willst", "du", "nur", "mit", "mir", "spie\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "APPR", "PPER", "VVINF", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Oder gefielen oder mi\u00dffielen", "tokens": ["O\u00b7der", "ge\u00b7fie\u00b7len", "o\u00b7der", "mi\u00df\u00b7fie\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVPP", "KON", "VVINF"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Dir Taten, die ich tat?", "tokens": ["Dir", "Ta\u00b7ten", ",", "die", "ich", "tat", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Warum bist du so still?", "tokens": ["Wa\u00b7rum", "bist", "du", "so", "still", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Soll ich dich beklagen?", "tokens": ["Soll", "ich", "dich", "be\u00b7kla\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Sag doch einmal: \u00bbIch will ......\u00ab", "tokens": ["Sag", "doch", "ein\u00b7mal", ":", "\u00bb", "Ich", "will", "......", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct", "punct"], "pos": ["NN", "ADV", "ADV", "$.", "$(", "PPER", "VMFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Oder sonst ein deutliches Wort. \u2013", "tokens": ["O\u00b7der", "sonst", "ein", "deut\u00b7li\u00b7ches", "Wort", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "$.", "$("], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Soll ich dich verjagen?", "tokens": ["Soll", "ich", "dich", "ver\u00b7ja\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Ja. Geh zu!", "tokens": ["Ja", ".", "Geh", "zu", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "$.", "NE", "PTKVZ", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Nein! \u2013 Du!", "tokens": ["Nein", "!", "\u2013", "Du", "!"], "token_info": ["word", "punct", "punct", "word", "punct"], "pos": ["PTKANT", "$.", "$(", "PPER", "$."], "meter": "++", "measure": "spondeus"}, "line.8": {"text": "Bitte, bitte, geh nicht fort!", "tokens": ["Bit\u00b7te", ",", "bit\u00b7te", ",", "geh", "nicht", "fort", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PTKANT", "$,", "VVFIN", "PTKNEG", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}