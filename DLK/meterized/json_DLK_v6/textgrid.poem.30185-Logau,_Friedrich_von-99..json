{"textgrid.poem.30185": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "99.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Kohl-Sack und ein Wolle-Sack, da die beysammen stunden,", "tokens": ["Ein", "Kohl\u00b7Sack", "und", "ein", "Wol\u00b7le\u00b7Sack", ",", "da", "die", "bey\u00b7sam\u00b7men", "stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,", "KOUS", "ART", "ADJA", "ADJA", "$,"], "meter": "-++--+-+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Da schu\u00df Cupido, und der Pfeil ward in dem schwartzen funden;", "tokens": ["Da", "schu\u00df", "Cu\u00b7pi\u00b7do", ",", "und", "der", "Pfeil", "ward", "in", "dem", "schwart\u00b7zen", "fun\u00b7den", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "$,", "KON", "ART", "NN", "VAFIN", "APPR", "ART", "ADJA", "VVFIN", "$."], "meter": "-+--+--+-+-+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Die Lieb ist an die Farbe nicht, dieweil sie blind, gebunden.", "tokens": ["Die", "Lieb", "ist", "an", "die", "Far\u00b7be", "nicht", ",", "die\u00b7weil", "sie", "blind", ",", "ge\u00b7bun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "ART", "NN", "PTKNEG", "$,", "KOUS", "PPER", "ADJD", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}}}}