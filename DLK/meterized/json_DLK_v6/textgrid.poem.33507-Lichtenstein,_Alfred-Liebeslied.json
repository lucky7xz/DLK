{"textgrid.poem.33507": {"metadata": {"author": {"name": "Lichtenstein, Alfred", "birth": "N.A.", "death": "N.A."}, "title": "Liebeslied", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Helle L\u00e4nder sind deine Augen.", "tokens": ["Hel\u00b7le", "L\u00e4n\u00b7der", "sind", "dei\u00b7ne", "Au\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-++-+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "V\u00f6gelchen sind deine Blicke,", "tokens": ["V\u00f6\u00b7gel\u00b7chen", "sind", "dei\u00b7ne", "Bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.3": {"text": "Zierliche Winke aus T\u00fcchern beim Abschied.", "tokens": ["Zier\u00b7li\u00b7che", "Win\u00b7ke", "aus", "T\u00fc\u00b7chern", "beim", "Ab\u00b7schied", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NN", "APPRART", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}}, "stanza.2": {"line.1": {"text": "In deinem L\u00e4cheln ruh ich wie in spielenden Booten.", "tokens": ["In", "dei\u00b7nem", "L\u00e4\u00b7cheln", "ruh", "ich", "wie", "in", "spie\u00b7len\u00b7den", "Boo\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "KOKOM", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Deine kleinen Geschichten sind aus Seide.", "tokens": ["Dei\u00b7ne", "klei\u00b7nen", "Ge\u00b7schich\u00b7ten", "sind", "aus", "Sei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "APPR", "NE", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Ich mu\u00df dich immer ansehen.", "tokens": ["Ich", "mu\u00df", "dich", "im\u00b7mer", "an\u00b7se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}