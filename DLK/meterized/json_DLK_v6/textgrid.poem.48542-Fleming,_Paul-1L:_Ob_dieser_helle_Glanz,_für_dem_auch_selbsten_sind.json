{"textgrid.poem.48542": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ob dieser helle Glanz, f\u00fcr dem auch selbsten sind", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ob dieser helle Glanz, f\u00fcr dem auch selbsten sind", "tokens": ["Ob", "die\u00b7ser", "hel\u00b7le", "Glanz", ",", "f\u00fcr", "dem", "auch", "selbs\u00b7ten", "sind"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PDAT", "ADJA", "NN", "$,", "APPR", "PRELS", "ADV", "ADV", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "die lichten Blitze blind, mein Augenlicht verblendet,", "tokens": ["die", "lich\u00b7ten", "Blit\u00b7ze", "blind", ",", "mein", "Au\u00b7gen\u00b7licht", "ver\u00b7blen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "$,", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "so la\u00df, Lieb, darum doch dein Antlitz unverwendet!", "tokens": ["so", "la\u00df", ",", "Lieb", ",", "da\u00b7rum", "doch", "dein", "Ant\u00b7litz", "un\u00b7ver\u00b7wen\u00b7det", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "NN", "$,", "PAV", "ADV", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-----+-+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Mein Herze sieht dich doch, sind schon die Augen blind.", "tokens": ["Mein", "Her\u00b7ze", "sieht", "dich", "doch", ",", "sind", "schon", "die", "Au\u00b7gen", "blind", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN"