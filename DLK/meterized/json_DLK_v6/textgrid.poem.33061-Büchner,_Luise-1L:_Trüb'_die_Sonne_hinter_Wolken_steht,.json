{"textgrid.poem.33061": {"metadata": {"author": {"name": "B\u00fcchner, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Tr\u00fcb' die Sonne hinter Wolken steht,", "genre": "verse", "period": "N.A.", "pub_year": 1849, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tr\u00fcb' die Sonne hinter Wolken steht,", "tokens": ["Tr\u00fcb'", "die", "Son\u00b7ne", "hin\u00b7ter", "Wol\u00b7ken", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Feucht der Westhauch \u00fcber Stoppeln weht,", "tokens": ["Feucht", "der", "West\u00b7hauch", "\u00fc\u00b7ber", "Stop\u00b7peln", "weht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Herbstes\u00f6de ruht auf dem Gefild,", "tokens": ["Herbs\u00b7te\u00b7s\u00f6\u00b7de", "ruht", "auf", "dem", "Ge\u00b7fild", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Herbstes\u00f6de \u2013 meiner Seele Bild!", "tokens": ["Herbs\u00b7te\u00b7s\u00f6\u00b7de", "\u2013", "mei\u00b7ner", "See\u00b7le", "Bild", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "M\u00fc\u00dft ihr Fluren auch entlaubet steh'n,", "tokens": ["M\u00fc\u00dft", "ihr", "Flu\u00b7ren", "auch", "ent\u00b7lau\u00b7bet", "steh'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "ADV", "VVFIN", "VVINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Einen Fr\u00fchling habt ihr doch geseh'n,", "tokens": ["Ei\u00b7nen", "Fr\u00fch\u00b7ling", "habt", "ihr", "doch", "ge\u00b7seh'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Eines Sommerkusses Gluth gef\u00fchlt,", "tokens": ["Ei\u00b7nes", "Som\u00b7mer\u00b7kus\u00b7ses", "Gluth", "ge\u00b7f\u00fchlt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Eh' euch kalter Todeshauch durchw\u00fchlt.", "tokens": ["Eh'", "euch", "kal\u00b7ter", "To\u00b7des\u00b7hauch", "durch\u00b7w\u00fchlt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Ach! mir hat kein Fr\u00fchling noch gelacht,", "tokens": ["Ach", "!", "mir", "hat", "kein", "Fr\u00fch\u00b7ling", "noch", "ge\u00b7lacht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPER", "VAFIN", "PIAT", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Keine Bl\u00fcth' entspro\u00df des Herzens Nacht,", "tokens": ["Kei\u00b7ne", "Bl\u00fcth'", "ent\u00b7spro\u00df", "des", "Her\u00b7zens", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und es naht kein hei\u00dfer Sommertag,", "tokens": ["Und", "es", "naht", "kein", "hei\u00b7\u00dfer", "Som\u00b7mer\u00b7tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Der mit gl\u00fch'ndem Ku\u00df sie lichten mag.", "tokens": ["Der", "mit", "gl\u00fch'n\u00b7dem", "Ku\u00df", "sie", "lich\u00b7ten", "mag."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["ART", "APPR", "ADJA", "NN", "PPER", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Fr\u00fchling kehrt zur\u00fcck mit jungem Gr\u00fcn,", "tokens": ["Fr\u00fch\u00b7ling", "kehrt", "zu\u00b7r\u00fcck", "mit", "jun\u00b7gem", "Gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKVZ", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Blumen \u00fcber Gr\u00e4bern neu erbl\u00fch'n;", "tokens": ["Blu\u00b7men", "\u00fc\u00b7ber", "Gr\u00e4\u00b7bern", "neu", "er\u00b7bl\u00fch'n", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Fr\u00fchling! heil' des m\u00fcden Herzens Noth,", "tokens": ["Fr\u00fch\u00b7ling", "!", "heil'", "des", "m\u00fc\u00b7den", "Her\u00b7zens", "Noth", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VVFIN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Gl\u00fcckesbl\u00fcthen gib' ihm, oder Tod!", "tokens": ["Gl\u00fc\u00b7ckes\u00b7bl\u00fc\u00b7then", "gib'", "ihm", ",", "o\u00b7der", "Tod", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "KON", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}