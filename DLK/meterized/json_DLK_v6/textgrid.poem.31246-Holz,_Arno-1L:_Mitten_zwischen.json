{"textgrid.poem.31246": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mitten zwischen ", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mitten zwischen ", "tokens": ["Mit\u00b7ten", "zwi\u00b7schen"], "token_info": ["word", "word"], "pos": ["ADV", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "sizz ich dr\u00e4llernd auff dem ", "tokens": ["sizz", "ich", "dr\u00e4l\u00b7lernd", "auff", "dem"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADJD", "APPR", "ART"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "siht man mir zu F\u00fc\u00dfen knien.", "tokens": ["siht", "man", "mir", "zu", "F\u00fc\u00b7\u00dfen", "kni\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Selbst ", "tokens": ["Selbst"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "horcht auff meine bundten Th\u00f6ne;", "tokens": ["horcht", "auff", "mei\u00b7ne", "bund\u00b7ten", "Th\u00f6\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "s\u00fcsser noch al\u00df ", "tokens": ["s\u00fcs\u00b7ser", "noch", "al\u00df"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ADV", "KOUS"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "pfeiff ich ihr ein Hirten-Lid.", "tokens": ["pfeiff", "ich", "ihr", "ein", "Hir\u00b7ten\u00b7Lid", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "dantzen \u00fcmb mich \u00fcber Bluhmen;", "tokens": ["dant\u00b7zen", "\u00fcmb", "mich", "\u00fc\u00b7ber", "Bluh\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PRF", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "wa\u00df sie forn und hindten zihrt/", "tokens": ["wa\u00df", "sie", "forn", "und", "hind\u00b7ten", "zihrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKVZ", "KON", "ADV", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "ist au\u00df nichts wie Schnee formirt.", "tokens": ["ist", "au\u00df", "nichts", "wie", "Schnee", "for\u00b7mirt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PIS", "KOKOM", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "rund \u00fcmb seine dikke ", "tokens": ["rund", "\u00fcmb", "sei\u00b7ne", "dik\u00b7ke"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "der von dem/ wa\u00df sie erlebt/", "tokens": ["der", "von", "dem", "/", "wa\u00df", "sie", "er\u00b7lebt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "$(", "KOUS", "PPER", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "noch am Rokk das Stroh anklebt.", "tokens": ["noch", "am", "Rokk", "das", "Stroh", "an\u00b7klebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "acht ich nicht drey K\u00fcpffer-Dreyer;", "tokens": ["acht", "ich", "nicht", "drey", "K\u00fcpf\u00b7fer\u00b7Drey\u00b7er", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "PPER", "PTKNEG", "CARD", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "st\u00f6\u00dft fast bald die Milch-Bahn an!", "tokens": ["st\u00f6\u00dft", "fast", "bald", "die", "Milch\u00b7Bahn", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}