{"dta.poem.21800": {"metadata": {"author": {"name": "Stieler, Kaspar von", "birth": "N.A.", "death": "N.A."}, "title": "IiI.  \n  Liebe/ Sinnen-raub.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1660", "urn": "urn:nbn:de:kobv:b4-20586-2", "language": ["de:0.99"], "booktitle": "Filidor der Dorfferer [i. e. Stieler, Kaspar von]: Die Geharnschte Venus. Hamburg, 1660."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Lieb baht mich in einen Garten", "tokens": ["Mein", "Lieb", "baht", "mich", "in", "ei\u00b7nen", "Gar\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "wo der verliebte Westenwind", "tokens": ["wo", "der", "ver\u00b7lieb\u00b7te", "Wes\u00b7ten\u00b7wind"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "der Floren pfleget auffzuwarten", "tokens": ["der", "Flo\u00b7ren", "pfle\u00b7get", "auff\u00b7zu\u00b7war\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "VVIZU"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "die Lufft war fahl/ Apollens Kind/", "tokens": ["die", "Lufft", "war", "fahl", "/", "A\u00b7pol\u00b7lens", "Kind", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$(", "NE", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "der Tag begunnte gleich zu sterben", "tokens": ["der", "Tag", "be\u00b7gunn\u00b7te", "gleich", "zu", "ster\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "und seine Sch\u00f6nheit zu verf\u00e4rben.", "tokens": ["und", "sei\u00b7ne", "Sch\u00f6n\u00b7heit", "zu", "ver\u00b7f\u00e4r\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Kaum war ich dar hinein gegangen/", "tokens": ["Kaum", "war", "ich", "dar", "hin\u00b7ein", "ge\u00b7gan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKVZ", "APZR", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Diktinna bl\u00e4\u00dfte Licht und Wangen", "tokens": ["Dik\u00b7tin\u00b7na", "bl\u00e4\u00df\u00b7te", "Licht", "und", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "KON", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "und Hesperus wich in das Meer.", "tokens": ["und", "Hes\u00b7pe\u00b7rus", "wich", "in", "das", "Meer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Der schwarze Schatten wurd\u2019 erhellet", "tokens": ["Der", "schwar\u00b7ze", "Schat\u00b7ten", "wurd'", "er\u00b7hel\u00b7let"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "und in den g\u00f6ldnen Tag verstellet.", "tokens": ["und", "in", "den", "g\u00f6ld\u00b7nen", "Tag", "ver\u00b7stel\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}