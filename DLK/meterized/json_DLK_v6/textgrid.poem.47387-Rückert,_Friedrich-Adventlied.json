{"textgrid.poem.47387": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Adventlied", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein K\u00f6nig kommt in niedern H\u00fcllen,", "tokens": ["Dein", "K\u00f6\u00b7nig", "kommt", "in", "nie\u00b7dern", "H\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ihn tr\u00e4gt der lastbar'n Es'lin F\u00fcllen,", "tokens": ["Ihn", "tr\u00e4gt", "der", "last\u00b7ba\u00b7r'n", "Es'\u00b7lin", "F\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Empfang ihn froh, Jerusalem!", "tokens": ["Emp\u00b7fang", "ihn", "froh", ",", "Je\u00b7ru\u00b7sa\u00b7lem", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "$,", "NE", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Trag ihm entgegen Friedenspalmen,", "tokens": ["Trag", "ihm", "ent\u00b7ge\u00b7gen", "Frie\u00b7dens\u00b7pal\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Bestreu' den Pfad mit gr\u00fcnen Halmen!", "tokens": ["Be\u00b7streu'", "den", "Pfad", "mit", "gr\u00fc\u00b7nen", "Hal\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So ist's dem Herren angenehm.", "tokens": ["So", "ist's", "dem", "Her\u00b7ren", "an\u00b7ge\u00b7nehm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "O m\u00e4cht'ger Herrscher ohne Heere,", "tokens": ["O", "m\u00e4cht'\u00b7ger", "Herr\u00b7scher", "oh\u00b7ne", "Hee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gewalt'ger K\u00e4mpfer ohne Speere,", "tokens": ["Ge\u00b7walt'\u00b7ger", "K\u00e4mp\u00b7fer", "oh\u00b7ne", "Spee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "O Friedensf\u00fcrst von gro\u00dfer Macht!", "tokens": ["O", "Frie\u00b7dens\u00b7f\u00fcrst", "von", "gro\u00b7\u00dfer", "Macht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es wollen dir der Erde Herren", "tokens": ["Es", "wol\u00b7len", "dir", "der", "Er\u00b7de", "Her\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Den Weg zu deinem Throne sperren,", "tokens": ["Den", "Weg", "zu", "dei\u00b7nem", "Thro\u00b7ne", "sper\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Doch du gewinnst ihn ohne Schlacht.", "tokens": ["Doch", "du", "ge\u00b7winnst", "ihn", "oh\u00b7ne", "Schlacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Dein Reich ist nicht von dieser Erden,", "tokens": ["Dein", "Reich", "ist", "nicht", "von", "die\u00b7ser", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKNEG", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Doch aller Erde Reiche werden", "tokens": ["Doch", "al\u00b7ler", "Er\u00b7de", "Rei\u00b7che", "wer\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "NE", "VAFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dem, was du gr\u00fcndest, unterthan.", "tokens": ["Dem", ",", "was", "du", "gr\u00fcn\u00b7dest", ",", "un\u00b7ter\u00b7than", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "$,", "PWS", "PPER", "VVFIN", "$,", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bewaffnet mit des Glaubens Worten,", "tokens": ["Be\u00b7waff\u00b7net", "mit", "des", "Glau\u00b7bens", "Wor\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Zieht deine Schar nach den vier Orten", "tokens": ["Zieht", "dei\u00b7ne", "Schar", "nach", "den", "vier", "Or\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "ART", "CARD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Welt hinaus und macht dir Bahn.", "tokens": ["Der", "Welt", "hin\u00b7aus", "und", "macht", "dir", "Bahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "KON", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und wo du kommest hergezogen,", "tokens": ["Und", "wo", "du", "kom\u00b7mest", "her\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da ebnen sich des Meeres Wogen,", "tokens": ["Da", "eb\u00b7nen", "sich", "des", "Mee\u00b7res", "Wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Es schweigt der Sturm, von dir bedroht.", "tokens": ["Es", "schweigt", "der", "Sturm", ",", "von", "dir", "be\u00b7droht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du k\u00f6mmst, auf den emp\u00f6rten Triften", "tokens": ["Du", "k\u00f6mmst", ",", "auf", "den", "em\u00b7p\u00f6r\u00b7ten", "Trif\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Des Lebens neuen Bund zu stiften", "tokens": ["Des", "Le\u00b7bens", "neu\u00b7en", "Bund", "zu", "stif\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und schl\u00e4gst in Fessel S\u00fcnd' und Tod.", "tokens": ["Und", "schl\u00e4gst", "in", "Fes\u00b7sel", "S\u00fcnd'", "und", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "O Herr von gro\u00dfer Huld und Treue,", "tokens": ["O", "Herr", "von", "gro\u00b7\u00dfer", "Huld", "und", "Treu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "O komme du auch jetzt aufs neue", "tokens": ["O", "kom\u00b7me", "du", "auch", "jetzt", "aufs", "neu\u00b7e"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "ADV", "ADV", "APPRART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Zu uns, die wir sind schwer verst\u00f6rt.", "tokens": ["Zu", "uns", ",", "die", "wir", "sind", "schwer", "ver\u00b7st\u00f6rt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PRELS", "PPER", "VAFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Not ist es, da\u00df du selbst hienieden", "tokens": ["Not", "ist", "es", ",", "da\u00df", "du", "selbst", "hien\u00b7ie\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "$,", "KOUS", "PPER", "ADV", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Kommst, zu erneuen deinen Frieden,", "tokens": ["Kommst", ",", "zu", "er\u00b7neu\u00b7en", "dei\u00b7nen", "Frie\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PTKZU", "VVINF", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dagegen sich die Welt emp\u00f6rt.", "tokens": ["Da\u00b7ge\u00b7gen", "sich", "die", "Welt", "em\u00b7p\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "O la\u00df dein Licht auf Erden siegen,", "tokens": ["O", "la\u00df", "dein", "Licht", "auf", "Er\u00b7den", "sie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Macht der Finsternis erliegen,", "tokens": ["Die", "Macht", "der", "Fins\u00b7ter\u00b7nis", "er\u00b7lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und l\u00f6sch' der Zwietracht Glimmen aus;", "tokens": ["Und", "l\u00f6sch'", "der", "Zwiet\u00b7racht", "Glim\u00b7men", "aus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df wir, die V\u00f6lker und die Thronen,", "tokens": ["Da\u00df", "wir", ",", "die", "V\u00f6l\u00b7ker", "und", "die", "Thro\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Vereint als Br\u00fcder wieder wohnen", "tokens": ["Ver\u00b7eint", "als", "Br\u00fc\u00b7der", "wie\u00b7der", "woh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "KOUS", "NN", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "In deines gro\u00dfen Vaters Haus!", "tokens": ["In", "dei\u00b7nes", "gro\u00b7\u00dfen", "Va\u00b7ters", "Haus", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}