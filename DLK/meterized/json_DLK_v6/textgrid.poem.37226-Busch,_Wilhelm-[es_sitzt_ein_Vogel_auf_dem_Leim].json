{"textgrid.poem.37226": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "[es sitzt ein Vogel auf dem Leim]", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es sitzt ein Vogel auf dem Leim,", "tokens": ["Es", "sitzt", "ein", "Vo\u00b7gel", "auf", "dem", "Leim", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er flattert sehr und kann nicht heim.", "tokens": ["Er", "flat\u00b7tert", "sehr", "und", "kann", "nicht", "heim", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "KON", "VMFIN", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein schwarzer Kater schleicht herzu,", "tokens": ["Ein", "schwar\u00b7zer", "Ka\u00b7ter", "schleicht", "her\u00b7zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Krallen scharf, die Augen gluh.", "tokens": ["Die", "Kral\u00b7len", "scharf", ",", "die", "Au\u00b7gen", "gluh", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Am Baum hinauf und immer h\u00f6her", "tokens": ["Am", "Baum", "hin\u00b7auf", "und", "im\u00b7mer", "h\u00f6\u00b7her"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "PTKVZ", "KON", "ADV", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Kommt er dem armen Vogel n\u00e4her.", "tokens": ["Kommt", "er", "dem", "ar\u00b7men", "Vo\u00b7gel", "n\u00e4\u00b7her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NE", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Vogel denkt: Weil das so ist", "tokens": ["Der", "Vo\u00b7gel", "denkt", ":", "Weil", "das", "so", "ist"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NE", "VVFIN", "$.", "KOUS", "PDS", "ADV", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und weil mich doch der Kater fri\u00dft,", "tokens": ["Und", "weil", "mich", "doch", "der", "Ka\u00b7ter", "fri\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So will ich keine Zeit verlieren,", "tokens": ["So", "will", "ich", "kei\u00b7ne", "Zeit", "ver\u00b7lie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Will noch ein wenig quinquilieren", "tokens": ["Will", "noch", "ein", "we\u00b7nig", "quin\u00b7qui\u00b7lie\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "ART", "PIS", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und lustig pfeifen wie zuvor.", "tokens": ["Und", "lus\u00b7tig", "pfei\u00b7fen", "wie", "zu\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "KOKOM", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Vogel, scheint mir, hat Humor.", "tokens": ["Der", "Vo\u00b7gel", ",", "scheint", "mir", ",", "hat", "Hu\u00b7mor", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NE", "$,", "VVFIN", "PPER", "$,", "VAFIN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}