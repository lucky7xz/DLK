{"textgrid.poem.49876": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Scherz", "genre": "verse", "period": "N.A.", "pub_year": 1829, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einen Morgengru\u00df ihr fr\u00fch zu bringen,", "tokens": ["Ei\u00b7nen", "Mor\u00b7gen\u00b7gru\u00df", "ihr", "fr\u00fch", "zu", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und mein Morgenbrot bei ihr zu holen,", "tokens": ["Und", "mein", "Mor\u00b7gen\u00b7brot", "bei", "ihr", "zu", "ho\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Geh ich sachte an des M\u00e4dchens T\u00fcre,", "tokens": ["Geh", "ich", "sach\u00b7te", "an", "des", "M\u00e4d\u00b7chens", "T\u00fc\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "\u00d6ffne rasch, da steht mein schlankes B\u00e4umchen", "tokens": ["\u00d6ff\u00b7ne", "rasch", ",", "da", "steht", "mein", "schlan\u00b7kes", "B\u00e4um\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "ADV", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Vor dem Spiegel schon und wascht sich emsig.", "tokens": ["Vor", "dem", "Spie\u00b7gel", "schon", "und", "wascht", "sich", "em\u00b7sig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "KON", "VVFIN", "PRF", "ADJD", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "O wie lieblich tr\u00e4uft die wei\u00dfe Stirne,", "tokens": ["O", "wie", "lieb\u00b7lich", "tr\u00e4uft", "die", "wei\u00b7\u00dfe", "Stir\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Tr\u00e4uft die Rosenwange Silbern\u00e4sse!", "tokens": ["Tr\u00e4uft", "die", "Ro\u00b7sen\u00b7wan\u00b7ge", "Sil\u00b7ber\u00b7n\u00e4s\u00b7se", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Hangen aufgel\u00f6st die s\u00fc\u00dfen Haare!", "tokens": ["Han\u00b7gen", "auf\u00b7ge\u00b7l\u00f6st", "die", "s\u00fc\u00b7\u00dfen", "Haa\u00b7re", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Locker spielen T\u00fccher und Gew\u00e4nder.", "tokens": ["Lo\u00b7cker", "spie\u00b7len", "T\u00fc\u00b7cher", "und", "Ge\u00b7w\u00e4n\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Aber wie sie zagt und scheucht und abwehrt!", "tokens": ["A\u00b7ber", "wie", "sie", "zagt", "und", "scheucht", "und", "ab\u00b7wehrt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Gleich, sogleich soll ich den R\u00fcckzug nehmen!", "tokens": ["Gleich", ",", "sog\u00b7leich", "soll", "ich", "den", "R\u00fcck\u00b7zug", "neh\u00b7men", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VMFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "\u00bbn\u00e4rrchen\u00ab, rief ich, \u00bbsei mir so kein N\u00e4rrchen:", "tokens": ["\u00bb", "n\u00e4rr\u00b7chen", "\u00ab", ",", "rief", "ich", ",", "\u00bb", "sei", "mir", "so", "kein", "N\u00e4rr\u00b7chen", ":"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVINF", "$(", "$,", "VVFIN", "PPER", "$,", "$(", "VAFIN", "PPER", "ADV", "PIAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.13": {"text": "Das ist Brautrecht, ist Verlobtensitte.", "tokens": ["Das", "ist", "Braut\u00b7recht", ",", "ist", "Ver\u00b7lob\u00b7ten\u00b7sit\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NN", "$,", "VAFIN", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.14": {"text": "La\u00df mich nur, ich will ja blind und lahm sein,", "tokens": ["La\u00df", "mich", "nur", ",", "ich", "will", "ja", "blind", "und", "lahm", "sein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "$,", "PPER", "VMFIN", "ADV", "ADJD", "KON", "VVFIN", "VAINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.15": {"text": "Will den Kopf und alle beiden Augen", "tokens": ["Will", "den", "Kopf", "und", "al\u00b7le", "bei\u00b7den", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ART", "NN", "KON", "PIS", "PIAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.16": {"text": "In die F\u00fclle deiner Locken stecken,", "tokens": ["In", "die", "F\u00fcl\u00b7le", "dei\u00b7ner", "Lo\u00b7cken", "ste\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.17": {"text": "Will die H\u00e4nde mit den Flechten binden \u2013\u00ab", "tokens": ["Will", "die", "H\u00e4n\u00b7de", "mit", "den", "Flech\u00b7ten", "bin\u00b7den", "\u2013", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "ART", "NN", "APPR", "ART", "NN", "VVINF", "$(", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.18": {"text": "\u00bbnein, du gehst!\u00ab \u00bbIm Winkel la\u00df mich stehen,", "tokens": ["\u00bb", "nein", ",", "du", "gehst", "!", "\u00ab", "\u00bb", "Im", "Win\u00b7kel", "la\u00df", "mich", "ste\u00b7hen", ","], "token_info": ["punct", "word", "punct", "word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKANT", "$,", "PPER", "VVFIN", "$.", "$(", "$(", "APPRART", "NN", "VVIMP", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.19": {"text": "Dir bescheidentlich den R\u00fccken kehren!\u00ab", "tokens": ["Dir", "be\u00b7schei\u00b7dent\u00b7lich", "den", "R\u00fc\u00b7cken", "keh\u00b7ren", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "ADJD", "ART", "NN", "VVINF", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.20": {"text": "\u00bbei, so mag's, damit ich Ruhe habe!\u00ab", "tokens": ["\u00bb", "ei", ",", "so", "mag's", ",", "da\u00b7mit", "ich", "Ru\u00b7he", "ha\u00b7be", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ITJ", "$,", "ADV", "NE", "$,", "KOUS", "PPER", "NN", "VAFIN", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.21": {"text": "Und ich stand gehorsam in der Ecke,", "tokens": ["Und", "ich", "stand", "ge\u00b7hor\u00b7sam", "in", "der", "E\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.22": {"text": "L\u00e4cherlich, wie ein gestrafter Junge,", "tokens": ["L\u00e4\u00b7cher\u00b7lich", ",", "wie", "ein", "ge\u00b7straf\u00b7ter", "Jun\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.23": {"text": "Der die Lektion nicht wohl bestanden,", "tokens": ["Der", "die", "Lek\u00b7ti\u00b7on", "nicht", "wohl", "be\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PTKNEG", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.24": {"text": "Muckste nicht und k\u00fchlte mir die Lippen", "tokens": ["Mucks\u00b7te", "nicht", "und", "k\u00fchl\u00b7te", "mir", "die", "Lip\u00b7pen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PTKNEG", "KON", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.25": {"text": "An der wei\u00dfen Wand mit leisem Kusse,", "tokens": ["An", "der", "wei\u00b7\u00dfen", "Wand", "mit", "lei\u00b7sem", "Kus\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.26": {"text": "Eine volle, eine lange Stunde;", "tokens": ["Ei\u00b7ne", "vol\u00b7le", ",", "ei\u00b7ne", "lan\u00b7ge", "Stun\u00b7de", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.27": {"text": "Ja, so wahr ich lebe. Doch, wer etwa", "tokens": ["Ja", ",", "so", "wahr", "ich", "le\u00b7be", ".", "Doch", ",", "wer", "et\u00b7wa"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PTKANT", "$,", "ADV", "ADJD", "PPER", "VVFIN", "$.", "KON", "$,", "PWS", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Einen kleinen Zweifel m\u00f6chte haben", "tokens": ["Ei\u00b7nen", "klei\u00b7nen", "Zwei\u00b7fel", "m\u00f6ch\u00b7te", "ha\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VMFIN", "VAFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.29": {"text": "(was ich ihm just nicht verargen d\u00fcrfte),", "tokens": ["(", "was", "ich", "ihm", "just", "nicht", "ver\u00b7ar\u00b7gen", "d\u00fcrf\u00b7te", ")", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWS", "PPER", "PPER", "ADV", "PTKNEG", "VVINF", "VMFIN", "$(", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.30": {"text": "Nun, der frage nur das M\u00e4dchen selber:", "tokens": ["Nun", ",", "der", "fra\u00b7ge", "nur", "das", "M\u00e4d\u00b7chen", "sel\u00b7ber", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PRELS", "VVFIN", "ADV", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.31": {"text": "Die wird ihn \u2013 noch zierlicher bel\u00fcgen.", "tokens": ["Die", "wird", "ihn", "\u2013", "noch", "zier\u00b7li\u00b7cher", "be\u00b7l\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "$(", "ADV", "ADV", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}}}}