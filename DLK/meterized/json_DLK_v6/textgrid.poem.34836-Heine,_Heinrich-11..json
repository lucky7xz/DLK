{"textgrid.poem.34836": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "11.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Am einsamen Strande pl\u00e4tschert die Flut,", "tokens": ["Am", "ein\u00b7sa\u00b7men", "Stran\u00b7de", "pl\u00e4t\u00b7schert", "die", "Flut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Mond ist aufgegangen,", "tokens": ["Der", "Mond", "ist", "auf\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Auf wei\u00dfer D\u00fcne der Ritter ruht,", "tokens": ["Auf", "wei\u00b7\u00dfer", "D\u00fc\u00b7ne", "der", "Rit\u00b7ter", "ruht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Von bunten Tr\u00e4umen befangen.", "tokens": ["Von", "bun\u00b7ten", "Tr\u00e4u\u00b7men", "be\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Die sch\u00f6nen Nixen, im Schleiergewand,", "tokens": ["Die", "sch\u00f6\u00b7nen", "Ni\u00b7xen", ",", "im", "Schlei\u00b7er\u00b7ge\u00b7wand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPRART", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Entsteigen der Meerestiefe.", "tokens": ["Ent\u00b7stei\u00b7gen", "der", "Mee\u00b7res\u00b7tie\u00b7fe", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sie nahen sich leise dem jungen Fant,", "tokens": ["Sie", "na\u00b7hen", "sich", "lei\u00b7se", "dem", "jun\u00b7gen", "Fant", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Sie glaubten wahrhaftig, er schliefe.", "tokens": ["Sie", "glaub\u00b7ten", "wahr\u00b7haf\u00b7tig", ",", "er", "schlie\u00b7fe", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "PPER", "VVFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "Die eine betastet mit Neubegier", "tokens": ["Die", "ei\u00b7ne", "be\u00b7tas\u00b7tet", "mit", "Neu\u00b7be\u00b7gier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "VVFIN", "APPR", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Die Federn auf seinem Barette.", "tokens": ["Die", "Fe\u00b7dern", "auf", "sei\u00b7nem", "Ba\u00b7ret\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Die andre nestelt am Bandelier", "tokens": ["Die", "and\u00b7re", "nes\u00b7telt", "am", "Ban\u00b7de\u00b7lier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "VVFIN", "APPRART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und an der Waffenkette.", "tokens": ["Und", "an", "der", "Waf\u00b7fen\u00b7ket\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die dritte lacht, und ihr Auge blitzt,", "tokens": ["Die", "drit\u00b7te", "lacht", ",", "und", "ihr", "Au\u00b7ge", "blitzt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "$,", "KON", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sie zieht das Schwert aus der Scheide,", "tokens": ["Sie", "zieht", "das", "Schwert", "aus", "der", "Schei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und auf dem blanken Schwert gest\u00fctzt", "tokens": ["Und", "auf", "dem", "blan\u00b7ken", "Schwert", "ge\u00b7st\u00fctzt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Beschaut sie den Ritter mit Freude.", "tokens": ["Be\u00b7schaut", "sie", "den", "Rit\u00b7ter", "mit", "Freu\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.5": {"line.1": {"text": "Die vierte t\u00e4nzelt wohl hin und her", "tokens": ["Die", "vier\u00b7te", "t\u00e4n\u00b7zelt", "wohl", "hin", "und", "her"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "VVFIN", "ADV", "PTKVZ", "KON", "ADV"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und fl\u00fcstert aus tiefem Gem\u00fcte:", "tokens": ["Und", "fl\u00fcs\u00b7tert", "aus", "tie\u00b7fem", "Ge\u00b7m\u00fc\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "\u00bboh, da\u00df ich doch dein Liebchen w\u00e4r,", "tokens": ["\u00bb", "oh", ",", "da\u00df", "ich", "doch", "dein", "Lieb\u00b7chen", "w\u00e4r", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "$,", "KOUS", "PPER", "ADV", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du holde Menschenbl\u00fcte!\u00ab", "tokens": ["Du", "hol\u00b7de", "Men\u00b7schen\u00b7bl\u00fc\u00b7te", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PPER", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Die f\u00fcnfte k\u00fc\u00dft des Ritters H\u00e4nd',", "tokens": ["Die", "f\u00fcnf\u00b7te", "k\u00fc\u00dft", "des", "Rit\u00b7ters", "H\u00e4nd'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Sehnsucht und Verlangen;", "tokens": ["Mit", "Sehn\u00b7sucht", "und", "Ver\u00b7lan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die sechste z\u00f6gert und k\u00fc\u00dft am End'", "tokens": ["Die", "sechs\u00b7te", "z\u00f6\u00b7gert", "und", "k\u00fc\u00dft", "am", "End'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "VVFIN", "KON", "VVFIN", "APPRART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die Lippen und die Wangen.", "tokens": ["Die", "Lip\u00b7pen", "und", "die", "Wan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Der Ritter ist klug, es f\u00e4llt ihm nicht ein,", "tokens": ["Der", "Rit\u00b7ter", "ist", "klug", ",", "es", "f\u00e4llt", "ihm", "nicht", "ein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Augen \u00f6ffnen zu m\u00fcssen;", "tokens": ["Die", "Au\u00b7gen", "\u00f6ff\u00b7nen", "zu", "m\u00fcs\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "PTKZU", "VMINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Er l\u00e4\u00dft sich ruhig im Mondenschein", "tokens": ["Er", "l\u00e4\u00dft", "sich", "ru\u00b7hig", "im", "Mon\u00b7den\u00b7schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "APPRART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Von sch\u00f6nen Nixen k\u00fcssen.", "tokens": ["Von", "sch\u00f6\u00b7nen", "Ni\u00b7xen", "k\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}