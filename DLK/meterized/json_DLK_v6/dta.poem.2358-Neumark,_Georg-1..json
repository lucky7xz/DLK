{"dta.poem.2358": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1652", "urn": "urn:nbn:de:kobv:b4-20428-0", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch- und Musikalisches Lustw\u00e4ldchen. Hamburg, 1652."}, "poem": {"stanza.1": {"line.1": {"text": "Sey nur getrost und unverzaget/", "tokens": ["Sey", "nur", "ge\u00b7trost", "und", "un\u00b7ver\u00b7za\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVPP", "KON", "ADJD", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn Dich o Jsrael mein Kind/", "tokens": ["Wenn", "Dich", "o", "Js\u00b7rael", "mein", "Kind", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "FM", "NE", "PPOSAT", "NN", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Betr\u00fcbn\u00fc\u00df/ Angst und Wehmuht plaget/", "tokens": ["Be\u00b7tr\u00fcb\u00b7n\u00fc\u00df", "/", "Angst", "und", "Weh\u00b7muht", "pla\u00b7get", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wen\u0303 Dir der N\u00f6rdlich' Ungl\u00fckswind/", "tokens": ["We\u00f1", "Dir", "der", "N\u00f6rd\u00b7lich'", "Un\u00b7gl\u00fcks\u00b7wind", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mit seinem Sturm' und starkem Sausen", "tokens": ["Mit", "sei\u00b7nem", "Sturm'", "und", "star\u00b7kem", "Sau\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wil rasend unter Augen Brausen.", "tokens": ["Wil", "ra\u00b7send", "un\u00b7ter", "Au\u00b7gen", "Brau\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVPP", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Steh als ein Man\u0303/ la\u00df Dich nicht schtekken/", "tokens": ["Steh", "als", "ein", "Ma\u00f1", "/", "la\u00df", "Dich", "nicht", "schtek\u00b7ken", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "ART", "NN", "$(", "VVIMP", "PPER", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Halt aus/ und bleibe fest bestehn/", "tokens": ["Halt", "aus", "/", "und", "blei\u00b7be", "fest", "be\u00b7stehn", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$(", "KON", "VVFIN", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "La\u00df Dir es keine Furcht erwekken/", "tokens": ["La\u00df", "Dir", "es", "kei\u00b7ne", "Furcht", "er\u00b7wek\u00b7ken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPER", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "La\u00df dieses Wetter \u00fcber gehn.", "tokens": ["La\u00df", "die\u00b7ses", "Wet\u00b7ter", "\u00fc\u00b7ber", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PDAT", "NN", "APPR", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du wirst sehr grosse Freud' empfinden/", "tokens": ["Du", "wirst", "sehr", "gros\u00b7se", "Freud'", "emp\u00b7fin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Nach diesen herb- und rauhen Winden.", "tokens": ["Nach", "die\u00b7sen", "her\u00b7b", "und", "rau\u00b7hen", "Win\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "TRUNC", "KON", "ADJA", "NN", "$."], "meter": "---+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "La\u00df ab von deinen schweren Sorgen/", "tokens": ["La\u00df", "ab", "von", "dei\u00b7nen", "schwe\u00b7ren", "Sor\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich habe ja mein Angesicht", "tokens": ["Ich", "ha\u00b7be", "ja", "mein", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nur eine kleine Zeit verborgen/", "tokens": ["Nur", "ei\u00b7ne", "klei\u00b7ne", "Zeit", "ver\u00b7bor\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und von Dir Jsrael gericht/", "tokens": ["Und", "von", "Dir", "Js\u00b7rael", "ge\u00b7richt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "NE", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ich wil nur sehn in solchen F\u00e4llen", "tokens": ["Ich", "wil", "nur", "sehn", "in", "sol\u00b7chen", "F\u00e4l\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "VVINF", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wie Du/ mein Kind/ Dich an wirst-stellen.", "tokens": ["Wie", "Du", "/", "mein", "Kind", "/", "Dich", "an", "wir\u00b7ststel\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$(", "PPOSAT", "NN", "$(", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich wil ein wenig nur versuchen/", "tokens": ["Ich", "wil", "ein", "we\u00b7nig", "nur", "ver\u00b7su\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "PIS", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie gro\u00df doch deine Liebe sey/", "tokens": ["Wie", "gro\u00df", "doch", "dei\u00b7ne", "Lie\u00b7be", "sey", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADV", "PPOSAT", "NN", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ob Du Mir auch wirst trotzig fluchen/", "tokens": ["Ob", "Du", "Mir", "auch", "wirst", "trot\u00b7zig", "flu\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NE", "ADV", "VAFIN", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und wie best\u00e4ndig deine Treu/", "tokens": ["Und", "wie", "be\u00b7st\u00e4n\u00b7dig", "dei\u00b7ne", "Treu", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADJD", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich steupe Dich \u00fcm zu-zu-sehen", "tokens": ["Ich", "steu\u00b7pe", "Dich", "\u00fcm", "zu\u00b7zu\u00b7se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wie Du bey deinem Gott wirst stehen.", "tokens": ["Wie", "Du", "bey", "dei\u00b7nem", "Gott", "wirst", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "PPOSAT", "NN", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ich habe Dich bey deinem Nahmen", "tokens": ["Ich", "ha\u00b7be", "Dich", "bey", "dei\u00b7nem", "Nah\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Geruffen/ und Mir au\u00dferwehlt/", "tokens": ["Ge\u00b7ruf\u00b7fen", "/", "und", "Mir", "au\u00b7\u00dfer\u00b7wehlt", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$(", "KON", "NE", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich habe Dich und deinen Sahmen", "tokens": ["Ich", "ha\u00b7be", "Dich", "und", "dei\u00b7nen", "Sah\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Erl\u00f6set/ und mang Die gez\u00e4hlt", "tokens": ["Er\u00b7l\u00f6\u00b7set", "/", "und", "mang", "Die", "ge\u00b7z\u00e4hlt"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$(", "KON", "VVFIN", "ART", "VVPP"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Die sich gehorsamlich erzeigen/", "tokens": ["Die", "sich", "ge\u00b7hor\u00b7sam\u00b7lich", "er\u00b7zei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Denn Jsrael du bist mein eigen.", "tokens": ["Denn", "Js\u00b7rael", "du", "bist", "mein", "ei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "PPER", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Geh sicherlich durch Wasserfluhten/", "tokens": ["Geh", "si\u00b7cher\u00b7lich", "durch", "Was\u00b7ser\u00b7fluh\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PRF", "APPR", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du wirst gantz unver letzet gehn/", "tokens": ["Du", "wirst", "gantz", "un\u00b7ver", "let\u00b7zet", "gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Geh ungescheut durch Feuersgluhten/", "tokens": ["Geh", "un\u00b7ge\u00b7scheut", "durch", "Feu\u00b7ers\u00b7gluh\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "APPR", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Kein Brandmahl wird man an Dir sehn/", "tokens": ["Kein", "Brand\u00b7mahl", "wird", "man", "an", "Dir", "sehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PIS", "APPR", "PPER", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Denn Ich wil allzeit bey Dir bleiben/", "tokens": ["Denn", "Ich", "wil", "all\u00b7zeit", "bey", "Dir", "blei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "ADV", "APPR", "PPER", "VVINF", "$("], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Und allen Unfall von Dir treiben.", "tokens": ["Und", "al\u00b7len", "Un\u00b7fall", "von", "Dir", "trei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Kan Ich Dich nun in Diesen sch\u00fctzen/", "tokens": ["Kan", "Ich", "Dich", "nun", "in", "Die\u00b7sen", "sch\u00fct\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ADV", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was meinstu wol mein Jsrael/", "tokens": ["Was", "meins\u00b7tu", "wol", "mein", "Js\u00b7rael", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "PPOSAT", "NE", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Was Ich Dir werde k\u00f6nnen n\u00fctzen/", "tokens": ["Was", "Ich", "Dir", "wer\u00b7de", "k\u00f6n\u00b7nen", "n\u00fct\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPER", "VAFIN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn Dich betreffen andre F\u00e4ll'?", "tokens": ["Wenn", "Dich", "be\u00b7tref\u00b7fen", "and\u00b7re", "F\u00e4ll'", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich bin dein Heyland/ dein Erretter/", "tokens": ["Ich", "bin", "dein", "Hey\u00b7land", "/", "dein", "Er\u00b7ret\u00b7ter", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$(", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dr\u00fcm f\u00fcrcht Dich nicht im Ungl\u00fckswetter.", "tokens": ["Dr\u00fcm", "f\u00fcrcht", "Dich", "nicht", "im", "Un\u00b7gl\u00fcks\u00b7wet\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}