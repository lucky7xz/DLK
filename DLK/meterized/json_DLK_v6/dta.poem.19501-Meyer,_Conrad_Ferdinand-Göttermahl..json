{"dta.poem.19501": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "G\u00f6ttermahl.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Wo die Tannen finst're Schatten werfen", "tokens": ["Wo", "die", "Tan\u00b7nen", "finst'\u00b7re", "Schat\u00b7ten", "wer\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VVFIN", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ueber H\u00e4nge goldbesonnt,", "tokens": ["Ue\u00b7ber", "H\u00e4n\u00b7ge", "gold\u00b7be\u00b7sonnt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Unverwundet von der Firne Sch\u00e4rfen", "tokens": ["Un\u00b7ver\u00b7wun\u00b7det", "von", "der", "Fir\u00b7ne", "Sch\u00e4r\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Blaut der reine Horizont,", "tokens": ["Blaut", "der", "rei\u00b7ne", "Ho\u00b7ri\u00b7zont", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wo das Spiel den rastlos weh'nden Winden", "tokens": ["Wo", "das", "Spiel", "den", "rast\u00b7los", "weh'n\u00b7den", "Win\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ART", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Kein Geb\u00e4lk und keine Mauer wehrt,", "tokens": ["Kein", "Ge\u00b7b\u00e4lk", "und", "kei\u00b7ne", "Mau\u00b7er", "wehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wo, wie einer dunkeln Sorge Schwinden,", "tokens": ["Wo", ",", "wie", "ei\u00b7ner", "dun\u00b7keln", "Sor\u00b7ge", "Schwin\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "PWAV", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Jede Wolke sich verzehrt,", "tokens": ["Je\u00b7de", "Wol\u00b7ke", "sich", "ver\u00b7zehrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PRF", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wo das braune Rind wie Juno schauend", "tokens": ["Wo", "das", "brau\u00b7ne", "Rind", "wie", "Ju\u00b7no", "schau\u00b7end"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "KOKOM", "NN", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Weidet und mit heller Glocke t\u00f6nt,", "tokens": ["Wei\u00b7det", "und", "mit", "hel\u00b7ler", "Glo\u00b7cke", "t\u00f6nt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wo das Zicklein l\u00fcstern wiederkauend", "tokens": ["Wo", "das", "Zic\u00b7klein", "l\u00fcs\u00b7tern", "wie\u00b7der\u00b7kau\u00b7end"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADJD", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Den bemoosten Felsen kr\u00f6nt,", "tokens": ["Den", "be\u00b7moos\u00b7ten", "Fel\u00b7sen", "kr\u00f6nt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Schl\u00fcrf' ich k\u00fchle Luft und wilde W\u00fcrzen,", "tokens": ["Schl\u00fcr\u00b7f'", "ich", "k\u00fch\u00b7le", "Luft", "und", "wil\u00b7de", "W\u00fcr\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit den sel'gen G\u00f6ttern kost ich da \u2014", "tokens": ["Mit", "den", "sel'\u00b7gen", "G\u00f6t\u00b7tern", "kost", "ich", "da"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "PPER", "ADV", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Die mich nicht aus ihrem Himmel st\u00fcrzen \u2014", "tokens": ["Die", "mich", "nicht", "aus", "ih\u00b7rem", "Him\u00b7mel", "st\u00fcr\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PTKNEG", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Nectar und Ambrosia!", "tokens": ["Nec\u00b7tar", "und", "A\u00b7mbro\u00b7sia", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}