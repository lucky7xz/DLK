{"textgrid.poem.38039": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Klage der Churf\u00fcrstin,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach Gott mich thut verlangen,", "tokens": ["Ach", "Gott", "mich", "thut", "ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "PPER", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nach dem, der jezt gefangen,", "tokens": ["Nach", "dem", ",", "der", "jezt", "ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PRELS", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Den liebsten F\u00fcrsten mein,", "tokens": ["Den", "liebs\u00b7ten", "F\u00fcrs\u00b7ten", "mein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df ich ihn so mu\u00df meiden,", "tokens": ["Da\u00df", "ich", "ihn", "so", "mu\u00df", "mei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Bringt mir ein herzlich Leiden;", "tokens": ["Bringt", "mir", "ein", "herz\u00b7lich", "Lei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ach Gott hilf ihm aus dieser Pein.", "tokens": ["Ach", "Gott", "hilf", "ihm", "aus", "die\u00b7ser", "Pein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "VVFIN", "PPER", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Er ist in Kaisers H\u00e4nden,", "tokens": ["Er", "ist", "in", "Kai\u00b7sers", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mein Gott thu es bald wenden,", "tokens": ["Mein", "Gott", "thu", "es", "bald", "wen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-++--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Dem Kaiser gieb den Muth,", "tokens": ["Dem", "Kai\u00b7ser", "gieb", "den", "Muth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVIMP", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df er recht thu bedenken,", "tokens": ["Da\u00df", "er", "recht", "thu", "be\u00b7den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Woher komm dieses Z\u00e4nken,", "tokens": ["Wo\u00b7her", "komm", "die\u00b7ses", "Z\u00e4n\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PDAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Dem F\u00fcrsten geb wieder sein Gut.", "tokens": ["Dem", "F\u00fcrs\u00b7ten", "geb", "wie\u00b7der", "sein", "Gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ob er was h\u00e4tt verbrochen,", "tokens": ["Ob", "er", "was", "h\u00e4tt", "ver\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "F\u00fcrwahr ist g'nug gerochen,", "tokens": ["F\u00fcr\u00b7wahr", "ist", "g'\u00b7nug", "ge\u00b7ro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Land, Leut hat man verderbt,", "tokens": ["Land", ",", "Leut", "hat", "man", "ver\u00b7derbt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "VAFIN", "PIS", "ADJD", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Den F\u00fcrsten abgef\u00fchret,", "tokens": ["Den", "F\u00fcrs\u00b7ten", "ab\u00b7ge\u00b7f\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mein Herz damit ger\u00fchret,", "tokens": ["Mein", "Herz", "da\u00b7mit", "ge\u00b7r\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PAV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Chur hat man ihn enterbt.", "tokens": ["Der", "Chur", "hat", "man", "ihn", "en\u00b7terbt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}