{"textgrid.poem.65485": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "58. Die Reisenden", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf einem Esel reitest du, dein Vordermann auf einem Ro\u00df,", "tokens": ["Auf", "ei\u00b7nem", "E\u00b7sel", "rei\u00b7test", "du", ",", "dein", "Vor\u00b7der\u00b7mann", "auf", "ei\u00b7nem", "Ro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Und hinter deinen Fersen keucht zu Fu\u00df ein ungez\u00e4hlter Tro\u00df.", "tokens": ["Und", "hin\u00b7ter", "dei\u00b7nen", "Fer\u00b7sen", "keucht", "zu", "Fu\u00df", "ein", "un\u00b7ge\u00b7z\u00e4hl\u00b7ter", "Tro\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVFIN", "APPR", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "Du siehst mit Neid dem Einen nach, wie Viele sehn dir hinterdrein,", "tokens": ["Du", "siehst", "mit", "Neid", "dem", "Ei\u00b7nen", "nach", ",", "wie", "Vie\u00b7le", "sehn", "dir", "hin\u00b7ter\u00b7drein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ART", "ART", "APPR", "$,", "PWAV", "PIS", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "Und wenn die Herberg' ist erreicht, gehn Alle doch zu Fu\u00df hinein.", "tokens": ["Und", "wenn", "die", "Her\u00b7ber\u00b7g'", "ist", "er\u00b7reicht", ",", "gehn", "Al\u00b7le", "doch", "zu", "Fu\u00df", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VAFIN", "VVPP", "$,", "VVFIN", "PIS", "ADV", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+--+-+-+-+-+-+", "measure": "iambic.octa.plus.relaxed"}}}}}