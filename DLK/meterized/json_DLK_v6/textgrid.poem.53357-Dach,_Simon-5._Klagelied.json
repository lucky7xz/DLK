{"textgrid.poem.53357": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "5. Klagelied", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Morgenr\u00f6the f\u00e4hrt kaum aus Tithonus Bette,", "tokens": ["Die", "Mor\u00b7gen\u00b7r\u00f6\u00b7the", "f\u00e4hrt", "kaum", "aus", "Ti\u00b7tho\u00b7nus", "Bet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Schrifftgelehrten sind schon auff als umb die wette,", "tokens": ["Die", "Schrifft\u00b7ge\u00b7lehr\u00b7ten", "sind", "schon", "auff", "als", "umb", "die", "wet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "APPR", "KOKOM", "APPR", "ART", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das Volck verwundert sich was heiligs sie heraus", "tokens": ["Das", "Volck", "ver\u00b7wun\u00b7dert", "sich", "was", "hei\u00b7ligs", "sie", "he\u00b7raus"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "PWS", "ADJA", "PPER", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Gelockt, denn keiner macht sich in des Herren Haus.", "tokens": ["Ge\u00b7lockt", ",", "denn", "kei\u00b7ner", "macht", "sich", "in", "des", "Her\u00b7ren", "Haus", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KON", "PIS", "VVFIN", "PRF", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Vielleicht weil man zu raht sol gehn von hohen Dingen,", "tokens": ["Viel\u00b7leicht", "weil", "man", "zu", "raht", "sol", "gehn", "von", "ho\u00b7hen", "Din\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PIS", "APPR", "NN", "VMFIN", "VVINF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Denn es viel Rahts bedarff die Vnschuld umbzubringen,", "tokens": ["Denn", "es", "viel", "Rahts", "be\u00b7darff", "die", "Vn\u00b7schuld", "um\u00b7bzu\u00b7brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PIAT", "NN", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Sie setzen sich, ihr Hertz ist voll von neuer List,", "tokens": ["Sie", "set\u00b7zen", "sich", ",", "ihr", "Hertz", "ist", "voll", "von", "neu\u00b7er", "List", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "$,", "PPOSAT", "NN", "VAFIN", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Als welcher Stirn auch gar von Lastern schwanger ist.", "tokens": ["Als", "wel\u00b7cher", "Stirn", "auch", "gar", "von", "Las\u00b7tern", "schwan\u00b7ger", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PWAT", "NN", "ADV", "ADV", "APPR", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Theils rahten da\u00df man sol einhellig dahin schliessen,", "tokens": ["Theils", "rah\u00b7ten", "da\u00df", "man", "sol", "ein\u00b7hel\u00b7lig", "da\u00b7hin", "schlies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KOUS", "PIS", "VMFIN", "ADJD", "PAV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Da\u00df nach der V\u00e4ter Recht er werde hingerissen,", "tokens": ["Da\u00df", "nach", "der", "V\u00e4\u00b7ter", "Recht", "er", "wer\u00b7de", "hin\u00b7ge\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "NN", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "O nein, spricht Caiphas: der P\u00f6fel macht ihn frey,", "tokens": ["O", "nein", ",", "spricht", "Cai\u00b7phas", ":", "der", "P\u00f6\u00b7fel", "macht", "ihn", "frey", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKANT", "$,", "VVFIN", "NE", "$.", "ART", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.12": {"text": "Man weis, da\u00df er ohn Schuld bey uns in Mi\u00dfgunst sey.", "tokens": ["Man", "weis", ",", "da\u00df", "er", "ohn", "Schuld", "bey", "uns", "in", "Mi\u00df\u00b7gunst", "sey", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PTKVZ", "$,", "KOUS", "PPER", "APPR", "NN", "APPR", "PPER", "APPR", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wie, wenn wir nur den Schein der Vnschuld \u00fcberkommen?", "tokens": ["Wie", ",", "wenn", "wir", "nur", "den", "Schein", "der", "Vn\u00b7schuld", "\u00fc\u00b7ber\u00b7kom\u00b7men", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "KOUS", "PPER", "ADV", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Was hindert es, kan uns nur, ungerecht seyn, frommen?", "tokens": ["Was", "hin\u00b7dert", "es", ",", "kan", "uns", "nur", ",", "un\u00b7ge\u00b7recht", "seyn", ",", "from\u00b7men", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "VMFIN", "PPER", "ADV", "$,", "ADJD", "VAINF", "$,", "ADJA", "$."], "meter": "-+-++--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.15": {"text": "Nur das Pilat mit Macht dem tollen P\u00f6fel wehr,", "tokens": ["Nur", "das", "Pi\u00b7lat", "mit", "Macht", "dem", "tol\u00b7len", "P\u00f6\u00b7fel", "wehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "NN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Vnd, als der Richter, sey von allem Argwohn leer.", "tokens": ["Vnd", ",", "als", "der", "Rich\u00b7ter", ",", "sey", "von", "al\u00b7lem", "Arg\u00b7wohn", "leer", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "ART", "NN", "$,", "VAFIN", "APPR", "PIS", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Wie solte nicht der Spruch nach Wunsch sich zu uns neigen?", "tokens": ["Wie", "sol\u00b7te", "nicht", "der", "Spruch", "nach", "Wunsch", "sich", "zu", "uns", "nei\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PTKNEG", "ART", "NN", "APPR", "NN", "PRF", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Selbst die Gerechtigkeit hat Schuld bey diesen Zeugen.", "tokens": ["Selbst", "die", "Ge\u00b7rech\u00b7tig\u00b7keit", "hat", "Schuld", "bey", "die\u00b7sen", "Zeu\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "NN", "APPR", "PDAT", "NN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.19": {"text": "Offt weissagt Caiphas ohn Willen und Verstand,", "tokens": ["Offt", "weis\u00b7sagt", "Cai\u00b7phas", "ohn", "Wil\u00b7len", "und", "Ver\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "APPR", "NN", "KON", "NN", "$,"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.20": {"text": "Denn die Gerechtigkeit wird schuldig jetzt erkand.", "tokens": ["Denn", "die", "Ge\u00b7rech\u00b7tig\u00b7keit", "wird", "schul\u00b7dig", "jetzt", "er\u00b7kand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADJD", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Nun mu\u00df der Herr und sie vor einem R\u00f6mer stehen,", "tokens": ["Nun", "mu\u00df", "der", "Herr", "und", "sie", "vor", "ei\u00b7nem", "R\u00f6\u00b7mer", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "KON", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Doch die Gerichts Ahrt mu\u00df auff R\u00f6misch nicht ergehen.", "tokens": ["Doch", "die", "Ge\u00b7richts", "Ahrt", "mu\u00df", "auff", "R\u00f6\u00b7misch", "nicht", "er\u00b7ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "VMFIN", "APPR", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Denn hie steht der Beklagt', und da der Kl\u00e4ger Schaar,", "tokens": ["Denn", "hie", "steht", "der", "Be\u00b7klagt'", ",", "und", "da", "der", "Kl\u00e4\u00b7ger", "Schaar", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "$,", "KON", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.24": {"text": "Was in Jerusalem von Priestern irgends war.", "tokens": ["Was", "in", "Je\u00b7ru\u00b7sa\u00b7lem", "von", "Pries\u00b7tern", "ir\u00b7gends", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NE", "APPR", "NN", "ADV", "VAFIN", "$."], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.25": {"text": "Man wei\u00df nicht ob sie mehr Klag oder Schm\u00e4hwort f\u00fchren,", "tokens": ["Man", "wei\u00df", "nicht", "ob", "sie", "mehr", "Klag", "o\u00b7der", "Schm\u00e4h\u00b7wort", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PTKNEG", "KOUS", "PPER", "PIAT", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.26": {"text": "Dies wei\u00df man, beydes m\u00fcss' aus lauter Mi\u00dfgunst r\u00fchren.", "tokens": ["Dies", "wei\u00df", "man", ",", "bey\u00b7des", "m\u00fcss'", "aus", "lau\u00b7ter", "Mi\u00df\u00b7gunst", "r\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIS", "$,", "PIS", "VMFIN", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Den Schultheis jammert selbst, der solches Vnrecht sieht,", "tokens": ["Den", "Schult\u00b7heis", "jam\u00b7mert", "selbst", ",", "der", "sol\u00b7ches", "Vn\u00b7recht", "sieht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Wird dem Beklagten hold, weil ihm zu viel geschieht.", "tokens": ["Wird", "dem", "Be\u00b7klag\u00b7ten", "hold", ",", "weil", "ihm", "zu", "viel", "ge\u00b7schieht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJD", "$,", "KOUS", "PPER", "PTKA", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Vnd weil er gern verzeucht, ihn gern gesch\u00fctzet h\u00e4tte,", "tokens": ["Vnd", "weil", "er", "gern", "ver\u00b7zeucht", ",", "ihn", "gern", "ge\u00b7sch\u00fct\u00b7zet", "h\u00e4t\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VVPP", "$,", "PPER", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Vnd alle Kunst erdenckt wie er die Vnschuld rette,", "tokens": ["Vnd", "al\u00b7le", "Kunst", "er\u00b7denckt", "wie", "er", "die", "Vn\u00b7schuld", "ret\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVPP", "KOKOM", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Sagt einer ohn gefehr, er bringt viel Volcks zu hauff,", "tokens": ["Sagt", "ei\u00b7ner", "ohn", "ge\u00b7fehr", ",", "er", "bringt", "viel", "Volcks", "zu", "hauff", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "APPR", "NN", "$,", "PPER", "VVFIN", "PIAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Ist er aus Galilee, das wiegelt er gantz auff;", "tokens": ["Ist", "er", "aus", "Ga\u00b7li\u00b7lee", ",", "das", "wie\u00b7gelt", "er", "gantz", "auff", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "NN", "$,", "PDS", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+---+", "measure": "unknown.measure.penta"}, "line.33": {"text": "Die Sach' und er geh\u00f6rt vor mich nicht, darumb steh' er", "tokens": ["Die", "Sach'", "und", "er", "ge\u00b7h\u00f6rt", "vor", "mich", "nicht", ",", "da\u00b7rumb", "steh'", "er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "KON", "PPER", "VVFIN", "APPR", "PPER", "PTKNEG", "$,", "PAV", "VVFIN", "PPER"], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.34": {"text": "Vor seinem K\u00f6nig, weg, er ist ein Galileer.", "tokens": ["Vor", "sei\u00b7nem", "K\u00f6\u00b7nig", ",", "weg", ",", "er", "ist", "ein", "Ga\u00b7li\u00b7leer", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PTKVZ", "$,", "PPER", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "So spricht Pilat, und l\u00e4st den Richtstuel ledig stehn,", "tokens": ["So", "spricht", "Pi\u00b7lat", ",", "und", "l\u00e4st", "den", "Richt\u00b7stu\u00b7el", "le\u00b7dig", "stehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "$,", "KON", "VVFIN", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.36": {"text": "Da siht man alles Volck stracks zum Herodes gehn.", "tokens": ["Da", "siht", "man", "al\u00b7les", "Volck", "stracks", "zum", "He\u00b7ro\u00b7des", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PIAT", "NN", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Er k\u00f6mpt, der, K\u00f6nig, dich aus deinem Reich wil heben,", "tokens": ["Er", "k\u00f6mpt", ",", "der", ",", "K\u00f6\u00b7nig", ",", "dich", "aus", "dei\u00b7nem", "Reich", "wil", "he\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "$,", "NN", "$,", "PRF", "APPR", "PPOSAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Mein h\u00fct dich, schaw wie viel Soldaten ihn umbgeben.", "tokens": ["Mein", "h\u00fct", "dich", ",", "schaw", "wie", "viel", "Sol\u00b7da\u00b7ten", "ihn", "umb\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "PPER", "$,", "ADJD", "KOKOM", "PIAT", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Vnd dieses einig schwert der Schrifftgelehrten Raht,", "tokens": ["Vnd", "die\u00b7ses", "ei\u00b7nig", "schwert", "der", "Schrifft\u00b7ge\u00b7lehr\u00b7ten", "Raht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Ihr Zeug' ist Gott,