{"textgrid.poem.37726": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Verschiedenheit", "genre": "verse", "period": "N.A.", "pub_year": 1808, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie Ketten, die in H\u00f6llengluth verbunden,", "tokens": ["Wie", "Ket\u00b7ten", ",", "die", "in", "H\u00f6l\u00b7len\u00b7gluth", "ver\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "PRELS", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So schaute ich in unsrer Zeit die B\u00f6sen,", "tokens": ["So", "schau\u00b7te", "ich", "in", "uns\u00b7rer", "Zeit", "die", "B\u00f6\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Guten sind gefesselt und geschunden", "tokens": ["Die", "Gu\u00b7ten", "sind", "ge\u00b7fes\u00b7selt", "und", "ge\u00b7schun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP", "KON", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und jeder m\u00f6chte sich daraus erl\u00f6sen,", "tokens": ["Und", "je\u00b7der", "m\u00f6ch\u00b7te", "sich", "da\u00b7raus", "er\u00b7l\u00f6\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VMFIN", "PRF", "PAV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Doch keiner kann den anderen verstehen,", "tokens": ["Doch", "kei\u00b7ner", "kann", "den", "an\u00b7de\u00b7ren", "ver\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VMFIN", "ART", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und einzeln m\u00fcssen sie zu Grunde gehen.", "tokens": ["Und", "ein\u00b7zeln", "m\u00fcs\u00b7sen", "sie", "zu", "Grun\u00b7de", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Woher der B\u00f6sen Macht, der Guten Schw\u00e4che?", "tokens": ["Wo\u00b7her", "der", "B\u00f6\u00b7sen", "Macht", ",", "der", "Gu\u00b7ten", "Schw\u00e4\u00b7che", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es trockneten des Glaubens Segensb\u00e4che", "tokens": ["Es", "trock\u00b7ne\u00b7ten", "des", "Glau\u00b7bens", "Se\u00b7gens\u00b7b\u00e4\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Im Gl\u00fccke aus, und nun das Ungl\u00fcck kommen,", "tokens": ["Im", "Gl\u00fc\u00b7cke", "aus", ",", "und", "nun", "das", "Un\u00b7gl\u00fcck", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$,", "KON", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "H\u00e4lt Zweifel eure Herzen noch beklommen,", "tokens": ["H\u00e4lt", "Zwei\u00b7fel", "eu\u00b7re", "Her\u00b7zen", "noch", "be\u00b7klom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PPOSAT", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ihr theilt euch zwischen Gott und eurem Zweifel,", "tokens": ["Ihr", "theilt", "euch", "zwi\u00b7schen", "Gott", "und", "eu\u00b7rem", "Zwei\u00b7fel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und jene sind ganz eins mit ihrem Teufel.", "tokens": ["Und", "je\u00b7ne", "sind", "ganz", "eins", "mit", "ih\u00b7rem", "Teu\u00b7fel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "ADV", "PIS", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}