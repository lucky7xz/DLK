{"textgrid.poem.56870": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Kaum glaub ichs noch! Katull, du bist daheim!", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kaum glaub ichs noch! Katull, du bist daheim!", "tokens": ["Kaum", "glaub", "ichs", "noch", "!", "Ka\u00b7tull", ",", "du", "bist", "da\u00b7heim", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "$.", "NE", "$,", "PPER", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Daheim auf deinem lieben Sirmio!", "tokens": ["Da\u00b7heim", "auf", "dei\u00b7nem", "lie\u00b7ben", "Sir\u00b7mio", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Oh Sirmio, Sirmio, Kronjuwel Neptuns!", "tokens": ["Oh", "Sir\u00b7mio", ",", "Sir\u00b7mio", ",", "Kron\u00b7ju\u00b7wel", "Nep\u00b7tuns", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["XY", "XY", "$,", "NE", "$,", "NE", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In allen Meeren, Str\u00f6men, Seen sucht", "tokens": ["In", "al\u00b7len", "Mee\u00b7ren", ",", "Str\u00f6\u00b7men", ",", "Seen", "sucht"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["APPR", "PIAT", "NN", "$,", "NN", "$,", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Deingleichen man umsonst: Kein Vorgebirg,", "tokens": ["Dein\u00b7glei\u00b7chen", "man", "um\u00b7sonst", ":", "Kein", "Vor\u00b7ge\u00b7birg", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PIS", "ADV", "$.", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "kein Halbeiland, kein Eiland kommt dir gleich!", "tokens": ["kein", "Hal\u00b7bei\u00b7land", ",", "kein", "Ei\u00b7land", "kommt", "dir", "gleich", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wie gern bin ich zu dir zur\u00fcck geeilt!", "tokens": ["Wie", "gern", "bin", "ich", "zu", "dir", "zu\u00b7r\u00fcck", "ge\u00b7eilt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "PPER", "APPR", "PPER", "PTKVZ", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Wie sch\u00f6n, die Sorg und all den fremden Kram,", "tokens": ["Wie", "sch\u00f6n", ",", "die", "Sorg", "und", "all", "den", "frem\u00b7den", "Kram", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "ART", "NN", "KON", "PIAT", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "der mir nichts ist, im R\u00fccken weit, weit, weit,", "tokens": ["der", "mir", "nichts", "ist", ",", "im", "R\u00fc\u00b7cken", "weit", ",", "weit", ",", "weit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "PPER", "PIS", "VAFIN", "$,", "APPRART", "NN", "ADJD", "$,", "ADJD", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "am eignen Tisch, im eignen Bett zu ruhn!", "tokens": ["am", "eig\u00b7nen", "Tisch", ",", "im", "eig\u00b7nen", "Bett", "zu", "ruhn", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "APPRART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Das ist doch noch ein Lohn nach so viel Plag!", "tokens": ["Das", "ist", "doch", "noch", "ein", "Lohn", "nach", "so", "viel", "Plag", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADV", "ART", "NN", "APPR", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Mein Zauber-Sirmio, freust du dich denn auch?", "tokens": ["Mein", "Zau\u00b7ber\u00b7Sir\u00b7mio", ",", "freust", "du", "dich", "denn", "auch", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "PPER", "PRF", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Und du, mein See, brandest du mir Willkomm?", "tokens": ["Und", "du", ",", "mein", "See", ",", "bran\u00b7dest", "du", "mir", "Will\u00b7komm", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "PPER", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Ja, alles lacht und ruft: Katull ist da!", "tokens": ["Ja", ",", "al\u00b7les", "lacht", "und", "ruft", ":", "Ka\u00b7tull", "ist", "da", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PIS", "VVFIN", "KON", "VVFIN", "$.", "NE", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}