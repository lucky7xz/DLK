{"textgrid.poem.36776": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Stanzen des Raffael", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sagen Sie mir, um Vergebung, was stellt das Ding an der Wand hier", "tokens": ["Sa\u00b7gen", "Sie", "mir", ",", "um", "Ver\u00b7ge\u00b7bung", ",", "was", "stellt", "das", "Ding", "an", "der", "Wand", "hier"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "$,", "KOUI", "NN", "$,", "PWS", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "ADV"], "meter": "+---+-+--+-+--+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Eigentlich vor? \u2013 \u00bbEi das ist ja die Schul' von Athen!\u00ab \u2013", "tokens": ["Ei\u00b7gent\u00b7lich", "vor", "?", "\u2013", "\u00bb", "Ei", "das", "ist", "ja", "die", "Schul'", "von", "A\u00b7then", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "PTKVZ", "$.", "$(", "$(", "NN", "PDS", "VAFIN", "ADV", "ART", "NN", "APPR", "NE", "$.", "$(", "$("], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "So, die Schul' von Athen? Nun f\u00fchren Sie doch mich, ich bitte,", "tokens": ["So", ",", "die", "Schul'", "von", "A\u00b7then", "?", "Nun", "f\u00fch\u00b7ren", "Sie", "doch", "mich", ",", "ich", "bit\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "ART", "NN", "APPR", "NE", "$.", "ADV", "VVFIN", "PPER", "ADV", "PPER", "$,", "PPER", "ADV", "$,"], "meter": "--+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Weiter, ich m\u00f6cht' auch gern Raffaels Stanzen besehn.", "tokens": ["Wei\u00b7ter", ",", "ich", "m\u00f6cht'", "auch", "gern", "Raf\u00b7faels", "Stan\u00b7zen", "be\u00b7sehn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "VMFIN", "ADV", "ADV", "NN", "NN", "VVINF", "$."], "meter": "+--+--+-+--+", "measure": "dactylic.di.plus"}}}}}