{"textgrid.poem.55539": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "An meine Lieder", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seid, geliebte kleine Lieder,", "tokens": ["Seid", ",", "ge\u00b7lieb\u00b7te", "klei\u00b7ne", "Lie\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VAIMP", "$,", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zeugen meiner Fr\u00f6hlichkeit;", "tokens": ["Zeu\u00b7gen", "mei\u00b7ner", "Fr\u00f6h\u00b7lich\u00b7keit", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach sie k\u00f6mmt gewi\u00df nicht wieder,", "tokens": ["Ach", "sie", "k\u00f6mmt", "ge\u00b7wi\u00df", "nicht", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PPER", "VVFIN", "ADV", "PTKNEG", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dieser Tage Fr\u00fchlingszeit.", "tokens": ["Die\u00b7ser", "Ta\u00b7ge", "Fr\u00fch\u00b7lings\u00b7zeit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Bald entflieht der Freund der Scherze,", "tokens": ["Bald", "ent\u00b7flieht", "der", "Freund", "der", "Scher\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Er, dem ich euch sang, mein Freund.", "tokens": ["Er", ",", "dem", "ich", "euch", "sang", ",", "mein", "Freund", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "PPER", "VVFIN", "$,", "PPOSAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ach, da\u00df auch vielleicht dies Herze", "tokens": ["Ach", ",", "da\u00df", "auch", "viel\u00b7leicht", "dies", "Her\u00b7ze"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "KOUS", "ADV", "ADV", "PDS", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bald um meine Liebste weint!", "tokens": ["Bald", "um", "mei\u00b7ne", "Liebs\u00b7te", "weint", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch wenn nach der Trennung Leiden", "tokens": ["Doch", "wenn", "nach", "der", "Tren\u00b7nung", "Lei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "APPR", "ART", "NN", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Einst auf euch ihr Auge blickt,", "tokens": ["Einst", "auf", "euch", "ihr", "Au\u00b7ge", "blickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.3": {"text": "Dann erinnert sie der Freuden,", "tokens": ["Dann", "e\u00b7rin\u00b7nert", "sie", "der", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die uns sonst vereint erquickt.", "tokens": ["Die", "uns", "sonst", "ver\u00b7eint", "er\u00b7quickt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVPP", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}