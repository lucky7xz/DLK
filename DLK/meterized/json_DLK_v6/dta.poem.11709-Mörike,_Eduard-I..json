{"dta.poem.11709": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "I.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Der Spiegel dieser treuen, braunen Augen", "tokens": ["Der", "Spie\u00b7gel", "die\u00b7ser", "treu\u00b7en", ",", "brau\u00b7nen", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "PDAT", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ist wie von innerm Gold ein Widerschein;", "tokens": ["Ist", "wie", "von", "in\u00b7nerm", "Gold", "ein", "Wi\u00b7der\u00b7schein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "APPR", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Tief aus dem Busen scheint er's anzusaugen,", "tokens": ["Tief", "aus", "dem", "Bu\u00b7sen", "scheint", "er's", "an\u00b7zu\u00b7sau\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "PIS", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dort mag solch Gold in heil'gem Gram gedeih'n.", "tokens": ["Dort", "mag", "solch", "Gold", "in", "heil'\u00b7gem", "Gram", "ge\u00b7deih'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["ADV", "VMFIN", "PIAT", "NN", "APPR", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "In diese Nacht des Blickes mich zu tauchen,", "tokens": ["In", "die\u00b7se", "Nacht", "des", "Bli\u00b7ckes", "mich", "zu", "tau\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ART", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Unwissend Kind, du selber l\u00e4dst mich ein,", "tokens": ["Un\u00b7wis\u00b7send", "Kind", ",", "du", "sel\u00b7ber", "l\u00e4dst", "mich", "ein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "PPER", "ADV", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Willst, ich soll kecklich mich und dich entz\u00fcnden,", "tokens": ["Willst", ",", "ich", "soll", "keck\u00b7lich", "mich", "und", "dich", "ent\u00b7z\u00fcn\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "$,", "PPER", "VMFIN", "ADJD", "PPER", "KON", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Reichst l\u00e4chelnd mir den Tod im Kelch der S\u00fcnden!", "tokens": ["Reichst", "l\u00e4\u00b7chelnd", "mir", "den", "Tod", "im", "Kelch", "der", "S\u00fcn\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "ART", "NN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}