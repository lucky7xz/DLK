{"dta.poem.2768": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Uber die Kaltsinnigkeit der Liebsten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dunckle H\u00f6len/ finstre Schatten/", "tokens": ["Dunck\u00b7le", "H\u00f6\u00b7len", "/", "finst\u00b7re", "Schat\u00b7ten", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meines Lebens Auffenthalt/", "tokens": ["Mei\u00b7nes", "Le\u00b7bens", "Auf\u00b7fent\u00b7halt", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "W\u00fcste Felder/ stille Matten/", "tokens": ["W\u00fcs\u00b7te", "Fel\u00b7der", "/", "stil\u00b7le", "Mat\u00b7ten", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einsam und verschwiegner Wald/", "tokens": ["Ein\u00b7sam", "und", "ver\u00b7schwieg\u00b7ner", "Wald", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "K\u00f6nnt ihr auch die Seuffzer zehlen/", "tokens": ["K\u00f6nnt", "ihr", "auch", "die", "Seuff\u00b7zer", "zeh\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die ich t\u00e4glich abgeschickt/", "tokens": ["Die", "ich", "t\u00e4g\u00b7lich", "ab\u00b7ge\u00b7schickt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wenn mein Hertz mit neuem Qu\u00e4hlen", "tokens": ["Wenn", "mein", "Hertz", "mit", "neu\u00b7em", "Qu\u00e4h\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ein besch werlich Leyd gedruckt.", "tokens": ["Ein", "be\u00b7sch", "wer\u00b7lich", "Leyd", "ge\u00b7druckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJD", "NN", "VVPP", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.9": {"text": "Ach ihr Zeugen meiner Schmertzen/", "tokens": ["Ach", "ihr", "Zeu\u00b7gen", "mei\u00b7ner", "Schmert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PPOSAT", "NN", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Sagt doch k\u00fchnlich was ihr wi\u00dft;", "tokens": ["Sagt", "doch", "k\u00fchn\u00b7lich", "was", "ihr", "wi\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "PWS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Und ihr hellen Sternen-Kertzen/", "tokens": ["Und", "ihr", "hel\u00b7len", "Ster\u00b7nen\u00b7Kert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Suchet wo der Ursprung ist.", "tokens": ["Su\u00b7chet", "wo", "der", "Ur\u00b7sprung", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PWAV", "ART", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Anemonens edle Tugend", "tokens": ["A\u00b7ne\u00b7mo\u00b7nens", "ed\u00b7le", "Tu\u00b7gend"], "token_info": ["word", "word", "word"], "pos": ["NE", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Die mein Abgott ist und heist/", "tokens": ["Die", "mein", "Ab\u00b7gott", "ist", "und", "heist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VAFIN", "KON", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Macht/ da\u00df Liebe meine Jugend", "tokens": ["Macht", "/", "da\u00df", "Lie\u00b7be", "mei\u00b7ne", "Ju\u00b7gend"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$(", "KOUS", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "An die st\u00e4rcksten Ketten schleust.", "tokens": ["An", "die", "st\u00e4rcks\u00b7ten", "Ket\u00b7ten", "schleust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Keine hat mich k\u00f6nnen binden/", "tokens": ["Kei\u00b7ne", "hat", "mich", "k\u00f6n\u00b7nen", "bin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "VMFIN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Aber dieser Nymfen Zier", "tokens": ["A\u00b7ber", "die\u00b7ser", "Nym\u00b7fen", "Zier"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PDAT", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Wuste mich bald zu entz\u00fcnden/", "tokens": ["Wus\u00b7te", "mich", "bald", "zu", "ent\u00b7z\u00fcn\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKZU", "VVINF", "$("], "meter": "+-+++-+-", "measure": "unknown.measure.penta"}, "line.20": {"text": "Da sie doch nicht g\u00fcnstig mir.", "tokens": ["Da", "sie", "doch", "nicht", "g\u00fcns\u00b7tig", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKNEG", "ADJD", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "Jhr sittsamen Geberden", "tokens": ["Ihr", "sitt\u00b7sa\u00b7men", "Ge\u00b7ber\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.22": {"text": "Haben nicht genommen ein/", "tokens": ["Ha\u00b7ben", "nicht", "ge\u00b7nom\u00b7men", "ein", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "VVPP", "ART", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.23": {"text": "Da\u00df ich ihr verpflicht must werden/", "tokens": ["Da\u00df", "ich", "ihr", "ver\u00b7pflicht", "must", "wer\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "VMFIN", "VAINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Weil die Geister in mir seyn.", "tokens": ["Weil", "die", "Geis\u00b7ter", "in", "mir", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "PPER", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.25": {"text": "Noch verlacht die Anemone", "tokens": ["Noch", "ver\u00b7lacht", "die", "A\u00b7ne\u00b7mo\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Mich/ da\u00df ich verliebet bin/", "tokens": ["Mich", "/", "da\u00df", "ich", "ver\u00b7lie\u00b7bet", "bin", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "KOUS", "PPER", "VVFIN", "VAFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.27": {"text": "Da sie doch die Lebens-Krone", "tokens": ["Da", "sie", "doch", "die", "Le\u00b7bens\u00b7Kro\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Tag und Nacht mir ligt im Sinn.", "tokens": ["Tag", "und", "Nacht", "mir", "ligt", "im", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.29": {"text": "Was ich in Gedancken f\u00fchre", "tokens": ["Was", "ich", "in", "Ge\u00b7dan\u00b7cken", "f\u00fch\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "APPR", "NN", "VVFIN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.30": {"text": "Ist von ihrer Sch\u00f6nheit Pracht.", "tokens": ["Ist", "von", "ih\u00b7rer", "Sch\u00f6n\u00b7heit", "Pracht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.31": {"text": "Wenn ich fr\u00fch vom Schlaf mich r\u00fchre/", "tokens": ["Wenn", "ich", "fr\u00fch", "vom", "Schlaf", "mich", "r\u00fch\u00b7re", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "APPRART", "NN", "PPER", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Hab ich schon an sie gedacht.", "tokens": ["Hab", "ich", "schon", "an", "sie", "ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.33": {"text": "Soll ich denn nun Flammen leiden", "tokens": ["Soll", "ich", "denn", "nun", "Flam\u00b7men", "lei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Da sie nichts als kaltes Ei\u00df.", "tokens": ["Da", "sie", "nichts", "als", "kal\u00b7tes", "Ei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "KOKOM", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.35": {"text": "Blut das kan den Demant scheiden;", "tokens": ["Blut", "das", "kan", "den", "De\u00b7mant", "schei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PDS", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "Aber wenn mein Todes-Schweis/", "tokens": ["A\u00b7ber", "wenn", "mein", "To\u00b7des\u00b7Sch\u00b7weis", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "$("], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.37": {"text": "Gleich auff allen Gliedern s\u00e4sse/", "tokens": ["Gleich", "auff", "al\u00b7len", "Glie\u00b7dern", "s\u00e4s\u00b7se", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIAT", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.38": {"text": "Glaub ich doch nicht/ da\u00df ihr Sinn", "tokens": ["Glaub", "ich", "doch", "nicht", "/", "da\u00df", "ihr", "Sinn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPER", "ADV", "PTKNEG", "$(", "KOUS", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.39": {"text": "Lieb und Hulden mir zum\u00e4sse/", "tokens": ["Lieb", "und", "Hul\u00b7den", "mir", "zu\u00b7m\u00e4s\u00b7se", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PPER", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.40": {"text": "Sondern liesse mich dahin.", "tokens": ["Son\u00b7dern", "lies\u00b7se", "mich", "da\u00b7hin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.41": {"text": "Anemone Zeit bricht Eisen", "tokens": ["A\u00b7ne\u00b7mo\u00b7ne", "Zeit", "bricht", "Ei\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "NN", "VVFIN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.42": {"text": "Und zermalmt den Marmelstein.", "tokens": ["Und", "zer\u00b7malmt", "den", "Mar\u00b7mel\u00b7stein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.43": {"text": "Willst du dich stets so erweisen", "tokens": ["Willst", "du", "dich", "stets", "so", "er\u00b7wei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PRF", "ADV", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.44": {"text": "Und wie Stahl und Felsen seyn?", "tokens": ["Und", "wie", "Stahl", "und", "Fel\u00b7sen", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "NN", "KON", "NN", "VAINF", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.45": {"text": "Kan dich nicht mein Flehn erweichen", "tokens": ["Kan", "dich", "nicht", "mein", "Flehn", "er\u00b7wei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PTKNEG", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.46": {"text": "Das unendlich zu dir schreyt/", "tokens": ["Das", "un\u00b7end\u00b7lich", "zu", "dir", "schreyt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "APPR", "PPER", "VVFIN", "$("], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.47": {"text": "Ey so mu\u00df ich nur verbleichen", "tokens": ["Ey", "so", "mu\u00df", "ich", "nur", "ver\u00b7blei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.48": {"text": "In der besten Bl\u00fcthe-Zeit.", "tokens": ["In", "der", "bes\u00b7ten", "Bl\u00fcthe\u00b7Zeit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.49": {"text": "Doch betrachte da\u00df auf Erden/", "tokens": ["Doch", "be\u00b7trach\u00b7te", "da\u00df", "auf", "Er\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOUS", "APPR", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.50": {"text": "So wie ich dich treu geliebt/", "tokens": ["So", "wie", "ich", "dich", "treu", "ge\u00b7liebt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PPER", "PRF", "ADJD", "VVPP", "$("], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.51": {"text": "Du nicht kanst geliebet werden:", "tokens": ["Du", "nicht", "kanst", "ge\u00b7lie\u00b7bet", "wer\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "VMFIN", "VVPP", "VAINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.52": {"text": "Und ob mich das Gl\u00fcck betr\u00fcbt/", "tokens": ["Und", "ob", "mich", "das", "Gl\u00fcck", "be\u00b7tr\u00fcbt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ART", "NN", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.53": {"text": "Ey so soll best\u00e4ndig lieben", "tokens": ["Ey", "so", "soll", "be\u00b7st\u00e4n\u00b7dig", "lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "VMFIN", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.54": {"text": "Mit mir noch zu Grabe gehn/", "tokens": ["Mit", "mir", "noch", "zu", "Gra\u00b7be", "gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "APPR", "NN", "VVINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.55": {"text": "Und da\u00df ich umb dich geblieben/", "tokens": ["Und", "da\u00df", "ich", "umb", "dich", "ge\u00b7blie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "PPER", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.56": {"text": "Soll in allen B\u00fcchern stehn.", "tokens": ["Soll", "in", "al\u00b7len", "B\u00fc\u00b7chern", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}