{"textgrid.poem.46103": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[o Traum der W\u00fcste, Liebe, endlos Sehnen]", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Traum der W\u00fcste, Liebe, endlos Sehnen,", "tokens": ["O", "Traum", "der", "W\u00fcs\u00b7te", ",", "Lie\u00b7be", ",", "end\u00b7los", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "NN", "$,", "NN", "$,", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Blau \u00fcberspannt vom Zelte, Stern an Stern;", "tokens": ["Blau", "\u00fc\u00b7bers\u00b7pannt", "vom", "Zel\u00b7te", ",", "Stern", "an", "Stern", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "$,", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "O W\u00fcstenglut voll Tau, o Lieb' voll Tr\u00e4nen,", "tokens": ["O", "W\u00fcs\u00b7ten\u00b7glut", "voll", "Tau", ",", "o", "Lieb'", "voll", "Tr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADJD", "NN", "$,", "FM", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Weil sich unendlich Nahes ewig fern.", "tokens": ["Weil", "sich", "un\u00b7end\u00b7lich", "Na\u00b7hes", "e\u00b7wig", "fern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ADJD", "ADJA", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "O W\u00fcstentraum, wo Lieb' auf Herzschlag lauschet,", "tokens": ["O", "W\u00fcs\u00b7ten\u00b7traum", ",", "wo", "Lieb'", "auf", "Herz\u00b7schlag", "lau\u00b7schet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWAV", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn fl\u00fccht'gen Wildes Huf die W\u00fcste drischt,", "tokens": ["Wenn", "fl\u00fccht'\u00b7gen", "Wil\u00b7des", "Huf", "die", "W\u00fcs\u00b7te", "drischt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "O Traum, wo der Geliebten Schleier rauschet,", "tokens": ["O", "Traum", ",", "wo", "der", "Ge\u00b7lieb\u00b7ten", "Schlei\u00b7er", "rau\u00b7schet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWAV", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn Geierflug im Sandmeer Schlangen fischt.", "tokens": ["Wenn", "Gei\u00b7er\u00b7flug", "im", "Sand\u00b7meer", "Schlan\u00b7gen", "fischt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPRART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "O W\u00fcstentraum, wo Liebe tr\u00e4umt zu fassen", "tokens": ["O", "W\u00fcs\u00b7ten\u00b7traum", ",", "wo", "Lie\u00b7be", "tr\u00e4umt", "zu", "fas\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "PWAV", "NN", "VVPP", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Jetzt Josephs Mantelsaum mit durst'ger Hand,", "tokens": ["Jetzt", "Jo\u00b7se\u00b7phs", "Man\u00b7tel\u00b7saum", "mit", "dur\u00b7st'\u00b7ger", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-----+-+", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Da gei\u00dfelt wach, verh\u00f6hnt halb, ganz verlassen", "tokens": ["Da", "gei\u00b7\u00dfelt", "wach", ",", "ver\u00b7h\u00f6hnt", "halb", ",", "ganz", "ver\u00b7las\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PTKVZ", "$,", "VVFIN", "ADJD", "$,", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ihr Herz, der W\u00fcste Gei\u00dfel, gl\u00fcher Sand.", "tokens": ["Ihr", "Herz", ",", "der", "W\u00fcs\u00b7te", "Gei\u00b7\u00dfel", ",", "gl\u00fc\u00b7her", "Sand", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "O Liebe, W\u00fcstentraum der Sehnsuchtspalme,", "tokens": ["O", "Lie\u00b7be", ",", "W\u00fcs\u00b7ten\u00b7traum", "der", "Sehn\u00b7suchts\u00b7pal\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die bl\u00fctenlos Gezweig zum Himmel streckt,", "tokens": ["Die", "bl\u00fc\u00b7ten\u00b7los", "Ge\u00b7zweig", "zum", "Him\u00b7mel", "streckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bis segnend in des h\u00f6chsten Liedes Psalme", "tokens": ["Bis", "seg\u00b7nend", "in", "des", "h\u00f6chs\u00b7ten", "Lie\u00b7des", "Psal\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJD", "APPR", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Engel sie mit heil'gem Fruchtstaub weckt.", "tokens": ["Der", "En\u00b7gel", "sie", "mit", "heil'\u00b7gem", "Frucht\u00b7staub", "weckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "O W\u00fcste, Traum der Liebe, die verachtet", "tokens": ["O", "W\u00fcs\u00b7te", ",", "Traum", "der", "Lie\u00b7be", ",", "die", "ver\u00b7ach\u00b7tet"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "NN", "$,", "NN", "ART", "NN", "$,", "PRELS", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vom Haus versto\u00dfen mit der Hagar irrt,", "tokens": ["Vom", "Haus", "ver\u00b7sto\u00b7\u00dfen", "mit", "der", "Ha\u00b7gar", "irrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "APPR", "ART", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wo schl\u00e4ft der Quell? da Ismael verschmachtet,", "tokens": ["Wo", "schl\u00e4ft", "der", "Quell", "?", "da", "Is\u00b7mael", "ver\u00b7schmach\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "$.", "KOUS", "NE", "VVPP", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Bis deine Brust ihm eine Amme wird.", "tokens": ["Bis", "dei\u00b7ne", "Brust", "ihm", "ei\u00b7ne", "Am\u00b7me", "wird", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "O W\u00fcstentraum der Liebe, die sich sehnet,", "tokens": ["O", "W\u00fcs\u00b7ten\u00b7traum", "der", "Lie\u00b7be", ",", "die", "sich", "seh\u00b7net", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "NN", "$,", "PRELS", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Steigt nie ein Weiherauch aus dir empor?", "tokens": ["Steigt", "nie", "ein", "Wei\u00b7her\u00b7auch", "aus", "dir", "em\u00b7por", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Geht duftend, auf den Br\u00e4utigam gelehnet,", "tokens": ["Geht", "duf\u00b7tend", ",", "auf", "den", "Br\u00e4u\u00b7ti\u00b7gam", "ge\u00b7leh\u00b7net", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,", "APPR", "ART", "NE", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nie meine Seele heil aus dir hervor?", "tokens": ["Nie", "mei\u00b7ne", "See\u00b7le", "heil", "aus", "dir", "her\u00b7vor", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "ADJD", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "O W\u00fcste, wo das Wort der ew'gen Liebe", "tokens": ["O", "W\u00fcs\u00b7te", ",", "wo", "das", "Wort", "der", "ew'\u00b7gen", "Lie\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "PWAV", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im unversehrten Dorn vor Moses flammt,", "tokens": ["Im", "un\u00b7ver\u00b7sehr\u00b7ten", "Dorn", "vor", "Mo\u00b7ses", "flammt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPR", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein Zeugnis, da\u00df die Mutter Jungfrau bliebe,", "tokens": ["Ein", "Zeug\u00b7nis", ",", "da\u00df", "die", "Mut\u00b7ter", "Jung\u00b7frau", "blie\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Aus deren Scho\u00df der Sohn der Gottheit stammt.", "tokens": ["Aus", "de\u00b7ren", "Scho\u00df", "der", "Sohn", "der", "Got\u00b7theit", "stammt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Lieb', W\u00fcstentraum, so laut des Rufers Stimme,", "tokens": ["Lieb'", ",", "W\u00fcs\u00b7ten\u00b7traum", ",", "so", "laut", "des", "Ru\u00b7fers", "Stim\u00b7me", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "ADV", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "\u00bbbereit' den Weg des Herrn!\u00ab dir mahnend schallt,", "tokens": ["\u00bb", "be\u00b7reit'", "den", "Weg", "des", "Herrn", "!", "\u00ab", "dir", "mah\u00b7nend", "schallt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ART", "NN", "ART", "NN", "$.", "$(", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Summt in des L\u00f6wen Schlund dir doch die Imme,", "tokens": ["Summt", "in", "des", "L\u00f6\u00b7wen", "Schlund", "dir", "doch", "die", "Im\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.4": {"text": "Die S\u00fc\u00dfes baut im Rachen der Gewalt.", "tokens": ["Die", "S\u00fc\u00b7\u00dfes", "baut", "im", "Ra\u00b7chen", "der", "Ge\u00b7walt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "O Durst der Liebe, W\u00fcstentraum, wann spaltet", "tokens": ["O", "Durst", "der", "Lie\u00b7be", ",", "W\u00fcs\u00b7ten\u00b7traum", ",", "wann", "spal\u00b7tet"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["NE", "NN", "ART", "NN", "$,", "NN", "$,", "PWAV", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Herr den Fels, da\u00df Wasser gibt der Stein,", "tokens": ["Der", "Herr", "den", "Fels", ",", "da\u00df", "Was\u00b7ser", "gibt", "der", "Stein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "KOUS", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wann deckt in dir den Tisch, der g\u00fctig waltet,", "tokens": ["Wann", "deckt", "in", "dir", "den", "Tisch", ",", "der", "g\u00fc\u00b7tig", "wal\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "APPR", "PPER", "ART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wann sammle ich das Himmelbrot mir ein?", "tokens": ["Wann", "samm\u00b7le", "ich", "das", "Him\u00b7mel\u00b7brot", "mir", "ein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Durst, Liebe, W\u00fcstentraum, dort scheint am H\u00fcgel", "tokens": ["Durst", ",", "Lie\u00b7be", ",", "W\u00fcs\u00b7ten\u00b7traum", ",", "dort", "scheint", "am", "H\u00fc\u00b7gel"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "NN", "$,", "NN", "$,", "ADV", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Morgenstrahl, ein Hirtenfeuer wei\u00df,", "tokens": ["Der", "Mor\u00b7gen\u00b7strahl", ",", "ein", "Hir\u00b7ten\u00b7feu\u00b7er", "wei\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wo Durst gew\u00e4hnt des Wasserfalles Spiegel", "tokens": ["Wo", "Durst", "ge\u00b7w\u00e4hnt", "des", "Was\u00b7ser\u00b7fal\u00b7les", "Spie\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VVPP", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Fand Liebe ein Geschiebe Fraueneis.", "tokens": ["Fand", "Lie\u00b7be", "ein", "Ge\u00b7schie\u00b7be", "Frau\u00b7e\u00b7neis", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "O Liebe, W\u00fcstentraum des Heimatkranken,", "tokens": ["O", "Lie\u00b7be", ",", "W\u00fcs\u00b7ten\u00b7traum", "des", "Hei\u00b7mat\u00b7kran\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihr Paradiese, schimmernd in der Luft,", "tokens": ["Ihr", "Pa\u00b7ra\u00b7die\u00b7se", ",", "schim\u00b7mernd", "in", "der", "Luft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ihr Sehnsuchtsstr\u00f6me, die durch Wiesen ranken,", "tokens": ["Ihr", "Sehn\u00b7suchts\u00b7str\u00f6\u00b7me", ",", "die", "durch", "Wie\u00b7sen", "ran\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ihr Palmenhaine, lockend in dem Duft.", "tokens": ["Ihr", "Pal\u00b7men\u00b7hai\u00b7ne", ",", "lo\u00b7ckend", "in", "dem", "Duft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "O Liebe, W\u00fcstentraumquell, beim Erwachen", "tokens": ["O", "Lie\u00b7be", ",", "W\u00fcs\u00b7ten\u00b7traum\u00b7quell", ",", "beim", "Er\u00b7wa\u00b7chen"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["NE", "NN", "$,", "NN", "$,", "APPRART", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Rauscht dir kein Quell, es wirbelt gl\u00fcher Sand,", "tokens": ["Rauscht", "dir", "kein", "Quell", ",", "es", "wir\u00b7belt", "gl\u00fc\u00b7her", "Sand", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", 