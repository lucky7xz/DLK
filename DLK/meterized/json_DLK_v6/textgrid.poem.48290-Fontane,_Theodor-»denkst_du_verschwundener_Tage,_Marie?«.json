{"textgrid.poem.48290": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "\u00bbdenkst du verschwundener Tage, Marie?\u00ab", "genre": "verse", "period": "N.A.", "pub_year": 1858, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbdenkst du verschwundener Tage, Marie,", "tokens": ["\u00bb", "denkst", "du", "ver\u00b7schwun\u00b7de\u00b7ner", "Ta\u00b7ge", ",", "Ma\u00b7rie", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "ADJA", "NN", "$,", "NE", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Wenn du starrst ins Feuer bei Nacht?", "tokens": ["Wenn", "du", "starrst", "ins", "Feu\u00b7er", "bei", "Nacht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPRART", "NN", "APPR", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "W\u00fcnschst du die hellen Tage zur\u00fcck,", "tokens": ["W\u00fcnschst", "du", "die", "hel\u00b7len", "Ta\u00b7ge", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Wo du selbst wie die Sonne gelacht?\u00ab", "tokens": ["Wo", "du", "selbst", "wie", "die", "Son\u00b7ne", "ge\u00b7lacht", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "PPER", "ADV", "KOKOM", "ART", "NN", "VVPP", "$.", "$("], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}}, "stanza.2": {"line.1": {"text": "\u00bbich denk' der verschwundenen Tage, Johann,", "tokens": ["\u00bb", "ich", "denk'", "der", "ver\u00b7schwun\u00b7de\u00b7nen", "Ta\u00b7ge", ",", "Jo\u00b7hann", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,", "NE", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und denk' an all ihr Gl\u00fcck,", "tokens": ["Und", "denk'", "an", "all", "ihr", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch der sonnigste Tag, der \u00fcber mich kam,", "tokens": ["Doch", "der", "son\u00b7nigs\u00b7te", "Tag", ",", "der", "\u00fc\u00b7ber", "mich", "kam", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$,"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Ich w\u00fcnsch' ihn nicht zur\u00fcck.\u00ab", "tokens": ["Ich", "w\u00fcn\u00b7sch'", "ihn", "nicht", "zu\u00b7r\u00fcck", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$.", "$("], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.3": {"line.1": {"text": "\u00bbdenkst du an gestorbenes Hoffen, Marie,", "tokens": ["\u00bb", "denkst", "du", "an", "ge\u00b7stor\u00b7be\u00b7nes", "Hof\u00b7fen", ",", "Ma\u00b7rie", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,", "NE", "$,"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Wenn du starrst ins Feuer bei Nacht?", "tokens": ["Wenn", "du", "starrst", "ins", "Feu\u00b7er", "bei", "Nacht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPRART", "NN", "APPR", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Der Tau, der auf dein Hoffen fiel,", "tokens": ["Der", "Tau", ",", "der", "auf", "dein", "Hof\u00b7fen", "fiel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hat dich um die Ernte gebracht.\u00ab", "tokens": ["Hat", "dich", "um", "die", "Ern\u00b7te", "ge\u00b7bracht", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ART", "NN", "VVPP", "$.", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "\u00bbich denk' an gestorbenes Hoffen, Johann,", "tokens": ["\u00bb", "ich", "denk'", "an", "ge\u00b7stor\u00b7be\u00b7nes", "Hof\u00b7fen", ",", "Jo\u00b7hann", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,", "NE", "$,"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Aber tu's in stillem Sinn,", "tokens": ["A\u00b7ber", "tu's", "in", "stil\u00b7lem", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Es starb, wie eine Rose stirbt, \u2013", "tokens": ["Es", "starb", ",", "wie", "ei\u00b7ne", "Ro\u00b7se", "stirbt", ",", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ART", "NN", "VVFIN", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und was hin ist, ist hin.\u00ab", "tokens": ["Und", "was", "hin", "ist", ",", "ist", "hin", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["KON", "PWS", "ADV", "VAFIN", "$,", "VAFIN", "ADV", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "\u00bbdenkst du gestorbener Freunde, Marie,", "tokens": ["\u00bb", "denkst", "du", "ge\u00b7stor\u00b7be\u00b7ner", "Freun\u00b7de", ",", "Ma\u00b7rie", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "ADJA", "NN", "$,", "NE", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Wenn du starrst ins Feuer bei Nacht?", "tokens": ["Wenn", "du", "starrst", "ins", "Feu\u00b7er", "bei", "Nacht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPRART", "NN", "APPR", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "W\u00fcnschst du sie zur\u00fcck an den einsamen Herd,", "tokens": ["W\u00fcnschst", "du", "sie", "zu\u00b7r\u00fcck", "an", "den", "ein\u00b7sa\u00b7men", "Herd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PTKVZ", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Den sie einst dir so heimisch gemacht?\u00ab", "tokens": ["Den", "sie", "einst", "dir", "so", "hei\u00b7misch", "ge\u00b7macht", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "PPER", "ADV", "PPER", "ADV", "ADJD", "VVPP", "$.", "$("], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "\u00bbich denk' der gestorbenen Freunde, Johann,", "tokens": ["\u00bb", "ich", "denk'", "der", "ge\u00b7stor\u00b7be\u00b7nen", "Freun\u00b7de", ",", "Jo\u00b7hann", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,", "NE", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Sie sind allezeit mein Gl\u00fcck,", "tokens": ["Sie", "sind", "al\u00b7le\u00b7zeit", "mein", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch, die mir die liebsten gewesen sind,", "tokens": ["Doch", ",", "die", "mir", "die", "liebs\u00b7ten", "ge\u00b7we\u00b7sen", "sind", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PRELS", "PPER", "ART", "ADJA", "VAPP", "VAFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Ich w\u00fcnsche sie nicht zur\u00fcck.\u00ab", "tokens": ["Ich", "w\u00fcn\u00b7sche", "sie", "nicht", "zu\u00b7r\u00fcck", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}