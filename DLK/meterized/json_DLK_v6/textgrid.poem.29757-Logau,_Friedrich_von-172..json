{"textgrid.poem.29757": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "172.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wann mich Gott f\u00fcr Schanden dort und f\u00fcr Schanden hier bewahrt,", "tokens": ["Wann", "mich", "Gott", "f\u00fcr", "Schan\u00b7den", "dort", "und", "f\u00fcr", "Schan\u00b7den", "hier", "be\u00b7wahrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "APPR", "NN", "ADV", "KON", "APPR", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Wann er an mir Seelen-Brot, wann er Mund-Brot nur nicht spart,", "tokens": ["Wann", "er", "an", "mir", "See\u00b7len\u00b7Brot", ",", "wann", "er", "Mun\u00b7dBrot", "nur", "nicht", "spart", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "PPER", "NN", "$,", "PWAV", "PPER", "NN", "ADV", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Geht mein Gl\u00fccke, wie ich wil, in der allerbesten Fahrt.", "tokens": ["Geht", "mein", "Gl\u00fc\u00b7cke", ",", "wie", "ich", "wil", ",", "in", "der", "al\u00b7ler\u00b7bes\u00b7ten", "Fahrt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "PWAV", "PPER", "VMFIN", "$,", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}}}}}