{"textgrid.poem.60541": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gewaltig tobt der Wind und beugt", "genre": "verse", "period": "N.A.", "pub_year": 1840, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gewaltig tobt der Wind und beugt", "tokens": ["Ge\u00b7wal\u00b7tig", "tobt", "der", "Wind", "und", "beugt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Wildbusch, sausend in der Schlucht,", "tokens": ["Den", "Wild\u00b7busch", ",", "sau\u00b7send", "in", "der", "Schlucht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Bach beschleunigt seine Flucht,", "tokens": ["Der", "Bach", "be\u00b7schleu\u00b7nigt", "sei\u00b7ne", "Flucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von Regenwolken gro\u00dfges\u00e4ugt.", "tokens": ["Von", "Re\u00b7gen\u00b7wol\u00b7ken", "gro\u00df\u00b7ge\u00b7s\u00e4ugt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Nach S\u00fcden eilt hinab der Bach,", "tokens": ["Nach", "S\u00fc\u00b7den", "eilt", "hin\u00b7ab", "der", "Bach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nach Norden spritzt ihn das Geschnaub,", "tokens": ["Nach", "Nor\u00b7den", "spritzt", "ihn", "das", "Ge\u00b7schnaub", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und unst\u00e4t irrt das d\u00fcrre Laub", "tokens": ["Und", "un\u00b7st\u00e4t", "irrt", "das", "d\u00fcr\u00b7re", "Laub"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem \u00bbWasser und dem Winde nach.", "tokens": ["Dem", "\u00bb", "Was\u00b7ser", "und", "dem", "Win\u00b7de", "nach", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "NN", "KON", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Nun gilt des Herbstes Sterbgebot,", "tokens": ["Nun", "gilt", "des", "Herbs\u00b7tes", "Sterb\u00b7ge\u00b7bot", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch ungl\u00fcckselig ist das Tal,", "tokens": ["Doch", "un\u00b7gl\u00fcck\u00b7se\u00b7lig", "ist", "das", "Tal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df hin der holde Sommerstrahl,", "tokens": ["Da\u00df", "hin", "der", "hol\u00b7de", "Som\u00b7mer\u00b7strahl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und alles grollt und schm\u00e4ht den Tod.", "tokens": ["Und", "al\u00b7les", "grollt", "und", "schm\u00e4ht", "den", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADJD", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Mit schwerem Kampf das Leben bricht,", "tokens": ["Mit", "schwe\u00b7rem", "Kampf", "das", "Le\u00b7ben", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Baum, der Busch, so todesmatt,", "tokens": ["Der", "Baum", ",", "der", "Busch", ",", "so", "to\u00b7des\u00b7matt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "H\u00e4lt seufzend fest am letzten Blatt;", "tokens": ["H\u00e4lt", "seuf\u00b7zend", "fest", "am", "letz\u00b7ten", "Blatt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie gut der Tod, sie glaubens nicht.", "tokens": ["Wie", "gut", "der", "Tod", ",", "sie", "glau\u00b7bens", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "$,", "PPER", "ADV", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}