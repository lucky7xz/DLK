{"textgrid.poem.63375": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zwar es ist schon Mittagszeit,", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zwar es ist schon Mittagszeit,", "tokens": ["Zwar", "es", "ist", "schon", "Mit\u00b7tags\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "ADV", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sonne steht schon hell am Himmel \u2013", "tokens": ["Son\u00b7ne", "steht", "schon", "hell", "am", "Him\u00b7mel", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ADJD", "APPRART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In den Stra\u00dfen: welch Gewimmel,", "tokens": ["In", "den", "Stra\u00b7\u00dfen", ":", "welch", "Ge\u00b7wim\u00b7mel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$.", "PWAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In den Herzen: welches Leid \u2013", "tokens": ["In", "den", "Her\u00b7zen", ":", "wel\u00b7ches", "Leid", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$.", "PWAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Manches Segel bauscht der Wind,", "tokens": ["Man\u00b7ches", "Se\u00b7gel", "bauscht", "der", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Mancher Kutter bleibt im Hafen \u2013", "tokens": ["Man\u00b7cher", "Kut\u00b7ter", "bleibt", "im", "Ha\u00b7fen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "APPRART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Du sollst schlafen,", "tokens": ["Du", "sollst", "schla\u00b7fen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$,"], "meter": "-+--", "measure": "dactylic.init"}, "line.8": {"text": "Du sollst schlafen,", "tokens": ["Du", "sollst", "schla\u00b7fen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$,"], "meter": "-+--", "measure": "dactylic.init"}, "line.9": {"text": "Du sollst schlafen, liebes Kind.", "tokens": ["Du", "sollst", "schla\u00b7fen", ",", "lie\u00b7bes", "Kind", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$,", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Siebzigmal littst du Haitang,", "tokens": ["Sieb\u00b7zig\u00b7mal", "littst", "du", "Hai\u00b7tang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "F\u00fcnfzigmal starbst du Johanna \u2013", "tokens": ["F\u00fcnf\u00b7zig\u00b7mal", "starbst", "du", "Jo\u00b7han\u00b7na", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NE", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Schmecktest S\u00fc\u00dfigkeit und Manna,", "tokens": ["Schmeck\u00b7test", "S\u00fc\u00b7\u00dfig\u00b7keit", "und", "Man\u00b7na", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn der Quell der Qualen sprang.", "tokens": ["Wenn", "der", "Quell", "der", "Qua\u00b7len", "sprang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "S\u00fc\u00dfes, junges Blut \u2013 es rinnt \u2013", "tokens": ["S\u00fc\u00b7\u00dfes", ",", "jun\u00b7ges", "Blut", "\u2013", "es", "rinnt", "\u2013"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$(", "PPER", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "K\u00fcsse, Dolche flammten, trafen \u2013", "tokens": ["K\u00fcs\u00b7se", ",", "Dol\u00b7che", "flamm\u00b7ten", ",", "tra\u00b7fen", "\u2013"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "VVINF", "$,", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Du sollst schlafen,", "tokens": ["Du", "sollst", "schla\u00b7fen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$,"], "meter": "-+--", "measure": "dactylic.init"}, "line.8": {"text": "Du sollst schlafen,", "tokens": ["Du", "sollst", "schla\u00b7fen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$,"], "meter": "-+--", "measure": "dactylic.init"}, "line.9": {"text": "Du sollst schlafen, liebes Kind.", "tokens": ["Du", "sollst", "schla\u00b7fen", ",", "lie\u00b7bes", "Kind", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$,", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Einmal endet sich das Spiel,", "tokens": ["Ein\u00b7mal", "en\u00b7det", "sich", "das", "Spiel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Einmal endet sich das Grausen,", "tokens": ["Ein\u00b7mal", "en\u00b7det", "sich", "das", "Grau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die Ewigkeit wird k\u00fchl", "tokens": ["Und", "die", "E\u00b7wig\u00b7keit", "wird", "k\u00fchl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VAFIN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dir um Brust und Schl\u00e4fen sausen.", "tokens": ["Dir", "um", "Brust", "und", "Schl\u00e4\u00b7fen", "sau\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sand deckt dich wie Wolle lind,", "tokens": ["Sand", "deckt", "dich", "wie", "Wol\u00b7le", "lind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "KOKOM", "NE", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und der Hirte bl\u00e4st den Schafen \u2013", "tokens": ["Und", "der", "Hir\u00b7te", "bl\u00e4st", "den", "Scha\u00b7fen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN", "$("], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.7": {"text": "Du sollst schlafen,", "tokens": ["Du", "sollst", "schla\u00b7fen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$,"], "meter": "-+--", "measure": "dactylic.init"}, "line.8": {"text": "Du sollst schlafen,", "tokens": ["Du", "sollst", "schla\u00b7fen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$,"], "meter": "-+--", "measure": "dactylic.init"}, "line.9": {"text": "Du sollst schlafen, liebes Kind.", "tokens": ["Du", "sollst", "schla\u00b7fen", ",", "lie\u00b7bes", "Kind", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$,", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}