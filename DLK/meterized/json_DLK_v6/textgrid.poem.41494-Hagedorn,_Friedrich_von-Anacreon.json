{"textgrid.poem.41494": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Anacreon", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In Tejos und in Samos", "tokens": ["In", "Te\u00b7jos", "und", "in", "Sa\u00b7mos"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "KON", "APPR", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und in der Stadt Minervens", "tokens": ["Und", "in", "der", "Stadt", "Mi\u00b7ner\u00b7vens"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NE"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Sang ich von Wein und Liebe,", "tokens": ["Sang", "ich", "von", "Wein", "und", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Von Rosen und vom Fr\u00fchling,", "tokens": ["Von", "Ro\u00b7sen", "und", "vom", "Fr\u00fch\u00b7ling", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Von Freundschaft und von T\u00e4nzen;", "tokens": ["Von", "Freund\u00b7schaft", "und", "von", "T\u00e4n\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Doch h\u00f6hnt' ich nicht die G\u00f6tter,", "tokens": ["Doch", "h\u00f6hnt'", "ich", "nicht", "die", "G\u00f6t\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Auch nicht der G\u00f6tter Diener,", "tokens": ["Auch", "nicht", "der", "G\u00f6t\u00b7ter", "Die\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Auch nicht der G\u00f6tter Tempel,", "tokens": ["Auch", "nicht", "der", "G\u00f6t\u00b7ter", "Tem\u00b7pel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Wie hie\u00df ich sonst der Weise?", "tokens": ["Wie", "hie\u00df", "ich", "sonst", "der", "Wei\u00b7se", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ihr Dichter voller Jugend,", "tokens": ["Ihr", "Dich\u00b7ter", "vol\u00b7ler", "Ju\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wollt ihr bei froher Mu\u00dfe", "tokens": ["Wollt", "ihr", "bei", "fro\u00b7her", "Mu\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Anacreontisch singen:", "tokens": ["A\u00b7na\u00b7cre\u00b7on\u00b7tisch", "sin\u00b7gen", ":"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So singt von milden Reben,", "tokens": ["So", "singt", "von", "mil\u00b7den", "Re\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Von rosenreichen Hecken,", "tokens": ["Von", "ro\u00b7sen\u00b7rei\u00b7chen", "He\u00b7cken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Vom Fr\u00fchling und von T\u00e4nzen,", "tokens": ["Vom", "Fr\u00fch\u00b7ling", "und", "von", "T\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Von Freundschaft und von Liebe;", "tokens": ["Von", "Freund\u00b7schaft", "und", "von", "Lie\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Doch h\u00f6hnet nicht die Gottheit,", "tokens": ["Doch", "h\u00f6h\u00b7net", "nicht", "die", "Got\u00b7theit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-++-+", "measure": "unknown.measure.tetra"}, "line.9": {"text": "Auch nicht der Gottheit Diener,", "tokens": ["Auch", "nicht", "der", "Got\u00b7theit", "Die\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Auch nicht der Gottheit Tempel.", "tokens": ["Auch", "nicht", "der", "Got\u00b7theit", "Tem\u00b7pel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Verdienet, selbst im Scherzen,", "tokens": ["Ver\u00b7die\u00b7net", ",", "selbst", "im", "Scher\u00b7zen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Den Namen \u00e4chter Weisen.", "tokens": ["Den", "Na\u00b7men", "\u00e4ch\u00b7ter", "Wei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}