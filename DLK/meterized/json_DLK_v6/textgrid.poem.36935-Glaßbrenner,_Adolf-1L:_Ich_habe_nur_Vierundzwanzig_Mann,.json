{"textgrid.poem.36935": {"metadata": {"author": {"name": "Gla\u00dfbrenner, Adolf", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich habe nur Vierundzwanzig Mann,", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich habe nur Vierundzwanzig Mann,", "tokens": ["Ich", "ha\u00b7be", "nur", "Vie\u00b7rund\u00b7zwan\u00b7zig", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "CARD", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Kleine schwarze Husaren,", "tokens": ["Klei\u00b7ne", "schwar\u00b7ze", "Hu\u00b7sa\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Und dennoch trotzen sie der Gewalt", "tokens": ["Und", "den\u00b7noch", "trot\u00b7zen", "sie", "der", "Ge\u00b7walt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Mit allen ihren Schaaren.", "tokens": ["Mit", "al\u00b7len", "ih\u00b7ren", "Schaa\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Aus ihren Augen blitzt Geist und Kraft,", "tokens": ["Aus", "ih\u00b7ren", "Au\u00b7gen", "blitzt", "Geist", "und", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Sie k\u00e4mpfen mit heil'ger Leidenschaft,", "tokens": ["Sie", "k\u00e4mp\u00b7fen", "mit", "heil'\u00b7ger", "Lei\u00b7den\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Und nimmer f\u00fcr Orden, nimmer f\u00fcr Geld:", "tokens": ["Und", "nim\u00b7mer", "f\u00fcr", "Or\u00b7den", ",", "nim\u00b7mer", "f\u00fcr", "Geld", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "$,", "ADV", "APPR", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "F\u00fcr die Menschen allein und ihre Welt.", "tokens": ["F\u00fcr", "die", "Men\u00b7schen", "al\u00b7lein", "und", "ih\u00b7re", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "KON", "PPOSAT", "NN", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Ich habe nur Vierundzwanzig Mann,", "tokens": ["Ich", "ha\u00b7be", "nur", "Vie\u00b7rund\u00b7zwan\u00b7zig", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "CARD", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Nicht mehr als Vierundzwanzig!", "tokens": ["Nicht", "mehr", "als", "Vie\u00b7rund\u00b7zwan\u00b7zig", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PIAT", "KOKOM", "CARD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und doch sind sie hier, und sind zugleich", "tokens": ["Und", "doch", "sind", "sie", "hier", ",", "und", "sind", "zu\u00b7gleich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADV", "$,", "KON", "VAFIN", "ADV"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "In Boston, Wien und Danzig,", "tokens": ["In", "Bos\u00b7ton", ",", "Wi\u00b7en", "und", "Dan\u00b7zig", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "NE", "KON", "NE", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "In Constantinopel, in Petersburg;", "tokens": ["In", "Con\u00b7stan\u00b7ti\u00b7no\u00b7pel", ",", "in", "Pe\u00b7ters\u00b7burg", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "APPR", "NE", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Durch alle Festungen hau'n sie sich durch;", "tokens": ["Durch", "al\u00b7le", "Fes\u00b7tun\u00b7gen", "hau'n", "sie", "sich", "durch", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PPER", "PRF", "APPR", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Bald sind sie so ernst, bald sind sie so flott,", "tokens": ["Bald", "sind", "sie", "so", "ernst", ",", "bald", "sind", "sie", "so", "flott", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJD", "$,", "ADV", "VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Und immer hei\u00dft es: mit Gott, mit Gott!", "tokens": ["Und", "im\u00b7mer", "hei\u00dft", "es", ":", "mit", "Gott", ",", "mit", "Gott", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$.", "APPR", "NN", "$,", "APPR", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Ich habe nur Vierundzwanzig Mann,", "tokens": ["Ich", "ha\u00b7be", "nur", "Vie\u00b7rund\u00b7zwan\u00b7zig", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "CARD", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Doch sind's gar kecke Reiter!", "tokens": ["Doch", "sin\u00b7d's", "gar", "ke\u00b7cke", "Rei\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Doch sind es f\u00fcr Freiheit, Sch\u00f6nheit und Recht", "tokens": ["Doch", "sind", "es", "f\u00fcr", "Frei\u00b7heit", ",", "Sch\u00f6n\u00b7heit", "und", "Recht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "APPR", "NN", "$,", "NN", "KON", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Brave, unsterbliche Streiter!", "tokens": ["Bra\u00b7ve", ",", "uns\u00b7terb\u00b7li\u00b7che", "Strei\u00b7ter", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.5": {"text": "Und wo sie kommen mit Spiel und mit Sang,", "tokens": ["Und", "wo", "sie", "kom\u00b7men", "mit", "Spiel", "und", "mit", "Sang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Da bringt man den Kriegern Thr\u00e4nen zu Dank,", "tokens": ["Da", "bringt", "man", "den", "Krie\u00b7gern", "Thr\u00e4\u00b7nen", "zu", "Dank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ART", "NN", "NN", "APPR", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Denn Thr\u00e4nen der Freud' und der Wehmuth allein,", "tokens": ["Denn", "Thr\u00e4\u00b7nen", "der", "Freud'", "und", "der", "Weh\u00b7muth", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "NN", "KON", "ART", "NN", "ADV", "$,"], "meter": "-+--+--++-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Das ist ihr Manna, das ist ihr Wein.", "tokens": ["Das", "ist", "ihr", "Man\u00b7na", ",", "das", "ist", "ihr", "Wein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "$,", "PDS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Und bin ich gleich A, ihr General,", "tokens": ["Und", "bin", "ich", "gleich", "A", ",", "ihr", "Ge\u00b7ne\u00b7ral", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "NE", "$,", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Bin nicht mehr als sie, nicht minder;", "tokens": ["Bin", "nicht", "mehr", "als", "sie", ",", "nicht", "min\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ADV", "KOUS", "PPER", "$,", "PTKNEG", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bin reicher, vornehmer, stolzer nicht,", "tokens": ["Bin", "rei\u00b7cher", ",", "vor\u00b7neh\u00b7mer", ",", "stol\u00b7zer", "nicht", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "ADJA", "$,", "ADJA", "PTKNEG", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Nicht muthiger, nicht geschwinder.", "tokens": ["Nicht", "mut\u00b7hi\u00b7ger", ",", "nicht", "ge\u00b7schwin\u00b7der", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "$,", "PTKNEG", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wir sind keine Sclaven \u2013 die stie\u00dfen wir aus,", "tokens": ["Wir", "sind", "kei\u00b7ne", "Scla\u00b7ven", "\u2013", "die", "stie\u00b7\u00dfen", "wir", "aus", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "$(", "ART", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Die leben beim Feinde in Saus und Braus,", "tokens": ["Die", "le\u00b7ben", "beim", "Fein\u00b7de", "in", "Saus", "und", "Braus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPRART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Und ziehen sie gegen uns selber zur Schlacht,", "tokens": ["Und", "zie\u00b7hen", "sie", "ge\u00b7gen", "uns", "sel\u00b7ber", "zur", "Schlacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.8": {"text": "Die S\u00f6ldner! sie werden verh\u00f6hnt und verlacht!", "tokens": ["Die", "S\u00f6ld\u00b7ner", "!", "sie", "wer\u00b7den", "ver\u00b7h\u00f6hnt", "und", "ver\u00b7lacht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "PPER", "VAFIN", "VVPP", "KON", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.5": {"line.1": {"text": "Ja, F\u00fcnfundzwanzig Mann sind wir nur,", "tokens": ["Ja", ",", "F\u00fcn\u00b7fund\u00b7zwan\u00b7zig", "Mann", "sind", "wir", "nur", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "CARD", "NN", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Doch voll der edelsten Triebe;", "tokens": ["Doch", "voll", "der", "e\u00b7dels\u00b7ten", "Trie\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Der Himmel, das ist unser Vaterland,", "tokens": ["Der", "Him\u00b7mel", ",", "das", "ist", "un\u00b7ser", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PDS", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Freiheit unsere Liebe!", "tokens": ["Die", "Frei\u00b7heit", "un\u00b7se\u00b7re", "Lie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "So ziehn wir umher in der weiten Welt,", "tokens": ["So", "ziehn", "wir", "um\u00b7her", "in", "der", "wei\u00b7ten", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Und wo ein Bruder getroffen auch f\u00e4llt \u2013", "tokens": ["Und", "wo", "ein", "Bru\u00b7der", "ge\u00b7trof\u00b7fen", "auch", "f\u00e4llt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VVPP", "ADV", "VVFIN", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Flugs reitet er weiter, dem Feinde zum Spott,", "tokens": ["Flugs", "rei\u00b7tet", "er", "wei\u00b7ter", ",", "dem", "Fein\u00b7de", "zum", "Spott", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.8": {"text": "Und singet und k\u00e4mpfet mit Gott, mit Gott!", "tokens": ["Und", "sin\u00b7get", "und", "k\u00e4mp\u00b7fet", "mit", "Gott", ",", "mit", "Gott", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "APPR", "NN", "$,", "APPR", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}}}}