{"dta.poem.519": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Auf der  Amarianen Contrefait.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Dis Bildni\u00df gleichet dir Amariane sehr/", "tokens": ["Dis", "Bild\u00b7ni\u00df", "glei\u00b7chet", "dir", "A\u00b7ma\u00b7ri\u00b7a\u00b7ne", "sehr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VVFIN", "PPER", "NE", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn es giebt gleich wie du nicht meiner Klag Geh\u00f6r/", "tokens": ["Denn", "es", "giebt", "gleich", "wie", "du", "nicht", "mei\u00b7ner", "Klag", "Ge\u00b7h\u00f6r", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "KOKOM", "PPER", "PTKNEG", "PPOSAT", "NN", "NN", "$("], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Es bleibet unbewegt bey meinen Liebes-Schmertzen/", "tokens": ["Es", "blei\u00b7bet", "un\u00b7be\u00b7wegt", "bey", "mei\u00b7nen", "Lie\u00b7bes\u00b7Schmert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und will gleich als wie du mit meinen Flammen schertzen.", "tokens": ["Und", "will", "gleich", "als", "wie", "du", "mit", "mei\u00b7nen", "Flam\u00b7men", "schert\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "KOUS", "PWAV", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}