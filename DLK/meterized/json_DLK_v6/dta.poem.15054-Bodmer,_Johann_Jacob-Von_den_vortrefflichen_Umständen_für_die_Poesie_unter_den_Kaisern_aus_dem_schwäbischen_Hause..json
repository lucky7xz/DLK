{"dta.poem.15054": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Von den vortrefflichen Umst\u00e4nden  \n f\u00fcr die Poesie unter den Kaisern aus  \n dem schw\u00e4bischen Hause.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198476", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gut das ist Gitikeit ein Klobe", "tokens": ["Gut", "das", "ist", "Gi\u00b7ti\u00b7keit", "ein", "Klo\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "PDS", "VAFIN", "NN", "ART", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Dem es ist lieber denne Got u. weltlich ere, ich", "tokens": ["Dem", "es", "ist", "lie\u00b7ber", "den\u00b7ne", "Got", "u.", "welt\u00b7lich", "e\u00b7re", ",", "ich"], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation", "word", "word", "punct", "word"], "pos": ["PDS", "PPER", "VAFIN", "ADV", "ART", "NN", "APPR", "PWAT", "ADJA", "$,", "PPER"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "wen er tobe.", "tokens": ["wen", "er", "to\u00b7be", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}