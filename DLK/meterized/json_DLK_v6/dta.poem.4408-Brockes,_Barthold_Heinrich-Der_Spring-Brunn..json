{"dta.poem.4408": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Spring-Brunn.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1727", "urn": "urn:nbn:de:kobv:b4-200905198599", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Grosser GOtt! aus Dessen Willen", "tokens": ["Gros\u00b7ser", "Gott", "!", "aus", "Des\u00b7sen", "Wil\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Alle Meer\u2019, als B\u00e4chlein, qvillen,", "tokens": ["Al\u00b7le", "Meer'", ",", "als", "B\u00e4ch\u00b7lein", ",", "qvil\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PIAT", "NN", "$,", "KOUS", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und durch Dessen Wort allein", "tokens": ["Und", "durch", "Des\u00b7sen", "Wort", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PDAT", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich die unergr\u00fcnd\u2019ten Gr\u00fcnde,", "tokens": ["Sich", "die", "un\u00b7er\u00b7gr\u00fcn\u00b7d'\u00b7ten", "Gr\u00fcn\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Aller Tiefen dunk\u2019le Schl\u00fcnde,", "tokens": ["Al\u00b7ler", "Tie\u00b7fen", "dunk'\u00b7le", "Schl\u00fcn\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mit dem Schwall der Wasser f\u00fcllen;", "tokens": ["Mit", "dem", "Schwall", "der", "Was\u00b7ser", "f\u00fcl\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Dieses kleine Wasser-Spiel", "tokens": ["Die\u00b7ses", "klei\u00b7ne", "Was\u00b7ser\u00b7Spiel"], "token_info": ["word", "word", "word"], "pos": ["PDAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Zeigt mir viel.", "tokens": ["Zeigt", "mir", "viel", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "Du nur hast der weichen Flut,", "tokens": ["Du", "nur", "hast", "der", "wei\u00b7chen", "Flut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Deiner Creatur zu gut,", "tokens": ["Dei\u00b7ner", "Crea\u00b7tur", "zu", "gut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKA", "ADJD", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.11": {"text": "Diese Wunder-Eigenschaft,", "tokens": ["Die\u00b7se", "Wun\u00b7der\u00b7Ei\u00b7gen\u00b7schaft", ","], "token_info": ["word", "word", "punct"], "pos": ["PDAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Da\u00df sie fl\u00fcssig ist, gegeben,", "tokens": ["Da\u00df", "sie", "fl\u00fcs\u00b7sig", "ist", ",", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN", "$,", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Und durch eig\u2019ner Schwere Kraft", "tokens": ["Und", "durch", "eig'\u00b7ner", "Schwe\u00b7re", "Kraft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.14": {"text": "Auch geschickt ist, sich zu heben,", "tokens": ["Auch", "ge\u00b7schickt", "ist", ",", "sich", "zu", "he\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VAFIN", "$,", "PRF", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Um dann durch ihr strenges Senken", "tokens": ["Um", "dann", "durch", "ihr", "stren\u00b7ges", "Sen\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "ADV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "F\u00fcglicher die Welt zu tr\u00e4nken:", "tokens": ["F\u00fcg\u00b7li\u00b7cher", "die", "Welt", "zu", "tr\u00e4n\u00b7ken", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Wie wir auf der Berge H\u00f6h\u2019n", "tokens": ["Wie", "wir", "auf", "der", "Ber\u00b7ge", "H\u00f6h'n"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "Lauter Wasser-K\u00fcnste sehn.", "tokens": ["Lau\u00b7ter", "Was\u00b7ser\u00b7K\u00fcns\u00b7te", "sehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Alle Wasser-F\u00e4ll\u2019 und Meere", "tokens": ["Al\u00b7le", "Was\u00b7ser\u00b7F\u00e4ll'", "und", "Mee\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Spielen, HErr, zu deiner Ehre.", "tokens": ["Spie\u00b7len", ",", "Herr", ",", "zu", "dei\u00b7ner", "Eh\u00b7re", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}