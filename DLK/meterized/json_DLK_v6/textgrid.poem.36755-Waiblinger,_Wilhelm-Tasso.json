{"textgrid.poem.36755": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Tasso", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du wirst bleiben, so lange Musik und melodischer Wohllaut", "tokens": ["Du", "wirst", "blei\u00b7ben", ",", "so", "lan\u00b7ge", "Mu\u00b7sik", "und", "me\u00b7lo\u00b7di\u00b7scher", "Wohl\u00b7laut"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVINF", "$,", "ADV", "ADJA", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+--++", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Dein entz\u00fcckendes Welsch noch sich zur Wiege bestimmt,", "tokens": ["Dein", "ent\u00b7z\u00fc\u00b7cken\u00b7des", "Welsch", "noch", "sich", "zur", "Wie\u00b7ge", "be\u00b7stimmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADV", "PRF", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Und so lange die Lieb' in z\u00e4rtlichem Feuer die Sprache", "tokens": ["Und", "so", "lan\u00b7ge", "die", "Lieb'", "in", "z\u00e4rt\u00b7li\u00b7chem", "Feu\u00b7er", "die", "Spra\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "ART", "NN", "APPR", "ADJA", "NN", "ART", "NN"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Der Musik, und des Reichs lieblicher T\u00f6ne sich w\u00e4hlt.", "tokens": ["Der", "Mu\u00b7sik", ",", "und", "des", "Reichs", "lieb\u00b7li\u00b7cher", "T\u00f6\u00b7ne", "sich", "w\u00e4hlt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "ART", "NN", "ADJA", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-++--+--+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Aber Homer, er gef\u00e4llt mir schon nicht im Virgil, wie gefiele", "tokens": ["A\u00b7ber", "Ho\u00b7mer", ",", "er", "ge\u00b7f\u00e4llt", "mir", "schon", "nicht", "im", "Vir\u00b7gil", ",", "wie", "ge\u00b7fie\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "NE", "$,", "PPER", "VVFIN", "PPER", "ADV", "PTKNEG", "APPRART", "NE", "$,", "PWAV", "VVFIN"], "meter": "+-+-+-+--+-+-+-+-", "measure": "trochaic.octa.plus.relaxed"}, "line.6": {"text": "Darum in deinem Gedicht, Tasso, mir gar nun Virgil?", "tokens": ["Da\u00b7rum", "in", "dei\u00b7nem", "Ge\u00b7dicht", ",", "Tas\u00b7so", ",", "mir", "gar", "nun", "Vir\u00b7gil", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "PPOSAT", "NN", "$,", "NE", "$,", "PPER", "ADV", "ADV", "NE", "$."], "meter": "-+-+---+-+-+-+", "measure": "unknown.measure.hexa"}}}}}