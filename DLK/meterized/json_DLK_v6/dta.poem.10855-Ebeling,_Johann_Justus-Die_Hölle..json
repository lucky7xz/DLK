{"dta.poem.10855": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Die H\u00f6lle.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198774", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich wage mich auch an die Schwellen,", "tokens": ["Ich", "wa\u00b7ge", "mich", "auch", "an", "die", "Schwel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des finstern Marterreichs der H\u00f6llen,", "tokens": ["Des", "fins\u00b7tern", "Mar\u00b7ter\u00b7reichs", "der", "H\u00f6l\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und werde einen Pful gewahr", "tokens": ["Und", "wer\u00b7de", "ei\u00b7nen", "Pful", "ge\u00b7wahr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Worin gleichsam ein Schweffel stin-", "tokens": ["Wo\u00b7rin", "gleich\u00b7sam", "ein", "Schwef\u00b7fel", "stin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "ART", "NN", "TRUNC"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "O! weh dem, der dahin versinket", "tokens": ["O", "!", "weh", "dem", ",", "der", "da\u00b7hin", "ver\u00b7sin\u00b7ket"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "$.", "ADV", "ART", "$,", "PRELS", "PAV", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Zur ungl\u00fcklich verdammten Schaar.", "tokens": ["Zur", "un\u00b7gl\u00fck\u00b7lich", "ver\u00b7damm\u00b7ten", "Schaar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Mein Geist erschrikt schon an der Pforte,", "tokens": ["Mein", "Geist", "er\u00b7schrikt", "schon", "an", "der", "Pfor\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vor diesem tief verdekten Orte,", "tokens": ["Vor", "die\u00b7sem", "tief", "ver\u00b7dek\u00b7ten", "Or\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wo weder Licht noch Sonne scheint,", "tokens": ["Wo", "we\u00b7der", "Licht", "noch", "Son\u00b7ne", "scheint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "KON", "NN", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein grauser Nebel schwarzer Schatten,", "tokens": ["Ein", "grau\u00b7ser", "Ne\u00b7bel", "schwar\u00b7zer", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wobei sich Furcht und Elend gatten,", "tokens": ["Wo\u00b7bei", "sich", "Furcht", "und", "E\u00b7lend", "gat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ist hier mit Finsterni\u00df vereint.", "tokens": ["Ist", "hier", "mit", "Fins\u00b7ter\u00b7ni\u00df", "ver\u00b7eint", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ein Abgrund wo die Bosheit zittert,", "tokens": ["Ein", "Ab\u00b7grund", "wo", "die", "Bos\u00b7heit", "zit\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und die Verzweiflung rast und wittert,", "tokens": ["Und", "die", "Ver\u00b7zwei\u00b7flung", "rast", "und", "wit\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "In f\u00fcrchterlicher Dunkelheit,", "tokens": ["In", "f\u00fcrch\u00b7ter\u00b7li\u00b7cher", "Dun\u00b7kel\u00b7heit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "L\u00e4st mir zuerst die Teufel sehen,", "tokens": ["L\u00e4st", "mir", "zu\u00b7erst", "die", "Teu\u00b7fel", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die hier empfinden alle Wehen,", "tokens": ["Die", "hier", "emp\u00b7fin\u00b7den", "al\u00b7le", "We\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Grenzenlosen Ewigkeit.", "tokens": ["Der", "Gren\u00b7zen\u00b7lo\u00b7sen", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Was vor ein Dampf und geistisch Bla-\nsen?", "tokens": ["Was", "vor", "ein", "Dampf", "und", "geis\u00b7tisch", "Bla", "sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "KON", "ADJD", "TRUNC", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Entsteht aus greulich bangen Rasen", "tokens": ["Ent\u00b7steht", "aus", "greu\u00b7lich", "ban\u00b7gen", "Ra\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Geister, die von ", "tokens": ["Der", "Geis\u00b7ter", ",", "die", "von"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR"], "meter": "-+-++", "measure": "unknown.measure.tri"}, "line.4": {"text": "O! weh! was vor ein knirschend Zagen,", "tokens": ["O", "!", "weh", "!", "was", "vor", "ein", "knir\u00b7schend", "Za\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PTKVZ", "$.", "PWS", "APPR", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Mir deucht, das sind die H\u00f6llen Plagen,", "tokens": ["Mir", "deucht", ",", "das", "sind", "die", "H\u00f6l\u00b7len", "Pla\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PDS", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Worinn das Heer der Teufel brennt.", "tokens": ["Wo\u00b7rinn", "das", "Heer", "der", "Teu\u00b7fel", "brennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Was schw\u00e4rmet dort in tr\u00fcben Win-\nkel?", "tokens": ["Was", "schw\u00e4r\u00b7met", "dort", "in", "tr\u00fc\u00b7ben", "Win", "kel", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "APPR", "ADJA", "TRUNC", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Betriegt mich nicht ein falscher D\u00fcnkel;", "tokens": ["Be\u00b7triegt", "mich", "nicht", "ein", "fal\u00b7scher", "D\u00fcn\u00b7kel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So ist es der verdammten Zahl,", "tokens": ["So", "ist", "es", "der", "ver\u00b7damm\u00b7ten", "Zahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die nach der Art der finstren Eulen,", "tokens": ["Die", "nach", "der", "Art", "der", "finst\u00b7ren", "Eu\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Erb\u00e4rmlich winseln, klagen, heulen,", "tokens": ["Er\u00b7b\u00e4rm\u00b7lich", "win\u00b7seln", ",", "kla\u00b7gen", ",", "heu\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJD", "VVINF", "$,", "VVFIN", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ob der empfundnen Seelen Qual.", "tokens": ["Ob", "der", "emp\u00b7fund\u00b7nen", "See\u00b7len", "Qual", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Hie ist ein w\u00fctend grauser Lermen,", "tokens": ["Hie", "ist", "ein", "w\u00fc\u00b7tend", "grau\u00b7ser", "Ler\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und wer erregt dies bange Schwermen?", "tokens": ["Und", "wer", "er\u00b7regt", "dies", "ban\u00b7ge", "Schwer\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "PDS", "ADV", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mir deucht, ich h\u00f6r es aus den Thon,", "tokens": ["Mir", "deucht", ",", "ich", "h\u00f6r", "es", "aus", "den", "Thon", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es sind die Sp\u00f6tter, die da schm\u00e4hlen,", "tokens": ["Es", "sind", "die", "Sp\u00f6t\u00b7ter", ",", "die", "da", "schm\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und sich, wie sie verlacht, erz\u00e4hlen", "tokens": ["Und", "sich", ",", "wie", "sie", "ver\u00b7lacht", ",", "er\u00b7z\u00e4h\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KON", "PRF", "$,", "PWAV", "PPER", "VVPP", "$,", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Was kommt dort vor ein wilder Haufen,", "tokens": ["Was", "kommt", "dort", "vor", "ein", "wil\u00b7der", "Hau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Verwirrt und winselnd hergelauffen?", "tokens": ["Ver\u00b7wirrt", "und", "win\u00b7selnd", "her\u00b7ge\u00b7lauf\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der gleich den wilden Thieren sch\u00e4umt?", "tokens": ["Der", "gleich", "den", "wil\u00b7den", "Thie\u00b7ren", "sch\u00e4umt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich seh, es sind die falschen Christen,", "tokens": ["Ich", "seh", ",", "es", "sind", "die", "fal\u00b7schen", "Chris\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die bei der Herrschaft eitler L\u00fcsten,", "tokens": ["Die", "bei", "der", "Herr\u00b7schaft", "eit\u00b7ler", "L\u00fcs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Stets voller Hofnung nur getr\u00e4umt.", "tokens": ["Stets", "vol\u00b7ler", "Hof\u00b7nung", "nur", "ge\u00b7tr\u00e4umt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Da ist die Rotte die verdammet,", "tokens": ["Da", "ist", "die", "Rot\u00b7te", "die", "ver\u00b7dam\u00b7met", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Weil sie in wilden Feur geflammet,", "tokens": ["Weil", "sie", "in", "wil\u00b7den", "Feur", "ge\u00b7flam\u00b7met", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Kreuzzigung der Lust geha\u00dft;", "tokens": ["Die", "Kreuz\u00b7zi\u00b7gung", "der", "Lust", "ge\u00b7ha\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Geilheit die vor Wildheit lodert,", "tokens": ["Die", "Geil\u00b7heit", "die", "vor", "Wild\u00b7heit", "lo\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Hat die die nasse Tropfen fordert,", "tokens": ["Hat", "die", "die", "nas\u00b7se", "Trop\u00b7fen", "for\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Trunkenheit noch angefa\u00dft.", "tokens": ["Die", "Trun\u00b7ken\u00b7heit", "noch", "an\u00b7ge\u00b7fa\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Sie fluchen den geno\u00dfnen Freuden,", "tokens": ["Sie", "flu\u00b7chen", "den", "ge\u00b7no\u00df\u00b7nen", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und w\u00fcnschen sich dennoch zu weiden,", "tokens": ["Und", "w\u00fcn\u00b7schen", "sich", "den\u00b7noch", "zu", "wei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "In dem verdammten S\u00fcndenwust;", "tokens": ["In", "dem", "ver\u00b7damm\u00b7ten", "S\u00fcn\u00b7den\u00b7wust", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein sonst vergn\u00fcgtes Angedenken,", "tokens": ["Ein", "sonst", "ver\u00b7gn\u00fcg\u00b7tes", "An\u00b7ge\u00b7den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Mu\u00df sie mit bittren Nachschmak kr\u00e4nken.", "tokens": ["Mu\u00df", "sie", "mit", "bit\u00b7tren", "Nach\u00b7schmak", "kr\u00e4n\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wie gallicht schmekt nun ihre Lust?", "tokens": ["Wie", "gal\u00b7licht", "schmekt", "nun", "ih\u00b7re", "Lust", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Hie kommt der Stolz vom Schmerz ge-\nb\u00fckket,", "tokens": ["Hie", "kommt", "der", "Stolz", "vom", "Schmerz", "ge", "b\u00fck\u00b7ket", ","], "token_info": ["word", "