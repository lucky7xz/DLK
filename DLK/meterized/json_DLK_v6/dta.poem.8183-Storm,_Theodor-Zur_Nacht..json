{"dta.poem.8183": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Zur Nacht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1852", "urn": "urn:nbn:de:kobv:b4-200905196098", "language": ["de:0.99"], "booktitle": "Storm, Theodor: Gedichte. Kiel, 1852."}, "poem": {"stanza.1": {"line.1": {"text": "Genie\u00dfen dieser Stunde vollen Frieden!", "tokens": ["Ge\u00b7nie\u00b7\u00dfen", "die\u00b7ser", "Stun\u00b7de", "vol\u00b7len", "Frie\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PDAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nun sind wir unser; von der frechen Welt", "tokens": ["Nun", "sind", "wir", "un\u00b7ser", ";", "von", "der", "fre\u00b7chen", "Welt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "$.", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Hat endlich uns die heilige Nacht geschieden.", "tokens": ["Hat", "end\u00b7lich", "uns", "die", "hei\u00b7li\u00b7ge", "Nacht", "ge\u00b7schie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPER", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "La\u00df einmal noch, eh' sich dein Auge schlie\u00dft,", "tokens": ["La\u00df", "ein\u00b7mal", "noch", ",", "eh'", "sich", "dein", "Au\u00b7ge", "schlie\u00dft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ADV", "$,", "KOUS", "PRF", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Liebe Strahl sich r\u00fcckhaltlos entz\u00fcnden;", "tokens": ["Der", "Lie\u00b7be", "Strahl", "sich", "r\u00fcck\u00b7halt\u00b7los", "ent\u00b7z\u00fcn\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PRF", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Noch einmal, eh' im Traum' sie sich vergi\u00dft,", "tokens": ["Noch", "ein\u00b7mal", ",", "eh'", "im", "Traum'", "sie", "sich", "ver\u00b7gi\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUS", "APPRART", "NN", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mich deiner Stimme lieben Laut empfinden!", "tokens": ["Mich", "dei\u00b7ner", "Stim\u00b7me", "lie\u00b7ben", "Laut", "emp\u00b7fin\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "VVFIN", "APPR", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Was giebt es mehr? der stille Knabe winkt", "tokens": ["Was", "giebt", "es", "mehr", "?", "der", "stil\u00b7le", "Kna\u00b7be", "winkt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "$.", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Zu seinem Strande lockender und lieber;", "tokens": ["Zu", "sei\u00b7nem", "Stran\u00b7de", "lo\u00b7cken\u00b7der", "und", "lie\u00b7ber", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "KON", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und wie die Brust dir athmend schwellt und sinkt,", "tokens": ["Und", "wie", "die", "Brust", "dir", "ath\u00b7mend", "schwellt", "und", "sinkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "PPER", "ADJD", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Tr\u00e4gt uns des Schlummers Welle sanft hin\u00fcber.", "tokens": ["Tr\u00e4gt", "uns", "des", "Schlum\u00b7mers", "Wel\u00b7le", "sanft", "hin\u00b7\u00fc\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "ADJD", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}