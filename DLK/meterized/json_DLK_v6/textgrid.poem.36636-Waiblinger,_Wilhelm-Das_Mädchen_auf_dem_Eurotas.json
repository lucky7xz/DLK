{"textgrid.poem.36636": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Das M\u00e4dchen auf dem Eurotas", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schwankend auf der Spiegelwelle", "tokens": ["Schwan\u00b7kend", "auf", "der", "Spie\u00b7gel\u00b7wel\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tanzt der leichtbewegte Kahn:", "tokens": ["Tanzt", "der", "leicht\u00b7be\u00b7weg\u00b7te", "Kahn", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie so freundlich, klar und helle!", "tokens": ["Wie", "so", "freund\u00b7lich", ",", "klar", "und", "hel\u00b7le", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "$,", "ADJD", "KON", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bald hinunter, bald hinan!", "tokens": ["Bald", "hin\u00b7un\u00b7ter", ",", "bald", "hi\u00b7nan", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie die alte liebe Sonne", "tokens": ["Wie", "die", "al\u00b7te", "lie\u00b7be", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nieder aus dem Aether quillt,", "tokens": ["Nie\u00b7der", "aus", "dem", "A\u00b7e\u00b7ther", "quillt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Und mit junger Lebenswonne", "tokens": ["Und", "mit", "jun\u00b7ger", "Le\u00b7bens\u00b7won\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Den erw\u00e4rmten Busen f\u00fcllt!", "tokens": ["Den", "er\u00b7w\u00e4rm\u00b7ten", "Bu\u00b7sen", "f\u00fcllt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und die L\u00e4mmer, wie sie klettern", "tokens": ["Und", "die", "L\u00e4m\u00b7mer", ",", "wie", "sie", "klet\u00b7tern"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "PWAV", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Um die vollen H\u00fcgel hin!", "tokens": ["Um", "die", "vol\u00b7len", "H\u00fc\u00b7gel", "hin", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Unter gr\u00fcnen Lorbeerbl\u00e4ttern", "tokens": ["Un\u00b7ter", "gr\u00fc\u00b7nen", "Lor\u00b7beer\u00b7bl\u00e4t\u00b7tern"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zarte wei\u00dfe Schw\u00e4ne ziehn!", "tokens": ["Zar\u00b7te", "wei\u00b7\u00dfe", "Schw\u00e4\u00b7ne", "ziehn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und der Berge Duftgestalten", "tokens": ["Und", "der", "Ber\u00b7ge", "Duft\u00b7ge\u00b7stal\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie mit weichem Liebes-Ku\u00df", "tokens": ["Wie", "mit", "wei\u00b7chem", "Lie\u00b7bes\u00b7Ku\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "In der Ferne sich entfalten", "tokens": ["In", "der", "Fer\u00b7ne", "sich", "ent\u00b7fal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PRF", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ueber'm blauen K\u00f6nigsflu\u00df!", "tokens": ["Ue\u00b7ber'm", "blau\u00b7en", "K\u00f6\u00b7nigs\u00b7flu\u00df", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ach! was ist's, das aus dem Laube", "tokens": ["Ach", "!", "was", "ist's", ",", "das", "aus", "dem", "Lau\u00b7be"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "PWS", "VAFIN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sanft und mild her\u00fcberschwirrt,", "tokens": ["Sanft", "und", "mild", "her\u00b7\u00fc\u00b7bersc\u00b7hwirrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Z\u00e4rtlich, wie die Turteltaube,", "tokens": ["Z\u00e4rt\u00b7lich", ",", "wie", "die", "Tur\u00b7tel\u00b7tau\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die aus gr\u00fcner Myrrthe girrt?", "tokens": ["Die", "aus", "gr\u00fc\u00b7ner", "Myrrt\u00b7he", "girrt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Aus der N\u00e4he, aus der Ferne,", "tokens": ["Aus", "der", "N\u00e4\u00b7he", ",", "aus", "der", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Aus dem Schatten, aus dem Licht,", "tokens": ["Aus", "dem", "Schat\u00b7ten", ",", "aus", "dem", "Licht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.7": {"text": "Wie die Bilder blasser Sterne,", "tokens": ["Wie", "die", "Bil\u00b7der", "blas\u00b7ser", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Zum verwandten Herzen spricht?", "tokens": ["Zum", "ver\u00b7wand\u00b7ten", "Her\u00b7zen", "spricht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ach! die Arme kann's nicht sagen,", "tokens": ["Ach", "!", "die", "Ar\u00b7me", "kann's", "nicht", "sa\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ART", "NN", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn's auch tief im Busen wallt,", "tokens": ["Wenn's", "auch", "tief", "im", "Bu\u00b7sen", "wallt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADJD", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und mit leisen Liebesklagen", "tokens": ["Und", "mit", "lei\u00b7sen", "Lie\u00b7bes\u00b7kla\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weinend durch die Seele hallt,", "tokens": ["Wei\u00b7nend", "durch", "die", "See\u00b7le", "hallt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie des Windes stilles F\u00e4cheln,", "tokens": ["Wie", "des", "Win\u00b7des", "stil\u00b7les", "F\u00e4\u00b7cheln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Der um Zweig und Blatt sich regt,", "tokens": ["Der", "um", "Zweig", "und", "Blatt", "sich", "regt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "KON", "NN", "PRF", "VVFIN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.7": {"text": "Blumen, die im Thale l\u00e4cheln,", "tokens": ["Blu\u00b7men", ",", "die", "im", "Tha\u00b7le", "l\u00e4\u00b7cheln", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Gr\u00e4schen auf der Au' bewegt.", "tokens": ["Gr\u00e4sc\u00b7hen", "auf", "der", "Au'", "be\u00b7wegt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Nenn' ich es ein heilig Gl\u00fchen,", "tokens": ["Nenn'", "ich", "es", "ein", "hei\u00b7lig", "Gl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ART", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das an Wesen Wesen zwingt,", "tokens": ["Das", "an", "We\u00b7sen", "We\u00b7sen", "zwingt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und den Keim zum heitern Bl\u00fchen,", "tokens": ["Und", "den", "Keim", "zum", "hei\u00b7tern", "Bl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und das Kind zur Mutter bringt?", "tokens": ["Und", "das", "Kind", "zur", "Mut\u00b7ter", "bringt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ist's der Himmelslaut der Liebe,", "tokens": ["Ist's", "der", "Him\u00b7mels\u00b7laut", "der", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Der das Innerste durchklingt,", "tokens": ["Der", "das", "In\u00b7ners\u00b7te", "durch\u00b7klingt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und mit namenlosem Triebe", "tokens": ["Und", "mit", "na\u00b7men\u00b7lo\u00b7sem", "Trie\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Herz an Herz zusammenschlingt?", "tokens": ["Herz", "an", "Herz", "zu\u00b7sam\u00b7men\u00b7schlingt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Was ich ahnte, was ich f\u00fchlte,", "tokens": ["Was", "ich", "ahn\u00b7te", ",", "was", "ich", "f\u00fchl\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Noch als kaum entquoll'nes Kind,", "tokens": ["Noch", "als", "kaum", "ent\u00b7quoll'\u00b7nes", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was mir meine Wangen k\u00fchlte,", "tokens": ["Was", "mir", "mei\u00b7ne", "Wan\u00b7gen", "k\u00fchl\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach so oft und doch so lind!", "tokens": ["Ach", "so", "oft", "und", "doch", "so", "lind", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "ADV", "KON", "ADV", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Was mir zart, wie Mondlicht, webend", "tokens": ["Was", "mir", "zart", ",", "wie", "Mond\u00b7licht", ",", "we\u00b7bend"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PWS", "PPER", "ADJD", "$,", "PWAV", "NN", "$,", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Oft ins nasse Auge kam,", "tokens": ["Oft", "ins", "nas\u00b7se", "Au\u00b7ge", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und wie Lindenbl\u00fcthe, bebend", "tokens": ["Und", "wie", "Lin\u00b7den\u00b7bl\u00fct\u00b7he", ",", "be\u00b7bend"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["KON", "PWAV", "NE", "$,", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Durch den off'nen Busen schwamm,", "tokens": ["Durch", "den", "off'\u00b7nen", "Bu\u00b7sen", "schwamm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Naht es nun mit leisem Wogen,", "tokens": ["Naht", "es", "nun", "mit", "lei\u00b7sem", "Wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stillt es nun mein weinend Herz?", "tokens": ["Stillt", "es", "nun", "mein", "wei\u00b7nend", "Herz", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach! ich ward so oft betrogen,", "tokens": ["Ach", "!", "ich", "ward", "so", "oft", "be\u00b7tro\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPER", "VAFIN", "ADV", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und er ist so tief, mein Schmerz!", "tokens": ["Und", "er", "ist", "so", "tief", ",", "mein", "Schmerz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "ADJD", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und so gl\u00fchend ist mein Sehnen!", "tokens": ["Und", "so", "gl\u00fc\u00b7hend", "ist", "mein", "Seh\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ahn' ich, ahn' ich deine Spur?", "tokens": ["Ahn'", "ich", ",", "ahn'", "ich", "dei\u00b7ne", "Spur", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ist mein Hoffen, ist's kein W\u00e4hnen,", "tokens": ["Ist", "mein", "Hof\u00b7fen", ",", "ist's", "kein", "W\u00e4h\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$,", "VAFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ew'ge heilige Natur?", "tokens": ["Ew'\u00b7ge", "hei\u00b7li\u00b7ge", "Na\u00b7tur", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Diese Ruhe, diese Stille!", "tokens": ["Die\u00b7se", "Ru\u00b7he", ",", "die\u00b7se", "Stil\u00b7le", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PDAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ja du bist es! Welche Lust!", "tokens": ["Ja", "du", "bist", "es", "!", "Wel\u00b7che", "Lust", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "VAFIN", "PPER", "$.", "PWAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Welche zarte Liebesf\u00fclle", "tokens": ["Wel\u00b7che", "zar\u00b7te", "Lie\u00b7bes\u00b7f\u00fcl\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["PWAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "An der warmen Mutterbrust!", "tokens": ["An", "der", "war\u00b7men", "Mut\u00b7ter\u00b7brust", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie ein ausgehaucht Verlangen,", "tokens": ["Wie", "ein", "aus\u00b7ge\u00b7haucht", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "VVPP", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Liegt vor mir die volle Flur!", "tokens": ["Liegt", "vor", "mir", "die", "vol\u00b7le", "Flur", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Da\u00df ich k\u00f6nnte dich umfangen,", "tokens": ["Da\u00df", "ich", "k\u00f6nn\u00b7te", "dich", "um\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ew'ge, heilige Natur!", "tokens": ["Ew'\u00b7ge", ",", "hei\u00b7li\u00b7ge", "Na\u00b7tur", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}