{"textgrid.poem.25319": {"metadata": {"author": {"name": "Stadler, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die morsche Harfe blitzt auf seinen Knien", "genre": "verse", "period": "N.A.", "pub_year": 1898, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die morsche Harfe blitzt auf seinen Knien", "tokens": ["Die", "mor\u00b7sche", "Har\u00b7fe", "blitzt", "auf", "sei\u00b7nen", "Kni\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "die blassen H\u00e4nde l\u00f6sen von der Saiten", "tokens": ["die", "blas\u00b7sen", "H\u00e4n\u00b7de", "l\u00f6\u00b7sen", "von", "der", "Sai\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "vergl\u00fchtem Golde welke Melodien", "tokens": ["ver\u00b7gl\u00fch\u00b7tem", "Gol\u00b7de", "wel\u00b7ke", "Me\u00b7lo\u00b7dien"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "PWAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "die fremd und schwer wie Perlenketten gleiten", "tokens": ["die", "fremd", "und", "schwer", "wie", "Per\u00b7len\u00b7ket\u00b7ten", "glei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "KON", "ADJD", "KOKOM", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "indes sein Blick traumvoll und halb erhellt", "tokens": ["in\u00b7des", "sein", "Blick", "traum\u00b7voll", "und", "halb", "er\u00b7hellt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "ADJD", "KON", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "durch aufgeworfner Decken Samtgeh\u00e4nge", "tokens": ["durch", "auf\u00b7ge\u00b7worf\u00b7ner", "De\u00b7cken", "Samt\u00b7ge\u00b7h\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "hintaumelt \u00fcber mondberonnen Feld:", "tokens": ["hin\u00b7tau\u00b7melt", "\u00fc\u00b7ber", "mond\u00b7be\u00b7ron\u00b7nen", "Feld", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df er sich mit den zarten Wolken schw\u00e4nge", "tokens": ["Da\u00df", "er", "sich", "mit", "den", "zar\u00b7ten", "Wol\u00b7ken", "schw\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "die lind die Nacht zu goldnen Inseln tr\u00e4gt", "tokens": ["die", "lind", "die", "Nacht", "zu", "gold\u00b7nen", "In\u00b7seln", "tr\u00e4gt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "verzaubert glitte auf begl\u00e4nzten Fl\u00fcgeln", "tokens": ["ver\u00b7zau\u00b7bert", "glit\u00b7te", "auf", "be\u00b7gl\u00e4nz\u00b7ten", "Fl\u00fc\u00b7geln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "zum Meer das fern an wei\u00dfe K\u00fcsten schl\u00e4gt", "tokens": ["zum", "Meer", "das", "fern", "an", "wei\u00b7\u00dfe", "K\u00fcs\u00b7ten", "schl\u00e4gt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "ADJD", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "und s\u00fc\u00dfem Strom und blassen Rebenh\u00fcgeln.", "tokens": ["und", "s\u00fc\u00b7\u00dfem", "Strom", "und", "blas\u00b7sen", "Re\u00b7ben\u00b7h\u00fc\u00b7geln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}