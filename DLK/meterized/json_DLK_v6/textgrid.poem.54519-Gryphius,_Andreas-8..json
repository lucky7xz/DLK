{"textgrid.poem.54519": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "8.", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Geist grosser Geist der sich in Feur-getheilten Zungen", "tokens": ["Geist", "gros\u00b7ser", "Geist", "der", "sich", "in", "Feur\u00b7get\u00b7heil\u00b7ten", "Zun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADJA", "NN", "ART", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit Segen-reicher Macht der h\u00f6chsten Wissenschafft", "tokens": ["Mit", "Se\u00b7gen\u00b7rei\u00b7cher", "Macht", "der", "h\u00f6chs\u00b7ten", "Wis\u00b7sen\u00b7schafft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und Felsen-fester St\u00e4rck' und unerschreckter Krafft", "tokens": ["Und", "Fel\u00b7sen\u00b7fes\u00b7ter", "St\u00e4rck", "und", "un\u00b7er\u00b7schreck\u00b7ter", "Krafft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Auf die vor mehr denn bl\u00f6d' itzt kluge Schaar geschwungen/", "tokens": ["Auf", "die", "vor", "mehr", "denn", "bl\u00f6d'", "itzt", "klu\u00b7ge", "Schaar", "ge\u00b7schwun\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "APPR", "ADV", "ADV", "VVFIN", "ADV", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Erwecke doch mein Hertz/ wenn ich/ in Angst gedrungen/", "tokens": ["Er\u00b7we\u00b7cke", "doch", "mein", "Hertz", "/", "wenn", "ich", "/", "in", "Angst", "ge\u00b7drun\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "$(", "KOUS", "PPER", "$(", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Zag ohne festen Trost/ wenn nun kein Rath mehr hafft/", "tokens": ["Zag", "oh\u00b7ne", "fes\u00b7ten", "Trost", "/", "wenn", "nun", "kein", "Rath", "mehr", "hafft", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$(", "KOUS", "ADV", "PIAT", "NN", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und mich der S\u00fcnden Grimm gleich einer Blum' hinrafft/", "tokens": ["Und", "mich", "der", "S\u00fcn\u00b7den", "Grimm", "gleich", "ei\u00b7ner", "Blum'", "hin\u00b7rafft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "NE", "ADV", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wenn mich der Erden Trotz und H\u00f6llen-Angst besprungen.", "tokens": ["Wenn", "mich", "der", "Er\u00b7den", "Trotz", "und", "H\u00f6l\u00b7len\u00b7Angst", "be\u00b7sprun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Was wil mein bl\u00f6der Mund? wil er sich unterwinden", "tokens": ["Was", "wil", "mein", "bl\u00f6\u00b7der", "Mund", "?", "wil", "er", "sich", "un\u00b7ter\u00b7win\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "PPOSAT", "ADJA", "NN", "$.", "VMFIN", "PPER", "PRF", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Den L\u00e4ndern vorzustehn/ der F\u00fcrsten Hand zu binden?", "tokens": ["Den", "L\u00e4n\u00b7dern", "vor\u00b7zu\u00b7stehn", "/", "der", "F\u00fcrs\u00b7ten", "Hand", "zu", "bin\u00b7den", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$(", "ART", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Nein Herr/ die Lipp' erstarrt und zittert und erstirbt.", "tokens": ["Nein", "Herr", "/", "die", "Lipp'", "er\u00b7starrt", "und", "zit\u00b7tert", "und", "er\u00b7stirbt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "NN", "$(", "ART", "NN", "VVPP", "KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ach rede du durch mich/ gib Muth frey auszusprechen/", "tokens": ["Ach", "re\u00b7de", "du", "durch", "mich", "/", "gib", "Muth", "frey", "aus\u00b7zu\u00b7spre\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PPER", "APPR", "PPER", "$(", "VVIMP", "NN", "ADJD", "VVIZU", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wenn mir Bedeckens-Zeit und Wort und Zung gebrechen/", "tokens": ["Wenn", "mir", "Be\u00b7de\u00b7cken\u00b7sZeit", "und", "Wort", "und", "Zung", "ge\u00b7bre\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "KON", "NN", "KON", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Da\u00df man den Nachdruck schau wenn Gott durch Menschen wirbt.", "tokens": ["Da\u00df", "man", "den", "Nach\u00b7druck", "schau", "wenn", "Gott", "durch", "Men\u00b7schen", "wirbt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "ADJD", "KOUS", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}