{"dta.poem.7594": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "9.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es kann dir freilich nicht auf dieser Welt gefallen,", "tokens": ["Es", "kann", "dir", "frei\u00b7lich", "nicht", "auf", "die\u00b7ser", "Welt", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "PTKNEG", "APPR", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da deine Seele wohnt in sch\u00f6nern Himmelshallen.", "tokens": ["Da", "dei\u00b7ne", "See\u00b7le", "wohnt", "in", "sch\u00f6\u00b7nern", "Him\u00b7mels\u00b7hal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der Abstand ist zu weit, die Kluft wird niemals voll,", "tokens": ["Der", "Ab\u00b7stand", "ist", "zu", "weit", ",", "die", "Kluft", "wird", "nie\u00b7mals", "voll", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKA", "ADJD", "$,", "ART", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die aufg\u00e4hnt zwischen dem was ist, und werden soll.", "tokens": ["Die", "auf\u00b7g\u00e4hnt", "zwi\u00b7schen", "dem", "was", "ist", ",", "und", "wer\u00b7den", "soll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ART", "PWS", "VAFIN", "$,", "KON", "VAINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die Worte die du h\u00f6rst, die Mienen die du siehst,", "tokens": ["Die", "Wor\u00b7te", "die", "du", "h\u00f6rst", ",", "die", "Mie\u00b7nen", "die", "du", "siehst", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "PPER", "VVFIN", "$,", "ART", "NN", "ART", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sind lauter Widerspruch mit dem wovor du kniest,", "tokens": ["Sind", "lau\u00b7ter", "Wi\u00b7der\u00b7spruch", "mit", "dem", "wo\u00b7vor", "du", "kniest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "APPR", "ART", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Der Menschheit sch\u00f6nem Bild, wie es mu\u00df einst auf Erden", "tokens": ["Der", "Menschheit", "sch\u00f6\u00b7nem", "Bild", ",", "wie", "es", "mu\u00df", "einst", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "PWAV", "PPER", "VMFIN", "ADV", "APPR", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Gewesen seyn, und wie es mu\u00df einst wieder werden.", "tokens": ["Ge\u00b7we\u00b7sen", "seyn", ",", "und", "wie", "es", "mu\u00df", "einst", "wie\u00b7der", "wer\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAINF", "$,", "KON", "PWAV", "PPER", "VMFIN", "ADV", "ADV", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Wes ganzes Streben ist auf dieses Ziel gerichtet,", "tokens": ["Wes", "gan\u00b7zes", "Stre\u00b7ben", "ist", "auf", "die\u00b7ses", "Ziel", "ge\u00b7rich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "VAFIN", "APPR", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist von der Welt getrennt, und ist ihr doch verpflichtet,", "tokens": ["Ist", "von", "der", "Welt", "ge\u00b7trennt", ",", "und", "ist", "ihr", "doch", "ver\u00b7pflich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "VVPP", "$,", "KON", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Will ihr mit Liebeszorn, was sie nicht will, aufdringen,", "tokens": ["Will", "ihr", "mit", "Lie\u00b7bes\u00b7zorn", ",", "was", "sie", "nicht", "will", ",", "auf\u00b7drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "NN", "$,", "PRELS", "PPER", "PTKNEG", "VMFIN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "F\u00fchlt da\u00df ers mu\u00df, und f\u00fchlt da\u00df es ihm mu\u00df mislingen.", "tokens": ["F\u00fchlt", "da\u00df", "ers", "mu\u00df", ",", "und", "f\u00fchlt", "da\u00df", "es", "ihm", "mu\u00df", "mis\u00b7lin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOUS", "PIS", "VMFIN", "$,", "KON", "VVFIN", "KOUS", "PPER", "PPER", "VMFIN", "VVINF", "$."], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}}}}}