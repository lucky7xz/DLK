{"dta.poem.871": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "LvIII.  Auf den Sontag de\u00df Wunderw\u016frcken-  \n den Helfers/ oder  XXI.  Sontag nach dem Fest  \n der H. Dreyeinigkeit Joh. 4.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Ach! Erden gute Nacht/ Mein Heyland wil nicht h\u00f6r&#7869; ", "tokens": ["Ach", "!", "Er\u00b7den", "gu\u00b7te", "Nacht", "/", "Mein", "Hey\u00b7land", "wil", "nicht", "h\u00f6r", "&#7869;"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "XML_entity"], "pos": ["ITJ", "$.", "NN", "ADJA", "NN", "$(", "PPOSAT", "NN", "VMFIN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hier nutzt kein Menscheu Raht/ ich geh die letzte bahn!", "tokens": ["Hier", "nutzt", "kein", "Men\u00b7scheu", "Raht", "/", "ich", "geh", "die", "letz\u00b7te", "bahn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "NN", "$(", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Tod spannt schon die ", "tokens": ["Der", "Tod", "spannt", "schon", "die"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "chen Kahn ", "tokens": ["chen", "Kahn"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Den Leib auff dieser Klipp zu scheittern vnd zu st\u00f6ren.", "tokens": ["Den", "Leib", "auff", "die\u00b7ser", "Klipp", "zu", "scheit\u00b7tern", "vnd", "zu", "st\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PDAT", "NN", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Mein Leben f\u00e4hrt dahin: weil sich die schmertzen mehren/ ", "tokens": ["Mein", "Le\u00b7ben", "f\u00e4hrt", "da\u00b7hin", ":", "weil", "sich", "die", "schmert\u00b7zen", "meh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PAV", "$.", "KOUS", "PRF", "ART", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vnd Geister vntergehn/ es ist mit mir gethan", "tokens": ["Vnd", "Geis\u00b7ter", "vn\u00b7ter\u00b7gehn", "/", "es", "ist", "mit", "mir", "ge\u00b7than"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVINF", "$(", "PPER", "VAFIN", "APPR", "PPER", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Augen brechen mir/ der ", "tokens": ["Die", "Au\u00b7gen", "bre\u00b7chen", "mir", "/", "der"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "$(", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wird mich in dieser angst/ wenn Niemand hilfft versehren! ", "tokens": ["Wird", "mich", "in", "die\u00b7ser", "angst", "/", "wenn", "Nie\u00b7mand", "hilfft", "ver\u00b7seh\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PDAT", "NN", "$(", "KOUS", "PIS", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ach! eyll ich denn ins Grab! O Himmels\u00fcsse Trew", "tokens": ["Ach", "!", "eyll", "ich", "denn", "ins", "Grab", "!", "O", "Him\u00b7mel\u00b7s\u00fcs\u00b7se", "Trew"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "$.", "NE", "PPER", "ADV", "APPRART", "NN", "$.", "NE", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "HeRR ", "tokens": ["HeRR"], "token_info": ["word"], "pos": ["NE"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Wie kanst du denn was ich so sehnlich bitt\u2019/ abschlagen", "tokens": ["Wie", "kanst", "du", "denn", "was", "ich", "so", "sehn\u00b7lich", "bitt'", "/", "ab\u00b7schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PWAV", "VMFIN", "PPER", "KON", "PWS", "PPER", "ADV", "ADV", "PTKANT", "$(", "VVINF"], "meter": "-+-+-+-+--++-", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Kom eilend/ ehr der Tod die scharffen Pfeyl absche\u00fc\u00dft/ ", "tokens": ["Kom", "ei\u00b7lend", "/", "ehr", "der", "Tod", "die", "scharf\u00b7fen", "Pfeyl", "ab\u00b7sche\u00fc\u00dft", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "$(", "NN", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ehr als das sieche Fleisch/ die m\u00fcede ", "tokens": ["Ehr", "als", "das", "sie\u00b7che", "Fleisch", "/", "die", "m\u00fce\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "KOKOM", "ART", "ADJA", "NN", "$(", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Hei\u00df leben/ vmb den du dein Leben m\u00fcssen wagen.", "tokens": ["Hei\u00df", "le\u00b7ben", "/", "vmb", "den", "du", "dein", "Le\u00b7ben", "m\u00fcs\u00b7sen", "wa\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$(", "APPR", "ART", "PPER", "PPOSAT", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}