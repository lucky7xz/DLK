{"dta.poem.20457": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Verliebte Arien.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ist denn dein hertze gar erfroren?", "tokens": ["Ist", "denn", "dein", "hert\u00b7ze", "gar", "er\u00b7fro\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bist du aus schnes und ei\u00df gebohren?", "tokens": ["Bist", "du", "aus", "schnes", "und", "ei\u00df", "ge\u00b7boh\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ADJA", "KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "H\u00f6rst du mein seuffzen nicht/", "tokens": ["H\u00f6rst", "du", "mein", "seuff\u00b7zen", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "PTKNEG", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und was mein unmuth spricht?", "tokens": ["Und", "was", "mein", "un\u00b7muth", "spricht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Soll ich dich g\u00f6ttin nennen?", "tokens": ["Soll", "ich", "dich", "g\u00f6t\u00b7tin", "nen\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "NE", "VVINF", "$."], "meter": "++-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "So nim des himmels wehmuth an/", "tokens": ["So", "nim", "des", "him\u00b7mels", "weh\u00b7muth", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "ART", "NN", "ADJD", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der leichtlich sich erbarmen kan/", "tokens": ["Der", "leicht\u00b7lich", "sich", "er\u00b7bar\u00b7men", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PRF", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und uns nicht ewig l\u00e4\u00dft in hoffnungs-flammen brennen.", "tokens": ["Und", "uns", "nicht", "e\u00b7wig", "l\u00e4\u00dft", "in", "hoff\u00b7nungs\u00b7flam\u00b7men", "bren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PTKNEG", "ADJD", "VVFIN", "APPR", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Des blutes-regung zu vermeiden/", "tokens": ["Des", "blu\u00b7tes\u00b7re\u00b7gung", "zu", "ver\u00b7mei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und gantz von fleisch und blut zu scheiden/", "tokens": ["Und", "gantz", "von", "fleisch", "und", "blut", "zu", "schei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ADJD", "KON", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ist nirgends ein gebot/", "tokens": ["Ist", "nir\u00b7gends", "ein", "ge\u00b7bot", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Es heissets auch nicht GOtt;", "tokens": ["Es", "heis\u00b7sets", "auch", "nicht", "Gott", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sich selber zu verlassen", "tokens": ["Sich", "sel\u00b7ber", "zu", "ver\u00b7las\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PRF", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ist eine flucht/ so str\u00e4fflich ist/", "tokens": ["Ist", "ei\u00b7ne", "flucht", "/", "so", "str\u00e4ff\u00b7lich", "ist", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "VVFIN", "$(", "ADV", "ADJD", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und wer ihm solche bahn erkiest/", "tokens": ["Und", "wer", "ihm", "sol\u00b7che", "bahn", "er\u00b7kiest", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Den mu\u00df die menschlichkeit als einen unmensch hassen.", "tokens": ["Den", "mu\u00df", "die", "menschlich\u00b7keit", "als", "ei\u00b7nen", "un\u00b7mensch", "has\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "ART", "NN", "KOKOM", "ART", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}, "stanza.3": {"line.1": {"text": "Du kanst ja deiner nicht geniessen/", "tokens": ["Du", "kanst", "ja", "dei\u00b7ner", "nicht", "ge\u00b7nies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PPOSAT", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Kein mund wei\u00df selber sich zu k\u00fcssen/", "tokens": ["Kein", "mund", "wei\u00df", "sel\u00b7ber", "sich", "zu", "k\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ADV", "PRF", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der schnee auff deiner brust", "tokens": ["Der", "schnee", "auff", "dei\u00b7ner", "brust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Bringt dir geringe lust.", "tokens": ["Bringt", "dir", "ge\u00b7rin\u00b7ge", "lust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die fleischichten Granaten", "tokens": ["Die", "flei\u00b7schich\u00b7ten", "Gra\u00b7na\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Seynd nicht allein vor dich erdacht/", "tokens": ["Seynd", "nicht", "al\u00b7lein", "vor", "dich", "er\u00b7dacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ADV", "APPR", "PPER", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Kein mensch ist vor sich selbst gemacht;", "tokens": ["Kein", "mensch", "ist", "vor", "sich", "selbst", "ge\u00b7macht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "APPR", "PRF", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Es wei\u00df der kl\u00fcgste geist ihm hier nicht recht zu rathen.", "tokens": ["Es", "wei\u00df", "der", "kl\u00fcgs\u00b7te", "geist", "ihm", "hier", "nicht", "recht", "zu", "ra\u00b7then", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "PPER", "ADV", "PTKNEG", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}