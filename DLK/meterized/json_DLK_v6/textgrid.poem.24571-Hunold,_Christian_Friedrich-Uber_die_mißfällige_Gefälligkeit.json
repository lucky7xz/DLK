{"textgrid.poem.24571": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Uber die mi\u00dff\u00e4llige Gef\u00e4lligkeit", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verlasse doch die Art zu leben;", "tokens": ["Ver\u00b7las\u00b7se", "doch", "die", "Art", "zu", "le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Oder ich verlasse dich:", "tokens": ["O\u00b7der", "ich", "ver\u00b7las\u00b7se", "dich", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn die Gef\u00e4lligkeit/ du Thor/ erz\u00fcrnet mich.", "tokens": ["Denn", "die", "Ge\u00b7f\u00e4l\u00b7lig\u00b7keit", "/", "du", "Thor", "/", "er\u00b7z\u00fcr\u00b7net", "mich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$(", "PPER", "NN", "$(", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zu allen lachen hei\u00dft in nichts Verpflichtung geben.", "tokens": ["Zu", "al\u00b7len", "la\u00b7chen", "hei\u00dft", "in", "nichts", "Ver\u00b7pflich\u00b7tung", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVINF", "VVFIN", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wer \u00fcberal ein Ja-Herr seyn/", "tokens": ["Wer", "\u00fc\u00b7be\u00b7ral", "ein", "Ja\u00b7Herr", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "VAINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und sein Verdienst dadurch will machen/", "tokens": ["Und", "sein", "Ver\u00b7dienst", "da\u00b7durch", "will", "ma\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PAV", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Der f\u00e4hrt auf einem umgekehrten Nachen/", "tokens": ["Der", "f\u00e4hrt", "auf", "ei\u00b7nem", "um\u00b7ge\u00b7kehr\u00b7ten", "Na\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und kriegt zur Antwort schweigend: Nein.", "tokens": ["Und", "kriegt", "zur", "Ant\u00b7wort", "schwei\u00b7gend", ":", "Nein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ADJD", "$.", "PTKANT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Dennoch so liebst du die Manier;", "tokens": ["Den\u00b7noch", "so", "liebst", "du", "die", "Ma\u00b7nier", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und allen Menschen komt nichts mehr verdr\u00fc\u00dflich f\u00fcr/", "tokens": ["Und", "al\u00b7len", "Men\u00b7schen", "komt", "nichts", "mehr", "ver\u00b7dr\u00fc\u00df\u00b7lich", "f\u00fcr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PIS", "ADV", "ADJD", "APPR", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Als so ein Lob/ das man zur Unzeit spricht:", "tokens": ["Als", "so", "ein", "Lob", "/", "das", "man", "zur", "Un\u00b7zeit", "spricht", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "$(", "PRELS", "PIS", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Viel lieber/ da\u00df man mir vor solches R\u00fchmen fluchet.", "tokens": ["Viel", "lie\u00b7ber", "/", "da\u00df", "man", "mir", "vor", "sol\u00b7ches", "R\u00fch\u00b7men", "flu\u00b7chet", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$(", "KOUS", "PIS", "PPER", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wer aller Welt so zu gefallen suchet/", "tokens": ["Wer", "al\u00b7ler", "Welt", "so", "zu", "ge\u00b7fal\u00b7len", "su\u00b7chet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIAT", "NN", "ADV", "PTKZU", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Gef\u00e4llet keinem Klugen nicht.", "tokens": ["Ge\u00b7f\u00e4l\u00b7let", "kei\u00b7nem", "Klu\u00b7gen", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}