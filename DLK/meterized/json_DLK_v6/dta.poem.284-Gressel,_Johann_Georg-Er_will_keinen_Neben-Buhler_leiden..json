{"dta.poem.284": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Er will keinen Neben-Buhler leiden.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Wer das sch\u00f6ne Auge kennet/", "tokens": ["Wer", "das", "sch\u00f6\u00b7ne", "Au\u00b7ge", "ken\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "So mein mattes Hertze brennet/", "tokens": ["So", "mein", "mat\u00b7tes", "Hert\u00b7ze", "bren\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "St\u00fcrtze sich nicht in Gefahr/", "tokens": ["St\u00fcrt\u00b7ze", "sich", "nicht", "in", "Ge\u00b7fahr", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "PTKNEG", "APPR", "NN", "$("], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "Sehen mag er\u2019s/ doch nicht lieben/", "tokens": ["Se\u00b7hen", "mag", "er's", "/", "doch", "nicht", "lie\u00b7ben", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "VMFIN", "PIS", "$(", "ADV", "PTKNEG", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sonsten m\u00f6chte Rache \u00fcben/", "tokens": ["Sons\u00b7ten", "m\u00f6ch\u00b7te", "Ra\u00b7che", "\u00fc\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Der/ der l\u00e4ngst sein eigen war.", "tokens": ["Der", "/", "der", "l\u00e4ngst", "sein", "ei\u00b7gen", "war", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "ART", "ADV", "PPOSAT", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Nach dem Schatten dieser Sch\u00f6nen", "tokens": ["Nach", "dem", "Schat\u00b7ten", "die\u00b7ser", "Sch\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ehrerbietig sich zu sehnen/", "tokens": ["Ehr\u00b7er\u00b7bie\u00b7tig", "sich", "zu", "seh\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PRF", "PTKZU", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Lasse ich noch wol geschehn;", "tokens": ["Las\u00b7se", "ich", "noch", "wol", "ge\u00b7schehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Jhre Sch\u00f6nheit zu verehren", "tokens": ["Ih\u00b7re", "Sch\u00f6n\u00b7heit", "zu", "ver\u00b7eh\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Will ich keinen Menschen wehren", "tokens": ["Will", "ich", "kei\u00b7nen", "Men\u00b7schen", "weh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PIAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Noch von Ferne sie zu sehn.", "tokens": ["Noch", "von", "Fer\u00b7ne", "sie", "zu", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie zwey grosse starcke Hunde/", "tokens": ["Wie", "zwey", "gros\u00b7se", "star\u00b7cke", "Hun\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "CARD", "ADJA", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Selten ohne Bi\u00df und Wunde", "tokens": ["Sel\u00b7ten", "oh\u00b7ne", "Bi\u00df", "und", "Wun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "K\u00f6nnen bey einander seyn", "tokens": ["K\u00f6n\u00b7nen", "bey", "ein\u00b7an\u00b7der", "seyn"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "PRF", "PPOSAT"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "So mu\u00df auch bey diesem Beine/", "tokens": ["So", "mu\u00df", "auch", "bey", "die\u00b7sem", "Bei\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "APPR", "PDAT", "NN", "$("], "meter": "++-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Der mich lassen nur alleine", "tokens": ["Der", "mich", "las\u00b7sen", "nur", "al\u00b7lei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "VVFIN", "ADV", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wer die Wunden denckt zu scheun.", "tokens": ["Wer", "die", "Wun\u00b7den", "denckt", "zu", "scheun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Jm verschlossenen Geh\u00e4ge", "tokens": ["Jm", "ver\u00b7schlos\u00b7se\u00b7nen", "Ge\u00b7h\u00e4\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Giebt es ungemeine Schl\u00e4ge", "tokens": ["Giebt", "es", "un\u00b7ge\u00b7mei\u00b7ne", "Schl\u00e4\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn ein Fremder es betritt/", "tokens": ["Wenn", "ein", "Frem\u00b7der", "es", "be\u00b7tritt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und das Wild gedenckt zu fangen/", "tokens": ["Und", "das", "Wild", "ge\u00b7denckt", "zu", "fan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVPP", "PTKZU", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Was der Herre zuerlangen", "tokens": ["Was", "der", "Her\u00b7re", "zu\u00b7er\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Selbsten eyfrig sich bem\u00fcht.", "tokens": ["Selbs\u00b7ten", "ey\u00b7frig", "sich", "be\u00b7m\u00fcht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PRF", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}