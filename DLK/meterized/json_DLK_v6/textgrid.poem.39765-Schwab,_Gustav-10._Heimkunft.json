{"textgrid.poem.39765": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "10. Heimkunft", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jetzo steh ich vor dem Thale,", "tokens": ["Jet\u00b7zo", "steh", "ich", "vor", "dem", "Tha\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das der Dunst nicht mehr verh\u00fcllt,", "tokens": ["Das", "der", "Dunst", "nicht", "mehr", "ver\u00b7h\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "PTKNEG", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Das sich, eine blanke Schale,", "tokens": ["Das", "sich", ",", "ei\u00b7ne", "blan\u00b7ke", "Scha\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bis zum Rand mit Sonne f\u00fcllt.", "tokens": ["Bis", "zum", "Rand", "mit", "Son\u00b7ne", "f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Bin aus ihm gleich einem Diebe", "tokens": ["Bin", "aus", "ihm", "gleich", "ei\u00b7nem", "Die\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "PPER", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch der Nebel Nacht entflohn;", "tokens": ["Durch", "der", "Ne\u00b7bel", "Nacht", "ent\u00b7flohn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Komme jetzt voll Heimatliebe", "tokens": ["Kom\u00b7me", "jetzt", "voll", "Hei\u00b7mat\u00b7lie\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Her, wie der verlorne Sohn.", "tokens": ["Her", ",", "wie", "der", "ver\u00b7lor\u00b7ne", "Sohn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ART", "ADJA", "NN", "$."], "meter": "----+-+", "measure": "unknown.measure.di"}}, "stanza.3": {"line.1": {"text": "Und dort winkt's aus hellen Fenstern,", "tokens": ["Und", "dort", "winkt's", "aus", "hel\u00b7len", "Fens\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Arme, K\u00f6pfe kreuzen sich.", "tokens": ["Ar\u00b7me", ",", "K\u00f6p\u00b7fe", "kreu\u00b7zen", "sich", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "VVFIN", "PRF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Keine Schaar von Nachtgespenstern!", "tokens": ["Kei\u00b7ne", "Schaar", "von", "Nacht\u00b7ge\u00b7spens\u00b7tern", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Traute Blicke gr\u00fc\u00dfen mich.", "tokens": ["Trau\u00b7te", "Bli\u00b7cke", "gr\u00fc\u00b7\u00dfen", "mich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Mutter, Kinder! was sind Bl\u00fcten", "tokens": ["Mut\u00b7ter", ",", "Kin\u00b7der", "!", "was", "sind", "Bl\u00fc\u00b7ten"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$.", "PWS", "VAFIN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gegen euch, was Berg und Wald?", "tokens": ["Ge\u00b7gen", "euch", ",", "was", "Berg", "und", "Wald", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PWS", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sch\u00e4tze giebt es hier zu h\u00fcten;", "tokens": ["Sch\u00e4t\u00b7ze", "giebt", "es", "hier", "zu", "h\u00fc\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wieder wandr' ich nicht so bald.", "tokens": ["Wie\u00b7der", "wandr'", "ich", "nicht", "so", "bald", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "J\u00fcngster Knabe, komm und funkle", "tokens": ["J\u00fcngs\u00b7ter", "Kna\u00b7be", ",", "komm", "und", "funk\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "VVFIN", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mich mit schwarzen Augen an:", "tokens": ["Mich", "mit", "schwar\u00b7zen", "Au\u00b7gen", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie das Erdenleben dunkle,", "tokens": ["Wie", "das", "Er\u00b7den\u00b7le\u00b7ben", "dunk\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "So ein Stral macht sich noch Bahn.", "tokens": ["So", "ein", "Stral", "macht", "sich", "noch", "Bahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PRF", "ADV", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.6": {"line.1": {"text": "Alle k\u00fcnftigen Geschicke", "tokens": ["Al\u00b7le", "k\u00fcnf\u00b7ti\u00b7gen", "Ge\u00b7schi\u00b7cke"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Des bewegten Vaterlands", "tokens": ["Des", "be\u00b7weg\u00b7ten", "Va\u00b7ter\u00b7lands"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Les' ich hier in diesem Blicke,", "tokens": ["Les'", "ich", "hier", "in", "die\u00b7sem", "Bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dieser Kinderaugen Glanz.", "tokens": ["Die\u00b7ser", "Kin\u00b7der\u00b7au\u00b7gen", "Glanz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Wachse r\u00fcstig, lieber Knabe!", "tokens": ["Wach\u00b7se", "r\u00fcs\u00b7tig", ",", "lie\u00b7ber", "Kna\u00b7be", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "ADV", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Vieles wartet wohl auf dich.", "tokens": ["Vie\u00b7les", "war\u00b7tet", "wohl", "auf", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch als Greis am Wanderstabe", "tokens": ["Doch", "als", "Greis", "am", "Wan\u00b7der\u00b7sta\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Siehst du Sch\u00f6neres, denn ich!", "tokens": ["Siehst", "du", "Sch\u00f6\u00b7ne\u00b7res", ",", "denn", "ich", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,", "KON", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}