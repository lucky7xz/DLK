{"textgrid.poem.33050": {"metadata": {"author": {"name": "B\u00fcchner, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Glockenstimmen erschallen,", "genre": "verse", "period": "N.A.", "pub_year": 1849, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Glockenstimmen erschallen,", "tokens": ["Die", "Glo\u00b7ckens\u00b7tim\u00b7men", "er\u00b7schal\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Mild leuchtet der Abendstern,", "tokens": ["Mild", "leuch\u00b7tet", "der", "A\u00b7bends\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und feierlich k\u00fcndet ihr Hallen", "tokens": ["Und", "fei\u00b7er\u00b7lich", "k\u00fcn\u00b7det", "ihr", "Hal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPOSAT", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Die Auferstehung des Herrn.", "tokens": ["Die", "Auf\u00b7er\u00b7ste\u00b7hung", "des", "Herrn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Ihr hellen Osterglocken,", "tokens": ["Ihr", "hel\u00b7len", "Os\u00b7ter\u00b7glo\u00b7cken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich h\u00f6rt' euch schon manches Jahr,", "tokens": ["Ich", "h\u00f6rt'", "euch", "schon", "man\u00b7ches", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PIAT", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Bald unter Scherz und Frohlocken,", "tokens": ["Bald", "un\u00b7ter", "Scherz", "und", "Froh\u00b7lo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-++-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Bald wenn ich in Thr\u00e4nen war.", "tokens": ["Bald", "wenn", "ich", "in", "Thr\u00e4\u00b7nen", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "APPR", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Heut' t\u00f6nt mir euer L\u00e4uten", "tokens": ["Heut'", "t\u00f6nt", "mir", "eu\u00b7er", "L\u00e4u\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So tr\u00fcb' und so ahnungsvoll,", "tokens": ["So", "tr\u00fcb'", "und", "so", "ah\u00b7nungs\u00b7voll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "ADV", "ADJD", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Nicht wei\u00df ich, was mir bedeuten", "tokens": ["Nicht", "wei\u00df", "ich", ",", "was", "mir", "be\u00b7deu\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "VVFIN", "PPER", "$,", "PWS", "PPER", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Das ernste Hallen soll.", "tokens": ["Das", "erns\u00b7te", "Hal\u00b7len", "soll", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wie m\u00f6g't ihr mir wohl erklingen,", "tokens": ["Wie", "m\u00f6g't", "ihr", "mir", "wohl", "er\u00b7klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PPER", "ADV", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wenn wieder ein Jahr hinab?", "tokens": ["Wenn", "wie\u00b7der", "ein", "Jahr", "hin\u00b7ab", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wie Weinen, wie fr\u00f6hlich Singen,", "tokens": ["Wie", "Wei\u00b7nen", ",", "wie", "fr\u00f6h\u00b7lich", "Sin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "PWAV", "ADJD", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Oder auf meinem Grab?", "tokens": ["O\u00b7der", "auf", "mei\u00b7nem", "Grab", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}