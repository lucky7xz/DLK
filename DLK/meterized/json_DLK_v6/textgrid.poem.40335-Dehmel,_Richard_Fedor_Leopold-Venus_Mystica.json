{"textgrid.poem.40335": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Venus Mystica", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbich m\u00f6chte die Flamme umarmen!\u00ab", "tokens": ["\u00bb", "ich", "m\u00f6ch\u00b7te", "die", "Flam\u00b7me", "um\u00b7ar\u00b7men", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VMFIN", "ART", "NN", "VVINF", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Aus schwerem Schlaf", "tokens": ["Aus", "schwe\u00b7rem", "Schlaf"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "in stiller Nacht", "tokens": ["in", "stil\u00b7ler", "Nacht"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "weckte mich dies Wort;", "tokens": ["weck\u00b7te", "mich", "dies", "Wort", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDS", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "ich wei\u00df nicht, wer es sprach;", "tokens": ["ich", "wei\u00df", "nicht", ",", "wer", "es", "sprach", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Stimme, wer bist du?", "tokens": ["Stim\u00b7me", ",", "wer", "bist", "du", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "VAFIN", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Nackt, mit bettelnden Fingern,", "tokens": ["Nackt", ",", "mit", "bet\u00b7teln\u00b7den", "Fin\u00b7gern", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "weiten Armen,", "tokens": ["wei\u00b7ten", "Ar\u00b7men", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "mit Weibesbr\u00fcsten,", "tokens": ["mit", "Wei\u00b7bes\u00b7br\u00fcs\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "ein irrer Mund,", "tokens": ["ein", "ir\u00b7rer", "Mund", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "flehst du aus der Nacht", "tokens": ["flehst", "du", "aus", "der", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "die gro\u00dfe strahlende Flamme an?", "tokens": ["die", "gro\u00b7\u00dfe", "strah\u00b7len\u00b7de", "Flam\u00b7me", "an", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Weg! sie brennt!", "tokens": ["Weg", "!", "sie", "brennt", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Trunken naht ein grauer Blick,", "tokens": ["Trun\u00b7ken", "naht", "ein", "grau\u00b7er", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "schwelt;", "tokens": ["schwelt", ";"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-", "measure": "single.down"}, "line.3": {"text": "um die klare Glut", "tokens": ["um", "die", "kla\u00b7re", "Glut"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "mit beiden Knieen", "tokens": ["mit", "bei\u00b7den", "Kni\u00b7e\u00b7en"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIAT", "NN"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.5": {"text": "schlingt sich hei\u00df ein hitziger Schoo\u00df.", "tokens": ["schlingt", "sich", "hei\u00df", "ein", "hit\u00b7zi\u00b7ger", "Schoo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Weib: so nicht!", "tokens": ["Weib", ":", "so", "nicht", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "ADV", "PTKNEG", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "Kalt, aufrecht seh ich", "tokens": ["Kalt", ",", "auf\u00b7recht", "seh", "ich"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ADJD", "VVFIN", "PPER"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "in dein rauchschwarz flackerndes Haar", "tokens": ["in", "dein", "rauch\u00b7schwarz", "fla\u00b7ckern\u00b7des", "Haar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJD", "ADJA", "NN"], "meter": "-+-++--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "die lichte Lohe fassen,", "tokens": ["die", "lich\u00b7te", "Lo\u00b7he", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "dich verzehrend.", "tokens": ["dich", "ver\u00b7zeh\u00b7rend", "."], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVPP", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Rein und ruhig", "tokens": ["Rein", "und", "ru\u00b7hig"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "ADJD"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "steigt die feurige S\u00e4ule", "tokens": ["steigt", "die", "feu\u00b7ri\u00b7ge", "S\u00e4u\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.7": {"text": "aus der kurzen Beschattung", "tokens": ["aus", "der", "kur\u00b7zen", "Be\u00b7schat\u00b7tung"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "mit dir auf.", "tokens": ["mit", "dir", "auf", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "Stimme, so, nun darfst du", "tokens": ["Stim\u00b7me", ",", "so", ",", "nun", "darfst", "du"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ADV", "$,", "ADV", "VVFIN", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "\u2013 jauchze! \u2013 die Flamme umarmen.", "tokens": ["\u2013", "jauch\u00b7ze", "!", "\u2013", "die", "Flam\u00b7me", "um\u00b7ar\u00b7men", "."], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$.", "$(", "ART", "NN", "VVINF", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.5": {"line.1": {"text": ". . . . . . . . . . . . . . . . . . . . . . . . . . . .", "tokens": [".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", ".", "."], "token_info": ["punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct"], "pos": ["$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$.", "$."]}}, "stanza.6": {"line.1": {"text": "Wohl: so hat mein Herz in Z\u00fcchten", "tokens": ["Wohl", ":", "so", "hat", "mein", "Herz", "in", "Z\u00fcch\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$.", "ADV", "VAFIN", "PPOSAT", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "mein unz\u00fcchtig Blut bek\u00e4mpft,", "tokens": ["mein", "un\u00b7z\u00fcch\u00b7tig", "Blut", "be\u00b7k\u00e4mpft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "VVPP", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "hat in Angst vor seinen S\u00fcchten", "tokens": ["hat", "in", "Angst", "vor", "sei\u00b7nen", "S\u00fcch\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "seine Sehnsuchtsglut ged\u00e4mpft,", "tokens": ["sei\u00b7ne", "Sehn\u00b7suchts\u00b7glut", "ge\u00b7d\u00e4mpft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "hat mir Sieg auf Sieg errungen,", "tokens": ["hat", "mir", "Sieg", "auf", "Sieg", "er\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "aber Frieden, Frieden \u2013 nein!", "tokens": ["a\u00b7ber", "Frie\u00b7den", ",", "Frie\u00b7den", "\u2013", "nein", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "NN", "$,", "NN", "$(", "PTKANT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In gespenstischen Peinigungen", "tokens": ["In", "ge\u00b7spens\u00b7ti\u00b7schen", "Pei\u00b7ni\u00b7gun\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "lebt'ich schreckhaft, bis selbst Dein", "tokens": ["lebt'\u00b7ich", "schreck\u00b7haft", ",", "bis", "selbst", "Dein"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "ADJD", "$,", "KOUS", "ADV", "PPOSAT"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "reines Lichtgel\u00fcst mich reute,", "tokens": ["rei\u00b7nes", "Licht\u00b7ge\u00b7l\u00fcst", "mich", "reu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "tief in einer tr\u00fcben Nacht,", "tokens": ["tief", "in", "ei\u00b7ner", "tr\u00fc\u00b7ben", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "die ich schlaflos so wie heute", "tokens": ["die", "ich", "schlaf\u00b7los", "so", "wie", "heu\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PRELS", "PPER", "ADJD", "ADV", "KOKOM", "ADV"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "unter Geistern zugebracht,", "tokens": ["un\u00b7ter", "Geis\u00b7tern", "zu\u00b7ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}