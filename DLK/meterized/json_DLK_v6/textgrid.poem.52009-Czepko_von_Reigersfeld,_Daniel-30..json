{"textgrid.poem.52009": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "30.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als auff Patroclus Stuhl Oppanus wolte kreisten,", "tokens": ["Als", "auff", "Pat\u00b7ro\u00b7clus", "Stuhl", "Op\u00b7pa\u00b7nus", "wol\u00b7te", "kreis\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NE", "NE", "NE", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und \u00fcber der Geburth fieng m\u00fchsam an zu seyn,", "tokens": ["Und", "\u00fc\u00b7ber", "der", "Ge\u00b7burth", "fi\u00b7eng", "m\u00fch\u00b7sam", "an", "zu", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ADJD", "PTKVZ", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Fleucht eine Wespe zu, und setzt den Stachel ein", "tokens": ["Fleucht", "ei\u00b7ne", "Wes\u00b7pe", "zu", ",", "und", "setzt", "den", "Sta\u00b7chel", "ein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "$,", "KON", "VVFIN", "ART", "NN", "ART"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ans Jungfern liebe Ding und wil ihm Beystand leisten.", "tokens": ["Ans", "Jung\u00b7fern", "lie\u00b7be", "Ding", "und", "wil", "ihm", "Beys\u00b7tand", "leis\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "KON", "VMFIN", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der Stich ist voller Gifft. Was aber anzustellen?", "tokens": ["Der", "Stich", "ist", "vol\u00b7ler", "Gifft", ".", "Was", "a\u00b7ber", "an\u00b7zu\u00b7stel\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJA", "NN", "$.", "PWS", "ADV", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mein Freund du must zu dir den krancken Stachel ziehn.", "tokens": ["Mein", "Freund", "du", "must", "zu", "dir", "den", "kran\u00b7cken", "Sta\u00b7chel", "ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VMFIN", "APPR", "PPER", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Wesp' ist Nemesis. Dann dencke, da\u00df vorhin", "tokens": ["Die", "Wesp'", "ist", "Ne\u00b7me\u00b7sis", ".", "Dann", "den\u00b7cke", ",", "da\u00df", "vor\u00b7hin"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VAFIN", "NE", "$.", "ADV", "VVFIN", "$,", "KOUS", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Auch manche Wunde must umb seinetwillen schwellen.", "tokens": ["Auch", "man\u00b7che", "Wun\u00b7de", "must", "umb", "sei\u00b7net\u00b7wil\u00b7len", "schwel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VMFIN", "APPR", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}