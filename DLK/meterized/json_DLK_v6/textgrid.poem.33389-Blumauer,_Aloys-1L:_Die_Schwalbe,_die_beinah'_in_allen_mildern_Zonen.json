{"textgrid.poem.33389": {"metadata": {"author": {"name": "Blumauer, Aloys", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Schwalbe, die beinah' in allen mildern Zonen", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Schwalbe, die beinah' in allen mildern Zonen", "tokens": ["Die", "Schwal\u00b7be", ",", "die", "bei\u00b7nah'", "in", "al\u00b7len", "mil\u00b7dern", "Zo\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Erde sich ihr kleines Nestchen baut,", "tokens": ["Der", "Er\u00b7de", "sich", "ihr", "klei\u00b7nes", "Nest\u00b7chen", "baut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und mit den Menschen, die sie liebt, vertraut,", "tokens": ["Und", "mit", "den", "Men\u00b7schen", ",", "die", "sie", "liebt", ",", "ver\u00b7traut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Gern an den Orten wohnt, wo Menschen wohnen,", "tokens": ["Gern", "an", "den", "Or\u00b7ten", "wohnt", ",", "wo", "Men\u00b7schen", "woh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "$,", "PWAV", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Kam einst in eines reichen Mannes Haus,", "tokens": ["Kam", "einst", "in", "ei\u00b7nes", "rei\u00b7chen", "Man\u00b7nes", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und suchte sich, um Junge da zu hecken,", "tokens": ["Und", "such\u00b7te", "sich", ",", "um", "Jun\u00b7ge", "da", "zu", "he\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "$,", "KOUI", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Im Stillen ein ger\u00e4umig Pl\u00e4tzchen aus.", "tokens": ["Im", "Stil\u00b7len", "ein", "ge\u00b7r\u00e4u\u00b7mig", "Pl\u00e4tz\u00b7chen", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJD", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Allein das Haus war schon an allen Ecken", "tokens": ["Al\u00b7lein", "das", "Haus", "war", "schon", "an", "al\u00b7len", "E\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VAFIN", "ADV", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Voll Nester mancher Art: es nisteten", "tokens": ["Voll", "Nes\u00b7ter", "man\u00b7cher", "Art", ":", "es", "nis\u00b7te\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "NN", "PIAT", "NN", "$.", "PPER", "VVFIN"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.10": {"text": "Darinnen Raben, Elstern, Kr\u00e4h'n,", "tokens": ["Da\u00b7rin\u00b7nen", "Ra\u00b7ben", ",", "Els\u00b7tern", ",", "Kr\u00e4h'n", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NE", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Die s\u00e4mmtlich ihrem sonst gro\u00dfm\u00fcthigen", "tokens": ["Die", "s\u00e4mmt\u00b7lich", "ih\u00b7rem", "sonst", "gro\u00df\u00b7m\u00fct\u00b7hi\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "PPOSAT", "ADV", "ADJA"], "meter": "-+-+-+-++-", "measure": "unknown.measure.penta"}, "line.12": {"text": "Hausherren in den Ohren lagen,", "tokens": ["Haus\u00b7her\u00b7ren", "in", "den", "Oh\u00b7ren", "la\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Die fromme Schwalbe zu verjagen.", "tokens": ["Die", "from\u00b7me", "Schwal\u00b7be", "zu", "ver\u00b7ja\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Vor allen that ein Dompfaff sich hervor \u2013", "tokens": ["Vor", "al\u00b7len", "that", "ein", "Dom\u00b7pfaff", "sich", "her\u00b7vor", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVFIN", "ART", "NN", "PRF", "PTKVZ", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Ein Vogel, dessen Kehle zwar zum Singen", "tokens": ["Ein", "Vo\u00b7gel", ",", "des\u00b7sen", "Keh\u00b7le", "zwar", "zum", "Sin\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NE", "$,", "PRELAT", "NN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Nichts taugt, doch desto mehr zum Schlingen", "tokens": ["Nichts", "taugt", ",", "doch", "des\u00b7to", "mehr", "zum", "Schlin\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "$,", "ADV", "ADV", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Gemacht ist \u2013 dieser schrie dem Hausherrn laut in's Ohr:", "tokens": ["Ge\u00b7macht", "ist", "\u2013", "die\u00b7ser", "schrie", "dem", "Haus\u00b7herrn", "laut", "in's", "Ohr", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$(", "PDS", "VVFIN", "ART", "NN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Es w\u00fcrd' an seinem Haus kein Stein am andern bleiben,", "tokens": ["Es", "w\u00fcrd'", "an", "sei\u00b7nem", "Haus", "kein", "Stein", "am", "an\u00b7dern", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "PIAT", "NN", "APPRART", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "G\u00e4b' er nicht gleich Befehl die Schwalbe zu vertreiben.", "tokens": ["G\u00e4b'", "er", "nicht", "gleich", "Be\u00b7fehl", "die", "Schwal\u00b7be", "zu", "ver\u00b7trei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADV", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Der Dompfaff schrie, die Schwalbe mu\u00dfte fort,", "tokens": ["Der", "Dom\u00b7pfaff", "schrie", ",", "die", "Schwal\u00b7be", "mu\u00df\u00b7te", "fort", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VMFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Und baute k\u00fcmmerlich bald da, bald dort", "tokens": ["Und", "bau\u00b7te", "k\u00fcm\u00b7mer\u00b7lich", "bald", "da", ",", "bald", "dort"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "ADJD", "ADV", "ADV", "$,", "ADV", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Nur im Verborg'nen sich ihr Nestchen f\u00fcr die Jungen,", "tokens": ["Nur", "im", "Ver\u00b7bor\u00b7g'\u00b7nen", "sich", "ihr", "Nest\u00b7chen", "f\u00fcr", "die", "Jun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "PRF", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.23": {"text": "Zufrieden, wenn es ihr gelungen,", "tokens": ["Zu\u00b7frie\u00b7den", ",", "wenn", "es", "ihr", "ge\u00b7lun\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "In Jahresfrist mit eifrigem Bem\u00fch'n", "tokens": ["In", "Jah\u00b7res\u00b7frist", "mit", "eif\u00b7ri\u00b7gem", "Be\u00b7m\u00fch'n"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Ein Paar derselben gro\u00df zu zieh'n.", "tokens": ["Ein", "Paar", "der\u00b7sel\u00b7ben", "gro\u00df", "zu", "zieh'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["ART", "NN", "PDAT", "ADJD", "PTKZU", "VVFIN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Allein ihr Jammer hatte bald ein Ende:", "tokens": ["Al\u00b7lein", "ihr", "Jam\u00b7mer", "hat\u00b7te", "bald", "ein", "En\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Das Haus kam nun in and're H\u00e4nde.", "tokens": ["Das", "Haus", "kam", "nun", "in", "an\u00b7d'\u00b7re", "H\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.28": {"text": "Der neue Hausherr trieb die Kr\u00e4h'n und Elstern aus,", "tokens": ["Der", "neu\u00b7e", "Haus\u00b7herr", "trieb", "die", "Kr\u00e4h'n", "und", "Els\u00b7tern", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Und nahm die Schwalbe in sein Haus.", "tokens": ["Und", "nahm", "die", "Schwal\u00b7be", "in", "sein", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Da baut sie nun ihr Nestchen in der Stille,", "tokens": ["Da", "baut", "sie", "nun", "ihr", "Nest\u00b7chen", "in", "der", "Stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Heckt ruhig Jungen aus die F\u00fclle,", "tokens": ["Heckt", "ru\u00b7hig", "Jun\u00b7gen", "aus", "die", "F\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Und pflegt sie alle m\u00fctterlich;", "tokens": ["Und", "pflegt", "sie", "al\u00b7le", "m\u00fct\u00b7ter\u00b7lich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIS", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Der Hausherr aber freuet sich,", "tokens": ["Der", "Haus\u00b7herr", "a\u00b7ber", "freu\u00b7et", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Da\u00df mit der Schwalbe, die er aufgenommen,", "tokens": ["Da\u00df", "mit", "der", "Schwal\u00b7be", ",", "die", "er", "auf\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "Auch Segen in sein Haus gekommen.", "tokens": ["Auch", "Se\u00b7gen", "in", "sein", "Haus", "ge\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wir Maurer sind die Schwalbe, liebe Br\u00fcder!", "tokens": ["Wir", "Mau\u00b7rer", "sind", "die", "Schwal\u00b7be", ",", "lie\u00b7be", "Br\u00fc\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "NN", "VAFIN", "ART", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wir fliegen nun in Josephs grossem Haus,", "tokens": ["Wir", "flie\u00b7gen", "nun", "in", "Jo\u00b7se\u00b7phs", "gros\u00b7sem", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Von ihm geduldet, ruhig ein und aus.", "tokens": ["Von", "ihm", "ge\u00b7dul\u00b7det", ",", "ru\u00b7hig", "ein", "und", "aus", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "$,", "ADJD", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Er war's bei dem f\u00fcr ihre Glieder", "tokens": ["Er", "wa\u00b7r's", "bei", "dem", "f\u00fcr", "ih\u00b7re", "Glie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ART", "APPR", "PPOSAT", "NN"], "meter": "----+-+-+-", "measure": "unknown.measure.tri"}, "line.5": {"text": "Die Manrerei nun Schutz und Duldung fand.", "tokens": ["Die", "Man\u00b7re\u00b7rei", "nun", "Schutz", "und", "Dul\u00b7dung", "fand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Sie lie\u00df, aus manchem Staat verbannt,", "tokens": ["Sie", "lie\u00df", ",", "aus", "man\u00b7chem", "Staat", "ver\u00b7bannt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sich froh in seinem Reiche nieder,", "tokens": ["Sich", "froh", "in", "sei\u00b7nem", "Rei\u00b7che", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "APPR", "PPOSAT", "NE", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und bringt Segen in sein Land.", "tokens": ["Und", "bringt", "Se\u00b7gen", "in", "sein", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}}}}