{"textgrid.poem.52806": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Die letzten Worte des Gutsbesitzers Herrn von Zips", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kommt es einst mit mir zum Sterben,", "tokens": ["Kommt", "es", "einst", "mit", "mir", "zum", "Ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nun so setz' ich keinen Erben,", "tokens": ["Nun", "so", "setz'", "ich", "kei\u00b7nen", "Er\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ich mach' auch kein Testament,", "tokens": ["Ich", "mach'", "auch", "kein", "Tes\u00b7ta\u00b7ment", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIAT", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Meinen n\u00e4chsten Blutsverwandten,", "tokens": ["Mei\u00b7nen", "n\u00e4chs\u00b7ten", "Bluts\u00b7ver\u00b7wand\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Guten Freunden und Bekannten", "tokens": ["Gu\u00b7ten", "Freun\u00b7den", "und", "Be\u00b7kann\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wird mein Nachla\u00df gern geg\u00f6nnt.", "tokens": ["Wird", "mein", "Nachl\u00b7a\u00df", "gern", "ge\u00b7g\u00f6nnt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Mich wird Niemand balsamiren,", "tokens": ["Mich", "wird", "Nie\u00b7mand", "bal\u00b7sa\u00b7mi\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auch in kein Gew\u00f6lbe f\u00fchren,", "tokens": ["Auch", "in", "kein", "Ge\u00b7w\u00f6l\u00b7be", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wozu nutzt auch dieser Pracht?", "tokens": ["Wo\u00b7zu", "nutzt", "auch", "die\u00b7ser", "Pracht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "PDAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Gott befehl' ich meine Seele", "tokens": ["Gott", "be\u00b7fehl'", "ich", "mei\u00b7ne", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und den Leib der frischen H\u00f6hle,", "tokens": ["Und", "den", "Leib", "der", "fri\u00b7schen", "H\u00f6h\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die ein Todtengr\u00e4ber macht.", "tokens": ["Die", "ein", "Tod\u00b7ten\u00b7gr\u00e4\u00b7ber", "macht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Keine Frau soll mich begleiten,", "tokens": ["Kei\u00b7ne", "Frau", "soll", "mich", "be\u00b7glei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Denn ich hab' an meiner Seiten", "tokens": ["Denn", "ich", "hab'", "an", "mei\u00b7ner", "Sei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ein solch' Kleinod nicht gek\u00fc\u00dft;", "tokens": ["Ein", "solch'", "Klei\u00b7nod", "nicht", "ge\u00b7k\u00fc\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "PTKNEG", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Also darf sich keine gr\u00e4men,", "tokens": ["Al\u00b7so", "darf", "sich", "kei\u00b7ne", "gr\u00e4\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PRF", "PIAT", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Noch vor andern Weibern sch\u00e4men,", "tokens": ["Noch", "vor", "an\u00b7dern", "Wei\u00b7bern", "sch\u00e4\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df sie Wittwe worden ist.", "tokens": ["Da\u00df", "sie", "Witt\u00b7we", "wor\u00b7den", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VAPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Es soll Niemand um mich trauern,", "tokens": ["Es", "soll", "Nie\u00b7mand", "um", "mich", "trau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIS", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Noch in Briefen mich bedauern,", "tokens": ["Noch", "in", "Brie\u00b7fen", "mich", "be\u00b7dau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schont das schwarze Siegellack!", "tokens": ["Schont", "das", "schwar\u00b7ze", "Sie\u00b7gel\u00b7lack", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Woher r\u00fchrt das tolle Weinen,", "tokens": ["Wo\u00b7her", "r\u00fchrt", "das", "tol\u00b7le", "Wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "In verh\u00fcllter Tracht erscheinen,", "tokens": ["In", "ver\u00b7h\u00fcll\u00b7ter", "Tracht", "er\u00b7schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Als von heidnischem Geschmack?", "tokens": ["Als", "von", "heid\u00b7ni\u00b7schem", "Ge\u00b7schmack", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Einen Leichnam zu begraben,", "tokens": ["Ei\u00b7nen", "Leich\u00b7nam", "zu", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sollt ihr keine Kosten haben,", "tokens": ["Sollt", "ihr", "kei\u00b7ne", "Kos\u00b7ten", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PIAT", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Das verbittert nur das Leid.", "tokens": ["Das", "ver\u00b7bit\u00b7tert", "nur", "das", "Leid", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn man todt ist, geht's auch ohne", "tokens": ["Wenn", "man", "todt", "ist", ",", "geht's", "auch", "oh\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PIS", "ADJD", "VAFIN", "$,", "VVFIN", "ADV", "APPR"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wei\u00dfe Handschuh' und Citrone,", "tokens": ["Wei\u00b7\u00dfe", "Hand\u00b7schuh'", "und", "Cit\u00b7ro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+--++-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Diesem Dank der Eitelkeit.", "tokens": ["Die\u00b7sem", "Dank", "der", "Ei\u00b7tel\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Todtenhemd mit theu'ren Spitzen", "tokens": ["Tod\u00b7ten\u00b7hemd", "mit", "theu'\u00b7ren", "Spit\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Brauchen nicht an mir zu blitzen,", "tokens": ["Brau\u00b7chen", "nicht", "an", "mir", "zu", "blit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Nach dem Tod' ist Niemand sch\u00f6n,", "tokens": ["Nach", "dem", "Tod'", "ist", "Nie\u00b7mand", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "PIS", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Arme Leute aus dem Spittel", "tokens": ["Ar\u00b7me", "Leu\u00b7te", "aus", "dem", "Spit\u00b7tel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "M\u00f6gen meinen Sterbekittel", "tokens": ["M\u00f6\u00b7gen", "mei\u00b7nen", "Ster\u00b7be\u00b7kit\u00b7tel"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schlecht und recht zusammenn\u00e4h'n.", "tokens": ["Schlecht", "und", "recht", "zu\u00b7sam\u00b7men\u00b7n\u00e4h'", "n."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["NN", "KON", "ADJD", "VVFIN", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Um den Nu\u00dfbaum w\u00e4r' es Schade,", "tokens": ["Um", "den", "Nu\u00df\u00b7baum", "w\u00e4r'", "es", "Scha\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "VAFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Leget mich in eine Lade,", "tokens": ["Le\u00b7get", "mich", "in", "ei\u00b7ne", "La\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die von Tannenholz besteht;", "tokens": ["Die", "von", "Tan\u00b7nen\u00b7holz", "be\u00b7steht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Griff und Leisten k\u00f6nnt ihr sparen,", "tokens": ["Griff", "und", "Leis\u00b7ten", "k\u00f6nnt", "ihr", "spa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nur mit Zapfen mich verwahren,", "tokens": ["Nur", "mit", "Zap\u00b7fen", "mich", "ver\u00b7wah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bis der Sarg zu Grabe geht.", "tokens": ["Bis", "der", "Sarg", "zu", "Gra\u00b7be", "geht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Putzet mich mit keinen Rosen,", "tokens": ["Put\u00b7zet", "mich", "mit", "kei\u00b7nen", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00f6hnt nicht den Empfindungslosen,", "tokens": ["H\u00f6hnt", "nicht", "den", "Emp\u00b7fin\u00b7dungs\u00b7lo\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Der sich nicht bedanken kann,", "tokens": ["Der", "sich", "nicht", "be\u00b7dan\u00b7ken", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fcllt die h\u00f6lzerne Pastete", "tokens": ["F\u00fcllt", "die", "h\u00f6l\u00b7zer\u00b7ne", "Pas\u00b7te\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "Statt dem irdischen Ger\u00e4the", "tokens": ["Statt", "dem", "ir\u00b7di\u00b7schen", "Ge\u00b7r\u00e4\u00b7the"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Blos mit Hobelsp\u00e4hnen an!", "tokens": ["Blos", "mit", "Ho\u00b7bel\u00b7sp\u00e4h\u00b7nen", "an", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Lasset keine Glocken l\u00e4uten,", "tokens": ["Las\u00b7set", "kei\u00b7ne", "Glo\u00b7cken", "l\u00e4u\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Denn es schauert oft den Leuten,", "tokens": ["Denn", "es", "schau\u00b7ert", "oft", "den", "Leu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn sich so die Anstalt h\u00e4uft,", "tokens": ["Wenn", "sich", "so", "die", "An\u00b7stalt", "h\u00e4uft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "La\u00dft mich ohne Pferd' und Wagen", "tokens": ["La\u00dft", "mich", "oh\u00b7ne", "Pferd'", "und", "Wa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Durch sechs arme M\u00e4nner tragen,", "tokens": ["Durch", "sechs", "ar\u00b7me", "M\u00e4n\u00b7ner", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die es weniger ergreift.", "tokens": ["Die", "es", "we\u00b7ni\u00b7ger", "er\u00b7greift", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Lobt mich auch nicht nach dem Tode,", "tokens": ["Lobt", "mich", "auch", "nicht", "nach", "dem", "To\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKNEG", "APPR", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Oeffnet nicht nach neuer Mode", "tokens": ["Oeff\u00b7net", "nicht", "nach", "neu\u00b7er", "Mo\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mich nach meinem Todesfall;", "tokens": ["Mich", "nach", "mei\u00b7nem", "To\u00b7des\u00b7fall", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn was wollet ihr ergr\u00fcnden?", "tokens": ["Denn", "was", "wol\u00b7let", "ihr", "er\u00b7gr\u00fcn\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Da ist nichts Apparts zu finden", "tokens": ["Da", "ist", "nichts", "Ap\u00b7parts", "zu", "fin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PIS", "ADV", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie an einem Feldmarschall.", "tokens": ["Wie", "an", "ei\u00b7nem", "Feld\u00b7mar\u00b7schall", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Ich will nicht, da\u00df von den Reisen", "tokens": ["Ich", "will", "nicht", ",", "da\u00df", "von", "den", "Rei\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PTKNEG", "$,", "KOUS", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Oder guten Handlungsweisen", "tokens": ["O\u00b7der", "gu\u00b7ten", "Hand\u00b7lungs\u00b7wei\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Jemand nach dem Tode schreibt,", "tokens": ["Je\u00b7mand", "nach", "dem", "To\u00b7de", "schreibt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ich will, da\u00df ihr mich vergesset,", "tokens": ["Ich", "will", ",", "da\u00df", "ihr", "mich", "ver\u00b7ges\u00b7set", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Und das Brod mit Freuden esset,", "tokens": ["Und", "das", "Brod", "mit", "Freu\u00b7den", "es\u00b7set", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Was von mir noch \u00fcbrig bleibt!", "tokens": ["Was", "von", "mir", "noch", "\u00fcb\u00b7rig", "bleibt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Pyramid und Monumenten", "tokens": ["Py\u00b7ra\u00b7mid", "und", "Mo\u00b7nu\u00b7men\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["NE", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ueber'm Grabe zu verschwenden,", "tokens": ["Ue\u00b7ber'm", "Gra\u00b7be", "zu", "ver\u00b7schwen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00df' ich gern der gro\u00dfen Welt;", "tokens": ["La\u00df'", "ich", "gern", "der", "gro\u00b7\u00dfen", "Welt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fcrst und Diener gleicherweise", "tokens": ["F\u00fcrst", "und", "Die\u00b7ner", "glei\u00b7cher\u00b7wei\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Werden doch der W\u00fcrmer Speise,", "tokens": ["Wer\u00b7den", "doch", "der", "W\u00fcr\u00b7mer", "Spei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Weil der Tod vom Rang Nichts h\u00e4lt.", "tokens": ["Weil", "der", "Tod", "vom", "Rang", "Nichts", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPRART", "NN", "PIS", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Hier war Nichts an mir zu loben,", "tokens": ["Hier", "war", "Nichts", "an", "mir", "zu", "lo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Das Jerusalem dort droben", "tokens": ["Das", "Je\u00b7ru\u00b7sa\u00b7lem", "dort", "dro\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "PIS", "ADV", "ADV"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ist der Ort nach meinem Sinn,", "tokens": ["Ist", "der", "Ort", "nach", "mei\u00b7nem", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Weil ich Gott gef\u00fcrchtet habe,", "tokens": ["Weil", "ich", "Gott", "ge\u00b7f\u00fcrch\u00b7tet", "ha\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Lieg' ich fr\u00f6hlich in dem Grabe,", "tokens": ["Lieg'", "ich", "fr\u00f6h\u00b7lich", "in", "dem", "Gra\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Zieh' ich ohne Murren hin!", "tokens": ["Zieh'", "ich", "oh\u00b7ne", "Mur\u00b7ren", "hin", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"