{"textgrid.poem.50679": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie Mancher d\u00fcnket sich im Gl\u00fcck ein Held zu sein,", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie Mancher d\u00fcnket sich im Gl\u00fcck ein Held zu sein,", "tokens": ["Wie", "Man\u00b7cher", "d\u00fcn\u00b7ket", "sich", "im", "Gl\u00fcck", "ein", "Held", "zu", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVFIN", "PRF", "APPRART", "NN", "ART", "NN", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der in der Noth verzagt! Das Ungl\u00fcck ist's allein,", "tokens": ["Der", "in", "der", "Noth", "ver\u00b7zagt", "!", "Das", "Un\u00b7gl\u00fcck", "ist's", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVPP", "$.", "ART", "NN", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das in das Innerste des falschen Herzens dringet", "tokens": ["Das", "in", "das", "In\u00b7ners\u00b7te", "des", "fal\u00b7schen", "Her\u00b7zens", "drin\u00b7get"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und den verf\u00fchrten Tropf zur Selbsterkenntni\u00df bringet.", "tokens": ["Und", "den", "ver\u00b7f\u00fchr\u00b7ten", "Tropf", "zur", "Selbs\u00b7ter\u00b7kennt\u00b7ni\u00df", "brin\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ein ", "tokens": ["Ein"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Doch wenn's ", "tokens": ["Doch", "wenn's"], "token_info": ["word", "word"], "pos": ["KON", "KOUS"], "meter": "-+", "measure": "iambic.single"}}}}}