{"textgrid.poem.49963": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "1", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kastor und Pollux hei\u00dfen ein Paar Ammoniten (der Vater", "tokens": ["Kas\u00b7tor", "und", "Pol\u00b7lux", "hei\u00b7\u00dfen", "ein", "Paar", "Am\u00b7mo\u00b7ni\u00b7ten", "(", "der", "Va\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "KON", "NN", "VVFIN", "ART", "NN", "NN", "$(", "ART", "NN"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Kann sie dir zeigen im Schrank); f\u00fcglich vergleich ich sie euch,", "tokens": ["Kann", "sie", "dir", "zei\u00b7gen", "im", "Schrank", ")", ";", "f\u00fcg\u00b7lich", "ver\u00b7gleich", "ich", "sie", "euch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "VVFIN", "APPRART", "NN", "$(", "$.", "ADV", "ADJD", "PPER", "PPER", "PPER", "$,"], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Emil und Theodor. Denn br\u00fcderlich sieht man die sch\u00f6nen", "tokens": ["E\u00b7mil", "und", "Theo\u00b7dor", ".", "Denn", "br\u00fc\u00b7der\u00b7lich", "sieht", "man", "die", "sch\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "KON", "NE", "$.", "KON", "ADJD", "VVFIN", "PIS", "ART", "ADJA"], "meter": "-+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Immer gesellt. Freut euch! heute noch habt ihr euch so.", "tokens": ["Im\u00b7mer", "ge\u00b7sellt", ".", "Freut", "euch", "!", "heu\u00b7te", "noch", "habt", "ihr", "euch", "so", "."], "token_info": ["word", "word", "punct", "word", "