{"textgrid.poem.51173": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Weisheitsvoll und prophetisch betrieb er und schrieb er Geschichte;", "genre": "verse", "period": "N.A.", "pub_year": 1880, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Weisheitsvoll und prophetisch betrieb er und schrieb er Geschichte;", "tokens": ["Weis\u00b7heits\u00b7voll", "und", "pro\u00b7phe\u00b7tisch", "be\u00b7trieb", "er", "und", "schrieb", "er", "Ge\u00b7schich\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "VVFIN", "PPER", "KON", "VVFIN", "PPER", "NN", "$."], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Als sie mit blitzendem Schild aufstand, purzelt' er um!", "tokens": ["Als", "sie", "mit", "blit\u00b7zen\u00b7dem", "Schild", "auf\u00b7stand", ",", "pur\u00b7zelt'", "er", "um", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$,", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "\u00bbw\u00e4r ich doch lieber ein K\u00e4tzlein, ein sch\u00e4biges, welches Miau schreit,", "tokens": ["\u00bb", "w\u00e4r", "ich", "doch", "lie\u00b7ber", "ein", "K\u00e4tz\u00b7lein", ",", "ein", "sch\u00e4\u00b7bi\u00b7ges", ",", "wel\u00b7ches", "Mi\u00b7au", "schreit", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PPER", "ADV", "ADV", "ART", "NN", "$,", "ART", "ADJA", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+--+--+-+-+-+-+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Als ein solcher Prophet!\u00ab riefen die Dichter im Chor.", "tokens": ["Als", "ein", "sol\u00b7cher", "Pro\u00b7phet", "!", "\u00ab", "rie\u00b7fen", "die", "Dich\u00b7ter", "im", "Chor", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "PIAT", "NN", "$.", "$(", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}}}}}