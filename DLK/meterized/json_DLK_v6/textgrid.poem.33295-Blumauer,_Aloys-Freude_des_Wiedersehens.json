{"textgrid.poem.33295": {"metadata": {"author": {"name": "Blumauer, Aloys", "birth": "N.A.", "death": "N.A."}, "title": "Freude des Wiedersehens", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O, wie s\u00fcsse", "tokens": ["O", ",", "wie", "s\u00fcs\u00b7se"], "token_info": ["word", "punct", "word", "word"], "pos": ["NE", "$,", "PWAV", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Lebt es sich!", "tokens": ["Lebt", "es", "sich", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Ich geniesse", "tokens": ["Ich", "ge\u00b7nies\u00b7se"], "token_info": ["word", "word"], "pos": ["PPER", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Wieder mich.", "tokens": ["Wie\u00b7der", "mich", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "PPER", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "In der N\u00e4he", "tokens": ["In", "der", "N\u00e4\u00b7he"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "--+-", "measure": "anapaest.init"}, "line.6": {"text": "Hab' und sehe", "tokens": ["Hab'", "und", "se\u00b7he"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Ich mein All;", "tokens": ["Ich", "mein", "All", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Wer sie kennet,", "tokens": ["Wer", "sie", "ken\u00b7net", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Der durchrennet", "tokens": ["Der", "durch\u00b7ren\u00b7net"], "token_info": ["word", "word"], "pos": ["PDS", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.10": {"text": "Berg und Thal;", "tokens": ["Berg", "und", "Thal", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.11": {"text": "Ach, ich kannte,", "tokens": ["Ach", ",", "ich", "kann\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.12": {"text": "Ach, ich rannte", "tokens": ["Ach", ",", "ich", "rann\u00b7te"], "token_info": ["word", "punct", "word", "word"], "pos": ["ITJ", "$,", "PPER", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.13": {"text": "Weit, o weit,", "tokens": ["Weit", ",", "o", "weit", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "FM", "ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.14": {"text": "Sie zu k\u00fcssen", "tokens": ["Sie", "zu", "k\u00fcs\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["PPER", "PTKZU", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.15": {"text": "Und im s\u00fcssen", "tokens": ["Und", "im", "s\u00fcs\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["KON", "APPRART", "ADJA"], "meter": "+-+-", "measure": "trochaic.di"}, "line.16": {"text": "Unbefang", "tokens": ["Un\u00b7be\u00b7fang"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.17": {"text": "Hing ich trunken,", "tokens": ["Hing", "ich", "trun\u00b7ken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.18": {"text": "Wie versunken,", "tokens": ["Wie", "ver\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "punct"], "pos": ["PWAV", "VVPP", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.19": {"text": "Stundenlang.", "tokens": ["Stun\u00b7den\u00b7lang", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.20": {"text": "Wie ein Engel", "tokens": ["Wie", "ein", "En\u00b7gel"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ART", "NN"], "meter": "--+-", "measure": "anapaest.init"}, "line.21": {"text": "Kam ich ihr,", "tokens": ["Kam", "ich", "ihr", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PPER", "PPER", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.22": {"text": "Ihre sch\u00f6nen", "tokens": ["Ih\u00b7re", "sch\u00f6\u00b7nen"], "token_info": ["word", "word"], "pos": ["PPOSAT", "ADJA"], "meter": "+-+-", "measure": "trochaic.di"}, "line.23": {"text": "Wonnethr\u00e4nen", "tokens": ["Won\u00b7net\u00b7hr\u00e4\u00b7nen"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.24": {"text": "Sagten's mir;", "tokens": ["Sag\u00b7ten's", "mir", ";"], "token_info": ["word", "word", "punct"], "pos": ["NE", "PPER", "$."], "meter": "+--", "measure": "dactylic.init"}, "line.25": {"text": "Und ihr Blicken", "tokens": ["Und", "ihr", "Bli\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPOSAT", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.26": {"text": "Und ihr Dr\u00fccken", "tokens": ["Und", "ihr", "Dr\u00fc\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPOSAT", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.27": {"text": "Sagt' es mir,", "tokens": ["Sagt'", "es", "mir", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.28": {"text": "Mein Verflummen", "tokens": ["Mein", "Ver\u00b7flum\u00b7men"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.29": {"text": "Mein Verstummen", "tokens": ["Mein", "Ver\u00b7stum\u00b7men"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.30": {"text": "Sagt es ihr.", "tokens": ["Sagt", "es", "ihr", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.31": {"text": "All' mein Sehnen,", "tokens": ["All'", "mein", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.32": {"text": "All' mein Thr\u00e4nen", "tokens": ["All'", "mein", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "PPOSAT", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.33": {"text": "Ist dahin;", "tokens": ["Ist", "da\u00b7hin", ";"], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "PAV", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.34": {"text": "All' erheitert,", "tokens": ["All'", "er\u00b7hei\u00b7tert", ","], "token_info": ["word", "word", "punct"], "pos": ["PIS", "VVPP", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.35": {"text": "Und erweitert", "tokens": ["Und", "er\u00b7wei\u00b7tert"], "token_info": ["word", "word"], "pos": ["KON", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.36": {"text": "Herz und Sinn,", "tokens": ["Herz", "und", "Sinn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.37": {"text": "F\u00fchl ich wieder,", "tokens": ["F\u00fchl", "ich", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.38": {"text": "Was ich bin,", "tokens": ["Was", "ich", "bin", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VAFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.39": {"text": "Singe Lieder,", "tokens": ["Sin\u00b7ge", "Lie\u00b7der", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.40": {"text": "H\u00fcpfe hin,", "tokens": ["H\u00fcp\u00b7fe", "hin", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.41": {"text": "Herze meine", "tokens": ["Her\u00b7ze", "mei\u00b7ne"], "token_info": ["word", "word"], "pos": ["VVFIN", "PPOSAT"], "meter": "+-+-", "measure": "trochaic.di"}, "line.42": {"text": "Liebe, kleine", "tokens": ["Lie\u00b7be", ",", "klei\u00b7ne"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$,", "ADJA"], "meter": "+-+-", "measure": "trochaic.di"}, "line.43": {"text": "Pflegerin.", "tokens": ["Pfle\u00b7ge\u00b7rin", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+--", "measure": "dactylic.init"}}}}}