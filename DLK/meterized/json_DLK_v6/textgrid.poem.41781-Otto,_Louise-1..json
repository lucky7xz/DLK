{"textgrid.poem.41781": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein lichter Sommertag! von wunderbaren", "tokens": ["Ein", "lich\u00b7ter", "Som\u00b7mer\u00b7tag", "!", "von", "wun\u00b7der\u00b7ba\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "APPR", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Prachtfarben gl\u00e4nzen ringsum Berg und Thal.", "tokens": ["Pracht\u00b7far\u00b7ben", "gl\u00e4n\u00b7zen", "ring\u00b7sum", "Berg", "und", "Thal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "NN", "KON", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Es wogt der Main mit seiner spiegelklaren,", "tokens": ["Es", "wogt", "der", "Main", "mit", "sei\u00b7ner", "spie\u00b7gel\u00b7kla\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Krystallnen Flut, und l\u00e4\u00dft im Sonnenstrahl", "tokens": ["Krys\u00b7tall\u00b7nen", "Flut", ",", "und", "l\u00e4\u00dft", "im", "Son\u00b7nen\u00b7strahl"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "NN", "$,", "KON", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die wei\u00dfen Segel schimmern, die er tr\u00e4gt,", "tokens": ["Die", "wei\u00b7\u00dfen", "Se\u00b7gel", "schim\u00b7mern", ",", "die", "er", "tr\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Das Ruder blitzen, das im Takt ihn schl\u00e4gt.", "tokens": ["Das", "Ru\u00b7der", "blit\u00b7zen", ",", "das", "im", "Takt", "ihn", "schl\u00e4gt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PRELS", "APPRART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Es scheint das Laub im Abendgold zu beben,", "tokens": ["Es", "scheint", "das", "Laub", "im", "A\u00b7bend\u00b7gold", "zu", "be\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das alle H\u00fcgel seiner Ufer schm\u00fcckt,", "tokens": ["Das", "al\u00b7le", "H\u00fc\u00b7gel", "sei\u00b7ner", "U\u00b7fer", "schm\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein holder Kranz von dichtgeschlung'nen Reben", "tokens": ["Ein", "hol\u00b7der", "Kranz", "von", "dicht\u00b7ge\u00b7schlung'\u00b7nen", "Re\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ist jedem Berge auf das Haupt gedr\u00fcckt,", "tokens": ["Ist", "je\u00b7dem", "Ber\u00b7ge", "auf", "das", "Haupt", "ge\u00b7dr\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Da funkelt hell wie Gold und Edelstein", "tokens": ["Da", "fun\u00b7kelt", "hell", "wie", "Gold", "und", "E\u00b7del\u00b7stein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD", "KOKOM", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Die Traube jetzt, im R\u00f6mer einst der Wein.", "tokens": ["Die", "Trau\u00b7be", "jetzt", ",", "im", "R\u00f6\u00b7mer", "einst", "der", "Wein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "APPRART", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ein J\u00fcngling wandelt sinnend am Gestade,", "tokens": ["Ein", "J\u00fcng\u00b7ling", "wan\u00b7delt", "sin\u00b7nend", "am", "Ge\u00b7sta\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und schaut den Segeln, schaut dem Strome zu:", "tokens": ["Und", "schaut", "den", "Se\u00b7geln", ",", "schaut", "dem", "Stro\u00b7me", "zu", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "\u00bbo k\u00f6nnt' ich fort mit Dir auf Deinem Pfade,", "tokens": ["\u00bb", "o", "k\u00f6nnt'", "ich", "fort", "mit", "Dir", "auf", "Dei\u00b7nem", "Pfa\u00b7de", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "VMFIN", "PPER", "PTKVZ", "APPR", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "O w\u00e4r' ich frei und fessellos wie Du!", "tokens": ["O", "w\u00e4r'", "ich", "frei", "und", "fes\u00b7sel\u00b7los", "wie", "Du", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "KOKOM", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Was soll ich nur des Weinstocks Pfleger sein,", "tokens": ["Was", "soll", "ich", "nur", "des", "Wein\u00b7stocks", "Pfle\u00b7ger", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "ART", "NN", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Es g\u00e4rt in mir ein andrer Feuerwein!\u00ab", "tokens": ["Es", "g\u00e4rt", "in", "mir", "ein", "an\u00b7drer", "Feu\u00b7er\u00b7wein", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "So seufzt er oft, wenn er die Reben schneidet,", "tokens": ["So", "seufzt", "er", "oft", ",", "wenn", "er", "die", "Re\u00b7ben", "schnei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die seiner Eltern traulich Haus umstehn,", "tokens": ["Die", "sei\u00b7ner", "El\u00b7tern", "trau\u00b7lich", "Haus", "um\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADJD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Arme sehnend er zur Ferne breitet,", "tokens": ["Die", "Ar\u00b7me", "seh\u00b7nend", "er", "zur", "Fer\u00b7ne", "brei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die er in seinen Tr\u00e4umen nur gesehn.", "tokens": ["Die", "er", "in", "sei\u00b7nen", "Tr\u00e4u\u00b7men", "nur", "ge\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Da kommt ein Flo\u00df! \u2013 \u00bbO tr\u00fcg' es doch mich gleich", "tokens": ["Da", "kommt", "ein", "Flo\u00df", "!", "\u2013", "\u00bb", "O", "tr\u00fcg'", "es", "doch", "mich", "gleich"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "$(", "$(", "NE", "VVFIN", "PPER", "ADV", "PPER", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Hinab, hinein ins weite deutsche Reich!\u00ab", "tokens": ["Hin\u00b7ab", ",", "hin\u00b7ein", "ins", "wei\u00b7te", "deut\u00b7sche", "Reich", "!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "$,", "ADV", "APPRART", "ADJA", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Der F\u00fchrer h\u00f6rt's und gr\u00fc\u00dft mit Freundeswinken,", "tokens": ["Der", "F\u00fch\u00b7rer", "h\u00f6rt's", "und", "gr\u00fc\u00dft", "mit", "Freun\u00b7des\u00b7win\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es lockt der Main mit seinem Wellenspiel,", "tokens": ["Es", "lockt", "der", "Main", "mit", "sei\u00b7nem", "Wel\u00b7len\u00b7spiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das Ruder lockt mit hellem Silberblinken,", "tokens": ["Das", "Ru\u00b7der", "lockt", "mit", "hel\u00b7lem", "Sil\u00b7ber\u00b7blin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Konrad ruft: \u00bbSo trage mich ans Ziel!\u00ab", "tokens": ["Und", "Kon\u00b7rad", "ruft", ":", "\u00bb", "So", "tra\u00b7ge", "mich", "ans", "Ziel", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "NE", "VVFIN", "$.", "$(", "ADV", "VVFIN", "PPER", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ein Gru\u00df, ein Sprung \u2013 da stand er auf dem Flo\u00df,", "tokens": ["Ein", "Gru\u00df", ",", "ein", "Sprung", "\u2013", "da", "stand", "er", "auf", "dem", "Flo\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$(", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Das pfeilgeschwind den Main hinunterscho\u00df.", "tokens": ["Das", "pfeil\u00b7ge\u00b7schwind", "den", "Main", "hin\u00b7un\u00b7ter\u00b7scho\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Noch t\u00f6nt das Abendgl\u00f6cklein von Wipfelde \u2013", "tokens": ["Noch", "t\u00f6nt", "das", "A\u00b7bend\u00b7gl\u00f6c\u00b7klein", "von", "Wip\u00b7fel\u00b7de", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NN", "$("], "meter": "-+-+--+-+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ist es ein Mahnruf? ist's ein Segensgru\u00df?", "tokens": ["Ist", "es", "ein", "Mahn\u00b7ruf", "?", "ist's", "ein", "Se\u00b7gens\u00b7gru\u00df", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$.", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und wenn das Fahrzeug auch im Sturm zerschellte \u2013", "tokens": ["Und", "wenn", "das", "Fahr\u00b7zeug", "auch", "im", "Sturm", "zer\u00b7schell\u00b7te", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADV", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Was k\u00fcmmert's Konrad noch? \u2013 er mu\u00df \u2013 er mu\u00df!", "tokens": ["Was", "k\u00fcm\u00b7mert's", "Kon\u00b7rad", "noch", "?", "\u2013", "er", "mu\u00df", "\u2013", "er", "mu\u00df", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "NE", "ADV", "$.", "$(", "PPER", "VMFIN", "$(", "PPER", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und ob die Heimat seinem Blick entschwand \u2013:", "tokens": ["Und", "ob", "die", "Hei\u00b7mat", "sei\u00b7nem", "Blick", "ent\u00b7schwand", "\u2013", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$(", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ihm ist die Heimat jetzt das deutsche Land!", "tokens": ["Ihm", "ist", "die", "Hei\u00b7mat", "jetzt", "das", "deut\u00b7sche", "Land", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Fort geht die Fahrt, den ganzen Main hinunter", "tokens": ["Fort", "geht", "die", "Fahrt", ",", "den", "gan\u00b7zen", "Main", "hin\u00b7un\u00b7ter"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "NN", "APPO"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bis wo der Rhein sich stolzer ihm gesellt,", "tokens": ["Bis", "wo", "der", "Rhein", "sich", "stol\u00b7zer", "ihm", "ge\u00b7sellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAV", "ART", "NE", "PRF", "ADJA", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Auch rheinhinab \u2013 und immer neue Wunder,", "tokens": ["Auch", "rhein\u00b7hi\u00b7nab", "\u2013", "und", "im\u00b7mer", "neu\u00b7e", "Wun\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$(", "KON", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und eine neue ungeahnte Welt!", "tokens": ["Und", "ei\u00b7ne", "neu\u00b7e", "un\u00b7ge\u00b7ahn\u00b7te", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wie braust er hin der k\u00f6nigliche Strom", "tokens": ["Wie", "braust", "er", "hin", "der", "k\u00f6\u00b7nig\u00b7li\u00b7che", "Strom"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und spiegelt wechselnd Stadt und Burg und Dom!", "tokens": ["Und", "spie\u00b7gelt", "wech\u00b7selnd", "Stadt", "und", "Burg", "und", "Dom", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Und dreht bei Bingen sich das Flo\u00df im Kreise", "tokens": ["Und", "dreht", "bei", "Bin\u00b7gen", "sich", "das", "Flo\u00df", "im", "Krei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN", "PRF", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und ist er von der Loreley bedroht,", "tokens": ["Und", "ist", "er", "von", "der", "Lo\u00b7re\u00b7ley", "be\u00b7droht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Singt Konrad noch ein Lied zu Deutschlands Preise,", "tokens": ["Singt", "Kon\u00b7rad", "noch", "ein", "Lied", "zu", "Deutschlands", "Prei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "ADV", "ART", "NN", "APPR", "NE", "NE", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und achtet nicht Gefahr und Kampf und Not,", "tokens": ["Und", "ach\u00b7tet", "nicht", "Ge\u00b7fahr", "und", "Kampf", "und", "Not", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und betet nur: \u00bbSo mag mein Leben sein!", "tokens": ["Und", "be\u00b7tet", "nur", ":", "\u00bb", "So", "mag", "mein", "Le\u00b7ben", "sein", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$.", "$(", "ADV", "VMFIN", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Wie diese Fahrt auf meinem deutschen Rhein.\u00ab", "tokens": ["Wie", "die\u00b7se", "Fahrt", "auf", "mei\u00b7nem", "deut\u00b7schen", "Rhein", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "PDAT", "NN", "APPR", "PPOSAT", "ADJA", "NE", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}