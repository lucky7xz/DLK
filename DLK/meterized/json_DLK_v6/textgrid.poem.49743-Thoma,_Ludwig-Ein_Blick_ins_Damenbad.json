{"textgrid.poem.49743": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Ein Blick ins Damenbad", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht all und jedes, meine Beste,", "tokens": ["Nicht", "all", "und", "je\u00b7des", ",", "mei\u00b7ne", "Bes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "PIAT", "KON", "PIAT", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ist reizend, was Ihr Kleid verhehlt.", "tokens": ["Ist", "rei\u00b7zend", ",", "was", "Ihr", "Kleid", "ver\u00b7hehlt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn manches, was das Mieder pre\u00dfte,", "tokens": ["Denn", "man\u00b7ches", ",", "was", "das", "Mie\u00b7der", "pre\u00df\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wird schwabbelig, wenn dieses fehlt.", "tokens": ["Wird", "schwab\u00b7be\u00b7lig", ",", "wenn", "die\u00b7ses", "fehlt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "KOUS", "PDS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ein h\u00fcbscher Stiefel, sch\u00f6ne Str\u00fcmpfe", "tokens": ["Ein", "h\u00fcb\u00b7scher", "Stie\u00b7fel", ",", "sch\u00f6\u00b7ne", "Str\u00fcmp\u00b7fe"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Beschwindeln uns oft sonderbar.", "tokens": ["Be\u00b7schwin\u00b7deln", "uns", "oft", "son\u00b7der\u00b7bar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Man sieht mit Schrecken, da\u00df die Nymphe", "tokens": ["Man", "sieht", "mit", "Schre\u00b7cken", ",", "da\u00df", "die", "Nym\u00b7phe"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "VVFIN", "APPR", "NN", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gespickt mit H\u00fchneraugen war.", "tokens": ["Ge\u00b7spickt", "mit", "H\u00fch\u00b7ner\u00b7au\u00b7gen", "war", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich spreche nicht von Hinterfronten,", "tokens": ["Ich", "spre\u00b7che", "nicht", "von", "Hin\u00b7ter\u00b7fron\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die, ungeb\u00fchrlich aufgebauscht,", "tokens": ["Die", ",", "un\u00b7ge\u00b7b\u00fchr\u00b7lich", "auf\u00b7ge\u00b7bauscht", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ART", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Uns nur so lang bezaubern konnten,", "tokens": ["Uns", "nur", "so", "lang", "be\u00b7zau\u00b7bern", "konn\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "ADJD", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als schwere Seide sie umrauscht.", "tokens": ["Als", "schwe\u00b7re", "Sei\u00b7de", "sie", "um\u00b7rauscht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Das Nackte kann die Tugend st\u00e4rken,", "tokens": ["Das", "Nack\u00b7te", "kann", "die", "Tu\u00b7gend", "st\u00e4r\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und vieles reizt uns nur umflort.", "tokens": ["Und", "vie\u00b7les", "reizt", "uns", "nur", "um\u00b7flort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich konnt' es durch die Wand bemerken,", "tokens": ["Ich", "konnt'", "es", "durch", "die", "Wand", "be\u00b7mer\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als ich ein Loch hineingebohrt.", "tokens": ["Als", "ich", "ein", "Loch", "hin\u00b7ein\u00b7ge\u00b7bohrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}