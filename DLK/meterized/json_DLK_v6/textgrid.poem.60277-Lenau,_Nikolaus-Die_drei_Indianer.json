{"textgrid.poem.60277": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Die drei Indianer", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "M\u00e4chtig z\u00fcrnt der Himmel im Gewitter,", "tokens": ["M\u00e4ch\u00b7tig", "z\u00fcrnt", "der", "Him\u00b7mel", "im", "Ge\u00b7wit\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Schmettert manche Rieseneich in Splitter,", "tokens": ["Schmet\u00b7tert", "man\u00b7che", "Rie\u00b7sen\u00b7eich", "in", "Split\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "\u00dcbert\u00f6nt des Niagara Stimme,", "tokens": ["\u00dc\u00b7ber\u00b7t\u00f6nt", "des", "Nia\u00b7ga\u00b7ra", "Stim\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NE", "NN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und mit seiner Blitze Flammenruten", "tokens": ["Und", "mit", "sei\u00b7ner", "Blit\u00b7ze", "Flam\u00b7men\u00b7ru\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Peitscht er schneller die besch\u00e4umten Fluten,", "tokens": ["Peitscht", "er", "schnel\u00b7ler", "die", "be\u00b7sch\u00e4um\u00b7ten", "Flu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Da\u00df sie st\u00fcrzen mit emp\u00f6rtem Grimme.", "tokens": ["Da\u00df", "sie", "st\u00fcr\u00b7zen", "mit", "em\u00b7p\u00f6r\u00b7tem", "Grim\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Indianer stehn am lauten Strande,", "tokens": ["In\u00b7di\u00b7a\u00b7ner", "stehn", "am", "lau\u00b7ten", "Stran\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Lauschen nach dem wilden Wogenbrande,", "tokens": ["Lau\u00b7schen", "nach", "dem", "wil\u00b7den", "Wo\u00b7gen\u00b7bran\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Nach des Waldes bangem Sterbgest\u00f6hne;", "tokens": ["Nach", "des", "Wal\u00b7des", "ban\u00b7gem", "Sterb\u00b7ge\u00b7st\u00f6h\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Greis der eine, mit ergrautem Haare,", "tokens": ["Greis", "der", "ei\u00b7ne", ",", "mit", "er\u00b7grau\u00b7tem", "Haa\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ART", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Aufrecht \u00fcberragend seine Jahre,", "tokens": ["Auf\u00b7recht", "\u00fc\u00b7berr\u00b7a\u00b7gend", "sei\u00b7ne", "Jah\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Die zwei andern seine starken S\u00f6hne.", "tokens": ["Die", "zwei", "an\u00b7dern", "sei\u00b7ne", "star\u00b7ken", "S\u00f6h\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "ADJA", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Seine S\u00f6hne jetzt der Greis betrachtet,", "tokens": ["Sei\u00b7ne", "S\u00f6h\u00b7ne", "jetzt", "der", "Greis", "be\u00b7trach\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und sein Blick sich dunkler jetzt umnachtet", "tokens": ["Und", "sein", "Blick", "sich", "dunk\u00b7ler", "jetzt", "um\u00b7nach\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "PRF", "ADJA", "ADV", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Als die Wolken, die den Himmel schw\u00e4rzen,", "tokens": ["Als", "die", "Wol\u00b7ken", ",", "die", "den", "Him\u00b7mel", "schw\u00e4r\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und sein Aug versendet wildre Blitze", "tokens": ["Und", "sein", "Aug", "ver\u00b7sen\u00b7det", "wild\u00b7re", "Blit\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVPP", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Als das Wetter durch die Wolkenritze,", "tokens": ["Als", "das", "Wet\u00b7ter", "durch", "die", "Wol\u00b7ken\u00b7rit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Und er spricht aus tiefemp\u00f6rtem Herzen:", "tokens": ["Und", "er", "spricht", "aus", "tie\u00b7fem\u00b7p\u00f6r\u00b7tem", "Her\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "\u00bbfluch den Wei\u00dfen! ihren letzten Spuren!", "tokens": ["\u00bb", "fluch", "den", "Wei\u00b7\u00dfen", "!", "ih\u00b7ren", "letz\u00b7ten", "Spu\u00b7ren", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "ART", "NN", "$.", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Jeder Welle Fluch, worauf sie fuhren,", "tokens": ["Je\u00b7der", "Wel\u00b7le", "Fluch", ",", "wo\u00b7rauf", "sie", "fuh\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "NN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Die einst Bettler unsern Strand erklettert!", "tokens": ["Die", "einst", "Bett\u00b7ler", "un\u00b7sern", "Strand", "er\u00b7klet\u00b7tert", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Fluch dem Windhauch, dienstbar ihrem Schiffe!", "tokens": ["Fluch", "dem", "Wind\u00b7hauch", ",", "dienst\u00b7bar", "ih\u00b7rem", "Schif\u00b7fe", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ADJD", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Hundert Fl\u00fcche jedem Felsenriffe,", "tokens": ["Hun\u00b7dert", "Fl\u00fc\u00b7che", "je\u00b7dem", "Fel\u00b7sen\u00b7rif\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Das sie nicht hat in den Grund geschmettert!", "tokens": ["Das", "sie", "nicht", "hat", "in", "den", "Grund", "ge\u00b7schmet\u00b7tert", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "PTKNEG", "VAFIN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "---+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "T\u00e4glich \u00fcbers Meer in wilder Eile", "tokens": ["T\u00e4g\u00b7lich", "\u00fc\u00b7bers", "Meer", "in", "wil\u00b7der", "Ei\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPRART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Fliegen ihre Schiffe, giftge Pfeile,", "tokens": ["Flie\u00b7gen", "ih\u00b7re", "Schif\u00b7fe", ",", "gift\u00b7ge", "Pfei\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Treffen unsre K\u00fcste mit Verderben.", "tokens": ["Tref\u00b7fen", "uns\u00b7re", "K\u00fcs\u00b7te", "mit", "Ver\u00b7der\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Nichts hat uns die R\u00e4uberbrut gelassen,", "tokens": ["Nichts", "hat", "uns", "die", "R\u00e4u\u00b7ber\u00b7brut", "ge\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Als im Herzen t\u00f6dlich bittres Hassen:", "tokens": ["Als", "im", "Her\u00b7zen", "t\u00f6d\u00b7lich", "bitt\u00b7res", "Has\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Kommt, ihr Kinder, kommt, wir wollen sterben!\u00ab", "tokens": ["Kommt", ",", "ihr", "Kin\u00b7der", ",", "kommt", ",", "wir", "wol\u00b7len", "ster\u00b7ben", "!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "$,", "VVFIN", "$,", "PPER", "VMFIN", "VVINF", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Also sprach der Alte, und sie schneiden", "tokens": ["Al\u00b7so", "sprach", "der", "Al\u00b7te", ",", "und", "sie", "schnei\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "KON", "PPER", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ihren Nachen von den Uferweiden,", "tokens": ["Ih\u00b7ren", "Na\u00b7chen", "von", "den", "U\u00b7fer\u00b7wei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Drauf sie nach des Stromes Mitte ringen;", "tokens": ["Drauf", "sie", "nach", "des", "Stro\u00b7mes", "Mit\u00b7te", "rin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und nun werfen sie weithin die Ruder,", "tokens": ["Und", "nun", "wer\u00b7fen", "sie", "weit\u00b7hin", "die", "Ru\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Armverschlungen Vater, Sohn und Bruder", "tokens": ["Arm\u00b7ver\u00b7schlun\u00b7gen", "Va\u00b7ter", ",", "Sohn", "und", "Bru\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Stimmen an, ihr Sterbelied zu singen.", "tokens": ["Stim\u00b7men", "an", ",", "ihr", "Ster\u00b7be\u00b7lied", "zu", "sin\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Laut ununterbrochne Donner krachen,", "tokens": ["Laut", "un\u00b7un\u00b7ter\u00b7broch\u00b7ne", "Don\u00b7ner", "kra\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Blitze flattern um den Todesnachen,", "tokens": ["Blit\u00b7ze", "flat\u00b7tern", "um", "den", "To\u00b7des\u00b7na\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Ihn umtaumeln M\u00f6wen sturmesmunter;", "tokens": ["Ihn", "um\u00b7tau\u00b7meln", "M\u00f6\u00b7wen", "stur\u00b7mes\u00b7mun\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "ADJD", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und die M\u00e4nner kommen festentschlossen", "tokens": ["Und", "die", "M\u00e4n\u00b7ner", "kom\u00b7men", "fes\u00b7tent\u00b7schlos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVINF", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Singend schon dem Falle zugeschossen,", "tokens": ["Sin\u00b7gend", "schon", "dem", "Fal\u00b7le", "zu\u00b7ge\u00b7schos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "St\u00fcrzen jetzt den Katarakt hinunter.", "tokens": ["St\u00fcr\u00b7zen", "jetzt", "den", "Ka\u00b7ta\u00b7rakt", "hin\u00b7un\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}