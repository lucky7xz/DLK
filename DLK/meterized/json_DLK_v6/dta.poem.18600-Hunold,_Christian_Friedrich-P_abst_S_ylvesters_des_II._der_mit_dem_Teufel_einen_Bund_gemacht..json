{"dta.poem.18600": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "P abst  S ylvesters des  II.  der mit  \n dem Teufel einen Bund gemacht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1702", "urn": "urn:nbn:de:kobv:b4-200905197766", "language": ["de:0.99"], "booktitle": "Hunold, Christian Friedrich: Die Edle Bem\u00fchung m\u00fcssiger Stunden. Hamburg, 1702."}, "poem": {"stanza.1": {"line.1": {"text": "So kan des Teuffels List das Kirchen Haupt verwirren/", "tokens": ["So", "kan", "des", "Teuf\u00b7fels", "List", "das", "Kir\u00b7chen", "Haupt", "ver\u00b7wir\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "NE", "ART", "NN", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Irrt nicht der Pabst zu grob in seiner Freundschaffts-", "tokens": ["Irrt", "nicht", "der", "Pabst", "zu", "grob", "in", "sei\u00b7ner", "Freund\u00b7schaffts"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "ART", "NN", "PTKA", "ADJD", "APPR", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wahl?", "tokens": ["Wahl", "?"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Und irrt sein Geist nicht auch von GOtt zur H\u00f6llen-Qvaal?", "tokens": ["Und", "irrt", "sein", "Geist", "nicht", "auch", "von", "Gott", "zur", "H\u00f6l\u00b7len\u00b7Qvaal", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PTKNEG", "ADV", "APPR", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wer saget denn wol mehr/ da\u00df P\u00e4bste niemahls irren?", "tokens": ["Wer", "sa\u00b7get", "denn", "wol", "mehr", "/", "da\u00df", "P\u00e4bs\u00b7te", "nie\u00b7mahls", "ir\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "ADV", "ADV", "$(", "KOUS", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}