{"dta.poem.7349": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Sonntag .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Das ist nun so ein schlimmer Tag,", "tokens": ["Das", "ist", "nun", "so", "ein", "schlim\u00b7mer", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie der April ihn bringen mag", "tokens": ["Wie", "der", "Ap\u00b7ril", "ihn", "brin\u00b7gen", "mag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "PPER", "VVINF", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit Schlacken, Schnee und Regen.", "tokens": ["Mit", "Schla\u00b7cken", ",", "Schnee", "und", "Re\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zum drittenmal in das Gebraus", "tokens": ["Zum", "drit\u00b7ten\u00b7mal", "in", "das", "Ge\u00b7braus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADV", "APPR", "ART", "NN"], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Streckt Jungfer Anne vor dem Haus", "tokens": ["Streckt", "Jung\u00b7fer", "An\u00b7ne", "vor", "dem", "Haus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "NE", "NE", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ihr kupfern Blendlaternchen aus,", "tokens": ["Ihr", "kup\u00b7fern", "Blend\u00b7la\u00b7tern\u00b7chen", "aus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und sp\u00e4ht l\u00e4ngs allen Wegen.", "tokens": ["Und", "sp\u00e4ht", "l\u00e4ngs", "al\u00b7len", "We\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "\u201ewo nur der Pfarrer bleiben kann?", "tokens": ["\u201e", "wo", "nur", "der", "Pfar\u00b7rer", "blei\u00b7ben", "kann", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "ADV", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ach, sicher ist dem guten Mann", "tokens": ["Ach", ",", "si\u00b7cher", "ist", "dem", "gu\u00b7ten", "Mann"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "ADJD", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was \u00fcber'n Weg gefahren!", "tokens": ["Was", "\u00fc\u00b7ber'n", "Weg", "ge\u00b7fah\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein Pfleger wohl, der Rechnung macht. \u2014", "tokens": ["Ein", "Pfle\u00b7ger", "wohl", ",", "der", "Rech\u00b7nung", "macht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADV", "$,", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Aus war der Gottesdienst um acht:", "tokens": ["Aus", "war", "der", "Got\u00b7tes\u00b7dienst", "um", "acht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VAFIN", "ART", "NN", "APPR", "CARD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Soll man so streifen in der Nacht", "tokens": ["Soll", "man", "so", "strei\u00b7fen", "in", "der", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PIS", "ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Bei Gicht und grauen Haaren!\u201c", "tokens": ["Bei", "Gicht", "und", "grau\u00b7en", "Haa\u00b7ren", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sie schlie\u00dft die Th\u00fcre, sch\u00fcttelt ba\u00df", "tokens": ["Sie", "schlie\u00dft", "die", "Th\u00fc\u00b7re", ",", "sch\u00fct\u00b7telt", "ba\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr Haupt und wischt am Brillenglas;", "tokens": ["Ihr", "Haupt", "und", "wischt", "am", "Bril\u00b7len\u00b7glas", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So gut d\u00fcnkt ihr die Stube;", "tokens": ["So", "gut", "d\u00fcnkt", "ihr", "die", "Stu\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im Ofen kracht's, der Lampenschein", "tokens": ["Im", "O\u00b7fen", "kracht's", ",", "der", "Lam\u00b7pen\u00b7schein"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "$,", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Hellt \u00fcber'm Tisch den Sonntagswein,", "tokens": ["Hellt", "\u00fc\u00b7ber'm", "Tisch", "den", "Sonn\u00b7tags\u00b7wein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und lockend l\u00e4dt der Sessel ein", "tokens": ["Und", "lo\u00b7ckend", "l\u00e4dt", "der", "Ses\u00b7sel", "ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mit seiner Kissengrube.", "tokens": ["Mit", "sei\u00b7ner", "Kis\u00b7sen\u00b7gru\u00b7be", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Pantoffeln, \u2014 Schlafrock, \u2014 alles recht!", "tokens": ["Pan\u00b7tof\u00b7feln", ",", "Schla\u00b7frock", ",", "al\u00b7les", "recht", "!"], "token_info": ["word", "punct", "punct", "word", "punct", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "$(", "NN", "$,", "$(", "PIS", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Sie horcht auf's neu; doch h\u00f6rt sie schlecht,", "tokens": ["Sie", "horcht", "auf's", "neu", ";", "doch", "h\u00f6rt", "sie", "schlecht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJD", "$.", "ADV", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es schwirrt ihr vor den Ohren.", "tokens": ["Es", "schwirrt", "ihr", "vor", "den", "Oh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "\u201ewie? hat's geklingelt? ei der Daus,", "tokens": ["\u201e", "wie", "?", "hat's", "ge\u00b7klin\u00b7gelt", "?", "ei", "der", "Daus", ","], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "$.", "VAFIN", "VVPP", "$.", "NE", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zum Zweitenmale! schnell hinaus!\u201c", "tokens": ["Zum", "Zwei\u00b7ten\u00b7ma\u00b7le", "!", "schnell", "hin\u00b7aus", "!", "\u201c"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "$.", "ADJD", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da tritt der Pfarrer schon in's Haus,", "tokens": ["Da", "tritt", "der", "Pfar\u00b7rer", "schon", "in's", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ganz blau und steif gefroren.", "tokens": ["Ganz", "blau", "und", "steif", "ge\u00b7fro\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Die Jungfrau blickt ein wenig quer,", "tokens": ["Die", "Jung\u00b7frau", "blickt", "ein", "we\u00b7nig", "quer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Beg\u00fctigend der Pfarrer her,", "tokens": ["Be\u00b7g\u00fc\u00b7ti\u00b7gend", "der", "Pfar\u00b7rer", "her", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie's recht in diesem Orden.", "tokens": ["Wie's", "recht", "in", "die\u00b7sem", "Or\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dann hustet er. \u201eNicht Mond noch Stern!", "tokens": ["Dann", "hus\u00b7tet", "er", ".", "\u201e", "Nicht", "Mond", "noch", "Stern", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "$(", "PTKNEG", "NN", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der lahme Friedrich h\u00f6rt doch gern", "tokens": ["Der", "lah\u00b7me", "Fried\u00b7rich", "h\u00f6rt", "doch", "gern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NE", "VVFIN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein christlich Wort am Tag des Herrn,", "tokens": ["Ein", "christ\u00b7lich", "Wort", "am", "Tag", "des", "Herrn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Es ist mir sp\u00e4t geworden!\u201c", "tokens": ["Es", "ist", "mir", "sp\u00e4t", "ge\u00b7wor\u00b7den", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "VAPP", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Nun sinkt er in die Kissen fest,", "tokens": ["Nun", "sinkt", "er", "in", "die", "Kis\u00b7sen", "fest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wirft ab die Kleider ganz durchn\u00e4\u00dft,", "tokens": ["Wirft", "ab", "die", "Klei\u00b7der", "ganz", "durc\u00b7hn\u00e4\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und schl\u00fcrft der Traube Segen.", "tokens": ["Und", "schl\u00fcrft", "der", "Trau\u00b7be", "Se\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ach Gott! nur wer jahraus, jahrein", "tokens": ["Ach", "Gott", "!", "nur", "wer", "ja\u00b7hraus", ",", "ja\u00b7hrein"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ITJ", "NN", "$.", "ADV", "PWS", "ADV", "$,", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "In And'rer Dienste lebt allein,", "tokens": ["In", "An\u00b7d'\u00b7rer", "Diens\u00b7te", "lebt", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ADV", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Wei\u00df was es hei\u00dft, bei'm Sonntagswein", "tokens": ["Wei\u00df", "was", "es", "hei\u00dft", ",", "bei'm", "Sonn\u00b7tags\u00b7wein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PWS", "PPER", "VVFIN", "$,", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sich auch ein wenig pflegen.", "tokens": ["Sich", "auch", "ein", "we\u00b7nig", "pfle\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "ART", "PIS", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}