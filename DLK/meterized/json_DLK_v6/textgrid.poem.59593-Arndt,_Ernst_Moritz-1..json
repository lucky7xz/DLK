{"textgrid.poem.59593": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Frischauf, ihr deutschen Br\u00fcder!", "tokens": ["Frisc\u00b7hauf", ",", "ihr", "deut\u00b7schen", "Br\u00fc\u00b7der", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Frischauf zum heil'gen Streit!", "tokens": ["Frisc\u00b7hauf", "zum", "heil'\u00b7gen", "Streit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Satan dr\u00fcckt uns nieder", "tokens": ["Der", "Sa\u00b7tan", "dr\u00fcckt", "uns", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und w\u00fctet weit und breit.", "tokens": ["Und", "w\u00fc\u00b7tet", "weit", "und", "breit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er will die Erdenflur", "tokens": ["Er", "will", "die", "Er\u00b7den\u00b7flur"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Zur Schlangenw\u00fcste machen,", "tokens": ["Zur", "Schlan\u00b7gen\u00b7w\u00fcs\u00b7te", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Mit Tigern und mit Drachen", "tokens": ["Mit", "Ti\u00b7gern", "und", "mit", "Dra\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Verheeren die Natur.", "tokens": ["Ver\u00b7hee\u00b7ren", "die", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Er will die Freiheit morden", "tokens": ["Er", "will", "die", "Frei\u00b7heit", "mor\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und brechen jedes Recht,", "tokens": ["Und", "bre\u00b7chen", "je\u00b7des", "Recht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Trug ist Herr geworden,", "tokens": ["Der", "Trug", "ist", "Herr", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "VAPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Es dient der Mut als Knecht,", "tokens": ["Es", "dient", "der", "Mut", "als", "Knecht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KOUS", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Wahrheit fliehet fern", "tokens": ["Die", "Wahr\u00b7heit", "flie\u00b7het", "fern"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Vom blutigen Get\u00fcmmel", "tokens": ["Vom", "blu\u00b7ti\u00b7gen", "Ge\u00b7t\u00fcm\u00b7mel"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Hoch in den lichten Himmel,", "tokens": ["Hoch", "in", "den", "lich\u00b7ten", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Sie klagt es Gott dem Herrn.", "tokens": ["Sie", "klagt", "es", "Gott", "dem", "Herrn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Drum auf, ihr deutschen Br\u00fcder!", "tokens": ["Drum", "auf", ",", "ihr", "deut\u00b7schen", "Br\u00fc\u00b7der", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "PTKVZ", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Es hat's der Herr geh\u00f6rt \u2013", "tokens": ["Es", "hat's", "der", "Herr", "ge\u00b7h\u00f6rt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auf! Schlagt die Schande nieder,", "tokens": ["Auf", "!", "Schlagt", "die", "Schan\u00b7de", "nie\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Recht und Licht zerst\u00f6rt!", "tokens": ["Die", "Recht", "und", "Licht", "zer\u00b7st\u00f6rt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Auf! Waffnet Herz und Hand", "tokens": ["Auf", "!", "Waff\u00b7net", "Herz", "und", "Hand"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "$.", "NN", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Mit alter deutscher Treue,", "tokens": ["Mit", "al\u00b7ter", "deut\u00b7scher", "Treu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Da\u00df Redlichkeit sich freue,", "tokens": ["Da\u00df", "Red\u00b7lich\u00b7keit", "sich", "freu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Da\u00df zittre Trug und Tand!", "tokens": ["Da\u00df", "zitt\u00b7re", "Trug", "und", "Tand", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Auf mit dem Herrn der Scharen!", "tokens": ["Auf", "mit", "dem", "Herrn", "der", "Scha\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wohlauf in Not und Tod!", "tokens": ["Wohl\u00b7auf", "in", "Not", "und", "Tod", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es wird euch wohl bewahren", "tokens": ["Es", "wird", "euch", "wohl", "be\u00b7wah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der alte treue Gott;", "tokens": ["Der", "al\u00b7te", "treu\u00b7e", "Gott", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Von ihm kommt alles her,", "tokens": ["Von", "ihm", "kommt", "al\u00b7les", "her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PIS", "PTKVZ", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Zu ihm geht alles wieder:", "tokens": ["Zu", "ihm", "geht", "al\u00b7les", "wie\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PIS", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Drum zagt nicht, deutsche Br\u00fcder!", "tokens": ["Drum", "zagt", "nicht", ",", "deut\u00b7sche", "Br\u00fc\u00b7der", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PTKNEG", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Gott steht mit euch im Heer.", "tokens": ["Gott", "steht", "mit", "euch", "im", "Heer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Gott steht mit euch im Leben,", "tokens": ["Gott", "steht", "mit", "euch", "im", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gott steht mit euch im Tod;", "tokens": ["Gott", "steht", "mit", "euch", "im", "Tod", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Will Gott den Arm erheben,", "tokens": ["Will", "Gott", "den", "Arm", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wo bleibet, was euch droht?", "tokens": ["Wo", "blei\u00b7bet", ",", "was", "euch", "droht", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mit Gott das Schwert zur Hand!", "tokens": ["Mit", "Gott", "das", "Schwert", "zur", "Hand", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Mit Gott hineingefallen!", "tokens": ["Mit", "Gott", "hin\u00b7ein\u00b7ge\u00b7fal\u00b7len", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und la\u00dft die Losung schallen:", "tokens": ["Und", "la\u00dft", "die", "Lo\u00b7sung", "schal\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}