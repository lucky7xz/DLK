{"dta.poem.7275": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Die Mergelgrube.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Sto\u00df deinen Scheit drei Spannen in den Sand,", "tokens": ["Sto\u00df", "dei\u00b7nen", "Scheit", "drei", "Span\u00b7nen", "in", "den", "Sand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "CARD", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Gesteine siehst du aus dem Schnitte ragen,", "tokens": ["Ge\u00b7stei\u00b7ne", "siehst", "du", "aus", "dem", "Schnit\u00b7te", "ra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Blau, gelb, zinnoberroth, als ob zur Gant", "tokens": ["Blau", ",", "gelb", ",", "zin\u00b7no\u00b7ber\u00b7roth", ",", "als", "ob", "zur", "Gant"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADJD", "$,", "VVFIN", "$,", "KOKOM", "KOUS", "APPRART", "NN"], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}, "line.4": {"text": "Natur die Tr\u00f6delbude aufgeschlagen.", "tokens": ["Na\u00b7tur", "die", "Tr\u00f6\u00b7del\u00b7bu\u00b7de", "auf\u00b7ge\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Kein Pardelfell war je so bunt gefleckt,", "tokens": ["Kein", "Par\u00b7del\u00b7fell", "war", "je", "so", "bunt", "ge\u00b7fleckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADV", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Kein Rebhuhn, keine Wachtel so gescheckt,", "tokens": ["Kein", "Reb\u00b7huhn", ",", "kei\u00b7ne", "Wach\u00b7tel", "so", "ge\u00b7scheckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Als das Ger\u00f6lle glei\u00dfend wie vom Schliff", "tokens": ["Als", "das", "Ge\u00b7r\u00f6l\u00b7le", "glei\u00b7\u00dfend", "wie", "vom", "Schliff"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJD", "KOKOM", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Sich aus der Scholle br\u00f6ckelt bei dem Griff", "tokens": ["Sich", "aus", "der", "Schol\u00b7le", "br\u00f6\u00b7ckelt", "bei", "dem", "Griff"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PRF", "APPR", "ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Der Hand, dem Scharren mit des Fu\u00dfes Spitze.", "tokens": ["Der", "Hand", ",", "dem", "Schar\u00b7ren", "mit", "des", "Fu\u00b7\u00dfes", "Spit\u00b7ze", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Wie z\u00fcrnend sturt dich an der schwarze Gneus,", "tokens": ["Wie", "z\u00fcr\u00b7nend", "sturt", "dich", "an", "der", "schwar\u00b7ze", "Gneus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Spatkugeln kollern nieder, milchig wei\u00df,", "tokens": ["Spat\u00b7ku\u00b7geln", "kol\u00b7lern", "nie\u00b7der", ",", "mil\u00b7chig", "wei\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKVZ", "$,", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Und um den Glimmer fahren Silberblitze;", "tokens": ["Und", "um", "den", "Glim\u00b7mer", "fah\u00b7ren", "Sil\u00b7ber\u00b7blit\u00b7ze", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Gesprenkelte Porphire, gro\u00df und klein,", "tokens": ["Ge\u00b7spren\u00b7kel\u00b7te", "Por\u00b7phi\u00b7re", ",", "gro\u00df", "und", "klein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Die Okerdruse und der Feuerstein \u2014", "tokens": ["Die", "O\u00b7ker\u00b7dru\u00b7se", "und", "der", "Feu\u00b7ers\u00b7tein"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Nur wenige hat dieser Grund gezeugt,", "tokens": ["Nur", "we\u00b7ni\u00b7ge", "hat", "die\u00b7ser", "Grund", "ge\u00b7zeugt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VAFIN", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Die zorn'ge Welle hat sie hergescheucht,", "tokens": ["Die", "zorn'\u00b7ge", "Wel\u00b7le", "hat", "sie", "her\u00b7ge\u00b7scheucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Leviathan mit seiner Riesenschuppe,", "tokens": ["Le\u00b7vi\u00b7a\u00b7than", "mit", "sei\u00b7ner", "Rie\u00b7sen\u00b7schup\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Als sch\u00e4umend \u00fcbern Sinai er fuhr,", "tokens": ["Als", "sch\u00e4u\u00b7mend", "\u00fc\u00b7bern", "Si\u00b7nai", "er", "fuhr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.19": {"text": "Des Himmels Schleusen drei\u00dfig Tage offen,", "tokens": ["Des", "Him\u00b7mels", "Schleu\u00b7sen", "drei\u00b7\u00dfig", "Ta\u00b7ge", "of\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "CARD", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Gebirge schmolzen ein wie Zuckerkand,", "tokens": ["Ge\u00b7bir\u00b7ge", "schmol\u00b7zen", "ein", "wie", "Zu\u00b7cker\u00b7kand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKVZ", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Als dann am Ararat die Arche stand,", "tokens": ["Als", "dann", "am", "Ar\u00b7a\u00b7rat", "die", "Ar\u00b7che", "stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Und, eine fremde, \u00fcppige Natur,", "tokens": ["Und", ",", "ei\u00b7ne", "frem\u00b7de", ",", "\u00fcp\u00b7pi\u00b7ge", "Na\u00b7tur", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "$,", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Ein neues Leben quoll aus neuen Stoffen. \u2014", "tokens": ["Ein", "neu\u00b7es", "Le\u00b7ben", "quoll", "aus", "neu\u00b7en", "Stof\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Findlinge nennt man sie, weil von der Brust,", "tokens": ["Find\u00b7lin\u00b7ge", "nennt", "man", "sie", ",", "weil", "von", "der", "Brust", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIS", "PPER", "$,", "KOUS", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.25": {"text": "Der m\u00fctterlichen sie gerissen sind,", "tokens": ["Der", "m\u00fct\u00b7ter\u00b7li\u00b7chen", "sie", "ge\u00b7ris\u00b7sen", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "In fremde Wiege schlummernd unbewu\u00dft,", "tokens": ["In", "frem\u00b7de", "Wie\u00b7ge", "schlum\u00b7mernd", "un\u00b7be\u00b7wu\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Die fremde Hand sie legt wie's Findelkind.", "tokens": ["Die", "frem\u00b7de", "Hand", "sie", "legt", "wie's", "Fin\u00b7del\u00b7kind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "O welch' ein Waisenhaus ist diese Haide,", "tokens": ["O", "welch'", "ein", "Wai\u00b7sen\u00b7haus", "ist", "die\u00b7se", "Hai\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PWAT", "ART", "NN", "VAFIN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Die Mohren, Bla\u00dfgesicht, und rothe Haut", "tokens": ["Die", "Moh\u00b7ren", ",", "Bla\u00df\u00b7ge\u00b7sicht", ",", "und", "ro\u00b7the", "Haut"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "NN", "$,", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "Gleichf\u00f6rmig h\u00fcllet mit dem braunen Kleide!", "tokens": ["Gleich\u00b7f\u00f6r\u00b7mig", "h\u00fcl\u00b7let", "mit", "dem", "brau\u00b7nen", "Klei\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Wie endlos ihre Zellenreihn gebaut!", "tokens": ["Wie", "end\u00b7los", "ih\u00b7re", "Zel\u00b7len\u00b7reihn", "ge\u00b7baut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Tief in's Gebr\u00f6ckel, in die Mergelgrube", "tokens": ["Tief", "in's", "Ge\u00b7br\u00f6\u00b7ckel", ",", "in", "die", "Mer\u00b7gel\u00b7gru\u00b7be"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "APPRART", "NN", "$,", "APPR", "ART", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "War ich gestiegen, denn der Wind zog scharf;", "tokens": ["War", "ich", "ge\u00b7stie\u00b7gen", ",", "denn", "der", "Wind", "zog", "scharf", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$,", "KON", "ART", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dort sa\u00df ich seitw\u00e4rts in der H\u00f6hlenstube,", "tokens": ["Dort", "sa\u00df", "ich", "seit\u00b7w\u00e4rts", "in", "der", "H\u00f6h\u00b7len\u00b7stu\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und horchte tr\u00e4umend auf der Luft Geharf.", "tokens": ["Und", "horch\u00b7te", "tr\u00e4u\u00b7mend", "auf", "der", "Luft", "Ge\u00b7harf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Es waren Kl\u00e4nge, wie wenn Geisterhall", "tokens": ["Es", "wa\u00b7ren", "Kl\u00e4n\u00b7ge", ",", "wie", "wenn", "Geis\u00b7ter\u00b7hall"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "NN", "$,", "KOKOM", "KOUS", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Melodisch schwinde im zerst\u00f6rten All;", "tokens": ["Me\u00b7lo\u00b7disch", "schwin\u00b7de", "im", "zer\u00b7st\u00f6r\u00b7ten", "All", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und dann ein Zischen, wie von Moores Klaffen,", "tokens": ["Und", "dann", "ein", "Zi\u00b7schen", ",", "wie", "von", "Moo\u00b7res", "Klaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$,", "PWAV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wenn brodelnd es in sich zusamm'gesunken;", "tokens": ["Wenn", "bro\u00b7delnd", "es", "in", "sich", "zu\u00b7sam\u00b7m'\u00b7ge\u00b7sun\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPER", "APPR", "PRF", "VVINF", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Mir \u00fcber'm Haupt ein Rispeln und ein Schaffen,", "tokens": ["Mir", "\u00fc\u00b7ber'm", "Haupt", "ein", "Ris\u00b7peln", "und", "ein", "Schaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Als scharre in der Asche man den Funken.", "tokens": ["Als", "schar\u00b7re", "in", "der", "A\u00b7sche", "man", "den", "Fun\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "APPR", "ART", "NN", "PIS", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Findlinge zog ich St\u00fcck auf St\u00fcck hervor,", "tokens": ["Find\u00b7lin\u00b7ge", "zog", "ich", "St\u00fcck", "auf", "St\u00fcck", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "NN", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Und lauschte, lauschte mit berauschtem Ohr.", "tokens": ["Und", "lauschte", ",", "lauschte", "mit", "be\u00b7rauschtem", "Ohr", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Vor mir, um mich der graue Mergel nur,", "tokens": ["Vor", "mir", ",", "um", "mich", "der", "grau\u00b7e", "Mer\u00b7gel", "nur", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "KOUI", "PRF", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Was dr\u00fcber sah ich nicht; doch die Natur", "tokens": ["Was", "dr\u00fc\u00b7ber", "sah", "ich", "nicht", ";", "doch", "die", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "PAV", "VVFIN", "PPER", "PTKNEG", "$.", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Schien mir ver\u00f6det, und ein Bild erstand", "tokens": ["Schien", "mir", "ver\u00b7\u00f6\u00b7det", ",", "und", "ein", "Bild", "er\u00b7stand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "VVPP", "$,", "KON", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Von einer Erde, m\u00fcrbe, ausgebrannt;", "tokens": ["Von", "ei\u00b7ner", "Er\u00b7de", ",", "m\u00fcr\u00b7be", ",", "aus\u00b7ge\u00b7brannt", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADJA", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ich selber schien ein Funken mir, der doch", "tokens": ["Ich", "sel\u00b7ber", "schien", "ein", "Fun\u00b7ken", "mir", ",", "der", "doch"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "ART", "NN", "PPER", "$,", "PRELS", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"te