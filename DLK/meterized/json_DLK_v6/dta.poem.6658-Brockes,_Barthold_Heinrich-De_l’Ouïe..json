{"dta.poem.6658": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "De l\u2019Ou\u00efe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Allein wir stellen itzt die Untersuchung ein,", "tokens": ["Al\u00b7lein", "wir", "stel\u00b7len", "itzt", "die", "Un\u00b7ter\u00b7su\u00b7chung", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wollen vom Gebrauch der gr\u00f6bern Sinne schweigen,", "tokens": ["Und", "wol\u00b7len", "vom", "Ge\u00b7brauch", "der", "gr\u00f6\u00b7bern", "Sin\u00b7ne", "schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPRART", "NN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wir werden mehr zufrieden seyn", "tokens": ["Wir", "wer\u00b7den", "mehr", "zu\u00b7frie\u00b7den", "seyn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "VAINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit W\u00fcrckungen, die weniger gemein,", "tokens": ["Mit", "W\u00fcr\u00b7ckun\u00b7gen", ",", "die", "we\u00b7ni\u00b7ger", "ge\u00b7mein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "ADV", "ADJD", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Und welche das ", "tokens": ["Und", "wel\u00b7che", "das"], "token_info": ["word", "word", "word"], "pos": ["KON", "PRELS", "ART"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Dies sind die edelsten von allen Sinnen, auch", "tokens": ["Dies", "sind", "die", "e\u00b7dels\u00b7ten", "von", "al\u00b7len", "Sin\u00b7nen", ",", "auch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "APPR", "PIAT", "NN", "$,", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die fertigst-sichersten und schnellsten im Gebrauch:", "tokens": ["Die", "fer\u00b7tigst\u00b7si\u00b7chers\u00b7ten", "und", "schnells\u00b7ten", "im", "Ge\u00b7brauch", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KON", "VVFIN", "APPRART", "NN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Von Kunst und Wissenschafft die besten Instrumente,", "tokens": ["Von", "Kunst", "und", "Wis\u00b7sen\u00b7schafft", "die", "bes\u00b7ten", "Inst\u00b7ru\u00b7men\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wodurch wir den Begriff aus allen Dingen ziehn.", "tokens": ["Wo\u00b7durch", "wir", "den", "Be\u00b7griff", "aus", "al\u00b7len", "Din\u00b7gen", "ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wenn wir die Welt-Weisheit nun treiben,", "tokens": ["Wenn", "wir", "die", "Welt\u00b7Weis\u00b7heit", "nun", "trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "So lasst uns auch dahin bem\u00fchn,", "tokens": ["So", "lasst", "uns", "auch", "da\u00b7hin", "be\u00b7m\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PAV", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Sie recht zu kennen, zu beschreiben!", "tokens": ["Sie", "recht", "zu", "ken\u00b7nen", ",", "zu", "be\u00b7schrei\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}