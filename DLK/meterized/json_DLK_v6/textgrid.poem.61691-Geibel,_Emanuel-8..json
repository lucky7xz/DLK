{"textgrid.poem.61691": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "8.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So bist du's wieder,", "tokens": ["So", "bist", "du's", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "ADV", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Vertrauter Raum?", "tokens": ["Ver\u00b7trau\u00b7ter", "Raum", "?"], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Die Jahre schwanden,", "tokens": ["Die", "Jah\u00b7re", "schwan\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Mir ist's, wie Traum.", "tokens": ["Mir", "ist's", ",", "wie", "Traum", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PWAV", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Die Jahre schwanden,", "tokens": ["Die", "Jah\u00b7re", "schwan\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Seitdem voll Gram", "tokens": ["Seit\u00b7dem", "voll", "Gram"], "token_info": ["word", "word", "word"], "pos": ["PAV", "ADJD", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Auf jenen Stufen", "tokens": ["Auf", "je\u00b7nen", "Stu\u00b7fen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PDAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Ich Abschied nahm.", "tokens": ["Ich", "Ab\u00b7schied", "nahm", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Noch zieht, wie damals,", "tokens": ["Noch", "zieht", ",", "wie", "da\u00b7mals", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PWAV", "ADV", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Im Tal der Flu\u00df,", "tokens": ["Im", "Tal", "der", "Flu\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Es rauscht der Garten", "tokens": ["Es", "rauscht", "der", "Gar\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Mir seinen Gru\u00df;", "tokens": ["Mir", "sei\u00b7nen", "Gru\u00df", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Am Fenster gr\u00fcnt noch", "tokens": ["Am", "Fens\u00b7ter", "gr\u00fcnt", "noch"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ADV"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Der Reben Kranz \u2013", "tokens": ["Der", "Re\u00b7ben", "Kranz", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Nur wir, wie sind wir", "tokens": ["Nur", "wir", ",", "wie", "sind", "wir"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PPER", "$,", "PWAV", "VAFIN", "PPER"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Verwandelt ganz!", "tokens": ["Ver\u00b7wan\u00b7delt", "ganz", "!"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Die wir uns bauten", "tokens": ["Die", "wir", "uns", "bau\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPER", "PPER", "VVFIN"], "meter": "++-+-", "measure": "iambic.di"}, "line.2": {"text": "Mit k\u00fchnem Sinn,", "tokens": ["Mit", "k\u00fch\u00b7nem", "Sinn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Die goldnen Schl\u00f6sser,", "tokens": ["Die", "gold\u00b7nen", "Schl\u00f6s\u00b7ser", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Wo sind sie hin!", "tokens": ["Wo", "sind", "sie", "hin", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Die goldnen Tr\u00e4ume,", "tokens": ["Die", "gold\u00b7nen", "Tr\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Von Lieb' und Lust \u2013", "tokens": ["Von", "Lieb'", "und", "Lust", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und doch, was wogst du,", "tokens": ["Und", "doch", ",", "was", "wogst", "du", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PWS", "VVFIN", "PPER", "$,"], "meter": "-+---", "measure": "dactylic.init"}, "line.4": {"text": "Beklemmte Brust?", "tokens": ["Be\u00b7klemm\u00b7te", "Brust", "?"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}