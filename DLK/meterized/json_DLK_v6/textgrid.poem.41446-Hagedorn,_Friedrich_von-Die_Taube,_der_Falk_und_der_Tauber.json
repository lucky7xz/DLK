{"textgrid.poem.41446": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Die Taube, der Falk und der Tauber", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Blaufu\u00df steigt zum neuen Raube", "tokens": ["Ein", "Blau\u00b7fu\u00df", "steigt", "zum", "neu\u00b7en", "Rau\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Aus Nest und Wald empor, reviert in hoher Luft,", "tokens": ["Aus", "Nest", "und", "Wald", "em\u00b7por", ",", "re\u00b7viert", "in", "ho\u00b7her", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "PTKVZ", "$,", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Beschauet Berg und Thal, und sieht in einer Gruft", "tokens": ["Be\u00b7schau\u00b7et", "Berg", "und", "Thal", ",", "und", "sieht", "in", "ei\u00b7ner", "Gruft"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "NN", "KON", "NN", "$,", "KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Des treuen Taubers Lust, die sch\u00f6nste Turteltaube.", "tokens": ["Des", "treu\u00b7en", "Tau\u00b7bers", "Lust", ",", "die", "sch\u00f6ns\u00b7te", "Tur\u00b7tel\u00b7tau\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Auf sie st\u00f6\u00dft er herab, erreicht, und greift sie bald,", "tokens": ["Auf", "sie", "st\u00f6\u00dft", "er", "her\u00b7ab", ",", "er\u00b7reicht", ",", "und", "greift", "sie", "bald", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "ADV", "$,", "VVPP", "$,", "KON", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "Und ist schon im Begriff, die Arme zu zerrei\u00dfen,", "tokens": ["Und", "ist", "schon", "im", "Be\u00b7griff", ",", "die", "Ar\u00b7me", "zu", "zer\u00b7rei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "APPRART", "NN", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Als sie ihn girrend fragt: Wird dieses Siegen hei\u00dfen,", "tokens": ["Als", "sie", "ihn", "gir\u00b7rend", "fragt", ":", "Wird", "die\u00b7ses", "Sie\u00b7gen", "hei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "VVFIN", "$.", "VAFIN", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So man nicht k\u00e4mpft? Erweise die Gewalt;", "tokens": ["So", "man", "nicht", "k\u00e4mpft", "?", "Er\u00b7wei\u00b7se", "die", "Ge\u00b7walt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "PTKNEG", "VVFIN", "$.", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Doch nicht an mir, die ich vor Schrecken sterbe.", "tokens": ["Doch", "nicht", "an", "mir", ",", "die", "ich", "vor", "Schre\u00b7cken", "ster\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPR", "PPER", "$,", "PRELS", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Nein, da\u00df dein Muth den rechten Sieg erwerbe,", "tokens": ["Nein", ",", "da\u00df", "dein", "Muth", "den", "rech\u00b7ten", "Sieg", "er\u00b7wer\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KOUS", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "So falle nur den gro\u00dfen Reiher an,", "tokens": ["So", "fal\u00b7le", "nur", "den", "gro\u00b7\u00dfen", "Rei\u00b7her", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Den Adler selbst, und was sich wehren kann.", "tokens": ["Den", "Ad\u00b7ler", "selbst", ",", "und", "was", "sich", "weh\u00b7ren", "kann", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "KON", "PWS", "PRF", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "So wehre dich! versetzt der Falk, und dr\u00e4ute:", "tokens": ["So", "weh\u00b7re", "dich", "!", "ver\u00b7setzt", "der", "Falk", ",", "und", "dr\u00e4u\u00b7te", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "VVFIN", "ART", "NN", "$,", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So wehre dich! ist nicht dein Schnabel gnug zum Streite?", "tokens": ["So", "weh\u00b7re", "dich", "!", "ist", "nicht", "dein", "Schna\u00b7bel", "gnug", "zum", "Strei\u00b7te", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "VAFIN", "PTKNEG", "PPOSAT", "NN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Zum Streite? wie? er kennt nicht Streit noch Groll,", "tokens": ["Zum", "Strei\u00b7te", "?", "wie", "?", "er", "kennt", "nicht", "Streit", "noch", "Groll", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$.", "PWAV", "$.", "PPER", "VVFIN", "PTKNEG", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und hat nicht Kraft, als wann er schn\u00e4beln soll.", "tokens": ["Und", "hat", "nicht", "Kraft", ",", "als", "wann", "er", "schn\u00e4\u00b7beln", "soll", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PTKNEG", "NN", "$,", "KOUS", "PWAV", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es ist sein Sto\u00df, die Regung sanfter Triebe,", "tokens": ["Es", "ist", "sein", "Sto\u00df", ",", "die", "Re\u00b7gung", "sanf\u00b7ter", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nichts, als ein Spiel, ein Reiz, ein Ku\u00df der Liebe", "tokens": ["Nichts", ",", "als", "ein", "Spiel", ",", "ein", "Reiz", ",", "ein", "Ku\u00df", "der", "Lie\u00b7be"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "$,", "KOUS", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "F\u00fcr meinen Freund .... Und wer ist dieser Freund?", "tokens": ["F\u00fcr", "mei\u00b7nen", "Freund", "....", "Und", "wer", "ist", "die\u00b7ser", "Freund", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$.", "KON", "PWS", "VAFIN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Mein Tauber ist's: er schl\u00e4ft auf einem Zweige ...", "tokens": ["Mein", "Tau\u00b7ber", "ist's", ":", "er", "schl\u00e4ft", "auf", "ei\u00b7nem", "Zwei\u00b7ge", "..."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "$.", "PPER", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Man weck' ihn auf: es ist dein Held mein Feind.", "tokens": ["Man", "weck'", "ihn", "auf", ":", "es", "ist", "dein", "Held", "mein", "Feind", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PTKVZ", "$.", "PPER", "VAFIN", "PPOSAT", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Dir steh' er bei: ich will, da\u00df er sich zeige.", "tokens": ["Dir", "steh'", "er", "bei", ":", "ich", "will", ",", "da\u00df", "er", "sich", "zei\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$.", "PPER", "VMFIN", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Das T\u00e4ubchen seufzt. Ach nein, ich bitte, nein!", "tokens": ["Das", "T\u00e4ub\u00b7chen", "seufzt", ".", "Ach", "nein", ",", "ich", "bit\u00b7te", ",", "nein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "NN", "PTKANT", "$,", "PPER", "ADV", "$,", "PTKANT", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sonst w\u00fcrde nur mein Jammer gr\u00f6\u00dfer sein.", "tokens": ["Sonst", "w\u00fcr\u00b7de", "nur", "mein", "Jam\u00b7mer", "gr\u00f6\u00b7\u00dfer", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PPOSAT", "NN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Noch seufzet sie, und schnell erwacht der Gatte,", "tokens": ["Noch", "seuf\u00b7zet", "sie", ",", "und", "schnell", "er\u00b7wacht", "der", "Gat\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KON", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Er fliegt von selbst dahin, wo sie der R\u00e4uber hatte.", "tokens": ["Er", "fliegt", "von", "selbst", "da\u00b7hin", ",", "wo", "sie", "der", "R\u00e4u\u00b7ber", "hat\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADV", "PAV", "$,", "PWAV", "PPER", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mit ihr scheint auch der Tod ihm vor dem W\u00fcrger sch\u00f6n:", "tokens": ["Mit", "ihr", "scheint", "auch", "der", "Tod", "ihm", "vor", "dem", "W\u00fcr\u00b7ger", "sch\u00f6n", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ADV", "ART", "NN", "PPER", "APPR", "ART", "NN", "ADJD", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "Sie sterben Hals an Hals, da der den Mord ver\u00fcbte.", "tokens": ["Sie", "ster\u00b7ben", "Hals", "an", "Hals", ",", "da", "der", "den", "Mord", "ver\u00b7\u00fcb\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "NN", "$,", "KOUS", "ART", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Die hei\u00dfe Liebe sieht auf nichts, als das Geliebte;", "tokens": ["Die", "hei\u00b7\u00dfe", "Lie\u00b7be", "sieht", "auf", "nichts", ",", "als", "das", "Ge\u00b7lieb\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PIS", "$,", "KOUS", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die kluge h\u00e4tte nur auf die Gefahr gesehn.", "tokens": ["Die", "klu\u00b7ge", "h\u00e4t\u00b7te", "nur", "auf", "die", "Ge\u00b7fahr", "ge\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}}}}}