{"dta.poem.19006": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "F\u00fcr ermeltes Balleth.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-200905198111", "language": ["de:0.99"], "booktitle": "Weckherlin, Georg Rodolf: Gaistliche und Weltliche Gedichte. Amsterdam, 1641."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr Nymfen/ deren blick mit wunderbarem schein", "tokens": ["Ihr", "Nym\u00b7fen", "/", "de\u00b7ren", "blick", "mit", "wun\u00b7der\u00b7ba\u00b7rem", "schein"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$(", "PRELAT", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kan vnser hertz zugleich erlaben vnd versehren/", "tokens": ["Kan", "vn\u00b7ser", "hertz", "zu\u00b7gleich", "er\u00b7la\u00b7ben", "vnd", "ver\u00b7seh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "ADV", "VVINF", "KON", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vnd deren angesicht/ ein spiegel aller ehren/", "tokens": ["Vnd", "de\u00b7ren", "an\u00b7ge\u00b7sicht", "/", "ein", "spie\u00b7gel", "al\u00b7ler", "eh\u00b7ren", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVPP", "$(", "ART", "NN", "PIAT", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Erf\u00fcllet Vns mit forcht/ mit hofnung/ lust vnnd", "tokens": ["Er\u00b7f\u00fcl\u00b7let", "Vns", "mit", "forcht", "/", "mit", "hof\u00b7nung", "/", "lust", "vnnd"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "VVFIN", "$(", "APPR", "NN", "$(", "VVFIN", "KON"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "pein:", "tokens": ["pein", ":"], "token_info": ["word", "punct"], "pos": ["PTKVZ", "$."], "meter": "+", "measure": "single.up"}}, "stanza.2": {"line.1": {"text": "Wir bringen vnsern Kram von Spieglen klar vnd", "tokens": ["Wir", "brin\u00b7gen", "vn\u00b7sern", "Kram", "von", "Spieg\u00b7len", "klar", "vnd"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "ADJD", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "rein/", "tokens": ["rein", "/"], "token_info": ["word", "punct"], "pos": ["ADJD", "$("], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Mit bit/ jhr wollet euch zu spieglen nicht beschwere\u0304;", "tokens": ["Mit", "bit", "/", "jhr", "wol\u00b7let", "euch", "zu", "spieg\u00b7len", "nicht", "be\u00b7schwer\u0113", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$(", "PPER", "VMFIN", "PPER", "PTKZU", "VVINF", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die spiegel/ die so klar Vns ewre sch\u00f6nheit lehren/", "tokens": ["Die", "spie\u00b7gel", "/", "die", "so", "klar", "Vns", "ew\u00b7re", "sch\u00f6n\u00b7heit", "leh\u00b7ren", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "ADV", "ADJD", "PPER", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die lehren euch zumahl barmhertziger zu sein.", "tokens": ["Die", "leh\u00b7ren", "euch", "zu\u00b7mahl", "barm\u00b7hert\u00b7zi\u00b7ger", "zu", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "ADJD", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wol. So belieb es Euch mit lieblichen anblicken", "tokens": ["Wol", ".", "So", "be\u00b7lieb", "es", "Euch", "mit", "lieb\u00b7li\u00b7chen", "an\u00b7bli\u00b7cken"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$.", "ADV", "VVFIN", "PPER", "PRF", "APPR", "ADJA", "NN"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Erleuchtend freindlich Vns vnd vnsern leichten", "tokens": ["Er\u00b7leuch\u00b7tend", "freind\u00b7lich", "Vns", "vnd", "vn\u00b7sern", "leich\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "ADJD", "NN", "KON", "PPOSAT", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dantz", "tokens": ["Dantz"], "token_info": ["word"], "pos": ["NE"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Vnd spieglend Euch zumahl in Vns/ Vns zu", "tokens": ["Vnd", "spieg\u00b7lend", "Euch", "zu\u00b7mahl", "in", "Vns", "/", "Vns", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADJD", "PPER", "ADV", "APPR", "NE", "$(", "PPER", "PTKZU"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "erquicken;", "tokens": ["er\u00b7qui\u00b7cken", ";"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.4": {"line.1": {"text": "Solt aber vngefehr Vns ewrer Sch\u00f6nheit glantz/", "tokens": ["Solt", "a\u00b7ber", "vn\u00b7ge\u00b7fehr", "Vns", "ew\u00b7rer", "Sch\u00f6n\u00b7heit", "glantz", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADJD", "PPER", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vnnd ewrer haaren schein verblinden vnnd ver-", "tokens": ["Vnnd", "ew\u00b7rer", "haa\u00b7ren", "schein", "ver\u00b7blin\u00b7den", "vnnd", "ver"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "ADJD", "VVINF", "KON", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "stricken/", "tokens": ["stri\u00b7cken", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "So tr\u00f6ste beederseits Euch der Krantz/ Vns die", "tokens": ["So", "tr\u00f6s\u00b7te", "bee\u00b7der\u00b7seits", "Euch", "der", "Krantz", "/", "Vns", "die"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "ART", "NN", "$(", "PPER", "ART"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Schantz.", "tokens": ["Schantz", "."], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+", "measure": "single.up"}}}}}