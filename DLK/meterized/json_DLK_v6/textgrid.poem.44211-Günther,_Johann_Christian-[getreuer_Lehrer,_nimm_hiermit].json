{"textgrid.poem.44211": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "[getreuer Lehrer, nimm hiermit]", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Getreuer Lehrer, nimm hiermit", "tokens": ["Ge\u00b7treu\u00b7er", "Leh\u00b7rer", ",", "nimm", "hier\u00b7mit"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "NN", "$,", "VVIMP", "PAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den schlechten Danck von armen H\u00e4nden;", "tokens": ["Den", "schlech\u00b7ten", "Danck", "von", "ar\u00b7men", "H\u00e4n\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Denn da mich alles niedertritt,", "tokens": ["Denn", "da", "mich", "al\u00b7les", "nie\u00b7der\u00b7tritt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So kan ich nichts als Worte senden.", "tokens": ["So", "kan", "ich", "nichts", "als", "Wor\u00b7te", "sen\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PIS", "KOKOM", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Nur Worte? Nein, das Herz dabey,", "tokens": ["Nur", "Wor\u00b7te", "?", "Nein", ",", "das", "Herz", "da\u00b7bey", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$.", "PTKANT", "$,", "ART", "NN", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das Herz, in dem sich Groll und Reu,", "tokens": ["Das", "Herz", ",", "in", "dem", "sich", "Groll", "und", "Reu", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PRELS", "PRF", "ADJD", "KON", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Verzweiflung, Ha\u00df und Rachlust schlagen,", "tokens": ["Ver\u00b7zwei\u00b7flung", ",", "Ha\u00df", "und", "Rach\u00b7lust", "schla\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Da fremder Neid und eigner Sinn", "tokens": ["Da", "frem\u00b7der", "Neid", "und", "eig\u00b7ner", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Dem, de\u00dfen Fleisch und Blut ich bin,", "tokens": ["Dem", ",", "de\u00b7\u00dfen", "Fleisch", "und", "Blut", "ich", "bin", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELAT", "NN", "KON", "NN", "PPER", "VAFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.10": {"text": "Den Vater aus dem Herzen jagen.", "tokens": ["Den", "Va\u00b7ter", "aus", "dem", "Her\u00b7zen", "ja\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Verwegenheit thut ofters mehr,", "tokens": ["Ver\u00b7we\u00b7gen\u00b7heit", "thut", "of\u00b7ters", "mehr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zumahl bey schon verderbtem Leben,", "tokens": ["Zu\u00b7mahl", "bey", "schon", "ver\u00b7derb\u00b7tem", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Als wenn wir der Vernunft Geh\u00f6r", "tokens": ["Als", "wenn", "wir", "der", "Ver\u00b7nunft", "Ge\u00b7h\u00f6r"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und fauler Klugheit Zutritt geben.", "tokens": ["Und", "fau\u00b7ler", "Klug\u00b7heit", "Zu\u00b7tritt", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "O h\u00e4tt ich dies nur l\u00e4ngst gethan!", "tokens": ["O", "h\u00e4tt", "ich", "dies", "nur", "l\u00e4ngst", "ge\u00b7than", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "PDS", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Es kommt doch auf die Vorsicht an,", "tokens": ["Es", "kommt", "doch", "auf", "die", "Vor\u00b7sicht", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die unsern Sinn nothwendig lencket.", "tokens": ["Die", "un\u00b7sern", "Sinn", "noth\u00b7wen\u00b7dig", "len\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Vielleicht verfolgt ich jezt die Schaar,", "tokens": ["Viel\u00b7leicht", "ver\u00b7folgt", "ich", "jezt", "die", "Schaar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Die mich bey eu\u00dferster Gefahr", "tokens": ["Die", "mich", "bey", "eu\u00b7\u00dfers\u00b7ter", "Ge\u00b7fahr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.10": {"text": "So sinnreich mit Verleumdung kr\u00e4ncket.", "tokens": ["So", "sinn\u00b7reich", "mit", "Ver\u00b7leum\u00b7dung", "kr\u00e4n\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Nun mag ich dich mit H\u00fclf und Rath", "tokens": ["Nun", "mag", "ich", "dich", "mit", "H\u00fclf", "und", "Rath"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und um kein Mitleid mehr beschweren.", "tokens": ["Und", "um", "kein", "Mit\u00b7leid", "mehr", "be\u00b7schwe\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dies bitt ich: Wird mir eine That", "tokens": ["Dies", "bitt", "ich", ":", "Wird", "mir", "ei\u00b7ne", "That"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "$.", "VAFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des P\u00f6bels Ha\u00df und Fluch geb\u00e4hren,", "tokens": ["Des", "P\u00f6\u00b7bels", "Ha\u00df", "und", "Fluch", "ge\u00b7b\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "So rede weder schlimm noch gut", "tokens": ["So", "re\u00b7de", "we\u00b7der", "schlimm", "noch", "gut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "KON", "ADJD", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und sch\u00e4ze nur mein redlich Blut.", "tokens": ["Und", "sch\u00e4\u00b7ze", "nur", "mein", "red\u00b7lich", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "In \u00fcbrigen Gewi\u00dfenssachen", "tokens": ["In", "\u00fcb\u00b7ri\u00b7gen", "Ge\u00b7wi\u00b7\u00dfens\u00b7sa\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Darf doch kein andrer vor mich stehn.", "tokens": ["Darf", "doch", "kein", "an\u00b7drer", "vor", "mich", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PIAT", "PIS", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Wem Fluthen durch das Sto\u00dfbret gehn,", "tokens": ["Wem", "Flut\u00b7hen", "durch", "das", "Sto\u00df\u00b7bret", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Der mu\u00df Verlust zu Vortheil machen.", "tokens": ["Der", "mu\u00df", "Ver\u00b7lust", "zu", "Vor\u00b7theil", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}