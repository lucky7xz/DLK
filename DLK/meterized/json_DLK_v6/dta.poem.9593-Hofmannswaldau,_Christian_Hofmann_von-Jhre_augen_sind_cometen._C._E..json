{"dta.poem.9593": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Jhre augen sind cometen.  \n C. E.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.57", "da:0.42"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Man sagt: Lisette sey dem himmel anverwandt/", "tokens": ["Man", "sagt", ":", "Li\u00b7set\u00b7te", "sey", "dem", "him\u00b7mel", "an\u00b7ver\u00b7wandt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$.", "NE", "VAFIN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Jhr antlitz zeige nur die sch\u00f6nsten sonnen-blicke/", "tokens": ["Ihr", "ant\u00b7litz", "zei\u00b7ge", "nur", "die", "sch\u00f6ns\u00b7ten", "son\u00b7nen\u00b7bli\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Jhr auge wi\u00dfe nichts von schelmerey und t\u00fccke/", "tokens": ["Ihr", "au\u00b7ge", "wi\u00b7\u00dfe", "nichts", "von", "schel\u00b7me\u00b7rey", "und", "t\u00fc\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PIS", "APPR", "ADJD", "KON", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und was entz\u00fcndung hei\u00dft/ sey ihr noch unbekandt/", "tokens": ["Und", "was", "ent\u00b7z\u00fcn\u00b7dung", "hei\u00dft", "/", "sey", "ihr", "noch", "un\u00b7be\u00b7kandt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "NN", "VVFIN", "$(", "VAFIN", "PPER", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sie kenne keinen ha\u00df/ sie wisse nichts von plagen/", "tokens": ["Sie", "ken\u00b7ne", "kei\u00b7nen", "ha\u00df", "/", "sie", "wis\u00b7se", "nichts", "von", "pla\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$(", "PPER", "VVFIN", "PIS", "APPR", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Noch weniger von stoltz und grausamkeit zu sagen.", "tokens": ["Noch", "we\u00b7ni\u00b7ger", "von", "stoltz", "und", "grau\u00b7sam\u00b7keit", "zu", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NE", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Gewi\u00df! di\u00df alles hegt zwar einen g\u00fcldnen schein/", "tokens": ["Ge\u00b7wi\u00df", "!", "di\u00df", "al\u00b7les", "hegt", "zwar", "ei\u00b7nen", "g\u00fcld\u00b7nen", "schein", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PDS", "PIS", "VVFIN", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und will den sichren schlu\u00df uns ins ged\u00e4chtn\u00fcs binden/", "tokens": ["Und", "will", "den", "sich\u00b7ren", "schlu\u00df", "uns", "ins", "ge\u00b7d\u00e4cht\u00b7n\u00fcs", "bin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "ADJA", "NN", "PPER", "APPRART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wir werden lauter gl\u00fcck in ihrem umbgang finden/", "tokens": ["Wir", "wer\u00b7den", "lau\u00b7ter", "gl\u00fcck", "in", "ih\u00b7rem", "umb\u00b7gang", "fin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und niemals ohne trost und s\u00fcsse hoffnung seyn:", "tokens": ["Und", "nie\u00b7mals", "oh\u00b7ne", "trost", "und", "s\u00fcs\u00b7se", "hoff\u00b7nung", "seyn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "KON", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die liebe werde sich mit unsrer lust verm\u00e4hlen/", "tokens": ["Die", "lie\u00b7be", "wer\u00b7de", "sich", "mit", "uns\u00b7rer", "lust", "ver\u00b7m\u00e4h\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "PRF", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und die vergn\u00fcgung uns zum nechsten nachbar wehlen.", "tokens": ["Und", "die", "ver\u00b7gn\u00fc\u00b7gung", "uns", "zum", "nechs\u00b7ten", "nach\u00b7bar", "weh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPER", "APPRART", "ADJA", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ach! aber weit gefehlt/ es ist umb uns gethan/", "tokens": ["Ach", "!", "a\u00b7ber", "weit", "ge\u00b7fehlt", "/", "es", "ist", "umb", "uns", "ge\u00b7than", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "ADJD", "VVPP", "$(", "PPER", "VAFIN", "APPR", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wofern wir auf den stein des blo\u00dfen ansehns bauen/", "tokens": ["Wo\u00b7fern", "wir", "auf", "den", "stein", "des", "blo\u00b7\u00dfen", "an\u00b7sehns", "bau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und ihren blicken uns und unsre wohlfahrt trauen/", "tokens": ["Und", "ih\u00b7ren", "bli\u00b7cken", "uns", "und", "uns\u00b7re", "wohl\u00b7fahrt", "trau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "VVFIN", "PPER", "KON", "PPOSAT", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Was vor ver\u00e4ndrung wird man nicht in kurtzem sehn?", "tokens": ["Was", "vor", "ver\u00b7\u00e4n\u00b7drung", "wird", "man", "nicht", "in", "kurt\u00b7zem", "sehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NN", "VAFIN", "PIS", "PTKNEG", "APPR", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wo wird alsdenn der trost/ wo wird die hoffnung bleiben/", "tokens": ["Wo", "wird", "als\u00b7denn", "der", "trost", "/", "wo", "wird", "die", "hoff\u00b7nung", "blei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ADV", "ART", "NN", "$(", "PWAV", "VAFIN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wenn nun Lisette wird ihr lust-spiel mit uns treiben?", "tokens": ["Wenn", "nun", "Li\u00b7set\u00b7te", "wird", "ihr", "lust\u00b7spiel", "mit", "uns", "trei\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "NE", "VAFIN", "PPOSAT", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Zwar steh ichs selber zu: Sie ist dem himmel gleich/", "tokens": ["Zwar", "steh", "ichs", "sel\u00b7ber", "zu", ":", "Sie", "ist", "dem", "him\u00b7mel", "gleich", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "PTKVZ", "$.", "PPER", "VAFIN", "ART", "NN", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem himmel aber nur/ wenn er in wolcken blitzet/", "tokens": ["Dem", "him\u00b7mel", "a\u00b7ber", "nur", "/", "wenn", "er", "in", "wol\u00b7cken", "blit\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "$(", "KOUS", "PPER", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wenn er in schwartzer lufft die scharffen pfeile spitzet/", "tokens": ["Wenn", "er", "in", "schwart\u00b7zer", "lufft", "die", "scharf\u00b7fen", "pfei\u00b7le", "spit\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und zum gehorsam treibt der erden weites reich:", "tokens": ["Und", "zum", "ge\u00b7hor\u00b7sam", "treibt", "der", "er\u00b7den", "wei\u00b7tes", "reich", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "ADJD", "VVFIN", "ART", "NN", "ADJA", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "O weh! der armen welt/ wie wird die freude schwinden/", "tokens": ["O", "weh", "!", "der", "ar\u00b7men", "welt", "/", "wie", "wird", "die", "freu\u00b7de", "schwin\u00b7den", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$.", "ART", "ADJA", "NN", "$(", "KOKOM", "VAFIN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wenn man statt sonnen wird cometen vor sich finden.", "tokens": ["Wenn", "man", "statt", "son\u00b7nen", "wird", "co\u00b7me\u00b7ten", "vor", "sich", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "VVINF", "VAFIN", "ADJD", "APPR", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Heult dann/ ihr junges volck/ eur ende naht herbey/", "tokens": ["Heult", "dann", "/", "ihr", "jun\u00b7ges", "volck", "/", "eur", "en\u00b7de", "naht", "her\u00b7bey", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$(", "PPOSAT", "ADJA", "NN", "$(", "PPOSAT", "NN", "VVFIN", "PTKVZ", "$("], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Lisettens auge will nur lauter unfall dr\u00e4uen/", "tokens": ["Li\u00b7set\u00b7tens", "au\u00b7ge", "will", "nur", "lau\u00b7ter", "un\u00b7fall", "dr\u00e4u\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VMFIN", "ADV", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und nichts als feur und krieg/ und sterben prophezeyen.", "tokens": ["Und", "nichts", "als", "feur", "und", "krieg", "/", "und", "ster\u00b7ben", "pro\u00b7phe\u00b7ze\u00b7yen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "KOKOM", "ADJD", "KON", "NN", "$(", "KON", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Jtzt! itzt! entdeckt sie erst und lehrt uns was sie sey:", "tokens": ["Jtzt", "!", "itzt", "!", "ent\u00b7deckt", "sie", "erst", "und", "lehrt", "uns", "was", "sie", "sey", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "ADV", "$.", "VVFIN", "PPER", "ADV", "KON", "VVFIN", "PPER", "PWS", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Denn da sie sich geschickt auff mehr denn tausend leichen/", "tokens": ["Denn", "da", "sie", "sich", "ge\u00b7schickt", "auff", "mehr", "denn", "tau\u00b7send", "lei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "VVPP", "APPR", "ADV", "ADV", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Sagt mir; wer wird doch wol Lisettens grimm entweichen?", "tokens": ["Sagt", "mir", ";", "wer", "wird", "doch", "wol", "Li\u00b7set\u00b7tens", "grimm", "ent\u00b7wei\u00b7chen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "PWS", "VAFIN", "ADV", "ADV", "NE", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}