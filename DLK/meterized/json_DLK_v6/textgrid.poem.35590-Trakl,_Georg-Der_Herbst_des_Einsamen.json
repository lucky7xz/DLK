{"textgrid.poem.35590": {"metadata": {"author": {"name": "Trakl, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Der Herbst des Einsamen", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der dunkle Herbst kehrt ein voll Frucht und F\u00fclle,", "tokens": ["Der", "dunk\u00b7le", "Herbst", "kehrt", "ein", "voll", "Frucht", "und", "F\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vergilbter Glanz von sch\u00f6nen Sommertagen.", "tokens": ["Ver\u00b7gilb\u00b7ter", "Glanz", "von", "sch\u00f6\u00b7nen", "Som\u00b7mer\u00b7ta\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein reines Blau tritt aus verfallener H\u00fclle;", "tokens": ["Ein", "rei\u00b7nes", "Blau", "tritt", "aus", "ver\u00b7fal\u00b7le\u00b7ner", "H\u00fcl\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Der Flug der V\u00f6gel t\u00f6nt von alten Sagen.", "tokens": ["Der", "Flug", "der", "V\u00f6\u00b7gel", "t\u00f6nt", "von", "al\u00b7ten", "Sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Gekeltert ist der Wein, die milde Stille", "tokens": ["Ge\u00b7kel\u00b7tert", "ist", "der", "Wein", ",", "die", "mil\u00b7de", "Stil\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Erf\u00fcllt von leiser Antwort dunkler Fragen.", "tokens": ["Er\u00b7f\u00fcllt", "von", "lei\u00b7ser", "Ant\u00b7wort", "dunk\u00b7ler", "Fra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und hier und dort ein Kreuz auf \u00f6dem H\u00fcgel;", "tokens": ["Und", "hier", "und", "dort", "ein", "Kreuz", "auf", "\u00f6\u00b7dem", "H\u00fc\u00b7gel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KON", "ADV", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im roten Wald verliert sich eine Herde.", "tokens": ["Im", "ro\u00b7ten", "Wald", "ver\u00b7liert", "sich", "ei\u00b7ne", "Her\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Wolke wandert \u00fcbern Weiherspiegel;", "tokens": ["Die", "Wol\u00b7ke", "wan\u00b7dert", "\u00fc\u00b7bern", "Wei\u00b7her\u00b7spie\u00b7gel", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Es ruht des Landmanns ruhige Geberde.", "tokens": ["Es", "ruht", "des", "Land\u00b7manns", "ru\u00b7hi\u00b7ge", "Ge\u00b7ber\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Sehr leise r\u00fchrt des Abends blauer Fl\u00fcgel", "tokens": ["Sehr", "lei\u00b7se", "r\u00fchrt", "des", "A\u00b7bends", "blau\u00b7er", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ein Dach von d\u00fcrrem Stroh, die schwarze Erde.", "tokens": ["Ein", "Dach", "von", "d\u00fcr\u00b7rem", "Stroh", ",", "die", "schwar\u00b7ze", "Er\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Bald nisten Sterne in des M\u00fcden Brauen;", "tokens": ["Bald", "nis\u00b7ten", "Ster\u00b7ne", "in", "des", "M\u00fc\u00b7den", "Brau\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In k\u00fchle Stuben kehrt ein still Bescheiden", "tokens": ["In", "k\u00fch\u00b7le", "Stu\u00b7ben", "kehrt", "ein", "still", "Be\u00b7schei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "ADJD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und Engel treten leise aus den blauen", "tokens": ["Und", "En\u00b7gel", "tre\u00b7ten", "lei\u00b7se", "aus", "den", "blau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NE", "VVFIN", "ADJD", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Augen der Liebenden, die sanfter leiden.", "tokens": ["Au\u00b7gen", "der", "Lie\u00b7ben\u00b7den", ",", "die", "sanf\u00b7ter", "lei\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PRELS", "ADJD", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.5": {"text": "Es rauscht das Rohr; anf\u00e4llt ein kn\u00f6chern Grauen,", "tokens": ["Es", "rauscht", "das", "Rohr", ";", "an\u00b7f\u00e4llt", "ein", "kn\u00f6\u00b7chern", "Grau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wenn schwarz der Tau tropft von den kahlen Weiden.", "tokens": ["Wenn", "schwarz", "der", "Tau", "tropft", "von", "den", "kah\u00b7len", "Wei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}