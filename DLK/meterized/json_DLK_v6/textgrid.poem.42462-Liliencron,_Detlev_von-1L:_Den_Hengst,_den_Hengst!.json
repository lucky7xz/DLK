{"textgrid.poem.42462": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Den Hengst, den Hengst!", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Den Hengst, den Hengst!", "tokens": ["Den", "Hengst", ",", "den", "Hengst", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Gebt meinen Hengst mir!", "tokens": ["Gebt", "mei\u00b7nen", "Hengst", "mir", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "PPER", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Schaum spritzt ihm vom Z\u00fcgel, seine Flanken zittern.", "tokens": ["Schaum", "spritzt", "ihm", "vom", "Z\u00fc\u00b7gel", ",", "sei\u00b7ne", "Flan\u00b7ken", "zit\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPRART", "NN", "$,", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Der Grimm umrast mir den Helm, das Auge leuchtet.", "tokens": ["Der", "Grimm", "um\u00b7rast", "mir", "den", "Helm", ",", "das", "Au\u00b7ge", "leuch\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "PPER", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Gebt meinen Hengst mir,", "tokens": ["Gebt", "mei\u00b7nen", "Hengst", "mir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "PPER", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Den Hengst, den Hengst!", "tokens": ["Den", "Hengst", ",", "den", "Hengst", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Mir nach, mir nach!", "tokens": ["Mir", "nach", ",", "mir", "nach", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "PPER", "PTKVZ", "$."], "meter": "++-+", "measure": "iambic.di"}, "line.2": {"text": "Degen heraus jetzt!", "tokens": ["De\u00b7gen", "he\u00b7raus", "jetzt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "APZR", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Sturmmarsch h\u00f6r ich schlagen, h\u00f6re euer Hurra.", "tokens": ["Sturm\u00b7marsch", "h\u00f6r", "ich", "schla\u00b7gen", ",", "h\u00f6\u00b7re", "eu\u00b7er", "Hur\u00b7ra", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "VVINF", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "In Rauch und Blut seh ich euch, in Rauch und Flammen.", "tokens": ["In", "Rauch", "und", "Blut", "seh", "ich", "euch", ",", "in", "Rauch", "und", "Flam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "PPER", "PPER", "$,", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Degen heraus jetzt,", "tokens": ["De\u00b7gen", "he\u00b7raus", "jetzt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "APZR", "ADV", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Mir nach, mir nach!", "tokens": ["Mir", "nach", ",", "mir", "nach", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "PPER", "PTKVZ", "$."], "meter": "++-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Zum Sieg, zum Sieg!", "tokens": ["Zum", "Sieg", ",", "zum", "Sieg", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Erde, erbebe!", "tokens": ["Er\u00b7de", ",", "er\u00b7be\u00b7be", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Pulverdampf und Leichen. Vorw\u00e4rts ohne Wanken.", "tokens": ["Pul\u00b7ver\u00b7dampf", "und", "Lei\u00b7chen", ".", "Vor\u00b7w\u00e4rts", "oh\u00b7ne", "Wan\u00b7ken", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$.", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Durch Glanz und Glut geht die Bahn; die Fahnen flattern.", "tokens": ["Durch", "Glanz", "und", "Glut", "geht", "die", "Bahn", ";", "die", "Fah\u00b7nen", "flat\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "ART", "NN", "$.", "ART", "NN", "VVINF", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Erde, erbebe,", "tokens": ["Er\u00b7de", ",", "er\u00b7be\u00b7be", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Zum Sieg, zum Sieg!", "tokens": ["Zum", "Sieg", ",", "zum", "Sieg", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Komm, Tod! komm, Tod!", "tokens": ["Komm", ",", "Tod", "!", "komm", ",", "Tod", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$.", "VVFIN", "$,", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Feind ist erschlagen!", "tokens": ["Feind", "ist", "er\u00b7schla\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Letzte Kugel, triff mich! Strahlend bricht mein Auge:", "tokens": ["Letz\u00b7te", "Ku\u00b7gel", ",", "triff", "mich", "!", "Strah\u00b7lend", "bricht", "mein", "Au\u00b7ge", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PPER", "$.", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Mein Vaterland hat den Sieg! Es lebe, lebe!", "tokens": ["Mein", "Va\u00b7ter\u00b7land", "hat", "den", "Sieg", "!", "Es", "le\u00b7be", ",", "le\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$.", "PPER", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Feind ist erschlagen!", "tokens": ["Feind", "ist", "er\u00b7schla\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Komm, Tod! komm, Tod!", "tokens": ["Komm", ",", "Tod", "!", "komm", ",", "Tod", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$.", "VVFIN", "$,", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}