{"textgrid.poem.57725": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "1L: Auf Beckeln bei Harpstedt ist Erntefest;", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf Beckeln bei Harpstedt ist Erntefest;", "tokens": ["Auf", "Be\u00b7ckeln", "bei", "Harps\u00b7tedt", "ist", "Ern\u00b7te\u00b7fest", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NE", "VAFIN", "NE", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Der Ritter sich nicht lumpen l\u00e4\u00dft.", "tokens": ["Der", "Rit\u00b7ter", "sich", "nicht", "lum\u00b7pen", "l\u00e4\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "PTKNEG", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Herr Simon von Beckeln liebt Scherz und Spa\u00df;", "tokens": ["Herr", "Si\u00b7mon", "von", "Be\u00b7ckeln", "liebt", "Scherz", "und", "Spa\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "APPR", "NE", "VVFIN", "NN", "KON", "NN", "$."], "meter": "++--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Er geht umher mit dem vollen Glas.", "tokens": ["Er", "geht", "um\u00b7her", "mit", "dem", "vol\u00b7len", "Glas", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Er st\u00f6\u00dft mit Bauern und Knechten an;", "tokens": ["Er", "st\u00f6\u00dft", "mit", "Bau\u00b7ern", "und", "Knech\u00b7ten", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "\u00bbein jeder trink', so viel er kann!", "tokens": ["\u00bb", "ein", "je\u00b7der", "trink'", ",", "so", "viel", "er", "kann", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "PIS", "VVFIN", "$,", "ADV", "ADV", "PPER", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der Hase der ist zart und jung;", "tokens": ["Der", "Ha\u00b7se", "der", "ist", "zart", "und", "jung", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nun e\u00dft man zu, es gibt genung!", "tokens": ["Nun", "e\u00dft", "man", "zu", ",", "es", "gibt", "ge\u00b7nung", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PTKVZ", "$,", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}}, "stanza.5": {"line.1": {"text": "Der Hase der ist jung und zart;", "tokens": ["Der", "Ha\u00b7se", "der", "ist", "jung", "und", "zart", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht war, das schmeckt nach Herrenart?\u00ab", "tokens": ["Nicht", "war", ",", "das", "schmeckt", "nach", "Her\u00b7ren\u00b7art", "?", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PTKNEG", "VAFIN", "$,", "PDS", "VVFIN", "APPR", "NE", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Der Ritter spendiert ein F\u00e4\u00dfchen Wein,", "tokens": ["Der", "Rit\u00b7ter", "spen\u00b7diert", "ein", "F\u00e4\u00df\u00b7chen", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Eine neue Sch\u00fcssel kommt herein.", "tokens": ["Ei\u00b7ne", "neu\u00b7e", "Sch\u00fcs\u00b7sel", "kommt", "her\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Und als man den Deckel herunterhebt,", "tokens": ["Und", "als", "man", "den", "De\u00b7ckel", "her\u00b7un\u00b7ter\u00b7hebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Den Bauern der Wein im Halse klebt.", "tokens": ["Den", "Bau\u00b7ern", "der", "Wein", "im", "Hal\u00b7se", "klebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "\u00bbverdonner noch mal, was ist denn das?", "tokens": ["\u00bb", "ver\u00b7don\u00b7ner", "noch", "mal", ",", "was", "ist", "denn", "das", "?"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "ADV", "ADV", "$,", "PWS", "VAFIN", "ADV", "PDS", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ein Katerkopp? Vermuckter Spa\u00df!", "tokens": ["Ein", "Ka\u00b7ter\u00b7kopp", "?", "Ver\u00b7muck\u00b7ter", "Spa\u00df", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Der Satan soll den Ritter hol'n;", "tokens": ["Der", "Sa\u00b7tan", "soll", "den", "Rit\u00b7ter", "hol'n", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wir wollen ihm das Fell versohl'n!", "tokens": ["Wir", "wol\u00b7len", "ihm", "das", "Fell", "ver\u00b7sohl'n", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Wo ist er hin, der schlechte Hund?", "tokens": ["Wo", "ist", "er", "hin", ",", "der", "schlech\u00b7te", "Hund", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PTKVZ", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Stopft ihm den Katerkopf in den Schlund!", "tokens": ["Stopft", "ihm", "den", "Ka\u00b7ter\u00b7kopf", "in", "den", "Schlund", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}}, "stanza.11": {"line.1": {"text": "F\u00fcr Schwei\u00df und M\u00fche Schimpf und Schand?\u00ab", "tokens": ["F\u00fcr", "Schwei\u00df", "und", "M\u00fc\u00b7he", "Schimpf", "und", "Schand", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "KON", "NN", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In die Scheune fliegt der Feuerbrand", "tokens": ["In", "die", "Scheu\u00b7ne", "fliegt", "der", "Feu\u00b7er\u00b7brand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.12": {"line.1": {"text": "Und fliegt von da ins Herrenhaus;", "tokens": ["Und", "fliegt", "von", "da", "ins", "Her\u00b7ren\u00b7haus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Simon von Beckeln schl\u00fcpft hinten hinaus.", "tokens": ["Si\u00b7mon", "von", "Be\u00b7ckeln", "schl\u00fcpft", "hin\u00b7ten", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "+-----+--+", "measure": "iambic.tri.chol"}}, "stanza.13": {"line.1": {"text": "Die M\u00fchle und der Speicher brennt;", "tokens": ["Die", "M\u00fch\u00b7le", "und", "der", "Spei\u00b7cher", "brennt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Simon von Beckeln von dannen rennt,", "tokens": ["Si\u00b7mon", "von", "Be\u00b7ckeln", "von", "dan\u00b7nen", "rennt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "APPR", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.14": {"line.1": {"text": "Ins Wildesh\u00e4usener Stift er kriecht;", "tokens": ["Ins", "Wil\u00b7des\u00b7h\u00e4u\u00b7se\u00b7ner", "Stift", "er", "kriecht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Spa\u00df ihm allzu brenzlig riecht.", "tokens": ["Der", "Spa\u00df", "ihm", "all\u00b7zu", "brenz\u00b7lig", "riecht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "PTKA", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.15": {"line.1": {"text": "Er schickt den Pater Abraham;", "tokens": ["Er", "schickt", "den", "Pa\u00b7ter", "Ab\u00b7ra\u00b7ham", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00dcble Antwort der mitbekam:", "tokens": ["\u00dcb\u00b7le", "Ant\u00b7wort", "der", "mit\u00b7be\u00b7kam", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.16": {"line.1": {"text": "\u00bbmeldet dem Herrn unser Gebot:", "tokens": ["\u00bb", "mel\u00b7det", "dem", "Herrn", "un\u00b7ser", "Ge\u00b7bot", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+--++--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Wir schlagen ihn lebendig tot!\u00ab", "tokens": ["Wir", "schla\u00b7gen", "ihn", "le\u00b7ben\u00b7dig", "tot", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "ADJD", "$.", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.17": {"line.1": {"text": "Der Ritter kratzt sich hinter dem Ohr,", "tokens": ["Der", "Rit\u00b7ter", "kratzt", "sich", "hin\u00b7ter", "dem", "Ohr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Und wagt sich nicht aus dem Stift hervor.", "tokens": ["Und", "wagt", "sich", "nicht", "aus", "dem", "Stift", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "PTKNEG", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.18": {"line.1": {"text": "Er lebte dort noch manches Jahr,", "tokens": ["Er", "leb\u00b7te", "dort", "noch", "man\u00b7ches", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bis da\u00df es mit ihm zu Ende war.", "tokens": ["Bis", "da\u00df", "es", "mit", "ihm", "zu", "En\u00b7de", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "APPR", "PPER", "APPR", "NN", "VAFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.19": {"line.1": {"text": "Die Klosterglocke schwingt hin und her:", "tokens": ["Die", "Klos\u00b7ter\u00b7glo\u00b7cke", "schwingt", "hin", "und", "her", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Von Beckeln nun und nimmermehr!", "tokens": ["Von", "Be\u00b7ckeln", "nun", "und", "nim\u00b7mer\u00b7mehr", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADV", "KON", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}