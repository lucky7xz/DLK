{"textgrid.poem.55787": {"metadata": {"author": {"name": "D\u00f6rmann, Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Int\u00e9rieur von lichter Scharlachseide,", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Int\u00e9rieur von lichter Scharlachseide,", "tokens": ["Ein", "In\u00b7t\u00e9rieur", "von", "lich\u00b7ter", "Schar\u00b7lach\u00b7sei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ein wohldurchw\u00e4rmtes, traulich-enges Heim.", "tokens": ["Ein", "wohl\u00b7durch\u00b7w\u00e4rm\u00b7tes", ",", "trau\u00b7lich\u00b7en\u00b7ges", "Heim", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Aus schlankgeformten St\u00e4nderlampen quillt,", "tokens": ["Aus", "schlank\u00b7ge\u00b7form\u00b7ten", "St\u00e4n\u00b7der\u00b7lam\u00b7pen", "quillt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Von buntgef\u00e4rbten Abas-jours ged\u00e4mpft, \u2013", "tokens": ["Von", "bunt\u00b7ge\u00b7f\u00e4rb\u00b7ten", "Ab\u00b7as\u00b7jours", "ge\u00b7d\u00e4mpft", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ein rosig warmer Lichtstrom zitternd nieder.", "tokens": ["Ein", "ro\u00b7sig", "war\u00b7mer", "Licht\u00b7strom", "zit\u00b7ternd", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "VVPP", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Orangen und Narzissen hauchen tr\u00e4umend", "tokens": ["O\u00b7ran\u00b7gen", "und", "Nar\u00b7zis\u00b7sen", "hau\u00b7chen", "tr\u00e4u\u00b7mend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Die duftig-schweren Bl\u00fcthenseelen aus \u2013", "tokens": ["Die", "duf\u00b7tig\u00b7schwe\u00b7ren", "Bl\u00fc\u00b7then\u00b7see\u00b7len", "aus", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und tiefes, tiefes Schweigen. \u2013 Hingelagert", "tokens": ["Und", "tie\u00b7fes", ",", "tie\u00b7fes", "Schwei\u00b7gen", ".", "\u2013", "Hin\u00b7ge\u00b7la\u00b7gert"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "word"], "pos": ["KON", "ADJA", "$,", "ADJA", "NN", "$.", "$(", "NE"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Auf \u00fcppig weichen Eisb\u00e4rfellen, ruht", "tokens": ["Auf", "\u00fcp\u00b7pig", "wei\u00b7chen", "Eis\u00b7b\u00e4r\u00b7fel\u00b7len", ",", "ruht"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Ein schlankes Weib, die Lippen halberbrochen,", "tokens": ["Ein", "schlan\u00b7kes", "Weib", ",", "die", "Lip\u00b7pen", "hal\u00b7ber\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Mit leicht-umblauten, m\u00fcden Schw\u00e4rmeraugen, \u2013", "tokens": ["Mit", "leicht\u00b7um\u00b7blau\u00b7ten", ",", "m\u00fc\u00b7den", "Schw\u00e4r\u00b7me\u00b7rau\u00b7gen", ",", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "$,", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Und tr\u00e4umt und tr\u00e4umt von seelenhei\u00dfer Freude,", "tokens": ["Und", "tr\u00e4umt", "und", "tr\u00e4umt", "von", "see\u00b7len\u00b7hei\u00b7\u00dfer", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Von z\u00fcgellosem Schwelgen, trunknem Rasen,", "tokens": ["Von", "z\u00fc\u00b7gel\u00b7lo\u00b7sem", "Schwel\u00b7gen", ",", "trunk\u00b7nem", "Ra\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Von einem hochgepeitschten Taumelreigen", "tokens": ["Von", "ei\u00b7nem", "hoch\u00b7ge\u00b7peitschten", "Tau\u00b7mel\u00b7rei\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Der abgestumpften, wurzelwelken Nerven,", "tokens": ["Der", "ab\u00b7ge\u00b7stumpf\u00b7ten", ",", "wur\u00b7zel\u00b7wel\u00b7ken", "Ner\u00b7ven", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Von einem letzten, niegekannten Gl\u00fcck,", "tokens": ["Von", "ei\u00b7nem", "letz\u00b7ten", ",", "nie\u00b7ge\u00b7kann\u00b7ten", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Von einer Wonne, die der Wonnen h\u00f6chste", "tokens": ["Von", "ei\u00b7ner", "Won\u00b7ne", ",", "die", "der", "Won\u00b7nen", "h\u00f6chs\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ART", "NN", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Und doch nicht Liebe hei\u00dft \u2013 und tr\u00e4umt und tr\u00e4umt.", "tokens": ["Und", "doch", "nicht", "Lie\u00b7be", "hei\u00dft", "\u2013", "und", "tr\u00e4umt", "und", "tr\u00e4umt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKNEG", "NN", "VVFIN", "$(", "KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}