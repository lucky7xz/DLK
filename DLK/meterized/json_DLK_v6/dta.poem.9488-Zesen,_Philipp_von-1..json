{"dta.poem.9488": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dich/ Lust-Kind Echo/ will ich fragen/", "tokens": ["Dich", "/", "Lust\u00b7Kind", "E\u00b7cho", "/", "will", "ich", "fra\u00b7gen", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "NE", "NE", "$(", "VMFIN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ech. Dis dein klagen/", "tokens": ["Ech", ".", "Dis", "dein", "kla\u00b7gen", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PDS", "PPOSAT", "VVINF", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Ja klagen: wo f\u00fchl ich die schmertzen?", "tokens": ["Ja", "kla\u00b7gen", ":", "wo", "f\u00fchl", "ich", "die", "schmert\u00b7zen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "VVINF", "$.", "PWAV", "VVFIN", "PPER", "ART", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "E. In dem hertzen.", "tokens": ["E.", "In", "dem", "hert\u00b7zen", "."], "token_info": ["abbreviation", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Wer macht die gluth die mich entz\u00fcndet?", "tokens": ["Wer", "macht", "die", "gluth", "die", "mich", "ent\u00b7z\u00fcn\u00b7det", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "ART", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "E. Die dich bindet.", "tokens": ["E.", "Die", "dich", "bin\u00b7det", "."], "token_info": ["abbreviation", "word", "word", "word", "punct"], "pos": ["NN", "ART", "PPER", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Wer macht die Angst die mich betr\u00fcbet?", "tokens": ["Wer", "macht", "die", "Angst", "die", "mich", "be\u00b7tr\u00fc\u00b7bet", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "ART", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "E. Die dich liebet.", "tokens": ["E.", "Die", "dich", "lie\u00b7bet", "."], "token_info": ["abbreviation", "word", "word", "word", "punct"], "pos": ["NN", "ART", "PPER", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Was kann doch lindern solch Betr\u00fcben?", "tokens": ["Was", "kann", "doch", "lin\u00b7dern", "solch", "Be\u00b7tr\u00fc\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "E. Treulich lieben.", "tokens": ["E.", "Treu\u00b7lich", "lie\u00b7ben", "."], "token_info": ["abbreviation", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Wie soll mich dann mein Schatz \u00fcmbfangen?", "tokens": ["Wie", "soll", "mich", "dann", "mein", "Schatz", "\u00fcmb\u00b7fan\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "E. Mit den Wangen.", "tokens": ["E.", "Mit", "den", "Wan\u00b7gen", "."], "token_info": ["abbreviation", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Wenn wird die Last von mier genommen?", "tokens": ["Wenn", "wird", "die", "Last", "von", "mier", "ge\u00b7nom\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "ART", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "E Sie wird kommen.", "tokens": ["E", "Sie", "wird", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VAFIN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Wird Sie denn balde mich befreyen?", "tokens": ["Wird", "Sie", "denn", "bal\u00b7de", "mich", "be\u00b7fre\u00b7yen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+---", "measure": "unknown.measure.tri"}, "line.4": {"text": "E. Gantz verneuen.", "tokens": ["E.", "Gantz", "ver\u00b7neu\u00b7en", "."], "token_info": ["abbreviation", "word", "word", "punct"], "pos": ["NE", "ADV", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Wie lange wird es sich verzihen?", "tokens": ["Wie", "lan\u00b7ge", "wird", "es", "sich", "ver\u00b7zi\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "E. Zeit mu\u00df flihen.", "tokens": ["E.", "Zeit", "mu\u00df", "fli\u00b7hen", "."], "token_info": ["abbreviation", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VMFIN", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "Wovor wird doch geschickt die Krone?", "tokens": ["Wo\u00b7vor", "wird", "doch", "ge\u00b7schickt", "die", "Kro\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ADV", "VVPP", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "E. Dier zu Lohne.", "tokens": ["E.", "Dier", "zu", "Loh\u00b7ne", "."], "token_info": ["abbreviation", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Was macht der Krantz von Gold und Seiden?", "tokens": ["Was", "macht", "der", "Krantz", "von", "Gold", "und", "Sei\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "E. Lauter freuden.", "tokens": ["E.", "Lau\u00b7ter", "freu\u00b7den", "."], "token_info": ["abbreviation", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Ach m\u00f6cht ich nur Sie selbsten haben!", "tokens": ["Ach", "m\u00f6cht", "ich", "nur", "Sie", "selbs\u00b7ten", "ha\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ADV", "PPER", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "E. Dich zu laben.", "tokens": ["E.", "Dich", "zu", "la\u00b7ben", "."], "token_info": ["abbreviation", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.5": {"line.1": {"text": "Wird Sie bald kommen mich zu k\u00fcssen?", "tokens": ["Wird", "Sie", "bald", "kom\u00b7men", "mich", "zu", "k\u00fcs\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVFIN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "E. Sie wird m\u00fcssen.", "tokens": ["E.", "Sie", "wird", "m\u00fcs\u00b7sen", "."], "token_info": ["abbreviation", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VAFIN", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Ach sih! der Tag leufft schon zum ende/", "tokens": ["Ach", "sih", "!", "der", "Tag", "leufft", "schon", "zum", "en\u00b7de", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ITJ", "$.", "ART", "NN", "VVFIN", "ADV", "APPRART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "E. Gar behende.", "tokens": ["E.", "Gar", "be\u00b7hen\u00b7de", "."], "token_info": ["abbreviation", "word", "word", "punct"], "pos": ["NE", "ADV", "ADJA", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Nun k\u00f6m\u0303t Sie/ Meine Freuden-Sonne/", "tokens": ["Nun", "k\u00f6m\u0303t", "Sie", "/", "Mei\u00b7ne", "Freu\u00b7den\u00b7Son\u00b7ne", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$(", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "E. Deine Wonne.", "tokens": ["E.", "Dei\u00b7ne", "Won\u00b7ne", "."], "token_info": ["abbreviation", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.6": {"line.1": {"text": "Nun will ich Ku\u00df \u00fcmb Ku\u00df Jhr geben/", "tokens": ["Nun", "will", "ich", "Ku\u00df", "\u00fcmb", "Ku\u00df", "Ihr", "ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "NN", "APPR", "NN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "E. und dein Leben.", "tokens": ["E.", "und", "dein", "Le\u00b7ben", "."], "token_info": ["abbreviation", "word", "word", "word", "punct"], "pos": ["NE", "KON", "PPOSAT", "NN", "$."], "meter": "--+-", "measure": "anapaest.init"}, "line.3": {"text": "Sie wird mir Lust und Freude machen/", "tokens": ["Sie", "wird", "mir", "Lust", "und", "Freu\u00b7de", "ma\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "NN", "KON", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "E. Dich anlachen.", "tokens": ["E.", "Dich", "an\u00b7la\u00b7chen", "."], "token_info": ["abbreviation", "word", "word", "punct"], "pos": ["NE", "PPER", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Sie wird mir alles seyn zu willen/", "tokens": ["Sie", "wird", "mir", "al\u00b7les", "seyn", "zu", "wil\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PIS", "VAINF", "APPR", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "E. Dich zu stillen.", "tokens": ["E.", "Dich", "zu", "stil\u00b7len", "."], "token_info": ["abbreviation", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.7": {"line.1": {"text": "Wo seyn doch hin die langen stunden?", "tokens": ["Wo", "seyn", "doch", "hin", "die", "lan\u00b7gen", "stun\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADV", "ADV", "ART", "ADJA", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "E. Gantz verschwunden.", "tokens": ["E.", "Gantz", "ver\u00b7schwun\u00b7den", "."], "token_info": ["abbreviation", "word", "word", "punct"], "pos": ["NE", "ADV", "VVPP", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Soll ich nun fort in Freuden schweben?", "tokens": ["Soll", "ich", "nun", "fort", "in", "Freu\u00b7den", "schwe\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PTKVZ", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "E. Lustig leben.", "tokens": ["E.", "Lus\u00b7tig", "le\u00b7ben", "."], "token_info": ["abbreviation", "word", "word", "punct"], "pos": ["NE", "NE", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Nun will ich ruhen in den Armen;", "tokens": ["Nun", "will", "ich", "ru\u00b7hen", "in", "den", "Ar\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "E. Zuerwarmen.", "tokens": ["E.", "Zu\u00b7er\u00b7war\u00b7men", "."], "token_info": ["abbreviation", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.8": {"line.1": {"text": "Nun f\u00fchlich weder Augst noch schmertzen/", "tokens": ["Nun", "f\u00fch\u00b7lich", "we\u00b7der", "Augst", "noch", "schmert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "E. In dem Hertzen.", "tokens": ["E.", "In", "dem", "Hert\u00b7zen", "."], "token_info": ["abbreviation", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Der Schmertz ist jtzo gantz verschwunden/", "tokens": ["Der", "Schmertz", "ist", "jt\u00b7zo", "gantz", "ver\u00b7schwun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "E. Mit den stunden.", "tokens": ["E.", "Mit", "den", "stun\u00b7den", "."], "token_info": ["abbreviation", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "ADJA", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Nun kann ich meine Liebste hertzen/", "tokens": ["Nun", "kann", "ich", "mei\u00b7ne", "Liebs\u00b7te", "hert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "E. Mit jhr schertzen.", "tokens": ["E.", "Mit", "jhr", "schert\u00b7zen", "."], "token_info": ["abbreviation", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPER", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.9": {"line.1": {"text": "Jhr Zucker-Mund mu\u00df mich erquicken/", "tokens": ["Ihr", "Zu\u00b7cke\u00b7rMund", "mu\u00df", "mich", "er\u00b7qui\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "E. und dich dr\u00fccken.", "tokens": ["E.", "und", "dich", "dr\u00fc\u00b7cken", "."], "token_info": ["abbreviation", "word", "word", "word", "punct"], "pos": ["NE", "KON", "PPER", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Was soll ich jtzo thun und \u00fcben?", "tokens": ["Was", "soll", "ich", "jt\u00b7zo", "thun", "und", "\u00fc\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "E. Lauter lieben.", "tokens": ["E.", "Lau\u00b7ter", "lie\u00b7ben", "."], "token_info": ["abbreviation", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "So lange bi\u00df die Nacht verstiebet", "tokens": ["So", "lan\u00b7ge", "bi\u00df", "die", "Nacht", "ver\u00b7stie\u00b7bet"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "E. Sie dich liebet.", "tokens": ["E.", "Sie", "dich", "lie\u00b7bet", "."], "token_info": ["abbreviation", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PRF", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.10": {"line.1": {"text": "Ey nun wil ich den Schmertzen meiden", "tokens": ["Ey", "nun", "wil", "ich", "den", "Schmert\u00b7zen", "mei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "VMFIN", "PPER", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "E. Angst und Leiden.", "tokens": ["E.", "Angst", "und", "Lei\u00b7den", "."], "token_info": ["abbreviation", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Nun mag ein ander Leide tragen", "tokens": ["Nun", "mag", "ein", "an\u00b7der", "Lei\u00b7de", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "E. Angst und klagen.", "tokens": ["E.", "Angst", "und", "kla\u00b7gen", "."], "token_info": ["abbreviation", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Ich kann nun f\u00fcr und f\u00fcr mich \u00fcben/", "tokens": ["Ich", "kann", "nun", "f\u00fcr", "und", "f\u00fcr", "mich", "\u00fc\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "KON", "APPR", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "E. V\u00f6llig lieben.", "tokens": ["E.", "V\u00f6l\u00b7lig", "lie\u00b7ben", "."], "token_info": ["abbreviation", "word", "word", "punct"], "pos": ["NE", "ADJD", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}