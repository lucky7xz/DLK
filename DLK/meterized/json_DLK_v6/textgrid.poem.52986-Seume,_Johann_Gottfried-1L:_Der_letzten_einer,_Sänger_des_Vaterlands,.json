{"textgrid.poem.52986": {"metadata": {"author": {"name": "Seume, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der letzten einer, S\u00e4nger des Vaterlands,", "genre": "verse", "period": "N.A.", "pub_year": 1786, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der letzten einer, S\u00e4nger des Vaterlands,", "tokens": ["Der", "letz\u00b7ten", "ei\u00b7ner", ",", "S\u00e4n\u00b7ger", "des", "Va\u00b7ter\u00b7lands", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ART", "$,", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wag' ich zu reden; Ph\u00f6bus Apollo will:", "tokens": ["Wag'", "ich", "zu", "re\u00b7den", ";", "Ph\u00f6\u00b7bus", "A\u00b7pol\u00b7lo", "will", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKZU", "VVINF", "$.", "NE", "NE", "VMFIN", "$."], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Nehmt heiligend mich in die Mitte,", "tokens": ["Nehmt", "hei\u00b7li\u00b7gend", "mich", "in", "die", "Mit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df ihr die Worte des Sprechers h\u00f6ret.", "tokens": ["Da\u00df", "ihr", "die", "Wor\u00b7te", "des", "Spre\u00b7chers", "h\u00f6\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Will keiner bauen, S\u00e4nger des Vaterlands,", "tokens": ["Will", "kei\u00b7ner", "bau\u00b7en", ",", "S\u00e4n\u00b7ger", "des", "Va\u00b7ter\u00b7lands", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "VVINF", "$,", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Auf deren Stimme zwischen dem Rhein und Belt", "tokens": ["Auf", "de\u00b7ren", "Stim\u00b7me", "zwi\u00b7schen", "dem", "Rhein", "und", "Belt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELAT", "NN", "APPR", "ART", "NE", "KON", "NN"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Thuiskons Enkel lauschend horchen?", "tokens": ["Thu\u00b7is\u00b7kons", "En\u00b7kel", "lau\u00b7schend", "hor\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Keiner den Altar mit Blumen kr\u00e4nzen?", "tokens": ["Kei\u00b7ner", "den", "Al\u00b7tar", "mit", "Blu\u00b7men", "kr\u00e4n\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "NN", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Die Pieriden lieben Thuiskons Hain;", "tokens": ["Die", "Pie\u00b7ri\u00b7den", "lie\u00b7ben", "Thu\u00b7is\u00b7kons", "Hain", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NE", "NE", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Die Quelle rauschet ihrer Begeistrung voll.", "tokens": ["Die", "Quel\u00b7le", "rau\u00b7schet", "ih\u00b7rer", "Be\u00b7geis\u00b7trung", "voll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Zur\u00fcck, wer hier nicht reines Herzens", "tokens": ["Zu\u00b7r\u00fcck", ",", "wer", "hier", "nicht", "rei\u00b7nes", "Her\u00b7zens"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKVZ", "$,", "PWS", "ADV", "PTKNEG", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vor dem Gedanken der Weihe stehn kann.", "tokens": ["Vor", "dem", "Ge\u00b7dan\u00b7ken", "der", "Wei\u00b7he", "stehn", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.4": {"line.1": {"text": "Dione nennet Tochter Musarion;", "tokens": ["Dio\u00b7ne", "nen\u00b7net", "Toch\u00b7ter", "Mu\u00b7sa\u00b7ri\u00b7on", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "NN", "NN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Der weise Tejer k\u00fcsset den Silbergreis,", "tokens": ["Der", "wei\u00b7se", "Te\u00b7jer", "k\u00fcs\u00b7set", "den", "Sil\u00b7berg\u00b7reis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-++-+-+", "measure": "unknown.measure.hexa"}, "line.3": {"text": "Und Flakkus schwebt in Friedrichs Hallen", "tokens": ["Und", "Flak\u00b7kus", "schwebt", "in", "Fried\u00b7richs", "Hal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "APPR", "NE", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "V\u00e4terlich \u00fcber dem Lieblingssohne.", "tokens": ["V\u00e4\u00b7ter\u00b7lich", "\u00fc\u00b7ber", "dem", "Lieb\u00b7lings\u00b7soh\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Aglajens Schwestern winden im Morgenthau", "tokens": ["Ag\u00b7la\u00b7jens", "Schwes\u00b7tern", "win\u00b7den", "im", "Mor\u00b7gent\u00b7hau"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Aus jungen Veilchen, die mit Auroren sie", "tokens": ["Aus", "jun\u00b7gen", "Veil\u00b7chen", ",", "die", "mit", "Au\u00b7ro\u00b7ren", "sie"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "PRELS", "APPR", "NN", "PPER"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Am Fu\u00dfe des Himettus pfl\u00fcckten,", "tokens": ["Am", "Fu\u00b7\u00dfe", "des", "Hi\u00b7met\u00b7tus", "pfl\u00fcck\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Kr\u00e4nze den Schl\u00e4fen des Kinderfreundes.", "tokens": ["Kr\u00e4n\u00b7ze", "den", "Schl\u00e4\u00b7fen", "des", "Kin\u00b7der\u00b7freun\u00b7des", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Der Mann des Rachschwerts, der Legionen Roms", "tokens": ["Der", "Mann", "des", "Rach\u00b7schwerts", ",", "der", "Le\u00b7gi\u00b7o\u00b7nen", "Roms"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$,", "ART", "NN", "NE"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Gro\u00dfer Vertilger, Hermann der Deutsche, staunt,", "tokens": ["Gro\u00b7\u00dfer", "Ver\u00b7til\u00b7ger", ",", "Her\u00b7mann", "der", "Deut\u00b7sche", ",", "staunt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NE", "ART", "NN", "$,", "VVFIN", "$,"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Wie in dem Liede seines Barden", "tokens": ["Wie", "in", "dem", "Lie\u00b7de", "sei\u00b7nes", "Bar\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Furchtbar die blutige Feldschlacht fortt\u00f6nt.", "tokens": ["Furcht\u00b7bar", "die", "blu\u00b7ti\u00b7ge", "Feld\u00b7schlacht", "fort\u00b7t\u00f6nt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.7": {"line.1": {"text": "Will keiner bauen, S\u00e4nger des Vaterlands?", "tokens": ["Will", "kei\u00b7ner", "bau\u00b7en", ",", "S\u00e4n\u00b7ger", "des", "Va\u00b7ter\u00b7lands", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "VVINF", "$,", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mit schwachen H\u00e4nden richt' ich den Stein empor,", "tokens": ["Mit", "schwa\u00b7chen", "H\u00e4n\u00b7den", "richt'", "ich", "den", "Stein", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Um meine Blumen drauf zu streuen,", "tokens": ["Um", "mei\u00b7ne", "Blu\u00b7men", "drauf", "zu", "streu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "PAV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df er mir l\u00e4ndlich ein Altar stehe.", "tokens": ["Da\u00df", "er", "mir", "l\u00e4nd\u00b7lich", "ein", "Al\u00b7tar", "ste\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Zur\u00fcck, wem nicht von Tropfen die Wimper gl\u00fcht!", "tokens": ["Zu\u00b7r\u00fcck", ",", "wem", "nicht", "von", "Trop\u00b7fen", "die", "Wim\u00b7per", "gl\u00fcht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKVZ", "$,", "PWS", "PTKNEG", "APPR", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Utz ist gestorben, wie man zum Leben stirbt:", "tokens": ["Utz", "ist", "ge\u00b7stor\u00b7ben", ",", "wie", "man", "zum", "Le\u00b7ben", "stirbt", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "VVPP", "$,", "PWAV", "PIS", "APPRART", "NN", "VVFIN", "$."], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "In Flammenschrift seh' ich hier Wieland,", "tokens": ["In", "Flam\u00b7men\u00b7schrift", "seh'", "ich", "hier", "Wie\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ADV", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Klopstock und Ramler und Gleim und Wei\u00dfe.", "tokens": ["Klops\u00b7tock", "und", "Ram\u00b7ler", "und", "Gleim", "und", "Wei\u00b7\u00dfe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.9": {"line.1": {"text": "Wer nicht der V\u00e4ter Glorie kindlich ehrt,", "tokens": ["Wer", "nicht", "der", "V\u00e4\u00b7ter", "Glo\u00b7rie", "kind\u00b7lich", "ehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "ART", "NN", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der find' in Bav den S\u00e4nger des Heldenzorns,", "tokens": ["Der", "find'", "in", "Bav", "den", "S\u00e4n\u00b7ger", "des", "Hel\u00b7den\u00b7zorns", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "NE", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und lese mit des bleyrnen Stumpfsinns", "tokens": ["Und", "le\u00b7se", "mit", "des", "bleyr\u00b7nen", "Stumpf\u00b7sinns"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schwerer Entz\u00fcckung die Magelone.", "tokens": ["Schwe\u00b7rer", "Ent\u00b7z\u00fc\u00b7ckung", "die", "Ma\u00b7ge\u00b7lo\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}}}}