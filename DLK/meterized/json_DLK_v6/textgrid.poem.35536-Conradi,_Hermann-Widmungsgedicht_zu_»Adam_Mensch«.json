{"textgrid.poem.35536": {"metadata": {"author": {"name": "Conradi, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Widmungsgedicht zu \u00bbAdam Mensch\u00ab", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von einem Grabe komm' ich her. \u2013 Du wei\u00dft,", "tokens": ["Von", "ei\u00b7nem", "Gra\u00b7be", "komm'", "ich", "her", ".", "\u2013", "Du", "wei\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$.", "$(", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mein lieber Freund \u2013: von welchem Grabe \u2013", "tokens": ["Mein", "lie\u00b7ber", "Freund", "\u2013", ":", "von", "wel\u00b7chem", "Gra\u00b7be", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$(", "$.", "APPR", "PWAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Du wei\u00dft \u2013: wie viele Tr\u00e4ume, wie viel Gl\u00fcck \u2013", "tokens": ["Du", "wei\u00dft", "\u2013", ":", "wie", "vie\u00b7le", "Tr\u00e4u\u00b7me", ",", "wie", "viel", "Gl\u00fcck", "\u2013"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "$.", "PWAV", "PIAT", "NN", "$,", "PWAV", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wie viele Vergangenheit ich ", "tokens": ["Wie", "vie\u00b7le", "Ver\u00b7gan\u00b7gen\u00b7heit", "ich"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PIAT", "NN", "PPER"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Von des Vergessens Flut un\u00fcbersp\u00fclt", "tokens": ["Von", "des", "Ver\u00b7ges\u00b7sens", "Flut", "un\u00b7\u00fc\u00b7bers\u00b7p\u00fclt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Mahnt dieser H\u00fcgel noch im fernen S\u00fcden \u2013", "tokens": ["Mahnt", "die\u00b7ser", "H\u00fc\u00b7gel", "noch", "im", "fer\u00b7nen", "S\u00fc\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "NN", "ADV", "APPRART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Da wir so gro\u00df gelebt, so stark gef\u00fchlt,", "tokens": ["Da", "wir", "so", "gro\u00df", "ge\u00b7lebt", ",", "so", "stark", "ge\u00b7f\u00fchlt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VVPP", "$,", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "So hei\u00df gek\u00e4mpft um unsres Willens Frieden.", "tokens": ["So", "hei\u00df", "ge\u00b7k\u00e4mpft", "um", "uns\u00b7res", "Wil\u00b7lens", "Frie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Ob wir ihn fanden \u2013? Nun, mein lieber Freund \u2013", "tokens": ["Ob", "wir", "ihn", "fan\u00b7den", "\u2013", "?", "Nun", ",", "mein", "lie\u00b7ber", "Freund", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$(", "$.", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Wir l\u00e4cheln schmerzlich \u2013 doch wir ", "tokens": ["Wir", "l\u00e4\u00b7cheln", "schmerz\u00b7lich", "\u2013", "doch", "wir"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "$(", "KON", "PPER"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.11": {"text": "Wir sind allein \u2013 wir haben nur noch uns \u2013", "tokens": ["Wir", "sind", "al\u00b7lein", "\u2013", "wir", "ha\u00b7ben", "nur", "noch", "uns", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$(", "PPER", "VAFIN", "ADV", "ADV", "PPER", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "So bleiben wir zusammen f\u00fcr das Leben ...", "tokens": ["So", "blei\u00b7ben", "wir", "zu\u00b7sam\u00b7men", "f\u00fcr", "das", "Le\u00b7ben", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Der Regen klatscht an meine Fensterscheiben \u2013", "tokens": ["Der", "Re\u00b7gen", "klatscht", "an", "mei\u00b7ne", "Fens\u00b7ter\u00b7schei\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Ins Nordland wieder wurden wir verbannt \u2013", "tokens": ["Ins", "Nord\u00b7land", "wie\u00b7der", "wur\u00b7den", "wir", "ver\u00b7bannt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VAFIN", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Getrost mein Freund! Wir werden s\u00fcdw\u00e4rts treiben", "tokens": ["Ge\u00b7trost", "mein", "Freund", "!", "Wir", "wer\u00b7den", "s\u00fcd\u00b7w\u00e4rts", "trei\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "$.", "PPER", "VAFIN", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "In unsrer Sehnsucht \u2013 unsres ", "tokens": ["In", "uns\u00b7rer", "Sehn\u00b7sucht", "\u2013", "uns\u00b7res"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["APPR", "PPOSAT", "NN", "$(", "PPOSAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "Ein ", "tokens": ["Ein"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}, "line.18": {"text": "Hat's unterweil der junge Lenzgeschm\u00fcckt \u2013", "tokens": ["Hat's", "un\u00b7ter\u00b7weil", "der", "jun\u00b7ge", "Lenz\u00b7ge\u00b7schm\u00fcckt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "F\u00fcr das ", "tokens": ["F\u00fcr", "das"], "token_info": ["word", "word"], "pos": ["APPR", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.20": {"text": "Mit rotem Blut getauft der tiefsten Schmerzen", "tokens": ["Mit", "ro\u00b7tem", "Blut", "ge\u00b7tauft", "der", "tiefs\u00b7ten", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVPP", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Ward uns der Geist, der ", "tokens": ["Ward", "uns", "der", "Geist", ",", "der"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["VAFIN", "PPER", "ART", "NN", "$,", "PRELS"], "meter": "-+-+-", "measure": "iambic.di"}}}}}