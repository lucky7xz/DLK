{"textgrid.poem.42809": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Landflucht", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fort vom Lande, aus dem engen", "tokens": ["Fort", "vom", "Lan\u00b7de", ",", "aus", "dem", "en\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "APPRART", "NN", "$,", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "St\u00e4dtchen in die Gro\u00dfstadt flieht der Geist,", "tokens": ["St\u00e4dt\u00b7chen", "in", "die", "Gro\u00df\u00b7stadt", "flieht", "der", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wo im Kampf der Mengen", "tokens": ["Wo", "im", "Kampf", "der", "Men\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPRART", "NN", "ART", "NN"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Er zerrei\u00dft.", "tokens": ["Er", "zer\u00b7rei\u00dft", "."], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Dort, wo Puls und Uhr", "tokens": ["Dort", ",", "wo", "Puls", "und", "Uhr"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "NN", "KON", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Schneller ticken,", "tokens": ["Schnel\u00b7ler", "ti\u00b7cken", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Wird er sich zusammenflicken,", "tokens": ["Wird", "er", "sich", "zu\u00b7sam\u00b7men\u00b7fli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wenn er's erst versteht,", "tokens": ["Wenn", "er's", "erst", "ver\u00b7steht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "Da\u00df die unbezwingliche Natur", "tokens": ["Da\u00df", "die", "un\u00b7be\u00b7zwing\u00b7li\u00b7che", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.10": {"text": "Auch auf Radiowellen, Schienenspur", "tokens": ["Auch", "auf", "Ra\u00b7dio\u00b7wel\u00b7len", ",", "Schie\u00b7nen\u00b7spur"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ADV", "APPR", "NN", "$,", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.11": {"text": "Und Propellerschwingen weitergeht.", "tokens": ["Und", "Pro\u00b7pel\u00b7ler\u00b7schwin\u00b7gen", "wei\u00b7ter\u00b7geht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Wenn ihm das gelingt,", "tokens": ["Wenn", "ihm", "das", "ge\u00b7lingt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDS", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Wenn er nicht dar\u00fcber ganz verkommt,", "tokens": ["Wenn", "er", "nicht", "da\u00b7r\u00fc\u00b7ber", "ganz", "ver\u00b7kommt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "PAV", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wenn ihm die Erkenntnis frommt,", "tokens": ["Wenn", "ihm", "die", "Er\u00b7kennt\u00b7nis", "frommt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df die Nachtigall genau so singt", "tokens": ["Da\u00df", "die", "Nach\u00b7ti\u00b7gall", "ge\u00b7nau", "so", "singt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJD", "ADV", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Wie ein Spatz", "tokens": ["Wie", "ein", "Spatz"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Am Alexanderplatz, \u2013 \u2013 \u2013", "tokens": ["Am", "A\u00b7lex\u00b7an\u00b7der\u00b7platz", ",", "\u2013", "\u2013", "\u2013"], "token_info": ["word", "word", "punct", "punct", "punct", "punct"], "pos": ["APPRART", "NN", "$,", "$(", "$(", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ja, dann wird ihn wohl von Zeit zu Zeit", "tokens": ["Ja", ",", "dann", "wird", "ihn", "wohl", "von", "Zeit", "zu", "Zeit"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "ADV", "VAFIN", "PPER", "ADV", "APPR", "NN", "APPR", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Eine Sehnsucht wieder landw\u00e4rts tragen", "tokens": ["Ei\u00b7ne", "Sehn\u00b7sucht", "wie\u00b7der", "land\u00b7w\u00e4rts", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "ADV", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "In die Enge, in die Einsamkeit. \u2013 \u2013", "tokens": ["In", "die", "En\u00b7ge", ",", "in", "die", "Ein\u00b7sam\u00b7keit", ".", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$.", "$(", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.10": {"text": "Bis die simplen, friedlichen, gesunden", "tokens": ["Bis", "die", "sim\u00b7plen", ",", "fried\u00b7li\u00b7chen", ",", "ge\u00b7sun\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["APPR", "ART", "NN", "$,", "ADJA", "$,", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Bauern ihn nach Tagen", "tokens": ["Bau\u00b7ern", "ihn", "nach", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPER", "APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.12": {"text": "Oder Stunden", "tokens": ["O\u00b7der", "Stun\u00b7den"], "token_info": ["word", "word"], "pos": ["KON", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.13": {"text": "Wiederum verjagen;", "tokens": ["Wie\u00b7de\u00b7rum", "ver\u00b7ja\u00b7gen", ";"], "token_info": ["word", "word", "punct"], "pos": ["PWAV", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.14": {"text": "In die gro\u00dfe Stadt zur\u00fcck.", "tokens": ["In", "die", "gro\u00b7\u00dfe", "Stadt", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Und dort wird er sagen:", "tokens": ["Und", "dort", "wird", "er", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.16": {"text": "Nur im Ruhelosen ruht das Gl\u00fcck.", "tokens": ["Nur", "im", "Ru\u00b7he\u00b7lo\u00b7sen", "ruht", "das", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}