{"textgrid.poem.32634": {"metadata": {"author": {"name": "Pet\u00f6fi, S\u00e1ndor", "birth": "N.A.", "death": "N.A."}, "title": "1L: Au, mein R\u00fccken, \u2013 weh, mein R\u00fccken!", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Au, mein R\u00fccken, \u2013 weh, mein R\u00fccken!", "tokens": ["Au", ",", "mein", "R\u00fc\u00b7cken", ",", "\u2013", "weh", ",", "mein", "R\u00fc\u00b7cken", "!"], "token_info": ["word", "punct", "word", "word", "punct", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,", "$(", "PTKVZ", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eben heut", "tokens": ["E\u00b7ben", "heut"], "token_info": ["word", "word"], "pos": ["ADV", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Hat ihn mir der b\u00f6se Nachbar", "tokens": ["Hat", "ihn", "mir", "der", "b\u00f6\u00b7se", "Nach\u00b7bar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PPER", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Durchgebl\u00e4ut!", "tokens": ["Durch\u00b7ge\u00b7bl\u00e4ut", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Mit dem Kn\u00fcttel \u2013 h\u00e4tt' ihn daf\u00fcr", "tokens": ["Mit", "dem", "Kn\u00fct\u00b7tel", "\u2013", "h\u00e4tt'", "ihn", "da\u00b7f\u00fcr"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$(", "VAFIN", "PPER", "PAV"], "meter": "+-+-++-+", "measure": "unknown.measure.penta"}, "line.6": {"text": "Gott gestraft! \u2013", "tokens": ["Gott", "ge\u00b7straft", "!", "\u2013"], "token_info": ["word", "word", "punct", "punct"], "pos": ["NN", "VVPP", "$.", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Hieb er los auf meinen R\u00fccken", "tokens": ["Hieb", "er", "los", "auf", "mei\u00b7nen", "R\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKVZ", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Schauderhaft!", "tokens": ["Schau\u00b7der\u00b7haft", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Hat der Kerl nicht einen Garten?", "tokens": ["Hat", "der", "Kerl", "nicht", "ei\u00b7nen", "Gar\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PTKNEG", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und darin", "tokens": ["Und", "da\u00b7rin"], "token_info": ["word", "word"], "pos": ["KON", "PAV"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "B\u00e4ume, wo die allersch\u00f6nsten", "tokens": ["B\u00e4u\u00b7me", ",", "wo", "die", "al\u00b7ler\u00b7sch\u00f6ns\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Birnen bl\u00fchn?", "tokens": ["Bir\u00b7nen", "bl\u00fchn", "?"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "So verlockend haben die mich", "tokens": ["So", "ver\u00b7lo\u00b7ckend", "ha\u00b7ben", "die", "mich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVPP", "VAFIN", "ART", "PPER"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Angesehn,", "tokens": ["An\u00b7ge\u00b7sehn", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Kann ein schwacher Mensch, wie ich, da", "tokens": ["Kann", "ein", "schwa\u00b7cher", "Mensch", ",", "wie", "ich", ",", "da"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["VMFIN", "ART", "ADJA", "NN", "$,", "PWAV", "PPER", "$,", "KOUS"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Widerstehn?", "tokens": ["Wi\u00b7der\u00b7stehn", "?"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Und ich \u00fcbersprang die Planke,", "tokens": ["Und", "ich", "\u00fc\u00b7bers\u00b7prang", "die", "Plan\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Aber \u2013 krach,", "tokens": ["A\u00b7ber", "\u2013", "krach", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["KON", "$(", "ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Lag ich da, als ob mir jede", "tokens": ["Lag", "ich", "da", ",", "als", "ob", "mir", "je\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "$,", "KOKOM", "KOUS", "PPER", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Rippe brach!", "tokens": ["Rip\u00b7pe", "brach", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "So erwischte mich der Nachbar,", "tokens": ["So", "er\u00b7wischte", "mich", "der", "Nach\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Zum Verdru\u00df!", "tokens": ["Zum", "Ver\u00b7dru\u00df", "!"], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Alles gab mir der Patron, nur", "tokens": ["Al\u00b7les", "gab", "mir", "der", "Pat\u00b7ron", ",", "nur"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PIS", "VVFIN", "PPER", "ART", "NN", "$,", "ADV"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.8": {"text": "Keinen Ku\u00df!", "tokens": ["Kei\u00b7nen", "Ku\u00df", "!"], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "Herrgott, hat der Alte mores", "tokens": ["Herr\u00b7gott", ",", "hat", "der", "Al\u00b7te", "mo\u00b7res"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "VAFIN", "ART", "NN", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mich gelehrt!", "tokens": ["Mich", "ge\u00b7lehrt", "!"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVPP", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Alle Engel hab' ich singen", "tokens": ["Al\u00b7le", "En\u00b7gel", "hab'", "ich", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da geh\u00f6rt!", "tokens": ["Da", "ge\u00b7h\u00f6rt", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Hiebe gab's, und wieder Hiebe,", "tokens": ["Hie\u00b7be", "gab's", ",", "und", "wie\u00b7der", "Hie\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "$,", "KON", "ADV", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ohne Zahl,", "tokens": ["Oh\u00b7ne", "Zahl", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Ach, wie viele, wei\u00df Gott selber", "tokens": ["Ach", ",", "wie", "vie\u00b7le", ",", "wei\u00df", "Gott", "sel\u00b7ber"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "PWAV", "PIS", "$,", "VVFIN", "NN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Nicht einmal!", "tokens": ["Nicht", "ein\u00b7mal", "!"], "token_info": ["word", "word", "punct"], "pos": ["PTKNEG", "ADV", "$."], "meter": "+--", "measure": "dactylic.init"}}, "stanza.5": {"line.1": {"text": "Und der Mond stand hoch am Himmel,", "tokens": ["Und", "der", "Mond", "stand", "hoch", "am", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Und in Ruh'", "tokens": ["Und", "in", "Ruh'"], "token_info": ["word", "word", "word"], "pos": ["KON", "APPR", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Schaute anfangs er dem grimmen", "tokens": ["Schau\u00b7te", "an\u00b7fangs", "er", "dem", "grim\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "PPER", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Treiben zu;", "tokens": ["Trei\u00b7ben", "zu", ";"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Endlich h\u00fcllt' in Trauerwolken", "tokens": ["End\u00b7lich", "h\u00fcllt'", "in", "Trau\u00b7er\u00b7wol\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Er sich ein,", "tokens": ["Er", "sich", "ein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PRF", "PTKVZ", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Um des Mitleids stille Tr\u00e4ne", "tokens": ["Um", "des", "Mit\u00b7leids", "stil\u00b7le", "Tr\u00e4\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Mir zu weihn.", "tokens": ["Mir", "zu", "weihn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.6": {"line.1": {"text": "Nur der b\u00f6se Nachbar f\u00fchlte", "tokens": ["Nur", "der", "b\u00f6\u00b7se", "Nach\u00b7bar", "f\u00fchl\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nicht so fein,", "tokens": ["Nicht", "so", "fein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Immer haut' er, immer \u00e4rger", "tokens": ["Im\u00b7mer", "haut'", "er", ",", "im\u00b7mer", "\u00e4r\u00b7ger"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADV", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf mich ein,", "tokens": ["Auf", "mich", "ein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKVZ", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Schrecklich war's, ein rechter Jammer,", "tokens": ["Schreck\u00b7lich", "wa\u00b7r's", ",", "ein", "rech\u00b7ter", "Jam\u00b7mer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Hieb auf Hieb \u2013", "tokens": ["Hieb", "auf", "Hieb", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Noch ein Gl\u00fcck, da\u00df ich nicht auf dem", "tokens": ["Noch", "ein", "Gl\u00fcck", ",", "da\u00df", "ich", "nicht", "auf", "dem"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "$,", "KOUS", "PPER", "PTKNEG", "APPR", "ART"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Platze blieb!", "tokens": ["Plat\u00b7ze", "blieb", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.7": {"line.1": {"text": "'s ist ja gut! \u2013 Und \u00fcberwunden", "tokens": ["'s", "ist", "ja", "gut", "!", "\u2013", "Und", "\u00fc\u00b7berw\u00b7un\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$.", "$(", "KON", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hab' ich's bald,", "tokens": ["Hab'", "ich's", "bald", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADV", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Doch mit Zinsen wird's dem Nachbar", "tokens": ["Doch", "mit", "Zin\u00b7sen", "wird's", "dem", "Nach\u00b7bar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "VAFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Heimgezahlt!", "tokens": ["Heim\u00b7ge\u00b7zahlt", "!"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Denn ich wei\u00df, wie viel's geschlagen, \u2013", "tokens": ["Denn", "ich", "wei\u00df", ",", "wie", "viel's", "ge\u00b7schla\u00b7gen", ",", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "PWAV", "PIS", "VVPP", "$,", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df verflucht", "tokens": ["Da\u00df", "ver\u00b7flucht"], "token_info": ["word", "word"], "pos": ["KOUS", "VVPP"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Gern und oft auch er verbotne", "tokens": ["Gern", "und", "oft", "auch", "er", "ver\u00b7bot\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KON", "ADV", "ADV", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Fr\u00fcchte sucht!", "tokens": ["Fr\u00fcch\u00b7te", "sucht", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.8": {"line.1": {"text": "Abends hin?", "tokens": ["A\u00b7bends", "hin", "?"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Glaubst du, Alter, da\u00df ich gar so", "tokens": ["Glaubst", "du", ",", "Al\u00b7ter", ",", "da\u00df", "ich", "gar", "so"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "NN", "$,", "KOUS", "PPER", "ADV", "ADV"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Bl\u00f6de bin?", "tokens": ["Bl\u00f6\u00b7de", "bin", "?"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VAFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Splitter nur in fremden Augen", "tokens": ["Split\u00b7ter", "nur", "in", "frem\u00b7den", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Siehst du Wicht,", "tokens": ["Siehst", "du", "Wicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Doch den Balken in den eignen", "tokens": ["Doch", "den", "Bal\u00b7ken", "in", "den", "eig\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Siehst du nicht!", "tokens": ["Siehst", "du", "nicht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.9": {"line.1": {"text": "Weil ich ein paar Birnen mause,", "tokens": ["Weil", "ich", "ein", "paar", "Bir\u00b7nen", "mau\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seht doch an!", "tokens": ["Seht", "doch", "an", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Und er selber stiehlt, der Brave,", "tokens": ["Und", "er", "sel\u00b7ber", "stiehlt", ",", "der", "Bra\u00b7ve", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "VVFIN", "$,", "ART", "NN", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Wo er kann:", "tokens": ["Wo", "er", "kann", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VMFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Oft mit List,", "tokens": ["Oft", "mit", "List", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Wenn die Mutter gl\u00e4ubig just die", "tokens": ["Wenn", "die", "Mut\u00b7ter", "gl\u00e4u\u00b7big", "just", "die"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJD", "VVFIN", "ART"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Bibel liest.", "tokens": ["Bi\u00b7bel", "liest", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.10": {"line.1": {"text": "Dich erwisch' ich einmal vor dem", "tokens": ["Dich", "er\u00b7wi\u00b7sch'", "ich", "ein\u00b7mal", "vor", "dem"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "ART"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Fenster schon,", "tokens": ["Fens\u00b7ter", "schon", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADV", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Und dann, wett' ich, kommst du mir nicht", "tokens": ["Und", "dann", ",", "wett'", "ich", ",", "kommst", "du", "mir", "nicht"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "PPER", "PTKNEG"], "meter": "-++-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Heil davon!", "tokens": ["Heil", "da\u00b7von", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PAV", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Gleich hol' ich die Mutter selber", "tokens": ["Gleich", "hol'", "ich", "die", "Mut\u00b7ter", "sel\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ADV"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Aus dem Haus, \u2013", "tokens": ["Aus", "dem", "Haus", ",", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "$,", "$("], "meter": "--+", "measure": "anapaest.init"}, "line.7": {"text": "Oder gie\u00df' den Wassereimer", "tokens": ["O\u00b7der", "gie\u00df'", "den", "Was\u00b7se\u00b7rei\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVIMP", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Auf dich aus!", "tokens": ["Auf", "dich", "aus", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}