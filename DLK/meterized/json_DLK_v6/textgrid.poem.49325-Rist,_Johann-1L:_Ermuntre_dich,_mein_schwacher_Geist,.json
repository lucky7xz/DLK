{"textgrid.poem.49325": {"metadata": {"author": {"name": "Rist, Johann", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ermuntre dich, mein schwacher Geist,", "genre": "verse", "period": "N.A.", "pub_year": 1637, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ermuntre dich, mein schwacher Geist,", "tokens": ["Er\u00b7munt\u00b7re", "dich", ",", "mein", "schwa\u00b7cher", "Geist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und trage gro\u00df Verlangen,", "tokens": ["Und", "tra\u00b7ge", "gro\u00df", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein kleines Kind, das Vater hei\u00dft,", "tokens": ["Ein", "klei\u00b7nes", "Kind", ",", "das", "Va\u00b7ter", "hei\u00dft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Freuden zu empfangen!", "tokens": ["Mit", "Freu\u00b7den", "zu", "emp\u00b7fan\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die\u00df ist die Nacht, darin es kam", "tokens": ["Die\u00df", "ist", "die", "Nacht", ",", "da\u00b7rin", "es", "kam"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PAV", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und menschlich Wesen an sich nahm,", "tokens": ["Und", "menschlich", "We\u00b7sen", "an", "sich", "nahm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "NN", "APPR", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Dadurch die Welt mit Treuen", "tokens": ["Da\u00b7durch", "die", "Welt", "mit", "Treu\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Als seine Braut zu freien.", "tokens": ["Als", "sei\u00b7ne", "Braut", "zu", "frei\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Willkommen, s\u00fc\u00dfer Br\u00e4utigam,", "tokens": ["Will\u00b7kom\u00b7men", ",", "s\u00fc\u00b7\u00dfer", "Br\u00e4u\u00b7ti\u00b7gam", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Du K\u00f6nig aller Ehren,", "tokens": ["Du", "K\u00f6\u00b7nig", "al\u00b7ler", "Eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Willkommen Jesu, Gottes Lamm,", "tokens": ["Will\u00b7kom\u00b7men", "Je\u00b7su", ",", "Got\u00b7tes", "Lamm", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Ich wil dein Lob vermehren;", "tokens": ["Ich", "wil", "dein", "Lob", "ver\u00b7meh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Ich wil dir all mein Leben lang", "tokens": ["Ich", "wil", "dir", "all", "mein", "Le\u00b7ben", "lang"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "PIAT", "PPOSAT", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Vom Herzen sagen Preis und Dank", "tokens": ["Vom", "Her\u00b7zen", "sa\u00b7gen", "Preis", "und", "Dank"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Da\u00df du, da wir verloren,", "tokens": ["Da\u00df", "du", ",", "da", "wir", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "KOUS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "F\u00fcr uns bist Mensch geboren.", "tokens": ["F\u00fcr", "uns", "bist", "Mensch", "ge\u00b7bo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "O gro\u00dfer Gott, wie k\u00f6nt' es sein,", "tokens": ["O", "gro\u00b7\u00dfer", "Gott", ",", "wie", "k\u00f6nt'", "es", "sein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "PWAV", "VMFIN", "PPER", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Dein Himmelreich zu lassen,", "tokens": ["Dein", "Him\u00b7mel\u00b7reich", "zu", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Zu springen in die Welt hinein,", "tokens": ["Zu", "sprin\u00b7gen", "in", "die", "Welt", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Da nichts denn Neid und Hassen,", "tokens": ["Da", "nichts", "denn", "Neid", "und", "Has\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.21": {"text": "Wie k\u00f6ntest du die gro\u00dfe Macht,", "tokens": ["Wie", "k\u00f6n\u00b7test", "du", "die", "gro\u00b7\u00dfe", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Dein K\u00f6nigreich, den Freuden-Pracht,", "tokens": ["Dein", "K\u00f6\u00b7nig\u00b7reich", ",", "den", "Freu\u00b7den\u00b7Pracht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Ja, solch ein herrlichs Leben", "tokens": ["Ja", ",", "solch", "ein", "herr\u00b7lichs", "Le\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PIAT", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.24": {"text": "F\u00fcr deine Feind' hingeben?", "tokens": ["F\u00fcr", "dei\u00b7ne", "Feind'", "hin\u00b7ge\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.25": {"text": "Ist doch, Herr Jesu, deine Braut", "tokens": ["Ist", "doch", ",", "Herr", "Je\u00b7su", ",", "dei\u00b7ne", "Braut"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "ADV", "$,", "NN", "NE", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Ganz arm und voller Schanden;", "tokens": ["Ganz", "arm", "und", "vol\u00b7ler", "Schan\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.27": {"text": "Noch hast du sie dir selbst vertraut", "tokens": ["Noch", "hast", "du", "sie", "dir", "selbst", "ver\u00b7traut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "PPER", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Am Kreuz in Todesbanden.", "tokens": ["Am", "Kreuz", "in", "To\u00b7des\u00b7ban\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.29": {"text": "Ist sie doch nichts als Ueberdru\u00df,", "tokens": ["Ist", "sie", "doch", "nichts", "als", "Ue\u00b7berd\u00b7ru\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PIS", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Fluch, Unflat, Tod und Finsternus,", "tokens": ["Fluch", ",", "Un\u00b7flat", ",", "Tod", "und", "Fins\u00b7ter\u00b7nus", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Und du magst ihrentwegen", "tokens": ["Und", "du", "magst", "ih\u00b7rent\u00b7we\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "PPOSAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.32": {"text": "Den Scepter von dir legen!", "tokens": ["Den", "Scep\u00b7ter", "von", "dir", "le\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.33": {"text": "Du F\u00fcrst und Herscher dieser Welt,", "tokens": ["Du", "F\u00fcrst", "und", "Her\u00b7scher", "die\u00b7ser", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "KON", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Du Friedens Wiederbringer,", "tokens": ["Du", "Frie\u00b7dens", "Wie\u00b7der\u00b7brin\u00b7ger", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "NE", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.35": {"text": "Du kluger Rat und tapfrer Held,", "tokens": ["Du", "klu\u00b7ger", "Rat", "und", "tapf\u00b7rer", "Held", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Du starker Hellenzwinger,", "tokens": ["Du", "star\u00b7ker", "Hel\u00b7len\u00b7zwin\u00b7ger", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.37": {"text": "Wie war es m\u00fcglich, da\u00df du dich", "tokens": ["Wie", "war", "es", "m\u00fcg\u00b7lich", ",", "da\u00df", "du", "dich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "ADJD", "$,", "KOUS", "PPER", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Erniedrigtest so j\u00e4mmerlich,", "tokens": ["Er\u00b7nied\u00b7rig\u00b7test", "so", "j\u00e4m\u00b7mer\u00b7lich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Als w\u00e4rest du im Orden", "tokens": ["Als", "w\u00e4\u00b7rest", "du", "im", "Or\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "VAFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.40": {"text": "Der Bettler Mensch geworden?", "tokens": ["Der", "Bett\u00b7ler", "Mensch", "ge\u00b7wor\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.41": {"text": "O Freudenzeit, o Wundernacht,", "tokens": ["O", "Freu\u00b7den\u00b7zeit", ",", "o", "Wun\u00b7der\u00b7nacht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Dergleichen nie gefunden!", "tokens": ["Derg\u00b7lei\u00b7chen", "nie", "ge\u00b7fun\u00b7den", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.43": {"text": "Du hast den Heiland hergebracht,", "tokens": ["Du", "hast", "den", "Hei\u00b7land", "her\u00b7ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.44": {"text": "Der alles \u00fcberwunden.", "tokens": ["Der", "al\u00b7les", "\u00fc\u00b7berw\u00b7un\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.45": {"text": "Du hast gebracht den starken Mann,", "tokens": ["Du", "hast", "ge\u00b7bracht", "den", "star\u00b7ken", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.46": {"text": "Der Feur und Wolken zwingen kan,", "tokens": ["Der", "Feur", "und", "Wol\u00b7ken", "zwin\u00b7gen", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.47": {"text": "F\u00fcr dem die Himmel zittren,", "tokens": ["F\u00fcr", "dem", "die", "Him\u00b7mel", "zitt\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.48": {"text": "Und alle Berg ersch\u00fcttren.", "tokens": ["Und", "al\u00b7le", "Berg", "er\u00b7sch\u00fctt\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.49": {"text": "Du bleicher Mond, halt eiligst ein", "tokens": ["Du", "blei\u00b7cher", "Mond", ",", "halt", "ei\u00b7ligst", "ein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "$,", "VVFIN", "ADV", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.50": {"text": "Den blassen Schein auf Erden,", "tokens": ["Den", "blas\u00b7sen", "Schein", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.51": {"text": "Wirf deinen Glanz zum Stall hinein,", "tokens": ["Wirf", "dei\u00b7nen", "Glanz", "zum", "Stall", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.52": {"text": "Gott sol ges\u00e4uget werden.", "tokens": ["Gott", "sol", "ge\u00b7s\u00e4u\u00b7get", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.53": {"text": "Ihr helle Sternlein, stehet stil", "tokens": ["Ihr", "hel\u00b7le", "Stern\u00b7lein", ",", "ste\u00b7het", "stil"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "VVFIN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.54": {"text": "Und horcht, was euer Sch\u00f6pfer wil,", "tokens": ["Und", "horcht", ",", "was", "eu\u00b7er", "Sch\u00f6p\u00b7fer", "wil", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PRELS", "PPOSAT", "NN", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.55": {"text": "Der schwach und ungewieget", "tokens": ["Der", "schwach", "und", "un\u00b7ge\u00b7wie\u00b7get"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.56": {"text": "In einem Kripplein liget.", "tokens": ["In", "ei\u00b7nem", "Krip\u00b7plein", "li\u00b7get", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.57": {"text": "Du tummes Vieh, was bl\u00f6kest du", "tokens": ["Du", "tum\u00b7mes", "Vieh", ",", "was", "bl\u00f6\u00b7kest", "du"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "$,", "PWS", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.58": {"text": "Dort bei des Herren Mutter?", "tokens": ["Dort", "bei", "des", "Her\u00b7ren", "Mut\u00b7ter", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.59": {"text": "Immanuel h\u00e4lt seine Ruh", "tokens": ["Im\u00b7ma\u00b7nu\u00b7el", "h\u00e4lt", "sei\u00b7ne", "Ruh"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "lin