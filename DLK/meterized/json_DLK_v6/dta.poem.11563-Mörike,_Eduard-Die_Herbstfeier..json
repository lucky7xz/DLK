{"dta.poem.11563": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Die Herbstfeier.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Auf! im traubenschwersten Thale", "tokens": ["Auf", "!", "im", "trau\u00b7ben\u00b7schwers\u00b7ten", "Tha\u00b7le"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["APPR", "$.", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stellt ein Fest des Bacchus an!", "tokens": ["Stellt", "ein", "Fest", "des", "Bac\u00b7chus", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NE", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Becher her und Opferschale!", "tokens": ["Be\u00b7cher", "her", "und", "Op\u00b7fer\u00b7scha\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und des Gottes Bild voran!", "tokens": ["Und", "des", "Got\u00b7tes", "Bild", "vo\u00b7ran", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Fl\u00f6te mit Gesang verk\u00fcnde", "tokens": ["Fl\u00f6\u00b7te", "mit", "Ge\u00b7sang", "ver\u00b7k\u00fcn\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Gleich des Tages letzten Rest,", "tokens": ["Gleich", "des", "Ta\u00b7ges", "letz\u00b7ten", "Rest", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Mit dem Abendstern entz\u00fcnde", "tokens": ["Mit", "dem", "A\u00b7bends\u00b7tern", "ent\u00b7z\u00fcn\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.8": {"text": "Sich auch unser Freudenfest!", "tokens": ["Sich", "auch", "un\u00b7ser", "Freu\u00b7den\u00b7fest", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Braune M\u00e4nner, sch\u00f6ne Frauen", "tokens": ["Brau\u00b7ne", "M\u00e4n\u00b7ner", ",", "sch\u00f6\u00b7ne", "Frau\u00b7en"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "NN", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Soll man hier versammelt seh'n,", "tokens": ["Soll", "man", "hier", "ver\u00b7sam\u00b7melt", "seh'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ADV", "VVPP", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Greise auch, die ehrengrauen,", "tokens": ["Grei\u00b7se", "auch", ",", "die", "eh\u00b7ren\u00b7grau\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "PRELS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "D\u00fcrfen nicht von ferne steh'n;", "tokens": ["D\u00fcr\u00b7fen", "nicht", "von", "fer\u00b7ne", "steh'n", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "APPR", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Knaben, so die Kr\u00fcge f\u00fcllen,", "tokens": ["Kna\u00b7ben", ",", "so", "die", "Kr\u00fc\u00b7ge", "f\u00fcl\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und, da\u00df er vollkommen sey,", "tokens": ["Und", ",", "da\u00df", "er", "voll\u00b7kom\u00b7men", "sey", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Treten z\u00f6gernd auch die stillen", "tokens": ["Tre\u00b7ten", "z\u00f6\u00b7gernd", "auch", "die", "stil\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "ADV", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "M\u00e4dchen unserm Kranze bei.", "tokens": ["M\u00e4d\u00b7chen", "un\u00b7serm", "Kran\u00b7ze", "bei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Noch ist vor der nahen Feier", "tokens": ["Noch", "ist", "vor", "der", "na\u00b7hen", "Fei\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "S\u00fc\u00df beklommen manche Brust,", "tokens": ["S\u00fc\u00df", "be\u00b7klom\u00b7men", "man\u00b7che", "Brust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber weiter bald und freier", "tokens": ["A\u00b7ber", "wei\u00b7ter", "bald", "und", "frei\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Uebergibt sie sich der Lust.", "tokens": ["Ue\u00b7berg\u00b7ibt", "sie", "sich", "der", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Thaut euch nicht wie Fr\u00fchlingsregen", "tokens": ["Thaut", "euch", "nicht", "wie", "Fr\u00fch\u00b7lings\u00b7re\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "KOKOM", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Lieblicher Gedankenschwarm?", "tokens": ["Lieb\u00b7li\u00b7cher", "Ge\u00b7dan\u00b7ken\u00b7schwarm", "?"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Erdenleben, la\u00df dich hegen,", "tokens": ["Er\u00b7den\u00b7le\u00b7ben", ",", "la\u00df", "dich", "he\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Uns ist wohl in deinem Arm!", "tokens": ["Uns", "ist", "wohl", "in", "dei\u00b7nem", "Arm", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wahrlich und schon mit Entz\u00fccken", "tokens": ["Wahr\u00b7lich", "und", "schon", "mit", "Ent\u00b7z\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KON", "ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist der Gott in vollem Lauf,", "tokens": ["Ist", "der", "Gott", "in", "vol\u00b7lem", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schlie\u00dft vor den erw\u00e4rmten Blicken", "tokens": ["Schlie\u00dft", "vor", "den", "er\u00b7w\u00e4rm\u00b7ten", "Bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Seine goldnen Himmel auf.", "tokens": ["Sei\u00b7ne", "gold\u00b7nen", "Him\u00b7mel", "auf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Amor auch hat nichts dawider,", "tokens": ["A\u00b7mor", "auch", "hat", "nichts", "da\u00b7wi\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VAFIN", "PIS", "PAV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wenn sich Wang' an Wange neigt,", "tokens": ["Wenn", "sich", "Wang'", "an", "Wan\u00b7ge", "neigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und der Mund, im Takt der Lieder,", "tokens": ["Und", "der", "Mund", ",", "im", "Takt", "der", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "APPRART", "NN", "ART", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.8": {"text": "Sich dem Mund entgegen beugt.", "tokens": ["Sich", "dem", "Mund", "ent\u00b7ge\u00b7gen", "beugt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "PTKVZ", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}