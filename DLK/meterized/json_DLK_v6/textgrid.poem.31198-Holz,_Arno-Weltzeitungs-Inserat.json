{"textgrid.poem.31198": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "Weltzeitungs-Inserat", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gesucht wird f\u00fcr sofort ein t\u00fcchtger M\u00e4her.", "tokens": ["Ge\u00b7sucht", "wird", "f\u00fcr", "so\u00b7fort", "ein", "t\u00fccht\u00b7ger", "M\u00e4\u00b7her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Adressen sub Bureau zum grossen Pan.", "tokens": ["Ad\u00b7res\u00b7sen", "sub", "Bu\u00b7re\u00b7au", "zum", "gros\u00b7sen", "Pan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Denn dreigekr\u00f6nt sitzt noch ein Pharis\u00e4er", "tokens": ["Denn", "drei\u00b7ge\u00b7kr\u00f6nt", "sitzt", "noch", "ein", "Pha\u00b7ri\u00b7s\u00e4\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auf seinem S\u00fcndenstuhl im Vatikan.", "tokens": ["Auf", "sei\u00b7nem", "S\u00fcn\u00b7den\u00b7stuhl", "im", "Va\u00b7ti\u00b7kan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}