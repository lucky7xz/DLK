{"dta.poem.23366": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Krebsgang.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ihr passet recht zu unsern Zeiten,", "tokens": ["Ihr", "pas\u00b7set", "recht", "zu", "un\u00b7sern", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und wisset was uns n\u00fctzt und frommt!", "tokens": ["Und", "wis\u00b7set", "was", "uns", "n\u00fctzt", "und", "frommt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PWS", "PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr werdet immer r\u00fcckw\u00e4rts schreiten,", "tokens": ["Ihr", "wer\u00b7det", "im\u00b7mer", "r\u00fcck\u00b7w\u00e4rts", "schrei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bis ihr zur Schlacht von Jena kommt.", "tokens": ["Bis", "ihr", "zur", "Schlacht", "von", "Je\u00b7na", "kommt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "APPRART", "NN", "APPR", "NE", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch, lieben Leute, la\u00dft euch sagen:", "tokens": ["Doch", ",", "lie\u00b7ben", "Leu\u00b7te", ",", "la\u00dft", "euch", "sa\u00b7gen", ":"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADJA", "NN", "$,", "VVIMP", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Erreicht ihr wieder euren Zweck,", "tokens": ["Er\u00b7reicht", "ihr", "wie\u00b7der", "eu\u00b7ren", "Zweck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr werdet wiederum geschlagen,", "tokens": ["Ihr", "wer\u00b7det", "wie\u00b7de\u00b7rum", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und Staat und Kirche liegt im Dreck.", "tokens": ["Und", "Staat", "und", "Kir\u00b7che", "liegt", "im", "Dreck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}