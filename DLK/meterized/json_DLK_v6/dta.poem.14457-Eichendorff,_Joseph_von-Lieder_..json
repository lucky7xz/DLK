{"dta.poem.14457": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Lieder .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Denk' ich Dein, mu\u00df bald verwehen", "tokens": ["Denk'", "ich", "Dein", ",", "mu\u00df", "bald", "ver\u00b7we\u00b7hen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PPOSAT", "$,", "VMFIN", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Alle Tr\u00fcbni\u00df weit und breit,", "tokens": ["Al\u00b7le", "Tr\u00fcb\u00b7ni\u00df", "weit", "und", "breit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die frischen Blicke gehen", "tokens": ["Und", "die", "fri\u00b7schen", "Bli\u00b7cke", "ge\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie in einen Garten weit.", "tokens": ["Wie", "in", "ei\u00b7nen", "Gar\u00b7ten", "weit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}