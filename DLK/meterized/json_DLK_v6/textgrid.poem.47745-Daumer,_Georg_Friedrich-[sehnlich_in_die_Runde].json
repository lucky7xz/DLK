{"textgrid.poem.47745": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[sehnlich in die Runde]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sehnlich in die Runde", "tokens": ["Sehn\u00b7lich", "in", "die", "Run\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Such' ich alle Stunde", "tokens": ["Such'", "ich", "al\u00b7le", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PIAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Mit des Auges Blitz,", "tokens": ["Mit", "des", "Au\u00b7ges", "Blitz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Suche mir ein T\u00e4ubchen,", "tokens": ["Su\u00b7che", "mir", "ein", "T\u00e4ub\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Suche mir ein Weibchen,", "tokens": ["Su\u00b7che", "mir", "ein", "Weib\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Einen Goldbesitz.", "tokens": ["Ei\u00b7nen", "Gold\u00b7be\u00b7sitz", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Fragst du, was f\u00fcr Eine", "tokens": ["Fragst", "du", ",", "was", "f\u00fcr", "Ei\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "PRELS", "APPR", "ART"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Mir der Geist empfehle?", "tokens": ["Mir", "der", "Geist", "emp\u00b7feh\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "Haben soll im Busen,", "tokens": ["Ha\u00b7ben", "soll", "im", "Bu\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VMFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.10": {"text": "Die ich will und w\u00e4hle,", "tokens": ["Die", "ich", "will", "und", "w\u00e4h\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VMFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.11": {"text": "Eine zarte Seele", "tokens": ["Ei\u00b7ne", "zar\u00b7te", "See\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.12": {"text": "Und im Haupte Witz.", "tokens": ["Und", "im", "Haup\u00b7te", "Witz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "NN", "$."], "meter": "--+-+", "measure": "anapaest.init"}, "line.13": {"text": "Haben soll sie weiter", "tokens": ["Ha\u00b7ben", "soll", "sie", "wei\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "VMFIN", "PPER", "ADV"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.14": {"text": "Eine Zung' im Munde,", "tokens": ["Ei\u00b7ne", "Zung'", "im", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.15": {"text": "Welche keine Wunde,", "tokens": ["Wel\u00b7che", "kei\u00b7ne", "Wun\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAT", "PIAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.16": {"text": "Keine b\u00f6se sticht,", "tokens": ["Kei\u00b7ne", "b\u00f6\u00b7se", "sticht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.17": {"text": "Augen hell un heiter,", "tokens": ["Au\u00b7gen", "hell", "un", "hei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "FM", "ADJD", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.18": {"text": "Wie der Sterne Licht,", "tokens": ["Wie", "der", "Ster\u00b7ne", "Licht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.19": {"text": "Warme, s\u00fc\u00dfe Lippen", "tokens": ["War\u00b7me", ",", "s\u00fc\u00b7\u00dfe", "Lip\u00b7pen"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.20": {"text": "Und ein Liebe l\u00e4chelnd", "tokens": ["Und", "ein", "Lie\u00b7be", "l\u00e4\u00b7chelnd"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJD"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.21": {"text": "Holdes Angesicht.", "tokens": ["Hol\u00b7des", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}