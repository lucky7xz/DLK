{"textgrid.poem.53537": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Gute Nacht!", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich geh mit meinen Wanzen schlafen,", "tokens": ["Ich", "geh", "mit", "mei\u00b7nen", "Wan\u00b7zen", "schla\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "rotbraun und platt.", "tokens": ["rot\u00b7braun", "und", "platt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Quartiert bin ich bei einem Grafen,", "tokens": ["Quar\u00b7tiert", "bin", "ich", "bei", "ei\u00b7nem", "Gra\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "der viele hat.", "tokens": ["der", "vie\u00b7le", "hat", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PIS", "VAFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Des Nachts, wenn alle Sterne funkeln,", "tokens": ["Des", "Nachts", ",", "wenn", "al\u00b7le", "Ster\u00b7ne", "fun\u00b7keln", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "$,", "KOUS", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "dann ziehen still", "tokens": ["dann", "zie\u00b7hen", "still"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVINF", "VMFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "die flei\u00dfigen Scharen hin im Dunkeln,", "tokens": ["die", "flei\u00b7\u00dfi\u00b7gen", "Scha\u00b7ren", "hin", "im", "Dun\u00b7keln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "APPRART", "ADJA", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "wie Gott es will.", "tokens": ["wie", "Gott", "es", "will", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PPER", "VMFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Sie kommen aus den schmalen Ritzen,", "tokens": ["Sie", "kom\u00b7men", "aus", "den", "schma\u00b7len", "Rit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "aus dem Parkett;", "tokens": ["aus", "dem", "Par\u00b7kett", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "die feinern aber fastend sitzen", "tokens": ["die", "fei\u00b7nern", "a\u00b7ber", "fas\u00b7tend", "sit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADV", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "des Tags im Bett.", "tokens": ["des", "Tags", "im", "Bett", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Sie pieken mich. Es schwillt zu riesigen", "tokens": ["Sie", "pie\u00b7ken", "mich", ".", "Es", "schwillt", "zu", "rie\u00b7si\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$.", "PPER", "VVFIN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Fleischkl\u00fcmpchen an, was sie gepackt;", "tokens": ["Fleischkl\u00fcmp\u00b7chen", "an", ",", "was", "sie", "ge\u00b7packt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "das macht die Bei\u00dfekunst der Hiesigen \u2013", "tokens": ["das", "macht", "die", "Bei\u00b7\u00df\u00b7e\u00b7kunst", "der", "Hie\u00b7si\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "die sind exakt.", "tokens": ["die", "sind", "ex\u00b7akt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Sie pieken mich. Es juckt. Zum Gl\u00fccke", "tokens": ["Sie", "pie\u00b7ken", "mich", ".", "Es", "juckt", ".", "Zum", "Gl\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$.", "PPER", "VVFIN", "$.", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "ist morgen alles wieder rein.", "tokens": ["ist", "mor\u00b7gen", "al\u00b7les", "wie\u00b7der", "rein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIS", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wenn ich eine sanft zerdr\u00fccke,", "tokens": ["Und", "wenn", "ich", "ei\u00b7ne", "sanft", "zer\u00b7dr\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ART", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "gedenk ich dein.", "tokens": ["ge\u00b7denk", "ich", "dein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}