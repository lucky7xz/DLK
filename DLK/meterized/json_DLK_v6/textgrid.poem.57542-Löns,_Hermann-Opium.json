{"textgrid.poem.57542": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Opium", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00df mich deinen Leib umfangen,", "tokens": ["La\u00df", "mich", "dei\u00b7nen", "Leib", "um\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wilde Dirne, k\u00fcsse mich,", "tokens": ["Wil\u00b7de", "Dir\u00b7ne", ",", "k\u00fcs\u00b7se", "mich", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00df an deinem Mund mich hangen,", "tokens": ["La\u00df", "an", "dei\u00b7nem", "Mund", "mich", "han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "PPOSAT", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Heut, nur heute liebe mich,", "tokens": ["Heut", ",", "nur", "heu\u00b7te", "lie\u00b7be", "mich", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "ADV", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "K\u00fcsse mich, du fremde Dirne,", "tokens": ["K\u00fcs\u00b7se", "mich", ",", "du", "frem\u00b7de", "Dir\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Deine Lippen, liebesrot,", "tokens": ["Dei\u00b7ne", "Lip\u00b7pen", ",", "lie\u00b7bes\u00b7rot", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Press' mir auf die hei\u00dfe Stirne \u2013", "tokens": ["Press'", "mir", "auf", "die", "hei\u00b7\u00dfe", "Stir\u00b7ne", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPR", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Heute rot und morgen tot.", "tokens": ["Heu\u00b7te", "rot", "und", "mor\u00b7gen", "tot", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Rote Blumen in den Haaren,", "tokens": ["Ro\u00b7te", "Blu\u00b7men", "in", "den", "Haa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Feldmohnbl\u00fcten, schnell verweht,", "tokens": ["Feld\u00b7mohn\u00b7bl\u00fc\u00b7ten", ",", "schnell", "ver\u00b7weht", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Morgen ist im Sturm zerfahren,", "tokens": ["Mor\u00b7gen", "ist", "im", "Sturm", "zer\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was noch heute prangend steht,", "tokens": ["Was", "noch", "heu\u00b7te", "pran\u00b7gend", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "K\u00fcsse mich und sing mir Lieder,", "tokens": ["K\u00fcs\u00b7se", "mich", "und", "sing", "mir", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "KON", "VVFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Lieder hei\u00df und brennend rot,", "tokens": ["Lie\u00b7der", "hei\u00df", "und", "bren\u00b7nend", "rot", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "ADJD", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "K\u00fcss' mich immer, immer wieder \u2013", "tokens": ["K\u00fcss'", "mich", "im\u00b7mer", ",", "im\u00b7mer", "wie\u00b7der", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "$,", "ADV", "ADV", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Heute rot und morgen tot.", "tokens": ["Heu\u00b7te", "rot", "und", "mor\u00b7gen", "tot", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Rote Flecken auf den Wangen,", "tokens": ["Ro\u00b7te", "Fle\u00b7cken", "auf", "den", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kranke Brust und Jubelton,", "tokens": ["Kran\u00b7ke", "Brust", "und", "Ju\u00b7bel\u00b7ton", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In den Augen Todesbangen,", "tokens": ["In", "den", "Au\u00b7gen", "To\u00b7des\u00b7ban\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf den Lippen Spott und Hohn,", "tokens": ["Auf", "den", "Lip\u00b7pen", "Spott", "und", "Hohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "K\u00fcsse mich bis zum Ersticken,", "tokens": ["K\u00fcs\u00b7se", "mich", "bis", "zum", "Er\u00b7sti\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "K\u00fcss' die blasse Wang mir rot,", "tokens": ["K\u00fcss'", "die", "blas\u00b7se", "Wang", "mir", "rot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Heute Jubel und Entz\u00fccken \u2013", "tokens": ["Heu\u00b7te", "Ju\u00b7bel", "und", "Ent\u00b7z\u00fc\u00b7cken", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Heute rot und morgen tot.", "tokens": ["Heu\u00b7te", "rot", "und", "mor\u00b7gen", "tot", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wei\u00dfe Arme, schwarze Locken,", "tokens": ["Wei\u00b7\u00dfe", "Ar\u00b7me", ",", "schwar\u00b7ze", "Lo\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Vollen Busens hei\u00dfer Schlag,", "tokens": ["Vol\u00b7len", "Bu\u00b7sens", "hei\u00b7\u00dfer", "Schlag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schau, da fall'n des Mohnes Flocken,", "tokens": ["Schau", ",", "da", "fall'n", "des", "Moh\u00b7nes", "Flo\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bl\u00fchten einen kurzen Tag,", "tokens": ["Bl\u00fch\u00b7ten", "ei\u00b7nen", "kur\u00b7zen", "Tag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "La\u00df an meine Brust dich pressen,", "tokens": ["La\u00df", "an", "mei\u00b7ne", "Brust", "dich", "pres\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "K\u00fcss' mit Lippen, liebesrot,", "tokens": ["K\u00fcss'", "mit", "Lip\u00b7pen", ",", "lie\u00b7bes\u00b7rot", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "APPR", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "K\u00fcsse mich bis zum Vergessen \u2013", "tokens": ["K\u00fcs\u00b7se", "mich", "bis", "zum", "Ver\u00b7ges\u00b7sen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "APPRART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Heute rot und morgen tot.", "tokens": ["Heu\u00b7te", "rot", "und", "mor\u00b7gen", "tot", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}