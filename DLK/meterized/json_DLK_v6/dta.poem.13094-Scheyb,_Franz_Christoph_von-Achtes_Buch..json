{"dta.poem.13094": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Achtes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jnzwischen nahm ich wahr, da\u00df wieder jene sprach", "tokens": ["I\u00b7nzwi\u00b7schen", "nahm", "ich", "wahr", ",", "da\u00df", "wie\u00b7der", "je\u00b7ne", "sprach"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$,", "KOUS", "ADV", "PDS", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "290Die man die Dicht-Kunst hie\u00df, und Anfangs unterbrach:", "tokens": ["man", "die", "Dicht\u00b7Kunst", "hie\u00df", ",", "und", "An\u00b7fangs", "un\u00b7ter\u00b7brach", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "NN", "VVFIN", "$,", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Sie wies sich unbesorgt, was sie zu melden habe,", "tokens": ["Sie", "wies", "sich", "un\u00b7be\u00b7sorgt", ",", "was", "sie", "zu", "mel\u00b7den", "ha\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "$,", "PRELS", "PPER", "PTKZU", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da sie sich unerregt so zu vernehmen gabe:", "tokens": ["Da", "sie", "sich", "un\u00b7er\u00b7regt", "so", "zu", "ver\u00b7neh\u00b7men", "ga\u00b7be", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "ADV", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandr