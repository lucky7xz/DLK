{"textgrid.poem.67224": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Menschen finden sich in dieser Welt zum Leben,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Menschen finden sich in dieser Welt zum Leben,", "tokens": ["Die", "Men\u00b7schen", "fin\u00b7den", "sich", "in", "die\u00b7ser", "Welt", "zum", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "PDAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie Jahre sind, wie Zeiten h\u00f6her streben,", "tokens": ["Wie", "Jah\u00b7re", "sind", ",", "wie", "Zei\u00b7ten", "h\u00f6\u00b7her", "stre\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VAFIN", "$,", "PWAV", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So wie der Wechsel ist, ist \u00fcbrig vieles Wahre,", "tokens": ["So", "wie", "der", "Wech\u00b7sel", "ist", ",", "ist", "\u00fcb\u00b7rig", "vie\u00b7les", "Wah\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "VAFIN", "$,", "VAFIN", "ADJD", "PIS", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df Dauer kommt in die verschiednen Jahre;", "tokens": ["Da\u00df", "Dau\u00b7er", "kommt", "in", "die", "ver\u00b7schied\u00b7nen", "Jah\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Vollkommenheit vereint sich so in diesem Leben,", "tokens": ["Voll\u00b7kom\u00b7men\u00b7heit", "ver\u00b7eint", "sich", "so", "in", "die\u00b7sem", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df diesem sich bequemt der Menschen edles Streben.", "tokens": ["Da\u00df", "die\u00b7sem", "sich", "be\u00b7quemt", "der", "Men\u00b7schen", "ed\u00b7les", "Stre\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "PRF", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}