{"textgrid.poem.60206": {"metadata": {"author": {"name": "Karsch, Anna Louisa", "birth": "N.A.", "death": "N.A."}, "title": "1L: O du! an den ich t\u00e4glich eine Menge", "genre": "verse", "period": "N.A.", "pub_year": 1762, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O du! an den ich t\u00e4glich eine Menge", "tokens": ["O", "du", "!", "an", "den", "ich", "t\u00e4g\u00b7lich", "ei\u00b7ne", "Men\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "$.", "APPR", "PRELS", "PPER", "ADJD", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Klagvoller Seufzer abgesandt,", "tokens": ["Klag\u00b7vol\u00b7ler", "Seuf\u00b7zer", "ab\u00b7ge\u00b7sandt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mi\u00df mein Gef\u00fchl nicht nach des Briefes L\u00e4nge,", "tokens": ["Mi\u00df", "mein", "Ge\u00b7f\u00fchl", "nicht", "nach", "des", "Brie\u00b7fes", "L\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PTKNEG", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ihn schrieb die zitternde Hand.", "tokens": ["Ihn", "schrieb", "die", "zit\u00b7tern\u00b7de", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Des Fiebers Gluth, empor ins Haupt gestiegen,", "tokens": ["Des", "Fie\u00b7bers", "Gluth", ",", "em\u00b7por", "ins", "Haupt", "ge\u00b7stie\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PTKVZ", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Fra\u00df den Gedanken, ehe er sich", "tokens": ["Fra\u00df", "den", "Ge\u00b7dan\u00b7ken", ",", "e\u00b7he", "er", "sich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "KOUS", "PPER", "PRF"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Entwickelte, da wo Gedanken liegen", "tokens": ["Ent\u00b7wi\u00b7ckel\u00b7te", ",", "da", "wo", "Ge\u00b7dan\u00b7ken", "lie\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "ADV", "PWAV", "NN", "VVFIN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "In der Empfindung f\u00fcr dich!", "tokens": ["In", "der", "Emp\u00b7fin\u00b7dung", "f\u00fcr", "dich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "PPER", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "So sengt in heissen unbew\u00f6lkten Tagen", "tokens": ["So", "sengt", "in", "heis\u00b7sen", "un\u00b7be\u00b7w\u00f6lk\u00b7ten", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Mittags-Sonne Blumen ab,", "tokens": ["Die", "Mit\u00b7tags\u00b7Son\u00b7ne", "Blu\u00b7men", "ab", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die halb verh\u00fcllt noch in der Knospe lagen.", "tokens": ["Die", "halb", "ver\u00b7h\u00fcllt", "noch", "in", "der", "Knos\u00b7pe", "la\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVPP", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So fliehen Bl\u00e4tter herab", "tokens": ["So", "flie\u00b7hen", "Bl\u00e4t\u00b7ter", "her\u00b7ab"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "ADV"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Vom Lindenbaum, wenn vor den Ungewittern,", "tokens": ["Vom", "Lin\u00b7den\u00b7baum", ",", "wenn", "vor", "den", "Un\u00b7ge\u00b7wit\u00b7tern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KOUS", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der losgela\u00dfne Sturm ihn schwenkt,", "tokens": ["Der", "los\u00b7ge\u00b7la\u00df\u00b7ne", "Sturm", "ihn", "schwenkt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und einen Gott mit unterdr\u00fccktem Zittern", "tokens": ["Und", "ei\u00b7nen", "Gott", "mit", "un\u00b7ter\u00b7dr\u00fcck\u00b7tem", "Zit\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der S\u00fcnder f\u00fchlet und denkt.", "tokens": ["Der", "S\u00fcn\u00b7der", "f\u00fch\u00b7let", "und", "denkt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "O du Geliebter! ahnde nicht mein Schweigen;", "tokens": ["O", "du", "Ge\u00b7lieb\u00b7ter", "!", "ahn\u00b7de", "nicht", "mein", "Schwei\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "$.", "VVFIN", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gezwungne S\u00fcnden r\u00e4che nicht!", "tokens": ["Ge\u00b7zwung\u00b7ne", "S\u00fcn\u00b7den", "r\u00e4\u00b7che", "nicht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gieb mir, gieb mir oft deiner Liebe Zeugen.", "tokens": ["Gieb", "mir", ",", "gieb", "mir", "oft", "dei\u00b7ner", "Lie\u00b7be", "Zeu\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "VVIMP", "PPER", "ADV", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das harte Siegel zerbricht", "tokens": ["Das", "har\u00b7te", "Sie\u00b7gel", "zer\u00b7bricht"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "Von meinem Ku\u00df, der heftig aufgedr\u00fccket", "tokens": ["Von", "mei\u00b7nem", "Ku\u00df", ",", "der", "hef\u00b7tig", "auf\u00b7ge\u00b7dr\u00fc\u00b7cket"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PRELS", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von Lippen wird, die geizig dich", "tokens": ["Von", "Lip\u00b7pen", "wird", ",", "die", "gei\u00b7zig", "dich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "VAFIN", "$,", "PRELS", "ADJD", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Erwarten, Freund! wie werd ich dann entz\u00fccket!", "tokens": ["Er\u00b7war\u00b7ten", ",", "Freund", "!", "wie", "werd", "ich", "dann", "ent\u00b7z\u00fc\u00b7cket", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$.", "PWAV", "VAFIN", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "An deine heften sie sich;", "tokens": ["An", "dei\u00b7ne", "hef\u00b7ten", "sie", "sich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.7": {"line.1": {"text": "Und rednerisch wird unter tausend K\u00fcssen", "tokens": ["Und", "red\u00b7ne\u00b7risch", "wird", "un\u00b7ter", "tau\u00b7send", "K\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VAFIN", "APPR", "CARD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mein Herz, mit Wollust vollgetr\u00e4nkt,", "tokens": ["Mein", "Herz", ",", "mit", "Wol\u00b7lust", "voll\u00b7ge\u00b7tr\u00e4nkt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dir s\u00fcsse Nahmen herzustammeln wissen,", "tokens": ["Dir", "s\u00fcs\u00b7se", "Nah\u00b7men", "her\u00b7zu\u00b7stam\u00b7meln", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Sapho selber nicht denkt.", "tokens": ["Die", "Sa\u00b7pho", "sel\u00b7ber", "nicht", "denkt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "ADV", "PTKNEG", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}