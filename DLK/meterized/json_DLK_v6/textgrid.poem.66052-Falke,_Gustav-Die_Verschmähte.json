{"textgrid.poem.66052": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Die Verschm\u00e4hte", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Komm ich l\u00e4ngs der gr\u00fcnen Weide,", "tokens": ["Komm", "ich", "l\u00e4ngs", "der", "gr\u00fc\u00b7nen", "Wei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo die kleinen L\u00e4mmer grasen,", "tokens": ["Wo", "die", "klei\u00b7nen", "L\u00e4m\u00b7mer", "gra\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Immer h\u00f6r ich mir zu Leide", "tokens": ["Im\u00b7mer", "h\u00f6r", "ich", "mir", "zu", "Lei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eine helle Fl\u00f6te blasen.", "tokens": ["Ei\u00b7ne", "hel\u00b7le", "Fl\u00f6\u00b7te", "bla\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und da hockt er morgenmunter", "tokens": ["Und", "da", "hockt", "er", "mor\u00b7gen\u00b7mun\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Auf umbuschtem Erlensitze,", "tokens": ["Auf", "um\u00b7buschtem", "Er\u00b7len\u00b7sit\u00b7ze", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bl\u00e4st sein leichtes Lied herunter,", "tokens": ["Bl\u00e4st", "sein", "leich\u00b7tes", "Lied", "her\u00b7un\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich, den Schafen und dem Spitze.", "tokens": ["Sich", ",", "den", "Scha\u00b7fen", "und", "dem", "Spit\u00b7ze", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "$,", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Geh ich zehnmal hin und wieder,", "tokens": ["Geh", "ich", "zehn\u00b7mal", "hin", "und", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "PTKVZ", "KON", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wird er zehnmal mich nicht sehen;", "tokens": ["Wird", "er", "zehn\u00b7mal", "mich", "nicht", "se\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PPER", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und doch leuchtet rot mein Mieder,", "tokens": ["Und", "doch", "leuch\u00b7tet", "rot", "mein", "Mie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ADJD", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die hellen R\u00f6cke wehen.", "tokens": ["Und", "die", "hel\u00b7len", "R\u00f6\u00b7cke", "we\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Unerh\u00f6rte Liebesn\u00f6te", "tokens": ["Un\u00b7er\u00b7h\u00f6r\u00b7te", "Lie\u00b7bes\u00b7n\u00f6\u00b7te"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jeden Tag und jede Stunde.", "tokens": ["Je\u00b7den", "Tag", "und", "je\u00b7de", "Stun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "L\u00e4g doch statt der dummen Fl\u00f6te", "tokens": ["L\u00e4g", "doch", "statt", "der", "dum\u00b7men", "Fl\u00f6\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ich einmal an seinem Munde!", "tokens": ["Ich", "ein\u00b7mal", "an", "sei\u00b7nem", "Mun\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Doch er kann den Mund nur spitzen,", "tokens": ["Doch", "er", "kann", "den", "Mund", "nur", "spit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn es gilt, die Fl\u00f6te blasen;", "tokens": ["Wenn", "es", "gilt", ",", "die", "Fl\u00f6\u00b7te", "bla\u00b7sen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nichts kann ihm das Blut erhitzen,", "tokens": ["Nichts", "kann", "ihm", "das", "Blut", "er\u00b7hit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Als wenn L\u00e4mmer abseits grasen.", "tokens": ["Als", "wenn", "L\u00e4m\u00b7mer", "ab\u00b7seits", "gra\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Und in diesen T\u00f6lpel mu\u00df ich,", "tokens": ["Und", "in", "die\u00b7sen", "T\u00f6l\u00b7pel", "mu\u00df", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PDAT", "NN", "VMFIN", "PPER", "$,"], "meter": "+-+-+-++", "measure": "unknown.measure.penta"}, "line.2": {"text": "Dumme Liese, mich vergucken.", "tokens": ["Dum\u00b7me", "Lie\u00b7se", ",", "mich", "ver\u00b7gu\u00b7cken", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach, wie f\u00fchl nach seinem Ku\u00df ich", "tokens": ["Ach", ",", "wie", "f\u00fchl", "nach", "sei\u00b7nem", "Ku\u00df", "ich"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PWAV", "VVFIN", "APPR", "PPOSAT", "NN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meine Lippen jucken!", "tokens": ["Mei\u00b7ne", "Lip\u00b7pen", "ju\u00b7cken", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}