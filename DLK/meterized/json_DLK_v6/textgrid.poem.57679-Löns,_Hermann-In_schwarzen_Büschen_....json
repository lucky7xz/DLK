{"textgrid.poem.57679": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "In schwarzen B\u00fcschen ...", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In schwarzen B\u00fcschen fl\u00fcstert der Nachtwind,", "tokens": ["In", "schwar\u00b7zen", "B\u00fc\u00b7schen", "fl\u00fcs\u00b7tert", "der", "Nacht\u00b7wind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ein Eulenruf schallt aus dem Moor,", "tokens": ["Ein", "Eu\u00b7len\u00b7ruf", "schallt", "aus", "dem", "Moor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Grau ist die Nacht, zwei Sterne blinken", "tokens": ["Grau", "ist", "die", "Nacht", ",", "zwei", "Ster\u00b7ne", "blin\u00b7ken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$,", "CARD", "NN", "VVINF"], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Aus grauen Wolken schimmernd hervor.", "tokens": ["Aus", "grau\u00b7en", "Wol\u00b7ken", "schim\u00b7mernd", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "Die Sterne blitzen, die Sterne blinken,", "tokens": ["Die", "Ster\u00b7ne", "blit\u00b7zen", ",", "die", "Ster\u00b7ne", "blin\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "S\u00fc\u00dfes Gedenken mein Herz umspinnt,", "tokens": ["S\u00fc\u00b7\u00dfes", "Ge\u00b7den\u00b7ken", "mein", "Herz", "um\u00b7spinnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Aus deinen Augen schimmern mir Sterne,", "tokens": ["Aus", "dei\u00b7nen", "Au\u00b7gen", "schim\u00b7mern", "mir", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wenn sie ganz nahe an meinen sind.", "tokens": ["Wenn", "sie", "ganz", "na\u00b7he", "an", "mei\u00b7nen", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "APPR", "PPOSAT", "VAFIN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}}}}