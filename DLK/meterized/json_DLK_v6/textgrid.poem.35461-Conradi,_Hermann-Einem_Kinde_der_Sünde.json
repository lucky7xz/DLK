{"textgrid.poem.35461": {"metadata": {"author": {"name": "Conradi, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Einem Kinde der S\u00fcnde", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ob's deine Augen auch verneinen", "tokens": ["Ob's", "dei\u00b7ne", "Au\u00b7gen", "auch", "ver\u00b7nei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "PPOSAT", "NN", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit ihrem hellen, klaren Licht;", "tokens": ["Mit", "ih\u00b7rem", "hel\u00b7len", ",", "kla\u00b7ren", "Licht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ob auch auf deinem zarten, feinen,", "tokens": ["Ob", "auch", "auf", "dei\u00b7nem", "zar\u00b7ten", ",", "fei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "PPOSAT", "ADJA", "$,", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Madonnensch\u00f6nen Angesicht", "tokens": ["Ma\u00b7don\u00b7nen\u00b7sch\u00f6\u00b7nen", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es liegt, als w\u00e4re deine Seele", "tokens": ["Es", "liegt", ",", "als", "w\u00e4\u00b7re", "dei\u00b7ne", "See\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOKOM", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein seltner Kelch, der niemals trog,", "tokens": ["Ein", "selt\u00b7ner", "Kelch", ",", "der", "nie\u00b7mals", "trog", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Drin Keuschheit sich und Kraft verm\u00e4hle:", "tokens": ["Drin", "Keuschheit", "sich", "und", "Kraft", "ver\u00b7m\u00e4h\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "PRF", "KON", "NN", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Ob deine Augen drohend blitzen \u2013", "tokens": ["Ob", "dei\u00b7ne", "Au\u00b7gen", "dro\u00b7hend", "blit\u00b7zen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVPP", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ob du auch zitternd, zornbewehrt,", "tokens": ["Ob", "du", "auch", "zit\u00b7ternd", ",", "zorn\u00b7be\u00b7wehrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVPP", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dich vor dem Frechen suchst zu sch\u00fctzen,", "tokens": ["Dich", "vor", "dem", "Fre\u00b7chen", "suchst", "zu", "sch\u00fct\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den deiner Sch\u00f6nheit Reiz bet\u00f6rt, \u2013", "tokens": ["Den", "dei\u00b7ner", "Sch\u00f6n\u00b7heit", "Reiz", "be\u00b7t\u00f6rt", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "PPOSAT", "NN", "NE", "VVFIN", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der deines Nackens holde F\u00fclle", "tokens": ["Der", "dei\u00b7nes", "Na\u00b7ckens", "hol\u00b7de", "F\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Umspannen will mit engem Joch \u2013", "tokens": ["Um\u00b7span\u00b7nen", "will", "mit", "en\u00b7gem", "Joch", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVINF", "VMFIN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ein Bild der lieblichsten Idylle! \u2013", "tokens": ["Ein", "Bild", "der", "lieb\u00b7lichs\u00b7ten", "I\u00b7dyl\u00b7le", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}}, "stanza.3": {"line.1": {"text": "Ob du auch sittsam deine frommen", "tokens": ["Ob", "du", "auch", "sitt\u00b7sam", "dei\u00b7ne", "from\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "PPOSAT", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Blauaugen niederschl\u00e4gst, wenn jach,", "tokens": ["Blau\u00b7au\u00b7gen", "nie\u00b7der\u00b7schl\u00e4gst", ",", "wenn", "jach", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "KOUS", "ADJD", "$,"], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie's just passiert, ein Wort gekommen \u2013", "tokens": ["Wie's", "just", "pas\u00b7siert", ",", "ein", "Wort", "ge\u00b7kom\u00b7men", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$,", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Wort von bravem, derbem Schlag \u2013", "tokens": ["Ein", "Wort", "von", "bra\u00b7vem", ",", "der\u00b7bem", "Schlag", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "$,", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es f\u00e4hrt heraus \u2013 die andern kichern:", "tokens": ["Es", "f\u00e4hrt", "he\u00b7raus", "\u2013", "die", "an\u00b7dern", "ki\u00b7chern", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$(", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "\u00bbein Witz, der nicht zum feinsten roch!\u00ab", "tokens": ["\u00bb", "ein", "Witz", ",", "der", "nicht", "zum", "feins\u00b7ten", "roch", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "$,", "PRELS", "PTKNEG", "APPRART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ob du auch kalt sie's l\u00e4\u00dft versichern \u2013", "tokens": ["Ob", "du", "auch", "kalt", "sie's", "l\u00e4\u00dft", "ver\u00b7si\u00b7chern", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "PIS", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Denn ich, Madonna, mu\u00df es wissen \u2013", "tokens": ["Denn", "ich", ",", "Ma\u00b7don\u00b7na", ",", "mu\u00df", "es", "wis\u00b7sen", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "NE", "$,", "VMFIN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du hast es selbst mir unges\u00e4umt", "tokens": ["Du", "hast", "es", "selbst", "mir", "un\u00b7ge\u00b7s\u00e4umt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gebeichtet, da auf weichen Kissen", "tokens": ["Ge\u00b7beich\u00b7tet", ",", "da", "auf", "wei\u00b7chen", "Kis\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "KOUS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich manche Nacht bei dir vertr\u00e4umt ...", "tokens": ["Ich", "man\u00b7che", "Nacht", "bei", "dir", "ver\u00b7tr\u00e4umt", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dein sch\u00f6ner Leib ist so gesellig", "tokens": ["Dein", "sch\u00f6\u00b7ner", "Leib", "ist", "so", "ge\u00b7sel\u00b7lig"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ADV", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und Kosen d\u00fcnkt ihn wunderfein \u2013", "tokens": ["Und", "Ko\u00b7sen", "d\u00fcnkt", "ihn", "wun\u00b7der\u00b7fein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Drum bist du heimlich gern gef\u00e4llig:", "tokens": ["Drum", "bist", "du", "heim\u00b7lich", "gern", "ge\u00b7f\u00e4l\u00b7lig", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PPER", "ADJD", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}