{"textgrid.poem.24452": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Uber das \u00f6ffentliche und stete B\u00fccher-beten des Marcus in der Kirchen", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du betest laut und viel/ kanst auch die Fasten leiden/", "tokens": ["Du", "be\u00b7test", "laut", "und", "viel", "/", "kanst", "auch", "die", "Fas\u00b7ten", "lei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "ADV", "$(", "VMFIN", "ADV", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Papistisch lebst du so/ und plapperst wie die Heyden.", "tokens": ["Pa\u00b7pis\u00b7tisch", "lebst", "du", "so", "/", "und", "plap\u00b7perst", "wie", "die", "Hey\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "$(", "KON", "VVFIN", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wer aber/ denckest du/ wer kan das innre sehn.", "tokens": ["Wer", "a\u00b7ber", "/", "den\u00b7ckest", "du", "/", "wer", "kan", "das", "inn\u00b7re", "sehn", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "$(", "VVFIN", "PPER", "$(", "PWS", "VMFIN", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ja bete nur/ man h\u00f6rt schon Pharis\u00e4er gehn.", "tokens": ["Ja", "be\u00b7te", "nur", "/", "man", "h\u00f6rt", "schon", "Pha\u00b7ri\u00b7s\u00e4\u00b7er", "gehn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "VVFIN", "ADV", "$(", "PIS", "VVFIN", "ADV", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und deines Betens Frucht kan man dir leicht gewehren:", "tokens": ["Und", "dei\u00b7nes", "Be\u00b7tens", "Frucht", "kan", "man", "dir", "leicht", "ge\u00b7weh\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "VMFIN", "PIS", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wenn dich Gott nicht erh\u00f6rt/ da\u00df dich die Menschen h\u00f6ren.", "tokens": ["Wenn", "dich", "Gott", "nicht", "er\u00b7h\u00f6rt", "/", "da\u00df", "dich", "die", "Men\u00b7schen", "h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "PTKNEG", "VVPP", "$(", "KOUS", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}