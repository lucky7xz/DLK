{"textgrid.poem.59711": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein V\u00f6glein flog wohl hier zum Rhein", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein V\u00f6glein flog wohl hier zum Rhein", "tokens": ["Ein", "V\u00f6\u00b7glein", "flog", "wohl", "hier", "zum", "Rhein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "APPRART", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit goldnen Fl\u00fcgeln und goldnem Schnabel,", "tokens": ["Mit", "gold\u00b7nen", "Fl\u00fc\u00b7geln", "und", "gold\u00b7nem", "Schna\u00b7bel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Sang Gr\u00fc\u00dfe mir ins Haus hinein,", "tokens": ["Sang", "Gr\u00fc\u00b7\u00dfe", "mir", "ins", "Haus", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PPER", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als w\u00e4r's Frau M\u00e4r oder Jungfer Fabel.", "tokens": ["Als", "w\u00e4r's", "Frau", "M\u00e4r", "o\u00b7der", "Jung\u00b7fer", "Fa\u00b7bel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "NN", "NN", "KON", "NE", "NE", "$."], "meter": "---+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "So schien es, aber Geb\u00e4r' und Gesicht", "tokens": ["So", "schien", "es", ",", "a\u00b7ber", "Ge\u00b7b\u00e4r'", "und", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KON", "NN", "KON", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Sprachen anders, sie sprachen: Die ist es nicht.", "tokens": ["Spra\u00b7chen", "an\u00b7ders", ",", "sie", "spra\u00b7chen", ":", "Die", "ist", "es", "nicht", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "PPER", "VVFIN", "$.", "PDS", "VAFIN", "PPER", "PTKNEG", "$."], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Jungfer Fabel tr\u00e4gt leichteren Flug und Sinn,", "tokens": ["Jung\u00b7fer", "Fa\u00b7bel", "tr\u00e4gt", "leich\u00b7te\u00b7ren", "Flug", "und", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Leichter als Aprilensonnenstrahlen", "tokens": ["Leich\u00b7ter", "als", "Ap\u00b7ri\u00b7len\u00b7son\u00b7nen\u00b7strah\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["NN", "KOUS", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Fliegt's ihr auf dem Angesicht her und hin,", "tokens": ["Fliegt's", "ihr", "auf", "dem", "An\u00b7ge\u00b7sicht", "her", "und", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPR", "ART", "NN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Kein Maler k\u00f6nnte die Wechsel malen:", "tokens": ["Kein", "Ma\u00b7ler", "k\u00f6nn\u00b7te", "die", "Wech\u00b7sel", "ma\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "So spielt sie mit Scheinen, selbst eitel Schein \u2013", "tokens": ["So", "spielt", "sie", "mit", "Schei\u00b7nen", ",", "selbst", "ei\u00b7tel", "Schein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "$,", "ADV", "ADJD", "NN", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Diese Leichte, Lustige kann's nicht sein.", "tokens": ["Die\u00b7se", "Leich\u00b7te", ",", "Lus\u00b7ti\u00b7ge", "kann's", "nicht", "sein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "NN", "VMFIN", "PTKNEG", "VAINF", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Wie nenn' ich mir denn das gefl\u00fcgelte Kind?", "tokens": ["Wie", "nenn'", "ich", "mir", "denn", "das", "ge\u00b7fl\u00fc\u00b7gel\u00b7te", "Kind", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Wie deut' ich die freundliche, s\u00fc\u00dfe Stimme,", "tokens": ["Wie", "deut'", "ich", "die", "freund\u00b7li\u00b7che", ",", "s\u00fc\u00b7\u00dfe", "Stim\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Die s\u00e4uselt wie sanft durch den Bl\u00fctenwind", "tokens": ["Die", "s\u00e4u\u00b7selt", "wie", "sanft", "durch", "den", "Bl\u00fc\u00b7ten\u00b7wind"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "KOKOM", "ADJD", "APPR", "ART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Das Lenzgesumse der Honigimme?", "tokens": ["Das", "Lenz\u00b7ge\u00b7sum\u00b7se", "der", "Ho\u00b7ni\u00b7gim\u00b7me", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Ges\u00e4usel, das tief in den Busen dringt", "tokens": ["Ge\u00b7s\u00e4u\u00b7sel", ",", "das", "tief", "in", "den", "Bu\u00b7sen", "dringt"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "ADJD", "APPR", "ART", "NN", "VVFIN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Und l\u00e4ngstverklungene T\u00f6ne klingt?", "tokens": ["Und", "l\u00e4ngst\u00b7ver\u00b7klun\u00b7ge\u00b7ne", "T\u00f6\u00b7ne", "klingt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Du bist es, du bist es, die ewig jung", "tokens": ["Du", "bist", "es", ",", "du", "bist", "es", ",", "die", "e\u00b7wig", "jung"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "$,", "PPER", "VAFIN", "PPER", "$,", "PRELS", "ADJD", "ADJD"], "meter": "-+-++--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wie Fr\u00fchling gr\u00fcnet bei grauen Locken,", "tokens": ["Wie", "Fr\u00fch\u00b7ling", "gr\u00fc\u00b7net", "bei", "grau\u00b7en", "Lo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Du Seligste bist es, Erinnerung:", "tokens": ["Du", "Se\u00b7ligs\u00b7te", "bist", "es", ",", "E\u00b7rin\u00b7ne\u00b7rung", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "NN", "VAFIN", "PPER", "$,", "NN", "$."], "meter": "--+-+--+--", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Du wehst der Vergangenheit Bl\u00fctenflocken", "tokens": ["Du", "wehst", "der", "Ver\u00b7gan\u00b7gen\u00b7heit", "Bl\u00fc\u00b7ten\u00b7flo\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Mit stillem Sehnen aufs alte Herz,", "tokens": ["Mit", "stil\u00b7lem", "Seh\u00b7nen", "aufs", "al\u00b7te", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Die alte Freude, den alten Schmerz.", "tokens": ["Die", "al\u00b7te", "Freu\u00b7de", ",", "den", "al\u00b7ten", "Schmerz", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Du bist es, die echte, die rechte M\u00e4r,", "tokens": ["Du", "bist", "es", ",", "die", "ech\u00b7te", ",", "die", "rech\u00b7te", "M\u00e4r", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$,", "ART", "ADJA", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Nicht jene, die leichthin t\u00e4ndelt und flattert,", "tokens": ["Nicht", "je\u00b7ne", ",", "die", "leich\u00b7thin", "t\u00e4n\u00b7delt", "und", "flat\u00b7tert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PDS", "$,", "PRELS", "ADV", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die, was in der Welt ist herrlich und hehr,", "tokens": ["Die", ",", "was", "in", "der", "Welt", "ist", "herr\u00b7lich", "und", "hehr", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PRELS", "APPR", "ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Zum Spott und Gel\u00e4chter hinunterschnattert \u2013", "tokens": ["Zum", "Spott", "und", "Ge\u00b7l\u00e4ch\u00b7ter", "hin\u00b7un\u00b7ter\u00b7schnat\u00b7tert", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "NN", "VVPP", "$("], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Du bist es, graulockig, doch ewig jung,", "tokens": ["Du", "bist", "es", ",", "grau\u00b7lo\u00b7ckig", ",", "doch", "e\u00b7wig", "jung", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$,", "ADJD", "$,", "ADV", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Du bist es, holde Erinnerung.", "tokens": ["Du", "bist", "es", ",", "hol\u00b7de", "E\u00b7rin\u00b7ne\u00b7rung", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$,", "ADJA", "NN", "$."], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}}}}}