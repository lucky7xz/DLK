{"textgrid.poem.41000": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Was sie alle meinen", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nasen kluger Philosophen!", "tokens": ["Na\u00b7sen", "klu\u00b7ger", "Phi\u00b7lo\u00b7so\u00b7phen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "O wie fein ihr ausgewittert,", "tokens": ["O", "wie", "fein", "ihr", "aus\u00b7ge\u00b7wit\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADJD", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df der H\u00f6lle Feuerofen", "tokens": ["Da\u00df", "der", "H\u00f6l\u00b7le", "Feu\u00b7er\u00b7o\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die Geister mich zersplittert;", "tokens": ["Und", "die", "Geis\u00b7ter", "mich", "zer\u00b7split\u00b7tert", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Da\u00df ich irre schmerzzerrissen", "tokens": ["Da\u00df", "ich", "ir\u00b7re", "schmerz\u00b7zer\u00b7ris\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch die Flur, ein armer Greiner,", "tokens": ["Durch", "die", "Flur", ",", "ein", "ar\u00b7mer", "Grei\u00b7ner", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie von einer Katz' gebissen,", "tokens": ["Wie", "von", "ei\u00b7ner", "Katz'", "ge\u00b7bis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die man magisch trieb aus einer;", "tokens": ["Die", "man", "ma\u00b7gisch", "trieb", "aus", "ei\u00b7ner", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADJD", "VVFIN", "APPR", "ART", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Da\u00df ich sehne mich vergebens", "tokens": ["Da\u00df", "ich", "seh\u00b7ne", "mich", "ver\u00b7ge\u00b7bens"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "PPER", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In den Jubel sonn'ger Tage", "tokens": ["In", "den", "Ju\u00b7bel", "sonn'\u00b7ger", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aus der Nacht des Geisterlebens \u2013", "tokens": ["Aus", "der", "Nacht", "des", "Geis\u00b7ter\u00b7le\u00b7bens", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Daher meines Liedes Klage. \u2013", "tokens": ["Da\u00b7her", "mei\u00b7nes", "Lie\u00b7des", "Kla\u00b7ge", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PAV", "PPOSAT", "NN", "NN", "$.", "$("], "meter": "----+-+-", "measure": "unknown.measure.di"}}, "stanza.4": {"line.1": {"text": "Feine philosoph'sche Nasen!", "tokens": ["Fei\u00b7ne", "phi\u00b7lo\u00b7so\u00b7ph'\u00b7sche", "Na\u00b7sen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Schmerz ist Grundton meines Herzens,", "tokens": ["Schmerz", "ist", "Grund\u00b7ton", "mei\u00b7nes", "Her\u00b7zens", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Von Natur ihm eingeblasen,", "tokens": ["Von", "Na\u00b7tur", "ihm", "ein\u00b7ge\u00b7bla\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schmerz der Grund selbst seines Scherzens.", "tokens": ["Schmerz", "der", "Grund", "selbst", "sei\u00b7nes", "Scher\u00b7zens", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Jener Schmerzenslieder viele", "tokens": ["Je\u00b7ner", "Schmer\u00b7zens\u00b7lie\u00b7der", "vie\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["PDAT", "NN", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hat der Knabe schon gesungen,", "tokens": ["Hat", "der", "Kna\u00b7be", "schon", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die ihr in der Geisterschw\u00fcle", "tokens": ["Die", "ihr", "in", "der", "Geis\u00b7ter\u00b7schw\u00fc\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mannesherzen meint entsprungen.", "tokens": ["Man\u00b7nes\u00b7her\u00b7zen", "meint", "ent\u00b7sprun\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Was ich schau' im Geisterreiche,", "tokens": ["Was", "ich", "schau'", "im", "Geis\u00b7ter\u00b7rei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kann mich nicht zur Klage stimmen,", "tokens": ["Kann", "mich", "nicht", "zur", "Kla\u00b7ge", "stim\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Das Gespenst, das ernste, bleiche,", "tokens": ["Das", "Ge\u00b7spenst", ",", "das", "erns\u00b7te", ",", "blei\u00b7che", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Macht nur dem, der's nicht glaubt, Grimmen.", "tokens": ["Macht", "nur", "dem", ",", "der's", "nicht", "glaubt", ",", "Grim\u00b7men", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "ADV", "ART", "$,", "PRELS", "PTKNEG", "VVFIN", "$,", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Schmerzlicher als irre Schatten", "tokens": ["Schmerz\u00b7li\u00b7cher", "als", "ir\u00b7re", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "KOKOM", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sind mir irre Menschenbengel,", "tokens": ["Sind", "mir", "ir\u00b7re", "Men\u00b7schen\u00b7ben\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die, weil ", "tokens": ["Die", ",", "weil"], "token_info": ["word", "punct", "word"], "pos": ["ART", "$,", "KOUS"], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Glauben dort sich flugs als Engel.", "tokens": ["Glau\u00b7ben", "dort", "sich", "flugs", "als", "En\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PRF", "ADV", "KOUS", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Liegt mein K\u00f6rper eine Leiche,", "tokens": ["Liegt", "mein", "K\u00f6r\u00b7per", "ei\u00b7ne", "Lei\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist mein Geist noch nicht am Ziele:", "tokens": ["Ist", "mein", "Geist", "noch", "nicht", "am", "Zie\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "PTKNEG", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn in meines Vaters Reiche", "tokens": ["Denn", "in", "mei\u00b7nes", "Va\u00b7ters", "Rei\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sind der Wohnungen gar viele.", "tokens": ["Sind", "der", "Woh\u00b7nun\u00b7gen", "gar", "vie\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "PIS", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.9": {"line.1": {"text": "Einst aus Vaters Hand will nehmen", "tokens": ["Einst", "aus", "Va\u00b7ters", "Hand", "will", "neh\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "NN", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ich mein Los, dem\u00fctig, stille.", "tokens": ["Ich", "mein", "Los", ",", "de\u00b7m\u00fc\u00b7tig", ",", "stil\u00b7le", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "$,", "ADJD", "$,", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schweb' ich auch mit irren Schemen \u2013", "tokens": ["Schweb'", "ich", "auch", "mit", "ir\u00b7ren", "Sche\u00b7men", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Vater! es gescheh' dein Wille!", "tokens": ["Va\u00b7ter", "!", "es", "ge\u00b7scheh'", "dein", "Wil\u00b7le", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Gottes Liebe tief im Busen,", "tokens": ["Got\u00b7tes", "Lie\u00b7be", "tief", "im", "Bu\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lieb' ich, die er schuf, die Erde,", "tokens": ["Lieb'", "ich", ",", "die", "er", "schuf", ",", "die", "Er\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PRELS", "PPER", "VVFIN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Lieb' ich Liebe, Wein und Musen,", "tokens": ["Lieb'", "ich", "Lie\u00b7be", ",", "Wein", "und", "Mu\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bis ich Geist bei Geistern werde.", "tokens": ["Bis", "ich", "Geist", "bei", "Geis\u00b7tern", "wer\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "APPR", "NN", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}