{"dta.poem.8738": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "XvIl.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Bet\u00e4ubt und stumm,", "tokens": ["Be\u00b7t\u00e4ubt", "und", "stumm", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Du fragst, o frage", "tokens": ["Du", "fragst", ",", "o", "fra\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "FM", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Mich nicht, warum?", "tokens": ["Mich", "nicht", ",", "wa\u00b7rum", "?"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPER", "PTKNEG", "$,", "PWAV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Das Herz ersch\u00fcttert", "tokens": ["Das", "Herz", "er\u00b7sch\u00fct\u00b7tert"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVPP"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "So manche Pein,", "tokens": ["So", "man\u00b7che", "Pein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Und k\u00f6nnt' ich je", "tokens": ["Und", "k\u00f6nnt'", "ich", "je"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Zu d\u00fcster seyn?", "tokens": ["Zu", "d\u00fcs\u00b7ter", "seyn", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VAINF", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Der Baum verdorrt,", "tokens": ["Der", "Baum", "ver\u00b7dorrt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Der Duft vergeht,", "tokens": ["Der", "Duft", "ver\u00b7geht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Die Bl\u00e4tter liegen", "tokens": ["Die", "Bl\u00e4t\u00b7ter", "lie\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "So gelb im Beet,", "tokens": ["So", "gelb", "im", "Beet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Es st\u00fcrmt ein Schauer", "tokens": ["Es", "st\u00fcrmt", "ein", "Schau\u00b7er"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Mit Macht herein,", "tokens": ["Mit", "Macht", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Und k\u00f6nnt' ich je", "tokens": ["Und", "k\u00f6nnt'", "ich", "je"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Zu d\u00fcster seyn?", "tokens": ["Zu", "d\u00fcs\u00b7ter", "seyn", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VAINF", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}