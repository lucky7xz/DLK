{"textgrid.poem.67798": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Die S\u00fcnde", "genre": "verse", "period": "N.A.", "pub_year": 1772, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer war ich, als mich Deine Hand", "tokens": ["Wer", "war", "ich", ",", "als", "mich", "Dei\u00b7ne", "Hand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "PPER", "$,", "KOUS", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zum Menschen kam zu bilden!", "tokens": ["Zum", "Men\u00b7schen", "kam", "zu", "bil\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein Erdenklo\u00df, mein Vaterland", "tokens": ["Ein", "Er\u00b7den\u00b7klo\u00df", ",", "mein", "Va\u00b7ter\u00b7land"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Staub auf Staubgefilden!", "tokens": ["Ein", "Staub", "auf", "Staub\u00b7ge\u00b7fil\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Da kam Dein Finger, Herr, und fuhr", "tokens": ["Da", "kam", "Dein", "Fin\u00b7ger", ",", "Herr", ",", "und", "fuhr"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,", "NN", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Um meine Glieder; seine Spur", "tokens": ["Um", "mei\u00b7ne", "Glie\u00b7der", ";", "sei\u00b7ne", "Spur"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KOUI", "PPOSAT", "NN", "$.", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Schuf in mich Bildsamkeiten.", "tokens": ["Schuf", "in", "mich", "Bild\u00b7sam\u00b7kei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Da kam Dein Odem, und der Thon", "tokens": ["Da", "kam", "Dein", "O\u00b7dem", ",", "und", "der", "Thon"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,", "KON", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Ward durch Dein Leben, Gottes Sohn,", "tokens": ["Ward", "durch", "Dein", "Le\u00b7ben", ",", "Got\u00b7tes", "Sohn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPOSAT", "NN", "$,", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Voll Lebensregsamkeiten.", "tokens": ["Voll", "Le\u00b7bens\u00b7re\u00b7gsam\u00b7kei\u00b7ten", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Was spiegelt uns die S\u00fcnde vor?", "tokens": ["Was", "spie\u00b7gelt", "uns", "die", "S\u00fcn\u00b7de", "vor", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Macht unsern Ruhm zu Leide!", "tokens": ["Macht", "un\u00b7sern", "Ruhm", "zu", "Lei\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In Lust wird unser Aug' ein Thor,", "tokens": ["In", "Lust", "wird", "un\u00b7ser", "Aug'", "ein", "Thor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Spricht uns von Vaters Neide,", "tokens": ["Spricht", "uns", "von", "Va\u00b7ters", "Nei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Von Neide De\u00df, der Alles giebt,", "tokens": ["Von", "Nei\u00b7de", "De\u00df", ",", "der", "Al\u00b7les", "giebt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wird fremde Dem, der Alle liebt,", "tokens": ["Wird", "frem\u00b7de", "Dem", ",", "der", "Al\u00b7le", "liebt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Traut ihm nicht, und \u2013 der Schlange!", "tokens": ["Traut", "ihm", "nicht", ",", "und", "\u2013", "der", "Schlan\u00b7ge", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,", "KON", "$(", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.8": {"text": "\u00bbsie i\u00dft, die Schlange? \u2013 sie allein.", "tokens": ["\u00bb", "sie", "i\u00dft", ",", "die", "Schlan\u00b7ge", "?", "\u2013", "sie", "al\u00b7lein", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "$,", "ART", "NN", "$.", "$(", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "So werd' ich Gott und G\u00f6ttin sein,", "tokens": ["So", "werd'", "ich", "Gott", "und", "G\u00f6t\u00b7tin", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "KON", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Wenn ich, wie sie, dort prange.\u00ab", "tokens": ["Wenn", "ich", ",", "wie", "sie", ",", "dort", "pran\u00b7ge", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "$,", "PWAV", "PPER", "$,", "ADV", "VVFIN", "$.", "$("], "meter": "-++-++-", "measure": "unknown.measure.tetra"}}, "stanza.3": {"line.1": {"text": "Gewollt, ach schnell gethan, bereut,", "tokens": ["Ge\u00b7wollt", ",", "ach", "schnell", "ge\u00b7than", ",", "be\u00b7reut", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVPP", "$,", "ADV", "ADJD", "VVPP", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bereut mit Furcht und Zittern!", "tokens": ["Be\u00b7reut", "mit", "Furcht", "und", "Zit\u00b7tern", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da kommt der Gott der Freundlichkeit;", "tokens": ["Da", "kommt", "der", "Gott", "der", "Freund\u00b7lich\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kommt er in Ungewittern?", "tokens": ["Kommt", "er", "in", "Un\u00b7ge\u00b7wit\u00b7tern", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Im Abends\u00e4useln kommt der Herr,", "tokens": ["Im", "A\u00b7bend\u00b7s\u00e4u\u00b7seln", "kommt", "der", "Herr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ruft linde seine Schuldener,", "tokens": ["Ruft", "lin\u00b7de", "sei\u00b7ne", "Schul\u00b7de\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.7": {"text": "Sie gn\u00e4dig zu verschonen;", "tokens": ["Sie", "gn\u00e4\u00b7dig", "zu", "ver\u00b7scho\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Er fr\u00e4gt, er h\u00f6rt als Vater sie,", "tokens": ["Er", "fr\u00e4gt", ",", "er", "h\u00f6rt", "als", "Va\u00b7ter", "sie", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "KOKOM", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "F\u00fcr S\u00fcnde will er sie mit M\u00fch", "tokens": ["F\u00fcr", "S\u00fcn\u00b7de", "will", "er", "sie", "mit", "M\u00fch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VMFIN", "PPER", "PPER", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und neuer Wohlfahrt lohnen.", "tokens": ["Und", "neu\u00b7er", "Wohl\u00b7fahrt", "loh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Barmherzig doch, auch wenn er straft,", "tokens": ["Barm\u00b7her\u00b7zig", "doch", ",", "auch", "wenn", "er", "straft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "ADV", "KOUS", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auch noch sein Fluch ist Segen!", "tokens": ["Auch", "noch", "sein", "Fluch", "ist", "Se\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "VAFIN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein harter Segen, aber noch", "tokens": ["Ein", "har\u00b7ter", "Se\u00b7gen", ",", "a\u00b7ber", "noch"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zum Wohl auf unsern Wegen.", "tokens": ["Zum", "Wohl", "auf", "un\u00b7sern", "We\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dem Menschenvater wird sein Brod", "tokens": ["Dem", "Men\u00b7schen\u00b7va\u00b7ter", "wird", "sein", "Brod"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In Schwei\u00df und Kummer, M\u00fch und Noth,", "tokens": ["In", "Schwei\u00df", "und", "Kum\u00b7mer", ",", "M\u00fch", "und", "Noth", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da\u00df es ihm schmackhaft werde.", "tokens": ["Da\u00df", "es", "ihm", "schmack\u00b7haft", "wer\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Tod wird sein Urtheil; aber Tod,", "tokens": ["Tod", "wird", "sein", "Ur\u00b7theil", ";", "a\u00b7ber", "Tod", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "$.", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Der Retter nur von M\u00fch und Noth;", "tokens": ["Der", "Ret\u00b7ter", "nur", "von", "M\u00fch", "und", "Noth", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "NE", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Wird Erde, sanft zur Erde.", "tokens": ["Wird", "Er\u00b7de", ",", "sanft", "zur", "Er\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Auch wenn er straft, barmherzig doch,", "tokens": ["Auch", "wenn", "er", "straft", ",", "barm\u00b7her\u00b7zig", "doch", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ADJD", "$,", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sein Fluch ist schwerer Segen!", "tokens": ["Sein", "Fluch", "ist", "schwe\u00b7rer", "Se\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein harter Segen, aber noch", "tokens": ["Ein", "har\u00b7ter", "Se\u00b7gen", ",", "a\u00b7ber", "noch"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zum Wohl auf unsern Wegen.", "tokens": ["Zum", "Wohl", "auf", "un\u00b7sern", "We\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der Menschenmutter wird der Schmerz", "tokens": ["Der", "Men\u00b7schen\u00b7mut\u00b7ter", "wird", "der", "Schmerz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zur Freude, da\u00df ihr Mutterherz", "tokens": ["Zur", "Freu\u00b7de", ",", "da\u00df", "ihr", "Mut\u00b7ter\u00b7herz"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mit Sieg ihr Kindlein herze;", "tokens": ["Mit", "Sieg", "ihr", "Kin\u00b7dlein", "her\u00b7ze", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Dem Manne wird sie unterthan,", "tokens": ["Dem", "Man\u00b7ne", "wird", "sie", "un\u00b7ter\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Da\u00df treu und stark und fest am Mann", "tokens": ["Da\u00df", "treu", "und", "stark", "und", "fest", "am", "Mann"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "KON", "ADJD", "KON", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Sie Lebensleid verschmerze.", "tokens": ["Sie", "Le\u00b7bens\u00b7leid", "ver\u00b7schmer\u00b7ze", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Auch wenn er straft, barmherzig noch,", "tokens": ["Auch", "wenn", "er", "straft", ",", "barm\u00b7her\u00b7zig", "noch", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ADJD", "$,", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auch noch sein Fluch ist Segen!", "tokens": ["Auch", "noch", "sein", "Fluch", "ist", "Se\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "VAFIN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein harter Segen, aber noch", "tokens": ["Ein", "har\u00b7ter", "Se\u00b7gen", ",", "a\u00b7ber", "noch"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Uns noth zu unsern Wegen.", "tokens": ["Uns", "noth", "zu", "un\u00b7sern", "We\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Da kleidet Gott sein nacktes Kind.", "tokens": ["Da", "klei\u00b7det", "Gott", "sein", "nack\u00b7tes", "Kind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ihr H\u00fcllen, die mein Elend sind,", "tokens": ["Ihr", "H\u00fcl\u00b7len", ",", "die", "mein", "E\u00b7lend", "sind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Das Denkmal