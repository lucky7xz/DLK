ord"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit tausend Flammenschaaren,", "tokens": ["Mit", "tau\u00b7send", "Flam\u00b7men\u00b7schaa\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Er winkt mir, da\u00df ich aufw\u00e4rts seh',", "tokens": ["Er", "winkt", "mir", ",", "da\u00df", "ich", "auf\u00b7w\u00e4rts", "seh'", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zum Paradiese h\u00f6hrer H\u00f6h,", "tokens": ["Zum", "Pa\u00b7ra\u00b7die\u00b7se", "h\u00f6h\u00b7rer", "H\u00f6h", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Was nie sich kann verlieren;", "tokens": ["Was", "nie", "sich", "kann", "ver\u00b7lie\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PRF", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Da wird in neuen Jugendraum,", "tokens": ["Da", "wird", "in", "neu\u00b7en", "Ju\u00b7gen\u00b7draum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Da wird zum neuen Lebensbaum", "tokens": ["Da", "wird", "zum", "neu\u00b7en", "Le\u00b7bens\u00b7baum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Mein Bruder hin mich f\u00fchren!", "tokens": ["Mein", 