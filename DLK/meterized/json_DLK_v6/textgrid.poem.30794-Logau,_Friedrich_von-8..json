{"textgrid.poem.30794": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "8.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Granmund sagt von hohen Dingen, von viel thun und von viel wagen;", "tokens": ["Gran\u00b7mund", "sagt", "von", "ho\u00b7hen", "Din\u00b7gen", ",", "von", "viel", "thun", "und", "von", "viel", "wa\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN", "$,", "APPR", "PIS", "VVINF", "KON", "APPR", "PIS", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Wachs zum siegeln werden Bienen, die sonst Zippelpeltze tragen,", "tokens": ["Wachs", "zum", "sie\u00b7geln", "wer\u00b7den", "Bie\u00b7nen", ",", "die", "sonst", "Zip\u00b7pel\u00b7pelt\u00b7ze", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "VVINF", "VAFIN", "NN", "$,", "PRELS", "ADV", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Willig w\u00fcrcken, zu best\u00e4rcken, was uns Granmund pflegt zu sagen.", "tokens": ["Wil\u00b7lig", "w\u00fcr\u00b7cken", ",", "zu", "be\u00b7st\u00e4r\u00b7cken", ",", "was", "uns", "Gran\u00b7mund", "pflegt", "zu", "sa\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "$,", "PTKZU", "VVINF", "$,", "PRELS", "PPER", "NN", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}}}}