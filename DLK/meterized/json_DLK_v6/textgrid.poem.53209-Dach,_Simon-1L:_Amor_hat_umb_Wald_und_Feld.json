{"textgrid.poem.53209": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Amor hat umb Wald und Feld", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Amor hat umb Wald und Feld", "tokens": ["A\u00b7mor", "hat", "umb", "Wald", "und", "Feld"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Seine Jagt jetzt angestellt,", "tokens": ["Sei\u00b7ne", "Jagt", "jetzt", "an\u00b7ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Er durchfleuget Berg und Thal,", "tokens": ["Er", "durch\u00b7fleu\u00b7get", "Berg", "und", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Hetzt und jaget \u00fcberal.", "tokens": ["Hetzt", "und", "ja\u00b7get", "\u00fc\u00b7be\u00b7ral", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "M\u00e4chtig trifft sein Bogen ein,", "tokens": ["M\u00e4ch\u00b7tig", "trifft", "sein", "Bo\u00b7gen", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Was er nur beziehlt, ist sein,", "tokens": ["Was", "er", "nur", "be\u00b7ziehlt", ",", "ist", "sein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVFIN", "$,", "VAFIN", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Vnd sein K\u00f6cher wird nicht leer,", "tokens": ["Vnd", "sein", "K\u00f6\u00b7cher", "wird", "nicht", "leer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PTKNEG", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00e4llt er st\u00fcndlich gleich ein Heer.", "tokens": ["F\u00e4llt", "er", "st\u00fcnd\u00b7lich", "gleich", "ein", "Heer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Darumb liegen kranck und wund", "tokens": ["Da\u00b7rumb", "lie\u00b7gen", "kranck", "und", "wund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ADJD", "KON", "NN"], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.2": {"text": "Dieser Erden grosses Rund,", "tokens": ["Die\u00b7ser", "Er\u00b7den", "gros\u00b7ses", "Rund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "B\u00fcsche, Felder, Wald und Stein", "tokens": ["B\u00fc\u00b7sche", ",", "Fel\u00b7der", ",", "Wald", "und", "Stein"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Vnd die s\u00fcssen V\u00f6gelein,", "tokens": ["Vnd", "die", "s\u00fcs\u00b7sen", "V\u00f6\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Alles wild und zahme Thier,", "tokens": ["Al\u00b7les", "wild", "und", "zah\u00b7me", "Thier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADJD", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Denn kein Vnterscheid ist hier,", "tokens": ["Denn", "kein", "Vn\u00b7ter\u00b7scheid", "ist", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Auch der Mensch, die kleine Welt,", "tokens": ["Auch", "der", "Mensch", ",", "die", "klei\u00b7ne", "Welt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Liegt durch seine Macht gef\u00e4llt.", "tokens": ["Liegt", "durch", "sei\u00b7ne", "Macht", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "An der Liebe liegen matt", "tokens": ["An", "der", "Lie\u00b7be", "lie\u00b7gen", "matt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00e4uffig beydes Dorff und Stadt,", "tokens": ["H\u00e4uf\u00b7fig", "bey\u00b7des", "Dorff", "und", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seht umbher, wie manches Haus", "tokens": ["Seht", "um\u00b7bher", ",", "wie", "man\u00b7ches", "Haus"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PTKVZ", "$,", "PWAV", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Putzt sich heut mit Tannen aus.", "tokens": ["Putzt", "sich", "heut", "mit", "Tan\u00b7nen", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "H\u00f6ffe, G\u00e4rten und der Sal", "tokens": ["H\u00f6f\u00b7fe", ",", "G\u00e4r\u00b7ten", "und", "der", "Sal"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "KON", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Halten s\u00e4mptlich Hochzeit-mahl,", "tokens": ["Hal\u00b7ten", "s\u00e4mpt\u00b7lich", "Hoch\u00b7zeit\u00b7mahl", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da die s\u00fcsse Music singt", "tokens": ["Da", "die", "s\u00fcs\u00b7se", "Mu\u00b7sic", "singt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Vnd die Kunst der Seiten klingt.", "tokens": ["Vnd", "die", "Kunst", "der", "Sei\u00b7ten", "klingt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Da vergisset man der Noht,", "tokens": ["Da", "ver\u00b7gis\u00b7set", "man", "der", "Noht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Die uns drewet Brand und Tod,", "tokens": ["Die", "uns", "dre\u00b7wet", "Brand", "und", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Keiner Zeitung, die man schreibt,", "tokens": ["Kei\u00b7ner", "Zei\u00b7tung", ",", "die", "man", "schreibt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird beym s\u00fcssen Trunck gegl\u00e4ubt.", "tokens": ["Wird", "beym", "s\u00fcs\u00b7sen", "Trunck", "ge\u00b7gl\u00e4ubt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Was, Herr J\u00e4ger, macht denn ihr?", "tokens": ["Was", ",", "Herr", "J\u00e4\u00b7ger", ",", "macht", "denn", "ihr", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "$,", "NN", "NN", "$,", "VVFIN", "KON", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Jagt ihr euch kein liebes Thier?", "tokens": ["Jagt", "ihr", "euch", "kein", "lie\u00b7bes", "Thier", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil der Tod euch nachgestellt", "tokens": ["Weil", "der", "Tod", "euch", "nach\u00b7ge\u00b7stellt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PPER", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Vnd euch ewer Hertz gef\u00e4llt?", "tokens": ["Vnd", "euch", "e\u00b7wer", "Hertz", "ge\u00b7f\u00e4llt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Vmb der Tangen gr\u00fcnen Rand", "tokens": ["Vmb", "der", "Tan\u00b7gen", "gr\u00fc\u00b7nen", "Rand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Jagt' er ewrer Seelen Pfand,", "tokens": ["Jagt'", "er", "ew\u00b7rer", "See\u00b7len", "Pfand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Das er auch durch strenge Macht", "tokens": ["Das", "er", "auch", "durch", "stren\u00b7ge", "Macht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Endlich in sein Garn gebracht.", "tokens": ["End\u00b7lich", "in", "sein", "Garn", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Sie, die Sch\u00f6ne stirbt dahin,", "tokens": ["Sie", ",", "die", "Sch\u00f6\u00b7ne", "stirbt", "da\u00b7hin", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "NN", "VVFIN", "PAV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hochbetr\u00fcbt wird ewer Sinn,", "tokens": ["Hoch\u00b7be\u00b7tr\u00fcbt", "wird", "e\u00b7wer", "Sinn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ewer Bett' ist \u00f6d' und wild", "tokens": ["E\u00b7wer", "Bett'", "ist", "\u00f6d'", "und", "wild"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Vnd mit Jammer nur erf\u00fcllt.", "tokens": ["Vnd", "mit", "Jam\u00b7mer", "nur", "er\u00b7f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Vnterdessen dringt herein", "tokens": ["Vn\u00b7ter\u00b7des\u00b7sen", "dringt", "her\u00b7ein"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Der gew\u00fcnschte Vorjahrs-Schein,", "tokens": ["Der", "ge\u00b7w\u00fcnschte", "Vor\u00b7jahr\u00b7sSchein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr vernehmt, das Amor jagt,", "tokens": ["Ihr", "ver\u00b7nehmt", ",", "das", "A\u00b7mor", "jagt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "NE", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sitzt ihr einsam und verzagt?", "tokens": ["Sitzt", "ihr", "ein\u00b7sam", "und", "ver\u00b7zagt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "KON", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Nein, ihr setzet mit ihm an,", "tokens": ["Nein", ",", "ihr", "set\u00b7zet", "mit", "ihm", "an", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Seyd ein guter Weidemann,", "tokens": ["Seyd", "ein", "gu\u00b7ter", "Wei\u00b7de\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was die M\u00fcmmel sch\u00f6nes f\u00fchrt,", "tokens": ["Was", "die", "M\u00fcm\u00b7mel", "sch\u00f6\u00b7nes", "f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "ADJA", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Das wird von euch ausgesp\u00fcrt.", "tokens": ["Das", "wird", "von", "euch", "aus\u00b7ge\u00b7sp\u00fcrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "PPER", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.13": {"line.1": {"text": "Ewrer Augen heller Stral", "tokens": ["Ew\u00b7rer", "Au\u00b7gen", "hel\u00b7ler", "Stral"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Der durchdringet allzumal,", "tokens": ["Der", "durch\u00b7drin\u00b7get", "all\u00b7zu\u00b7mal", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch er\u00e4ugt sich da kein Wild,", "tokens": ["Doch", "er\u00b7\u00e4ugt", "sich", "da", "kein", "Wild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADV", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Welches ewren Vorsatz stillt.", "tokens": ["Wel\u00b7ches", "ew\u00b7ren", "Vor\u00b7satz", "stillt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "K\u00f6nigsberg die Edle Stad", "tokens": ["K\u00f6\u00b7nigs\u00b7berg", "die", "Ed\u00b7le", "Stad"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Giebt erst ewrem Kummer Raht,", "tokens": ["Giebt", "erst", "ew\u00b7rem", "Kum\u00b7mer", "Raht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Vnd die sch\u00f6ne Lunizinn", "tokens": ["Vnd", "die", "sch\u00f6\u00b7ne", "Lu\u00b7ni\u00b7zinn"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mus begn\u00fcgen ewren Sinn.", "tokens": ["Mus", "be\u00b7gn\u00fc\u00b7gen", "ew\u00b7ren", "Sinn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Zwar ihr habet sie erjagt,", "tokens": ["Zwar", "ihr", "ha\u00b7bet", "sie", "er\u00b7jagt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df sie kranck an Liebe klagt,", "tokens": ["Da\u00df", "sie", "kranck", "an", "Lie\u00b7be", "klagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber sie im gegentheil", "tokens": ["A\u00b7ber", "sie", "im", "ge\u00b7gen\u00b7theil"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Trifft euch auch durch ihren Pfeil.", "tokens": ["Trifft", "euch", "auch", "durch", "ih\u00b7ren", "Pfeil", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "Ihrer Augen Plitz und Schein", "tokens": ["Ih\u00b7rer", "Au\u00b7gen", "Plitz", "und", "Schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "NE", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dringt zu eurem Hertzen ein,", "tokens": ["Dringt", "zu", "eu\u00b7rem", "Hert\u00b7zen", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihre Wangen und ihr Mund", "tokens": ["Ih\u00b7re", "Wan\u00b7gen", "und", "ihr", "Mund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Machen euch nicht minder wund.", "tokens": ["Ma\u00b7chen", "euch", "nicht", "min\u00b7der", "wund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.17": {"line.1": {"text": "Also liegen beyde nu", "tokens": ["Al\u00b7so", "lie\u00b7gen", "bey\u00b7de", "nu"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wild und Weidemann dazu,", "tokens": ["Wild", "und", "Wei\u00b7de\u00b7mann", "da\u00b7zu", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "PAV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch der gutten Hoffnung voll,", "tokens": ["Doch", "der", "gut\u00b7ten", "Hoff\u00b7nung", "voll", ","],