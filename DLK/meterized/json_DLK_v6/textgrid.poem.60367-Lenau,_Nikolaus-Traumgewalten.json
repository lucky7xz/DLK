{"textgrid.poem.60367": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Traumgewalten", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Traum war so wild, der Traum war so schaurig", "tokens": ["Der", "Traum", "war", "so", "wild", ",", "der", "Traum", "war", "so", "schau\u00b7rig"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$,", "ART", "NN", "VAFIN", "ADV", "ADJD"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "So tief ersch\u00fctternd, unendlich traurig.", "tokens": ["So", "tief", "er\u00b7sch\u00fct\u00b7ternd", ",", "un\u00b7end\u00b7lich", "trau\u00b7rig", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$,", "ADJD", "ADJD", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ich m\u00f6chte gerne mir sagen:", "tokens": ["Ich", "m\u00f6ch\u00b7te", "ger\u00b7ne", "mir", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PPER", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Da\u00df ich a fest geschlafen hab,", "tokens": ["Da\u00df", "ich", "a", "fest", "ge\u00b7schla\u00b7fen", "hab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NE", "ADJD", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df ich ja nicht getr\u00e4umet hab,", "tokens": ["Da\u00df", "ich", "ja", "nicht", "ge\u00b7tr\u00e4u\u00b7met", "hab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKNEG", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Doch rinnen mir noch die Tr\u00e4nen herab,", "tokens": ["Doch", "rin\u00b7nen", "mir", "noch", "die", "Tr\u00e4\u00b7nen", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "NN", "ADV", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Ich h\u00f6re mein Herz noch schlagen.", "tokens": ["Ich", "h\u00f6\u00b7re", "mein", "Herz", "noch", "schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADV", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Ich bin erwacht in banger Ermattung,", "tokens": ["Ich", "bin", "er\u00b7wacht", "in", "ban\u00b7ger", "Er\u00b7mat\u00b7tung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ich finde mein Tuch durchn\u00e4\u00dft am Kissen,", "tokens": ["Ich", "fin\u00b7de", "mein", "Tuch", "durc\u00b7hn\u00e4\u00dft", "am", "Kis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wie mans heimbringt von einer Bestattung;", "tokens": ["Wie", "mans", "heim\u00b7bringt", "von", "ei\u00b7ner", "Be\u00b7stat\u00b7tung", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Hab ichs im Traume hervorgerissen", "tokens": ["Hab", "ichs", "im", "Trau\u00b7me", "her\u00b7vor\u00b7ge\u00b7ris\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PIS", "APPRART", "NN", "VVINF"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und mir getrocknet das Gesicht?", "tokens": ["Und", "mir", "ge\u00b7trock\u00b7net", "das", "Ge\u00b7sicht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVPP", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich wei\u00df es nicht.", "tokens": ["Ich", "wei\u00df", "es", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Doch waren sie da, die schlimmen G\u00e4ste,", "tokens": ["Doch", "wa\u00b7ren", "sie", "da", ",", "die", "schlim\u00b7men", "G\u00e4s\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Sie waren da zum n\u00e4chtlichen Feste.", "tokens": ["Sie", "wa\u00b7ren", "da", "zum", "n\u00e4cht\u00b7li\u00b7chen", "Fes\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Ich schlief, mein Haus war preisgegeben,", "tokens": ["Ich", "schlief", ",", "mein", "Haus", "war", "preis\u00b7ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie f\u00fchrten darin ein w\u00fcstes Leben.", "tokens": ["Sie", "f\u00fchr\u00b7ten", "da\u00b7rin", "ein", "w\u00fcs\u00b7tes", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Nun sind sie fort, die wilden Naturen;", "tokens": ["Nun", "sind", "sie", "fort", ",", "die", "wil\u00b7den", "Na\u00b7tu\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKVZ", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "In diesen Tr\u00e4nen find ich die Spuren,", "tokens": ["In", "die\u00b7sen", "Tr\u00e4\u00b7nen", "find", "ich", "die", "Spu\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Wie sie mir alles zusammen ger\u00fcttet", "tokens": ["Wie", "sie", "mir", "al\u00b7les", "zu\u00b7sam\u00b7men", "ge\u00b7r\u00fct\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PPER", "PIS", "ADV", "VVPP"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Und \u00fcber den Tisch den Wein gesch\u00fcttet.", "tokens": ["Und", "\u00fc\u00b7ber", "den", "Tisch", "den", "Wein", "ge\u00b7sch\u00fct\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}}}}