{"textgrid.poem.64462": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "84. Auf einen irrenden Marquis", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Durch manches Land irrt' er um Unterhalt,", "tokens": ["Durch", "man\u00b7ches", "Land", "irrt'", "er", "um", "Un\u00b7ter\u00b7halt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das er daf\u00fcr barbarisch schalt;", "tokens": ["Das", "er", "da\u00b7f\u00fcr", "bar\u00b7ba\u00b7risch", "schalt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "PAV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im Himmel wird f\u00fcr ihn selbst was zu tadeln seyn,", "tokens": ["Im", "Him\u00b7mel", "wird", "f\u00fcr", "ihn", "selbst", "was", "zu", "ta\u00b7deln", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "APPR", "PPER", "ADV", "PIS", "PTKZU", "VVINF", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Kommt er nur da hinein.", "tokens": ["Kommt", "er", "nur", "da", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}