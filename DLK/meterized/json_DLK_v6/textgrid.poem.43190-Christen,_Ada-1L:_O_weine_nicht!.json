{"textgrid.poem.43190": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: O weine nicht!", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O weine nicht!", "tokens": ["O", "wei\u00b7ne", "nicht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKNEG", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Deine Aeuglein sind", "tokens": ["Dei\u00b7ne", "A\u00b7e\u00b7u\u00b7glein", "sind"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "So blau und licht,", "tokens": ["So", "blau", "und", "licht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Schlaf ein, mein Kind.", "tokens": ["Schlaf", "ein", ",", "mein", "Kind", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Dem V\u00f6glein im Wald", "tokens": ["Dem", "V\u00f6\u00b7glein", "im", "Wald"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Ist kalt, ach kalt.", "tokens": ["Ist", "kalt", ",", "ach", "kalt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "ADV", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Und f\u00fcr Dein reines", "tokens": ["Und", "f\u00fcr", "Dein", "rei\u00b7nes"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Blumengesicht,", "tokens": ["Blu\u00b7men\u00b7ge\u00b7sicht", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Du Kind, Du kleines,", "tokens": ["Du", "Kind", ",", "Du", "klei\u00b7nes", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "PPER", "ADJA", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "Taugt Regen nicht.", "tokens": ["Taugt", "Re\u00b7gen", "nicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PTKNEG", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Du liegst so warm", "tokens": ["Du", "liegst", "so", "warm"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.12": {"text": "In meinem Arm, \u2013", "tokens": ["In", "mei\u00b7nem", "Arm", ",", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.13": {"text": "H\u00f6r' wie der Wind", "tokens": ["H\u00f6r'", "wie", "der", "Wind"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "KOKOM", "ART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.14": {"text": "Die Zweiglein bricht! \u2013", "tokens": ["Die", "Zwei\u00b7glein", "bricht", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.15": {"text": "Schlaf ein geschwind", "tokens": ["Schlaf", "ein", "ge\u00b7schwind"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.16": {"text": "Und weine nicht! ...", "tokens": ["Und", "wei\u00b7ne", "nicht", "!", "..."], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}}}}}