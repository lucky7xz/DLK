{"textgrid.poem.65605": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "78. Gesellige Liebensw\u00fcrdigkeit", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Willst du der Leute Liebling sein,", "tokens": ["Willst", "du", "der", "Leu\u00b7te", "Lieb\u00b7ling", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sei scharmant und sei gemein.", "tokens": ["Sei", "schar\u00b7mant", "und", "sei", "ge\u00b7mein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "KON", "VAFIN", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Was sie nicht k\u00f6nnen und was sie nicht fassen,", "tokens": ["Was", "sie", "nicht", "k\u00f6n\u00b7nen", "und", "was", "sie", "nicht", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PTKNEG", "VMFIN", "KON", "PWS", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie werden's verspotten oder hassen.", "tokens": ["Sie", "wer\u00b7den's", "ver\u00b7spot\u00b7ten", "o\u00b7der", "has\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "KON", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}}}}