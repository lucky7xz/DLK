{"dta.poem.11665": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Kalter Streich.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Ich will mich selber just nicht r\u00fchmen;", "tokens": ["Ich", "will", "mich", "sel\u00b7ber", "just", "nicht", "r\u00fch\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Doch darf ich sagen: Es ist so im Geist", "tokens": ["Doch", "darf", "ich", "sa\u00b7gen", ":", "Es", "ist", "so", "im", "Geist"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "VVINF", "$.", "PPER", "VAFIN", "ADV", "APPRART", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Von \u201eStunden der Andacht.\u201c", "tokens": ["Von", "\u201e", "Stun\u00b7den", "der", "An\u00b7dacht", ".", "\u201c"], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "$(", "NN", "ART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}