{"dta.poem.19050": {"metadata": {"author": {"name": "Zinzendorf, Nicolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "XxXI.  Auf seiner Gemahlin 24. Geburts-  \n Tag.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20688-6", "language": ["de:0.85", "af:0.14"], "booktitle": "Zinzendorf, Nicolaus Ludwig von: Teutscher Gedichte Erster Theil. Herrnhuth, 1735."}, "poem": {"stanza.1": {"line.1": {"text": "Du ewigs Liebes-Wesen du,", "tokens": ["Du", "e\u00b7wigs", "Lie\u00b7bes\u00b7We\u00b7sen", "du", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sey ewiglich gepreist,", "tokens": ["Sey", "e\u00b7wig\u00b7lich", "ge\u00b7preist", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df du aus deiner tiefen Ruh", "tokens": ["Da\u00df", "du", "aus", "dei\u00b7ner", "tie\u00b7fen", "Ruh"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Uns Tag vor Tag erfreust.", "tokens": ["Uns", "Tag", "vor", "Tag", "er\u00b7freust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wo fang ich deinen Ruhm nur an,", "tokens": ["Wo", "fang", "ich", "dei\u00b7nen", "Ruhm", "nur", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PPOSAT", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo h\u00f6rt mein Loben auf?", "tokens": ["Wo", "h\u00f6rt", "mein", "Lo\u00b7ben", "auf", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du unsrer Seele lieber Mann!", "tokens": ["Du", "uns\u00b7rer", "See\u00b7le", "lie\u00b7ber", "Mann", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dein Trieb ist schnell im Lauf.", "tokens": ["Dein", "Trieb", "ist", "schnell", "im", "Lauf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Hier hast du unser beyder Geist,", "tokens": ["Hier", "hast", "du", "un\u00b7ser", "bey\u00b7der", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Formir dir etwas draus,", "tokens": ["For\u00b7mir", "dir", "et\u00b7was", "draus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PIS", "PAV", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Das dich nach allen W\u00fcrden preist:", "tokens": ["Das", "dich", "nach", "al\u00b7len", "W\u00fcr\u00b7den", "preist", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er sey dein Tempel-Hau\u00df.", "tokens": ["Er", "sey", "dein", "Tem\u00b7pel\u00b7Hau\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und siehst du ein und anders noch,", "tokens": ["Und", "siehst", "du", "ein", "und", "an\u00b7ders", "noch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "KON", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das dir nicht wohlgef\u00e4llt,", "tokens": ["Das", "dir", "nicht", "wohl\u00b7ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du gute Liebe! t\u00f6dt\u2019 es doch,", "tokens": ["Du", "gu\u00b7te", "Lie\u00b7be", "!", "t\u00f6dt'", "es", "doch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$.", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als ein gewaltger Held.", "tokens": ["Als", "ein", "ge\u00b7walt\u00b7ger", "Held", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Hier hast du unser beyder Hand,", "tokens": ["Hier", "hast", "du", "un\u00b7ser", "bey\u00b7der", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die du zusammen schlugst,", "tokens": ["Die", "du", "zu\u00b7sam\u00b7men", "schlugst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als du an unserm Ehestand", "tokens": ["Als", "du", "an", "un\u00b7serm", "E\u00b7hes\u00b7tand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Wohlgefallen trugst.", "tokens": ["Ein", "Wohl\u00b7ge\u00b7fal\u00b7len", "trugst", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Sinckt unser Muth und Freudigkeit;", "tokens": ["Sinckt", "un\u00b7ser", "Muth", "und", "Freu\u00b7dig\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So fasse uns dabey,", "tokens": ["So", "fas\u00b7se", "uns", "da\u00b7bey", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PAV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df aller Sturm und tr\u00fcbe Zeit", "tokens": ["Da\u00df", "al\u00b7ler", "Sturm", "und", "tr\u00fc\u00b7be", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIAT", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Uns nicht gef\u00e4hrlich sey.", "tokens": ["Uns", "nicht", "ge\u00b7f\u00e4hr\u00b7lich", "sey", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "ADJD", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Hier hast du unser Aug und Ohr:", "tokens": ["Hier", "hast", "du", "un\u00b7ser", "Aug", "und", "Ohr", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Aug erleuchte dir;", "tokens": ["Das", "Aug", "er\u00b7leuch\u00b7te", "dir", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Ohren aber die durchbohr", "tokens": ["Die", "Oh\u00b7ren", "a\u00b7ber", "die", "durch\u00b7bohr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An deiner Gnaden-Th\u00fcr.", "tokens": ["An", "dei\u00b7ner", "Gna\u00b7den", "Th\u00fcr."], "token_info": ["word", "word", "word", "abbreviation"], "pos": ["APPR", "PPOSAT", "TRUNC", "NN"], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "Hier hast du unsern ", "tokens": ["Hier", "hast", "du", "un\u00b7sern"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Daselbst bereite du", "tokens": ["Da\u00b7selbst", "be\u00b7rei\u00b7te", "du"], "token_info": ["word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dir einen k\u00f6niglichen Sitz,", "tokens": ["Dir", "ei\u00b7nen", "k\u00f6\u00b7nig\u00b7li\u00b7chen", "Sitz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu ungest\u00f6rter Ruh.", "tokens": ["Zu", "un\u00b7ge\u00b7st\u00f6r\u00b7ter", "Ruh", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Hier hast du unsern ", "tokens": ["Hier", "hast", "du", "un\u00b7sern"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Sey ewiglich sein Ziel,", "tokens": ["Sey", "e\u00b7wig\u00b7lich", "sein", "Ziel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und rege dich im Hertzens-Grund", "tokens": ["Und", "re\u00b7ge", "dich", "im", "Hert\u00b7zens\u00b7Grund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit stetem Liebes-Spiel.", "tokens": ["Mit", "ste\u00b7tem", "Lie\u00b7bes\u00b7Spiel", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Hier hast du unsern Ehren-Stand,", "tokens": ["Hier", "hast", "du", "un\u00b7sern", "Eh\u00b7ren\u00b7Stand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Tritt mit den F\u00fcssen drauf:", "tokens": ["Tritt", "mit", "den", "F\u00fcs\u00b7sen", "drauf", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Dagegen hebe deine Hand", "tokens": ["Da\u00b7ge\u00b7gen", "he\u00b7be", "dei\u00b7ne", "Hand"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Uns an das Creutz hinauf!", "tokens": ["Uns", "an", "das", "Creutz", "hin\u00b7auf", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Hier hast du unsern zarten Sohn,", "tokens": ["Hier", "hast", "du", "un\u00b7sern", "zar\u00b7ten", "Sohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Pfand von unsrer Eh:", "tokens": ["Das", "Pfand", "von", "uns\u00b7rer", "Eh", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wir werfen ihn vor deinen Thron:", "tokens": ["Wir", "wer\u00b7fen", "ihn", "vor", "dei\u00b7nen", "Thron", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ach! segn\u2019 ihn aus der H\u00f6h.", "tokens": ["Ach", "!", "segn'", "ihn", "aus", "der", "H\u00f6h", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Dir, unsrer Liebe, wurde er", "tokens": ["Dir", ",", "uns\u00b7rer", "Lie\u00b7be", ",", "wur\u00b7de", "er"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "$,", "PPOSAT", "NN", "$,", "VAFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vom Anfang eingeweiht:", "tokens": ["Vom", "An\u00b7fang", "ein\u00b7ge\u00b7weiht", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er bleibt dein armer Wanderer", "tokens": ["Er", "bleibt", "dein", "ar\u00b7mer", "Wan\u00b7de\u00b7rer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.4": {"text": "Zur Stadt der Ewigkeit.", "tokens": ["Zur", "Stadt", "der", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Des Wegs sey wenig oder viel,", "tokens": ["Des", "Wegs", "sey", "we\u00b7nig", "o\u00b7der", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIS", "KON", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den er noch gehen mu\u00df;", "tokens": ["Den", "er", "noch", "ge\u00b7hen", "mu\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVINF", "VMFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "So ist doch sein, wie unser Ziel", "tokens": ["So", "ist", "doch", "sein", ",", "wie", "un\u00b7ser", "Ziel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "VAINF", "$,", "PWAV", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dein seliger Genu\u00df.", "tokens": ["Dein", "se\u00b7li\u00b7ger", "Ge\u00b7nu\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Hier hast du unser gantzes Volck:", "tokens": ["Hier", "hast", "du", "un\u00b7ser", "gant\u00b7zes", "Volck", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bekehre es zu dir;", "tokens": ["Be\u00b7keh\u00b7re", "es", "zu", "dir", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auch werde unsre Zeugen-Wolck,", "tokens": ["Auch", "wer\u00b7de", "uns\u00b7re", "Zeu\u00b7gen\u00b7Wolck", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und bleibe werth vor dir.", "tokens": ["Und", "blei\u00b7be", "werth", "vor", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Hier hast du uns, so wie wir sind,", "tokens": ["Hier", "hast", "du", "uns", ",", "so", "wie", "wir", "sind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "$,", "ADV", "KOKOM", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hier hast du unser Flehn:", "tokens": ["Hier", "hast", "du", "un\u00b7ser", "Flehn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn man uns heute an dich bindt,", "tokens": ["Wenn", "man", "uns", "heu\u00b7te", "an", "dich", "bindt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So ist uns gnung geschehn.", "tokens": ["So", "ist", "uns", "gnung", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}