nkt.", "tokens": ["quer\u00b7\u00fc\u00b7ber\u00b7feld", "ein", "For\u00b7stad\u00b7junkt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}