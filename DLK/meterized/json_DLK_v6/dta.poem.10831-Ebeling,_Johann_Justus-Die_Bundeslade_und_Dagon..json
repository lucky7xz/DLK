{"dta.poem.10831": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Die  \n  Bundeslade  \n und  \n  Dagon.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198774", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das Sinnbild von des H\u00f6chsten Gna-\nde,", "tokens": ["Das", "Sinn\u00b7bild", "von", "des", "H\u00f6chs\u00b7ten", "Gna", "de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Gottheit Thron, die Bun-", "tokens": ["Der", "Got\u00b7theit", "Thron", ",", "die", "Bun"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "ART", "TRUNC"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wodurch ganz Jsrael besch\u00fczt,", "tokens": ["Wo\u00b7durch", "ganz", "Js\u00b7rael", "be\u00b7sch\u00fczt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "NE", "VVPP", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Weil Zebaoth davon geblizt,", "tokens": ["Weil", "Ze\u00b7bao\u00b7th", "da\u00b7von", "ge\u00b7blizt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ward einsten als das Volk geschlagen,", "tokens": ["Ward", "eins\u00b7ten", "als", "das", "Volk", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "KOKOM", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Zur Straff von Feinden weggetragen.", "tokens": ["Zur", "Straff", "von", "Fein\u00b7den", "weg\u00b7ge\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Man h\u00e4tte sie mit Vorbedacht,", "tokens": ["Man", "h\u00e4t\u00b7te", "sie", "mit", "Vor\u00b7be\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Zu Dagons Tempel hingebracht;", "tokens": ["Zu", "Da\u00b7gons", "Tem\u00b7pel", "hin\u00b7ge\u00b7bracht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Zu ihres Abgotts heilgen H\u00f6hen,", "tokens": ["Zu", "ih\u00b7res", "Ab\u00b7gotts", "heil\u00b7gen", "H\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Da solte sie verwahret stehen.", "tokens": ["Da", "sol\u00b7te", "sie", "ver\u00b7wah\u00b7ret", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Allein o! Wunder welch ein Spiel!", "tokens": ["Al\u00b7lein", "o", "!", "Wun\u00b7der", "welch", "ein", "Spiel", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "FM", "$.", "NN", "PWAT", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Das Bild des eitlen Dagons fiel,", "tokens": ["Das", "Bild", "des", "eit\u00b7len", "Da\u00b7gons", "fiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Es st\u00fcrzte; Dagon muste liegen,", "tokens": ["Es", "st\u00fcrz\u00b7te", ";", "Da\u00b7gon", "mus\u00b7te", "lie\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PAV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Bei der Philister ihren Siegen.", "tokens": ["Bei", "der", "Phi\u00b7lis\u00b7ter", "ih\u00b7ren", "Sie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "So geht es denen die mit Hohn,", "tokens": ["So", "geht", "es", "de\u00b7nen", "die", "mit", "Hohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PDS", "ART", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Verspotten die Religion,", "tokens": ["Ver\u00b7spot\u00b7ten", "die", "Re\u00b7li\u00b7gi\u00b7on", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.17": {"text": "Es kan den Sp\u00f6ttern wol gelingen,", "tokens": ["Es", "kan", "den", "Sp\u00f6t\u00b7tern", "wol", "ge\u00b7lin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Da\u00df sie ein Jsrael bezwingen:", "tokens": ["Da\u00df", "sie", "ein", "Js\u00b7rael", "be\u00b7zwin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NE", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Allein das wird doch nie geschehn,", "tokens": ["Al\u00b7lein", "das", "wird", "doch", "nie", "ge\u00b7schehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "VAFIN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Da\u00df sie den Glauben fallen sehn.", "tokens": ["Da\u00df", "sie", "den", "Glau\u00b7ben", "fal\u00b7len", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Es steht des Heilands Lehrgeb\u00e4ude,", "tokens": ["Es", "steht", "des", "Hei\u00b7lands", "Lehr\u00b7ge\u00b7b\u00e4u\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Auch fest bei aller Teuffel Neide.", "tokens": ["Auch", "fest", "bei", "al\u00b7ler", "Teuf\u00b7fel", "Nei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Wer Christum und den Belial,", "tokens": ["Wer", "Chris\u00b7tum", "und", "den", "Be\u00b7li\u00b7al", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NE", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Vereint, der kommt zulezt zum Fall.", "tokens": ["Ver\u00b7eint", ",", "der", "kommt", "zu\u00b7lezt", "zum", "Fall", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PRELS", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}