{"textgrid.poem.53844": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Feldfr\u00fcchte", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sinnend geh ich durch den Garten,", "tokens": ["Sin\u00b7nend", "geh", "ich", "durch", "den", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "still gedeiht er hinterm Haus;", "tokens": ["still", "ge\u00b7deiht", "er", "hin\u00b7term", "Haus", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Suppenkr\u00e4uter, hundert Arten,", "tokens": ["Sup\u00b7pen\u00b7kr\u00e4u\u00b7ter", ",", "hun\u00b7dert", "Ar\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "CARD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bauernblumen, bunter Strau\u00df.", "tokens": ["Bau\u00b7ern\u00b7blu\u00b7men", ",", "bun\u00b7ter", "Strau\u00df", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Petersilie und Tomaten,", "tokens": ["Pe\u00b7ter\u00b7si\u00b7lie", "und", "To\u00b7ma\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "$,"], "meter": "+--+-+--", "measure": "iambic.tri.invert"}, "line.6": {"text": "eine Bohnengalerie,", "tokens": ["ei\u00b7ne", "Boh\u00b7nen\u00b7ga\u00b7le\u00b7rie", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "ganz besonders ist geraten", "tokens": ["ganz", "be\u00b7son\u00b7ders", "ist", "ge\u00b7ra\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "VAFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "der beliebte Sellerie.", "tokens": ["der", "be\u00b7lieb\u00b7te", "Sel\u00b7le\u00b7rie", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Ja, und hier \u2013? Ein kleines Wieschen?", "tokens": ["Ja", ",", "und", "hier", "\u2013", "?", "Ein", "klei\u00b7nes", "Wie\u00b7schen", "?"], "token_info": ["word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KON", "ADV", "$(", "$.", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Da w\u00e4chst in der Erde leis", "tokens": ["Da", "w\u00e4chst", "in", "der", "Er\u00b7de", "leis"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "ADJD"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.11": {"text": "das bescheidene Radieschen:", "tokens": ["das", "be\u00b7schei\u00b7de\u00b7ne", "Ra\u00b7die\u00b7schen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.12": {"text": "au\u00dfen, rot und innen wei\u00df.", "tokens": ["au\u00b7\u00dfen", ",", "rot", "und", "in\u00b7nen", "wei\u00df", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADJD", "KON", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sinnend geh ich durch den Garten", "tokens": ["Sin\u00b7nend", "geh", "ich", "durch", "den", "Gar\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "unsrer deutschen Politik;", "tokens": ["uns\u00b7rer", "deut\u00b7schen", "Po\u00b7li\u00b7tik", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Suppenkohl in allen Arten", "tokens": ["Sup\u00b7pen\u00b7kohl", "in", "al\u00b7len", "Ar\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "im Kompost der Republik.", "tokens": ["im", "Kom\u00b7post", "der", "Re\u00b7pub\u00b7lik", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Bonzen, Brillen, Gehberockte,", "tokens": ["Bon\u00b7zen", ",", "Bril\u00b7len", ",", "Geh\u00b7be\u00b7rock\u00b7te", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Parlamentsroutinendreh . . .", "tokens": ["Par\u00b7la\u00b7ments\u00b7rou\u00b7ti\u00b7nen\u00b7dreh", ".", ".", "."], "token_info": ["word", "punct", "punct", "punct"], "pos": ["NE", "$.", "$.", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.7": {"text": "Ja, und hier \u2013? Die ganz verbockte", "tokens": ["Ja", ",", "und", "hier", "\u2013", "?", "Die", "ganz", "ver\u00b7bock\u00b7te"], "token_info": ["word", "punct", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "KON", "ADV", "$(", "$.", "ART", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "liebe gute SPD.", "tokens": ["lie\u00b7be", "gu\u00b7te", "SpD", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "Hermann M\u00fcller, Hilferlieschen", "tokens": ["Her\u00b7mann", "M\u00fcl\u00b7ler", ",", "Hil\u00b7fer\u00b7lie\u00b7schen"], "token_info": ["word", "word", "punct", "word"], "pos": ["NE", "NE", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "bl\u00fchn so harmlos, doof und leis", "tokens": ["bl\u00fchn", "so", "harm\u00b7los", ",", "doof", "und", "leis"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADJD", "$,", "NE", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "wie bescheidene Radieschen:", "tokens": ["wie", "be\u00b7schei\u00b7de\u00b7ne", "Ra\u00b7die\u00b7schen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$."], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.12": {"text": "au\u00dfen rot und innen wei\u00df.", "tokens": ["au\u00b7\u00dfen", "rot", "und", "in\u00b7nen", "wei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}