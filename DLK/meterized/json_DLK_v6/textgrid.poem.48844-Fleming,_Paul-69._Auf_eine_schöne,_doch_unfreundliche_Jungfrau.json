{"textgrid.poem.48844": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "69. Auf eine sch\u00f6ne, doch unfreundliche Jungfrau", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich mu\u00df es selbst gestehn, du gleichest ganz an Sch\u00f6ne", "tokens": ["Ich", "mu\u00df", "es", "selbst", "ge\u00b7stehn", ",", "du", "glei\u00b7chest", "ganz", "an", "Sch\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VVPP", "$,", "PPER", "VVFIN", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "der Amathusen selbst. Di\u00df ist das g\u00fcldne Haar,", "tokens": ["der", "A\u00b7mat\u00b7hu\u00b7sen", "selbst", ".", "Di\u00df", "ist", "das", "g\u00fcld\u00b7ne", "Haar", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$.", "PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "das Jovis Tochter tr\u00e4gt, die ihm sein H\u00e4upt gebar,", "tokens": ["das", "Jo\u00b7vis", "Toch\u00b7ter", "tr\u00e4gt", ",", "die", "ihm", "sein", "H\u00e4upt", "ge\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "NN", "VVFIN", "$,", "PRELS", "PPER", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "so sieht Thaliens Mund bei seiner Hippokrene:", "tokens": ["so", "sieht", "Tha\u00b7li\u00b7ens", "Mund", "bei", "sei\u00b7ner", "Hip\u00b7po\u00b7kre\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-++--+-+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Aglaiens ist die Brust, die alle Venuss\u00f6hne", "tokens": ["Ag\u00b7lai\u00b7ens", "ist", "die", "Brust", ",", "die", "al\u00b7le", "Ve\u00b7nus\u00b7s\u00f6h\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VAFIN", "ART", "NN", "$,", "PRELS", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "f\u00fcr Alles achten hoch. Dich r\u00fchmt mit Rechte zwar", "tokens": ["f\u00fcr", "Al\u00b7les", "ach\u00b7ten", "hoch", ".", "Dich", "r\u00fchmt", "mit", "Rech\u00b7te", "zwar"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIS", "VVFIN", "ADJD", "$.", "PPER", "VVFIN", "APPR", "NN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "vom Ansehn Jederman. Und, das fast halb ist wahr,", "tokens": ["vom", "An\u00b7sehn", "Je\u00b7der\u00b7man", ".", "Und", ",", "das", "fast", "halb", "ist", "wahr", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PIS", "$.", "KON", "$,", "PRELS", "ADV", "ADJD", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "du bist bald sch\u00f6ner noch als meine ", "tokens": ["du", "bist", "bald", "sch\u00f6\u00b7ner", "noch", "als", "mei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "ADV", "KOKOM", "PPOSAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "W\u00e4r' Amathusens Geist, Minervens ihr Beginnen,", "tokens": ["W\u00e4r'", "A\u00b7mat\u00b7hu\u00b7sens", "Geist", ",", "Mi\u00b7ner\u00b7vens", "ihr", "Be\u00b7gin\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "NN", "$,", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Aglajens H\u00f6flichkeit und Basilenens Sinnen", "tokens": ["Ag\u00b7la\u00b7jens", "H\u00f6f\u00b7lich\u00b7keit", "und", "Ba\u00b7si\u00b7le\u00b7nens", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NN", "KON", "NE", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "so wahr hier, als ihr Leib, so w\u00fc\u00dft ich f\u00fcr dir Rat.", "tokens": ["so", "wahr", "hier", ",", "als", "ihr", "Leib", ",", "so", "w\u00fc\u00dft", "ich", "f\u00fcr", "dir", "Rat", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "$,", "KOUS", "PPOSAT", "NN", "$,", "ADV", "VVFIN", "PPER", "APPR", "PPER", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Du w\u00e4rest auf der Welt so lange nicht geblieben.", "tokens": ["Du", "w\u00e4\u00b7rest", "auf", "der", "Welt", "so", "lan\u00b7ge", "nicht", "ge\u00b7blie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "ADV", "ADV", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So lobt ein Jeder dich und will dich niemand lieben.", "tokens": ["So", "lobt", "ein", "Je\u00b7der", "dich", "und", "will", "dich", "nie\u00b7mand", "lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "PIS", "PPER", "KON", "VMFIN", "PRF", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Was ist der sch\u00f6nste Leib, der keine Seele hat?", "tokens": ["Was", "ist", "der", "sch\u00f6ns\u00b7te", "Leib", ",", "der", "kei\u00b7ne", "See\u00b7le", "hat", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "ADJA", "NN", "$,", "PRELS", "PIAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}