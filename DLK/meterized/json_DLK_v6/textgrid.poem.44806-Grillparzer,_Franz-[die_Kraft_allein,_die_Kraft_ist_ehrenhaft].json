{"textgrid.poem.44806": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[die Kraft allein, die Kraft ist ehrenhaft]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Kraft allein, die Kraft ist ehrenhaft,", "tokens": ["Die", "Kraft", "al\u00b7lein", ",", "die", "Kraft", "ist", "eh\u00b7ren\u00b7haft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So ruft das deutsche Volk in seiner Hoheit.", "tokens": ["So", "ruft", "das", "deut\u00b7sche", "Volk", "in", "sei\u00b7ner", "Ho\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch da man Kraft so schnell sich nicht verschafft,", "tokens": ["Doch", "da", "man", "Kraft", "so", "schnell", "sich", "nicht", "ver\u00b7schafft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "NN", "ADV", "ADJD", "PRF", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Begn\u00fcgt man sich indessen mit der Roheit.", "tokens": ["Be\u00b7gn\u00fcgt", "man", "sich", "in\u00b7des\u00b7sen", "mit", "der", "Ro\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PRF", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}