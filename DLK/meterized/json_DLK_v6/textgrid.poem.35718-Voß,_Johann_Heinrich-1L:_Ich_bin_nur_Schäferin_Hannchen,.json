{"textgrid.poem.35718": {"metadata": {"author": {"name": "Vo\u00df, Johann Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich bin nur Sch\u00e4ferin Hannchen,", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin nur Sch\u00e4ferin Hannchen,", "tokens": ["Ich", "bin", "nur", "Sch\u00e4\u00b7fe\u00b7rin", "Hann\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "NE", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Nicht h\u00e4\u00dflich und nicht sch\u00f6n;", "tokens": ["Nicht", "h\u00e4\u00df\u00b7lich", "und", "nicht", "sch\u00f6n", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "KON", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch schwerlich tauscht' ich mit manchen,", "tokens": ["Doch", "schwer\u00b7lich", "tauscht'", "ich", "mit", "man\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "APPR", "PIAT", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die stolz ihr K\u00f6pfchen drehn.", "tokens": ["Die", "stolz", "ihr", "K\u00f6pf\u00b7chen", "drehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "La\u00df manche prunken und scheinen:", "tokens": ["La\u00df", "man\u00b7che", "prun\u00b7ken", "und", "schei\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PIS", "VVINF", "KON", "VVFIN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "Ich schm\u00fccke mich nur leicht", "tokens": ["Ich", "schm\u00fc\u00b7cke", "mich", "nur", "leicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Mit selbstgesponnenen Leinen,", "tokens": ["Mit", "selbst\u00b7ge\u00b7spon\u00b7ne\u00b7nen", "Lei\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "---+--+-", "measure": "iambic.di.relaxed"}, "line.8": {"text": "Gebl\u00fcmt und hell gebleicht.", "tokens": ["Ge\u00b7bl\u00fcmt", "und", "hell", "ge\u00b7bleicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wann Tau am Grase noch blitzet,", "tokens": ["Wann", "Tau", "am", "Gra\u00b7se", "noch", "blit\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPRART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Treib' ich, weil Hurtig bellt,", "tokens": ["Treib'", "ich", ",", "weil", "Hur\u00b7tig", "bellt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "NE", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vom Halmenhute besch\u00fctzet,", "tokens": ["Vom", "Hal\u00b7men\u00b7hu\u00b7te", "be\u00b7sch\u00fct\u00b7zet", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Des Vaters Herd' ins Feld.", "tokens": ["Des", "Va\u00b7ters", "Herd'", "ins", "Feld", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Sch\u00e4fchen bl\u00f6ken und grasen,", "tokens": ["Die", "Sch\u00e4f\u00b7chen", "bl\u00f6\u00b7ken", "und", "gra\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Wo Klee und Quendel bl\u00fcht:", "tokens": ["Wo", "Klee", "und", "Quen\u00b7del", "bl\u00fcht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ich strick' auf schattigem Rasen,", "tokens": ["Ich", "strick'", "auf", "schat\u00b7ti\u00b7gem", "Ra\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Und sing' ein Sch\u00e4ferlied.", "tokens": ["Und", "sing'", "ein", "Sch\u00e4\u00b7fer\u00b7lied", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Am Mittag deck' ich zum Mahle", "tokens": ["Am", "Mit\u00b7tag", "deck'", "ich", "zum", "Mah\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Den Rasen, weich und fein,", "tokens": ["Den", "Ra\u00b7sen", ",", "weich", "und", "fein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit Spillbauml\u00f6ffel und Schale,", "tokens": ["Mit", "Spill\u00b7baum\u00b7l\u00f6f\u00b7fel", "und", "Scha\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und schmause ganz allein.", "tokens": ["Und", "schmau\u00b7se", "ganz", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Mutter f\u00fcllte die Taschen", "tokens": ["Die", "Mut\u00b7ter", "f\u00fcll\u00b7te", "die", "Ta\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Mit reifer Gartenfrucht,", "tokens": ["Mit", "rei\u00b7fer", "Gar\u00b7ten\u00b7frucht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und Felderdbeeren zum Naschen", "tokens": ["Und", "Fel\u00b7derd\u00b7bee\u00b7ren", "zum", "Na\u00b7schen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "APPRART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Gl\u00fchn ringsher ungesucht.", "tokens": ["Gl\u00fchn", "rings\u00b7her", "un\u00b7ge\u00b7sucht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Von Kr\u00e4utern, B\u00fcschen und B\u00e4umen", "tokens": ["Von", "Kr\u00e4u\u00b7tern", ",", "B\u00fc\u00b7schen", "und", "B\u00e4u\u00b7men"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ert\u00f6net um und um", "tokens": ["Er\u00b7t\u00f6\u00b7net", "um", "und", "um"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PTKVZ", "KON", "KOUI"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gesang der V\u00f6gel und Heimen,", "tokens": ["Ge\u00b7sang", "der", "V\u00f6\u00b7gel", "und", "Hei\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Des Bienenvolks Gesumm.", "tokens": ["Des", "Bie\u00b7nen\u00b7volks", "Ge\u00b7summ", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Oft flecht' ich Blumen zum Kranze,", "tokens": ["Oft", "flecht'", "ich", "Blu\u00b7men", "zum", "Kran\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "APPRART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Und spiegle mich als Braut", "tokens": ["Und", "spieg\u00b7le", "mich", "als", "Braut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "KOUS", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Am Quell im zitternden Glanze,", "tokens": ["Am", "Quell", "im", "zit\u00b7tern\u00b7den", "Glan\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Und sinn', und lache laut.", "tokens": ["Und", "sinn'", ",", "und", "la\u00b7che", "laut", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KON", "VVFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Auch macht mein L\u00e4mmchen mir Freude:", "tokens": ["Auch", "macht", "mein", "L\u00e4mm\u00b7chen", "mir", "Freu\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PPER", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Es folgt mir, wie am Band,", "tokens": ["Es", "folgt", "mir", ",", "wie", "am", "Band", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PWAV", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Empf\u00e4ngt die blumige Weide,", "tokens": ["Emp\u00b7f\u00e4ngt", "die", "blu\u00b7mi\u00b7ge", "Wei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und lecket mir die Hand.", "tokens": ["Und", "le\u00b7cket", "mir", "die", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Doch wird ein Nestchen gefunden", "tokens": ["Doch", "wird", "ein", "Nest\u00b7chen", "ge\u00b7fun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ART", "NN", "VVPP"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Im dichtbelaubten Strauch,", "tokens": ["Im", "dicht\u00b7be\u00b7laub\u00b7ten", "Strauch", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Dann seufz' ich: Einsame Stunden!", "tokens": ["Dann", "seuf\u00b7z'", "ich", ":", "Ein\u00b7sa\u00b7me", "Stun\u00b7den", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "O baut' ich selber auch!", "tokens": ["O", "baut'", "ich", "sel\u00b7ber", "auch", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Wie manchen Abend, wie manchen", "tokens": ["Wie", "man\u00b7chen", "A\u00b7bend", ",", "wie", "man\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "PIAT", "NN", "$,", "PWAV", "PIAT"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sieht Robert \u00fcbern Zaun,", "tokens": ["Sieht", "Ro\u00b7bert", "\u00fc\u00b7bern", "Zaun", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und gr\u00fc\u00dft so freundlich: Mein Hannchen,", "tokens": ["Und", "gr\u00fc\u00dft", "so", "freund\u00b7lich", ":", "Mein", "Hann\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "$.", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Schlaf wohl, la\u00df dir nicht graun!", "tokens": ["Schlaf", "wohl", ",", "la\u00df", "dir", "nicht", "graun", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "VVIMP", "PPER", "PTKNEG", "VVINF", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Err\u00f6tend treib' ich die Schafe,", "tokens": ["Er\u00b7r\u00f6\u00b7tend", "treib'", "ich", "die", "Scha\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Und blicke vor mich hin;", "tokens": ["Und", "bli\u00b7cke", "vor", "mich", "hin", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Dann ist er Sch\u00e4fer im Schlafe,", "tokens": ["Dann", "ist", "er", "Sch\u00e4\u00b7fer", "im", "Schla\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NE", "APPRART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Und ich bin Sch\u00e4ferin.", "tokens": ["Und", "ich", "bin", "Sch\u00e4\u00b7fe\u00b7rin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}