{"textgrid.poem.54986": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Die Bekehrte", "genre": "verse", "period": "N.A.", "pub_year": 1796, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bei dem Glanze der Abendr\u00f6te", "tokens": ["Bei", "dem", "Glan\u00b7ze", "der", "A\u00b7bend\u00b7r\u00f6\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Ging ich still den Wald entlang,", "tokens": ["Ging", "ich", "still", "den", "Wald", "ent\u00b7lang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Damon sa\u00df und blies die Fl\u00f6te,", "tokens": ["Da\u00b7mon", "sa\u00df", "und", "blies", "die", "Fl\u00f6\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df es von den Felsen klang,", "tokens": ["Da\u00df", "es", "von", "den", "Fel\u00b7sen", "klang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "So la la!", "tokens": ["So", "la", "la", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "FM", "FM", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Und er zog mich, ach, an sich nieder,", "tokens": ["Und", "er", "zog", "mich", ",", "ach", ",", "an", "sich", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "$,", "ITJ", "$,", "APPR", "PRF", "PTKVZ", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "K\u00fc\u00dfte mich so hold, so s\u00fc\u00df.", "tokens": ["K\u00fc\u00df\u00b7te", "mich", "so", "hold", ",", "so", "s\u00fc\u00df", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "$,", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ich sagte: \u00bbBlase wieder!\u00ab", "tokens": ["Und", "ich", "sag\u00b7te", ":", "\u00bb", "Bla\u00b7se", "wie\u00b7der", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "VVFIN", "$.", "$(", "NN", "ADV", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und der gute Junge blies,", "tokens": ["Und", "der", "gu\u00b7te", "Jun\u00b7ge", "blies", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.5": {"text": "So la la!", "tokens": ["So", "la", "la", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "FM", "FM", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Meine Ruhe ist nun verloren,", "tokens": ["Mei\u00b7ne", "Ru\u00b7he", "ist", "nun", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Meine Freude floh davon,", "tokens": ["Mei\u00b7ne", "Freu\u00b7de", "floh", "da\u00b7von", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PAV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ich h\u00f6re vor meinen Ohren", "tokens": ["Und", "ich", "h\u00f6\u00b7re", "vor", "mei\u00b7nen", "Oh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Immer nur den alten Ton,", "tokens": ["Im\u00b7mer", "nur", "den", "al\u00b7ten", "Ton", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "So la la! le ralla!", "tokens": ["So", "la", "la", "!", "le", "ral\u00b7la", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "FM", "FM", "$.", "FM", "FM", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "(usw.)", "tokens": ["(", "us\u00b7w.", ")"], "token_info": ["punct", "abbreviation", "punct"], "pos": ["$(", "NE", "$("]}}}}}