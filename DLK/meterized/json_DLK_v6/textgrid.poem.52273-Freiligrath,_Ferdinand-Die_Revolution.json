{"textgrid.poem.52273": {"metadata": {"author": {"name": "Freiligrath, Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Die Revolution", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und ob ihr sie, ein edel Wild, mit euren Henkersknechten fingt;", "tokens": ["Und", "ob", "ihr", "sie", ",", "ein", "e\u00b7del", "Wild", ",", "mit", "eu\u00b7ren", "Hen\u00b7kers\u00b7knech\u00b7ten", "fingt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "$,", "ART", "ADJA", "NN", "$,", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Und ob ihr unterm Festungswall standrechten die Gefangne gingt;", "tokens": ["Und", "ob", "ihr", "un\u00b7term", "Fes\u00b7tungs\u00b7wall", "stand\u00b7rech\u00b7ten", "die", "Ge\u00b7fang\u00b7ne", "gingt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPRART", "NN", "VVFIN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "Und ob sie l\u00e4ngst der H\u00fcgel deckt, auf dessen Gr\u00fcn ums Morgenrot", "tokens": ["Und", "ob", "sie", "l\u00e4ngst", "der", "H\u00fc\u00b7gel", "deckt", ",", "auf", "des\u00b7sen", "Gr\u00fcn", "ums", "Mor\u00b7gen\u00b7rot"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADV", "ART", "NN", "VVFIN", "$,", "APPR", "PRELAT", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "Die junge B\u00e4urin Kr\u00e4nze legt \u2013 ", "tokens": ["Die", "jun\u00b7ge", "B\u00e4u\u00b7rin", "Kr\u00e4n\u00b7ze", "legt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und ob ihr von der hohen Stirn das wehnde Lockenhaar ihr schort;", "tokens": ["Und", "ob", "ihr", "von", "der", "ho\u00b7hen", "Stirn", "das", "wehn\u00b7de", "Lo\u00b7cken\u00b7haar", "ihr", "schort", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Und ob ihr zu Genossen ihr den M\u00f6rder und den Dieb erkort;", "tokens": ["Und", "ob", "ihr", "zu", "Ge\u00b7nos\u00b7sen", "ihr", "den", "M\u00f6r\u00b7der", "und", "den", "Dieb", "er\u00b7kort", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "NN", "PPER", "ART", "NN", "KON", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "Und ob sie Zuchthauskleider tr\u00e4gt, im Scho\u00df den Napf voll Erbsenbrei;", "tokens": ["Und", "ob", "sie", "Zucht\u00b7haus\u00b7klei\u00b7der", "tr\u00e4gt", ",", "im", "Scho\u00df", "den", "Napf", "voll", "Erb\u00b7sen\u00b7brei", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "NN", "VVFIN", "$,", "APPRART", "NN", "ART", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "Und ob sie Werg und Wolle spinnt \u2013 ", "tokens": ["Und", "ob", "sie", "Werg", "und", "Wol\u00b7le", "spinnt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "NE", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und ob ihr ins Exil sie jagt, von Lande sie zu Lande hetzt;", "tokens": ["Und", "ob", "ihr", "ins", "E\u00b7xil", "sie", "jagt", ",", "von", "Lan\u00b7de", "sie", "zu", "Lan\u00b7de", "hetzt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPRART", "NN", "PPER", "VVFIN", "$,", "APPR", "NN", "PPER", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Und ob sie fremde Herde sucht und stumm sich in die Asche setzt;", "tokens": ["Und", "ob", "sie", "frem\u00b7de", "Her\u00b7de", "sucht", "und", "stumm", "sich", "in", "die", "A\u00b7sche", "setzt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJA", "NN", "VVFIN", "KON", "ADJD", "PRF", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "Und ob sie wunde Sohlen taucht in ferner Wasserstr\u00f6me Lauf \u2013", "tokens": ["Und", "ob", "sie", "wun\u00b7de", "Soh\u00b7len", "taucht", "in", "fer\u00b7ner", "Was\u00b7ser\u00b7str\u00f6\u00b7me", "Lauf", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJA", "NN", "VVFIN", "APPR", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.4": {"line.1": {"text": "O nein \u2013 sie stellt sie vor sich hin; sie schl\u00e4gt sie trotzig, euch zum Trotz!", "tokens": ["O", "nein", "\u2013", "sie", "stellt", "sie", "vor", "sich", "hin", ";", "sie", "schl\u00e4gt", "sie", "trot\u00b7zig", ",", "euch", "zum", "Trotz", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PTKANT", "$(", "PPER", "VVFIN", "PPER", "APPR", "PRF", "PTKVZ", "$.", "PPER", "VVFIN", "PPER", "ADJD", "$,", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Sie spottet lachend des Exils, wie sie gespottet des Schafotts!", "tokens": ["Sie", "spot\u00b7tet", "la\u00b7chend", "des", "E\u00b7xils", ",", "wie", "sie", "ge\u00b7spot\u00b7tet", "des", "Scha\u00b7fotts", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "NN", "$,", "PWAV", "PPER", "VVPP", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+--+-", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Sie singt ein Lied, da\u00df ihr entsetzt von euren Sesseln euch erhebt;", "tokens": ["Sie", "singt", "ein", "Lied", ",", "da\u00df", "ihr", "ent\u00b7setzt", "von", "eu\u00b7ren", "Ses\u00b7seln", "euch", "er\u00b7hebt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "KOUS", "PPER", "VVPP", "APPR", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "Da\u00df euch das Herz \u2013 das feige Herz, das falsche Herz! \u2013 im Leibe hebt!", "tokens": ["Da\u00df", "euch", "das", "Herz", "\u2013", "das", "fei\u00b7ge", "Herz", ",", "das", "fal\u00b7sche", "Herz", "!", "\u2013", "im", "Lei\u00b7be", "hebt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "$(", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$.", "$(", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.5": {"line.1": {"text": "Noch minder gar ein Lied des Hohns auf das verworfne Zwischenspiel,", "tokens": ["Noch", "min\u00b7der", "gar", "ein", "Lied", "des", "Hohns", "auf", "das", "ver\u00b7worf\u00b7ne", "Zwi\u00b7schen\u00b7spiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ART", "NN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Die Bettleroper, die zurzeit ihr plump noch zu agieren wi\u00dft,", "tokens": ["Die", "Bett\u00b7le\u00b7ro\u00b7per", ",", "die", "zur\u00b7zeit", "ihr", "plump", "noch", "zu", "a\u00b7gie\u00b7ren", "wi\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PPOSAT", "ADJD", "ADV", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "Wie mottig euer Hermelin, wie faul auch euer Purpur ist!", "tokens": ["Wie", "mot\u00b7tig", "eu\u00b7er", "Her\u00b7me\u00b7lin", ",", "wie", "faul", "auch", "eu\u00b7er", "Pur\u00b7pur", "ist", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPOSAT", "NN", "$,", "PWAV", "ADJD", "ADV", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.6": {"line.1": {"text": "O nein, was sie den Wassern singt, ist nicht der Schmerz und nicht die Schmach \u2013", "tokens": ["O", "nein", ",", "was", "sie", "den", "Was\u00b7sern", "singt", ",", "ist", "nicht", "der", "Schmerz", "und", "nicht", "die", "Schmach", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKANT", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$,", "VAFIN", "PTKNEG", "ART", "NN", "KON", "PTKNEG", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Ist Siegeslied, Triumpheslied, Lied von der Zukunft gro\u00dfem Tag!", "tokens": ["Ist", "Sie\u00b7ges\u00b7lied", ",", "Tri\u00b7um\u00b7phes\u00b7lied", ",", "Lied", "von", "der", "Zu\u00b7kunft", "gro\u00b7\u00dfem", "Tag", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "NN", "$,", "NN", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+--+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Der Zukunft, die nicht fern mehr ist! Sie spricht mit dreistem Prophezein,", "tokens": ["Der", "Zu\u00b7kunft", ",", "die", "nicht", "fern", "mehr", "ist", "!", "Sie", "spricht", "mit", "dreis\u00b7tem", "Pro\u00b7phe\u00b7zein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PTKNEG", "ADJD", "ADV", "VAFIN", "$.", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "So gut wie weiland euer Gott: Ich war, ich bin \u2013 ich", "tokens": ["So", "gut", "wie", "wei\u00b7land", "eu\u00b7er", "Gott", ":", "Ich", "war", ",", "ich", "bin", "\u2013", "ich"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ADV", "ADJD", "KOKOM", "ADV", "PPOSAT", "NN", "$.", "PPER", "VAFIN", "$,", "PPER", "VAFIN", "$(", "PPER"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Ich werde sein, und wiederum voraus den V\u00f6lkern werd' ich gehn!", "tokens": ["Ich", "wer\u00b7de", "sein", ",", "und", "wie\u00b7de\u00b7rum", "vo\u00b7raus", "den", "V\u00f6l\u00b7kern", "werd'", "ich", "gehn", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VAINF", "$,", "KON", "ADV", "ADV", "ART", "NN", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Auf eurem Nacken, eurem Haupt, auf euren Kronen werd' ich stehn!", "tokens": ["Auf", "eu\u00b7rem", "Na\u00b7cken", ",", "eu\u00b7rem", "Haupt", ",", "auf", "eu\u00b7ren", "Kro\u00b7nen", "werd'", "ich", "stehn", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "Befreierin und R\u00e4cherin und Richterin, das Schwert entbl\u00f6\u00dft,", "tokens": ["Be\u00b7frei\u00b7e\u00b7rin", "und", "R\u00e4\u00b7che\u00b7rin", "und", "Rich\u00b7te\u00b7rin", ",", "das", "Schwert", "ent\u00b7bl\u00f6\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "Ausrecken den gewalt'gen Arm werd' ich, da\u00df er die Welt erl\u00f6st!", "tokens": ["Aus\u00b7re\u00b7cken", "den", "ge\u00b7walt'\u00b7gen", "Arm", "werd'", "ich", ",", "da\u00df", "er", "die", "Welt", "er\u00b7l\u00f6st", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "VAFIN", "PPER", "$,", "KOUS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.8": {"line.1": {"text": "Ihr seht mich in den Kerkern blo\u00df, ihr seht mich in der Grube nur,", "tokens": ["Ihr", "seht", "mich", "in", "den", "Ker\u00b7kern", "blo\u00df", ",", "ihr", "seht", "mich", "in", "der", "Gru\u00b7be", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "ADV", "$,", "PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Ihr seht mich nur als Irrende auf des Exiles dorn'ger Flur \u2013", "tokens": ["Ihr", "seht", "mich", "nur", "als", "Ir\u00b7ren\u00b7de", "auf", "des", "E\u00b7xi\u00b7les", "dorn'\u00b7ger", "Flur", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "KOUS", "NN", "APPR", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+--+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Ihr Bl\u00f6den, wohn' ich denn nicht auch, wo eure Macht ein Ende hat:", "tokens": ["Ihr", "Bl\u00f6\u00b7den", ",", "wohn'", "ich", "denn", "nicht", "auch", ",", "wo", "eu\u00b7re", "Macht", "ein", "En\u00b7de", "hat", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PWAV", "PPER", "ADV", "PTKNEG", "ADV", "$,", "PWAV", "PPOSAT", "NN", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "Bleibt mir nicht hinter jeder Stirn, in jedem Herzen eine Statt?", "tokens": ["Bleibt", "mir", "nicht", "hin\u00b7ter", "je\u00b7der", "Stirn", ",", "in", "je\u00b7dem", "Her\u00b7zen", "ei\u00b7ne", "Statt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "APPR", "PIAT", "NN", "$,", "APPR", "PIAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}, "stanza.9": {"line.1": {"text": "In jedem Haupt, das trotzig denkt? das hoch und ungebeugt sich tr\u00e4gt?", "tokens": ["In", "je\u00b7dem", "Haupt", ",", "das", "trot\u00b7zig", "denkt", "?", "das", "hoch", "und", "un\u00b7ge\u00b7beugt", "sich", "tr\u00e4gt", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$.", "ART", "ADJD", "KON", "ADJD", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Ist mein Asyl nicht jede Brust, die menschlich f\u00fchlt und menschlich schl\u00e4gt?", "tokens": ["Ist", "mein", "A\u00b7syl", "nicht", "je\u00b7de", "Brust", ",", "die", "menschlich", "f\u00fchlt", "und", "menschlich", "schl\u00e4gt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "PTKNEG", "PIAT", "NN", "$,", "PRELS", "ADJD", "VVFIN", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Nicht jede Werkstatt, drin es pocht? nicht jede H\u00fctte, drin es \u00e4chzt \u2013", "tokens": ["Nicht", "je\u00b7de", "Werk\u00b7statt", ",", "drin", "es", "pocht", "?", "nicht", "je\u00b7de", "H\u00fct\u00b7te", ",", "drin", "es", "\u00e4chzt", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PIAT", "NN", "$,", "ADV", "PPER", "VVFIN", "$.", "PTKNEG", "PIAT", "NN", "$,", "ADV", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "Bin ich der Menschheit odem nicht, die rastlos nach Befreiung lechzt?", "tokens": ["Bin", "ich", "der", "Menschheit", "o\u00b7dem", "nicht", ",", "die", "rast\u00b7los", "nach", "Be\u00b7frei\u00b7ung", "lechzt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "KON", "PTKNEG", "$,", "PRELS", "ADJD", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.10": {"line.1": {"text": "Auf eurem Nacken, eurem haupt, auf euren Kronen werd' ich stehn!", "tokens": ["Auf", "eu\u00b7rem", "Na\u00b7cken", ",", "eu\u00b7rem", "haupt", ",", "auf", "eu\u00b7ren", "Kro\u00b7nen", "werd'", "ich", "stehn", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "'s ist der Geschichte ehrnes Mu\u00df! Es ist kein R\u00fchmen, ist kein Drohn \u2013", "tokens": ["'s", "ist", "der", "Ge\u00b7schich\u00b7te", "ehr\u00b7nes", "Mu\u00df", "!", "Es", "ist", "kein", "R\u00fch\u00b7men", ",", "ist", "kein", "Drohn", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJA", "NN", "$.", "PPER", "VAFIN", "PIAT", "NN", "$,", "VAFIN", "PIAT", "NN", "$("], "meter": "-+--+-+-+-+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.3": {"text": "Der Tag wird hei\u00df \u2013 wie wehst du k\u00fchl, o Weidenlaub von Babylon!", "tokens": ["Der", "Tag", "wird", "hei\u00df", "\u2013", "wie", "wehst", "du", "k\u00fchl", ",", "o", "Wei\u00b7den\u00b7laub", "von", "Ba\u00b7by\u00b7lon", "!"], "token