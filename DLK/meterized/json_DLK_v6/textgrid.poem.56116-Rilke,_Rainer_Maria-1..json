{"textgrid.poem.56116": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da stieg ein Baum. O reine \u00dcbersteigung!", "tokens": ["Da", "stieg", "ein", "Baum", ".", "O", "rei\u00b7ne", "\u00dc\u00b7bers\u00b7tei\u00b7gung", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "O Orpheus singt! O hoher Baum im Ohr!", "tokens": ["O", "Or\u00b7pheus", "singt", "!", "O", "ho\u00b7her", "Baum", "im", "Ohr", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "$.", "NE", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und alles schwieg. Doch selbst in der Verschweigung", "tokens": ["Und", "al\u00b7les", "schwieg", ".", "Doch", "selbst", "in", "der", "Ver\u00b7schwei\u00b7gung"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "$.", "KON", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "ging neuer Anfang, Wink und Wandlung vor.", "tokens": ["ging", "neu\u00b7er", "An\u00b7fang", ",", "Wink", "und", "Wand\u00b7lung", "vor", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$,", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Tiere aus Stille drangen aus dem klaren", "tokens": ["Tie\u00b7re", "aus", "Stil\u00b7le", "dran\u00b7gen", "aus", "dem", "kla\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "VVFIN", "APPR", "ART", "ADJA"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "gel\u00f6sten Wald von Lager und Genist;", "tokens": ["ge\u00b7l\u00f6s\u00b7ten", "Wald", "von", "La\u00b7ger", "und", "Ge\u00b7nist", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "und da ergab sich, da\u00df sie nicht aus List", "tokens": ["und", "da", "er\u00b7gab", "sich", ",", "da\u00df", "sie", "nicht", "aus", "List"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PRF", "$,", "KOUS", "PPER", "PTKNEG", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "und nicht aus Angst in sich so leise waren,", "tokens": ["und", "nicht", "aus", "Angst", "in", "sich", "so", "lei\u00b7se", "wa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPR", "NN", "APPR", "PRF", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "sondern aus H\u00f6ren. Br\u00fcllen, Schrei, Ger\u00f6hr", "tokens": ["son\u00b7dern", "aus", "H\u00f6\u00b7ren", ".", "Br\u00fcl\u00b7len", ",", "Schrei", ",", "Ge\u00b7r\u00f6hr"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["KON", "APPR", "NN", "$.", "NN", "$,", "NN", "$,", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "schien klein in ihren Herzen. Und wo eben", "tokens": ["schien", "klein", "in", "ih\u00b7ren", "Her\u00b7zen", ".", "Und", "wo", "e\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "$.", "KON", "PWAV", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "kaum eine H\u00fctte war, dies zu empfangen,", "tokens": ["kaum", "ei\u00b7ne", "H\u00fct\u00b7te", "war", ",", "dies", "zu", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "$,", "PDS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "ein Unterschlupf aus dunkelstem Verlangen", "tokens": ["ein", "Un\u00b7ter\u00b7schlupf", "aus", "dun\u00b7kels\u00b7tem", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "mit einem Zugang, dessen Pfosten beben, \u2013", "tokens": ["mit", "ei\u00b7nem", "Zu\u00b7gang", ",", "des\u00b7sen", "Pfos\u00b7ten", "be\u00b7ben", ",", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELAT", "NN", "VVINF", "$,", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "da schufst du ihnen Tempel im Geh\u00f6r.", "tokens": ["da", "schufst", "du", "ih\u00b7nen", "Tem\u00b7pel", "im", "Ge\u00b7h\u00f6r", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}