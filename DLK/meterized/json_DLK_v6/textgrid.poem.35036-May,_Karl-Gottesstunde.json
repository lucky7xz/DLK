{"textgrid.poem.35036": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Gottesstunde", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du rechnest nach der Zeit der Erde", "tokens": ["Du", "rech\u00b7nest", "nach", "der", "Zeit", "der", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und ahnst noch nichts von Himmelszeit.", "tokens": ["Und", "ahnst", "noch", "nichts", "von", "Him\u00b7mels\u00b7zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "PIS", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nach welcher Gott wohl rechnen werde,", "tokens": ["Nach", "wel\u00b7cher", "Gott", "wohl", "rech\u00b7nen", "wer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "ADV", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dar\u00fcber wei\u00dft du nicht Bescheid.", "tokens": ["Da\u00b7r\u00fc\u00b7ber", "wei\u00dft", "du", "nicht", "Be\u00b7scheid", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PTKNEG", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zwar hast du dem metallnen Munde", "tokens": ["Zwar", "hast", "du", "dem", "me\u00b7tall\u00b7nen", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die irdschen Zeichen eingepr\u00e4gt,", "tokens": ["Die", "ird\u00b7schen", "Zei\u00b7chen", "ein\u00b7ge\u00b7pr\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Doch h\u00f6rst du nicht die wahre Stunde,", "tokens": ["Doch", "h\u00f6rst", "du", "nicht", "die", "wah\u00b7re", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die tief in deinem Innern schl\u00e4gt.", "tokens": ["Die", "tief", "in", "dei\u00b7nem", "In\u00b7nern", "schl\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Durch deine Zeit ward dir geboren", "tokens": ["Durch", "dei\u00b7ne", "Zeit", "ward", "dir", "ge\u00b7bo\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "PPER", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des Lebens ganze, schwere Last;", "tokens": ["Des", "Le\u00b7bens", "gan\u00b7ze", ",", "schwe\u00b7re", "Last", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die wahre Zeit ging dir verloren,", "tokens": ["Die", "wah\u00b7re", "Zeit", "ging", "dir", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Weil du sie nicht begriffen hast.", "tokens": ["Weil", "du", "sie", "nicht", "be\u00b7grif\u00b7fen", "hast", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PTKNEG", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nun schmerzt dich manche, manche Wunde,", "tokens": ["Nun", "schmerzt", "dich", "man\u00b7che", ",", "man\u00b7che", "Wun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "$,", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Doch machte keine noch dich klug:", "tokens": ["Doch", "mach\u00b7te", "kei\u00b7ne", "noch", "dich", "klug", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "ADV", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Du hast vers\u00e4umt die Gottesstunde,", "tokens": ["Du", "hast", "ver\u00b7s\u00e4umt", "die", "Got\u00b7tes\u00b7stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Als sie in deinem Innern schlug.", "tokens": ["Als", "sie", "in", "dei\u00b7nem", "In\u00b7nern", "schlug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Will's Gott in seiner Gnade geben,", "tokens": ["Will's", "Gott", "in", "sei\u00b7ner", "Gna\u00b7de", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df sie dir nochmals schlagen mag,", "tokens": ["Da\u00df", "sie", "dir", "noch\u00b7mals", "schla\u00b7gen", "mag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So trittst du in ein neues Leben", "tokens": ["So", "trittst", "du", "in", "ein", "neu\u00b7es", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "An deinem ersten Himmelstag.", "tokens": ["An", "dei\u00b7nem", "ers\u00b7ten", "Him\u00b7mels\u00b7tag", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nur lausche, lausche stets der Kunde,", "tokens": ["Nur", "lau\u00b7sche", ",", "lau\u00b7sche", "stets", "der", "Kun\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "$,", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die dir sein Engel abw\u00e4rts tr\u00e4gt;", "tokens": ["Die", "dir", "sein", "En\u00b7gel", "ab\u00b7w\u00e4rts", "tr\u00e4gt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPOSAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Vers\u00e4ume nicht die Gottesstunde,", "tokens": ["Ver\u00b7s\u00e4u\u00b7me", "nicht", "die", "Got\u00b7tes\u00b7stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wenn wieder an dein Herz sie schl\u00e4gt!", "tokens": ["Wenn", "wie\u00b7der", "an", "dein", "Herz", "sie", "schl\u00e4gt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}