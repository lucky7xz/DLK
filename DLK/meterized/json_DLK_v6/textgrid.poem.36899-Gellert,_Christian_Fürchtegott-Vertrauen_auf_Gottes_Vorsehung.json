{"textgrid.poem.36899": {"metadata": {"author": {"name": "Gellert, Christian F\u00fcrchtegott", "birth": "N.A.", "death": "N.A."}, "title": "Vertrauen auf Gottes Vorsehung", "genre": "verse", "period": "N.A.", "pub_year": 1742, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf Gott, und nicht auf meinen Rat", "tokens": ["Auf", "Gott", ",", "und", "nicht", "auf", "mei\u00b7nen", "Rat"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "KON", "PTKNEG", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Will ich mein Gl\u00fccke bauen,", "tokens": ["Will", "ich", "mein", "Gl\u00fc\u00b7cke", "bau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und dem, der mich erschaffen hat,", "tokens": ["Und", "dem", ",", "der", "mich", "er\u00b7schaf\u00b7fen", "hat", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "$,", "PRELS", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit ganzer Seele trauen.", "tokens": ["Mit", "gan\u00b7zer", "See\u00b7le", "trau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Er, der die Welt", "tokens": ["Er", ",", "der", "die", "Welt"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Allm\u00e4chtig h\u00e4lt,", "tokens": ["All\u00b7m\u00e4ch\u00b7tig", "h\u00e4lt", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Wird mich in meinen Tagen", "tokens": ["Wird", "mich", "in", "mei\u00b7nen", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Als Gott und Vater tragen.", "tokens": ["Als", "Gott", "und", "Va\u00b7ter", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Er sah von aller Ewigkeit,", "tokens": ["Er", "sah", "von", "al\u00b7ler", "E\u00b7wig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie viel mir n\u00fctzen w\u00fcrde,", "tokens": ["Wie", "viel", "mir", "n\u00fct\u00b7zen", "w\u00fcr\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bestimmte meine Lebenszeit,", "tokens": ["Be\u00b7stimm\u00b7te", "mei\u00b7ne", "Le\u00b7bens\u00b7zeit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Gl\u00fcck und meine B\u00fcrde.", "tokens": ["Mein", "Gl\u00fcck", "und", "mei\u00b7ne", "B\u00fcr\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Was zagt mein Herz?", "tokens": ["Was", "zagt", "mein", "Herz", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Ist auch ein Schmerz,", "tokens": ["Ist", "auch", "ein", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Der zu des Glaubens Ehre", "tokens": ["Der", "zu", "des", "Glau\u00b7bens", "Eh\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Nicht zu besiegen w\u00e4re?", "tokens": ["Nicht", "zu", "be\u00b7sie\u00b7gen", "w\u00e4\u00b7re", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Gott kennet, was mein Herz begehrt,", "tokens": ["Gott", "ken\u00b7net", ",", "was", "mein", "Herz", "be\u00b7gehrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und h\u00e4tte, was ich bitte,", "tokens": ["Und", "h\u00e4t\u00b7te", ",", "was", "ich", "bit\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "$,", "PWS", "PPER", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mir gn\u00e4dig, eh ich's bat, gew\u00e4hrt,", "tokens": ["Mir", "gn\u00e4\u00b7dig", ",", "eh", "ich's", "bat", ",", "ge\u00b7w\u00e4hrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "ADJD", "$,", "KOUS", "PIS", "VVFIN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn's seine Weisheit litte.", "tokens": ["Wenn's", "sei\u00b7ne", "Weis\u00b7heit", "lit\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Er sorgt f\u00fcr mich", "tokens": ["Er", "sorgt", "f\u00fcr", "mich"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Stets v\u00e4terlich.", "tokens": ["Stets", "v\u00e4\u00b7ter\u00b7lich", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Nicht was ich mir ersehe,", "tokens": ["Nicht", "was", "ich", "mir", "er\u00b7se\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PWS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Sein Wille, der geschehe!", "tokens": ["Sein", "Wil\u00b7le", ",", "der", "ge\u00b7sche\u00b7he", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ist nicht ein ungest\u00f6rtes Gl\u00fcck", "tokens": ["Ist", "nicht", "ein", "un\u00b7ge\u00b7st\u00f6r\u00b7tes", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PTKNEG", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Weit schwerer oft zu tragen,", "tokens": ["Weit", "schwe\u00b7rer", "oft", "zu", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Als selbst das widrige Geschick,", "tokens": ["Als", "selbst", "das", "wid\u00b7ri\u00b7ge", "Ge\u00b7schick", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bei dessen Last wir klagen?", "tokens": ["Bei", "des\u00b7sen", "Last", "wir", "kla\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die gr\u00f6\u00dfte Not", "tokens": ["Die", "gr\u00f6\u00df\u00b7te", "Not"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Hebt doch der Tod;", "tokens": ["Hebt", "doch", "der", "Tod", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Und Ehre, Gl\u00fcck und Habe", "tokens": ["Und", "Eh\u00b7re", ",", "Gl\u00fcck", "und", "Ha\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Verl\u00e4\u00dft mich doch im Grabe.", "tokens": ["Ver\u00b7l\u00e4\u00dft", "mich", "doch", "im", "Gra\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "An dem, was wahrhaft gl\u00fccklich macht,", "tokens": ["An", "dem", ",", "was", "wahr\u00b7haft", "gl\u00fcck\u00b7lich", "macht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PRELS", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "L\u00e4\u00dft Gott es keinem fehlen;", "tokens": ["L\u00e4\u00dft", "Gott", "es", "kei\u00b7nem", "feh\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PPER", "PIS", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gesundheit, Ehre, Gl\u00fcck und Pracht", "tokens": ["Ge\u00b7sund\u00b7heit", ",", "Eh\u00b7re", ",", "Gl\u00fcck", "und", "Pracht"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sind nicht das Gl\u00fcck der Seelen.", "tokens": ["Sind", "nicht", "das", "Gl\u00fcck", "der", "See\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wer Gottes Rat", "tokens": ["Wer", "Got\u00b7tes", "Rat"], "token_info": ["word", "word", "word"], "pos": ["PWS", "NN", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Vor Augen hat,", "tokens": ["Vor", "Au\u00b7gen", "hat", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Dem wird ein gut Gewissen", "tokens": ["Dem", "wird", "ein", "gut", "Ge\u00b7wis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "ADJD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die Tr\u00fcbsal auch vers\u00fc\u00dfen.", "tokens": ["Die", "Tr\u00fcb\u00b7sal", "auch", "ver\u00b7s\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Was ist des Lebens Herrlichkeit?", "tokens": ["Was", "ist", "des", "Le\u00b7bens", "Herr\u00b7lich\u00b7keit", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie bald ist sie verschwunden!", "tokens": ["Wie", "bald", "ist", "sie", "ver\u00b7schwun\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Was ist das Leiden dieser Zeit?", "tokens": ["Was", "ist", "das", "Lei\u00b7den", "die\u00b7ser", "Zeit", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie bald ist's \u00fcberwunden!", "tokens": ["Wie", "bald", "ist's", "\u00fc\u00b7berw\u00b7un\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Hofft auf den Herrn!", "tokens": ["Hofft", "auf", "den", "Herrn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Er hilft uns gern:", "tokens": ["Er", "hilft", "uns", "gern", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Seid fr\u00f6hlich, ihr Gerechten!", "tokens": ["Seid", "fr\u00f6h\u00b7lich", ",", "ihr", "Ge\u00b7rech\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der Herr hilft seinen Knechten.", "tokens": ["Der", "Herr", "hilft", "sei\u00b7nen", "Knech\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}