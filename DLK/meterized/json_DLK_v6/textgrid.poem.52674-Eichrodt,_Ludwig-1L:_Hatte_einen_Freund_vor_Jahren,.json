{"textgrid.poem.52674": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hatte einen Freund vor Jahren,", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hatte einen Freund vor Jahren,", "tokens": ["Hat\u00b7te", "ei\u00b7nen", "Freund", "vor", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eine feste edle Seele,", "tokens": ["Ei\u00b7ne", "fes\u00b7te", "ed\u00b7le", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber in der alten Welt", "tokens": ["A\u00b7ber", "in", "der", "al\u00b7ten", "Welt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Konnt er nimmer Ruhe finden.", "tokens": ["Konnt", "er", "nim\u00b7mer", "Ru\u00b7he", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Uebers Meer ist er gefahren,", "tokens": ["Ue\u00b7bers", "Meer", "ist", "er", "ge\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zu den thatenreichen Menschen,", "tokens": ["Zu", "den", "tha\u00b7ten\u00b7rei\u00b7chen", "Men\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dr\u00fcben in Amerika,", "tokens": ["Dr\u00fc\u00b7ben", "in", "A\u00b7me\u00b7ri\u00b7ka", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "Dr\u00fcben in dem Reich der Zukunft", "tokens": ["Dr\u00fc\u00b7ben", "in", "dem", "Reich", "der", "Zu\u00b7kunft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und er folgt dem k\u00fchnen Banner,", "tokens": ["Und", "er", "folgt", "dem", "k\u00fch\u00b7nen", "Ban\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das des Nordens Heldens\u00f6hne", "tokens": ["Das", "des", "Nor\u00b7dens", "Hel\u00b7den\u00b7s\u00f6h\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Tragen unter ewgem Sieg", "tokens": ["Tra\u00b7gen", "un\u00b7ter", "ew\u00b7gem", "Sieg"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "In die Mexikanerberge.", "tokens": ["In", "die", "Me\u00b7xi\u00b7ka\u00b7ner\u00b7ber\u00b7ge", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Durch die reichen Tropenst\u00e4dte", "tokens": ["Durch", "die", "rei\u00b7chen", "Tro\u00b7pen\u00b7st\u00e4d\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wandelt er mit stolzen Schritten", "tokens": ["Wan\u00b7delt", "er", "mit", "stol\u00b7zen", "Schrit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In dem Kleid der Republick,", "tokens": ["In", "dem", "Kleid", "der", "Re\u00b7pub\u00b7lick", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und es jauchzt ihm zu die Sonne.", "tokens": ["Und", "es", "jauchzt", "ihm", "zu", "die", "Son\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Durch die hei\u00dfen Kaktusw\u00e4lder,", "tokens": ["Durch", "die", "hei\u00b7\u00dfen", "Kak\u00b7tus\u00b7w\u00e4l\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch die Schluchten thierbev\u00f6lkert,", "tokens": ["Durch", "die", "Schluch\u00b7ten", "thier\u00b7be\u00b7v\u00f6l\u00b7kert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Durch die Stromgew\u00e4sser wild", "tokens": ["Durch", "die", "Strom\u00b7ge\u00b7w\u00e4s\u00b7ser", "wild"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird der Tapfre staunend schreiten.", "tokens": ["Wird", "der", "Tapf\u00b7re", "stau\u00b7nend", "schrei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Und sein Herz wird k\u00fchner schlagen", "tokens": ["Und", "sein", "Herz", "wird", "k\u00fch\u00b7ner", "schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf den sieggewohnten M\u00e4rschen,", "tokens": ["Auf", "den", "sieg\u00b7ge\u00b7wohn\u00b7ten", "M\u00e4r\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ueber sich der Berge Stern,", "tokens": ["Ue\u00b7ber", "sich", "der", "Ber\u00b7ge", "Stern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Den beschneiten Orizawa.", "tokens": ["Den", "be\u00b7schnei\u00b7ten", "O\u00b7ri\u00b7za\u00b7wa", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.7": {"line.1": {"text": "Um und um die ewgen Berge,", "tokens": ["Um", "und", "um", "die", "ew\u00b7gen", "Ber\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "KON", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie aus gl\u00fchem Erz gegossen,", "tokens": ["Wie", "aus", "gl\u00fc\u00b7hem", "Erz", "ge\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In der tausendfarbgen Pracht", "tokens": ["In", "der", "tau\u00b7send\u00b7farb\u00b7gen", "Pracht"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird die Heimath er vergessen.", "tokens": ["Wird", "die", "Hei\u00b7math", "er", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Wenn er schaut zu beiden Seiten", "tokens": ["Wenn", "er", "schaut", "zu", "bei\u00b7den", "Sei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gleich geschmolznen Diamanten", "tokens": ["Gleich", "ge\u00b7schmolz\u00b7nen", "Di\u00b7a\u00b7man\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Den gewaltgen Ocean,", "tokens": ["Den", "ge\u00b7walt\u00b7gen", "O\u00b7cean", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Wird der Heimath er gedenken.", "tokens": ["Wird", "der", "Hei\u00b7math", "er", "ge\u00b7den\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Wenn er schaut die Sonne tauchen", "tokens": ["Wenn", "er", "schaut", "die", "Son\u00b7ne", "tau\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gro\u00df und blutig in die Esse", "tokens": ["Gro\u00df", "und", "blu\u00b7tig", "in", "die", "Es\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "KON", "ADJD", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Des unendlich weiten Meers", "tokens": ["Des", "un\u00b7end\u00b7lich", "wei\u00b7ten", "Meers"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und sein Auge Thr\u00e4nen f\u00fcllen.", "tokens": ["Und", "sein", "Au\u00b7ge", "Thr\u00e4\u00b7nen", "f\u00fcl\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Wenn die Nacht die d\u00e4mmervolle", "tokens": ["Wenn", "die", "Nacht", "die", "d\u00e4m\u00b7mer\u00b7vol\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Niedersinkt ins Thal der Blumen,", "tokens": ["Nie\u00b7der\u00b7sinkt", "ins", "Thal", "der", "Blu\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der ungeheure Mond", "tokens": ["Und", "der", "un\u00b7ge\u00b7heu\u00b7re", "Mond"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Seine blauen Lichter sendet.", "tokens": ["Sei\u00b7ne", "blau\u00b7en", "Lich\u00b7ter", "sen\u00b7det", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Wo die Bl\u00fcthenb\u00e4ume tanzen", "tokens": ["Wo", "die", "Bl\u00fc\u00b7then\u00b7b\u00e4u\u00b7me", "tan\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die Quellen aufw\u00e4rts str\u00f6men,", "tokens": ["Und", "die", "Quel\u00b7len", "auf\u00b7w\u00e4rts", "str\u00f6\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Drein die Millionenschaar", "tokens": ["Drein", "die", "Mil\u00b7li\u00b7o\u00b7nen\u00b7schaar"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "S\u00fc\u00dfer S\u00e4nger musiziret.", "tokens": ["S\u00fc\u00b7\u00dfer", "S\u00e4n\u00b7ger", "mu\u00b7si\u00b7zi\u00b7ret", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Wo ihn gr\u00fc\u00dfen andre Sterne,", "tokens": ["Wo", "ihn", "gr\u00fc\u00b7\u00dfen", "and\u00b7re", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zaubergro\u00dfe, blitzeschleudernd,", "tokens": ["Zau\u00b7ber\u00b7gro\u00b7\u00dfe", ",", "blit\u00b7ze\u00b7schleu\u00b7dernd", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Himmel golden schwarz", "tokens": ["Und", "der", "Him\u00b7mel", "gol\u00b7den", "schwarz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJD", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Seine Sinnen \u00fcberw\u00e4ltigt.", "tokens": ["Sei\u00b7ne", "Sin\u00b7nen", "\u00fc\u00b7berw\u00b7\u00e4l\u00b7tigt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Wenn er dann hinab die Th\u00e4ler", "tokens": ["Wenn", "er", "dann", "hin\u00b7ab", "die", "Th\u00e4\u00b7ler"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zu den sch\u00f6nen Menschen steiget,", "tokens": ["Zu", "den", "sch\u00f6\u00b7nen", "Men\u00b7schen", "stei\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Zu der M\u00e4dchen ewgem Tanz", "tokens": ["Zu", "der", "M\u00e4d\u00b7chen", "ew\u00b7gem", "Tanz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "In die H\u00fctten von Puebla.", "tokens": ["In", "die", "H\u00fct\u00b7ten", "von", "Pueb\u00b7la", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NE", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.14": {"line.1": {"text": "Wenn die weichen Blumenarme", "tokens": ["Wenn", "die", "wei\u00b7chen", "Blu\u00b7men\u00b7ar\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die s\u00fc\u00dfen hellen Stimmen", "tokens": ["Und", "die", "s\u00fc\u00b7\u00dfen", "hel\u00b7len", "Stim\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit dem niegetr\u00e4umten Reiz", "tokens": ["Mit", "dem", "nie\u00b7ge\u00b7tr\u00e4um\u00b7ten", "Reiz"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihn umtaumeln und umscherzen.", "tokens": ["Ihn", "um\u00b7tau\u00b7meln", "und", "um\u00b7scher\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "KON", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Wenn ihm die Gazellenaugen", "tokens": ["Wenn", "ihm", "die", "Ga\u00b7zel\u00b7len\u00b7au\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Klug und seltsam, scheu und l\u00fcstern,", "tokens": ["Klug", "und", "selt\u00b7sam", ",", "scheu", "und", "l\u00fcs\u00b7tern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dringen bis ans tiefste Herz,", "tokens": ["Drin\u00b7gen", "bis", "ans", "tiefs\u00b7te", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Schauen auf den Grund der Seele.", "tokens": ["Schau\u00b7en", "auf", "den", "Grund", "der", "See\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "Wahrlich wenn er eines Tages", "tokens": ["Wahr\u00b7lich", "wenn", "er", "ei\u00b7nes", "Ta\u00b7ges"], "token_info": ["wo