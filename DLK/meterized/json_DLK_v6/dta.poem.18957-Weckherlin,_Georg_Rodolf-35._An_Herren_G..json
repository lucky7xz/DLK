, "pos": ["VMFIN", "PPER", "$(", "PWAV", "PPER", "ADV", "NN", "VMFIN", "VVINF", "$("], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Da\u00df man es riechend nicht erfahr/", "tokens": ["Da\u00df", "man", "es", "rie\u00b7chend", "nicht", "er\u00b7fahr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "ADJD", "PTKNEG", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So solt du darauff zwibel fressen/", "tokens": ["So", "solt", "du", "dar\u00b7auff", "zwi\u00b7bel", "fres\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PAV", "ADJD", "VVINF", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "So wirt man jenes nicht gewahr.", "tokens": ["So", "wirt", "man", "je\u00b7nes", "nicht", "ge\u00b7wahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "PDS", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Knoblauch/ die zwibel zu vertragen/", "tokens": ["Knob\u00b7lauch", "/", "die", "zwi\u00b7bel", "zu", "ver\u00b7tra\u00b7gen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "ART", "ADJD", "PTKZU", "VVFIN", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Vertreibet den geruch hinweg;", "tokens": ["Ver\u00b7trei\u00b7bet", "den", "ge\u00b7ruch", "hin\u00b7weg", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Des Knoblauchs geruch zu verjagen/", "tokens": ["Des", "Knob\u00b7lauchs", "ge\u00b7ruch", "zu", "ver\u00b7ja\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", 