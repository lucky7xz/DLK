{"textgrid.poem.56087": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Der K\u00e4ferstein", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sind nicht Sterne fast in deiner N\u00e4he", "tokens": ["Sind", "nicht", "Ster\u00b7ne", "fast", "in", "dei\u00b7ner", "N\u00e4\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PTKNEG", "NN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "und was giebt es, das du nicht umspannst,", "tokens": ["und", "was", "giebt", "es", ",", "das", "du", "nicht", "um\u00b7spannst", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "PPER", "$,", "PRELS", "PPER", "PTKNEG", "ADV", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "da du dieser harten Skarab\u00e4e", "tokens": ["da", "du", "die\u00b7ser", "har\u00b7ten", "Ska\u00b7ra\u00b7b\u00e4e"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PDAT", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Karneolkern gar nicht fassen kannst", "tokens": ["Kar\u00b7neol\u00b7kern", "gar", "nicht", "fas\u00b7sen", "kannst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "PTKNEG", "VVINF", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "ohne jenen Raum, der ihre Schilder", "tokens": ["oh\u00b7ne", "je\u00b7nen", "Raum", ",", "der", "ih\u00b7re", "Schil\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "$,", "PRELS", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "niederh\u00e4lt, auf deinem ganzen Blut", "tokens": ["nie\u00b7der\u00b7h\u00e4lt", ",", "auf", "dei\u00b7nem", "gan\u00b7zen", "Blut"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "mitzutragen; niemals war er milder,", "tokens": ["mit\u00b7zu\u00b7tra\u00b7gen", ";", "nie\u00b7mals", "war", "er", "mil\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$.", "ADV", "VAFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "n\u00e4her, hingegebener. Er ruht", "tokens": ["n\u00e4\u00b7her", ",", "hin\u00b7ge\u00b7ge\u00b7be\u00b7ner", ".", "Er", "ruht"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["ADJD", "$,", "ADV", "$.", "PPER", "VVFIN"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "seit Jahrtausenden auf diesen K\u00e4fern,", "tokens": ["seit", "Jahr\u00b7tau\u00b7sen\u00b7den", "auf", "die\u00b7sen", "K\u00e4\u00b7fern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "PDAT", "NN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "wo ihn keiner braucht und unterbricht;", "tokens": ["wo", "ihn", "kei\u00b7ner", "braucht", "und", "un\u00b7ter\u00b7bricht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PIS", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "und die K\u00e4fer schlie\u00dfen sich und schl\u00e4fern", "tokens": ["und", "die", "K\u00e4\u00b7fer", "schlie\u00b7\u00dfen", "sich", "und", "schl\u00e4\u00b7fern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "PRF", "KON", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "unter seinem wiegenden Gewicht.", "tokens": ["un\u00b7ter", "sei\u00b7nem", "wie\u00b7gen\u00b7den", "Ge\u00b7wicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}