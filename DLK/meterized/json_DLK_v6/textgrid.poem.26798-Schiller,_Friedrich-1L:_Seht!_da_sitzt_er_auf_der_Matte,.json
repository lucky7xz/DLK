{"textgrid.poem.26798": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Seht! da sitzt er auf der Matte,", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seht! da sitzt er auf der Matte,", "tokens": ["Seht", "!", "da", "sitzt", "er", "auf", "der", "Mat\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aufrecht sitzt er da,", "tokens": ["Auf\u00b7recht", "sitzt", "er", "da", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Mit dem Anstand, den er hatte,", "tokens": ["Mit", "dem", "An\u00b7stand", ",", "den", "er", "hat\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPER", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Als er's Licht noch sah.", "tokens": ["Als", "er's", "Licht", "noch", "sah", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Doch wo ist die Kraft der F\u00e4uste,", "tokens": ["Doch", "wo", "ist", "die", "Kraft", "der", "F\u00e4us\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.2": {"text": "Wo des Atems Hauch,", "tokens": ["Wo", "des", "A\u00b7tems", "Hauch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Der noch j\u00fcngst zum gro\u00dfen Geiste", "tokens": ["Der", "noch", "j\u00fcngst", "zum", "gro\u00b7\u00dfen", "Geis\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADV", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Blies der Pfeife Rauch?", "tokens": ["Blies", "der", "Pfei\u00b7fe", "Rauch", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Wo die Augen, falkenhelle,", "tokens": ["Wo", "die", "Au\u00b7gen", ",", "fal\u00b7ken\u00b7hel\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die des Rentiers Spur", "tokens": ["Die", "des", "Ren\u00b7tiers", "Spur"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Z\u00e4hlten auf des Grases Welle,", "tokens": ["Z\u00e4hl\u00b7ten", "auf", "des", "Gra\u00b7ses", "Wel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf dem Tau der Flur?", "tokens": ["Auf", "dem", "Tau", "der", "Flur", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Diese Schenkel, die behender", "tokens": ["Die\u00b7se", "Schen\u00b7kel", ",", "die", "be\u00b7hen\u00b7der"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PDAT", "NN", "$,", "PRELS", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Flohen durch den Schnee", "tokens": ["Flo\u00b7hen", "durch", "den", "Schnee"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Als der Hirsch, der Zwanzigender,", "tokens": ["Als", "der", "Hirsch", ",", "der", "Zwan\u00b7zi\u00b7gen\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Als des Berges Reh?", "tokens": ["Als", "des", "Ber\u00b7ges", "Reh", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Diese Arme, die den Bogen", "tokens": ["Die\u00b7se", "Ar\u00b7me", ",", "die", "den", "Bo\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PDAT", "NN", "$,", "PRELS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Spannten streng und straff!", "tokens": ["Spann\u00b7ten", "streng", "und", "straff", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Seht, das Leben ist entflogen,", "tokens": ["Seht", ",", "das", "Le\u00b7ben", "ist", "ent\u00b7flo\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seht, sie h\u00e4ngen schlaff!", "tokens": ["Seht", ",", "sie", "h\u00e4n\u00b7gen", "schlaff", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "VVFIN", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Wohl ihm! er ist hingegangen,", "tokens": ["Wohl", "ihm", "!", "er", "ist", "hin\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$.", "PPER", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo kein Schnee mehr ist,", "tokens": ["Wo", "kein", "Schnee", "mehr", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "ADV", "VAFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Wo mit Mais die Felder prangen,", "tokens": ["Wo", "mit", "Mais", "die", "Fel\u00b7der", "pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der von selber sprie\u00dft.", "tokens": ["Der", "von", "sel\u00b7ber", "sprie\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADV", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Wo mit V\u00f6geln alle Str\u00e4uche,", "tokens": ["Wo", "mit", "V\u00f6\u00b7geln", "al\u00b7le", "Str\u00e4u\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo der Wald mit Wild,", "tokens": ["Wo", "der", "Wald", "mit", "Wild", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "NE", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Wo mit Fischen alle Teiche", "tokens": ["Wo", "mit", "Fi\u00b7schen", "al\u00b7le", "Tei\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "NN", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lustig sind gef\u00fcllt.", "tokens": ["Lus\u00b7tig", "sind", "ge\u00b7f\u00fcllt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Mit den Geistern speist er droben,", "tokens": ["Mit", "den", "Geis\u00b7tern", "speist", "er", "dro\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lie\u00df uns hier allein,", "tokens": ["Lie\u00df", "uns", "hier", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Da\u00df wir seine Taten loben", "tokens": ["Da\u00df", "wir", "sei\u00b7ne", "Ta\u00b7ten", "lo\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und ihn scharren ein.", "tokens": ["Und", "ihn", "schar\u00b7ren", "ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.9": {"line.1": {"text": "Bringet her die letzten Gaben,", "tokens": ["Brin\u00b7get", "her", "die", "letz\u00b7ten", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stimmt die Totenklag!", "tokens": ["Stimmt", "die", "To\u00b7ten\u00b7klag", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Alles sei mit ihm begraben,", "tokens": ["Al\u00b7les", "sei", "mit", "ihm", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was ihn freuen mag.", "tokens": ["Was", "ihn", "freu\u00b7en", "mag."], "token_info": ["word", "word", "word", "abbreviation"], "pos": ["PWS", "PPER", "ADJA", "NN"], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.10": {"line.1": {"text": "Legt ihm unters Haupt die Beile,", "tokens": ["Legt", "ihm", "un\u00b7ters", "Haupt", "die", "Bei\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die er tapfer schwang,", "tokens": ["Die", "er", "tap\u00b7fer", "schwang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Auch des B\u00e4ren fette Keule,", "tokens": ["Auch", "des", "B\u00e4\u00b7ren", "fet\u00b7te", "Keu\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn der Weg ist lang.", "tokens": ["Denn", "der", "Weg", "ist", "lang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.11": {"line.1": {"text": "Auch das Messer scharf geschliffen,", "tokens": ["Auch", "das", "Mes\u00b7ser", "scharf", "ge\u00b7schlif\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das vom Feindeskopf", "tokens": ["Das", "vom", "Fein\u00b7des\u00b7kopf"], "token_info": ["word", "word", "word"], "pos": ["PDS", "APPRART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Rasch mit drei geschickten Griffen", "tokens": ["Rasch", "mit", "drei", "ge\u00b7schick\u00b7ten", "Grif\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "CARD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sch\u00e4lte Haut und Schopf.", "tokens": ["Sch\u00e4l\u00b7te", "Haut", "und", "Schopf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.12": {"line.1": {"text": "Farben auch, den Leib zu malen,", "tokens": ["Far\u00b7ben", "auch", ",", "den", "Leib", "zu", "ma\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Steckt ihm in die Hand,", "tokens": ["Steckt", "ihm", "in", "die", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Da\u00df er r\u00f6tlich m\u00f6ge strahlen", "tokens": ["Da\u00df", "er", "r\u00f6t\u00b7lich", "m\u00f6\u00b7ge", "strah\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In der Seelen Land.", "tokens": ["In", "der", "See\u00b7len", "Land", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}