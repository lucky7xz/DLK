{"dta.poem.14395": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "G\u00f6tterd\u00e4mmerung .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.71", "en:0.14", "cy:0.14"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Was will ich noch hoffen?", "tokens": ["Was", "will", "ich", "noch", "hof\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "+---+-", "measure": "dactylic.init"}, "line.2": {"text": "Hinauf, ach hinauf!", "tokens": ["Hin\u00b7auf", ",", "ach", "hin\u00b7auf", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Der Himmel ist offen,", "tokens": ["Der", "Him\u00b7mel", "ist", "of\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Nimm, Vater, mich auf!", "tokens": ["Nimm", ",", "Va\u00b7ter", ",", "mich", "auf", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "PPER", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}