{"textgrid.poem.39030": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wieder durchwandl' ich", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wieder durchwandl' ich", "tokens": ["Wie\u00b7der", "durch\u00b7wandl'", "ich"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "In fr\u00fcher Morgenk\u00fchle", "tokens": ["In", "fr\u00fc\u00b7her", "Mor\u00b7gen\u00b7k\u00fch\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Den Berg, und klettre hinauf und ab,", "tokens": ["Den", "Berg", ",", "und", "klett\u00b7re", "hin\u00b7auf", "und", "ab", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ganz den Segen f\u00fchlend der Natur.", "tokens": ["Ganz", "den", "Se\u00b7gen", "f\u00fch\u00b7lend", "der", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVPP", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Da t\u00f6nt von oben,", "tokens": ["Da", "t\u00f6nt", "von", "o\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADV", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Seltsamen Klanges", "tokens": ["Selt\u00b7sa\u00b7men", "Klan\u00b7ges"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Das Lied einer Hirtenpfeife,", "tokens": ["Das", "Lied", "ei\u00b7ner", "Hir\u00b7ten\u00b7pfei\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Und alsbald seh ich in Spr\u00fcngen", "tokens": ["Und", "als\u00b7bald", "seh", "ich", "in", "Spr\u00fcn\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Nach dem Takte tanzend", "tokens": ["Nach", "dem", "Tak\u00b7te", "tan\u00b7zend"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVPP"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.10": {"text": "Die muntre Ziegenheerde", "tokens": ["Die", "mun\u00b7tre", "Zie\u00b7gen\u00b7heer\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Von der Felsentreppe niedergaukeln,", "tokens": ["Von", "der", "Fel\u00b7sen\u00b7trep\u00b7pe", "nie\u00b7der\u00b7gau\u00b7keln", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Mit klugem Aug' und feinen Fu\u00df", "tokens": ["Mit", "klu\u00b7gem", "Aug'", "und", "fei\u00b7nen", "Fu\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Die Spr\u00fcnge sicher messend.", "tokens": ["Die", "Spr\u00fcn\u00b7ge", "si\u00b7cher", "mes\u00b7send", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Der F\u00fchrer der Schaar", "tokens": ["Der", "F\u00fch\u00b7rer", "der", "Schaar"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.15": {"text": "Ein brauner, kleiner Knabe,", "tokens": ["Ein", "brau\u00b7ner", ",", "klei\u00b7ner", "Kna\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Musizirt ernst mit voller Kraft", "tokens": ["Mu\u00b7si\u00b7zirt", "ernst", "mit", "vol\u00b7ler", "Kraft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "APPR", "ADJA", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.17": {"text": "Und freut sich seiner Scholaren.", "tokens": ["Und", "freut", "sich", "sei\u00b7ner", "Scho\u00b7la\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "PPOSAT", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.18": {"text": "Doch wie er nieder h\u00fcpft", "tokens": ["Doch", "wie", "er", "nie\u00b7der", "h\u00fcpft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "PTKVZ", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.19": {"text": "Und den Fremden gewahrt", "tokens": ["Und", "den", "Frem\u00b7den", "ge\u00b7wahrt"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVPP"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.20": {"text": "Steckt er alsbald", "tokens": ["Steckt", "er", "als\u00b7bald"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.21": {"text": "Sein Lied abbrechend", "tokens": ["Sein", "Lied", "ab\u00b7bre\u00b7chend"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VVPP"], "meter": "-+-+-", "measure": "iambic.di"}, "line.22": {"text": "Die Fl\u00f6te schnell und scheu in die Hirtentasche.", "tokens": ["Die", "Fl\u00f6\u00b7te", "schnell", "und", "scheu", "in", "die", "Hir\u00b7ten\u00b7ta\u00b7sche", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.23": {"text": "Ich red' ihn an, und err\u00f6thend", "tokens": ["Ich", "red'", "ihn", "an", ",", "und", "er\u00b7r\u00f6\u00b7thend"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "VVPP"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.24": {"text": "L\u00fcftet er den Hut und blondes Haar", "tokens": ["L\u00fcf\u00b7tet", "er", "den", "Hut", "und", "blon\u00b7des", "Haar"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "KON", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.25": {"text": "Rollt sich um die braunen Wangen,", "tokens": ["Rollt", "sich", "um", "die", "brau\u00b7nen", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Er athmet schwer und blickt von der Seite scheu.", "tokens": ["Er", "ath\u00b7met", "schwer", "und", "blickt", "von", "der", "Sei\u00b7te", "scheu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "VVFIN", "APPR", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.27": {"text": "Zeige mir, bitt' ich, die Pfeife,", "tokens": ["Zei\u00b7ge", "mir", ",", "bitt'", "ich", ",", "die", "Pfei\u00b7fe", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "VVFIN", "PPER", "$,", "ART", "NN", "$,"], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.28": {"text": "Die ich noch nie von dieser Form gesehn,", "tokens": ["Die", "ich", "noch", "nie", "von", "die\u00b7ser", "Form", "ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "APPR", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "So wie ich auch noch nie", "tokens": ["So", "wie", "ich", "auch", "noch", "nie"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "PPER", "ADV", "ADV", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.30": {"text": "So wunderlichen Ton vernommen.", "tokens": ["So", "wun\u00b7der\u00b7li\u00b7chen", "Ton", "ver\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Er h\u00e4lt mit beiden H\u00e4nden fest", "tokens": ["Er", "h\u00e4lt", "mit", "bei\u00b7den", "H\u00e4n\u00b7den", "fest"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Die Hintertasche geschlossen", "tokens": ["Die", "Hin\u00b7ter\u00b7ta\u00b7sche", "ge\u00b7schlos\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVPP"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.33": {"text": "Und ruft mir ein dreistes Nein entgegen.", "tokens": ["Und", "ruft", "mir", "ein", "dreis\u00b7tes", "Nein", "ent\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.34": {"text": "Was ich \u00fcberrede und schmeichle,", "tokens": ["Was", "ich", "\u00fc\u00b7berr\u00b7e\u00b7de", "und", "schmeich\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.35": {"text": "Alles vergebens,", "tokens": ["Al\u00b7les", "ver\u00b7ge\u00b7bens", ","], "token_info": ["word", "word", "punct"], "pos": ["PIS", "ADV", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.36": {"text": "Der Kleine beharrt auf seinem Eigensinn,", "tokens": ["Der", "Klei\u00b7ne", "be\u00b7harrt", "auf", "sei\u00b7nem", "Ei\u00b7gen\u00b7sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.37": {"text": "Mein Geschenk verweigert er fest,", "tokens": ["Mein", "Ge\u00b7schenk", "ver\u00b7wei\u00b7gert", "er", "fest", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.38": {"text": "Und steht auf dem Sprung", "tokens": ["Und", "steht", "auf", "dem", "Sprung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.39": {"text": "Seinen Ziegen zu folgen,", "tokens": ["Sei\u00b7nen", "Zie\u00b7gen", "zu", "fol\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.40": {"text": "Die von den n\u00e4chsten Klippen", "tokens": ["Die", "von", "den", "n\u00e4chs\u00b7ten", "Klip\u00b7pen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.41": {"text": "Fragend zu ihm her\u00fcbersehn. \u2013", "tokens": ["Fra\u00b7gend", "zu", "ihm", "her\u00b7\u00fc\u00b7ber\u00b7sehn", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "APPR", "PPER", "VVINF", "$.", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.42": {"text": "\u00bbso sage mir mindestens,", "tokens": ["\u00bb", "so", "sa\u00b7ge", "mir", "min\u00b7des\u00b7tens", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.43": {"text": "Warum ich die Pfeife nicht betrachten darf?\u00ab \u2013", "tokens": ["Wa\u00b7rum", "ich", "die", "Pfei\u00b7fe", "nicht", "be\u00b7trach\u00b7ten", "darf", "?", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "PTKNEG", "VVINF", "VMFIN", "$.", "$(", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.44": {"text": "Und er mit gro\u00dfen Augen:", "tokens": ["Und", "er", "mit", "gro\u00b7\u00dfen", "Au\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.45": {"text": "Wer eine solche Fl\u00f6te,", "tokens": ["Wer", "ei\u00b7ne", "sol\u00b7che", "Fl\u00f6\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.46": {"text": "So sch\u00f6n und herrlich,", "tokens": ["So", "sch\u00f6n", "und", "herr\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.47": {"text": "Einmal in H\u00e4nden hat,", "tokens": ["Ein\u00b7mal", "in", "H\u00e4n\u00b7den", "hat", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VAFIN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.48": {"text": "Giebt sie niemals zur\u00fcck. \u2013", "tokens": ["Giebt", "sie", "nie\u00b7mals", "zu\u00b7r\u00fcck", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKVZ", "$.", "$("], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.49": {"text": "Mit dem scheuen Worte", "tokens": ["Mit", "dem", "scheu\u00b7en", "Wor\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.50": {"text": "Rennt er \u00fcber die Steine fort,", "tokens": ["Rennt", "er", "\u00fc\u00b7ber", "die", "Stei\u00b7ne", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.51": {"text": "Und erst in der Ferne", "tokens": ["Und", "erst", "in", "der", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.52": {"text": "Tief unten im Thal", "tokens": ["Tief", "un\u00b7ten", "im", "Thal"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "ADV", "APPRART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.53": {"text": "Erklingt sein muntres Morgenlied von neuem.", "tokens": ["Er\u00b7klingt", "sein", "mun\u00b7tres", "Mor\u00b7gen\u00b7lied", "von", "neu\u00b7em", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "APPR", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}