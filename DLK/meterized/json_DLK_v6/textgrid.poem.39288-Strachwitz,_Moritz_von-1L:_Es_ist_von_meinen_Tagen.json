{"textgrid.poem.39288": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es ist von meinen Tagen", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist von meinen Tagen", "tokens": ["Es", "ist", "von", "mei\u00b7nen", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Noch kurz der junge Zug,", "tokens": ["Noch", "kurz", "der", "jun\u00b7ge", "Zug", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch hab' ich drin ertragen", "tokens": ["Doch", "hab'", "ich", "drin", "er\u00b7tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der schweren Leiden genug.", "tokens": ["Der", "schwe\u00b7ren", "Lei\u00b7den", "ge\u00b7nug", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Ich darf manch lange Stunde", "tokens": ["Ich", "darf", "manch", "lan\u00b7ge", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nicht schaun dein Angesicht,", "tokens": ["Nicht", "schaun", "dein", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich darf an deinem Munde", "tokens": ["Ich", "darf", "an", "dei\u00b7nem", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ach! ewig hangen nicht.", "tokens": ["Ach", "!", "e\u00b7wig", "han\u00b7gen", "nicht", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADJD", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Das ist mein schwerstes Tragen", "tokens": ["Das", "ist", "mein", "schwers\u00b7tes", "Tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und bricht mein Herz entzwei,", "tokens": ["Und", "bricht", "mein", "Herz", "ent\u00b7zwei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dir aber ist mein Klagen", "tokens": ["Dir", "a\u00b7ber", "ist", "mein", "Kla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ach! ewig einerlei.", "tokens": ["Ach", "!", "e\u00b7wig", "ei\u00b7ner\u00b7lei", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}