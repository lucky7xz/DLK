{"textgrid.poem.59595": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf! Die Schwerter hell heraus!", "tokens": ["Auf", "!", "Die", "Schwer\u00b7ter", "hell", "he\u00b7raus", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die Herzen froh gehoben!", "tokens": ["Und", "die", "Her\u00b7zen", "froh", "ge\u00b7ho\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Noch steht Gottes Himmelshaus,", "tokens": ["Noch", "steht", "Got\u00b7tes", "Him\u00b7mels\u00b7haus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Noch schwebt Gottes Rechte oben,", "tokens": ["Noch", "schwebt", "Got\u00b7tes", "Rech\u00b7te", "o\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "NN", "ADV", "$,"], "meter": "-++-+-+-", "measure": "unknown.measure.tetra"}, "line.5": {"text": "Noch h\u00e4lt Gott das Weltgericht.", "tokens": ["Noch", "h\u00e4lt", "Gott", "das", "Welt\u00b7ge\u00b7richt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Gott ist unsre Zuversicht.", "tokens": ["Gott", "ist", "uns\u00b7re", "Zu\u00b7ver\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "La\u00dft die Fahnen lustig wehn!", "tokens": ["La\u00dft", "die", "Fah\u00b7nen", "lus\u00b7tig", "wehn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "La\u00dft die Trommeln mutig klingen!", "tokens": ["La\u00dft", "die", "Trom\u00b7meln", "mu\u00b7tig", "klin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gott der Herr wird mit uns stehn", "tokens": ["Gott", "der", "Herr", "wird", "mit", "uns", "stehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "VAFIN", "APPR", "PPER", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und den Blitz der Rache schwingen,", "tokens": ["Und", "den", "Blitz", "der", "Ra\u00b7che", "schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Gott verl\u00e4\u00dft die Guten nicht.", "tokens": ["Gott", "ver\u00b7l\u00e4\u00dft", "die", "Gu\u00b7ten", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Gott ist unsre Zuversicht.", "tokens": ["Gott", "ist", "uns\u00b7re", "Zu\u00b7ver\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Tobe nur, du H\u00f6llenheer!", "tokens": ["To\u00b7be", "nur", ",", "du", "H\u00f6l\u00b7len\u00b7heer", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "PPER", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "W\u00fctet, mordet nur, Tyrannen!", "tokens": ["W\u00fc\u00b7tet", ",", "mor\u00b7det", "nur", ",", "Ty\u00b7ran\u00b7nen", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "ADV", "$,", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gott verweht wie Sand am Meer", "tokens": ["Gott", "ver\u00b7weht", "wie", "Sand", "am", "Meer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "KOKOM", "NN", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Lug und Trug und Schande dannen.", "tokens": ["Lug", "und", "Trug", "und", "Schan\u00b7de", "dan\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "KON", "NN", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Gott bestraft den B\u00f6sewicht.", "tokens": ["Gott", "be\u00b7straft", "den", "B\u00f6\u00b7se\u00b7wicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Gott ist unsre Zuversicht.", "tokens": ["Gott", "ist", "uns\u00b7re", "Zu\u00b7ver\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Auf! Mit Gott zum Heldenstreit!", "tokens": ["Auf", "!", "Mit", "Gott", "zum", "Hel\u00b7den\u00b7streit", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "APPR", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf f\u00fcr Freiheit, Recht und Ehre,", "tokens": ["Auf", "f\u00fcr", "Frei\u00b7heit", ",", "Recht", "und", "Eh\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df sich deutsche Redlichkeit,", "tokens": ["Da\u00df", "sich", "deut\u00b7sche", "Red\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df sich deutsche Treue mehre!", "tokens": ["Da\u00df", "sich", "deut\u00b7sche", "Treu\u00b7e", "meh\u00b7re", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Gott, der Tyrannei zerbricht,", "tokens": ["Gott", ",", "der", "Ty\u00b7ran\u00b7nei", "zer\u00b7bricht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Gott ist unsre Zuversicht.", "tokens": ["Gott", "ist", "uns\u00b7re", "Zu\u00b7ver\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Klingt denn, Trommeln! Fahnen weht!", "tokens": ["Klingt", "denn", ",", "Trom\u00b7meln", "!", "Fah\u00b7nen", "weht", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "NN", "$.", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Herzen weht in lichten Flammen!", "tokens": ["Her\u00b7zen", "weht", "in", "lich\u00b7ten", "Flam\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "F\u00fcr der Freiheit Majest\u00e4t,", "tokens": ["F\u00fcr", "der", "Frei\u00b7heit", "Ma\u00b7jes\u00b7t\u00e4t", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fcr das Vaterland zusammen!", "tokens": ["F\u00fcr", "das", "Va\u00b7ter\u00b7land", "zu\u00b7sam\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Frisch hinein, und zaget nicht!", "tokens": ["Frisch", "hin\u00b7ein", ",", "und", "za\u00b7get", "nicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$,", "KON", "VVFIN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Gott ist unsre Zuversicht.", "tokens": ["Gott", "ist", "uns\u00b7re", "Zu\u00b7ver\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}