{"dta.poem.3337": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "An Herrn Hofrath B   Siehe Herrn von Hagedorn Fabeln und Erzehlungen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1755", "urn": "urn:nbn:de:kobv:b4-20614-6", "language": ["de:0.99"], "booktitle": "Uz, Johann Peter: Lyrische und andere Gedichte. 2. Aufl. Ansbach, 1755."}, "poem": {"stanza.1": {"line.1": {"text": "Wie? B\u00fcrger einer Stadt sind Feinde?", "tokens": ["Wie", "?", "B\u00fcr\u00b7ger", "ei\u00b7ner", "Stadt", "sind", "Fein\u00b7de", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "NN", "ART", "NN", "VAFIN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Anstatt gesellig und als Freunde", "tokens": ["An\u00b7statt", "ge\u00b7sel\u00b7lig", "und", "als", "Freun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "KON", "KOUS", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Bey Scherz und frohem Wein zu gl\u00fchn;", "tokens": ["Bey", "Scherz", "und", "fro\u00b7hem", "Wein", "zu", "gl\u00fchn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Seh ich sie voreinander fliehn?", "tokens": ["Seh", "ich", "sie", "vor\u00b7ein\u00b7an\u00b7der", "fliehn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und eh sie einen Ku\u00df auf holden Lippen wagen,", "tokens": ["Und", "eh", "sie", "ei\u00b7nen", "Ku\u00df", "auf", "hol\u00b7den", "Lip\u00b7pen", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ART", "NN", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Erst \u00e4ngstlich fragen,", "tokens": ["Erst", "\u00e4ngst\u00b7lich", "fra\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Von welch politischer Parthey,", "tokens": ["Von", "welch", "po\u00b7li\u00b7ti\u00b7scher", "Par\u00b7they", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "ADJA", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Der Torris oder Whigs, ein artig M\u00e4dchen sey,", "tokens": ["Der", "Tor\u00b7ris", "o\u00b7der", "Whigs", ",", "ein", "ar\u00b7tig", "M\u00e4d\u00b7chen", "sey", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "KON", "NN", "$,", "ART", "ADJD", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Das oft nicht weis, was beyde klagen?", "tokens": ["Das", "oft", "nicht", "weis", ",", "was", "bey\u00b7de", "kla\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "PTKNEG", "PTKVZ", "$,", "PWS", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Jhr B\u00fcrger! welche Wuth hat euer Hirn verbrannt?", "tokens": ["Ihr", "B\u00fcr\u00b7ger", "!", "wel\u00b7che", "Wuth", "hat", "eu\u00b7er", "Hirn", "ver\u00b7brannt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "PWAT", "NN", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die Staatskunst sey euch unbekannt!", "tokens": ["Die", "Staats\u00b7kunst", "sey", "euch", "un\u00b7be\u00b7kannt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Trinkt euern Wein in Ruh, und schlaft bey euern Wei-", "tokens": ["Trinkt", "eu\u00b7ern", "Wein", "in", "Ruh", ",", "und", "schlaft", "bey", "eu\u00b7ern", "Wei"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "NN", "$,", "KON", "ADJD", "APPR", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "bern,", "tokens": ["bern", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "-", "measure": "single.down"}, "line.14": {"text": "So nutzt ihr doch dem Vaterland,", "tokens": ["So", "nutzt", "ihr", "doch", "dem", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Und wenigstens mit euern Leibern.", "tokens": ["Und", "we\u00b7nigs\u00b7tens", "mit", "eu\u00b7ern", "Lei\u00b7bern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Ich, der in kurzem scheiden mu\u00df,", "tokens": ["Ich", ",", "der", "in", "kur\u00b7zem", "schei\u00b7den", "mu\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "APPR", "ADJA", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Will meinen v\u00e4terlichen Segen", "tokens": ["Will", "mei\u00b7nen", "v\u00e4\u00b7ter\u00b7li\u00b7chen", "Se\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Auf dich, unruhig R\u00f6mhild! legen:", "tokens": ["Auf", "dich", ",", "un\u00b7ru\u00b7hig", "R\u00f6m\u00b7hild", "!", "le\u00b7gen", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPER", "$,", "ADJD", "NN", "$.", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.19": {"text": "Es fehle nie an Wein! Ly\u00e4ens Ueberflu\u00df", "tokens": ["Es", "feh\u00b7le", "nie", "an", "Wein", "!", "Ly\u00b7\u00e4\u00b7ens", "Ue\u00b7berf\u00b7lu\u00df"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN", "$.", "NE", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Entferne Zwietracht und Verdru\u00df,", "tokens": ["Ent\u00b7fer\u00b7ne", "Zwiet\u00b7racht", "und", "Ver\u00b7dru\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Die stets bey schlechtem Bier sich regen!", "tokens": ["Die", "stets", "bey", "schlech\u00b7tem", "Bier", "sich", "re\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ADJA", "NN", "PRF", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Der J\u00fcngling schmachte nicht umsonst um Wein und", "tokens": ["Der", "J\u00fcng\u00b7ling", "schmach\u00b7te", "nicht", "um\u00b7sonst", "um", "Wein", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "ADV", "APPR", "NN", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Ku\u00df,", "tokens": ["Ku\u00df", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.24": {"text": "Und sterbe keiner Spr\u00f6den wegen!", "tokens": ["Und", "ster\u00b7be", "kei\u00b7ner", "Spr\u00f6\u00b7den", "we\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "APPR", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}