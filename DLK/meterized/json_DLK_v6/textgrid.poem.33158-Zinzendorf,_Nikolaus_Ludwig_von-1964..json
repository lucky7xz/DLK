{"textgrid.poem.33158": {"metadata": {"author": {"name": "Zinzendorf, Nikolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "1964.", "genre": "verse", "period": "N.A.", "pub_year": 1730, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So la\u00df uns zum gl\u00fck den blutigen blik sich itzo verneun, da wir uns der seligen gnadenwahl freun.", "tokens": ["So", "la\u00df", "uns", "zum", "gl\u00fck", "den", "blu\u00b7ti\u00b7gen", "blik", "sich", "it\u00b7zo", "ver\u00b7neun", ",", "da", "wir", "uns", "der", "se\u00b7li\u00b7gen", "gna\u00b7den\u00b7wahl", "freun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "APPRART", "NN", "ART", "ADJA", "NN", "PRF", "ADV", "VVINF", "$,", "KOUS", "PPER", "PRF", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+-+--+-+--+-+-+---+--+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.2": {"line.1": {"text": "Du tratest uns nah; wir sahen dich da in der positur, als dir dort am creuze der athen entfuhr.", "tokens": ["Du", "tra\u00b7test", "uns", "nah", ";", "wir", "sa\u00b7hen", "dich", "da", "in", "der", "po\u00b7si\u00b7tur", ",", "als", "dir", "dort", "am", "creu\u00b7ze", "der", "at\u00b7hen", "ent\u00b7fuhr", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$.", "PPER", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,", "KOUS", "PPER", "ADV", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+-+-+-+-+--+-+--+--+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.3": {"line.1": {"text": "So rieff uns dein mund zum seligen bund, in deiner Gemein recht selige Seminaristen zu seyn.", "tokens": ["So", "rieff", "uns", "dein", "mund", "zum", "se\u00b7li\u00b7gen", "bund", ",", "in", "dei\u00b7ner", "Ge\u00b7mein", "recht", "se\u00b7li\u00b7ge", "Se\u00b7mi\u00b7na\u00b7ris\u00b7ten", "zu", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "$,", "APPR", "PPOSAT", "NN", "ADJD", "ADJA", "NN", "PTKZU", "VAINF", "$."], "meter": "-+--+-+--+-+--+-+--+--+--+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.4": {"line.1": {"text": "Nun w\u00e4ren wir gern dem leidenden Herrn zur ehre und zier in seinem bebluteten creuzes-revier.", "tokens": ["Nun", "w\u00e4\u00b7ren", "wir", "gern", "dem", "lei\u00b7den\u00b7den", "Herrn", "zur", "eh\u00b7re", "und", "zier", "in", "sei\u00b7nem", "be\u00b7blu\u00b7te\u00b7ten", "creu\u00b7zes\u00b7re\u00b7vier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "ADJA", "NN", "APPRART", "VVFIN", "KON", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+-+--+-+--+-+--+--+--+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.5": {"line.1": {"text": "Wir f\u00fchlen es ja, du bist uns so nah nach menschlicher art, als nie kein gesch\u00f6pfe dem anderen ward.", "tokens": ["Wir", "f\u00fch\u00b7len", "es", "ja", ",", "du", "bist", "uns", "so", "nah", "nach", "menschli\u00b7cher", "art", ",", "als", "nie", "kein", "ge\u00b7sch\u00f6p\u00b7fe", "dem", "an\u00b7de\u00b7ren", "ward", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "PPER", "VAFIN", "PPER", "ADV", "ADJD", "APPR", "ADJA", "NN", "$,", "KOUS", "ADV", "PIAT", "NN", "ART", "ADJA", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-+-+--+--+---", "measure": "iambic.octa.plus"}}, "stanza.6": {"line.1": {"text": "Wir habens erkant, ihn Vater genant; wir haben gesp\u00fcrt, da\u00df er unsre trauung mit dir celebrirt.", "tokens": ["Wir", "ha\u00b7bens", "er\u00b7kant", ",", "ihn", "Va\u00b7ter", "ge\u00b7nant", ";", "wir", "ha\u00b7ben", "ge\u00b7sp\u00fcrt", ",", "da\u00df", "er", "uns\u00b7re", "trau\u00b7ung", "mit", "dir", "ce\u00b7le\u00b7brirt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "PPER", "NN", "VVPP", "$.", "PPER", "VAFIN", "VVPP", "$,", "KOUS", "PPER", "PPOSAT", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+--+-+--+-+--+--+-+-+-+-+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.7": {"line.1": {"text": "Verg\u00f6nne uns nur, weil ", "tokens": ["Ver\u00b7g\u00f6n\u00b7ne", "uns", "nur", ",", "weil"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["VVFIN", "PPER", "ADV", "$,", "KOUS"], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}