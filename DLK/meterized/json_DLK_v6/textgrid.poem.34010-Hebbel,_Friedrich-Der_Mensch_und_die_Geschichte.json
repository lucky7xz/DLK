{"textgrid.poem.34010": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Mensch und die Geschichte", "genre": "verse", "period": "N.A.", "pub_year": 1841, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Weltgeschichte sucht aus spr\u00f6den Stoffen", "tokens": ["Die", "Welt\u00b7ge\u00b7schich\u00b7te", "sucht", "aus", "spr\u00f6\u00b7den", "Stof\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein reines Bild der Menschheit zu gestalten,", "tokens": ["Ein", "rei\u00b7nes", "Bild", "der", "Menschheit", "zu", "ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Vor dem, die jetzt sich schrankenlos entfalten,", "tokens": ["Vor", "dem", ",", "die", "jetzt", "sich", "schran\u00b7ken\u00b7los", "ent\u00b7fal\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PRELS", "ADV", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Individuen vergeh'n, die schroffen.", "tokens": ["Die", "In\u00b7di\u00b7vi\u00b7du\u00b7en", "ver\u00b7geh'n", ",", "die", "schrof\u00b7fen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PRELS", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die endliche Vollendung ist zu hoffen,", "tokens": ["Die", "end\u00b7li\u00b7che", "Vol\u00b7len\u00b7dung", "ist", "zu", "hof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PTKZU", "VVINF", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Denn diese K\u00fcnstlerin wird nie erkalten,", "tokens": ["Denn", "die\u00b7se", "K\u00fcnst\u00b7le\u00b7rin", "wird", "nie", "er\u00b7kal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VAFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Auch sehen wir, wenn sich die Nebel spalten,", "tokens": ["Auch", "se\u00b7hen", "wir", ",", "wenn", "sich", "die", "Ne\u00b7bel", "spal\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Schon manchen Zug des Bildes tief getroffen.", "tokens": ["Schon", "man\u00b7chen", "Zug", "des", "Bil\u00b7des", "tief", "ge\u00b7trof\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch wir, wie Kinder in der Werkstatt harrend,", "tokens": ["Doch", "wir", ",", "wie", "Kin\u00b7der", "in", "der", "Werk\u00b7statt", "har\u00b7rend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PWAV", "NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wir haschen nach den abgesprung'nen St\u00fccken,", "tokens": ["Wir", "ha\u00b7schen", "nach", "den", "ab\u00b7ge\u00b7sprung'\u00b7nen", "St\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die, wie sie schweigend mei\u00dfelt, nieder fallen;", "tokens": ["Die", ",", "wie", "sie", "schwei\u00b7gend", "mei\u00b7\u00dfelt", ",", "nie\u00b7der", "fal\u00b7len", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "$,", "PWAV", "PPER", "ADJD", "VVFIN", "$,", "PTKVZ", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Dann rufen wir, in Andacht dumpf erstarrend,", "tokens": ["Dann", "ru\u00b7fen", "wir", ",", "in", "An\u00b7dacht", "dumpf", "er\u00b7star\u00b7rend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "APPR", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit krummen Nacken und gebeugten R\u00fccken:", "tokens": ["Mit", "krum\u00b7men", "Na\u00b7cken", "und", "ge\u00b7beug\u00b7ten", "R\u00fc\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hier sind die G\u00f6tter! La\u00dft den Weihrauch wallen!", "tokens": ["Hier", "sind", "die", "G\u00f6t\u00b7ter", "!", "La\u00dft", "den", "Weih\u00b7rauch", "wal\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$.", "VVIMP", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}