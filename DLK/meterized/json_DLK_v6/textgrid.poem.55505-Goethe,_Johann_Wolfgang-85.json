{"textgrid.poem.55505": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "85", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn ihr's habt und wenn ihr's wi\u00dft,", "tokens": ["Wenn", "ih\u00b7r's", "habt", "und", "wenn", "ih\u00b7r's", "wi\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "KON", "KOUS", "PPER", "VVFIN", "$,"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Wi\u00dft ihr denn, wer es vermi\u00dft?", "tokens": ["Wi\u00dft", "ihr", "denn", ",", "wer", "es", "ver\u00b7mi\u00dft", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Bleibet eurem Sinne treu,", "tokens": ["Blei\u00b7bet", "eu\u00b7rem", "Sin\u00b7ne", "treu", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Neu ist alt und alt ist neu.", "tokens": ["Neu", "ist", "alt", "und", "alt", "ist", "neu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ADJD", "KON", "ADJD", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}