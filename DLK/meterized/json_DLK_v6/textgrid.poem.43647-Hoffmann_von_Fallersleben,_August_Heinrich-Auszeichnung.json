"stanza.2": {"line.1": {"text": "Dann lockt der Hahn von ferne,", "tokens": ["Dann", "lockt", "der", "Hahn", "von", "fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Schreit tack tack ta, tacke tacke ta,", "tokens": ["Schreit", "tack", "tack", "ta", ",", "ta\u00b7cke", "ta\u00b7cke", "ta", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$,", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Dann lockt der Hahn von ferne,", "tokens": ["Dann", "lockt", "der", "Hahn", "von", "fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und Alle sind sie da,", "tokens": ["Und", "Al\u00b7le", "sind", "sie", "da", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "tack ta,", "tokens": ["tack", "ta", ","], "token_info": ["word", "word", "punct"], "pos": ["FM.la", "FM.la", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "Und Alle sind sie da.", "tokens": ["Und", "Al\u00b7le", "sind", "sie", "da", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und wenn sie Alle schreien", "tokens": ["Und", "wenn", "sie", "Al\u00b7le", "schrei\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "PIS", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihr tuck tuck tick, ticke ticke tick,", "tokens": ["Ihr", "tuck", "tuck", "tick", ",", "ti\u00b7cke", "ti\u00b7cke", "tick", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NE", "NE", "$,", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "So kriegt doch nur mein H\u00fchnchen", "tokens": ["So", "kriegt", "doch", "nur", "mein", "H\u00fchn\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das allergr\u00f6\u00dfte St\u00fcck,", "tokens": ["Das", "al\u00b7ler\u00b7gr\u00f6\u00df\u00b7te", "St\u00fcck", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "tick tick,", "tokens": ["tick", "tick", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "Das allergr\u00f6\u00dfte St\u00fcck.", "tokens": ["Das", "al\u00b7ler\u00b7gr\u00f6\u00df\u00b7te", "St\u00fcck", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}