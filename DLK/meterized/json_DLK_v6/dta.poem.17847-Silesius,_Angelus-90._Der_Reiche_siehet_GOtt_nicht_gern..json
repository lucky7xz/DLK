{"dta.poem.17847": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "90.  Der Reiche siehet GOtt nicht gern.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Der Arme Christ ist GOtt: doch sieht des reichen Hau\u00df", "tokens": ["Der", "Ar\u00b7me", "Christ", "ist", "Gott", ":", "doch", "sieht", "des", "rei\u00b7chen", "Hau\u00df"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VAFIN", "NN", "$.", "ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gemeiniglich nicht gern den GOtt geyn ein und au\u00df.", "tokens": ["Ge\u00b7mei\u00b7nig\u00b7lich", "nicht", "gern", "den", "Gott", "geyn", "ein", "und", "au\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ADV", "ART", "NN", "ADJD", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}