{"textgrid.poem.40983": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was in stiller Mitternacht,", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was in stiller Mitternacht,", "tokens": ["Was", "in", "stil\u00b7ler", "Mit\u00b7ter\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn die Erde ringsum schlief,", "tokens": ["Wenn", "die", "Er\u00b7de", "ring\u00b7sum", "schlief", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mir oft aus dem Herzen tief", "tokens": ["Mir", "oft", "aus", "dem", "Her\u00b7zen", "tief"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "APPR", "ART", "NN", "ADJD"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Lieder hat hervorgebracht,", "tokens": ["Lie\u00b7der", "hat", "her\u00b7vor\u00b7ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "War des Lebens Schwere nur,", "tokens": ["War", "des", "Le\u00b7bens", "Schwe\u00b7re", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Die mir oft am Herzen zieht,", "tokens": ["Die", "mir", "oft", "am", "Her\u00b7zen", "zieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wie's Gewicht zieht an der Uhr,", "tokens": ["Wie's", "Ge\u00b7wicht", "zieht", "an", "der", "Uhr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Bis sie fl\u00f6tet laut ein Lied.", "tokens": ["Bis", "sie", "fl\u00f6\u00b7tet", "laut", "ein", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}