{"textgrid.poem.38903": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Erwacht ihr Melodieen", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Erwacht ihr Melodieen", "tokens": ["Er\u00b7wacht", "ihr", "Me\u00b7lo\u00b7di\u00b7e\u00b7en"], "token_info": ["word", "word", "word"], "pos": ["NN", "PPOSAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und tanzt auf den Saiten dahin!", "tokens": ["Und", "tanzt", "auf", "den", "Sai\u00b7ten", "da\u00b7hin", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "PAV", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Ha! meine Augen gl\u00fchen,", "tokens": ["Ha", "!", "mei\u00b7ne", "Au\u00b7gen", "gl\u00fc\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Alle Sorgen erdw\u00e4rts fliehen,", "tokens": ["Al\u00b7le", "Sor\u00b7gen", "erd\u00b7w\u00e4rts", "flie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Himmelw\u00e4rts entflattert der jauchzende Sinn.", "tokens": ["Him\u00b7mel\u00b7w\u00e4rts", "ent\u00b7flat\u00b7tert", "der", "jauch\u00b7zen\u00b7de", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+---+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "In goldenen Pokalen", "tokens": ["In", "gol\u00b7de\u00b7nen", "Po\u00b7ka\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Verbirget die Freude sich gern,", "tokens": ["Ver\u00b7bir\u00b7get", "die", "Freu\u00b7de", "sich", "gern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PRF", "ADV", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Es funkeln in den Schaalen", "tokens": ["Es", "fun\u00b7keln", "in", "den", "Schaa\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ha! des Weines liebe Strahlen,", "tokens": ["Ha", "!", "des", "Wei\u00b7nes", "lie\u00b7be", "Strah\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Es regt sich die Welle ein schimmernder Stern.", "tokens": ["Es", "regt", "sich", "die", "Wel\u00b7le", "ein", "schim\u00b7mern\u00b7der", "Stern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "In tiefen Bergeskl\u00fcften,", "tokens": ["In", "tie\u00b7fen", "Ber\u00b7ges\u00b7kl\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wo Gold und der Edelstein keimt,", "tokens": ["Wo", "Gold", "und", "der", "E\u00b7del\u00b7stein", "keimt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "In Meeres fernen Schl\u00fcften,", "tokens": ["In", "Mee\u00b7res", "fer\u00b7nen", "Schl\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In Adlers hohen L\u00fcften,", "tokens": ["In", "Ad\u00b7lers", "ho\u00b7hen", "L\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Nirgend Wein wie auf gl\u00fccklicher Erde sch\u00e4umt.", "tokens": ["Nir\u00b7gend", "Wein", "wie", "auf", "gl\u00fcck\u00b7li\u00b7cher", "Er\u00b7de", "sch\u00e4umt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "NN", "KOKOM", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Gern mancher sucht' in Schl\u00fcnden", "tokens": ["Gern", "man\u00b7cher", "sucht'", "in", "Schl\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PIS", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wo selber dem Bergmann graut,", "tokens": ["Wo", "sel\u00b7ber", "dem", "Berg\u00b7mann", "graut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "In felsigen Gewinden,", "tokens": ["In", "fel\u00b7si\u00b7gen", "Ge\u00b7win\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "K\u00f6nnt' er die Wonne finden,", "tokens": ["K\u00f6nnt'", "er", "die", "Won\u00b7ne", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die so freundlich uns aus dem Becher beschaut. \u2013", "tokens": ["Die", "so", "freund\u00b7lich", "uns", "aus", "dem", "Be\u00b7cher", "be\u00b7schaut", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADV", "ADJD", "PPER", "APPR", "ART", "NN", "VVPP", "$.", "$("], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Als das Gl\u00fcck von der Erde sich wandte,", "tokens": ["Als", "das", "Gl\u00fcck", "von", "der", "Er\u00b7de", "sich", "wand\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Das Geschick alle G\u00f6tter verbannte,", "tokens": ["Das", "Ge\u00b7schick", "al\u00b7le", "G\u00f6t\u00b7ter", "ver\u00b7bann\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "VVFIN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Da standen die Felsen so kahl,", "tokens": ["Da", "stan\u00b7den", "die", "Fel\u00b7sen", "so", "kahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Es verstummten der Liebenden Lieder,", "tokens": ["Es", "ver\u00b7stumm\u00b7ten", "der", "Lie\u00b7ben\u00b7den", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Sah der Mond auf Betr\u00fcbte hernieder,", "tokens": ["Sah", "der", "Mond", "auf", "Be\u00b7tr\u00fcb\u00b7te", "her\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "PTKVZ", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.6": {"text": "Vergingen die Blumen im Thal.", "tokens": ["Ver\u00b7gin\u00b7gen", "die", "Blu\u00b7men", "im", "Thal", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.6": {"line.1": {"text": "Sorg' und Angst und Gram ohne Ende,", "tokens": ["Sor\u00b7g'", "und", "Angst", "und", "Gram", "oh\u00b7ne", "En\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Nur zur Arbeit bewegten sich H\u00e4nde,", "tokens": ["Nur", "zur", "Ar\u00b7beit", "be\u00b7weg\u00b7ten", "sich", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ADJA", "PRF", "NN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Tr\u00fcb' und thr\u00e4nend der feurige Blick,", "tokens": ["Tr\u00fcb'", "und", "thr\u00e4\u00b7nend", "der", "feu\u00b7ri\u00b7ge", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Sehnsucht selber war nun entschwunden,", "tokens": ["Sehn\u00b7sucht", "sel\u00b7ber", "war", "nun", "ent\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "VAFIN", "ADV", "VVPP", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Keiner dachte der vorigen Stunden,", "tokens": ["Kei\u00b7ner", "dach\u00b7te", "der", "vo\u00b7ri\u00b7gen", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Keiner w\u00fcnschte sie heimlich zur\u00fcck. \u2013", "tokens": ["Kei\u00b7ner", "w\u00fcnschte", "sie", "heim\u00b7lich", "zu\u00b7r\u00fcck", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADJD", "PTKVZ", "$.", "$("], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.7": {"line.1": {"text": "Alle G\u00f6tter ohn' Erbarmen", "tokens": ["Al\u00b7le", "G\u00f6t\u00b7ter", "ohn'", "Er\u00b7bar\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sahn hinunter auf die Armen,", "tokens": ["Sahn", "hin\u00b7un\u00b7ter", "auf", "die", "Ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihr Verderben ihr Entschlu\u00df.", "tokens": ["Ihr", "Ver\u00b7der\u00b7ben", "ihr", "Ent\u00b7schlu\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "O! wer w\u00e4re Mensch verblieben,", "tokens": ["O", "!", "wer", "w\u00e4\u00b7re", "Mensch", "ver\u00b7blie\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PWS", "VAFIN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ohne G\u00f6tter, ohne Lieben,", "tokens": ["Oh\u00b7ne", "G\u00f6t\u00b7ter", ",", "oh\u00b7ne", "Lie\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KOUI", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ohne Sehnsucht, ohne Ku\u00df? \u2013", "tokens": ["Oh\u00b7ne", "Sehn\u00b7sucht", ",", "oh\u00b7ne", "Ku\u00df", "?", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "$,", "KOUI", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Bacchus sieht, ein junger Gott,", "tokens": ["Bac\u00b7chus", "sieht", ",", "ein", "jun\u00b7ger", "Gott", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "L\u00e4chelnder Wang', mit Blicken munter", "tokens": ["L\u00e4\u00b7cheln\u00b7der", "Wang'", ",", "mit", "Bli\u00b7cken", "mun\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "APPR", "NN", "ADJD"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Zur verla\u00dfnen Erd' hinunter,", "tokens": ["Zur", "ver\u00b7la\u00df\u00b7nen", "Erd'", "hin\u00b7un\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihn bewegt der Menschheit Noth.", "tokens": ["Ihn", "be\u00b7wegt", "der", "Menschheit", "Noth", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.9": {"line.1": {"text": "Und es spricht die Silberstimme:", "tokens": ["Und", "es", "spricht", "die", "Sil\u00b7ber\u00b7stim\u00b7me", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meine Freunde sind zu wild,", "tokens": ["Mei\u00b7ne", "Freun\u00b7de", "sind", "zu", "wild", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKA", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihrem eigensinn'gen Grimme", "tokens": ["Ih\u00b7rem", "ei\u00b7gen\u00b7sinn'\u00b7gen", "Grim\u00b7me"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unterliegt das Menschenbild.", "tokens": ["Un\u00b7ter\u00b7liegt", "das", "Men\u00b7schen\u00b7bild", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "D\u00fcrfen sie die Welt verh\u00f6hnen", "tokens": ["D\u00fcr\u00b7fen", "sie", "die", "Welt", "ver\u00b7h\u00f6h\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weil kein Tod uns G\u00f6ttern dr\u00e4ut?", "tokens": ["Weil", "kein", "Tod", "uns", "G\u00f6t\u00b7tern", "dr\u00e4ut", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "PPER", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sollen denn nur Angst und St\u00f6hnen", "tokens": ["Sol\u00b7len", "denn", "nur", "Angst", "und", "St\u00f6h\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "ADV", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Leben sein, und bittres Leid? \u2013", "tokens": ["Le\u00b7ben", "sein", ",", "und", "bitt\u00b7res", "Leid", "?", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VAINF", "$,", "KON", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Bacchus l\u00e4\u00dft die Rebe sprie\u00dfen,", "tokens": ["Bac\u00b7chus", "l\u00e4\u00dft", "die", "Re\u00b7be", "sprie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Saft durch ihre Bl\u00e4tter flie\u00dfen,", "tokens": ["Saft", "durch", "ih\u00b7re", "Bl\u00e4t\u00b7ter", "flie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "L\u00e4\u00dft sie weiche L\u00fcfte f\u00e4cheln,", "tokens": ["L\u00e4\u00dft", "sie", "wei\u00b7che", "L\u00fcf\u00b7te", "f\u00e4\u00b7cheln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sonnet sie mit seinem L\u00e4cheln.", "tokens": ["Son\u00b7net", "sie", "mit", "sei\u00b7nem", "L\u00e4\u00b7cheln", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Um die Ulme hingeschlungen", "tokens": ["Um", "die", "Ul\u00b7me", "hin\u00b7ge\u00b7schlun\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Steht die neue Pflanz' im Licht,", "tokens": ["Steht", "die", "neu\u00b7e", "Pflanz'", "im", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measu