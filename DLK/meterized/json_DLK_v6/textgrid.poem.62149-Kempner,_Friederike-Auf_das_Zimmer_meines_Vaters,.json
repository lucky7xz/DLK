{"textgrid.poem.62149": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "Auf das Zimmer meines Vaters,", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fast verfallen ist das Fenster,", "tokens": ["Fast", "ver\u00b7fal\u00b7len", "ist", "das", "Fens\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Keiner wohnt im Zimmer drin,", "tokens": ["Kei\u00b7ner", "wohnt", "im", "Zim\u00b7mer", "drin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der Erinnerung Gespenster,", "tokens": ["Der", "E\u00b7rin\u00b7ne\u00b7rung", "Ge\u00b7spens\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sie umnebeln meinen Sinn. \u2013", "tokens": ["Sie", "um\u00b7ne\u00b7beln", "mei\u00b7nen", "Sinn", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Wohnt der Vater nicht leibhaftig,", "tokens": ["Wohnt", "der", "Va\u00b7ter", "nicht", "leib\u00b7haf\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKNEG", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie das Leben selber drin?", "tokens": ["Wie", "das", "Le\u00b7ben", "sel\u00b7ber", "drin", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "W\u00fcnschend, wollend, einzig-kr\u00e4ftig,", "tokens": ["W\u00fcn\u00b7schend", ",", "wol\u00b7lend", ",", "ein\u00b7zig\u00b7kr\u00e4f\u00b7tig", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "VVPP", "$,", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Stets mit einem frischen Sinn? \u2013", "tokens": ["Stets", "mit", "ei\u00b7nem", "fri\u00b7schen", "Sinn", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Blaue Augen, braune Haare,", "tokens": ["Blau\u00b7e", "Au\u00b7gen", ",", "brau\u00b7ne", "Haa\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stark und gro\u00df, ein Riese fast,", "tokens": ["Stark", "und", "gro\u00df", ",", "ein", "Rie\u00b7se", "fast", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "$,", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ungebleicht, trotz sechzig Jahre,", "tokens": ["Un\u00b7ge\u00b7bleicht", ",", "trotz", "sech\u00b7zig", "Jah\u00b7re", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "CARD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Urgesch\u00e4ftig ohne Rast!", "tokens": ["Ur\u00b7ge\u00b7sch\u00e4f\u00b7tig", "oh\u00b7ne", "Rast", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und verschwunden ist das Alles,", "tokens": ["Und", "ver\u00b7schwun\u00b7den", "ist", "das", "Al\u00b7les", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "VAFIN", "ART", "PIS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die lebendige Gestalt,", "tokens": ["Die", "le\u00b7ben\u00b7di\u00b7ge", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und kein Nachhall eines Schalles,", "tokens": ["Und", "kein", "Nach\u00b7hall", "ei\u00b7nes", "Schal\u00b7les", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mehr aus diesen Fenstern schallt!!", "tokens": ["Mehr", "aus", "die\u00b7sen", "Fens\u00b7tern", "schallt", "!!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PDAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "O, die Tr\u00e4ume nur, sie leben,", "tokens": ["O", ",", "die", "Tr\u00e4u\u00b7me", "nur", ",", "sie", "le\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "ADV", "$,", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die Wirklichkeit sie stirbt, \u2013", "tokens": ["Und", "die", "Wirk\u00b7lich\u00b7keit", "sie", "stirbt", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "NN", "PPER", "VVFIN", "$,", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur der Dichtung Reich entschweben", "tokens": ["Nur", "der", "Dich\u00b7tung", "Reich", "ent\u00b7schwe\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Geister, die kein Hauch verdirbt!", "tokens": ["Geis\u00b7ter", ",", "die", "kein", "Hauch", "ver\u00b7dirbt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}