fo": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "PTKZU", "VVINF", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "und auf seiner bleichen Stirne", "tokens": ["und", "auf", "sei\u00b7ner", "blei\u00b7chen", "Stir\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "alles Licht der Nacht vereint.", "tokens": ["al\u00b7les", "Licht", "der", "Nacht", "ver\u00b7eint", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}