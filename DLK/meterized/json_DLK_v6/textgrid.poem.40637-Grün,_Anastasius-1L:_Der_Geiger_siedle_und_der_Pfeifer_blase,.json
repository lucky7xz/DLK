{"textgrid.poem.40637": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Geiger siedle und der Pfeifer blase,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Geiger siedle und der Pfeifer blase,", "tokens": ["Der", "Gei\u00b7ger", "sied\u00b7le", "und", "der", "Pfei\u00b7fer", "bla\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zum Hochlandsreih'n euch M\u00e4gdlein aufzufrischen,", "tokens": ["Zum", "Hoch\u00b7lands\u00b7reih'n", "euch", "M\u00e4gd\u00b7lein", "auf\u00b7zu\u00b7fri\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df die Gestalten sich, hinschwebend, mischen,", "tokens": ["Da\u00df", "die", "Ge\u00b7stal\u00b7ten", "sich", ",", "hin\u00b7schwe\u00b7bend", ",", "mi\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PRF", "$,", "VVPP", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie Gold- und Silberfischlein in dem Glase!", "tokens": ["Wie", "Gold", "und", "Sil\u00b7ber\u00b7fisc\u00b7hlein", "in", "dem", "Gla\u00b7se", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "TRUNC", "KON", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Gleicht ihr nicht selbst den Fischlein in der Vase?", "tokens": ["Gleicht", "ihr", "nicht", "selbst", "den", "Fisc\u00b7hlein", "in", "der", "Va\u00b7se", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADV", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So was vom Nixenhaften, Meeresfrischen,", "tokens": ["So", "was", "vom", "Ni\u00b7xen\u00b7haf\u00b7ten", ",", "Mee\u00b7res\u00b7fri\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "PWS", "APPRART", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein Zug der Sippe l\u00e4\u00dft sich nicht verwischen;", "tokens": ["Ein", "Zug", "der", "Sip\u00b7pe", "l\u00e4\u00dft", "sich", "nicht", "ver\u00b7wi\u00b7schen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "PRF", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Meerfei, traun, ist eure holde Base.", "tokens": ["Die", "Meer\u00b7fei", ",", "traun", ",", "ist", "eu\u00b7re", "hol\u00b7de", "Ba\u00b7se", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVINF", "$,", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Mir sei's kein Wunder, wenn die Budenw\u00e4nde", "tokens": ["Mir", "sei's", "kein", "Wun\u00b7der", ",", "wenn", "die", "Bu\u00b7den\u00b7w\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit Einem Schlag als blanke Wogen steigen!", "tokens": ["Mit", "Ei\u00b7nem", "Schlag", "als", "blan\u00b7ke", "Wo\u00b7gen", "stei\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KOUS", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Spielleut' st\u00f6rt es nicht, und nicht den Reigen:", "tokens": ["Die", "Spiel\u00b7leut'", "st\u00f6rt", "es", "nicht", ",", "und", "nicht", "den", "Rei\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "$,", "KON", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Auf Muscheln blasen sie das St\u00fcck zu Ende,", "tokens": ["Auf", "Mu\u00b7scheln", "bla\u00b7sen", "sie", "das", "St\u00fcck", "zu", "En\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihr tanzt zu End' im Meerschlo\u00df von Kristallen,", "tokens": ["Ihr", "tanzt", "zu", "End'", "im", "Meer\u00b7schlo\u00df", "von", "Kris\u00b7tal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "APPRART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+--++-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und geht dann ruhn zum Lusthain der Korallen.", "tokens": ["Und", "geht", "dann", "ruhn", "zum", "Lust\u00b7hain", "der", "Ko\u00b7ral\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "VVINF", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}