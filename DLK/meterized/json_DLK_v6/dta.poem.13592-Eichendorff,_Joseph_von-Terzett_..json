{"dta.poem.13592": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Terzett .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Doch Keiner athmet so den Strom von L\u00fcften,", "tokens": ["Doch", "Kei\u00b7ner", "ath\u00b7met", "so", "den", "Strom", "von", "L\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ADV", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als wie der J\u00e4ger in den gr\u00fcnen Kl\u00fcften!", "tokens": ["Als", "wie", "der", "J\u00e4\u00b7ger", "in", "den", "gr\u00fc\u00b7nen", "Kl\u00fcf\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wo Euch der Athem schwindelnd schon vergangen,", "tokens": ["Wo", "Euch", "der", "A\u00b7them", "schwin\u00b7delnd", "schon", "ver\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "ADJD", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Hat seine rechte Lust erst angefangen,", "tokens": ["Hat", "sei\u00b7ne", "rech\u00b7te", "Lust", "erst", "an\u00b7ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wenn tief das Thal auffunkelt durch die B\u00e4ume", "tokens": ["Wenn", "tief", "das", "Thal", "auf\u00b7fun\u00b7kelt", "durch", "die", "B\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der Aar sich aufschwingt in die klaren R\u00e4ume.", "tokens": ["Der", "Aar", "sich", "auf\u00b7schwingt", "in", "die", "kla\u00b7ren", "R\u00e4u\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}