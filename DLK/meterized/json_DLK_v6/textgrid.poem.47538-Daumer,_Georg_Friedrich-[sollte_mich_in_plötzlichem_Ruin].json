{"textgrid.poem.47538": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[sollte mich in pl\u00f6tzlichem Ruin]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sollte mich in pl\u00f6tzlichem Ruin", "tokens": ["Soll\u00b7te", "mich", "in", "pl\u00f6tz\u00b7li\u00b7chem", "Ru\u00b7in"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PRF", "APPR", "ADJA", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Feuerblick und heitre Laune flieh'n,", "tokens": ["Feu\u00b7er\u00b7blick", "und", "heit\u00b7re", "Lau\u00b7ne", "flieh'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Sollte sich durch Ader und Gebein", "tokens": ["Soll\u00b7te", "sich", "durch", "A\u00b7der", "und", "Ge\u00b7bein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PRF", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Bange Qual und dumpfe Schwere zieh'n \u2013", "tokens": ["Ban\u00b7ge", "Qual", "und", "dump\u00b7fe", "Schwe\u00b7re", "zieh'n", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "ADJA", "NN", "VVFIN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Nicht, o nicht mit herben Arzenei'n,", "tokens": ["Nicht", ",", "o", "nicht", "mit", "her\u00b7ben", "Ar\u00b7ze\u00b7nei'n", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "FM", "PTKNEG", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Denn ich hasse diese Medicin,", "tokens": ["Denn", "ich", "has\u00b7se", "die\u00b7se", "Me\u00b7di\u00b7cin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PDAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Komm zu mir mit einem Becher Wein,", "tokens": ["Komm", "zu", "mir", "mit", "ei\u00b7nem", "Be\u00b7cher", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Komm mit Laute, Fl\u00f6te, Tamburin!", "tokens": ["Komm", "mit", "Lau\u00b7te", ",", "Fl\u00f6\u00b7te", ",", "Tam\u00b7bu\u00b7rin", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Wirket das zu wenig auf mich ein,", "tokens": ["Wir\u00b7ket", "das", "zu", "we\u00b7nig", "auf", "mich", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "PTKA", "PIS", "APPR", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Komm mit einem s\u00fc\u00dfen Mundrubin!", "tokens": ["Komm", "mit", "ei\u00b7nem", "s\u00fc\u00b7\u00dfen", "Mun\u00b7dru\u00b7bin", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Wird umsonst auch diese M\u00fche sein \u2013", "tokens": ["Wird", "um\u00b7sonst", "auch", "die\u00b7se", "M\u00fc\u00b7he", "sein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "PDAT", "NN", "VAINF", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Dann Ade! dann sprich: Begrabet ihn!", "tokens": ["Dann", "A\u00b7de", "!", "dann", "sprich", ":", "Be\u00b7gra\u00b7bet", "ihn", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "NN", "$.", "ADV", "ADJD", "$.", "VVFIN", "PPER", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}}}}