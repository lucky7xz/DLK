{"dta.poem.23674": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Die Anfrage.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-200905197682", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Singt, ihr Dichter,", "tokens": ["Singt", ",", "ihr", "Dich\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Singt, und lobet,", "tokens": ["Singt", ",", "und", "lo\u00b7bet", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KON", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Singt, und schmeichelt,", "tokens": ["Singt", ",", "und", "schmei\u00b7chelt", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KON", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Singt, und bettelt,", "tokens": ["Singt", ",", "und", "bet\u00b7telt", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KON", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Singt von Helden.", "tokens": ["Singt", "von", "Hel\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Ich will singen,", "tokens": ["Ich", "will", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Ich will spielen,", "tokens": ["Ich", "will", "spie\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Aber warlich", "tokens": ["A\u00b7ber", "war\u00b7lich"], "token_info": ["word", "word"], "pos": ["KON", "ADV"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Nicht von Helden.", "tokens": ["Nicht", "von", "Hel\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.10": {"text": "H\u00f6rt, ihr Sch\u00f6nen,", "tokens": ["H\u00f6rt", ",", "ihr", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.11": {"text": "H\u00f6rt mich spielen!", "tokens": ["H\u00f6rt", "mich", "spie\u00b7len", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.12": {"text": "Meine Saiten", "tokens": ["Mei\u00b7ne", "Sai\u00b7ten"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.13": {"text": "Sind nicht blutig.", "tokens": ["Sind", "nicht", "blu\u00b7tig", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ADJD", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.14": {"text": "H\u00f6rt, ihr Sch\u00f6nen,", "tokens": ["H\u00f6rt", ",", "ihr", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.15": {"text": "H\u00f6rt mich singen!", "tokens": ["H\u00f6rt", "mich", "sin\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.16": {"text": "Meine Lieder", "tokens": ["Mei\u00b7ne", "Lie\u00b7der"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.17": {"text": "Sind nicht traurig.", "tokens": ["Sind", "nicht", "trau\u00b7rig", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ADJD", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.18": {"text": "H\u00f6rt, ich singe", "tokens": ["H\u00f6rt", ",", "ich", "sin\u00b7ge"], "token_info": ["word", "punct", "word", "word"], "pos": ["VVFIN", "$,", "PPER", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.19": {"text": "Nur von M\u00e4dchen,", "tokens": ["Nur", "von", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.20": {"text": "Und ich spiele,", "tokens": ["Und", "ich", "spie\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.21": {"text": "Nur von Liebe.", "tokens": ["Nur", "von", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.22": {"text": "Wolt ihr h\u00f6ren?", "tokens": ["Wolt", "ihr", "h\u00f6\u00b7ren", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}