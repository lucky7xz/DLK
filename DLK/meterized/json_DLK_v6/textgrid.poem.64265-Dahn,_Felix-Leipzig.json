{"textgrid.poem.64265": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "Leipzig", "genre": "verse", "period": "N.A.", "pub_year": 1873, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seit grauer Vorzeit wurde hier gek\u00e4mpft;", "tokens": ["Seit", "grau\u00b7er", "Vor\u00b7zeit", "wur\u00b7de", "hier", "ge\u00b7k\u00e4mpft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Doch niemals stand ein k\u00f6stlicheres Gut", "tokens": ["Doch", "nie\u00b7mals", "stand", "ein", "k\u00f6st\u00b7li\u00b7che\u00b7res", "Gut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Auf Schwertesspitze hier, als an dem Tag,", "tokens": ["Auf", "Schwer\u00b7tes\u00b7spit\u00b7ze", "hier", ",", "als", "an", "dem", "Tag", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "$,", "KOUS", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da Preu\u00dfen, \u00d6sterreich und Ru\u00dfland k\u00e4mpften,", "tokens": ["Da", "Preu\u00b7\u00dfen", ",", "\u00d6s\u00b7ter\u00b7reich", "und", "Ru\u00df\u00b7land", "k\u00e4mpf\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "$,", "ADJD", "KON", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Drei, Adler mit dem ungeheuern Geier,", "tokens": ["Drei", ",", "Ad\u00b7ler", "mit", "dem", "un\u00b7ge\u00b7heu\u00b7ern", "Gei\u00b7er", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "$,", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der seine Schwingen \u00fcber ganz Europa,", "tokens": ["Der", "sei\u00b7ne", "Schwin\u00b7gen", "\u00fc\u00b7ber", "ganz", "Eu\u00b7ro\u00b7pa", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPR", "ADV", "NE", "$,"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Von Polen bis nach Spanien hielt gespannt:", "tokens": ["Von", "Po\u00b7len", "bis", "nach", "Spa\u00b7ni\u00b7en", "hielt", "ge\u00b7spannt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADV", "APPR", "NE", "VVFIN", "VVPP", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Die Adler siegten und der Geier floh. \u2013", "tokens": ["Die", "Ad\u00b7ler", "sieg\u00b7ten", "und", "der", "Gei\u00b7er", "floh", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}