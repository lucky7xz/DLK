{"textgrid.poem.64841": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: O, w\u00e4re mir das heitre Loos gefallen,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O, w\u00e4re mir das heitre Loos gefallen,", "tokens": ["O", ",", "w\u00e4\u00b7re", "mir", "das", "heit\u00b7re", "Loos", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VAFIN", "PPER", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das still begl\u00fcckend andern Frauen f\u00e4llt,", "tokens": ["Das", "still", "be\u00b7gl\u00fc\u00b7ckend", "an\u00b7dern", "Frau\u00b7en", "f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "VVPP", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In schirmender Beschr\u00e4nkung hinzuwallen", "tokens": ["In", "schir\u00b7men\u00b7der", "Be\u00b7schr\u00e4n\u00b7kung", "hin\u00b7zu\u00b7wal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Durch eines engen Kreises kleine Welt;", "tokens": ["Durch", "ei\u00b7nes", "en\u00b7gen", "Krei\u00b7ses", "klei\u00b7ne", "Welt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Mein Herz gleich einer Blume zu verschlie\u00dfen,", "tokens": ["Mein", "Herz", "gleich", "ei\u00b7ner", "Blu\u00b7me", "zu", "ver\u00b7schlie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vor jedem Sturm und jedem Weh der Zeit,", "tokens": ["Vor", "je\u00b7dem", "Sturm", "und", "je\u00b7dem", "Weh", "der", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "KON", "PIAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Des Lebens Freuden harmlos zu genie\u00dfen", "tokens": ["Des", "Le\u00b7bens", "Freu\u00b7den", "harm\u00b7los", "zu", "ge\u00b7nie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In ahnungsloser Unbefangenheit!", "tokens": ["In", "ah\u00b7nungs\u00b7lo\u00b7ser", "Un\u00b7be\u00b7fan\u00b7gen\u00b7heit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch anders hat sich mein Geschick gewendet,", "tokens": ["Doch", "an\u00b7ders", "hat", "sich", "mein", "Ge\u00b7schick", "ge\u00b7wen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PRF", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Kampfplatz nur war meine Lebensbahn;", "tokens": ["Ein", "Kampf\u00b7platz", "nur", "war", "mei\u00b7ne", "Le\u00b7bens\u00b7bahn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Kindheit Bl\u00fcthenruh ward mir entwendet", "tokens": ["Der", "Kind\u00b7heit", "Bl\u00fc\u00b7then\u00b7ruh", "ward", "mir", "ent\u00b7wen\u00b7det"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VAFIN", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und hingeopfert einem eitlen Wahn!", "tokens": ["Und", "hin\u00b7ge\u00b7o\u00b7pfert", "ei\u00b7nem", "eit\u00b7len", "Wahn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "In starrem Zwang verflossen jene Tage,", "tokens": ["In", "star\u00b7rem", "Zwang", "ver\u00b7flos\u00b7sen", "je\u00b7ne", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In strenge Regeln \u00e4ngstlich eingeschult,", "tokens": ["In", "stren\u00b7ge", "Re\u00b7geln", "\u00e4ngst\u00b7lich", "ein\u00b7ge\u00b7schult", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Indessen meines jungen Herzens Klage", "tokens": ["In\u00b7des\u00b7sen", "mei\u00b7nes", "jun\u00b7gen", "Her\u00b7zens", "Kla\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Um frische Luft und Sonnenlicht gebuhlt.", "tokens": ["Um", "fri\u00b7sche", "Luft", "und", "Son\u00b7nen\u00b7licht", "ge\u00b7buhlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJA", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ich rang dawider, doch es war vergebens,", "tokens": ["Ich", "rang", "da\u00b7wi\u00b7der", ",", "doch", "es", "war", "ver\u00b7ge\u00b7bens", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "$,", "KON", "PPER", "VAFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und als ich nun entwachsen jener Zucht,", "tokens": ["Und", "als", "ich", "nun", "ent\u00b7wach\u00b7sen", "je\u00b7ner", "Zucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VVFIN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da drang die feindlich finstre Macht des Lebens", "tokens": ["Da", "drang", "die", "feind\u00b7lich", "finst\u00b7re", "Macht", "des", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJD", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wild auf mich ein mit ihrer ganzen Wucht.", "tokens": ["Wild", "auf", "mich", "ein", "mit", "ih\u00b7rer", "gan\u00b7zen", "Wucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PRF", "ART", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Mich schirmte keines Freundes treues Lieben,", "tokens": ["Mich", "schirm\u00b7te", "kei\u00b7nes", "Freun\u00b7des", "treu\u00b7es", "Lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "ADJA", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Durch meinen Frost drang keines Herzens Glut,", "tokens": ["Durch", "mei\u00b7nen", "Frost", "drang", "kei\u00b7nes", "Her\u00b7zens", "Glut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und in die Fremde ward ich fortgetrieben", "tokens": ["Und", "in", "die", "Frem\u00b7de", "ward", "ich", "fort\u00b7ge\u00b7trie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ohn' andre St\u00fctze als den eignen Muth.", "tokens": ["Ohn'", "and\u00b7re", "St\u00fct\u00b7ze", "als", "den", "eig\u00b7nen", "Muth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Was ich bedurfte, mu\u00dft' ich selbst erringen,", "tokens": ["Was", "ich", "be\u00b7durf\u00b7te", ",", "mu\u00dft'", "ich", "selbst", "er\u00b7rin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ausk\u00e4mpfen selber jeden herben Streit,", "tokens": ["Aus\u00b7k\u00e4mp\u00b7fen", "sel\u00b7ber", "je\u00b7den", "her\u00b7ben", "Streit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und dr\u00fcckend lasteten auf meinen Schwingen", "tokens": ["Und", "dr\u00fc\u00b7ckend", "las\u00b7te\u00b7ten", "auf", "mei\u00b7nen", "Schwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die schweren Fesseln der Nothwendigkeit.", "tokens": ["Die", "schwe\u00b7ren", "Fes\u00b7seln", "der", "Noth\u00b7wen\u00b7dig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Weh Jedem, der in seinem Thun und Lassen", "tokens": ["Weh", "Je\u00b7dem", ",", "der", "in", "sei\u00b7nem", "Thun", "und", "Las\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PIAT", "$,", "PRELS", "APPR", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dem inneren Gesetz nicht folgen kann!", "tokens": ["Dem", "in\u00b7ne\u00b7ren", "Ge\u00b7setz", "nicht", "fol\u00b7gen", "kann", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mein Ungl\u00fcck l\u00e4\u00dft sich in zwei Worte fassen:", "tokens": ["Mein", "Un\u00b7gl\u00fcck", "l\u00e4\u00dft", "sich", "in", "zwei", "Wor\u00b7te", "fas\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "APPR", "CARD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich war ein Weib und k\u00e4mpfte wie ein Mann!", "tokens": ["Ich", "war", "ein", "Weib", "und", "k\u00e4mpf\u00b7te", "wie", "ein", "Mann", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "KON", "VVFIN", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Da\u00df ihm am Tag der Schlacht die Wehr nicht fehle,", "tokens": ["Da\u00df", "ihm", "am", "Tag", "der", "Schlacht", "die", "Wehr", "nicht", "feh\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "ART", "NN", "ART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erwarb mein Geist sich Sch\u00e4rfe, Kraft und Licht,", "tokens": ["Er\u00b7warb", "mein", "Geist", "sich", "Sch\u00e4r\u00b7fe", ",", "Kraft", "und", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PRF", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch bl\u00fcthenlos blieb meine ernste Seele \u2013", "tokens": ["Doch", "bl\u00fc\u00b7then\u00b7los", "blieb", "mei\u00b7ne", "erns\u00b7te", "See\u00b7le", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im Waffenkleid pflegt man der Blumen nicht.", "tokens": ["Im", "Waf\u00b7fen\u00b7kleid", "pflegt", "man", "der", "Blu\u00b7men", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PIS", "ART", "NN", "PTKNEG", "$."], "meter": "-+-++--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.10": {"line.1": {"text": "Nur ", "tokens": ["Nur"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Verhei\u00dfend lag vor mir ein sch\u00f6nes Gl\u00fcck;", "tokens": ["Ver\u00b7hei\u00b7\u00dfend", "lag", "vor", "mir", "ein", "sch\u00f6\u00b7nes", "Gl\u00fcck", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch kaum erbl\u00fcht, sank es, zu Tod getroffen,", "tokens": ["Doch", "kaum", "er\u00b7bl\u00fcht", ",", "sank", "es", ",", "zu", "Tod", "ge\u00b7trof\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "$,", "VVFIN", "PPER", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und eine Wunde nur blieb mir zur\u00fcck.", "tokens": ["Und", "ei\u00b7ne", "Wun\u00b7de", "nur", "blieb", "mir", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.11": {"line.1": {"text": "So glitt, fast ungeahnt an mir vor\u00fcber", "tokens": ["So", "glitt", ",", "fast", "un\u00b7ge\u00b7ahnt", "an", "mir", "vor\u00b7\u00fc\u00b7ber"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "ADV", "ADJD", "APPR", "PPER", "PTKVZ"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Liebefr\u00fchlings tr\u00e4um'rische Gestalt,", "tokens": ["Des", "Lie\u00b7be\u00b7fr\u00fch\u00b7lings", "tr\u00e4um'\u00b7ri\u00b7sche", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und tr\u00fcber ward mein Sinn und immer tr\u00fcber,", "tokens": ["Und", "tr\u00fc\u00b7ber", "ward", "mein", "Sinn", "und", "im\u00b7mer", "tr\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPOSAT", "NN", "KON", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mein Herz, gleichwie die Todten, schwer und kalt.", "tokens": ["Mein", "Herz", ",", "gleich\u00b7wie", "die", "Tod\u00b7ten", ",", "schwer", "und", "kalt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KON", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Und wie vom Hauch der herbstlich scharfen Winde", "tokens": ["Und", "wie", "vom", "Hauch", "der", "herbst\u00b7lich", "schar\u00b7fen", "Win\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "APPRART", "NN", "ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sich rauh verh\u00e4rtet manch ein zartes Reis:", "tokens": ["Sich", "rauh", "ver\u00b7h\u00e4r\u00b7tet", "manch", "ein", "zar\u00b7tes", "Reis", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "VVFIN", "PIAT", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So legte sich um mein Gem\u00fcth die Rinde", "tokens": ["So", "leg\u00b7te", "sich", "um", "mein", "Ge\u00b7m\u00fcth", "die", "Rin\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des Lebens\u00fcberdrusses starr, wie Eis.", "tokens": ["Des", "Le\u00b7bens\u00b7\u00fc\u00b7berd\u00b7rus\u00b7ses", "starr", ",", "wie", "Eis", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "PWAV", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.13": {"line.1": {"text": "Und nun, da schon mein bess'res Theil im Grabe,", "tokens": ["Und", "nun", ",", "da", "schon", "mein", "bess'\u00b7res", 