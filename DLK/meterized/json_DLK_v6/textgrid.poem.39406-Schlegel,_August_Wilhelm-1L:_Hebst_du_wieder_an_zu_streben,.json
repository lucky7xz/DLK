{"textgrid.poem.39406": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hebst du wieder an zu streben,", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hebst du wieder an zu streben,", "tokens": ["Hebst", "du", "wie\u00b7der", "an", "zu", "stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wunsch der Lieb' in meiner Brust?", "tokens": ["Wunsch", "der", "Lieb'", "in", "mei\u00b7ner", "Brust", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie die Wellen sanft sich kr\u00e4useln,", "tokens": ["Wie", "die", "Wel\u00b7len", "sanft", "sich", "kr\u00e4u\u00b7seln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn am See die L\u00fcfte s\u00e4useln,", "tokens": ["Wenn", "am", "See", "die", "L\u00fcf\u00b7te", "s\u00e4u\u00b7seln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie sie um's Gestade weben,", "tokens": ["Wie", "sie", "um's", "Ge\u00b7sta\u00b7de", "we\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Also schmeichelt meiner Brust", "tokens": ["Al\u00b7so", "schmei\u00b7chelt", "mei\u00b7ner", "Brust"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Neue Sehnsucht, neue Lust.", "tokens": ["Neu\u00b7e", "Sehn\u00b7sucht", ",", "neu\u00b7e", "Lust", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Schmeichelt nur, ihr Wind' und Wellen!", "tokens": ["Schmei\u00b7chelt", "nur", ",", "ihr", "Wind'", "und", "Wel\u00b7len", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eure T\u00fcck' ist mir bewu\u00dft.", "tokens": ["Eu\u00b7re", "T\u00fcck", "ist", "mir", "be\u00b7wu\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wi\u00dft so k\u00fchl heran zu schwellen,", "tokens": ["Wi\u00dft", "so", "k\u00fchl", "he\u00b7ran", "zu", "schwel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wi\u00dft so lieblich einzuladen,", "tokens": ["Wi\u00dft", "so", "lieb\u00b7lich", "ein\u00b7zu\u00b7la\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "VVIZU", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sich in eurem Schoo\u00df zu baden:", "tokens": ["Sich", "in", "eu\u00b7rem", "Schoo\u00df", "zu", "ba\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Doch mit Kummer und Verlust", "tokens": ["Doch", "mit", "Kum\u00b7mer", "und", "Ver\u00b7lust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Lohnt ihr die beth\u00f6rte Brust.", "tokens": ["Lohnt", "ihr", "die", "be\u00b7th\u00f6r\u00b7te", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Einmal habt ihr mich belogen,", "tokens": ["Ein\u00b7mal", "habt", "ihr", "mich", "be\u00b7lo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schmeichell\u00fcfte, Silbersee!", "tokens": ["Schmei\u00b7chel\u00b7l\u00fcf\u00b7te", ",", "Sil\u00b7ber\u00b7see", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "L\u00fcstern sprang ich vom Gestade,", "tokens": ["L\u00fcs\u00b7tern", "sprang", "ich", "vom", "Ge\u00b7sta\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tauchte Stirn und Brust in Wogen;", "tokens": ["Tauch\u00b7te", "Stirn", "und", "Brust", "in", "Wo\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Fortgewiegt im lauen Bade,", "tokens": ["Fort\u00b7ge\u00b7wiegt", "im", "lau\u00b7en", "Ba\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sah ich rings nur glatten See,", "tokens": ["Sah", "ich", "rings", "nur", "glat\u00b7ten", "See", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Obenher azurne H\u00f6h.", "tokens": ["O\u00b7ben\u00b7her", "a\u00b7zur\u00b7ne", "H\u00f6h", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.4": {"line.1": {"text": "Ach, die blaue H\u00f6h ward d\u00fcster,", "tokens": ["Ach", ",", "die", "blau\u00b7e", "H\u00f6h", "ward", "d\u00fcs\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ART", "ADJA", "NN", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wild der Wa\u00dferwogen Scherz,", "tokens": ["Wild", "der", "Wa\u00b7\u00dfer\u00b7wo\u00b7gen", "Scherz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sausen ward des Winds Gefl\u00fcster.", "tokens": ["Sau\u00b7sen", "ward", "des", "Winds", "Ge\u00b7fl\u00fcs\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zwischen Strudeln, zwischen Klippen,", "tokens": ["Zwi\u00b7schen", "Stru\u00b7deln", ",", "zwi\u00b7schen", "Klip\u00b7pen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Klagt' ich mit erbla\u00dften Lippen,", "tokens": ["Klagt'", "ich", "mit", "er\u00b7bla\u00df\u00b7ten", "Lip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Rief nach Rettung, und mein Herz", "tokens": ["Rief", "nach", "Ret\u00b7tung", ",", "und", "mein", "Herz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "$,", "KON", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Fluchte nun auf Bad und Scherz.", "tokens": ["Fluch\u00b7te", "nun", "auf", "Bad", "und", "Scherz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "NE", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Doch durch Aufruhr und Get\u00fcmmel,", "tokens": ["Doch", "durch", "Auf\u00b7ruhr", "und", "Ge\u00b7t\u00fcm\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "KON", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Nacht und Grausen, fern und nah,", "tokens": ["Nacht", "und", "Grau\u00b7sen", ",", "fern", "und", "nah", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Zog zu des Gestades Schilfe", "tokens": ["Zog", "zu", "des", "Ge\u00b7sta\u00b7des", "Schil\u00b7fe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mich zur\u00fcck der G\u00f6tter H\u00fclfe.", "tokens": ["Mich", "zu\u00b7r\u00fcck", "der", "G\u00f6t\u00b7ter", "H\u00fcl\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sieh, da l\u00e4chelte der Himmel!", "tokens": ["Sieh", ",", "da", "l\u00e4\u00b7chel\u00b7te", "der", "Him\u00b7mel", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Leise L\u00fcfte, fern und nah!", "tokens": ["Lei\u00b7se", "L\u00fcf\u00b7te", ",", "fern", "und", "nah", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Spiegelflut, wohin ich sah!", "tokens": ["Spie\u00b7gel\u00b7flut", ",", "wo\u00b7hin", "ich", "sah", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Und ich seufzte Dank den Rettern,", "tokens": ["Und", "ich", "seufz\u00b7te", "Dank", "den", "Ret\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schaute matt zur Himmelsh\u00f6h.", "tokens": ["Schau\u00b7te", "matt", "zur", "Him\u00b7mels\u00b7h\u00f6h", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber bald, entbrannt im Grimme,", "tokens": ["A\u00b7ber", "bald", ",", "ent\u00b7brannt", "im", "Grim\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schwur ich, sang's mit rascher Stimme:", "tokens": ["Schwur", "ich", ",", "sang's", "mit", "ra\u00b7scher", "Stim\u00b7me", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nichts bei Menschen, nichts bei G\u00f6ttern,", "tokens": ["Nichts", "bei", "Men\u00b7schen", ",", "nichts", "bei", "G\u00f6t\u00b7tern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "NN", "$,", "PIS", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "In der Tief' und auf der H\u00f6h,", "tokens": ["In", "der", "Tie\u00b7f'", "und", "auf", "der", "H\u00f6h", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "APPR", "ART", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Sei so falsch wie dieser See.", "tokens": ["Sei", "so", "falsch", "wie", "die\u00b7ser", "See", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "KOKOM", "PDAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "F\u00fchllos schlanget zwar ihr Wellen", "tokens": ["F\u00fchl\u00b7los", "schlan\u00b7get", "zwar", "ihr", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meines Liedes Pfeil hinab.", "tokens": ["Mei\u00b7nes", "Lie\u00b7des", "Pfeil", "hin\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber will, bei eurem Schwellen,", "tokens": ["A\u00b7ber", "will", ",", "bei", "eu\u00b7rem", "Schwel\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nun mein Herz sich l\u00fcstern regen,", "tokens": ["Nun", "mein", "Herz", "sich", "l\u00fcs\u00b7tern", "re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "PRF", "ADJD", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Donnert euch mein Schwur entgegen;", "tokens": ["Don\u00b7nert", "euch", "mein", "Schwur", "ent\u00b7ge\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schelt' ich vom Gestad' herab:", "tokens": ["Schelt'", "ich", "vom", "Ge\u00b7stad'", "her\u00b7ab", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ADV", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.7": {"text": "Glatter Abgrund! l\u00e4chelnd Grab!", "tokens": ["Glat\u00b7ter", "Ab\u00b7grund", "!", "l\u00e4\u00b7chelnd", "Grab", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$.", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}