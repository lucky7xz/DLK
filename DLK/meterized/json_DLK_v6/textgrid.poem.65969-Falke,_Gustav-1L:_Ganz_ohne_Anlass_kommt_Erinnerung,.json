{"textgrid.poem.65969": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ganz ohne Anlass kommt Erinnerung,", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ganz ohne Anlass kommt Erinnerung,", "tokens": ["Ganz", "oh\u00b7ne", "An\u00b7lass", "kommt", "E\u00b7rin\u00b7ne\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Wie aus des Himmels weitem, leerem Blau", "tokens": ["Wie", "aus", "des", "Him\u00b7mels", "wei\u00b7tem", ",", "lee\u00b7rem", "Blau"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Versch\u00e4mt ein rosig Sommerw\u00f6lkchen taucht:", "tokens": ["Ver\u00b7sch\u00e4mt", "ein", "ro\u00b7sig", "Som\u00b7mer\u00b7w\u00f6lk\u00b7chen", "taucht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Still lag der Wald, still lagen Feld und Weg,", "tokens": ["Still", "lag", "der", "Wald", ",", "still", "la\u00b7gen", "Feld", "und", "Weg", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$,", "ADJD", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dar\u00fcber schon sein Sternentuch der Abend", "tokens": ["Da\u00b7r\u00fc\u00b7ber", "schon", "sein", "Ster\u00b7nen\u00b7tuch", "der", "A\u00b7bend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "ADV", "PPOSAT", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Von einem Ende bis zum andern spannte.", "tokens": ["Von", "ei\u00b7nem", "En\u00b7de", "bis", "zum", "an\u00b7dern", "spann\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "APPRART", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Kein Hauch, kein Laut. Nur aus der Ferne manchmal,", "tokens": ["Kein", "Hauch", ",", "kein", "Laut", ".", "Nur", "aus", "der", "Fer\u00b7ne", "manch\u00b7mal", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "$.", "ADV", "APPR", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Weit hinter uns, das ganz ged\u00e4mpfte Lachen", "tokens": ["Weit", "hin\u00b7ter", "uns", ",", "das", "ganz", "ge\u00b7d\u00e4mpf\u00b7te", "La\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "PPER", "$,", "PRELS", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Zur\u00fcckgebliebener trunkener Genossen.", "tokens": ["Zu\u00b7r\u00fcck\u00b7ge\u00b7blie\u00b7be\u00b7ner", "trun\u00b7ke\u00b7ner", "Ge\u00b7nos\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$."], "meter": "-+-+--+---+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Zwei, drei der P\u00e4rchen vor uns, weit voraus,", "tokens": ["Zwei", ",", "drei", "der", "P\u00e4r\u00b7chen", "vor", "uns", ",", "weit", "vo\u00b7raus", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["CARD", "$,", "CARD", "ART", "NN", "APPR", "PPER", "$,", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Denn eine schmale, schwarze Wetterwand", "tokens": ["Denn", "ei\u00b7ne", "schma\u00b7le", ",", "schwar\u00b7ze", "Wet\u00b7ter\u00b7wand"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Am Horizont trieb \u00c4ngstliche zur Eile.", "tokens": ["Am", "Ho\u00b7ri\u00b7zont", "trieb", "\u00c4ngst\u00b7li\u00b7che", "zur", "Ei\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Und wir allein so zwischen Wald und Feld", "tokens": ["Und", "wir", "al\u00b7lein", "so", "zwi\u00b7schen", "Wald", "und", "Feld"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ADV", "ADV", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Und schweigsam wie das Schweigen um uns her.", "tokens": ["Und", "schweig\u00b7sam", "wie", "das", "Schwei\u00b7gen", "um", "uns", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KOKOM", "ART", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Da murrte leise \u00fcbers Feld, ganz leise", "tokens": ["Da", "murr\u00b7te", "lei\u00b7se", "\u00fc\u00b7bers", "Feld", ",", "ganz", "lei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD", "APPRART", "NN", "$,", "ADV", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der erste Donner und erschrocken schmiegtest", "tokens": ["Der", "ers\u00b7te", "Don\u00b7ner", "und", "er\u00b7schro\u00b7cken", "schmieg\u00b7test"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "VVINF", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du n\u00e4her dich mit sanftem Druck mir an.", "tokens": ["Du", "n\u00e4\u00b7her", "dich", "mit", "sanf\u00b7tem", "Druck", "mir", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PRF", "APPR", "ADJA", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und wie ein Zittern lief's von deinem Arm", "tokens": ["Und", "wie", "ein", "Zit\u00b7tern", "lie\u00b7f's", "von", "dei\u00b7nem", "Arm"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "In meinen \u00fcber, und mein Herz schlug schneller.", "tokens": ["In", "mei\u00b7nen", "\u00fc\u00b7ber", ",", "und", "mein", "Herz", "schlug", "schnel\u00b7ler", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "APPR", "$,", "KON", "PPOSAT", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und wieder \u00fcbers Feld das leise Murren,", "tokens": ["Und", "wie\u00b7der", "\u00fc\u00b7bers", "Feld", "das", "lei\u00b7se", "Mur\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein kurzer Blick, halb schreckhaft, halb versch\u00e4mt", "tokens": ["Ein", "kur\u00b7zer", "Blick", ",", "halb", "schreck\u00b7haft", ",", "halb", "ver\u00b7sch\u00e4mt"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ADJD", "ADJD", "$,", "ADJD", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So voller r\u00fchrend scheuer Kinderangst", "tokens": ["So", "vol\u00b7ler", "r\u00fch\u00b7rend", "scheu\u00b7er", "Kin\u00b7de\u00b7rangst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVPP", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Traf mich aus deinen gro\u00dfen blauen Augen", "tokens": ["Traf", "mich", "aus", "dei\u00b7nen", "gro\u00b7\u00dfen", "blau\u00b7en", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und fragte deutlich: Find' ich Schutz bei dir?", "tokens": ["Und", "frag\u00b7te", "deut\u00b7lich", ":", "Find'", "ich", "Schutz", "bei", "dir", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$.", "VVFIN", "PPER", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "\u00bbso \u00e4ngstlich, Fr\u00e4ulein?\u00ab neckte ich und dr\u00fcckte", "tokens": ["\u00bb", "so", "\u00e4ngst\u00b7lich", ",", "Fr\u00e4u\u00b7lein", "?", "\u00ab", "neck\u00b7te", "ich", "und", "dr\u00fcck\u00b7te"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["$(", "ADV", "ADJD", "$,", "NN", "$.", "$(", "VVFIN", "PPER", "KON", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie zur Beruhigung die kleine Hand", "tokens": ["Wie", "zur", "Be\u00b7ru\u00b7hi\u00b7gung", "die", "klei\u00b7ne", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Und hielt sie fest, und spielte mit den Fingern", "tokens": ["Und", "hielt", "sie", "fest", ",", "und", "spiel\u00b7te", "mit", "den", "Fin\u00b7gern"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und f\u00fchlte durch den Seidenzwirn des Handschuhs", "tokens": ["Und", "f\u00fchl\u00b7te", "durch", "den", "Sei\u00b7denz\u00b7wirn", "des", "Hand\u00b7schuhs"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Das warme, junge warme Leben pulsen.", "tokens": ["Das", "war\u00b7me", ",", "jun\u00b7ge", "war\u00b7me", "Le\u00b7ben", "pul\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Und wieder \u00fcbers Feld ein Murren, lauter", "tokens": ["Und", "wie\u00b7der", "\u00fc\u00b7bers", "Feld", "ein", "Mur\u00b7ren", ",", "lau\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "ADV", "APPRART", "NN", "ART", "NN", "$,", "PIAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und l\u00e4nger wie zuvor, und wieder drauf", "tokens": ["Und", "l\u00e4n\u00b7ger", "wie", "zu\u00b7vor", ",", "und", "wie\u00b7der", "drauf"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJD", "KOKOM", "ADV", "$,", "KON", "ADV", "PAV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dein sanftes taubenscheues Anmichschmiegen.", "tokens": ["Dein", "sanf\u00b7tes", "tau\u00b7ben\u00b7scheu\u00b7es", "An\u00b7mich\u00b7schmie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "War's die Gewissheit eines leichten Sieges?", "tokens": ["Wa\u00b7r's", "die", "Ge\u00b7wiss\u00b7heit", "ei\u00b7nes", "leich\u00b7ten", "Sie\u00b7ges", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Weit breitete die Leidenschaft auf einmal", "tokens": ["Weit", "brei\u00b7te\u00b7te", "die", "Lei\u00b7den\u00b7schaft", "auf", "ein\u00b7mal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPR", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die starken Schwingen, und ein Falke stand", "tokens": ["Die", "star\u00b7ken", "Schwin\u00b7gen", ",", "und", "ein", "Fal\u00b7ke", "stand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sekunden sie, ganz Auge, ganz Begierde,", "tokens": ["Se\u00b7kun\u00b7den", "sie", ",", "ganz", "Au\u00b7ge", ",", "ganz", "Be\u00b7gier\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "ADV", "NN", "$,", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Sto\u00dfsicher \u00fcber ihrem scheuen Opfer.", "tokens": ["Sto\u00df\u00b7si\u00b7cher", "\u00fc\u00b7ber", "ih\u00b7rem", "scheu\u00b7en", "Op\u00b7fer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.8": {"line.1": {"text": "Da brach in j\u00e4hem flirrendem Zickzacklauf", "tokens": ["Da", "brach", "in", "j\u00e4\u00b7hem", "flir\u00b7ren\u00b7dem", "Zick\u00b7zack\u00b7lauf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der erste Blitz aus seiner dunklen Burg.", "tokens": ["Der", "ers\u00b7te", "Blitz", "aus", "sei\u00b7ner", "dunk\u00b7len", "Burg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Erschrocken sank mir der erhobene Arm,", "tokens": ["Er\u00b7schro\u00b7cken", "sank", "mir", "der", "er\u00b7ho\u00b7be\u00b7ne", "Arm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Der schulternah zum Kuss dich schon umfasste.", "tokens": ["Der", "schul\u00b7ter\u00b7nah", "zum", "Kuss", "dich", "schon", "um\u00b7fass\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPRART", "NN", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die ersten schweren, gro\u00dfen Tropfen fielen,", "tokens": ["Die", "ers\u00b7ten", "schwe\u00b7ren", ",", "gro\u00b7\u00dfen", "Trop\u00b7fen", "fie\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVINF", "$,", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und hinter uns in Eile nahten sich", "tokens": ["Und", "hin\u00b7ter", "uns", "in", "Ei\u00b7le", "nah\u00b7ten", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "APPR", "NN", "VVFIN", "PRF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Die aufgeschreckten trunkenen Genossen", "tokens": ["Die", "auf\u00b7ge\u00b7schreck\u00b7ten", "trun\u00b7ke\u00b7nen", "Ge\u00b7nos\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und mischten ihr Gejohle in das Grollen", "tokens": ["Und", "mischten", "ihr", "Ge\u00b7joh\u00b7le", "in", "das", "Grol\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Des Donners, der im Walde fern erstarb. \u2013", "tokens": ["Des", "Don\u00b7ners", ",", "der", "im", "Wal\u00b7de", "fern", "er\u00b7starb", ".", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN", "ADJD", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Ohn' Anlass kam mir die Erinnerung,", "tokens": ["Ohn'", "An\u00b7lass", "kam", "mir", "die", "E\u00b7rin\u00b7ne\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie aus des Himmels weitem leerem Blau", "tokens": ["Wie", "aus", "des", "Him\u00b7mels", "wei\u00b7tem", "lee\u00b7rem", "Blau"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Versch\u00e4mt ein rosig Sommerw\u00f6lkchen taucht.", "tokens": ["Ver\u00b7sch\u00e4mt", "ein", "ro\u00b7sig", "Som\u00b7mer\u00b7w\u00f6lk\u00b7chen", "taucht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}