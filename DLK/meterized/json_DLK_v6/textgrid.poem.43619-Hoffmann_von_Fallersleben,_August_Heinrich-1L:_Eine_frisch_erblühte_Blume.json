{"textgrid.poem.43619": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Eine frisch erbl\u00fchte Blume", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eine frisch erbl\u00fchte Blume", "tokens": ["Ei\u00b7ne", "frisch", "er\u00b7bl\u00fch\u00b7te", "Blu\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fand ihr Leben gar gering,", "tokens": ["Fand", "ihr", "Le\u00b7ben", "gar", "ge\u00b7ring", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und sie sah sich um und w\u00fcnschte:", "tokens": ["Und", "sie", "sah", "sich", "um", "und", "w\u00fcnschte", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "APPR", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "W\u00e4r' ich doch ein Schmetterling!", "tokens": ["W\u00e4r'", "ich", "doch", "ein", "Schmet\u00b7ter\u00b7ling", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nicht gebannt an diesen Boden", "tokens": ["Nicht", "ge\u00b7bannt", "an", "die\u00b7sen", "Bo\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADJD", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Z\u00f6g' ich frei durch Wies' und Feld;", "tokens": ["Z\u00f6g'", "ich", "frei", "durch", "Wies'", "und", "Feld", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Mir geh\u00f6rte Erd' und Himmel,", "tokens": ["Mir", "ge\u00b7h\u00f6r\u00b7te", "Erd'", "und", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ja, die ganze weite Welt! \u2013", "tokens": ["Ja", ",", "die", "gan\u00b7ze", "wei\u00b7te", "Welt", "!", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PTKANT", "$,", "ART", "ADJA", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Als sie kaum das Wort gesprochen,", "tokens": ["Als", "sie", "kaum", "das", "Wort", "ge\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kam ein Schmetterling herzu,", "tokens": ["Kam", "ein", "Schmet\u00b7ter\u00b7ling", "her\u00b7zu", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und er sprach: o sch\u00f6ne Blume,", "tokens": ["Und", "er", "sprach", ":", "o", "sch\u00f6\u00b7ne", "Blu\u00b7me", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$.", "FM", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00e4tt' ich doch ein Loos wie du!", "tokens": ["H\u00e4tt'", "ich", "doch", "ein", "Loos", "wie", "du", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "KOKOM", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "In der Hut der Menschen lebst du", "tokens": ["In", "der", "Hut", "der", "Men\u00b7schen", "lebst", "du"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "PPER"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "Ruhig deine Tage hin,", "tokens": ["Ru\u00b7hig", "dei\u00b7ne", "Ta\u00b7ge", "hin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "W\u00e4hrend ich ein armer Fl\u00fcchtling", "tokens": ["W\u00e4h\u00b7rend", "ich", "ein", "ar\u00b7mer", "Fl\u00fccht\u00b7ling"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Auf der sch\u00f6nen Erde bin. \u2013", "tokens": ["Auf", "der", "sch\u00f6\u00b7nen", "Er\u00b7de", "bin", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VAFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und erf\u00fcllet ward ihr W\u00fcnschen,", "tokens": ["Und", "er\u00b7f\u00fcl\u00b7let", "ward", "ihr", "W\u00fcn\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ehe kaum ein Jahr verging:", "tokens": ["E\u00b7he", "kaum", "ein", "Jahr", "ver\u00b7ging", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schmetterling ward eine Blume,", "tokens": ["Schmet\u00b7ter\u00b7ling", "ward", "ei\u00b7ne", "Blu\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die Blum' ein Schmetterling.", "tokens": ["Und", "die", "Blum'", "ein", "Schmet\u00b7ter\u00b7ling", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und da h\u00f6rt' ich beide wieder,", "tokens": ["Und", "da", "h\u00f6rt'", "ich", "bei\u00b7de", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PIS", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Als ich just im Garten ging \u2013", "tokens": ["Als", "ich", "just", "im", "Gar\u00b7ten", "ging", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Schmetterling sprach: w\u00e4r' ich Blume!", "tokens": ["Schmet\u00b7ter\u00b7ling", "sprach", ":", "w\u00e4r'", "ich", "Blu\u00b7me", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$.", "VAFIN", "PPER", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Blume: w\u00e4r' ich Schmetterling!", "tokens": ["Blu\u00b7me", ":", "w\u00e4r'", "ich", "Schmet\u00b7ter\u00b7ling", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VAFIN", "PPER", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}