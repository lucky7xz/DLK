{"textgrid.poem.47266": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "3. Schwalbengru\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Schwalbe kam geflogen;", "tokens": ["Die", "Schwal\u00b7be", "kam", "ge\u00b7flo\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Kaum hatt' ich sie gesehn,", "tokens": ["Kaum", "hatt'", "ich", "sie", "ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So ist sie weggezogen", "tokens": ["So", "ist", "sie", "weg\u00b7ge\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In rauher L\u00fcfte Weh'n.", "tokens": ["In", "rau\u00b7her", "L\u00fcf\u00b7te", "Weh'", "n."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["APPR", "ADJA", "NN", "NN", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sie gr\u00fc\u00dfte mich verstohlen,", "tokens": ["Sie", "gr\u00fc\u00df\u00b7te", "mich", "ver\u00b7stoh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie soll ich es verstehn?", "tokens": ["Wie", "soll", "ich", "es", "ver\u00b7stehn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es klang wie \u00bbGott befohlen\u00ab,", "tokens": ["Es", "klang", "wie", "\u00bb", "Gott", "be\u00b7foh\u00b7len", "\u00ab", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "$(", "NN", "VVPP", "$(", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nicht wie \u00bbauf Wiedersehn!\u00ab", "tokens": ["Nicht", "wie", "\u00bb", "auf", "Wie\u00b7der\u00b7sehn", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PTKNEG", "KOKOM", "$(", "APPR", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}