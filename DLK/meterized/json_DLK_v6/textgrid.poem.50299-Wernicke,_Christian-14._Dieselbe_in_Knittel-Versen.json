{"textgrid.poem.50299": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "14. Dieselbe in Knittel-Versen", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein Mann, nach dem wir Gestern greulich", "tokens": ["Dein", "Mann", ",", "nach", "dem", "wir", "Ge\u00b7stern", "greu\u00b7lich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "APPR", "PRELS", "PPER", "NN", "ADJD"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Gesoffen, wurde sehr vertr\u00e4ulich;", "tokens": ["Ge\u00b7sof\u00b7fen", ",", "wur\u00b7de", "sehr", "ver\u00b7tr\u00e4u\u00b7lich", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und sagte mir im truncknen Muht", "tokens": ["Und", "sag\u00b7te", "mir", "im", "trunck\u00b7nen", "Muht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er sagt', er wolte nicht viel fluchen,", "tokens": ["Er", "sagt'", ",", "er", "wol\u00b7te", "nicht", "viel", "flu\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VMFIN", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ich m\u00f6cht' es selbst mit dir versuchen,", "tokens": ["Ich", "m\u00f6cht'", "es", "selbst", "mit", "dir", "ver\u00b7su\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wo ich mich nur ", "tokens": ["Wo", "ich", "mich", "nur"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PRF", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Und bracht' es mir auf gutes Gl\u00fcck.", "tokens": ["Und", "bracht'", "es", "mir", "auf", "gu\u00b7tes", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Nun thuts mein Weib zwar ungebeten;", "tokens": ["Nun", "thuts", "mein", "Weib", "zwar", "un\u00b7ge\u00b7be\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Doch willst du ihre Stell' antreten,", "tokens": ["Doch", "willst", "du", "ih\u00b7re", "Stell'", "an\u00b7tre\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "So geb' ich dir, hier sag' ichs zu,", "tokens": ["So", "geb'", "ich", "dir", ",", "hier", "sag'", "ichs", "zu", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$,", "ADV", "VVFIN", "PIS", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}