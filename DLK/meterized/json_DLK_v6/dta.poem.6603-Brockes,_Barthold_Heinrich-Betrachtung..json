{"dta.poem.6603": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Man senckt sich in den Grund der tieffen See hinein.", "tokens": ["Man", "senckt", "sich", "in", "den", "Grund", "der", "tief\u00b7fen", "See", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "APPR", "ART", "NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ja man beg\u00e4be sich gern nach den schwartzen Schwellen", "tokens": ["Ja", "man", "be\u00b7g\u00e4\u00b7be", "sich", "gern", "nach", "den", "schwart\u00b7zen", "Schwel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "PIS", "VVFIN", "PRF", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Des fiustern Abgrunds, nach der H\u00f6llen,", "tokens": ["Des", "fius\u00b7tern", "Ab\u00b7grunds", ",", "nach", "der", "H\u00f6l\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Um dieses Schein-Gut, hin. Sich dessen zu erfreuen", "tokens": ["Um", "die\u00b7ses", "Schein\u00b7Gut", ",", "hin", ".", "Sich", "des\u00b7sen", "zu", "er\u00b7freu\u00b7en"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUI", "PDAT", "NN", "$,", "PTKVZ", "$.", "PRF", "PDS", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sieht man Lust, Stoltz und Geitz, noch M\u00fch\u2019 noch Laster", "tokens": ["Sieht", "man", "Lust", ",", "Stoltz", "und", "Geitz", ",", "noch", "M\u00fch'", "noch", "Las\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "NN", "$,", "NN", "KON", "NN", "$,", "ADV", "NN", "ADV", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "scheuen.", "tokens": ["scheu\u00b7en", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}