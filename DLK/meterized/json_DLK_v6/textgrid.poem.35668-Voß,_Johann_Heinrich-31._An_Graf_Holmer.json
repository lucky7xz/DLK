{"textgrid.poem.35668": {"metadata": {"author": {"name": "Vo\u00df, Johann Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "31. An Graf Holmer", "genre": "verse", "period": "N.A.", "pub_year": 1783, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie der S\u00e4nger des Hains in dem K\u00e4ficht, unter dem Maibusch,", "tokens": ["Wie", "der", "S\u00e4n\u00b7ger", "des", "Hains", "in", "dem", "K\u00e4\u00b7ficht", ",", "un\u00b7ter", "dem", "Mai\u00b7busch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Welchen die Tochter des Herrn sorgsam im Topfe gepflegt,", "tokens": ["Wel\u00b7chen", "die", "Toch\u00b7ter", "des", "Herrn", "sorg\u00b7sam", "im", "Top\u00b7fe", "ge\u00b7pflegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "ADJD", "APPRART", "NN", "VVPP", "$,"], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Um mit fr\u00fcherem Laube des Lieblings Haus zu beschatten,", "tokens": ["Um", "mit", "fr\u00fc\u00b7he\u00b7rem", "Lau\u00b7be", "des", "Lieb\u00b7lings", "Haus", "zu", "be\u00b7schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "APPR", "ADJA", "NN", "ART", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Froher des Sonnenscheins, h\u00fcpft und melodischer singt:", "tokens": ["Fro\u00b7her", "des", "Son\u00b7nen\u00b7scheins", ",", "h\u00fcpft", "und", "me\u00b7lo\u00b7di\u00b7scher", "singt", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "VVFIN", "KON", "ADJD", "VVFIN", "$."], "meter": "+--+--+-+-+-+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Kl\u00f6sterlich schwermutsvoll im Ofendunst an dem Fenster,", "tokens": ["Kl\u00f6s\u00b7ter\u00b7lich", "schwer\u00b7muts\u00b7voll", "im", "O\u00b7fen\u00b7dunst", "an", "dem", "Fens\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "APPRART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}, "line.6": {"text": "Welches von Nachtfrost blinkt', oder von Hagel und Sturm", "tokens": ["Wel\u00b7ches", "von", "Nacht\u00b7frost", "blinkt'", ",", "o\u00b7der", "von", "Ha\u00b7gel", "und", "Sturm"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "NN", "VVFIN", "$,", "KON", "APPR", "NN", "KON", "NN"], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}, "line.7": {"text": "Rasselte, sa\u00df er bisher mit strupfigter Schwinge, des Sommers", "tokens": ["Ras\u00b7sel\u00b7te", ",", "sa\u00df", "er", "bis\u00b7her", "mit", "stru\u00b7pfig\u00b7ter", "Schwin\u00b7ge", ",", "des", "Som\u00b7mers"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "$,", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$,", "ART", "NN"], "meter": "+--+-+-+-+-+--+-", "measure": "iambic.septa.invert"}, "line.8": {"text": "Eingedenk, da er frei W\u00e4lder und Auen durchflog;", "tokens": ["Ein\u00b7ge\u00b7denk", ",", "da", "er", "frei", "W\u00e4l\u00b7der", "und", "Au\u00b7en", "durch\u00b7flog", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "ADJD", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+---+--+--+", "measure": "trochaic.penta.relaxed"}, "line.9": {"text": "Aber nun h\u00fcpft er und singt vor dem offenen Fenster des Gartens,", "tokens": ["A\u00b7ber", "nun", "h\u00fcpft", "er", "und", "singt", "vor", "dem", "of\u00b7fe\u00b7nen", "Fens\u00b7ter", "des", "Gar\u00b7tens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.10": {"text": "Froher des Sonnenscheins, unter dem schimmernden Gr\u00fcn,", "tokens": ["Fro\u00b7her", "des", "Son\u00b7nen\u00b7scheins", ",", "un\u00b7ter", "dem", "schim\u00b7mern\u00b7den", "Gr\u00fcn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.11": {"text": "Da\u00df vor dem hellen Gesange die Jungfrau l\u00e4chelnd am N\u00e4hpult", "tokens": ["Da\u00df", "vor", "dem", "hel\u00b7len", "Ge\u00b7san\u00b7ge", "die", "Jung\u00b7frau", "l\u00e4\u00b7chelnd", "am", "N\u00e4h\u00b7pult"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN", "ART", "NN", "ADJD", "APPRART", "NN"], "meter": "---+--+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Sich ihr gellendes Ohr schirmet, und Ruh' ihm gebeut:", "tokens": ["Sich", "ihr", "gel\u00b7len\u00b7des", "Ohr", "schir\u00b7met", ",", "und", "Ruh'", "ihm", "ge\u00b7beut", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PPOSAT", "ADJA", "NN", "VVFIN", "$,", "KON", "NN", "PPER", "VVFIN", "$."], "meter": "--+---+--+--+", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Also freut sich der Dichter, der, lange verscheucht, sein umgr\u00fcntes", "tokens": ["Al\u00b7so", "freut", "sich", "der", "Dich\u00b7ter", ",", "der", ",", "lan\u00b7ge", "ver\u00b7scheucht", ",", "sein", "um\u00b7gr\u00fcn\u00b7tes"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "$,", "PRELS", "$,", "ADV", "VVPP", "$,", "PPOSAT", "ADJA"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.14": {"text": "Einsames Gartenhaus endlich in Friede bewohnt,", "tokens": ["Ein\u00b7sa\u00b7mes", "Gar\u00b7ten\u00b7haus", "end\u00b7lich", "in", "Frie\u00b7de", "be\u00b7wohnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "NN", "VVPP", "$,"], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.15": {"text": "Und aus traulicher Kammer, von Mond und Sonne beleuchtet,", "tokens": ["Und", "aus", "trau\u00b7li\u00b7cher", "Kam\u00b7mer", ",", "von", "Mond", "und", "Son\u00b7ne", "be\u00b7leuch\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "$,", "APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.16": {"text": "Garten und Insel und See, H\u00fcgel und W\u00e4lder umschaut.", "tokens": ["Gar\u00b7ten", "und", "In\u00b7sel", "und", "See", ",", "H\u00fc\u00b7gel", "und", "W\u00e4l\u00b7der", "um\u00b7schaut", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "$,", "NN", "KON", "NN", "VVPP", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.17": {"text": "Immer durchschw\u00e4rmt sein Blick die Gegenden: oft wie die Biene,", "tokens": ["Im\u00b7mer", "durch\u00b7schw\u00e4rmt", "sein", "Blick", "die", "Ge\u00b7gen\u00b7den", ":", "oft", "wie", "die", "Bie\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ART", "NN", "$.", "ADV", "KOKOM", "ART", "NN", "$,"], "meter": "+--+-+-+--+--+-", "measure": "hexameter"}, "line.18": {"text": "Welche Blumen umirrt, und bei den s\u00fc\u00dferen weilt;", "tokens": ["Wel\u00b7che", "Blu\u00b7men", "um\u00b7irrt", ",", "und", "bei", "den", "s\u00fc\u00b7\u00dfe\u00b7ren", "weilt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "VVPP", "$,", "KON", "APPR", "ART", "ADJA", "VVFIN", "$."], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.19": {"text": "St\u00fcrmend oft und entz\u00fcckt, wie der Adler Zeus, da er Nektar", "tokens": ["St\u00fcr\u00b7mend", "oft", "und", "ent\u00b7z\u00fcckt", ",", "wie", "der", "Ad\u00b7ler", "Zeus", ",", "da", "er", "Nek\u00b7tar"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "ADV", "KON", "VVPP", "$,", "PWAV", "ART", "NN", "NE", "$,", "KOUS", "PPER", "NN"], "meter": "+-+--+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.20": {"text": "Und Ambrosia einst aus der elysischen Flur", "tokens": ["Und", "A\u00b7mbro\u00b7sia", "einst", "aus", "der", "e\u00b7ly\u00b7si\u00b7schen", "Flur"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NE", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Brachte, dem Knaben zur Kost, der ein k\u00fcnftiger Herrscher des Donners,", "tokens": ["Brach\u00b7te", ",", "dem", "Kna\u00b7ben", "zur", "Kost", ",", "der", "ein", "k\u00fcnf\u00b7ti\u00b7ger", "Herr\u00b7scher", "des", "Don\u00b7ners", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ART", "NN", "APPRART", "NN", "$,", "PRELS", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.22": {"text": "Unter der Grott' im Glanz seiner Unsterblichkeit schlief.", "tokens": ["Un\u00b7ter", "der", "Grott'", "im", "Glanz", "sei\u00b7ner", "U\u00b7nsterb\u00b7lich\u00b7keit", "schlief", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}, "line.23": {"text": "Heil mir! ich zittre vor Wonn'! Ist es Wirklichkeit oder Erscheinung?", "tokens": ["Heil", "mir", "!", "ich", "zitt\u00b7re", "vor", "Wonn'", "!", "Ist", "es", "Wirk\u00b7lich\u00b7keit", "o\u00b7der", "Er\u00b7schei\u00b7nung", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "PPER", "VVFIN", "APPR", "NN", "$.", "VAFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "+--+--+--+-++--+-", "measure": "dactylic.tri.plus"}, "line.24": {"text": "Meine Stimme, wie hell! flie\u00dfet von selbst in Gesang!", "tokens": ["Mei\u00b7ne", "Stim\u00b7me", ",", "wie", "hell", "!", "flie\u00b7\u00dfet", "von", "selbst", "in", "Ge\u00b7sang", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PWAV", "ADJD", "$.", "VVFIN", "APPR", "ADV", "APPR", "NN", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.25": {"text": "Welchen unsterblichen Namen verk\u00fcndet der Welt und der Nachwelt", "tokens": ["Wel\u00b7chen", "uns\u00b7terb\u00b7li\u00b7chen", "Na\u00b7men", "ver\u00b7k\u00fcn\u00b7det", "der", "Welt", "und", "der", "Nach\u00b7welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAT", "ADJA", "NN", "VVFIN", "ART", "NN", "KON", "ART", "NN"], "meter": "+--+--+--+--+-+-+", "measure": "dactylic.tetra.plus"}, "line.26": {"text": "Mein Gesang? Wer schuf diese Gefild' um mich her?", "tokens": ["Mein", "Ge\u00b7sang", "?", "Wer", "schuf", "die\u00b7se", "Ge\u00b7fild'", "um", "mich", "her", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "PWS", "VVFIN", "PDAT", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "--+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.27": {"text": "Bin ich dem Markt' entflohn, und dem ringsumrasselten Rathaus?", "tokens": ["Bin", "ich", "dem", "Markt'", "ent\u00b7flohn", ",", "und", "dem", "rings\u00b7um\u00b7ras\u00b7sel\u00b7ten", "Rat\u00b7haus", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "PTKVZ", "$,", "KON", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.28": {"text": "Schreckt mich nicht mehr des Gerichts, oder der Gilden Tumult?", "tokens": ["Schreckt", "mich", "nicht", "mehr", "des", "Ge\u00b7richts", ",", "o\u00b7der", "der", "Gil\u00b7den", "Tu\u00b7mult", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADV", "ART", "NN", "$,", "KON", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-+-+-", "measure": "elegiambus"}, "line.29": {"text": "Nicht der Senatorschmaus, der, vom dr\u00e4ngenden P\u00f6bel bewundert,", "tokens": ["Nicht", "der", "Se\u00b7na\u00b7tor\u00b7schmaus", ",", "der", ",", "vom", "dr\u00e4n\u00b7gen\u00b7den", "P\u00f6\u00b7bel", "be\u00b7wun\u00b7dert", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "$,", "PRELS", "$,", "APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.30": {"text": "Laut in den Wiegengesang, \u00fcber der W\u00f6chnerin, tobt?", "tokens": ["Laut", "in", "den", "Wie\u00b7gen\u00b7ge\u00b7sang", ",", "\u00fc\u00b7ber", "der", "W\u00f6ch\u00b7ne\u00b7rin", ",", "tobt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$,", "VVFIN", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.31": {"text": "Nicht anwohnender Schergen Besuch, noch des B\u00fcrgergehorsams", "tokens": ["Nicht", "an\u00b7woh\u00b7nen\u00b7der", "Scher\u00b7gen", "Be\u00b7such", ",", "noch", "des", "B\u00fcr\u00b7ger\u00b7ge\u00b7hor\u00b7sams"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "ADJA", "NN", "NN", "$,", "ADV", "ART", "NN"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.32": {"text": "N\u00e4chtlicher L\u00e4rm? nicht mehr kreischender Buben Gew\u00fchl,", "tokens": ["N\u00e4cht\u00b7li\u00b7cher", "L\u00e4rm", "?", "nicht", "mehr", "krei\u00b7schen\u00b7der", "Bu\u00b7ben", "Ge\u00b7w\u00fchl", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "PTKNEG", "ADV", "ADJA", "NN", "NN", "$,"], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.33": {"text": "Zankender Kauf und Verkauf, und des Fuhrmanns Fluch, und der R\u00e4der", "tokens": ["Zan\u00b7ken\u00b7der", "Kauf", "und", "Ver\u00b7kauf", ",", "und", "des", "Fuhr\u00b7manns", "Fluch", ",", "und", "der", "R\u00e4\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "NN", "$,", "KON", "ART", "NN", "NN", "$,", "KON", "ART", "NN"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.34": {"text": "Rollen, die knallende Peitsch', oder der Hunde Gebell?", "tokens": ["Rol\u00b7len", ",", "die", "knal\u00b7len\u00b7de", "Peit\u00b7sch'", ",", "o\u00b7der", "der", "Hun\u00b7de", "Ge\u00b7bell", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "ADJA", "NN", "$,", "KON", "ART", "NN", "NN", "$."], "meter": "+--+--+--+-+--+", "measure": "dactylic.tri.plus"}, "line.35": {"text": "Noch der Gr\u00e4uel des Marktes, der gotische Pranger, des Galgens", "tokens": ["Noch", "der", "Gr\u00e4u\u00b7el", "des", "Mark\u00b7tes", ",", "der", "go\u00b7ti\u00b7sche", "Pran\u00b7ger", ",", "des", "Gal\u00b7gens"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ART", "NN", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,", "ART", "NN"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.36": {"text": "Bruder! zum Schaugepr\u00e4ng' hoch auf den H\u00fcgel gepflanzt?", "tokens": ["Bru\u00b7der", "!", "zum", "Schau\u00b7ge\u00b7pr\u00e4ng'", "hoch", "auf", "den", "H\u00fc\u00b7gel", "ge\u00b7pflanzt", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "APPRART", "NN", "ADJD", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.37": {"text": "Jetzo st\u00f6rt mich nur etwa die Nachtigall fern am beb\u00fcschten", "tokens": ["Jet\u00b7zo", "st\u00f6rt", "mich", "nur", "et\u00b7wa", "die", "Nach\u00b7ti\u00b7gall", "fern", "am", "be\u00b7b\u00fcschten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "ART", "NN", "ADJD", "APPRART", "ADJA"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.38": {"text": "See, die Schwalb' am Gesims', oder das purpurne Licht,", "tokens": ["See", ",", "die", "Schwalb'", "am", "Ge\u00b7sims'", ",", "o\u00b7der", "das", "pur\u00b7pur\u00b7ne", "Licht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "APPRART", "NN", "$,", "KON", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.39": {"text": "Welches durch wankende Rosen und Pfirsiche sanft in die Fenster", "tokens": ["Wel\u00b7ches", "durch", "wan\u00b7ken\u00b7de", "Ro\u00b7sen", "und", "Pfir\u00b7si\u00b7che", "sanft", "in", "die", "Fens\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "ADJA", "NN", "KON", "NN", "ADJD", "APPR", "ART", "NN"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.40": {"text": "Meines K\u00e4mmerleins schl\u00fcpft, und aus dem Traume mich weckt.", "tokens": ["Mei\u00b7nes", "K\u00e4m\u00b7mer\u00b7leins", "schl\u00fcpft", ",", "und", "aus", "dem", "Trau\u00b7me", "mich", "weckt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "KON", "APPR", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-++-+--+", "measure": "iambic.septa.chol"}, "line.41": {"text": "Oder, wandl' ich durch Blumen, von duftender Bl\u00fcte beschattet,", "tokens": ["O\u00b7der", ",", "wandl'", "ich", "durch", "Blu\u00b7men", ",", "von", "duf\u00b7ten\u00b7der", "Bl\u00fc\u00b7te", "be\u00b7schat\u00b7tet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "PPER", "APPR", "NN", "$,", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.42": {"text": "Denkend einher, dann umsumst etwa ein Bienchen mein Haupt;", "tokens": ["Den\u00b7kend", "ein\u00b7her", ",", "dann", "um\u00b7sumst", "et\u00b7wa", "ein", "Bien\u00b7chen", "mein", "Haupt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$,", "ADV", "VVFIN", "ADV", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.43": {"text": "Oder die Taube vom Dach ums\u00e4uselt mich; oder ein Sperling", "tokens": ["O\u00b7der", "die", "Tau\u00b7be", "vom", "Dach", "um\u00b7s\u00e4u\u00b7selt", "mich", ";", "o\u00b7der", "ein", "Sper\u00b7ling"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPRART", "NN", "VVFIN", "PPER", "$.", "KON", "ART", "NN"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.44": {"text": "Schwirrt aus dem Kirschenbaum, schwirrt aus den Erbsen empor.", "tokens": ["Schwirrt", "aus", "dem", "Kir\u00b7schen\u00b7baum", ",", "schwirrt", "aus", "den", "Erb\u00b7sen", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.45": {"text": "Oft auch, wann ich, beschirmt vor dem Mittag, unter dem Fruchtbaum", "tokens": ["Oft", "auch", ",", "wann", "ich", ",", "be\u00b7schirmt", "vor", "dem", "Mit\u00b7tag", ",", "un\u00b7ter", "dem", "Frucht\u00b7baum"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "PWAV", "PPER", "$,", "VVFIN", "APPR", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.46": {"text": "Lieg', und starrend mein Blick W\u00fcrmer im Grase verfolgt,", "tokens": ["Lieg'", ",", "und", "star\u00b7rend", "mein", "Blick", "W\u00fcr\u00b7mer", "im", "Gra\u00b7se", "ver\u00b7folgt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "ADJD", "PPOSAT", "NN", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.47": {"text": "Schreckt mich ein fallender Apfel zur Seit', und der gr\u00fcnliche Laubfrosch,", "tokens": ["Schreckt", "mich", "ein", "fal\u00b7len\u00b7der", "Ap\u00b7fel", "zur", "Seit'", ",", "und", "der", "gr\u00fcn\u00b7li\u00b7che", "Laub\u00b7fro\u00b7sch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "APPRART", "NN", "$,", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+--+--+-+", "measure": "iambic.septa.relaxed"}, "line.48": {"text": "Der im Johannsbeerbusch quakend den Regen erseufzt.", "tokens": ["Der", "im", "Jo\u00b7hanns\u00b7be\u00b7er\u00b7busch", "qua\u00b7kend", "den", "Re\u00b7gen", "er\u00b7seufzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "VVPP", "ART", "NN", "VVPP", "$."], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}, "line.49": {"text": "Oder wenn ich am pl\u00e4tschernden See, in der Linden Umschattung,", "tokens": ["O\u00b7der", "wenn", "ich", "am", "pl\u00e4t\u00b7schern\u00b7den", "See", ",", "in", "der", "Lin\u00b7den", "Um\u00b7schat\u00b7tung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPRART", "ADJA", "NN", "$,", "APPR", "ART", "NE", "NN", "$,"], "meter": "-+-+-+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.50": {"text": "Sinnend die Wellen z\u00e4hl', oder den \u00f6stlichen Blitz", "tokens": ["Sin\u00b7nend", "die", "Wel\u00b7len", "z\u00e4hl'", ",", "o\u00b7der", "den", "\u00f6st\u00b7li\