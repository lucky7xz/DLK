], "pos": ["PPER", "KON", "NE", "KON", "PPER", "$,", "VVFIN", "$.", "PWAV", "$,", "APPR", "ART", "NN"], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.76": {"text": "Und der sch\u00f6nen Natur himmlischem Nektar entflammt,", "tokens": ["Und", "der", "sch\u00f6\u00b7nen", "Na\u00b7tur", "himm\u00b7li\u00b7schem", "Nek\u00b7tar", "ent\u00b7flammt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.77": {"text": "Unsere Seelengespr\u00e4che den Edelsten unter den F\u00fcrsten", "tokens": ["Un\u00b7se\u00b7re", "See\u00b7len\u00b7ge\u00b7spr\u00e4\u00b7che", "den", "E\u00b7dels\u00b7ten", "un\u00b7ter", "den", "F\u00fcrs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ART", "NN", "APPR", "ART", "NN"], "meter": "+--+--+---+-+--+-", "measure": "dactylic.tri.plus"}, "line.78": {"text": "Segneten! Heiliger Baum, schauere Bl\u00fcten herab!", "tokens": ["Seg\u00b7ne\u00b7ten", "!", "Hei\u00b7li\u00b7ger", "Baum", ",", "schau\u00b7e\u00b7re", "Bl\u00fc\u00b7ten", "her\u00b7ab", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NE", "NE", "$,", "ADJA", "NN", "ADV", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.79": {"text": "Feiernd denk' ich Sein, des Edelsten, der nach der Arbeit", "tokens": ["Fei\u00b7ernd", "denk'", "ich", "Sein", ",", "des", "E\u00b7dels\u00b7ten", ",", "der", "nach", "der", "Ar\u00b7beit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "PPOSAT", "$,", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "+-+-+--+-++-+-", "measure": "trochaic.septa.relaxed"}, "line.80": {"text": "Hier zu ruhn mir verg\u00f6nnt; feiernd, o Holmer, auch dein:", "tokens": ["Hier", "zu", "ruhn", "mir", "ver\u00b7g\u00f6nnt", ";", "fei\u00b7ernd", ",", "o", "Hol\u00b7mer", ",", "auch", "dein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "PPER", "VVPP", "$.", "VVPP", "$,", "FM", "NN", "$,", "ADV", "PPOSAT", "$."], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.81": {"text": "Denn du sahst das Get\u00fcmmel um mich, und brachtest die Botschaft", "tokens": ["Denn", "du", "sahst", "das", "Ge\u00b7t\u00fcm\u00b7mel", "um", "mich", ",", "und", "brach\u00b7test", "die", "Bot\u00b7schaft"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "APPR", "PPER", "$,", "KON", "VVFIN", "ART", "NN"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.82": {"text": "Unserm Vater, der uns gerne wie Kinder erfreut!", "tokens": ["Un\u00b7serm", "Va\u00b7ter", ",", "der", "uns", "ger\u00b7ne", "wie", "Kin\u00b7der", "er\u00b7freut", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "ADV", "KOKOM", "NN", "VVFIN", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}}}}}