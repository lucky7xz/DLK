{"dta.poem.9811": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der verbrandte Cupido.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die liebliche Dione/", "tokens": ["Die", "lieb\u00b7li\u00b7che", "Dio\u00b7ne", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Der sch\u00f6nheit sch\u00f6nste krone/", "tokens": ["Der", "sch\u00f6n\u00b7heit", "sch\u00f6ns\u00b7te", "kro\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sa\u00df nechst auf ihrem throne/", "tokens": ["Sa\u00df", "nechst", "auf", "ih\u00b7rem", "thro\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und rief dem kleinen sohne/", "tokens": ["Und", "rief", "dem", "klei\u00b7nen", "soh\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dem nackten fl\u00fcgel-sch\u00fctzen/", "tokens": ["Dem", "nack\u00b7ten", "fl\u00fc\u00b7gel\u00b7sch\u00fct\u00b7zen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Dem sch\u00fctzen/ der mit flitzen", "tokens": ["Dem", "sch\u00fct\u00b7zen", "/", "der", "mit", "flit\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VVINF", "$(", "ART", "APPR", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Kan vieler hertz zerschlitzen/", "tokens": ["Kan", "vie\u00b7ler", "hertz", "zer\u00b7schlit\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und tieffe wunden ritzen/", "tokens": ["Und", "tief\u00b7fe", "wun\u00b7den", "rit\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "ADJA", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Und heisse flammen spr\u00fctzen.", "tokens": ["Und", "heis\u00b7se", "flam\u00b7men", "spr\u00fct\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "VVINF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Komm/ sagte sie/ geliebter/", "tokens": ["Komm", "/", "sag\u00b7te", "sie", "/", "ge\u00b7lieb\u00b7ter", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$(", "VVFIN", "PPER", "$(", "ADJA", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Jm lieben ausge\u00fcbter/", "tokens": ["Jm", "lie\u00b7ben", "aus\u00b7ge\u00b7\u00fcb\u00b7ter", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Und bitte mir ietzt g\u00e4ste", "tokens": ["Und", "bit\u00b7te", "mir", "ietzt", "g\u00e4s\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Zu meinem nahmens-feste.", "tokens": ["Zu", "mei\u00b7nem", "nah\u00b7mens\u00b7fes\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Du solt die jungfern laden/", "tokens": ["Du", "solt", "die", "jung\u00b7fern", "la\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Die schwimmenden Najaden/", "tokens": ["Die", "schwim\u00b7men\u00b7den", "Na\u00b7ja\u00b7den", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Die gr\u00fcnen Oreaden/", "tokens": ["Die", "gr\u00fc\u00b7nen", "O\u00b7rea\u00b7den", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.17": {"text": "Wie auch die Nereinen/", "tokens": ["Wie", "auch", "die", "Ne\u00b7rei\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.18": {"text": "Samt ihren lust-Delphinen/", "tokens": ["Samt", "ih\u00b7ren", "lust\u00b7Del\u00b7phi\u00b7nen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Die schnellen feld-Napeen/", "tokens": ["Die", "schnel\u00b7len", "feld\u00b7Na\u00b7peen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.20": {"text": "Und was du mehr stehst gehen", "tokens": ["Und", "was", "du", "mehr", "stehst", "ge\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "PPER", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.21": {"text": "Von meinem frauen-zimmer.", "tokens": ["Von", "mei\u00b7nem", "frau\u00b7en\u00b7zim\u00b7mer", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.22": {"text": "Doch h\u00fcte dich/ du schlimmer/", "tokens": ["Doch", "h\u00fc\u00b7te", "dich", "/", "du", "schlim\u00b7mer", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$(", "PPER", "ADJD", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.23": {"text": "Da\u00df du mir keine hertzest/", "tokens": ["Da\u00df", "du", "mir", "kei\u00b7ne", "hert\u00b7zest", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PIAT", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.24": {"text": "Noch gantz verwegen schertzest.", "tokens": ["Noch", "gantz", "ver\u00b7we\u00b7gen", "schert\u00b7zest", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.25": {"text": "Wenn du sie eingeladen/", "tokens": ["Wenn", "du", "sie", "ein\u00b7ge\u00b7la\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.26": {"text": "So bitte nicht Menaden", "tokens": ["So", "bit\u00b7te", "nicht", "Me\u00b7na\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "PTKNEG", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.27": {"text": "Und bockgef\u00fcste Faunen/", "tokens": ["Und", "bock\u00b7ge\u00b7f\u00fcs\u00b7te", "Fau\u00b7nen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.28": {"text": "Die auf den feld-posaunen/", "tokens": ["Die", "auf", "den", "feld\u00b7po\u00b7sau\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.29": {"text": "Ein garstig lermen blasen.", "tokens": ["Ein", "gars\u00b7tig", "ler\u00b7men", "bla\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVINF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.30": {"text": "Denn m\u00e4nner/ so nur rasen/", "tokens": ["Denn", "m\u00e4n\u00b7ner", "/", "so", "nur", "ra\u00b7sen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$(", "ADV", "ADV", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.31": {"text": "Die kan ich gar nicht leiden", "tokens": ["Die", "kan", "ich", "gar", "nicht", "lei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "PTKNEG", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.32": {"text": "Bey meiner wollust freuden.", "tokens": ["Bey", "mei\u00b7ner", "wol\u00b7lust", "freu\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.33": {"text": "Du must mir sch\u00f6ne knaben/", "tokens": ["Du", "must", "mir", "sch\u00f6\u00b7ne", "kna\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.34": {"text": "Die blut und geister haben/", "tokens": ["Die", "blut", "und", "geis\u00b7ter", "ha\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VAFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.35": {"text": "Zu meinem feste bitten/", "tokens": ["Zu", "mei\u00b7nem", "fes\u00b7te", "bit\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.36": {"text": "Sonst ist die lust verschnitten.", "tokens": ["Sonst", "ist", "die", "lust", "ver\u00b7schnit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.37": {"text": "Die lieblich k\u00f6nnen singen/", "tokens": ["Die", "lieb\u00b7lich", "k\u00f6n\u00b7nen", "sin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.38": {"text": "Die hurtig k\u00f6nnen springen/", "tokens": ["Die", "hur\u00b7tig", "k\u00f6n\u00b7nen", "sprin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.39": {"text": "Die tapffer k\u00f6nnen ringen", "tokens": ["Die", "tapf\u00b7fer", "k\u00f6n\u00b7nen", "rin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJD", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.40": {"text": "Und bey gestalten dingen", "tokens": ["Und", "bey", "ge\u00b7stal\u00b7ten", "din\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.41": {"text": "Dem frauen-zimmer bringen", "tokens": ["Dem", "frau\u00b7en\u00b7zim\u00b7mer", "brin\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.42": {"text": "Beliebt\u2019 erg\u00f6tzligkeiten/", "tokens": ["Be\u00b7liebt'", "er\u00b7g\u00f6tz\u00b7lig\u00b7kei\u00b7ten", "/"], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.43": {"text": "Und lust-vollkommenheiten.", "tokens": ["Und", "lust\u00b7voll\u00b7kom\u00b7men\u00b7hei\u00b7ten", "."], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.44": {"text": "Als liebreitz di\u00df geh\u00f6ret/", "tokens": ["Als", "lieb\u00b7reitz", "di\u00df", "ge\u00b7h\u00f6\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PDS", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.45": {"text": "Umfiel er gantz beth\u00f6ret", "tokens": ["Um\u00b7fiel", "er", "gantz", "be\u00b7th\u00f6\u00b7ret"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.46": {"text": "Der mutter perlen-br\u00fcste/", "tokens": ["Der", "mut\u00b7ter", "per\u00b7len\u00b7br\u00fcs\u00b7te", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.47": {"text": "Die er so sehnlich k\u00fcste/", "tokens": ["Die", "er", "so", "sehn\u00b7lich", "k\u00fcs\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.48": {"text": "Da\u00df auch auf seinen lippen/", "tokens": ["Da\u00df", "auch", "auf", "sei\u00b7nen", "lip\u00b7pen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.49": {"text": "Den hochgeschwollnen klippen/", "tokens": ["Den", "hoch\u00b7ge\u00b7schwoll\u00b7nen", "klip\u00b7pen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.50": {"text": "Der purpur blieben kleben/", "tokens": ["Der", "pur\u00b7pur", "blie\u00b7ben", "kle\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["