{"textgrid.poem.52411": {"metadata": {"author": {"name": "Reuter, Fritz", "birth": "N.A.", "death": "N.A."}, "title": "61. De Hauptsak", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr Moritz Gimpel kamm nah Stuer,", "tokens": ["Herr", "Mo\u00b7ritz", "Gim\u00b7pel", "kamm", "nah", "Stu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "NE", "VVFIN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dor was sin Bl\u00fcmchen in de Waterkur,", "tokens": ["Dor", "was", "sin", "Bl\u00fcm\u00b7chen", "in", "de", "Wa\u00b7ter\u00b7kur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Un seggt tau Kahlen \u2013 dunn was Kahl", "tokens": ["Un", "seggt", "tau", "Kah\u00b7len", "\u2013", "dunn", "was", "Kahl"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "CARD", "NN", "$(", "KON", "PWS", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Dor Dokter bi de Waterpump", "tokens": ["Dor", "Dok\u00b7ter", "bi", "de", "Wa\u00b7ter\u00b7pump"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Un heilt Herrn Moritz sin Gemahl", "tokens": ["Un", "heilt", "Herrn", "Mo\u00b7ritz", "sin", "Ge\u00b7mahl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "NN", "NE", "PPOSAT", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Mit Venusg\u00fcrtel, natten Strump,", "tokens": ["Mit", "Ve\u00b7nus\u00b7g\u00fcr\u00b7tel", ",", "nat\u00b7ten", "Strump", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mit Regenb\u00e4der un mit Duschen,", "tokens": ["Mit", "Re\u00b7gen\u00b7b\u00e4\u00b7der", "un", "mit", "Du\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "FM", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Mit k\u00e4uhlen Drunk von baben bet nah unn'n;", "tokens": ["Mit", "k\u00e4uh\u00b7len", "Drunk", "von", "ba\u00b7ben", "bet", "nah", "unn'n", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "NE", "VVFIN", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Ok m\u00fc\u00dft sch\u00f6n Bl\u00fcmchen m\u00e4nnig Stun'n", "tokens": ["Ok", "m\u00fc\u00dft", "sch\u00f6n", "Bl\u00fcm\u00b7chen", "m\u00e4n\u00b7nig", "Stun'n"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VMFIN", "ADJD", "NN", "ADJD", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.10": {"text": "In so'n verdammtes Sitzbad kuschen \u2013,", "tokens": ["In", "so'n", "ver\u00b7damm\u00b7tes", "Sitz\u00b7bad", "ku\u00b7schen", "\u2013", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "VVINF", "$(", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Un seggt tau Kahl'n: \u00bbHerr Dokter\u00ab, seggt'e,", "tokens": ["Un", "seggt", "tau", "Kahl'n", ":", "\u00bb", "Herr", "Dok\u00b7ter", "\u00ab", ",", "seggt'e", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "punct"], "pos": ["NE", "VVFIN", "CARD", "NN", "$.", "$(", "NN", "NN", "$(", "$,", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.12": {"text": "\u00bbin eine Notsach' bin ich hier,", "tokens": ["\u00bb", "in", "ei\u00b7ne", "Not\u00b7sach'", "bin", "ich", "hier", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ART", "NN", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "In der ich wohl das Bl\u00fcmchen sprechen m\u00f6chte:", "tokens": ["In", "der", "ich", "wohl", "das", "Bl\u00fcm\u00b7chen", "spre\u00b7chen", "m\u00f6ch\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Was noch ein Bruder is von ihr,", "tokens": ["Was", "noch", "ein", "Bru\u00b7der", "is", "von", "ihr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "FM", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Der Mauses, der is tot.\u00ab", "tokens": ["Der", "Mau\u00b7ses", ",", "der", "is", "tot", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "$,", "ART", "FM", "ADJD", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.16": {"text": "Je, seggt nu Kahl, wenn't hadd so grote Not,", "tokens": ["Je", ",", "seggt", "nu", "Kahl", ",", "wenn't", "hadd", "so", "gro\u00b7te", "Not", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVFIN", "ADV", "NN", "$,", "VVFIN", "VAFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Denn m\u00fc\u00dft dat sin, denn k\u00fcnn hei dat nich hinnern,", "tokens": ["Denn", "m\u00fc\u00dft", "dat", "sin", ",", "denn", "k\u00fcnn", "hei", "dat", "nich", "hin\u00b7nern", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "$,", "KON", "FM.nl", "FM.nl", "FM.nl", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Hei s\u00fcll de \u00c4werraschung \u00e4werst minnern,", "tokens": ["Hei", "s\u00fcll", "de", "\u00c4\u00b7wer\u00b7ras\u00b7chung", "\u00e4\u00b7werst", "min\u00b7nern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "FM", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.19": {"text": "Hei s\u00fcll ehr't nah un nah bibringen;", "tokens": ["Hei", "s\u00fcll", "ehr't", "nah", "un", "nah", "bib\u00b7rin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "ADJD", "VVINF", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.20": {"text": "De ganze Kur k\u00fcnn s\u00fcs mi\u00dflingen,", "tokens": ["De", "gan\u00b7ze", "Kur", "k\u00fcnn", "s\u00fcs", "mi\u00df\u00b7lin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Denn Bl\u00fcmchen wir sid gistern morg'n", "tokens": ["Denn", "Bl\u00fcm\u00b7chen", "wir", "sid", "gis\u00b7tern", "mor\u00b7g'n"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "PPER", "ADJD", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.22": {"text": "Tau sine Freud sihr elend word'n,", "tokens": ["Tau", "si\u00b7ne", "Freud", "sihr", "e\u00b7lend", "word'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PPER", "ADJD", "VAPP", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.23": {"text": "Un tau de Freud' von jedermann", "tokens": ["Un", "tau", "de", "Freud'", "von", "je\u00b7der\u00b7mann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "NE", "NN", "APPR", "PIS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "F\u00fcng sei all an tau duften an.", "tokens": ["F\u00fcng", "sei", "all", "an", "tau", "duf\u00b7ten", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PIAT", "APPR", "NE", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "\u00bbdas nenn'n wir 'Krisis', wir Doktoren,", "tokens": ["\u00bb", "das", "nenn'n", "wir", "'", "Kri\u00b7sis", "'", ",", "wir", "Dok\u00b7to\u00b7ren", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct", "punct", "word", "word", "punct"], "pos": ["$(", "PDS", "VVFIN", "PPER", "$(", "NE", "$(", "$,", "PPER", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Und was 'ne Krisis is, Herr Moritz Gimpel,", "tokens": ["Und", "was", "'ne", "Kri\u00b7sis", "is", ",", "Herr", "Mo\u00b7ritz", "Gim\u00b7pel", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "FM", "FM", "FM", "$,", "NN", "NE", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Das wei\u00df in Stuer jeder Simpel.\u00ab", "tokens": ["Das", "wei\u00df", "in", "Stu\u00b7er", "je\u00b7der", "Sim\u00b7pel", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VVFIN", "APPR", "NN", "PIAT", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Na, Moritz Gimpel ward von Kahl", "tokens": ["Na", ",", "Mo\u00b7ritz", "Gim\u00b7pel", "ward", "von", "Kahl"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "NE", "NE", "VAFIN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Herinner n\u00f6digt in den Saal,", "tokens": ["He\u00b7rin\u00b7ner", "n\u00f6\u00b7digt", "in", "den", "Saal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dor sitt sin Bl\u00fcmchen bla\u00df un welk", "tokens": ["Dor", "sitt", "sin", "Bl\u00fcm\u00b7chen", "bla\u00df", "un", "welk"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Un stippt en Stuten in de Melk", "tokens": ["Un", "stippt", "en", "Stu\u00b7ten", "in", "de", "Melk"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["FM", "FM", "FM", "NN", "APPR", "NE", "NE"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Un fohrt tau H\u00f6cht: \u00bbNu, Gimpelche, wos is?", "tokens": ["Un", "fohrt", "tau", "H\u00f6cht", ":", "\u00bb", "Nu", ",", "Gim\u00b7pel\u00b7che", ",", "wos", "is", "?"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$.", "$(", "ADV", "$,", "NN", "$,", "PWAV", "FM", "$."], "meter": "+-+--+---+", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Zu Haus' is wos pessiert gewi\u00df.\u00ab", "tokens": ["Zu", "Haus'", "is", "wos", "pes\u00b7siert", "ge\u00b7wi\u00df", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NE", "FM", "FM", "FM", "ADV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "\u00bbwos s\u00fcll da gepessiert denn sain?", "tokens": ["\u00bb", "wos", "s\u00fcll", "da", "ge\u00b7pes\u00b7siert", "denn", "sain", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "ADJD", "ADV", "VVPP", "KON", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.8": {"text": "Pessieren? \u2013 Nu, pessieren tut's", "tokens": ["Pes\u00b7sie\u00b7ren", "?", "\u2013", "Nu", ",", "pes\u00b7sie\u00b7ren", "tut's"], "token_info": ["word", "punct", "punct", "word", "punct", "word", "word"], "pos": ["NN", "$.", "$(", "ADV", "$,", "VVFIN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Ja alle Tag', bald Schlimm's, bald Gut's.", "tokens": ["Ja", "al\u00b7le", "Tag'", ",", "bald", "Schlim\u00b7m's", ",", "bald", "Gut'", "s."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "abbreviation"], "pos": ["PTKANT", "PIAT", "NN", "$,", "ADV", "NE", "$,", "ADV", "NN", "NE"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.10": {"text": "Doch halt mol still! Da f\u00e4llt mer ain,", "tokens": ["Doch", "halt", "mol", "still", "!", "Da", "f\u00e4llt", "mer", "ain", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PTKVZ", "$.", "ADV", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Der Borsch, der Itzig Rosenstain,", "tokens": ["Der", "Borsch", ",", "der", "It\u00b7zig", "Ro\u00b7sen\u00b7stain", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NE", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Der hat gewoltsam Schl\u00e4g' gekriegt.\u00ab", "tokens": ["Der", "hat", "ge\u00b7wolt\u00b7sam", "Schl\u00e4g'", "ge\u00b7kriegt", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VAFIN", "ADJD", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "\u00bbwo vor denn?\u00ab \u2013 \u00bbNu, vor's Rathaus von's Gericht.\u00ab", "tokens": ["\u00bb", "wo", "vor", "denn", "?", "\u00ab", "\u2013", "\u00bb", "Nu", ",", "vor's", "Rat\u00b7haus", "von's", "Ge\u00b7richt", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "punct", "punct", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWAV", "APPR", "ADV", "$.", "$(", "$(", "$(", "ADV", "$,", "APPRART", "NN", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "\u00bbdas frag' ich ja nich, Gimpelleben!", "tokens": ["\u00bb", "das", "frag'", "ich", "ja", "nich", ",", "Gim\u00b7pel\u00b7le\u00b7ben", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "PDS", "VVFIN", "PPER", "ADV", "PTKNEG", "$,", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Wor\u00fcber hob'n sie ihm die viele", "tokens": ["Wo\u00b7r\u00fc\u00b7ber", "hob'n", "sie", "ihm", "die", "vie\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "PPER", "ART", "PIAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Grausame Pr\u00fcgel denn gegeben?\u00ab", "tokens": ["Grau\u00b7sa\u00b7me", "Pr\u00fc\u00b7gel", "denn", "ge\u00b7ge\u00b7ben", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "KON", "VVPP", "$.", "$("], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.17": {"text": "\u00bbwor\u00fcber? \u2013 \u00dcber die Machile.\u00ab", "tokens": ["\u00bb", "wo\u00b7r\u00fc\u00b7ber", "?", "\u2013", "\u00dc\u00b7ber", "die", "Ma\u00b7chi\u00b7le", ".", "\u00ab"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWAV", "$.", "$(", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "\u00bbih, Gimpel, h\u00f6r mich doch mal ahn!", "tokens": ["\u00bb", "ih", ",", "Gim\u00b7pel", ",", "h\u00f6r", "mich", "doch", "mal", "ahn", "!"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "$,", "NN", "$,", "VVFIN", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Ich frage jo, wos hot er denn getan,", "tokens": ["Ich", "fra\u00b7ge", "jo", ",", "wos", "hot", "er", "denn", "ge\u00b7tan", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "$,", "PWAV", "VVFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Da\u00df sie so grausam schlugen ihn?\u00ab", "tokens": ["Da\u00df", "sie", "so", "grau\u00b7sam", "schlu\u00b7gen", "ihn", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VVFIN", "PPER", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "\u00bbgetan? Getan? \u2013 Au waih! hat er geschrien.\u00ab", "tokens": ["\u00bb", "ge\u00b7tan", "?", "Ge\u00b7tan", "?", "\u2013", "Au", "waih", "!", "hat", "er", "ge\u00b7schri\u00b7en", ".", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVPP", "$.", "NN", "$.", "$(", "NN", "VVFIN", "$.", "VAFIN", "PPER", "VVPP", "$.", "$("], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.22": {"text": "\u00bbih, Moritz, h\u0