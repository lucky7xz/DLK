{"dta.poem.20654": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "An  \n  eine Dichterin,  \n welche das Klavier spielte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Des Jovis, der Latona Sohn                 ", "tokens": ["Des", "Jo\u00b7vis", ",", "der", "La\u00b7to\u00b7na", "Sohn"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NE", "$,", "ART", "NE", "NN"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Hat mir ein Saitenspiel gegeben;", "tokens": ["Hat", "mir", "ein", "Sai\u00b7ten\u00b7spiel", "ge\u00b7ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Du aber kannst im s\u00fc\u00dfen Ton", "tokens": ["Du", "a\u00b7ber", "kannst", "im", "s\u00fc\u00b7\u00dfen", "Ton"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VMFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Stimme zum Gesang erheben.", "tokens": ["Die", "Stim\u00b7me", "zum", "Ge\u00b7sang", "er\u00b7he\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dein Finger h\u00fcpfet wie der West,", "tokens": ["Dein", "Fin\u00b7ger", "h\u00fcp\u00b7fet", "wie", "der", "West", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der an dem sch\u00f6nsten Tag des Mayen", "tokens": ["Der", "an", "dem", "sch\u00f6ns\u00b7ten", "Tag", "des", "Ma\u00b7yen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "In jugendliche Blumen bl\u00e4st,", "tokens": ["In", "ju\u00b7gend\u00b7li\u00b7che", "Blu\u00b7men", "bl\u00e4st", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Deines Lieblings Blick erfreuen.", "tokens": ["Die", "Dei\u00b7nes", "Lieb\u00b7lings", "Blick", "er\u00b7freu\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "H\u00f6r auf, geliebte Zauberin!", "tokens": ["H\u00f6r", "auf", ",", "ge\u00b7lieb\u00b7te", "Zau\u00b7be\u00b7rin", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "H\u00f6r auf zu singen und zu spielen;", "tokens": ["H\u00f6r", "auf", "zu", "sin\u00b7gen", "und", "zu", "spie\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich brenne, da ich weiblich bin,", "tokens": ["Ich", "bren\u00b7ne", ",", "da", "ich", "weib\u00b7lich", "bin", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was wird nicht dieser J\u00fcngling f\u00fchlen,", "tokens": ["Was", "wird", "nicht", "die\u00b7ser", "J\u00fcng\u00b7ling", "f\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PTKNEG", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der \u00fcber Deine Schultern sieht,", "tokens": ["Der", "\u00fc\u00b7ber", "Dei\u00b7ne", "Schul\u00b7tern", "sieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bald Deinen wei\u00dfen Hals betrachtet", "tokens": ["Bald", "Dei\u00b7nen", "wei\u00b7\u00dfen", "Hals", "be\u00b7trach\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Bald dieses Auge, welches gl\u00fcht", "tokens": ["Bald", "die\u00b7ses", "Au\u00b7ge", ",", "wel\u00b7ches", "gl\u00fcht"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "PDAT", "NN", "$,", "PRELS", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und redet, und im Sprechen schmachtet?", "tokens": ["Und", "re\u00b7det", ",", "und", "im", "Spre\u00b7chen", "schmach\u00b7tet", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KON", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "H\u00f6r auf, o M\u00e4dchen! jeder Schlag", "tokens": ["H\u00f6r", "auf", ",", "o", "M\u00e4d\u00b7chen", "!", "je\u00b7der", "Schlag"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NE", "PTKVZ", "$,", "FM", "NN", "$.", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dringt tiefer in des J\u00fcnglings Busen,", "tokens": ["Dringt", "tie\u00b7fer", "in", "des", "J\u00fcng\u00b7lings", "Bu\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und das, was Dein Klavier vermag,", "tokens": ["Und", "das", ",", "was", "Dein", "Kla\u00b7vier", "ver\u00b7mag", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Vermag kaum eine von den Musen.", "tokens": ["Ver\u00b7mag", "kaum", "ei\u00b7ne", "von", "den", "Mu\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}