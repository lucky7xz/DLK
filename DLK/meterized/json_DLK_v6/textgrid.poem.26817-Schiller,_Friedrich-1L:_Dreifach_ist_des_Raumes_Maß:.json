{"textgrid.poem.26817": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dreifach ist des Raumes Ma\u00df:", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dreifach ist des Raumes Ma\u00df:", "tokens": ["Drei\u00b7fach", "ist", "des", "Rau\u00b7mes", "Ma\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Rastlos fort ohn Unterla\u00df", "tokens": ["Rast\u00b7los", "fort", "ohn", "Un\u00b7ter\u00b7la\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "PTKVZ", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Strebt die L\u00e4nge, fort ins Weite", "tokens": ["Strebt", "die", "L\u00e4n\u00b7ge", ",", "fort", "ins", "Wei\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "PTKVZ", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Endlos gie\u00dfet sich die Breite,", "tokens": ["End\u00b7los", "gie\u00b7\u00dfet", "sich", "die", "Brei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Grundlos senkt die Tiefe sich.", "tokens": ["Grund\u00b7los", "senkt", "die", "Tie\u00b7fe", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "PRF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Dir ein Bild sind sie gegeben:", "tokens": ["Dir", "ein", "Bild", "sind", "sie", "ge\u00b7ge\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Rastlos vorw\u00e4rts mu\u00dft du streben,", "tokens": ["Rast\u00b7los", "vor\u00b7w\u00e4rts", "mu\u00dft", "du", "stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nie erm\u00fcdet stille stehn,", "tokens": ["Nie", "er\u00b7m\u00fc\u00b7det", "stil\u00b7le", "stehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Willst du die Vollendung sehn;", "tokens": ["Willst", "du", "die", "Vol\u00b7len\u00b7dung", "sehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "Mu\u00dft ins Breite dich entfalten,", "tokens": ["Mu\u00dft", "ins", "Brei\u00b7te", "dich", "ent\u00b7fal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Soll sich dir die Welt gestalten;", "tokens": ["Soll", "sich", "dir", "die", "Welt", "ge\u00b7stal\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "PPER", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "In die Tiefe mu\u00dft du steigen,", "tokens": ["In", "die", "Tie\u00b7fe", "mu\u00dft", "du", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Soll sich dir das Wesen zeigen.", "tokens": ["Soll", "sich", "dir", "das", "We\u00b7sen", "zei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "PPER", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Nur Beharrung f\u00fchrt zum Ziel,", "tokens": ["Nur", "Be\u00b7har\u00b7rung", "f\u00fchrt", "zum", "Ziel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Nur die F\u00fclle f\u00fchrt zur Klarheit,", "tokens": ["Nur", "die", "F\u00fcl\u00b7le", "f\u00fchrt", "zur", "Klar\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Und im Abgrund wohnt die Wahrheit.", "tokens": ["Und", "im", "Ab\u00b7grund", "wohnt", "die", "Wahr\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}