{"textgrid.poem.55743": {"metadata": {"author": {"name": "D\u00f6rmann, Felix", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die m\u00fcden Leiber ruhen", "tokens": ["Die", "m\u00fc\u00b7den", "Lei\u00b7ber", "ru\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Jedweder Regung bar,", "tokens": ["Jed\u00b7we\u00b7der", "Re\u00b7gung", "bar", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und um uns beide flutet", "tokens": ["Und", "um", "uns", "bei\u00b7de", "flu\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUI", "PPER", "PIS", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Narkotischen Duftes Dein Haar.", "tokens": ["Nar\u00b7ko\u00b7ti\u00b7schen", "Duf\u00b7tes", "Dein", "Haar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Noch sind die hei\u00dfen Glieder", "tokens": ["Noch", "sind", "die", "hei\u00b7\u00dfen", "Glie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Einander angeschmiegt,", "tokens": ["Ein\u00b7an\u00b7der", "an\u00b7ge\u00b7schmiegt", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Noch k\u00fcssen sich die Lippen,", "tokens": ["Noch", "k\u00fcs\u00b7sen", "sich", "die", "Lip\u00b7pen", ","], "token_info": ["word", "word