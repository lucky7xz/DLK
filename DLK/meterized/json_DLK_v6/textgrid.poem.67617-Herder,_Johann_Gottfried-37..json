{"textgrid.poem.67617": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "37.", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der L\u00f6we bat den Stier", "tokens": ["Der", "L\u00f6\u00b7we", "bat", "den", "Stier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NE", "VVFIN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Zum Mahl. Er stellt sich ein.", "tokens": ["Zum", "Mahl", ".", "Er", "stellt", "sich", "ein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$.", "PPER", "VVFIN", "PRF", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbda brennet's hier,", "tokens": ["\u00bb", "da", "bren\u00b7net's", "hier", ","], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Und dort sind Kessel, und nichts drein!", "tokens": ["Und", "dort", "sind", "Kes\u00b7sel", ",", "und", "nichts", "drein", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "NN", "$,", "KON", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Gilt das auch mir?\u00ab", "tokens": ["Gilt", "das", "auch", "mir", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PDS", "ADV", "PPER", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Er l\u00e4uft. \u00bbDas ist nicht fein,\u00ab", "tokens": ["Er", "l\u00e4uft", ".", "\u00bb", "Das", "ist", "nicht", "fein", ",", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "PDS", "VAFIN", "PTKNEG", "ADJD", "$,", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Spricht Leu, \u00bbmein Mahl zu fliehn!", "tokens": ["Spricht", "Leu", ",", "\u00bb", "mein", "Mahl", "zu", "fliehn", "!"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "$,", "$(", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Denn, Herr, ich pr\u00e4g's Ihm ein:", "tokens": ["Denn", ",", "Herr", ",", "ich", "pr\u00e4g's", "Ihm", "ein", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "NN", "$,", "PPER", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Ohn' Ihn", "tokens": ["Ohn'", "Ihn"], "token_info": ["word", "word"], "pos": ["APPR", "PPER"], "meter": "+-", "measure": "trochaic.single"}, "line.10": {"text": "Ist gar kein Mahl!\u00ab und w\u00fcrgt ihn.", "tokens": ["Ist", "gar", "kein", "Mahl", "!", "\u00ab", "und", "w\u00fcrgt", "ihn", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIAT", "NN", "$.", "$(", "KON", "VVFIN", "PPER", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Viel Dank f\u00fcr hohe Ehre,", "tokens": ["Viel", "Dank", "f\u00fcr", "ho\u00b7he", "Eh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die sonder meine Haut \u2013 nicht w\u00e4re.", "tokens": ["Die", "son\u00b7der", "mei\u00b7ne", "Haut", "\u2013", "nicht", "w\u00e4\u00b7re", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "KON", "PPOSAT", "NN", "$(", "PTKNEG", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}