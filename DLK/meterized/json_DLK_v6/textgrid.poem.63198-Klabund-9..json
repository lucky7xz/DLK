{"textgrid.poem.63198": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "9.", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Bauchschuss befindet sich auf dem Wege der Besserung.", "tokens": ["Ein", "Bauch\u00b7schuss", "be\u00b7fin\u00b7det", "sich", "auf", "dem", "We\u00b7ge", "der", "Bes\u00b7se\u00b7rung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+-+--+--+--", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ein (alkoholischer) Magenkatarrh beschwert sich \u00fcber Verw\u00e4sserung", "tokens": ["Ein", "(", "al\u00b7ko\u00b7ho\u00b7li\u00b7scher", ")", "Ma\u00b7gen\u00b7ka\u00b7tarrh", "be\u00b7schwert", "sich", "\u00fc\u00b7ber", "Ver\u00b7w\u00e4s\u00b7se\u00b7rung"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "$(", "ADJA", "$(", "NE", "VVFIN", "PRF", "APPR", "NN"], "meter": "-+-+--+--+-+-+--+-+", "measure": "iambic.octa.plus.relaxed"}, "line.3": {"text": "Des Magensaftes durch d\u00fcnne Medizinen.", "tokens": ["Des", "Ma\u00b7gen\u00b7saf\u00b7tes", "durch", "d\u00fcn\u00b7ne", "Me\u00b7di\u00b7zi\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Zwei Schwestern sind beflissen, einem Ohnebein zu dienen.", "tokens": ["Zwei", "Schwes\u00b7tern", "sind", "be\u00b7flis\u00b7sen", ",", "ei\u00b7nem", "Oh\u00b7ne\u00b7bein", "zu", "die\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "VVPP", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.2": {"line.1": {"text": "Ein Herzschuss m\u00f6chte zum Schluss noch etwas Sekt.", "tokens": ["Ein", "Herz\u00b7schuss", "m\u00f6ch\u00b7te", "zum", "Schluss", "noch", "et\u00b7was", "Sekt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "APPRART", "NN", "ADV", "PIAT", "NN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Eine Ruhr hat schon wieder das Bett verdreckt.", "tokens": ["Ei\u00b7ne", "Ruhr", "hat", "schon", "wie\u00b7der", "das", "Bett", "ver\u00b7dreckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "ART", "NN", "VVPP", "$."], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Eine Schenkeleiterung muss Liebesbriefe schmieren.", "tokens": ["Ei\u00b7ne", "Schen\u00b7ke\u00b7lei\u00b7te\u00b7rung", "muss", "Lie\u00b7bes\u00b7brie\u00b7fe", "schmie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.4": {"text": "Ein Streifschuss geht (draussen) in der Sonne spazieren.", "tokens": ["Ein", "Streif\u00b7schuss", "geht", "(", "draus\u00b7sen", ")", "in", "der", "Son\u00b7ne", "spa\u00b7zie\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$(", "VVFIN", "$(", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+-+--+-", "measure": "iambic.penta.relaxed"}}}}}