{"dta.poem.15826": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "31.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du sagst: die ganze Stadt bewohnt ein Thorensinn.", "tokens": ["Du", "sagst", ":", "die", "gan\u00b7ze", "Stadt", "be\u00b7wohnt", "ein", "Tho\u00b7ren\u00b7sinn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wohnst du, weiser Freund, nicht eben auch darinn?", "tokens": ["Und", "wohnst", "du", ",", "wei\u00b7ser", "Freund", ",", "nicht", "e\u00b7ben", "auch", "da\u00b7rinn", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,", "PTKNEG", "ADV", "ADV", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Du sagest: Nein und ja! der Mauer wohn' ich nah,", "tokens": ["Du", "sa\u00b7gest", ":", "Nein", "und", "ja", "!", "der", "Mau\u00b7er", "wohn'", "ich", "nah", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PTKANT", "KON", "ADV", "$.", "ART", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und bin nur halb darin, weil ich halb dr\u00fcber sah.", "tokens": ["Und", "bin", "nur", "halb", "da\u00b7rin", ",", "weil", "ich", "halb", "dr\u00fc\u00b7ber", "sah", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punc