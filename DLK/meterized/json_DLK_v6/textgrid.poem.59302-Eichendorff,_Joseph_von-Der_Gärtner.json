{"textgrid.poem.59302": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der G\u00e4rtner", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohin ich geh und schaue,", "tokens": ["Wo\u00b7hin", "ich", "geh", "und", "schau\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In Feld und Wald und Tal,", "tokens": ["In", "Feld", "und", "Wald", "und", "Tal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vom Berg hinab in die Aue:", "tokens": ["Vom", "Berg", "hin\u00b7ab", "in", "die", "Au\u00b7e", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Viel sch\u00f6ne, hohe Fraue,", "tokens": ["Viel", "sch\u00f6\u00b7ne", ",", "ho\u00b7he", "Frau\u00b7e", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Gr\u00fc\u00df ich dich tausendmal.", "tokens": ["Gr\u00fc\u00df", "ich", "dich", "tau\u00b7send\u00b7mal", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PRF", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "In meinem Garten find ich", "tokens": ["In", "mei\u00b7nem", "Gar\u00b7ten", "find", "ich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Viel Blumen, sch\u00f6n und fein,", "tokens": ["Viel", "Blu\u00b7men", ",", "sch\u00f6n", "und", "fein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Viel Kr\u00e4nze wohl draus wind ich", "tokens": ["Viel", "Kr\u00e4n\u00b7ze", "wohl", "draus", "wind", "ich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "ADV", "PAV", "VAFIN", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und tausend Gedanken bind ich", "tokens": ["Und", "tau\u00b7send", "Ge\u00b7dan\u00b7ken", "bind", "ich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "CARD", "NN", "VAFIN", "PPER"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Und Gr\u00fc\u00dfe mit darein.", "tokens": ["Und", "Gr\u00fc\u00b7\u00dfe", "mit", "da\u00b7rein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ihr darf ich keinen reichen,", "tokens": ["Ihr", "darf", "ich", "kei\u00b7nen", "rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PIAT", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sie ist zu hoch und sch\u00f6n,", "tokens": ["Sie", "ist", "zu", "hoch", "und", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKA", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die m\u00fcssen alle verbleichen,", "tokens": ["Die", "m\u00fcs\u00b7sen", "al\u00b7le", "ver\u00b7blei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PIS", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die Liebe nur ohnegleichen", "tokens": ["Die", "Lie\u00b7be", "nur", "oh\u00b7ne\u00b7glei\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Bleibt ewig im Herzen stehn.", "tokens": ["Bleibt", "e\u00b7wig", "im", "Her\u00b7zen", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPRART", "NN", "VVINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Ich schein wohl froher Dinge", "tokens": ["Ich", "schein", "wohl", "fro\u00b7her", "Din\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "ADV", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und schaffe auf und ab,", "tokens": ["Und", "schaf\u00b7fe", "auf", "und", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und, ob das Herz zerspringe,", "tokens": ["Und", ",", "ob", "das", "Herz", "zer\u00b7sprin\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich grabe fort und singe", "tokens": ["Ich", "gra\u00b7be", "fort", "und", "sin\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und grab mir bald mein Grab.", "tokens": ["Und", "grab", "mir", "bald", "mein", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}