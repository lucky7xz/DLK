{"textgrid.poem.40959": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Regen und Tr\u00e4nen", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df es j\u00fcngst geregnet hat,", "tokens": ["Da\u00df", "es", "j\u00fcngst", "ge\u00b7reg\u00b7net", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Zeigt der Tropfe auf dem Blatt,", "tokens": ["Zeigt", "der", "Trop\u00b7fe", "auf", "dem", "Blatt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und wenn na\u00df die Augen sind,", "tokens": ["Und", "wenn", "na\u00df", "die", "Au\u00b7gen", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADJD", "ART", "NN", "VAFIN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Sieht man, da\u00df geweint ein Kind.", "tokens": ["Sieht", "man", ",", "da\u00df", "ge\u00b7weint", "ein", "Kind", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "KOUS", "VVPP", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Blatt saugt bald die Tropfen ein,", "tokens": ["Blatt", "saugt", "bald", "die", "Trop\u00b7fen", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Bald wird's Auge trocken sein.", "tokens": ["Bald", "wird's", "Au\u00b7ge", "tro\u00b7cken", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Scheint die Sonne wieder klar,", "tokens": ["Scheint", "die", "Son\u00b7ne", "wie\u00b7der", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wei\u00df man nicht, da\u00df Regen war.", "tokens": ["Wei\u00df", "man", "nicht", ",", "da\u00df", "Re\u00b7gen", "war", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PTKNEG", "$,", "KOUS", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}