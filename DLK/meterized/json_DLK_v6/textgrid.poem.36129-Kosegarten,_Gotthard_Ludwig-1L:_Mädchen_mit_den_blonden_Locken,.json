{"textgrid.poem.36129": {"metadata": {"author": {"name": "Kosegarten, Gotthard Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: M\u00e4dchen mit den blonden Locken,", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "M\u00e4dchen mit den blonden Locken,", "tokens": ["M\u00e4d\u00b7chen", "mit", "den", "blon\u00b7den", "Lo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00f6re Deines S\u00e4ngers Lied.", "tokens": ["H\u00f6\u00b7re", "Dei\u00b7nes", "S\u00e4n\u00b7gers", "Lied", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie der Hall von Abendglocken", "tokens": ["Wie", "der", "Hall", "von", "A\u00b7bend\u00b7glo\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "R\u00fchr' es fei'rlich Dein Gem\u00fcth.", "tokens": ["R\u00fchr'", "es", "fei'\u00b7rlich", "Dein", "Ge\u00b7m\u00fcth", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADJD", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "M\u00e4dchen mit dem sanften Auge,", "tokens": ["M\u00e4d\u00b7chen", "mit", "dem", "sanf\u00b7ten", "Au\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Horch auf meiner Saiten Klang;", "tokens": ["Horch", "auf", "mei\u00b7ner", "Sai\u00b7ten", "Klang", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Lehre, die Dir heilsam tauge,", "tokens": ["Leh\u00b7re", ",", "die", "Dir", "heil\u00b7sam", "tau\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Weisheit t\u00f6nt Dir mein Gesang.", "tokens": ["Weis\u00b7heit", "t\u00f6nt", "Dir", "mein", "Ge\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+---+-+", "measure": "dactylic.init"}}, "stanza.2": {"line.1": {"text": "Dirne, Dirne, deutschen Samens", "tokens": ["Dir\u00b7ne", ",", "Dir\u00b7ne", ",", "deut\u00b7schen", "Sa\u00b7mens"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bist Du, bist ein deutsches Kind.", "tokens": ["Bist", "Du", ",", "bist", "ein", "deut\u00b7sches", "Kind", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "O, der Klang des stolzen Namens", "tokens": ["O", ",", "der", "Klang", "des", "stol\u00b7zen", "Na\u00b7mens"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "ART", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Schmettr' in Dir wie Nachtsturmwind.", "tokens": ["Schmett\u00b7r'", "in", "Dir", "wie", "Nachts\u00b7turm\u00b7wind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "KOKOM", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie den Strahl aus wettertr\u00e4cht'gen", "tokens": ["Wie", "den", "Strahl", "aus", "wet\u00b7ter\u00b7tr\u00e4cht'\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dunkeln Wolken f\u00fchl's Dein Geist,", "tokens": ["Dun\u00b7keln", "Wol\u00b7ken", "f\u00fchl's", "Dein", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Da\u00df Du deutsch, und deutscher M\u00e4dchen", "tokens": ["Da\u00df", "Du", "deutsch", ",", "und", "deut\u00b7scher", "M\u00e4d\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "$,", "KON", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Unverf\u00e4lschte Enklinn hei\u00dft.", "tokens": ["Un\u00b7ver\u00b7f\u00e4lschte", "En\u00b7klinn", "hei\u00dft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "+-++-+", "measure": "unknown.measure.tetra"}}, "stanza.3": {"line.1": {"text": "Gro\u00df wie die, und echt und bieder,", "tokens": ["Gro\u00df", "wie", "die", ",", "und", "echt", "und", "bie\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "$,", "KON", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hold und edel mu\u00dft Du sein;", "tokens": ["Hold", "und", "e\u00b7del", "mu\u00dft", "Du", "sein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "VMFIN", "PPER", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "W\u00fcrde strahle um Dich wieder,", "tokens": ["W\u00fcr\u00b7de", "strah\u00b7le", "um", "Dich", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPER", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie um Eichen Mondenschein,", "tokens": ["Wie", "um", "Ei\u00b7chen", "Mon\u00b7den\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und Dein Hochglanz scheuch' den losen", "tokens": ["Und", "Dein", "Hoch\u00b7glanz", "scheuch'", "den", "lo\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wei\u00df best\u00e4ubten Schmetterling,", "tokens": ["Wei\u00df", "be\u00b7st\u00e4ub\u00b7ten", "Schmet\u00b7ter\u00b7ling", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Der sich an der Wange Rosen,", "tokens": ["Der", "sich", "an", "der", "Wan\u00b7ge", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "An des Busens Lilien hing.", "tokens": ["An", "des", "Bu\u00b7sens", "Li\u00b7li\u00b7en", "hing", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "VVFIN", "$."], "meter": "--+-+---", "measure": "anapaest.init"}}, "stanza.4": {"line.1": {"text": "Dennoch schwelle m\u00e4chtig Feuer,", "tokens": ["Den\u00b7noch", "schwel\u00b7le", "m\u00e4ch\u00b7tig", "Feu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Edle, Deinen Busen hoch \u2013", "tokens": ["Ed\u00b7le", ",", "Dei\u00b7nen", "Bu\u00b7sen", "hoch", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "$,", "PPOSAT", "NN", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Freiheit, Freiheit, sei Dir theuer", "tokens": ["Frei\u00b7heit", ",", "Frei\u00b7heit", ",", "sei", "Dir", "theu\u00b7er"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "VAFIN", "PPER", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und wie Tod des Sklaven Joch.", "tokens": ["Und", "wie", "Tod", "des", "Skla\u00b7ven", "Joch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "NN", "ART", "NN", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie auf Bergen Gottes Zeder,", "tokens": ["Wie", "auf", "Ber\u00b7gen", "Got\u00b7tes", "Ze\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Hebe k\u00fchn Dein Haupt und steh,", "tokens": ["He\u00b7be", "k\u00fchn", "Dein", "Haupt", "und", "steh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "PPOSAT", "NN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und kein unbekannter, bl\u00f6der", "tokens": ["Und", "kein", "un\u00b7be\u00b7kann\u00b7ter", ",", "bl\u00f6\u00b7der"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["KON", "PIAT", "ADJA", "$,", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Schwachmuth st\u00fcrz' Dich von der H\u00f6h'.", "tokens": ["Schwach\u00b7muth", "st\u00fcrz'", "Dich", "von", "der", "H\u00f6h'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.5": {"line.1": {"text": "Doch auch Veilchen gleich im Thale", "tokens": ["Doch", "auch", "Veil\u00b7chen", "gleich", "im", "Tha\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "NN", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bl\u00fch ger\u00e4uschlos, still f\u00fcr Dich,", "tokens": ["Bl\u00fch", "ge\u00b7r\u00e4uschlos", ",", "still", "f\u00fcr", "Dich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "$,", "ADJD", "APPR", "PPER", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Und im Abendsonnenstrahle", "tokens": ["Und", "im", "A\u00b7bend\u00b7son\u00b7nen\u00b7strah\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["KON", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bade Deine Wange sich.", "tokens": ["Ba\u00b7de", "Dei\u00b7ne", "Wan\u00b7ge", "sich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PRF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Fleuch der Mode Thorheitschimmer,", "tokens": ["Fleuch", "der", "Mo\u00b7de", "Thor\u00b7heit\u00b7schim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie des Wandrers Irrlichtschein,", "tokens": ["Wie", "des", "Wand\u00b7rers", "Irr\u00b7licht\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und wie Mond im Wolkenflimmer", "tokens": ["Und", "wie", "Mond", "im", "Wol\u00b7ken\u00b7flim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "H\u00fclle Sittsamkeit Dich ein.", "tokens": ["H\u00fcl\u00b7le", "Sitt\u00b7sam\u00b7keit", "Dich", "ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "PPER", "PTKVZ", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.6": {"line.1": {"text": "M\u00e4dchen, M\u00e4dchen, auf dem Pfade,", "tokens": ["M\u00e4d\u00b7chen", ",", "M\u00e4d\u00b7chen", ",", "auf", "dem", "Pfa\u00b7de", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Den Dein schwacher Fu\u00df durchirrt,", "tokens": ["Den", "Dein", "schwa\u00b7cher", "Fu\u00df", "durch\u00b7irrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Liegt ein lustig Blumgestade,", "tokens": ["Liegt", "ein", "lus\u00b7tig", "Blum\u00b7ge\u00b7sta\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "L\u00fcstern lockend, bunt geziert.", "tokens": ["L\u00fcs\u00b7tern", "lo\u00b7ckend", ",", "bunt", "ge\u00b7ziert", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVPP", "$,", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Auf ihm sitzt im Modenflitter", "tokens": ["Auf", "ihm", "sitzt", "im", "Mo\u00b7den\u00b7flit\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Buhlend sch\u00f6n die Eitelkeit;", "tokens": ["Buh\u00b7lend", "sch\u00f6n", "die", "Ei\u00b7tel\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Winken wird sie Dir, doch zitter' \u2013", "tokens": ["Win\u00b7ken", "wird", "sie", "Dir", ",", "doch", "zit\u00b7ter'", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PPER", "$,", "ADV", "VVFIN", "$("], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Drachenschweife deckt ihr Kleid.", "tokens": ["Dra\u00b7chen\u00b7schwei\u00b7fe", "deckt", "ihr", "Kleid", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Wandle herzhaft auf dem Pfade", "tokens": ["Wand\u00b7le", "herz\u00b7haft", "auf", "dem", "Pfa\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weiter, und im stillen Licht", "tokens": ["Wei\u00b7ter", ",", "und", "im", "stil\u00b7len", "Licht"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KON", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Siehst Du dann noch ein Gestade,", "tokens": ["Siehst", "Du", "dann", "noch", "ein", "Ge\u00b7sta\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einfach sch\u00f6n und ungeschm\u00fcckt.", "tokens": ["Ein\u00b7fach", "sch\u00f6n", "und", "un\u00b7ge\u00b7schm\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dorten wohnt in Myrtenw\u00e4ldern", "tokens": ["Dor\u00b7ten", "wohnt", "in", "Myr\u00b7ten\u00b7w\u00e4l\u00b7dern"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "In der Unschuld wei\u00dfem Kleid,", "tokens": ["In", "der", "Un\u00b7schuld", "wei\u00b7\u00dfem", "Kleid", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und in monddurchstrahlten Feldern,", "tokens": ["Und", "in", "mond\u00b7durch\u00b7strahl\u00b7ten", "Fel\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Himmelstochter \u2013 Z\u00e4rtlichkeit.", "tokens": ["Him\u00b7mel\u00b7stoch\u00b7ter", "\u2013", "Z\u00e4rt\u00b7lich\u00b7keit", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$(", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Um sie fl\u00f6ten Nachtigallen,", "tokens": ["Um", "sie", "fl\u00f6\u00b7ten", "Nach\u00b7ti\u00b7gal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Um sie girrt ein Taubenpaar,", "tokens": ["Um", "sie", "girrt", "ein", "Tau\u00b7ben\u00b7paar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure"