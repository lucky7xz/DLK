word", "punct"], "pos": ["ADJA", "NN", "$,", "PDS", "VAFIN", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lieb' ist Tugend \u2013 zittre nicht,", "tokens": ["Lieb'", "ist", "Tu\u00b7gend", "\u2013", "zitt\u00b7re", "nicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "$(", "VVFIN", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schleu\u00df mir ja dem edlen Triebe", "tokens": ["Schleu\u00df", "mir", "ja", "dem", "ed\u00b7len", "Trie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Den ger\u00fchrten Busen nicht.", "tokens": ["Den", "ge\u00b7r\u00fchr\u00b7ten", "Bu\u00b7sen", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wirst Du einst den J\u00fcngling schauen", "tokens": ["Wirst", "Du", "einst", "den", "J\u00fcng\u00b7ling", "schau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Deutsch und bider so wie Du,", "tokens": ["Deutsch", "und", "bi\u00b7der", "so", "wie", "Du", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADV", "ADV", "KOKOM", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wird er Dir sein Herz vertrauen,", "tokens": ["Wird", "er", "Dir", "sein", "Herz", "ver\u00b7trau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"te