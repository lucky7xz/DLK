{"textgrid.poem.46232": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "An eben denselbigen Herren P\u00f6hmern meinen hochgeehrten freind", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nein, P\u00f6hmer, mein verdienst ist deinem lob nicht gleich,", "tokens": ["Nein", ",", "P\u00f6h\u00b7mer", ",", "mein", "ver\u00b7dienst", "ist", "dei\u00b7nem", "lob", "nicht", "gleich", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NN", "$,", "PPOSAT", "VVPP", "VAFIN", "PPOSAT", "NN", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "und kaum wei\u00df ich, ob mir ein solches zu erwerben,", "tokens": ["und", "kaum", "wei\u00df", "ich", ",", "ob", "mir", "ein", "sol\u00b7ches", "zu", "er\u00b7wer\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ART", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "weil unsre werk und wir, wie immer weis, from, reich,", "tokens": ["weil", "uns\u00b7re", "werk", "und", "wir", ",", "wie", "im\u00b7mer", "weis", ",", "from", ",", "reich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "KON", "PPER", "$,", "PWAV", "ADV", "PTKVZ", "$,", "ADJD", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "ohn underschied wie die, die torecht, b\u00f6s, arm sterben;", "tokens": ["ohn", "un\u00b7der\u00b7schied", "wie", "die", ",", "die", "to\u00b7recht", ",", "b\u00f6s", ",", "arm", "ster\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "KON", "PWAV", "ART", "$,", "PRELS", "ADJD", "$,", "ADJD", "$,", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Jung underlie\u00df ich nicht mit flei\u00df und kunst zugleich", "tokens": ["Jung", "un\u00b7der\u00b7lie\u00df", "ich", "nicht", "mit", "flei\u00df", "und", "kunst", "zu\u00b7gleich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "KON", "PPER", "PTKNEG", "APPR", "NN", "KON", "ADV", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "um g\u00fcter, gro\u00dfes lob und f\u00fcrstengunst zu werben,", "tokens": ["um", "g\u00fc\u00b7ter", ",", "gro\u00b7\u00dfes", "lob", "und", "f\u00fcrs\u00b7ten\u00b7gunst", "zu", "wer\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADJA", "NN", "KON", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "bald fanden meine hab und schriften in dem reich,", "tokens": ["bald", "fan\u00b7den", "mei\u00b7ne", "hab", "und", "schrif\u00b7ten", "in", "dem", "reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "VAFIN", "KON", "VVFIN", "APPR", "ART", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "wie freind und f\u00fcrsten auch, ihr g\u00e4nzliches verderben.", "tokens": ["wie", "freind", "und", "f\u00fcrs\u00b7ten", "auch", ",", "ihr", "g\u00e4nz\u00b7li\u00b7ches", "ver\u00b7der\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "KON", "VVFIN", "ADV", "$,", "PPOSAT", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Was ich nu sither wolt, von geiz und ehrgeiz frei,", "tokens": ["Was", "ich", "nu", "si\u00b7ther", "wolt", ",", "von", "geiz", "und", "ehr\u00b7geiz", "frei", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADJD", "VMFIN", "$,", "APPR", "NE", "KON", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "das wolt ich, nur mein herz von leid zu freien, schreiben,", "tokens": ["das", "wolt", "ich", ",", "nur", "mein", "herz", "von", "leid", "zu", "frei\u00b7en", ",", "schrei\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "$,", "ADV", "PPOSAT", "NN", "APPR", "ADJD", "PTKZU", "VVINF", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "und ist kaum lobens wert, weil es nicht welsch noch neu;", "tokens": ["und", "ist", "kaum", "lo\u00b7bens", "wert", ",", "weil", "es", "nicht", "welsch", "noch", "neu", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "ADJD", "$,", "KOUS", "PPER", "PTKNEG", "ADJD", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Doch mit der wahrheit schmuck dein wort einzuverleiben,", "tokens": ["Doch", "mit", "der", "wahr\u00b7heit", "schmuck", "dein", "wort", "ein\u00b7zu\u00b7ver\u00b7lei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPOSAT", "NN", "VVIZU", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "so wollest du, mein freind und ich will dir getreu", "tokens": ["so", "wol\u00b7lest", "du", ",", "mein", "freind", "und", "ich", "will", "dir", "ge\u00b7treu"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "$,", "PPOSAT", "NN", "KON", "PPER", "VMFIN", "PPER", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "und das lob soll dir selbs, dem es geb\u00fchret, bleiben.", "tokens": ["und", "das", "lob", "soll", "dir", "selbs", ",", "dem", "es", "ge\u00b7b\u00fch\u00b7ret", ",", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ART", "NN", "VMFIN", "PPER", "ADV", "$,", "PRELS", "PPER", "VVFIN", "$,", "VVINF", "$."], "meter": "--+--++--+-+-", "measure": "anapaest.di.plus"}}}}}