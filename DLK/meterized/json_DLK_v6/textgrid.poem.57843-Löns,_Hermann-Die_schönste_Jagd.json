 "line.2": {"text": "Und trifft er wenig an,", "tokens": ["Und", "trifft", "er", "we\u00b7nig", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIS", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dann sucht mein Schatz ein andres Wild,", "tokens": ["Dann", "sucht", "mein", "Schatz", "ein", "and\u00b7res", "Wild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf das er jagen kann;", "tokens": ["Auf", "das", "er", "ja\u00b7gen", "kann", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ja das Schie\u00dfen, das lernt sich,", "tokens": ["Ja", "das", "Schie\u00b7\u00dfen", ",", "das", "lernt", "sich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "ART", "NN", "$,", "PDS", "VVFIN", "PRF", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Wenn man flei\u00dfig es \u00fcbt,", "tokens": ["Wenn", "man", "flei\u00b7\u00dfig", "es", "\u00fcbt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "PPER", "VVFIN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Auf Hirsche und Hasen", "tokens": ["Auf", "Hir\u00b7sche", "und", "Ha\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Und was es sonst wohl noch gibt.", "tokens": ["Und", "was", "es", "sonst", "wohl", "noch", "gibt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}