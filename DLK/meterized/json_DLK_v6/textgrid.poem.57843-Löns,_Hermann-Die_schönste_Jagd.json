{"textgrid.poem.57843": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Die sch\u00f6nste Jagd", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Schatz, das ist ein freier Sch\u00fctz", "tokens": ["Mein", "Schatz", ",", "das", "ist", "ein", "frei\u00b7er", "Sch\u00fctz"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PDS", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wohl auf der braunen Heid',", "tokens": ["Wohl", "auf", "der", "brau\u00b7nen", "Heid'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er schie\u00dft die Hirsche und die Reh',", "tokens": ["Er", "schie\u00dft", "die", "Hir\u00b7sche", "und", "die", "Reh'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Denn das ist seine Freud';", "tokens": ["Denn", "das", "ist", "sei\u00b7ne", "Freud'", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ja das Schie\u00dfen, das lernt sich,", "tokens": ["Ja", "das", "Schie\u00b7\u00dfen", ",", "das", "lernt", "sich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "ART", "NN", "$,", "PDS", "VVFIN", "PRF", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Wenn man flei\u00dfig es \u00fcbt,", "tokens": ["Wenn", "man", "flei\u00b7\u00dfig", "es", "\u00fcbt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "PPER", "VVFIN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Auf Hirsche und Hasen", "tokens": ["Auf", "Hir\u00b7sche", "und", "Ha\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Und was es sonst wohl noch gibt.", "tokens": ["Und", "was", "es", "sonst", "wohl", "noch", "gibt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Und wenn die Nacht ganz dunkel ist,", "tokens": ["Und", "wenn", "die", "Nacht", "ganz", "dun\u00b7kel", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Mond gibt keinen Schein,", "tokens": ["Der", "Mond", "gibt", "kei\u00b7nen", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dann klopft es dreimal leise an", "tokens": ["Dann", "klopft", "es", "drei\u00b7mal", "lei\u00b7se", "an"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJD", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bei meinem Fensterlein;", "tokens": ["Bei", "mei\u00b7nem", "Fens\u00b7ter\u00b7lein", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ja das Schie\u00dfen, das lernt sich,", "tokens": ["Ja", 