{"dta.poem.2970": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "16.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In Mekka, floh er nicht, sie h\u00e4tten ihn gesteinigt;", "tokens": ["In", "Mek\u00b7ka", ",", "floh", "er", "nicht", ",", "sie", "h\u00e4t\u00b7ten", "ihn", "ge\u00b7stei\u00b7nigt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "VVFIN", "PPER", "PTKNEG", "$,", "PPER", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bald in Medina war die Schaar um ihn vereinigt:", "tokens": ["Bald", "in", "Me\u00b7di\u00b7na", "war", "die", "Schaar", "um", "ihn", "ver\u00b7ei\u00b7nigt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "VAFIN", "ART", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Bewiesen hat so gut wie der von Nazaret", "tokens": ["Be\u00b7wie\u00b7sen", "hat", "so", "gut", "wie", "der", "von", "Na\u00b7za\u00b7ret"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "ADV", "ADJD", "KOKOM", "ART", "APPR", "NE"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mit seinem Beispiel der arabische Profet:", "tokens": ["Mit", "sei\u00b7nem", "Bei\u00b7spiel", "der", "a\u00b7ra\u00b7bi\u00b7sche", "Pro\u00b7fet", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Da\u00df der Profet nicht gilt in seiner Vaterstadt,", "tokens": ["Da\u00df", "der", "Pro\u00b7fet", "nicht", "gilt", "in", "sei\u00b7ner", "Va\u00b7ter\u00b7stadt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PTKNEG", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Noch der Poet in der, die ihn geboren hat.", "tokens": ["Noch", "der", "Po\u00b7et", "in", "der", ",", "die", "ihn", "ge\u00b7bo\u00b7ren", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "ART", "$,", "PRELS", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}