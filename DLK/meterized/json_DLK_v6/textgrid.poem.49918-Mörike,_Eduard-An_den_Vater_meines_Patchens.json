{"textgrid.poem.49918": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "An den Vater meines Patchens", "genre": "verse", "period": "N.A.", "pub_year": 1845, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Knabe, der zehn Jahre sp\u00e4ter dir ein Freund", "tokens": ["Der", "Kna\u00b7be", ",", "der", "zehn", "Jah\u00b7re", "sp\u00e4\u00b7ter", "dir", "ein", "Freund"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "CARD", "NN", "ADJD", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und lange Zeit ein t\u00e4glicher Genosse war,", "tokens": ["Und", "lan\u00b7ge", "Zeit", "ein", "t\u00e4g\u00b7li\u00b7cher", "Ge\u00b7nos\u00b7se", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ART", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Daheim noch lebt' er in des lieben Vaters Haus,", "tokens": ["Da\u00b7heim", "noch", "lebt'", "er", "in", "des", "lie\u00b7ben", "Va\u00b7ters", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mit bl\u00fchenden Geschwistern selbst ein bl\u00fchender:", "tokens": ["Mit", "bl\u00fc\u00b7hen\u00b7den", "Ge\u00b7schwis\u00b7tern", "selbst", "ein", "bl\u00fc\u00b7hen\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sieh, diesen S\u00e4bel zur Husarenuniform", "tokens": ["Sieh", ",", "die\u00b7sen", "S\u00e4\u00b7bel", "zur", "Hu\u00b7sa\u00b7ren\u00b7u\u00b7ni\u00b7form"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "PDAT", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Trug er durch Hof und Garten und Alleen der Stadt.", "tokens": ["Trug", "er", "durch", "Hof", "und", "Gar\u00b7ten", "und", "Al\u00b7leen", "der", "Stadt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "KON", "NN", "KON", "NN", "ART", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.7": {"text": "Das sch\u00f6ne Kleid (du sahst wohl noch ein St\u00fcck davon,", "tokens": ["Das", "sch\u00f6\u00b7ne", "Kleid", "(", "du", "sahst", "wohl", "noch", "ein", "St\u00fcck", "da\u00b7von", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "PPER", "VVFIN", "ADV", "ADV", "ART", "NN", "PAV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Scharlachen, fein, mit Silberschn\u00f6rkelwerk besetzt),", "tokens": ["Schar\u00b7la\u00b7chen", ",", "fein", ",", "mit", "Sil\u00b7ber\u00b7schn\u00f6r\u00b7kel\u00b7werk", "be\u00b7setzt", ")", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "ADJD", "$,", "APPR", "NN", "VVPP", "$(", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.9": {"text": "Ist l\u00e4ngst dahin samt alle seinem Zubeh\u00f6r,", "tokens": ["Ist", "l\u00e4ngst", "da\u00b7hin", "samt", "al\u00b7le", "sei\u00b7nem", "Zu\u00b7be\u00b7h\u00f6r", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PAV", "VVFIN", "PIS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Bis auf dies Eisen, dem getreu die Scheide blieb.", "tokens": ["Bis", "auf", "dies", "Ei\u00b7sen", ",", "dem", "ge\u00b7treu", "die", "Schei\u00b7de", "blieb", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "PDS", "NN", "$,", "PRELS", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wem la\u00df ich nun die Waffe? Billig spart ich sie", "tokens": ["Wem", "la\u00df", "ich", "nun", "die", "Waf\u00b7fe", "?", "Bil\u00b7lig", "spart", "ich", "sie"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "ART", "NN", "$.", "ADJD", "VVFIN", "PPER", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Dem eignen Sohn; er bleibt nur gar zu lange aus!", "tokens": ["Dem", "eig\u00b7nen", "Sohn", ";", "er", "bleibt", "nur", "gar", "zu", "lan\u00b7ge", "aus", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "PPER", "VVFIN", "ADV", "ADV", "PTKA", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Am Ende, f\u00fcrcht ich ernstlich, kommt er nimmermehr;", "tokens": ["Am", "En\u00b7de", ",", "f\u00fcrcht", "ich", "ernst\u00b7lich", ",", "kommt", "er", "nim\u00b7mer\u00b7mehr", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "VVFIN", "PPER", "ADJD", "$,", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Sah ich doch selbst die Mutter bis zur Stunde nicht!", "tokens": ["Sah", "ich", "doch", "selbst", "die", "Mut\u00b7ter", "bis", "zur", "Stun\u00b7de", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ART", "NN", "APPR", "APPRART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Kurzum denn, Alter, deinem Erstgeborenen,", "tokens": ["Kur\u00b7zum", "denn", ",", "Al\u00b7ter", ",", "dei\u00b7nem", "Erst\u00b7ge\u00b7bo\u00b7re\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.16": {"text": "Dem deine Bruderliebe meinen Namen lieh,", "tokens": ["Dem", "dei\u00b7ne", "Bru\u00b7der\u00b7lie\u00b7be", "mei\u00b7nen", "Na\u00b7men", "lieh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "H\u00e4ng ich den S\u00e4bel, bis er ihn gebrauchen kann,", "tokens": ["H\u00e4ng", "ich", "den", "S\u00e4\u00b7bel", ",", "bis", "er", "ihn", "ge\u00b7brau\u00b7chen", "kann", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "$,", "KOUS", "PPER", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Am Nagel \u00fcbers Bettchen, ihm zu H\u00e4upten, auf,", "tokens": ["Am", "Na\u00b7gel", "\u00fc\u00b7bers", "Bett\u00b7chen", ",", "ihm", "zu", "H\u00e4up\u00b7ten", ",", "auf", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NE", "APPRART", "NN", "$,", "PPER", "APPR", "NN", "$,", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Unblutig Spielzeug, das von sch\u00f6ner Jugend wei\u00df", "tokens": ["Un\u00b7blu\u00b7tig", "Spiel\u00b7zeug", ",", "das", "von", "sch\u00f6\u00b7ner", "Ju\u00b7gend", "wei\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "NN", "$,", "PRELS", "APPR", "ADJA", "NN", "VVFIN"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.20": {"text": "Und deinem Knaben keine b\u00f6sen Tr\u00e4ume schafft.", "tokens": ["Und", "dei\u00b7nem", "Kna\u00b7ben", "kei\u00b7ne", "b\u00f6\u00b7sen", "Tr\u00e4u\u00b7me", "schafft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PIAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}