{"textgrid.poem.61564": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus diesem Tal des Kummers", "tokens": ["Aus", "die\u00b7sem", "Tal", "des", "Kum\u00b7mers"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vernimm, o Herr, mein Flehen!", "tokens": ["Ver\u00b7nimm", ",", "o", "Herr", ",", "mein", "Fle\u00b7hen", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "$,", "FM", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Voll Angst, beraubt des Schlummers", "tokens": ["Voll", "Angst", ",", "be\u00b7raubt", "des", "Schlum\u00b7mers"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "NN", "$,", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Lieg' ich die Nacht hindurch in hei\u00dfen Wehen;", "tokens": ["Lieg'", "ich", "die", "Nacht", "hin\u00b7durch", "in", "hei\u00b7\u00dfen", "We\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "PAV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Durch mein Gebein rinnt irr ein fiebernd Grausen,", "tokens": ["Durch", "mein", "Ge\u00b7bein", "rinnt", "irr", "ein", "fie\u00b7bernd", "Grau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ADV", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die wilden Wasser gehen", "tokens": ["Die", "wil\u00b7den", "Was\u00b7ser", "ge\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Hoch \u00fcber meine Seele hin mit Brausen.", "tokens": ["Hoch", "\u00fc\u00b7ber", "mei\u00b7ne", "See\u00b7le", "hin", "mit", "Brau\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nicht wei\u00df ich, wo ich bleibe,", "tokens": ["Nicht", "wei\u00df", "ich", ",", "wo", "ich", "blei\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPER", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von Tr\u00e4nen str\u00f6mt mein Bette;", "tokens": ["Von", "Tr\u00e4\u00b7nen", "str\u00f6mt", "mein", "Bet\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es ist an meinem Leibe", "tokens": ["Es", "ist", "an", "mei\u00b7nem", "Lei\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gesundes nichts und nichts, was Frieden h\u00e4tte.", "tokens": ["Ge\u00b7sun\u00b7des", "nichts", "und", "nichts", ",", "was", "Frie\u00b7den", "h\u00e4t\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PIS", "KON", "PIS", "$,", "PWS", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Von St\u00f6hnen heiser denk' ich meiner Fehle;", "tokens": ["Von", "St\u00f6h\u00b7nen", "hei\u00b7ser", "denk'", "ich", "mei\u00b7ner", "Feh\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "O rette, rette, rette", "tokens": ["O", "ret\u00b7te", ",", "ret\u00b7te", ",", "ret\u00b7te"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["NE", "VVFIN", "$,", "VVFIN", "$,", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Aus dieses Jammers Abgrund meine Seele!", "tokens": ["Aus", "die\u00b7ses", "Jam\u00b7mers", "Ab\u00b7grund", "mei\u00b7ne", "See\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wohl f\u00fchl' ich, ich bin schuldig,", "tokens": ["Wohl", "f\u00fchl'", "ich", ",", "ich", "bin", "schul\u00b7dig", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PPER", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich selbst an meinem Schaden:", "tokens": ["Ich", "selbst", "an", "mei\u00b7nem", "Scha\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch du bist, Herr, geduldig,", "tokens": ["Doch", "du", "bist", ",", "Herr", ",", "ge\u00b7dul\u00b7dig", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "$,", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein Heiland und ein Arzt von gro\u00dfen Gnaden.", "tokens": ["Ein", "Hei\u00b7land", "und", "ein", "Arzt", "von", "gro\u00b7\u00dfen", "Gna\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und w\u00e4re S\u00fcnde, rot wie Blut, die meine,", "tokens": ["Und", "w\u00e4\u00b7re", "S\u00fcn\u00b7de", ",", "rot", "wie", "Blut", ",", "die", "mei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VAFIN", "NN", "$,", "ADJD", "KOKOM", "NN", "$,", "PRELS", "PPOSAT", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Du kannst mich lauter baden,", "tokens": ["Du", "kannst", "mich", "lau\u00b7ter", "ba\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Da\u00df ich wie frischgefallner Schnee erscheine.", "tokens": ["Da\u00df", "ich", "wie", "frischge\u00b7fall\u00b7ner", "Schnee", "er\u00b7schei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "KOKOM", "ADJA", "NN", "NE", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Du kannst auch l\u00f6sen wieder", "tokens": ["Du", "kannst", "auch", "l\u00f6\u00b7sen", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "VVFIN", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dies Leid, das mir geschehen,", "tokens": ["Dies", "Leid", ",", "das", "mir", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Kannst die zerschlagnen Glieder", "tokens": ["Kannst", "die", "zer\u00b7schlag\u00b7nen", "Glie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Aufrichten, da\u00df sie fest wie S\u00e4ulen stehen.", "tokens": ["Auf\u00b7rich\u00b7ten", ",", "da\u00df", "sie", "fest", "wie", "S\u00e4u\u00b7len", "ste\u00b7hen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "ADJD", "KOKOM", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "O birg dein Antlitz nicht zu dieser Stunde!", "tokens": ["O", "birg", "dein", "Ant\u00b7litz", "nicht", "zu", "die\u00b7ser", "Stun\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PPOSAT", "NN", "PTKNEG", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "F\u00fcr Recht la\u00df Gnad' ergehen,", "tokens": ["F\u00fcr", "Recht", "la\u00df", "Gnad'", "er\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVIMP", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Da\u00df ich am Geist, da\u00df ich am Leib gesunde!", "tokens": ["Da\u00df", "ich", "am", "Geist", ",", "da\u00df", "ich", "am", "Leib", "ge\u00b7sun\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "$,", "KOUS", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Sieh an mein qualvoll Schwanken", "tokens": ["Sieh", "an", "mein", "qual\u00b7voll", "Schwan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "PPOSAT", "ADJD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gleich der verdorrten Blume;", "tokens": ["Gleich", "der", "ver\u00b7dorr\u00b7ten", "Blu\u00b7me", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "---+-+-", "measure": "unknown.measure.di"}, "line.3": {"text": "Wie soll mein Staub dir danken,", "tokens": ["Wie", "soll", "mein", "Staub", "dir", "dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPOSAT", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So du der Gruft mich gibst zum Eigentume?", "tokens": ["So", "du", "der", "Gruft", "mich", "gibst", "zum", "Ei\u00b7gen\u00b7tu\u00b7me", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ART", "NN", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die Toten schweigen deiner Herrlichkeiten;", "tokens": ["Die", "To\u00b7ten", "schwei\u00b7gen", "dei\u00b7ner", "Herr\u00b7lich\u00b7kei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Doch hell zu deinem Ruhme", "tokens": ["Doch", "hell", "zu", "dei\u00b7nem", "Ruh\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Will ich mein klingend Harfenspiel besaiten.", "tokens": ["Will", "ich", "mein", "klin\u00b7gend", "Har\u00b7fen\u00b7spiel", "be\u00b7sai\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "O hilf, da\u00df ich den Zagen", "tokens": ["O", "hilf", ",", "da\u00df", "ich", "den", "Za\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NE", "$,", "KOUS", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dein gn\u00e4dig Walten deute,", "tokens": ["Dein", "gn\u00e4\u00b7dig", "Wal\u00b7ten", "deu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und wie du Not und Klagen", "tokens": ["Und", "wie", "du", "Not", "und", "Kla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In Reigen kehrst und nimmst dem ", "tokens": ["In", "Rei\u00b7gen", "kehrst", "und", "nimmst", "dem"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "KON", "VVFIN", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Denn sanft im S\u00e4useln kommst du nach dem Wetter;", "tokens": ["Denn", "sanft", "im", "S\u00e4u\u00b7seln", "kommst", "du", "nach", "dem", "Wet\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "O komm, o hilf auch heute,", "tokens": ["O", "komm", ",", "o", "hilf", "auch", "heu\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "FM", "FM", "ADV", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Mein Fels und meine Burg, mein Hort und Retter!", "tokens": ["Mein", "Fels", "und", "mei\u00b7ne", "Burg", ",", "mein", "Hort", "und", "Ret\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}