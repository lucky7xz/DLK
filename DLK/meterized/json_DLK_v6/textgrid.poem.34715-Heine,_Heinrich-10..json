{"textgrid.poem.34715": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "10.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es hat die warme Fr\u00fchlingsnacht", "tokens": ["Es", "hat", "die", "war\u00b7me", "Fr\u00fch\u00b7lings\u00b7nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Blumen hervorgetrieben,", "tokens": ["Die", "Blu\u00b7men", "her\u00b7vor\u00b7ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und nimmt mein Herz sich nicht in acht,", "tokens": ["Und", "nimmt", "mein", "Herz", "sich", "nicht", "in", "acht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PRF", "PTKNEG", "APPR", "CARD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So wird es sich wieder verlieben.", "tokens": ["So", "wird", "es", "sich", "wie\u00b7der", "ver\u00b7lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "ADV", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Doch welche von den Blumen all'n", "tokens": ["Doch", "wel\u00b7che", "von", "den", "Blu\u00b7men", "all'n"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAT", "APPR", "ART", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wird mir das Herz umgarnen?", "tokens": ["Wird", "mir", "das", "Herz", "um\u00b7gar\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es wollen die singenden Nachtigall'n", "tokens": ["Es", "wol\u00b7len", "die", "sin\u00b7gen\u00b7den", "Nach\u00b7ti\u00b7gall'n"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Mich vor der Lilie warnen.", "tokens": ["Mich", "vor", "der", "Li\u00b7lie", "war\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}