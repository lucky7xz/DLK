{"dta.poem.21842": {"metadata": {"author": {"name": "Stieler, Kaspar von", "birth": "N.A.", "death": "N.A."}, "title": "Dem Eyfferigen:", "genre": "Lyrik", "period": "N.A.", "pub_year": "1660", "urn": "urn:nbn:de:kobv:b4-20586-2", "language": ["de:0.99"], "booktitle": "Filidor der Dorfferer [i. e. Stieler, Kaspar von]: Die Geharnschte Venus. Hamburg, 1660."}, "poem": {"stanza.1": {"line.1": {"text": "Sagt' ich dir nicht/ du solltst dich las-", "tokens": ["Sagt'", "ich", "dir", "nicht", "/", "du", "solltst", "dich", "las"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "PTKNEG", "$(", "PPER", "VMFIN", "PRF", "TRUNC"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "sen halten?", "tokens": ["sen", "hal\u00b7ten", "?"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Doch hastu dieses lezte Zehn/", "tokens": ["Doch", "has\u00b7tu", "die\u00b7ses", "lez\u00b7te", "Zehn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PDAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "als w\u00e4rstu schwanger/ m\u00fcssen sehn.", "tokens": ["als", "w\u00e4rs\u00b7tu", "schwan\u00b7ger", "/", "m\u00fcs\u00b7sen", "sehn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOKOM", "VAFIN", "ADJD", "$(", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nu/ h\u00f6r ich/ ist die Galle dir zerspalten.", "tokens": ["Nu", "/", "h\u00f6r", "ich", "/", "ist", "die", "Gal\u00b7le", "dir", "zer\u00b7spal\u00b7ten", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "VVFIN", "PPER", "$(", "VAFIN", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die Schuld ist dein. H\u00e4ttstu gelacht/", "tokens": ["Die", "Schuld", "ist", "dein", ".", "H\u00e4tts\u00b7tu", "ge\u00b7lacht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "$.", "NE", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "wie ichs/ der/ und der dritte macht", "tokens": ["wie", "ichs", "/", "der", "/", "und", "der", "drit\u00b7te", "macht"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "$(", "ART", "$(", "KON", "ART", "ADJA", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "so schmekkt\u2019 es dir im Munde nu nicht", "tokens": ["so", "schmekkt'", "es", "dir", "im", "Mun\u00b7de", "nu", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPRART", "NN", "ADV", "PTKNEG"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.9": {"text": "bitter.", "tokens": ["bit\u00b7ter", "."], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.10": {"text": "Der ist geschossen im Gehirn/", "tokens": ["Der", "ist", "ge\u00b7schos\u00b7sen", "im", "Ge\u00b7hirn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "VVPP", "APPRART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "wer murrt und runzelt denn die Stirn", "tokens": ["wer", "murrt", "und", "run\u00b7zelt", "denn", "die", "Stirn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "KON", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Wenn Amor singt und Venus schl\u00e4gt", "tokens": ["Wenn", "A\u00b7mor", "singt", "und", "Ve\u00b7nus", "schl\u00e4gt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "VVFIN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "die Zitter.", "tokens": ["die", "Zit\u00b7ter", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}}}}