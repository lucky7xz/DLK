{"textgrid.poem.60475": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie Merlin", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie Merlin", "tokens": ["Wie", "Mer\u00b7lin"], "token_info": ["word", "word"], "pos": ["PWAV", "NE"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "M\u00f6cht ich durch die W\u00e4lder ziehn;", "tokens": ["M\u00f6cht", "ich", "durch", "die", "W\u00e4l\u00b7der", "ziehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was die St\u00fcrme wehen,", "tokens": ["Was", "die", "St\u00fcr\u00b7me", "we\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Was die Donner rollen", "tokens": ["Was", "die", "Don\u00b7ner", "rol\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "ART", "NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Und die Blitze wollen,", "tokens": ["Und", "die", "Blit\u00b7ze", "wol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VMFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Was die B\u00e4ume sprechen,", "tokens": ["Was", "die", "B\u00e4u\u00b7me", "spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Wenn sie brechen,", "tokens": ["Wenn", "sie", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "M\u00f6cht ich wie Merlin verstehen.", "tokens": ["M\u00f6cht", "ich", "wie", "Mer\u00b7lin", "ver\u00b7ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "KOKOM", "NE", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Voll Gewitterlust", "tokens": ["Voll", "Ge\u00b7wit\u00b7ter\u00b7lust"], "token_info": ["word", "word"], "pos": ["ADJD", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Wirft im Sturme hin", "tokens": ["Wirft", "im", "Stur\u00b7me", "hin"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "NN", "PTKVZ"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Sein Gewand Merlin,", "tokens": ["Sein", "Ge\u00b7wand", "Mer\u00b7lin", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NE", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Da\u00df die L\u00fcfte k\u00fchlen,", "tokens": ["Da\u00df", "die", "L\u00fcf\u00b7te", "k\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Blitze ihm besp\u00fclen", "tokens": ["Blit\u00b7ze", "ihm", "be\u00b7sp\u00fc\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Seine nackte Brust.", "tokens": ["Sei\u00b7ne", "nack\u00b7te", "Brust", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Wurzelf\u00e4den streckt", "tokens": ["Wur\u00b7zel\u00b7f\u00e4\u00b7den", "streckt"], "token_info": ["word", "word"], "pos": ["NN", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Eiche in den Grund,", "tokens": ["Ei\u00b7che", "in", "den", "Grund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Unten saugt versteckt", "tokens": ["Un\u00b7ten", "saugt", "ver\u00b7steckt"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJD", "VVPP"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Tausendfach ihr Mund", "tokens": ["Tau\u00b7send\u00b7fach", "ihr", "Mund"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Leben aus geheimen Quellen,", "tokens": ["Le\u00b7ben", "aus", "ge\u00b7hei\u00b7men", "Quel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die den Stamm gen Himmel schwellen.", "tokens": ["Die", "den", "Stamm", "gen", "Him\u00b7mel", "schwel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Flattern l\u00e4\u00dft sein Haar Merlin", "tokens": ["Flat\u00b7tern", "l\u00e4\u00dft", "sein", "Haar", "Mer\u00b7lin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "In der Sturmnacht her und hin,", "tokens": ["In", "der", "Sturm\u00b7nacht", "her", "und", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und es spr\u00fchn die feurig falben", "tokens": ["Und", "es", "spr\u00fchn", "die", "feu\u00b7rig", "fal\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Blitze, ihm das Haupt zu salben;", "tokens": ["Blit\u00b7ze", ",", "ihm", "das", "Haupt", "zu", "sal\u00b7ben", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Die Natur, die offenbare,", "tokens": ["Die", "Na\u00b7tur", ",", "die", "of\u00b7fen\u00b7ba\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Traulich sich mit ihm verschwisternd,", "tokens": ["Trau\u00b7lich", "sich", "mit", "ihm", "ver\u00b7schwis\u00b7ternd", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PRF", "APPR", "PPER", "VVPP", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.7": {"text": "Tr\u00e4nkt sein Herz, wenn Blitze knisternd", "tokens": ["Tr\u00e4nkt", "sein", "Herz", ",", "wenn", "Blit\u00b7ze", "knis\u00b7ternd"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "KOUS", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "K\u00fcssen seine schwarzen Haare. \u2013 \u2013", "tokens": ["K\u00fcs\u00b7sen", "sei\u00b7ne", "schwar\u00b7zen", "Haa\u00b7re", ".", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$.", "$(", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Das Gewitter ist vollbracht,", "tokens": ["Das", "Ge\u00b7wit\u00b7ter", "ist", "voll\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Stille ward die Nacht;", "tokens": ["Stil\u00b7le", "ward", "die", "Nacht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Heiter in die tiefsten Gr\u00fcnde", "tokens": ["Hei\u00b7ter", "in", "die", "tiefs\u00b7ten", "Gr\u00fcn\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist der Himmel nach dem Streite;", "tokens": ["Ist", "der", "Him\u00b7mel", "nach", "dem", "Strei\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wer die Waldesruh verst\u00fcnde", "tokens": ["Wer", "die", "Wal\u00b7des\u00b7ruh", "ver\u00b7st\u00fcn\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "ART", "NN", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie Merlin, der Eingeweihte!", "tokens": ["Wie", "Mer\u00b7lin", ",", "der", "Ein\u00b7ge\u00b7weih\u00b7te", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "NE", "$,", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Fr\u00fchlingsnacht! kein L\u00fcftchen weht,", "tokens": ["Fr\u00fch\u00b7lings\u00b7nacht", "!", "kein", "L\u00fcft\u00b7chen", "weht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Nicht die schwanksten Halme nicken,", "tokens": ["Nicht", "die", "schwanks\u00b7ten", "Hal\u00b7me", "ni\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Jedes Blatt, von Mondesblicken", "tokens": ["Je\u00b7des", "Blatt", ",", "von", "Mon\u00b7des\u00b7bli\u00b7cken"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PIAT", "NN", "$,", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie bezaubert, stille steht.", "tokens": ["Wie", "be\u00b7zau\u00b7bert", ",", "stil\u00b7le", "steht", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "$,", "ADJA", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Still die G\u00f6tter zu beschleichen", "tokens": ["Still", "die", "G\u00f6t\u00b7ter", "zu", "be\u00b7schlei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "ART", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die ewigen Gesetze,", "tokens": ["Und", "die", "e\u00b7wi\u00b7gen", "Ge\u00b7set\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In den Schatten hoher Eichen", "tokens": ["In", "den", "Schat\u00b7ten", "ho\u00b7her", "Ei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wacht der Zaubrer, einsam sinnend,", "tokens": ["Wacht", "der", "Zaub\u00b7rer", ",", "ein\u00b7sam", "sin\u00b7nend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Zwischen ihre Zweige spinnend", "tokens": ["Zwi\u00b7schen", "ih\u00b7re", "Zwei\u00b7ge", "spin\u00b7nend"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Heimliche Gedankennetze.", "tokens": ["Heim\u00b7li\u00b7che", "Ge\u00b7dan\u00b7ken\u00b7net\u00b7ze", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Stimmen, die den andern schweigen,", "tokens": ["Stim\u00b7men", ",", "die", "den", "an\u00b7dern", "schwei\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "ADJA", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jenseits ihrer H\u00f6rbarkeiten,", "tokens": ["Jen\u00b7seits", "ih\u00b7rer", "H\u00f6r\u00b7bar\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00f6rt Merlin vor\u00fcbergleiten,", "tokens": ["H\u00f6rt", "Mer\u00b7lin", "vor\u00b7\u00fc\u00b7berg\u00b7lei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "NE", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alles rauscht im vollen Reigen", "tokens": ["Al\u00b7les", "rauscht", "im", "vol\u00b7len", "Rei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn die K\u00f6nigin der Elfen", "tokens": ["Denn", "die", "K\u00f6\u00b7ni\u00b7gin", "der", "El\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Oder eine kluge Norn", "tokens": ["O\u00b7der", "ei\u00b7ne", "klu\u00b7ge", "Norn"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "H\u00e4lt, dem Sinne nachzuhelfen,", "tokens": ["H\u00e4lt", ",", "dem", "Sin\u00b7ne", "nach\u00b7zu\u00b7hel\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ART", "NN", "VVIZU", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ihm ans Ohr ein Zauberhorn.", "tokens": ["Ihm", "ans", "Ohr", "ein", "Zau\u00b7ber\u00b7horn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Rieseln h\u00f6rt er, springend sch\u00e4umen", "tokens": ["Rie\u00b7seln", "h\u00f6rt", "er", ",", "sprin\u00b7gend", "sch\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "$,", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Lebensfluten in den B\u00e4umen;", "tokens": ["Le\u00b7bens\u00b7flu\u00b7ten", "in", "den", "B\u00e4u\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "V\u00f6gel schlummern auf den \u00c4sten", "tokens": ["V\u00f6\u00b7gel", "schlum\u00b7mern", "auf", "den", "\u00c4s\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Nach des Tages Liebesfesten,", "tokens": ["Nach", "des", "Ta\u00b7ges", "Lie\u00b7bes\u00b7fes\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Doch ihr Schlaf ist auch begl\u00fcckt;", "tokens": ["Doch", "ihr", "Schlaf", "ist", "auch", "be\u00b7gl\u00fcckt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Lauschend h\u00f6rt Merlin entz\u00fcckt", "tokens": ["Lau\u00b7schend", "h\u00f6rt", "Mer\u00b7lin", "ent\u00b7z\u00fcckt"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "NE", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Unter ihrem Brustgefieder", "tokens": ["Un\u00b7ter", "ih\u00b7rem", "Brust\u00b7ge\u00b7fie\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Tr\u00e4umen ihre k\u00fcnftgen Lieder.", "tokens": ["Tr\u00e4u\u00b7men", "ih\u00b7re", "k\u00fcnft\u00b7gen", "Lie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Klingend str\u00f6mt des Mondes Licht", "tokens": ["Klin\u00b7gend", "str\u00f6mt", "des", "Mon\u00b7des", "Licht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "Auf die Eich und Hagerose,", "tokens": ["Auf", "die", "Eich", "und", "Ha\u00b7ge\u00b7ro\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Und im Kelch der feinsten Moose", "tokens": ["Und", "im", "Kelch", "der", "feins\u00b7ten", "Moo\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "T\u00f6nt das ewige Gedicht.", "tokens": ["T\u00f6nt", "das", "e\u00b7wi\u00b7ge", "Ge\u00b7dicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}