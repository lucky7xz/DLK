{"textgrid.poem.34734": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "29.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es war ein alter K\u00f6nig,", "tokens": ["Es", "war", "ein", "al\u00b7ter", "K\u00f6\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sein Herz war schwer, sein Haupt war grau;", "tokens": ["Sein", "Herz", "war", "schwer", ",", "sein", "Haupt", "war", "grau", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der arme alte K\u00f6nig,", "tokens": ["Der", "ar\u00b7me", "al\u00b7te", "K\u00f6\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Er nahm eine junge Frau.", "tokens": ["Er", "nahm", "ei\u00b7ne", "jun\u00b7ge", "Frau", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Es war ein sch\u00f6ner Page,", "tokens": ["Es", "war", "ein", "sch\u00f6\u00b7ner", "Pa\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Blond war sein Haupt, leicht war sein Sinn;", "tokens": ["Blond", "war", "sein", "Haupt", ",", "leicht", "war", "sein", "Sinn", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "$,", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er trug die seidne Schleppe", "tokens": ["Er", "trug", "die", "seid\u00b7ne", "Schlep\u00b7pe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der jungen K\u00f6nigin.", "tokens": ["Der", "jun\u00b7gen", "K\u00f6\u00b7ni\u00b7gin", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Kennst du das alte Liedchen?", "tokens": ["Kennst", "du", "das", "al\u00b7te", "Lied\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Es klingt so s\u00fc\u00df, es klingt so tr\u00fcb!", "tokens": ["Es", "klingt", "so", "s\u00fc\u00df", ",", "es", "klingt", "so", "tr\u00fcb", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$,", "PPER", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie mu\u00dften beide sterben,", "tokens": ["Sie", "mu\u00df\u00b7ten", "bei\u00b7de", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie hatten sich viel zu lieb.", "tokens": ["Sie", "hat\u00b7ten", "sich", "viel", "zu", "lieb", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "ADV", "PTKA", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}