{"dta.poem.17427": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "48.  Eins kans nicht ohn das andre.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Zwey mussen es vollziehn: ich kans nicht ohne GOtt/", "tokens": ["Zwey", "mus\u00b7sen", "es", "voll\u00b7ziehn", ":", "ich", "kans", "nicht", "oh\u00b7ne", "Gott", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "VMFIN", "PPER", "VVINF", "$.", "PPER", "VMFIN", "PTKNEG", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und GOtt nicht ohne mich: das ich entgeh dem Tod.", "tokens": ["Und", "Gott", "nicht", "oh\u00b7ne", "mich", ":", "das", "ich", "ent\u00b7geh", "dem", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PTKNEG", "APPR", "PPER", "$.", "PRELS", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}