{"textgrid.poem.41160": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "21.", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seht ihr dort den alten Bau von Stein,", "tokens": ["Seht", "ihr", "dort", "den", "al\u00b7ten", "Bau", "von", "Stein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Totenstille ist's in ihm und leer,", "tokens": ["To\u00b7tens\u00b7til\u00b7le", "ist's", "in", "ihm", "und", "leer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "PPER", "KON", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Die Gem\u00e4cher sind gerissen ein,", "tokens": ["Die", "Ge\u00b7m\u00e4\u00b7cher", "sind", "ge\u00b7ris\u00b7sen", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Und die Eulen flattern drin umher.", "tokens": ["Und", "die", "Eu\u00b7len", "flat\u00b7tern", "drin", "um\u00b7her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVINF", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Einer einst bewohnte dieses Haus,", "tokens": ["Ei\u00b7ner", "einst", "be\u00b7wohn\u00b7te", "die\u00b7ses", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "ADJA", "PDAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Um ihn lebend schon des Hades Nacht,", "tokens": ["Um", "ihn", "le\u00b7bend", "schon", "des", "Ha\u00b7des", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "ADJD", "ADV", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Hier kein Freund ging freudig ein und aus,", "tokens": ["Hier", "kein", "Freund", "ging", "freu\u00b7dig", "ein", "und", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVFIN", "ADJD", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Hier ward nie geweint und nie gelacht.", "tokens": ["Hier", "ward", "nie", "ge\u00b7weint", "und", "nie", "ge\u00b7lacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "VVPP", "KON", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "Hier schlo\u00df Liebe niemals einen Bund,", "tokens": ["Hier", "schlo\u00df", "Lie\u00b7be", "nie\u00b7mals", "ei\u00b7nen", "Bund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "ADV", "ART", "NN", "$,"], "meter": "-++-+-+-+", "measure": "unknown.measure.penta"}, "line.10": {"text": "Hier war keine Mutter, war kein Kind,", "tokens": ["Hier", "war", "kei\u00b7ne", "Mut\u00b7ter", ",", "war", "kein", "Kind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "$,", "VAFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.11": {"text": "Nur ein m\u00fcrr'scher Diener und ein Hund", "tokens": ["Nur", "ein", "m\u00fcrr'\u00b7scher", "Die\u00b7ner", "und", "ein", "Hund"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "KON", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.12": {"text": "Waren hier des Herren Hausgesind'.", "tokens": ["Wa\u00b7ren", "hier", "des", "Her\u00b7ren", "Haus\u00b7ge\u00b7sind'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.13": {"text": "Wer ", "tokens": ["Wer"], "token_info": ["word"], "pos": ["PWS"], "meter": "-", "measure": "single.down"}, "line.14": {"text": "Ha! ein Wuchrer, sein sich Gott erbarm'!", "tokens": ["Ha", "!", "ein", "Wuch\u00b7rer", ",", "sein", "sich", "Gott", "er\u00b7barm'", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ART", "NN", "$,", "PPOSAT", "PRF", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.15": {"text": "In der Eisentruh' an Golde reich,", "tokens": ["In", "der", "Eis\u00b7en\u00b7truh'", "an", "Gol\u00b7de", "reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.16": {"text": "In dem Herzen doch an Liebe arm.", "tokens": ["In", "dem", "Her\u00b7zen", "doch", "an", "Lie\u00b7be", "arm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "APPR", "NN", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.17": {"text": "Kam ein Bettler, klopfend an das Haus,", "tokens": ["Kam", "ein", "Bett\u00b7ler", ",", "klop\u00b7fend", "an", "das", "Haus", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.18": {"text": "Go\u00df sich oft auf ihn ein Tintenfa\u00df,", "tokens": ["Go\u00df", "sich", "oft", "auf", "ihn", "ein", "Tin\u00b7ten\u00b7fa\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PRF", "ADV", "APPR", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.19": {"text": "Oder st\u00fcrzte wild der Hund heraus,", "tokens": ["O\u00b7der", "st\u00fcrz\u00b7te", "wild", "der", "Hund", "he\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.20": {"text": "Da\u00df der Arme fortfloh leichenbla\u00df.", "tokens": ["Da\u00df", "der", "Ar\u00b7me", "fort\u00b7floh", "lei\u00b7chen\u00b7bla\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.21": {"text": "Mancher trug noch seine letzte Kraft,", "tokens": ["Man\u00b7cher", "trug", "noch", "sei\u00b7ne", "letz\u00b7te", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.22": {"text": "Hoffend Zinse, in dies finstre Haus,", "tokens": ["Hof\u00b7fend", "Zin\u00b7se", ",", "in", "dies", "finst\u00b7re", "Haus", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "APPR", "PDS", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.23": {"text": "Doch was froh nach oben ward gebracht,", "tokens": ["Doch", "was", "froh", "nach", "o\u00b7ben", "ward", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADJD", "APPR", "ADV", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.24": {"text": "Kam nach unten nimmerfroh heraus.", "tokens": ["Kam", "nach", "un\u00b7ten", "nim\u00b7mer\u00b7froh", "he\u00b7raus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ADV", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.25": {"text": "Fest im Lehnstuhl sa\u00df er wie im Bann,", "tokens": ["Fest", "im", "Lehn\u00b7stuhl", "sa\u00df", "er", "wie", "im", "Bann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VVFIN", "PPER", "KOKOM", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.26": {"text": "Bleich, ein\u00e4ugig, z\u00e4hlend, w\u00e4gend Gold,", "tokens": ["Bleich", ",", "ein\u00b7\u00e4u\u00b7gig", ",", "z\u00e4h\u00b7lend", ",", "w\u00e4\u00b7gend", "Gold", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,", "VVPP", "$,", "KOUS", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.27": {"text": "Horchte man, selbst in der Nacht hat's dann", "tokens": ["Horch\u00b7te", "man", ",", "selbst", "in", "der", "Nacht", "hat's", "dann"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "$,", "ADV", "APPR", "ART", "NN", "VAFIN", "ADV"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.28": {"text": "Oft get\u00f6nt, wie wenn man Taler rollt.", "tokens": ["Oft", "ge\u00b7t\u00f6nt", ",", "wie", "wenn", "man", "Ta\u00b7ler", "rollt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "KOKOM", "KOUS", "PIS", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.29": {"text": "Als er so einst oben sa\u00df allein,", "tokens": ["Als", "er", "so", "einst", "o\u00b7ben", "sa\u00df", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADV", "VVFIN", "ADV", "$,"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.30": {"text": "Rechnend noch in mittern\u00e4cht'ger Stund',", "tokens": ["Rech\u00b7nend", "noch", "in", "mit\u00b7ter\u00b7n\u00e4cht'\u00b7ger", "Stund'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.31": {"text": "Trat zur T\u00fcre ein Gerippe ein,", "tokens": ["Trat", "zur", "T\u00fc\u00b7re", "ein", "Ge\u00b7rip\u00b7pe", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.32": {"text": "Legt die Hand ihm kalt auf Herz und Mund.", "tokens": ["Legt", "die", "Hand", "ihm", "kalt", "auf", "Herz", "und", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "ADJD", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.33": {"text": "Schreien wollt' er, konnt' es nimmermehr,", "tokens": ["Schrei\u00b7en", "wollt'", "er", ",", "konnt'", "es", "nim\u00b7mer\u00b7mehr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "$,", "VMFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.34": {"text": "'s war der Tod \u2013 doch schreiben noch mit Not:", "tokens": ["'s", "war", "der", "Tod", "\u2013", "doch", "schrei\u00b7ben", "noch", "mit", "Not", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$(", "ADV", "VVFIN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.35": {"text": "\u00bbhab' versteckt was in\u00ab \u2013 schrieb zitternd er", "tokens": ["\u00bb", "hab'", "ver\u00b7steckt", "was", "in", "\u00ab", "\u2013", "schrieb", "zit\u00b7ternd", "er"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["$(", "VAFIN", "VVPP", "PRELS", "APPR", "$(", "$(", "ADJD", "VVPP", "PPER"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.36": {"text": "Und sank drauf in seinen Lehnstuhl tot.", "tokens": ["Und", "sank", "drauf", "in", "sei\u00b7nen", "Lehn\u00b7stuhl", "tot", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "APPR", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.37": {"text": "Offen blickt sein Auge, hat geblickt,", "tokens": ["Of\u00b7fen", "blickt", "sein", "Au\u00b7ge", ",", "hat", "ge\u00b7blickt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$,", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.38": {"text": "Als wenn's hier noch wollte suchen was,", "tokens": ["Als", "wenn's", "hier", "noch", "woll\u00b7te", "su\u00b7chen", "was", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "ADV", "VMFIN", "VVINF", "PWS", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.39": {"text": "Niemand hat es liebend zugedr\u00fcckt,", "tokens": ["Nie\u00b7mand", "hat", "es", "lie\u00b7bend", "zu\u00b7ge\u00b7dr\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.40": {"text": "Und so morgens noch im Stuhl er sa\u00df.", "tokens": ["Und", "so", "mor\u00b7gens", "noch", "im", "Stuhl", "er", "sa\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADV", "APPRART", "NN", "PPER", "VVFIN", "$."], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.41": {"text": "Niemand gab zum Grab ihm das Geleit,", "tokens": ["Nie\u00b7mand", "gab", "zum", "Grab", "ihm", "das", "Ge\u00b7leit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPRART", "NN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.42": {"text": "Nur der m\u00fcrr'sche Diener und der Hund;", "tokens": ["Nur", "der", "m\u00fcrr'\u00b7sche", "Die\u00b7ner", "und", "der", "Hund", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "KON", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.43": {"text": "Wer es sah, dem kam kein Herzeleid,", "tokens": ["Wer", "es", "sah", ",", "dem", "kam", "kein", "Her\u00b7ze\u00b7leid", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "PRELS", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.44": {"text": "Kalt sie senkten ihn in Grabesgrund.", "tokens": ["Kalt", "sie", "senk\u00b7ten", "ihn", "in", "Gra\u00b7bes\u00b7grund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.45": {"text": "All sein Gut nahm das Gericht zur Hand,", "tokens": ["All", "sein", "Gut", "nahm", "das", "Ge\u00b7richt", "zur", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.46": {"text": "Lie\u00df auch suchen, ob was sei versteckt,", "tokens": ["Lie\u00df", "auch", "su\u00b7chen", ",", "ob", "was", "sei", "ver\u00b7steckt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "VVINF", "$,", "KOUS", "PIS", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.47": {"text": "Denn von einem gro\u00dfen Diamant", "tokens": ["Denn", "von", "ei\u00b7nem", "gro\u00b7\u00dfen", "Di\u00b7a\u00b7mant"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.48": {"text": "Sprach man laut, doch wurde nichts entdeckt.", "tokens": ["Sprach", "man", "laut", ",", "doch", "wur\u00b7de", "nichts", "ent\u00b7deckt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIS", "ADJD", "$,", "ADV", "VAFIN", "PIS", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.49": {"text": "Niemand wollt' bewohnen dieses Haus,", "tokens": ["Nie\u00b7mand", "wollt'", "be\u00b7woh\u00b7nen", "die\u00b7ses", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "VVINF", "PDAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.50": {"text": "Drum zu einer Scheuer ward's gemacht,", "tokens": ["Drum", "zu", "ei\u00b7ner", "Scheu\u00b7er", "ward's", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.51": {"text": "Und der Lehnstuhl wurde als ein Graus,", "tokens": ["Und", "der", "Lehn\u00b7stuhl", "wur\u00b7de", "als", "ein", "Graus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.52": {"text": "Wo er noch steht, unters Dach gebracht.", "tokens": ["Wo", "er", "noch", "steht", ",", "un\u00b7ters", "Dach", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVFIN", "$,", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.53": {"text": "Oft bei Tag ein Kater auf ihm sitzt,", "tokens": ["Oft", "bei", "Tag", "ein", "Ka\u00b7ter", "auf", "ihm", "sitzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "ART", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.54": {"text": "Schwarz, ein\u00e4ugig und unheimlich ganz,", "tokens": ["Schwarz", ",", "ein\u00b7\u00e4u\u00b7gig", "und", "un\u00b7heim\u00b7lich", "ganz", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "KON", "ADJD", "ADV", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.55": {"text": "Hell aus seinem einzlen Aug' es blitzt,", "tokens": ["Hell", "aus", "sei\u00b7nem", "einz\u00b7len", "Aug'", "es", "blitzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.56": {"text": "Als w\u00e4r's aus dem Stuhl ein Demantglanz.", "tokens": ["Als", "w\u00e4r's", "aus", "dem", "Stuhl", "ein", "De\u00b7mant\u00b7glanz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.57": {"text": "Doch wenn nachts ums Haus die Eule kreist,", "tokens": ["Doch", "wenn", "nachts", "ums", "Haus", "die", "Eu\u00b7le", "kreist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.58": {"text": "H\u00f6rt man Silberkl\u00e4nge wohlbekannt,", "tokens": ["H\u00f6rt", "man", "Sil\u00b7ber\u00b7kl\u00e4n\u00b7ge", "wohl\u00b7be\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.59": {"text": "In dem Lehnstuhl sitzt des Wuchrers Geist", "tokens": ["In", "dem", "Lehn\u00b7stuhl", "sitzt", "des", "Wuc\u00b7hrers", "Geist"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.60": {"text": "Mit dem Diamant in ihn gebannt.", "tokens": ["Mit", "dem", "Di\u00b7a\u00b7mant", "in", "ihn", "ge\u00b7bannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}