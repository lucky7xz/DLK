{"textgrid.poem.63746": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "6.", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Hirn voll Tand, im Herzen \u00f6de Leere,", "tokens": ["Das", "Hirn", "voll", "Tand", ",", "im", "Her\u00b7zen", "\u00f6\u00b7de", "Lee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "$,", "APPRART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sorgsam frisiert, geschminkt die welke Haut,", "tokens": ["Sorg\u00b7sam", "fri\u00b7siert", ",", "ge\u00b7schminkt", "die", "wel\u00b7ke", "Haut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,", "VVPP", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Mit jedes Hauses kleinem Klatsch vertraut,", "tokens": ["Mit", "je\u00b7des", "Hau\u00b7ses", "klei\u00b7nem", "Klatsch", "ver\u00b7traut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Als ob in aller Welt nichts H\u00f6h'res w\u00e4re,", "tokens": ["Als", "ob", "in", "al\u00b7ler", "Welt", "nichts", "H\u00f6h'\u00b7res", "w\u00e4\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "APPR", "PIAT", "NN", "PIAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "So schlendert dort der Veteran vom Heere", "tokens": ["So", "schlen\u00b7dert", "dort", "der", "Ve\u00b7te\u00b7ran", "vom", "Hee\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Stutzer, h\u00f6chlich von sich selbst erbaut,", "tokens": ["Der", "Stut\u00b7zer", ",", "h\u00f6ch\u00b7lich", "von", "sich", "selbst", "er\u00b7baut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "APPR", "PRF", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Voll Stolz, da\u00df er mit Ehren so ergraut", "tokens": ["Voll", "Stolz", ",", "da\u00df", "er", "mit", "Eh\u00b7ren", "so", "er\u00b7graut"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "NN", "$,", "KOUS", "PPER", "APPR", "NN", "ADV", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Im strengen Waffendienste der Cythere.", "tokens": ["Im", "stren\u00b7gen", "Waf\u00b7fen\u00b7diens\u00b7te", "der", "Cy\u00b7the\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Beruf und Ziel und Inhalt seines Lebens", "tokens": ["Be\u00b7ruf", "und", "Ziel", "und", "In\u00b7halt", "sei\u00b7nes", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "KON", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "War Frauenliebe; da ihn die verlassen,", "tokens": ["War", "Frau\u00b7en\u00b7lie\u00b7be", ";", "da", "ihn", "die", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$.", "KOUS", "PPER", "ART", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ist er zu nichts mehr auf der Welt zu brauchen,", "tokens": ["Ist", "er", "zu", "nichts", "mehr", "auf", "der", "Welt", "zu", "brau\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PIS", "ADV", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Als nur \u2013 ein Vorbild mannesw\u00fcrd'gen Strebens", "tokens": ["Als", "nur", "\u2013", "ein", "Vor\u00b7bild", "man\u00b7nes\u00b7w\u00fcrd'\u00b7gen", "Stre\u00b7bens"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "$(", "ART", "NN", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der goldnen Jugend \u2013 auf Neapels Gassen", "tokens": ["Der", "gold\u00b7nen", "Ju\u00b7gend", "\u2013", "auf", "Nea\u00b7pels", "Gas\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$(", "APPR", "NE", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die langen, schw\u00e4rzlichen Cavours zu rauchen.", "tokens": ["Die", "lan\u00b7gen", ",", "schw\u00e4rz\u00b7li\u00b7chen", "Ca\u00b7vours", "zu", "rau\u00b7chen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}