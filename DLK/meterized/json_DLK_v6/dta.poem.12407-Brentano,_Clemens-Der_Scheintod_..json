{"dta.poem.12407": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Der Scheintod .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1806", "urn": "urn:nbn:de:kobv:b4-20090519157", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Des Jerman Weizers Fraue ward               ", "tokens": ["Des", "Jer\u00b7man", "Wei\u00b7zers", "Frau\u00b7e", "ward"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NE", "NE", "NE", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit gro\u00dfer Angst beschweret,", "tokens": ["Mit", "gro\u00b7\u00dfer", "Angst", "be\u00b7schwe\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Von wunderbarer Krankheit Art,", "tokens": ["Von", "wun\u00b7der\u00b7ba\u00b7rer", "Krank\u00b7heit", "Art", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auch sollt sie bald geb\u00e4hren,", "tokens": ["Auch", "sollt", "sie", "bald", "ge\u00b7b\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sie betet: W\u00e4r das Kind zur Welt,", "tokens": ["Sie", "be\u00b7tet", ":", "W\u00e4r", "das", "Kind", "zur", "Welt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "VAFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Darnach, wenn's Gott dem Herrn gef\u00e4llt,", "tokens": ["Dar\u00b7nach", ",", "wenn's", "Gott", "dem", "Herrn", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "KOUS", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wollt sie auch gerne sterben.", "tokens": ["Wollt", "sie", "auch", "ger\u00b7ne", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sie starb zu ihrer Kinder Leid,", "tokens": ["Sie", "starb", "zu", "ih\u00b7rer", "Kin\u00b7der", "Leid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ward in ein Grab getragen,", "tokens": ["Ward", "in", "ein", "Grab", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Kinder gingen lange Zeit", "tokens": ["Die", "Kin\u00b7der", "gin\u00b7gen", "lan\u00b7ge", "Zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vielmal an allen Tagen,", "tokens": ["Viel\u00b7mal", "an", "al\u00b7len", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wohl auf den Kirchhof zu dem Grab,", "tokens": ["Wohl", "auf", "den", "Kirch\u00b7hof", "zu", "dem", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Sie weinten sich die Aeuglig ab,", "tokens": ["Sie", "wein\u00b7ten", "sich", "die", "A\u00b7e\u00b7ug\u00b7lig", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Im Hause still zu bleiben.", "tokens": ["Im", "Hau\u00b7se", "still", "zu", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Als nun die Frau neun Tage lang,", "tokens": ["Als", "nun", "die", "Frau", "neun", "Ta\u00b7ge", "lang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "CARD", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Grabe hat gelegen,", "tokens": ["Im", "Gra\u00b7be", "hat", "ge\u00b7le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Kinder nahmen ihren Gang,", "tokens": ["Die", "Kin\u00b7der", "nah\u00b7men", "ih\u00b7ren", "Gang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zum Kirchhof th\u00e4ten gehen,", "tokens": ["Zum", "Kirch\u00b7hof", "th\u00e4\u00b7ten", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Da h\u00f6rten sie ein lieblich Stimm", "tokens": ["Da", "h\u00f6r\u00b7ten", "sie", "ein", "lieb\u00b7lich", "Stimm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Auf ihrer Mutter Grab vernimm,", "tokens": ["Auf", "ih\u00b7rer", "Mut\u00b7ter", "Grab", "ver\u00b7nimm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ein Kinder-Liedlein singen.", "tokens": ["Ein", "Kin\u00b7der\u00b7Lied\u00b7lein", "sin\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Nun schlaf mein liebes Kindelein,", "tokens": ["Nun", "schlaf", "mein", "lie\u00b7bes", "Kin\u00b7de\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sangs mit der Mutter Tone,", "tokens": ["Sangs", "mit", "der", "Mut\u00b7ter", "To\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Kinder liefen freudig heim,", "tokens": ["Die", "Kin\u00b7der", "lie\u00b7fen", "freu\u00b7dig", "heim", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit einer Blumenkrone:", "tokens": ["Mit", "ei\u00b7ner", "Blu\u00b7men\u00b7kro\u00b7ne", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "\u201eo Vater, lieber Vater mein!", "tokens": ["\u201e", "o", "Va\u00b7ter", ",", "lie\u00b7ber", "Va\u00b7ter", "mein", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "FM", "NN", "$,", "ADV", "NN", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "\u201egeh mit uns auf den Kirchhof ein,", "tokens": ["\u201e", "geh", "mit", "uns", "auf", "den", "Kirch\u00b7hof", "ein", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "APPR", "PPER", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.7": {"text": "\u201edie Mutter singet sch\u00f6ne.", "tokens": ["\u201e", "die", "Mut\u00b7ter", "sin\u00b7get", "sch\u00f6\u00b7ne", "."], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "\u201esie wiegt im Grab ein Kindelein,", "tokens": ["\u201e", "sie", "wiegt", "im", "Grab", "ein", "Kin\u00b7de\u00b7lein", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201edarum wir Blumen tragen.\u201c", "tokens": ["\u201e", "da\u00b7rum", "wir", "Blu\u00b7men", "tra\u00b7gen", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PAV", "PPER", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u201eihr lieben Kinder bleibt daheim,", "tokens": ["\u201e", "ihr", "lie\u00b7ben", "Kin\u00b7der", "bleibt", "da\u00b7heim", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "ADJA", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u201eeur Mutter schl\u00e4ft ohn Klagen.\u201c", "tokens": ["\u201e", "eur", "Mut\u00b7ter", "schl\u00e4ft", "ohn", "Kla\u00b7gen", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPOSAT", "NN", "VVFIN", "APPR", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Kinder lie\u00dfen keine Ruh,", "tokens": ["Die", "Kin\u00b7der", "lie\u00b7\u00dfen", "kei\u00b7ne", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Vater ging dem Grabe zu,", "tokens": ["Der", "Va\u00b7ter", "ging", "dem", "Gra\u00b7be", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Th\u00e4t auch die Stimme h\u00f6ren.", "tokens": ["Th\u00e4t", "auch", "die", "Stim\u00b7me", "h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ein \u00fcberlieblich reine Stimm,", "tokens": ["Ein", "\u00fc\u00b7berl\u00b7ieb\u00b7lich", "rei\u00b7ne", "Stimm", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er h\u00f6rt an diesem Orte,", "tokens": ["Er", "h\u00f6rt", "an", "die\u00b7sem", "Or\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mit Wunderkraft, mit frohen Grimm", "tokens": ["Mit", "Wun\u00b7der\u00b7kraft", ",", "mit", "fro\u00b7hen", "Grimm"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "APPR", "ADJA", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er reisset auf die Pforte,", "tokens": ["Er", "reis\u00b7set", "auf", "die", "Pfor\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Er hebet auf den schweren Stein,", "tokens": ["Er", "he\u00b7bet", "auf", "den", "schwe\u00b7ren", "Stein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den eichnen Sarg er schlaget ein,", "tokens": ["Den", "eich\u00b7nen", "Sarg", "er", "schla\u00b7get", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dann st\u00fcrzt er betend nieder.", "tokens": ["Dann", "st\u00fcrzt", "er", "be\u00b7tend", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Es lag die sch\u00f6ne Fraue da,", "tokens": ["Es", "lag", "die", "sch\u00f6\u00b7ne", "Frau\u00b7e", "da", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Kind an ihrer Seite,", "tokens": ["Das", "Kind", "an", "ih\u00b7rer", "Sei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die andern Kinder treten nah,", "tokens": ["Die", "an\u00b7dern", "Kin\u00b7der", "tre\u00b7ten", "nah", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie th\u00e4t die Arme breiten:", "tokens": ["Sie", "th\u00e4t", "die", "Ar\u00b7me", "brei\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "\u201eherzlieber Mann, dein Kind nimm an,\u201c", "tokens": ["\u201e", "herz\u00b7lie\u00b7ber", "Mann", ",", "dein", "Kind", "nimm", "an", ",", "\u201c"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "NN", "$,", "PPOSAT", "NN", "VVIMP", "PTKVZ", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Er sah es voller Freuden an,", "tokens": ["Er", "sah", "es", "vol\u00b7ler", "Freu\u00b7den", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "\u201eund la\u00df dich nicht entsetzen.\u201c", "tokens": ["\u201e", "und", "la\u00df", "dich", "nicht", "ent\u00b7set\u00b7zen", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KON", "VVIMP", "PPER", "PTKNEG", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Das Kindlein lacht den Vater an,", "tokens": ["Das", "Kin\u00b7dlein", "lacht", "den", "Va\u00b7ter", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie gingen all nach Hause,", "tokens": ["Sie", "gin\u00b7gen", "all", "nach", "Hau\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein Bad man th\u00e4t anrichten denn,", "tokens": ["Ein", "Bad", "man", "th\u00e4t", "an\u00b7rich\u00b7ten", "denn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "PIS", "ADJD", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Man ladet viel zum Schmause.", "tokens": ["Man", "la\u00b7det", "viel", "zum", "Schmau\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Gelehrte kamen auch heran,", "tokens": ["Ge\u00b7lehr\u00b7te", "ka\u00b7men", "auch", "he\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zu schauen das Mirakel an,", "tokens": ["Zu", "schau\u00b7en", "das", "Mi\u00b7ra\u00b7kel", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Zu h\u00f6ren ohne Grausen.", "tokens": ["Zu", "h\u00f6\u00b7ren", "oh\u00b7ne", "Grau\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Da nahm sie einen Becher Wein,", "tokens": ["Da", "nahm", "sie", "ei\u00b7nen", "Be\u00b7cher", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dann gr\u00fc\u00dfte sie die Freunde,", "tokens": ["Dann", "gr\u00fc\u00