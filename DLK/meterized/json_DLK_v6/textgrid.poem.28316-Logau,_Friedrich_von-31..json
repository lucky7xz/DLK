{"textgrid.poem.28316": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "31.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da gleich das Jahr ietzund ist kummen in die Wochen", "tokens": ["Da", "gleich", "das", "Jahr", "ie\u00b7tzund", "ist", "kum\u00b7men", "in", "die", "Wo\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN", "ADV", "VAFIN", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und tr\u00e4gt uns g\u00fctig auff Confeckt und gute Kuchen,", "tokens": ["Und", "tr\u00e4gt", "uns", "g\u00fc\u00b7tig", "auff", "Con\u00b7feckt", "und", "gu\u00b7te", "Ku\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "APPR", "NE", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So viel der Unfall lie\u00df, da habt ihr, liebes Paar,", "tokens": ["So", "viel", "der", "Un\u00b7fall", "lie\u00df", ",", "da", "habt", "ihr", ",", "lie\u00b7bes", "Paar", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVFIN", "$,", "ADV", "VAFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Gleich euren Hochzeit-Tag. Gott la\u00df mich sagen wahr!", "tokens": ["Gleich", "eu\u00b7ren", "Hoch\u00b7zeit\u00b7Tag", ".", "Gott", "la\u00df", "mich", "sa\u00b7gen", "wahr", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$.", "NN", "VVIMP", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Das Heil m\u00fc\u00df alle Tag euch in den Wochen liegen", "tokens": ["Das", "Heil", "m\u00fc\u00df", "al\u00b7le", "Tag", "euch", "in", "den", "Wo\u00b7chen", "lie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "PIAT", "NN", "PPER", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und f\u00fcllen euer Haus mit Segen und Vergn\u00fcgen.", "tokens": ["Und", "f\u00fcl\u00b7len", "eu\u00b7er", "Haus", "mit", "Se\u00b7gen", "und", "Ver\u00b7gn\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und ihr thut wie das Jahr und mehret alle Jahr,", "tokens": ["Und", "ihr", "thut", "wie", "das", "Jahr", "und", "meh\u00b7ret", "al\u00b7le", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "KOKOM", "ART", "NN", "KON", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wo nicht mit einem Paar, mit einem euer Paar.", "tokens": ["Wo", "nicht", "mit", "ei\u00b7nem", "Paar", ",", "mit", "ei\u00b7nem", "eu\u00b7er", "Paar", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "APPR", "ART", "NN", "$,", "APPR", "ART", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}