{"textgrid.poem.47216": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Aus der Brieftasche eines Verzweifelten", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df ich nur verzweifeln k\u00f6nnte,", "tokens": ["Da\u00df", "ich", "nur", "ver\u00b7zwei\u00b7feln", "k\u00f6nn\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sprechen zu dem Elemente:", "tokens": ["Spre\u00b7chen", "zu", "dem", "E\u00b7le\u00b7men\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.3": {"text": "\u00bbnimm den matten Geist zur\u00fcck!\u00ab", "tokens": ["\u00bb", "nimm", "den", "mat\u00b7ten", "Geist", "zu\u00b7r\u00fcck", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVIMP", "ART", "ADJA", "NN", "PTKVZ", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df nicht in die n\u00e4cht'ge W\u00fcste", "tokens": ["Da\u00df", "nicht", "in", "die", "n\u00e4cht'\u00b7ge", "W\u00fcs\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Noch ein Strahl mir brechen m\u00fc\u00dfte,", "tokens": ["Noch", "ein", "Strahl", "mir", "bre\u00b7chen", "m\u00fc\u00df\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PPER", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Noch ein falscher Hoffnungsblick.", "tokens": ["Noch", "ein", "fal\u00b7scher", "Hoff\u00b7nungs\u00b7blick", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Hast du nicht genug gelitten?", "tokens": ["Hast", "du", "nicht", "ge\u00b7nug", "ge\u00b7lit\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hast du nicht dich m\u00fcd gestritten,", "tokens": ["Hast", "du", "nicht", "dich", "m\u00fcd", "ge\u00b7strit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "PPER", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Armes Herz, was willst du mehr?", "tokens": ["Ar\u00b7mes", "Herz", ",", "was", "willst", "du", "mehr", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PWS", "VMFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ferne Lebensquellen rauschen,", "tokens": ["Fer\u00b7ne", "Le\u00b7bens\u00b7quel\u00b7len", "rau\u00b7schen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Willst du noch beth\u00f6ret lauschen?", "tokens": ["Willst", "du", "noch", "be\u00b7th\u00f6\u00b7ret", "lau\u00b7schen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "VVFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sieh, dein eigner Quell ist leer.", "tokens": ["Sieh", ",", "dein", "eig\u00b7ner", "Quell", "ist", "leer", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "ADJA", "NN", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Freudig war ein Baum entsprossen,", "tokens": ["Freu\u00b7dig", "war", "ein", "Baum", "ent\u00b7spros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die gr\u00fcnen Zweige flossen", "tokens": ["Und", "die", "gr\u00fc\u00b7nen", "Zwei\u00b7ge", "flos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Frisch hinaus in Himmelslicht.", "tokens": ["Frisch", "hin\u00b7aus", "in", "Him\u00b7mels\u00b7licht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APZR", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wankst du Wipfel? Ach, sie haben", "tokens": ["Wankst", "du", "Wip\u00b7fel", "?", "Ach", ",", "sie", "ha\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "NN", "$.", "ITJ", "$,", "PPER", "VAFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Deine Wurzel abgegraben,", "tokens": ["Dei\u00b7ne", "Wur\u00b7zel", "ab\u00b7ge\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und der Wipfel wei\u00df es nicht.", "tokens": ["Und", "der", "Wip\u00b7fel", "wei\u00df", "es", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Schmeichle mir nicht, Strahl der Sonnen,", "tokens": ["Schmeich\u00b7le", "mir", "nicht", ",", "Strahl", "der", "Son\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Spare deine vollen Bronnen,", "tokens": ["Spa\u00b7re", "dei\u00b7ne", "vol\u00b7len", "Bron\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gro\u00dfe Mutter, G\u00e4rtnerin!", "tokens": ["Gro\u00b7\u00dfe", "Mut\u00b7ter", ",", "G\u00e4rt\u00b7ne\u00b7rin", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach, du wei\u00dft, ich kann nicht stehen,", "tokens": ["Ach", ",", "du", "wei\u00dft", ",", "ich", "kann", "nicht", "ste\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "$,", "PPER", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "O so la\u00df mich gleich vergehen,", "tokens": ["O", "so", "la\u00df", "mich", "gleich", "ver\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVIMP", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nimm mit einemmal mich hin.", "tokens": ["Nimm", "mit", "ei\u00b7nem\u00b7mal", "mich", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ADV", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Kannst du Blitz aus deinen Wettern", "tokens": ["Kannst", "du", "Blitz", "aus", "dei\u00b7nen", "Wet\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Felsenfestes nur zerschmettern?", "tokens": ["Fel\u00b7sen\u00b7fes\u00b7tes", "nur", "zer\u00b7schmet\u00b7tern", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Triff einmal ein morsches Herz!", "tokens": ["Triff", "ein\u00b7mal", "ein", "mor\u00b7sches", "Herz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "Hilf ihm, dem die Kr\u00e4ft' ermatten", "tokens": ["Hilf", "ihm", ",", "dem", "die", "Kr\u00e4ft'", "er\u00b7mat\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "PPER", "$,", "PRELS", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und ihm selber nicht gestatten,", "tokens": ["Und", "ihm", "sel\u00b7ber", "nicht", "ge\u00b7stat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Einzusargen seinen Schmerz.", "tokens": ["Ein\u00b7zu\u00b7sar\u00b7gen", "sei\u00b7nen", "Schmerz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Grabet mir mein k\u00fchles Bette,", "tokens": ["Gra\u00b7bet", "mir", "mein", "k\u00fch\u00b7les", "Bet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und auf meiner \u00f6den St\u00e4tte", "tokens": ["Und", "auf", "mei\u00b7ner", "\u00f6\u00b7den", "St\u00e4t\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sprech' ein rauhbemooster Stein:", "tokens": ["Sprech'", "ein", "rauh\u00b7be\u00b7moos\u00b7ter", "Stein", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00bbdiesen, da er nicht mehr wu\u00dfte,", "tokens": ["\u00bb", "die\u00b7sen", ",", "da", "er", "nicht", "mehr", "wu\u00df\u00b7te", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDAT", "$,", "KOUS", "PPER", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Was er drau\u00dfen sollt' und mu\u00dfte,", "tokens": ["Was", "er", "drau\u00b7\u00dfen", "sollt'", "und", "mu\u00df\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VMFIN", "KON", "VMFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nahm ein Gott zu sich herein.\u00ab", "tokens": ["Nahm", "ein", "Gott", "zu", "sich", "her\u00b7ein", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ART", "NN", "APPR", "PRF", "PTKVZ", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}