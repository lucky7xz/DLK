{"dta.poem.20433": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Wettstreit der tulpen/ nelcke/ rose/  \n lilie/ cypre\u00df/ narci\u00df und  \n hyacinth.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Beut iemand meinen ruhm itzt einen wettstreit an/", "tokens": ["Beut", "ie\u00b7mand", "mei\u00b7nen", "ruhm", "itzt", "ei\u00b7nen", "wett\u00b7streit", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPOSAT", "NN", "ADV", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Darff eine blume sich wohl wider mich erheben?", "tokens": ["Darff", "ei\u00b7ne", "blu\u00b7me", "sich", "wohl", "wi\u00b7der", "mich", "er\u00b7he\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "VVFIN", "PRF", "ADV", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nein bunte tulipan; ich mu\u00df dir widerstreben.", "tokens": ["Nein", "bun\u00b7te", "tu\u00b7li\u00b7pan", ";", "ich", "mu\u00df", "dir", "wi\u00b7der\u00b7stre\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADJA", "NN", "$.", "PPER", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich/ als die k\u00f6nigin in ieden garten plan.", "tokens": ["Ich", "/", "als", "die", "k\u00f6\u00b7ni\u00b7gin", "in", "ie\u00b7den", "gar\u00b7ten", "plan", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "KOUS", "ART", "NN", "APPR", "PIS", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mein himmlischer geruch/ was hat er nicht gethan?", "tokens": ["Mein", "himm\u00b7li\u00b7scher", "ge\u00b7ruch", "/", "was", "hat", "er", "nicht", "ge\u00b7than", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$(", "PWS", "VAFIN", "PPER", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Gibt er nicht sterbenden ein frisch beseeltes leben?", "tokens": ["Gibt", "er", "nicht", "ster\u00b7ben\u00b7den", "ein", "frisch", "be\u00b7seel\u00b7tes", "le\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "VVFIN", "ART", "ADJD", "ADJA", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.7": {"text": "Ich mu\u00df dem jungfer-volck die sch\u00f6nsten kr\u00e4ntze geben:", "tokens": ["Ich", "mu\u00df", "dem", "jung\u00b7fer\u00b7volck", "die", "sch\u00f6ns\u00b7ten", "kr\u00e4nt\u00b7ze", "ge\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die G\u00f6tter streuen selbst mit rosen ihre bahn.", "tokens": ["Die", "G\u00f6t\u00b7ter", "streu\u00b7en", "selbst", "mit", "ro\u00b7sen", "ih\u00b7re", "bahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Mein thron ist gr\u00fcner sammt/ mein scepter ist ein ast/", "tokens": ["Mein", "thron", "ist", "gr\u00fc\u00b7ner", "sammt", "/", "mein", "scep\u00b7ter", "ist", "ein", "ast", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "VVFIN", "$(", "PPOSAT", "ADJA", "VAFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Den hat der scharffe dorn in treue wacht gefast.", "tokens": ["Den", "hat", "der", "scharf\u00b7fe", "dorn", "in", "treu\u00b7e", "wacht", "ge\u00b7fast", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "VVFIN", "ADV", "APPR", "ADJA", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die purpur-m\u00e4ntel sind den rothen bl\u00e4ttern rein/", "tokens": ["Die", "pur\u00b7pur\u00b7m\u00e4n\u00b7tel", "sind", "den", "ro\u00b7then", "bl\u00e4t\u00b7tern", "rein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und die smaragdne schoo\u00df haucht s\u00fcsse liebes-winde/", "tokens": ["Und", "die", "sma\u00b7ragd\u00b7ne", "schoo\u00df", "haucht", "s\u00fcs\u00b7se", "lie\u00b7bes\u00b7win\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "VVFIN", "ADJD", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "So steckt der krone gold in gelben zeserlein/", "tokens": ["So", "steckt", "der", "kro\u00b7ne", "gold", "in", "gel\u00b7ben", "ze\u00b7ser\u00b7lein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Wer saget/ da\u00df er wo dergleichen hochzeit finde?", "tokens": ["Wer", "sa\u00b7get", "/", "da\u00df", "er", "wo", "derg\u00b7lei\u00b7chen", "hoch\u00b7zeit", "fin\u00b7de", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$(", "KOUS", "PPER", "PWAV", "PIS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}