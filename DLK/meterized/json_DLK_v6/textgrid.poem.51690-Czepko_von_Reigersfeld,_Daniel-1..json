{"textgrid.poem.51690": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was im Gesch\u00f6pff ein Werck, das ist in Gott ein Wesen,", "tokens": ["Was", "im", "Ge\u00b7sch\u00f6pff", "ein", "Werck", ",", "das", "ist", "in", "Gott", "ein", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPRART", "NN", "ART", "NN", "$,", "PDS", "VAFIN", "APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch, wo er wirckt, da hat er das Gesch\u00f6pff erlesen.", "tokens": ["Doch", ",", "wo", "er", "wirckt", ",", "da", "hat", "er", "das", "Ge\u00b7sch\u00f6pff", "er\u00b7le\u00b7sen", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "PPER", "VVFIN", "$,", "ADV", "VAFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}