{"dta.poem.18901": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "Auf das Ordensband  \n mit dem Bilde der Sonne und des K\u00f6niges .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Es segne Gott dies Band des edlen Glanzes;", "tokens": ["Es", "seg\u00b7ne", "Gott", "dies", "Band", "des", "ed\u00b7len", "Glan\u00b7zes", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "PDS", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Sonne zieht den Schleyer vor ihm weg.", "tokens": ["Die", "Son\u00b7ne", "zieht", "den", "Schle\u00b7yer", "vor", "ihm", "weg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sein Schmuck kam von des zweyten Mani Pinsel,", "tokens": ["Sein", "Schmuck", "kam", "von", "des", "zwey\u00b7ten", "Ma\u00b7ni", "Pin\u00b7sel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das Bild Fetch Ali Schahs mit Sonnenkrone.", "tokens": ["Das", "Bild", "Fetch", "A\u00b7li", "Schahs", "mit", "Son\u00b7nen\u00b7kro\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "NE", "NE", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ein Bote gross des Herrn mit Himmelshof", "tokens": ["Ein", "Bo\u00b7te", "gross", "des", "Herrn", "mit", "Him\u00b7mels\u00b7hof"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ist Abul Hassan Chan, gelehrt und weise,", "tokens": ["Ist", "A\u00b7bul", "Has\u00b7san", "Chan", ",", "ge\u00b7lehrt", "und", "wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "NE", "NE", "$,", "VVPP", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Von Haupt zu Fuss gesenkt in Herrschersperlen;", "tokens": ["Von", "Haupt", "zu", "Fuss", "ge\u00b7senkt", "in", "Herr\u00b7scher\u00b7sper\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "VVPP", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Den Dienstweg schritt vom Haupt zum Ende er,", "tokens": ["Den", "Dienst\u00b7weg", "schritt", "vom", "Haupt", "zum", "En\u00b7de", "er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "APPRART", "NN", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Da man sein Haupt zur Sonne wollt\u2019 erheben,", "tokens": ["Da", "man", "sein", "Haupt", "zur", "Son\u00b7ne", "wollt'", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPOSAT", "NN", "APPRART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Gab man ihm mit die Himmelssonn\u2019 als Diener.", "tokens": ["Gab", "man", "ihm", "mit", "die", "Him\u00b7mels\u00b7sonn'", "als", "Die\u00b7ner", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPER", "APPR", "ART", "NN", "KOUS", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "So frohe Botschaft ist von grossem Sinn", "tokens": ["So", "fro\u00b7he", "Bot\u00b7schaft", "ist", "von", "gros\u00b7sem", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "F\u00fcr den Gesandten edel und belobt;", "tokens": ["F\u00fcr", "den", "Ge\u00b7sand\u00b7ten", "e\u00b7del", "und", "be\u00b7lobt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Sein Bund ist Bund des Weltgebieters Dara,", "tokens": ["Sein", "Bund", "ist", "Bund", "des", "Welt\u00b7ge\u00b7bie\u00b7ters", "Da\u00b7ra", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Sein Wort ist Wort des Herrn mit Himmelsglanz.", "tokens": ["Sein", "Wort", "ist", "Wort", "des", "Herrn", "mit", "Him\u00b7mels\u00b7glanz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}