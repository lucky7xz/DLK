{"dta.poem.21121": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "An Selinen .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20676-2", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zum zweyten mal, o meine theure Seline,", "tokens": ["Zum", "zwey\u00b7ten", "mal", ",", "o", "mei\u00b7ne", "theu\u00b7re", "Se\u00b7li\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADV", "$,", "FM", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.2": {"text": "Rei\u00dft dich die schwarze Welle hin?", "tokens": ["Rei\u00dft", "dich", "die", "schwar\u00b7ze", "Wel\u00b7le", "hin", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zum zweyten mal schwimmst du auf tobendem Meere", "tokens": ["Zum", "zwey\u00b7ten", "mal", "schwimmst", "du", "auf", "to\u00b7ben\u00b7dem", "Mee\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Den grimmigsten Gefahren zu?", "tokens": ["Den", "grim\u00b7migs\u00b7ten", "Ge\u00b7fah\u00b7ren", "zu", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-++--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Mit banger Nacht schw\u00e4rzt sich der st\u00fcrmende Himmel,", "tokens": ["Mit", "ban\u00b7ger", "Nacht", "schw\u00e4rzt", "sich", "der", "st\u00fcr\u00b7men\u00b7de", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der Donner donnert vom Olymp;", "tokens": ["Der", "Don\u00b7ner", "don\u00b7nert", "vom", "O\u00b7lymp", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der wilde Blitz erleuchtet schrecklich den Abgrund,", "tokens": ["Der", "wil\u00b7de", "Blitz", "er\u00b7leuch\u00b7tet", "schreck\u00b7lich", "den", "Ab\u00b7grund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Der oft dein zagend Schif verschluckt.", "tokens": ["Der", "oft", "dein", "za\u00b7gend", "Schif", "ver\u00b7schluckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPOSAT", "ADJD", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Verfolge nicht ein ungl\u00fcckseliges M\u00e4dchen,", "tokens": ["Ver\u00b7fol\u00b7ge", "nicht", "ein", "un\u00b7gl\u00fcck\u00b7se\u00b7li\u00b7ges", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "-+-+----+-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Natur, mit so viel Grausamkeit!", "tokens": ["Na\u00b7tur", ",", "mit", "so", "viel", "Grau\u00b7sam\u00b7keit", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gebeut doch ietzt dem niederrollenden Donner,", "tokens": ["Ge\u00b7beut", "doch", "ietzt", "dem", "nie\u00b7der\u00b7rol\u00b7len\u00b7den", "Don\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Gebeut doch ietzt der frechen Fluth.", "tokens": ["Ge\u00b7beut", "doch", "ietzt", "der", "fre\u00b7chen", "Fluth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Du h\u00f6rst mich nicht? nicht das bewegliche Weinen", "tokens": ["Du", "h\u00f6rst", "mich", "nicht", "?", "nicht", "das", "be\u00b7weg\u00b7li\u00b7che", "Wei\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$.", "PTKNEG", "ART", "ADJA", "NN"], "meter": "-+-+++-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Des \u00e4rmsren Kindes, das verzagt?", "tokens": ["Des", "\u00e4rms\u00b7ren", "Kin\u00b7des", ",", "das", "ver\u00b7zagt", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auss neu w\u00e4lzt sich auf dunkeln w\u00fctenden Wellen", "tokens": ["Auss", "neu", "w\u00e4lzt", "sich", "auf", "dun\u00b7keln", "w\u00fc\u00b7ten\u00b7den", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "VVFIN", "PRF", "APPR", "ADJA", "ADJA", "NN"], "meter": "-+--++-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Die blasse Todesangst ins Schif.", "tokens": ["Die", "blas\u00b7se", "To\u00b7de\u00b7sangst", "ins", "Schif", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wohin, wohin rei\u00dft dich die brausende Woge", "tokens": ["Wo\u00b7hin", ",", "wo\u00b7hin", "rei\u00dft", "dich", "die", "brau\u00b7sen\u00b7de", "Wo\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "$,", "PWAV", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Seline, h\u00f6rst du mich nicht mehr?", "tokens": ["Se\u00b7li\u00b7ne", ",", "h\u00f6rst", "du", "mich", "nicht", "mehr", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "PRF", "PTKNEG", "ADV", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Ich ruf am Strand, mit aufgehabnen H\u00e4nden:", "tokens": ["Ich", "ruf", "am", "Strand", ",", "mit", "auf\u00b7ge\u00b7hab\u00b7nen", "H\u00e4n\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPRART", "NN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Seline, h\u00f6rst du mich nicht mehr?", "tokens": ["Se\u00b7li\u00b7ne", ",", "h\u00f6rst", "du", "mich", "nicht", "mehr", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "PRF", "PTKNEG", "ADV", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.6": {"line.1": {"text": "Was hoff ich noch am ungl\u00fccksvollen Gestade \u2014", "tokens": ["Was", "hoff", "ich", "noch", "am", "un\u00b7gl\u00fccks\u00b7vol\u00b7len", "Ge\u00b7sta\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "APPRART", "ADJA", "NN", "$("], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Empfange mich, grausames Meer!", "tokens": ["Emp\u00b7fan\u00b7ge", "mich", ",", "grau\u00b7sa\u00b7mes", "Meer", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Kan ich sie nicht auf diesem Tr\u00fcmmer erretten,", "tokens": ["Kan", "ich", "sie", "nicht", "auf", "die\u00b7sem", "Tr\u00fcm\u00b7mer", "er\u00b7ret\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "PTKNEG", "APPR", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "So sterb ich wenigstens mit ihr!", "tokens": ["So", "sterb", "ich", "we\u00b7nigs\u00b7tens", "mit", "ihr", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}