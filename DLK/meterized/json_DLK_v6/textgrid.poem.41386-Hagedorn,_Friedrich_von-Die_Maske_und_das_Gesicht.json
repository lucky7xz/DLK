{"textgrid.poem.41386": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Die Maske und das Gesicht", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bei Hof, an einem Carneval,", "tokens": ["Bei", "Hof", ",", "an", "ei\u00b7nem", "Car\u00b7ne\u00b7val", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sprach einst die Maske zum Gesichte:", "tokens": ["Sprach", "einst", "die", "Mas\u00b7ke", "zum", "Ge\u00b7sich\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Gib Acht, wie ich hier \u00fcberall", "tokens": ["Gib", "Acht", ",", "wie", "ich", "hier", "\u00fc\u00b7be\u00b7rall"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "CARD", "$,", "PWAV", "PPER", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Jetzt deinen Ruhm und Stolz zernichte,", "tokens": ["Jetzt", "dei\u00b7nen", "Ruhm", "und", "Stolz", "zer\u00b7nich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und mancher, den du sonst entfernt,", "tokens": ["Und", "man\u00b7cher", ",", "den", "du", "sonst", "ent\u00b7fernt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PRELS", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mir folgen und mir schmeicheln lernt.", "tokens": ["Mir", "fol\u00b7gen", "und", "mir", "schmei\u00b7cheln", "lernt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "KON", "PPER", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Venedig ist mein Vaterland;", "tokens": ["Ve\u00b7ne\u00b7dig", "ist", "mein", "Va\u00b7ter\u00b7land", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Drum sch\u00fctz' ich Freiheit, List und Liebe.", "tokens": ["Drum", "sch\u00fctz'", "ich", "Frei\u00b7heit", ",", "List", "und", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wer scheinet oder ist galant,", "tokens": ["Wer", "schei\u00b7net", "o\u00b7der", "ist", "ga\u00b7lant", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "KON", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durch den ich keinen Streich ver\u00fcbe?", "tokens": ["Durch", "den", "ich", "kei\u00b7nen", "Streich", "ver\u00b7\u00fc\u00b7be", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Man lobt, man ehrt mich tausendfach,", "tokens": ["Man", "lobt", ",", "man", "ehrt", "mich", "tau\u00b7send\u00b7fach", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PIS", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und sp\u00fcrt und tanzt und schleicht mir nach.", "tokens": ["Und", "sp\u00fcrt", "und", "tanzt", "und", "schleicht", "mir", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich lehr' in diesem Federhut", "tokens": ["Ich", "lehr'", "in", "die\u00b7sem", "Fe\u00b7der\u00b7hut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die kronenscheuen M\u00e4nner kr\u00f6nen.", "tokens": ["Die", "kro\u00b7nen\u00b7scheu\u00b7en", "M\u00e4n\u00b7ner", "kr\u00f6\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich schaffe stillen W\u00fcnschen Muth,", "tokens": ["Ich", "schaf\u00b7fe", "stil\u00b7len", "W\u00fcn\u00b7schen", "Muth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Muthe Gl\u00fcck, dem Gl\u00fccke Sch\u00f6nen.", "tokens": ["Dem", "Mu\u00b7the", "Gl\u00fcck", ",", "dem", "Gl\u00fc\u00b7cke", "Sch\u00f6\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Es k\u00f6nnen hier, durch mich allein,", "tokens": ["Es", "k\u00f6n\u00b7nen", "hier", ",", "durch", "mich", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "$,", "APPR", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Ungestalten grausam sein.", "tokens": ["Die", "Un\u00b7ge\u00b7stal\u00b7ten", "grau\u00b7sam", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ein wenig Prahlen steht dir frei:", "tokens": ["Ein", "we\u00b7nig", "Prah\u00b7len", "steht", "dir", "frei", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "War des Gesichtes Gegenrede.", "tokens": ["War", "des", "Ge\u00b7sich\u00b7tes", "Ge\u00b7gen\u00b7re\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Doch stimme meinem Vorzug bei,", "tokens": ["Doch", "stim\u00b7me", "mei\u00b7nem", "Vor\u00b7zug", "bei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und sch\u00e4me dich der k\u00fchnen Fehde,", "tokens": ["Und", "sch\u00e4\u00b7me", "dich", "der", "k\u00fch\u00b7nen", "Feh\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Weil dies nur deine Sch\u00f6nheit ist,", "tokens": ["Weil", "dies", "nur", "dei\u00b7ne", "Sch\u00f6n\u00b7heit", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "ADV", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df du mir oft so \u00e4hnlich bist.", "tokens": ["Da\u00df", "du", "mir", "oft", "so", "\u00e4hn\u00b7lich", "bist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Das Herz wird nur durch mich erkannt,", "tokens": ["Das", "Herz", "wird", "nur", "durch", "mich", "er\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durch mich, den Spiegel vom Gem\u00fcthe.", "tokens": ["Durch", "mich", ",", "den", "Spie\u00b7gel", "vom", "Ge\u00b7m\u00fc\u00b7the", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mein hoher Ernst beweist Verstand,", "tokens": ["Mein", "ho\u00b7her", "Ernst", "be\u00b7weist", "Ver\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein L\u00e4cheln zeugt von Treu' und G\u00fcte.", "tokens": ["Mein", "L\u00e4\u00b7cheln", "zeugt", "von", "Treu'", "und", "G\u00fc\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die Maske sprach: Mein stolz' Gesicht!", "tokens": ["Die", "Mas\u00b7ke", "sprach", ":", "Mein", "stolz'", "Ge\u00b7sicht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vielleicht wol sonst; bei Hose nicht.", "tokens": ["Viel\u00b7leicht", "wol", "sonst", ";", "bei", "Ho\u00b7se", "nicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "$.", "APPR", "NE", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}