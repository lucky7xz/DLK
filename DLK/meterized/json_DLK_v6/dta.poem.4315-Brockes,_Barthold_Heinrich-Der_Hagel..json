{"dta.poem.4315": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Hagel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie nun das weite Reich der Luft aus vielen Lagen", "tokens": ["Wie", "nun", "das", "wei\u00b7te", "Reich", "der", "Luft", "aus", "vie\u00b7len", "La\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "ADJA", "NN", "ART", "NN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Luft bestehet,", "tokens": ["Luft", "be\u00b7ste\u00b7het", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Die alle, nach der Art des Windes, der durch und an die-", "tokens": ["Die", "al\u00b7le", ",", "nach", "der", "Art", "des", "Win\u00b7des", ",", "der", "durch", "und", "an", "die"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "$,", "APPR", "ART", "NN", "ART", "NN", "$,", "PRELS", "APPR", "KON", "APPR", "TRUNC"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "selben wehet,", "tokens": ["sel\u00b7ben", "we\u00b7het", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Auch von verschiedner Eigenschaft, begiebt sich \u00f6fters,", "tokens": ["Auch", "von", "ver\u00b7schied\u00b7ner", "Ei\u00b7gen\u00b7schaft", ",", "be\u00b7giebt", "sich", "\u00f6f\u00b7ters", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,", "VVFIN", "PRF", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "da\u00df im Sinken", "tokens": ["da\u00df", "im", "Sin\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "APPRART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Die Tropfen solche kalte Stellen und solche kalte Luft be-", "tokens": ["Die", "Trop\u00b7fen", "sol\u00b7che", "kal\u00b7te", "Stel\u00b7len", "und", "sol\u00b7che", "kal\u00b7te", "Luft", "be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PIAT", "ADJA", "NN", "KON", "PIAT", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.8": {"text": "r\u00fchren,", "tokens": ["r\u00fch\u00b7ren", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "Da\u00df sie, indem sie durch sie fallen, sie in dem Augenblick", "tokens": ["Da\u00df", "sie", ",", "in\u00b7dem", "sie", "durch", "sie", "fal\u00b7len", ",", "sie", "in", "dem", "Au\u00b7gen\u00b7blick"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "KOUS", "PPER", "APPR", "PPER", "VVINF", "$,", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-++-+-+", "measure": "unknown.measure.octa.plus"}, "line.10": {"text": "gefrieren.", "tokens": ["ge\u00b7frie\u00b7ren", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.11": {"text": "Die\u00df ist sodann des Hagels Ursprung, woran wir augen-", "tokens": ["Die\u00df", "ist", "so\u00b7dann", "des", "Ha\u00b7gels", "Ur\u00b7sprung", ",", "wo\u00b7ran", "wir", "au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ADV", "ART", "NN", "NN", "$,", "PWAV", "PPER", "TRUNC"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "scheinlich sehn,", "tokens": ["schein\u00b7lich", "sehn", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.13": {"text": "An aller Schlossen Form und Gr\u00f6sse, da\u00df sie aus Tropfen", "tokens": ["An", "al\u00b7ler", "Schlos\u00b7sen", "Form", "und", "Gr\u00f6s\u00b7se", ",", "da\u00df", "sie", "aus", "Trop\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "NN", "KON", "NN", "$,", "KOUS", "PPER", "APPR", "NN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "blo\u00df bestehn.", "tokens": ["blo\u00df", "be\u00b7stehn", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVINF", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}