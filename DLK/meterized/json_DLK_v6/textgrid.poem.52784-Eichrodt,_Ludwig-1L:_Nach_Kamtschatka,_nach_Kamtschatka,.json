{"textgrid.poem.52784": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nach Kamtschatka, nach Kamtschatka,", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nach Kamtschatka, nach Kamtschatka,", "tokens": ["Nach", "Kamt\u00b7schat\u00b7ka", ",", "nach", "Kamt\u00b7schat\u00b7ka", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "APPR", "NE", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Dr\u00fcckt mich lang schon die Krawatka,", "tokens": ["Dr\u00fcckt", "mich", "lang", "schon", "die", "Kra\u00b7wat\u00b7ka", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ADV", "ART", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Wo der Wendegreis sich narrt;", "tokens": ["Wo", "der", "Wen\u00b7de\u00b7greis", "sich", "narrt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo der Russe endlich endigt,", "tokens": ["Wo", "der", "Rus\u00b7se", "end\u00b7lich", "en\u00b7digt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wo der Wandrer verelenndigt,", "tokens": ["Wo", "der", "Wand\u00b7rer", "ver\u00b7e\u00b7lenn\u00b7digt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und der Mutterwitz erstarrt \u2013", "tokens": ["Und", "der", "Mut\u00b7ter\u00b7witz", "er\u00b7starrt", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVPP", "$("], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.7": {"text": "Dahin \u2013", "tokens": ["Da\u00b7hin", "\u2013"], "token_info": ["word", "punct"], "pos": ["PAV", "$("], "meter": "-+", "measure": "iambic.single"}}, "stanza.2": {"line.1": {"text": "Nach Gorillien, nach Gorillien", "tokens": ["Nach", "Go\u00b7ril\u00b7li\u00b7en", ",", "nach", "Go\u00b7ril\u00b7li\u00b7en"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "NE", "$,", "APPR", "NE"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Thu' das Budget mir verwilligen,", "tokens": ["Thu'", "das", "Bud\u00b7get", "mir", "ver\u00b7wil\u00b7li\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wo der Waldmensch lebt und leibt;", "tokens": ["Wo", "der", "Wald\u00b7mensch", "lebt", "und", "leibt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo die Muse, die verirrte,", "tokens": ["Wo", "die", "Mu\u00b7se", ",", "die", "ver\u00b7irr\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mit Gewalt verliert die Myrthe,", "tokens": ["Mit", "Ge\u00b7walt", "ver\u00b7liert", "die", "Myr\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und der Wechselbalg sich str\u00e4ubt \u2013", "tokens": ["Und", "der", "Wech\u00b7sel\u00b7balg", "sich", "str\u00e4ubt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PRF", "VVFIN", "$("], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.7": {"text": "Dahin \u2013", "tokens": ["Da\u00b7hin", "\u2013"], "token_info": ["word", "punct"], "pos": ["PAV", "$("], "meter": "-+", "measure": "iambic.single"}}, "stanza.3": {"line.1": {"text": "Auf dem Dawala-lackhieri", "tokens": ["Auf", "dem", "Da\u00b7wa\u00b7la\u00b7lack\u00b7hieri"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "War ich auch schon schneeblind schieri,", "tokens": ["War", "ich", "auch", "schon", "schnee\u00b7blind", "schie\u00b7ri", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo die Pore r\u00f6thlich rinnt;", "tokens": ["Wo", "die", "Po\u00b7re", "r\u00f6th\u00b7lich", "rinnt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo der Mensch gen Himmel zipfelt,", "tokens": ["Wo", "der", "Mensch", "gen", "Him\u00b7mel", "zip\u00b7felt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "--+-+---", "measure": "anapaest.init"}, "line.5": {"text": "Wo der Unsinn grausam gipfelt,", "tokens": ["Wo", "der", "Un\u00b7sinn", "grau\u00b7sam", "gip\u00b7felt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und das Bohnenlied beginnt \u2013", "tokens": ["Und", "das", "Boh\u00b7nen\u00b7lied", "be\u00b7ginnt", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Dahin komm' ich schlie\u00dflich hin!", "tokens": ["Da\u00b7hin", "komm'", "ich", "schlie\u00df\u00b7lich", "hin", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}}}}