{"textgrid.poem.65307": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Das flotte Herz", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wann's im Schilfe s\u00e4uselt,", "tokens": ["Wann's", "im", "Schil\u00b7fe", "s\u00e4u\u00b7selt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Wann die Fluth sich kr\u00e4uselt,", "tokens": ["Wann", "die", "Fluth", "sich", "kr\u00e4u\u00b7selt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wird mir flott das Herz,", "tokens": ["Wird", "mir", "flott", "das", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "M\u00f6cht' aus der Brust mir fliegen,", "tokens": ["M\u00f6cht'", "aus", "der", "Brust", "mir", "flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "M\u00f6cht' auf den Wogen sich wiegen,", "tokens": ["M\u00f6cht'", "auf", "den", "Wo\u00b7gen", "sich", "wie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "Unter sich tauchen in Lust und in Schmerz.", "tokens": ["Un\u00b7ter", "sich", "tau\u00b7chen", "in", "Lust", "und", "in", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "VVFIN", "APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.2": {"line.1": {"text": "Fischerin, du kleine,", "tokens": ["Fi\u00b7sche\u00b7rin", ",", "du", "klei\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "ADJA", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Schiffe nicht alleine", "tokens": ["Schif\u00b7fe", "nicht", "al\u00b7lei\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["NN", "PTKNEG", "ADV"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "In das gro\u00dfe Meer!", "tokens": ["In", "das", "gro\u00b7\u00dfe", "Meer", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Hinter dir hergezogen", "tokens": ["Hin\u00b7ter", "dir", "her\u00b7ge\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPER", "VVINF"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Kommt schon mein Herz durch die Wogen \u2013", "tokens": ["Kommt", "schon", "mein", "Herz", "durch", "die", "Wo\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "APPR", "ART", "NN", "$("], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "Fischerin, sind deine Netze noch leer?", "tokens": ["Fi\u00b7sche\u00b7rin", ",", "sind", "dei\u00b7ne", "Net\u00b7ze", "noch", "leer", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "PPOSAT", "NN", "ADV", "ADJD", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.3": {"line.1": {"text": "Nimm's in deinen Nachen,", "tokens": ["Nim\u00b7m's", "in", "dei\u00b7nen", "Na\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "'s wird nicht schwer ihn machen,", "tokens": ["'s", "wird", "nicht", "schwer", "ihn", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "'s ist ja nichts darin,", "tokens": ["'s", "ist", "ja", "nichts", "da\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PIS", "PAV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Als nur du selber alleine,", "tokens": ["Als", "nur", "du", "sel\u00b7ber", "al\u00b7lei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Leichte, luftige Kleine,", "tokens": ["Leich\u00b7te", ",", "luf\u00b7ti\u00b7ge", "Klei\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "ADJA", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Du mit dem windigen, flatternden Sinn!", "tokens": ["Du", "mit", "dem", "win\u00b7di\u00b7gen", ",", "flat\u00b7tern\u00b7den", "Sinn", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "---+--+--+", "measure": "iambic.tri.relaxed"}}}}}