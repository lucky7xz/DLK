{"dta.poem.10877": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Vergn\u00fcgung  \n bey regnichtem Wetter im Sommer.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Uns Menschen kann so gar der Regen,", "tokens": ["Uns", "Men\u00b7schen", "kann", "so", "gar", "der", "Re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VMFIN", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn wir nur auf denselben achten,", "tokens": ["Wenn", "wir", "nur", "auf", "den\u00b7sel\u00b7ben", "ach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PDAT", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und die genetzte Welt betrachten,", "tokens": ["Und", "die", "ge\u00b7netz\u00b7te", "Welt", "be\u00b7trach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zur Lust und auch zum Dank bewegen.", "tokens": ["Zur", "Lust", "und", "auch", "zum", "Dank", "be\u00b7we\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Es wird die hei\u00dfe Luft, wenns regnet, lau und k\u00fchl,", "tokens": ["Es", "wird", "die", "hei\u00b7\u00dfe", "Luft", ",", "wenns", "reg\u00b7net", ",", "lau", "und", "k\u00fchl", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,", "KOUS", "VVFIN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und wirkt, auf unsre Haut, ein schaudrigtes Gef\u00fchl.", "tokens": ["Und", "wirkt", ",", "auf", "uns\u00b7re", "Haut", ",", "ein", "schaud\u00b7rig\u00b7tes", "Ge\u00b7f\u00fchl", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die D\u00fcft, indem sie um uns fliegen,", "tokens": ["Die", "D\u00fcft", ",", "in\u00b7dem", "sie", "um", "uns", "flie\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "APPR", "PPER", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Erregen unsrer Brust", "tokens": ["Er\u00b7re\u00b7gen", "uns\u00b7rer", "Brust"], "token_info": ["word", "word", "word"], "pos": ["NN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Zwar eine, doch nicht oft gef\u00fchlte, Lust,", "tokens": ["Zwar", "ei\u00b7ne", ",", "doch", "nicht", "oft", "ge\u00b7f\u00fchl\u00b7te", ",", "Lust", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ART", "$,", "ADV", "PTKNEG", "ADV", "VVFIN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Und leider! meistentheils ein unvermerkt, Vergn\u00fcgen.", "tokens": ["Und", "lei\u00b7der", "!", "meis\u00b7ten\u00b7theils", "ein", "un\u00b7ver\u00b7merkt", ",", "Ver\u00b7gn\u00fc\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "$.", "ADV", "ART", "ADJD", "$,", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Das eine zeitlang her, bestaubte, welke Gras", "tokens": ["Das", "ei\u00b7ne", "zeit\u00b7lang", "her", ",", "be\u00b7staub\u00b7te", ",", "wel\u00b7ke", "Gras"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PDS", "PIS", "VVFIN", "PTKVZ", "$,", "VVFIN", "$,", "PWAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wird, durch das reine Na\u00df,", "tokens": ["Wird", ",", "durch", "das", "rei\u00b7ne", "Na\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Gereinigt und getr\u00e4nkt. Ein lieblich Dunkelgr\u00fcn", "tokens": ["Ge\u00b7rei\u00b7nigt", "und", "ge\u00b7tr\u00e4nkt", ".", "Ein", "lieb\u00b7lich", "Dun\u00b7kel\u00b7gr\u00fcn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVPP", "KON", "VVPP", "$.", "ART", "ADJD", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Scheint Laub und Kraut so dann zu \u00fcberziehn;", "tokens": ["Scheint", "Laub", "und", "Kraut", "so", "dann", "zu", "\u00fc\u00b7ber\u00b7ziehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "ADV", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Ein schw\u00e4rzlich Dunkelbraun f\u00e4rbt Acker, Feld und Land;", "tokens": ["Ein", "schw\u00e4rz\u00b7lich", "Dun\u00b7kel\u00b7braun", "f\u00e4rbt", "A\u00b7cker", ",", "Feld", "und", "Land", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJD", "VVFIN", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Die Kl\u00f6sse kleben jetzt; es steht der rege Sand;", "tokens": ["Die", "Kl\u00f6s\u00b7se", "kle\u00b7ben", "jetzt", ";", "es", "steht", "der", "re\u00b7ge", "Sand", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$.", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Es leget sich der fl\u00fcchtge Staub;", "tokens": ["Es", "le\u00b7get", "sich", "der", "fl\u00fccht\u00b7ge", "Staub", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Die Blumen, die erfrischt, sind jetzt noch eins so sch\u00f6n,", "tokens": ["Die", "Blu\u00b7men", ",", "die", "er\u00b7frischt", ",", "sind", "jetzt", "noch", "eins", "so", "sch\u00f6n", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "$,", "VAFIN", "ADV", "ADV", "PIS", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Durch ihren dunklen Grund, erh\u00f6het, anzusehn;", "tokens": ["Durch", "ih\u00b7ren", "dunk\u00b7len", "Grund", ",", "er\u00b7h\u00f6\u00b7het", ",", "an\u00b7zu\u00b7sehn", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,", "VVFIN", "$,", "VVIZU", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Es rollen hier und dort, auf ihr gesteiftes Laub,", "tokens": ["Es", "rol\u00b7len", "hier", "und", "dort", ",", "auf", "ihr", "ge\u00b7steif\u00b7tes", "Laub", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "KON", "ADV", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Die runden Tr\u00f6pfgen, wie Krystallen,", "tokens": ["Die", "run\u00b7den", "Tr\u00f6pf\u00b7gen", ",", "wie", "Krys\u00b7tal\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PWAV", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Darin spielt eine weisse Gluht,", "tokens": ["Da\u00b7rin", "spielt", "ei\u00b7ne", "weis\u00b7se", "Gluht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Ein Schimmer, der, wie Silber, rein,", "tokens": ["Ein", "Schim\u00b7mer", ",", "der", ",", "wie", "Sil\u00b7ber", ",", "rein", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "$,", "PWAV", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Ein Diamanten-gleicher Schein.", "tokens": ["Ein", "Di\u00b7a\u00b7man\u00b7ten\u00b7glei\u00b7cher", "Schein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Auf der sonst glatt- und ebnen Fluth,", "tokens": ["Auf", "der", "sonst", "glat\u00b7t", "und", "eb\u00b7nen", "Fluth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "TRUNC", "KON", "ADJA", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.26": {"text": "Erregt der Tropfen rauschend Fallen,", "tokens": ["Er\u00b7regt", "der", "Trop\u00b7fen", "rau\u00b7schend", "Fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVPP", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Ein \u00e4msiges Geh\u00fcpf, da Blasen sich erh\u00f6hn,", "tokens": ["Ein", "\u00e4m\u00b7si\u00b7ges", "Ge\u00b7h\u00fcpf", ",", "da", "Bla\u00b7sen", "sich", "er\u00b7h\u00f6hn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KOUS", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Die schnell entstehen, schnell vergehn,", "tokens": ["Die", "schnell", "ent\u00b7ste\u00b7hen", ",", "schnell", "ver\u00b7gehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVINF", "$,", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Und dadurch unsern Blick vergn\u00fcgen.", "tokens": ["Und", "da\u00b7durch", "un\u00b7sern", "Blick", "ver\u00b7gn\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Man sieht bald hier, bald dort, im Gr\u00fcnen,", "tokens": ["Man", "sieht", "bald", "hier", ",", "bald", "dort", ",", "im", "Gr\u00fc\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "ADV", "$,", "ADV", "ADV", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Wo niedre Stellen sind, verschiedne kleine Seen,", "tokens": ["Wo", "nied\u00b7re", "Stel\u00b7len", "sind", ",", "ver\u00b7schied\u00b7ne", "klei\u00b7ne", "Seen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VAFIN", "$,", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "In gr\u00fcnen Ufern, und in ihnen,", "tokens": ["In", "gr\u00fc\u00b7nen", "U\u00b7fern", ",", "und", "in", "ih\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "KON", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Viel kleine gr\u00fcne Inseln liegen.", "tokens": ["Viel", "klei\u00b7ne", "gr\u00fc\u00b7ne", "In\u00b7seln", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "Dieselben gr\u00fcn\u2019 oft sch\u00f6n bebl\u00fcmte H\u00fcgel", "tokens": ["Die\u00b7sel\u00b7ben", "gr\u00fcn'", "oft", "sch\u00f6n", "be\u00b7bl\u00fcm\u00b7te", "H\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "ADV", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "Sind, in des Wassers klarem Spiegel,", "tokens": ["Sind", ",", "in", "des", "Was\u00b7sers", "kla\u00b7rem", "Spie\u00b7gel", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "Jm Wiederschein gedoppelt sch\u00f6n,", "tokens": ["Jm", "Wie\u00b7der\u00b7schein", "ge\u00b7dop\u00b7pelt", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Wenn man drauf achtet, anzusehn.", "tokens": ["Wenn", "man", "drauf", "ach\u00b7tet", ",", "an\u00b7zu\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PIS", "PAV", "VVFIN", "$,", "VVIZU", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Kurz, alles zeigt uns, wenn, wie wir billig sollten,", "tokens": ["Kurz", ",", "al\u00b7les", "zeigt", "uns", ",", "wenn", ",", "wie", "wir", "bil\u00b7lig", "soll\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PIS", "VVFIN", "PPER", "$,", "KOUS", "$,", "PWAV", "PPER", "ADJD", "VMFIN", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.39": {"text": "Wir alles nur beachten wollten,", "tokens": ["Wir", "al\u00b7les", "nur", "be\u00b7ach\u00b7ten", "woll\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.40": {"text": "Da\u00df, auch in feuchter Regenszeit,", "tokens": ["Da\u00df", ",", "auch", "in", "feuch\u00b7ter", "Re\u00b7gens\u00b7zeit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "Es einer aufmerksamen Seele,", "tokens": ["Es", "ei\u00b7ner", "auf\u00b7merk\u00b7sa\u00b7men", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.42": {"text": "An Anla\u00df zur Vergn\u00fcglichkeit,", "tokens": ["An", "An\u00b7la\u00df", "zur", "Ver\u00b7gn\u00fcg\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.43": {"text": "Und Vorw\u00fcrf, uns zur Lust, nicht fehle.", "tokens": ["Und", "Vor\u00b7w\u00fcrf", ",", "uns", "zur", "Lust", ",", "nicht", "feh\u00b7le", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PPER", "APPRART", "NN", "$,", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.44": {"text": "Ach so gew\u00f6hnt doch euren Sinn,", "tokens": ["Ach", "so", "ge\u00b7w\u00f6hnt", "doch", "eu\u00b7ren", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "VVPP", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.45": {"text": "Da\u00df er, zu aller Zeit, des Sch\u00f6pfers Wunderwerke", "tokens": ["Da\u00df", "er", ",", "zu", "al\u00b7ler", "Zeit", ",", "des", "Sch\u00f6p\u00b7fers", "Wun\u00b7der\u00b7wer\u00b7ke"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "APPR", "PIAT", "NN", "$,", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Nicht, wie bisher, nur \u00fcberhin;", "tokens": ["Nicht", ",", "wie", "bis\u00b7her", ",", "nur", "\u00fc\u00b7berh\u00b7in", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "PWAV", "ADV", "$,", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.47": {"text": "Nein! erstlich euch zur Lust, darauf zu Gottes Ehr,", "tokens": ["Nein", "!", "erst\u00b7lich", "euch", "zur", "Lust", ",", "da\u00b7rauf", "zu", "Got\u00b7tes", "Ehr", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "ADJD", "PPER", "APPRART", "NN", "$,", "PAV", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Zu seinem Preis und Lob und Dank je mehr und mehr,", "tokens": ["Zu", "sei\u00b7nem", "Preis", "und", "Lob", "und", "Dank", "je", "mehr", "und", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "KON", "NN", "ADV", "ADV", "KON", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Mit froher Achtsamkeit bemerke!", "tokens": ["Mit", "fro\u00b7her", "Acht\u00b7sam\u00b7keit", "be\u00b7mer\u00b7ke", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.50": {"text": "So gar des Regens tr\u00fcbe Zeit", "tokens": ["So", "gar", "des", "Re\u00b7gens", "tr\u00fc\u00b7be", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.51": {"text": "Giebt euch dazu Gelegenheit.", "tokens": ["Giebt", "euch", "da\u00b7zu", "Ge\u00b7le\u00b7gen\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PAV", "NN", "$."], "meter": "+-++-+-+", "measure": "unknown.measure.penta"}, "line.52": {"text": "Ach la\u00df uns doch f\u00fcr alle Gaben,", "tokens": ["Ach", "la\u00df", "uns", "doch", "f\u00fcr", "al\u00b7le", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVIMP", "PPER", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.53": {"text": "O Herr! die wir von dir allein,", "tokens": ["O", "Herr", "!", "die", "wir", "von", "dir", "al\u00b7lein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PRELS", "PPER", "APPR", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.54": {"text": "In 