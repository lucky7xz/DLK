{"textgrid.poem.56019": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Fremde Familie", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So wie der Staub, der irgendwie beginnt", "tokens": ["So", "wie", "der", "Staub", ",", "der", "ir\u00b7gend\u00b7wie", "be\u00b7ginnt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "KOKOM", "ART", "NN", "$,", "PRELS", "ADV", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "und nirgends ist, zu unerkl\u00e4rtem Zwecke", "tokens": ["und", "nir\u00b7gends", "ist", ",", "zu", "un\u00b7er\u00b7kl\u00e4r\u00b7tem", "Zwe\u00b7cke"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "an einem leeren Morgen in der Ecke", "tokens": ["an", "ei\u00b7nem", "lee\u00b7ren", "Mor\u00b7gen", "in", "der", "E\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "in die man sieht, ganz rasch zu Grau gerinnt,", "tokens": ["in", "die", "man", "sieht", ",", "ganz", "rasch", "zu", "Grau", "ge\u00b7rinnt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PIS", "VVFIN", "$,", "ADV", "ADJD", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "so bildeten sie sich, wer wei\u00df aus was,", "tokens": ["so", "bil\u00b7de\u00b7ten", "sie", "sich", ",", "wer", "wei\u00df", "aus", "was", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$,", "PWS", "VVFIN", "APPR", "PRELS", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "im letzten Augenblick vor deinen Schritten", "tokens": ["im", "letz\u00b7ten", "Au\u00b7gen\u00b7blick", "vor", "dei\u00b7nen", "Schrit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "und waren etwas Ungewisses mitten", "tokens": ["und", "wa\u00b7ren", "et\u00b7was", "Un\u00b7ge\u00b7wis\u00b7ses", "mit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PIAT", "NN", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "im nassen Niederschlag der Gasse, das", "tokens": ["im", "nas\u00b7sen", "Nie\u00b7der\u00b7schlag", "der", "Gas\u00b7se", ",", "das"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$,", "PRELS"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "nach dir verlangte. Oder nicht nach dir.", "tokens": ["nach", "dir", "ver\u00b7lang\u00b7te", ".", "O\u00b7der", "nicht", "nach", "dir", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "$.", "NE", "PTKNEG", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Denn eine Stimme, wie vom vorigen Jahr,", "tokens": ["Denn", "ei\u00b7ne", "Stim\u00b7me", ",", "wie", "vom", "vo\u00b7ri\u00b7gen", "Jahr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PWAV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "sang dich zwar an und blieb doch ein Geweine;", "tokens": ["sang", "dich", "zwar", "an", "und", "blieb", "doch", "ein", "Ge\u00b7wei\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKVZ", "KON", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "und eine Hand, die wie geliehen war,", "tokens": ["und", "ei\u00b7ne", "Hand", ",", "die", "wie", "ge\u00b7lie\u00b7hen", "war", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "KOKOM", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "kam zwar hervor und nahm doch nicht die deine.", "tokens": ["kam", "zwar", "her\u00b7vor", "und", "nahm", "doch", "nicht", "die", "dei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKVZ", "KON", "VVFIN", "ADV", "PTKNEG", "ART", "PPOSAT", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wer kommt denn noch? Wen meinen diese vier?", "tokens": ["Wer", "kommt", "denn", "noch", "?", "Wen", "mei\u00b7nen", "die\u00b7se", "vier", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "ADV", "$.", "PWS", "VVFIN", "PDAT", "CARD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}