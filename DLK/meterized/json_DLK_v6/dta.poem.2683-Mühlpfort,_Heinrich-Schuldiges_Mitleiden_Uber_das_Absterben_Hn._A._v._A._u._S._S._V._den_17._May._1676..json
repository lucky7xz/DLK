{"dta.poem.2683": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Schuldiges Mitleiden  \n  Uber das Absterben Hn. A. v. A. u. S.   S. V.  \n den 17. May. 1676.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.71", "nl:0.28"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sinckt jetzt das Aug in Nacht/ so lange Zeit gewachet/", "tokens": ["Sinckt", "jetzt", "das", "Aug", "in", "Nacht", "/", "so", "lan\u00b7ge", "Zeit", "ge\u00b7wa\u00b7chet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "APPR", "NN", "$(", "ADV", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und f\u00fcr das Vaterland Vorsorge hat gef\u00fchrt?", "tokens": ["Und", "f\u00fcr", "das", "Va\u00b7ter\u00b7land", "Vor\u00b7sor\u00b7ge", "hat", "ge\u00b7f\u00fchrt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Schweigt jetzt der g\u00fcldne Mund/ der Steine weich ge-", "tokens": ["Schweigt", "jetzt", "der", "g\u00fcld\u00b7ne", "Mund", "/", "der", "Stei\u00b7ne", "weich", "ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$(", "ART", "NN", "ADJD", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und hohen H\u00e4uptern offt die Hertzen hat ger\u00fchrt?", "tokens": ["Und", "ho\u00b7hen", "H\u00e4up\u00b7tern", "offt", "die", "Hert\u00b7zen", "hat", "ge\u00b7r\u00fchrt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADV", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Starrt die gelehrte Faust/ die Urtheil hat geschrieben/", "tokens": ["Starrt", "die", "ge\u00b7lehr\u00b7te", "Faust", "/", "die", "Ur\u00b7theil", "hat", "ge\u00b7schrie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$(", "ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und in dem weiten Recht viel Schrifften auffgesetzt?", "tokens": ["Und", "in", "dem", "wei\u00b7ten", "Recht", "viel", "Schriff\u00b7ten", "auff\u00b7ge\u00b7setzt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ist von dem theuren Mann denn nichts mehr \u00fcberblieben/", "tokens": ["Ist", "von", "dem", "theu\u00b7ren", "Mann", "denn", "nichts", "mehr", "\u00fc\u00b7berb\u00b7lie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "ADJA", "NN", "KON", "PIS", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Das auch die Nach-Welt noch zu ehren w\u00fcrdig sch\u00e4tzt?", "tokens": ["Das", "auch", "die", "Nach\u00b7Welt", "noch", "zu", "eh\u00b7ren", "w\u00fcr\u00b7dig", "sch\u00e4tzt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ART", "NN", "ADV", "PTKZU", "VVINF", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ach ja! ", "tokens": ["Ach", "ja", "!"], "token_info": ["word", "word", "punct"], "pos": ["ITJ", "ITJ", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.10": {"text": "Sein unverg\u00e4nglich Ruhm verlacht nur Grab und Grufft/", "tokens": ["Sein", "un\u00b7ver\u00b7g\u00e4ng\u00b7lich", "Ruhm", "ver\u00b7lacht", "nur", "Grab", "und", "Grufft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "VVFIN", "ADV", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und Fama wil den Prei\u00df der Dienste nicht verh\u00f6len", "tokens": ["Und", "Fa\u00b7ma", "wil", "den", "Prei\u00df", "der", "Diens\u00b7te", "nicht", "ver\u00b7h\u00f6\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VMFIN", "ART", "NN", "ART", "NN", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Indem sie durch die Welt sein ewig Lob ausrufft.", "tokens": ["In\u00b7dem", "sie", "durch", "die", "Welt", "sein", "e\u00b7wig", "Lob", "aus\u00b7rufft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "PPOSAT", "ADJD", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Was thut nicht unsre Stadt nun ihr Oracul schweiget?", "tokens": ["Was", "thut", "nicht", "uns\u00b7re", "Stadt", "nun", "ihr", "O\u00b7ra\u00b7cul", "schwei\u00b7get", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PTKNEG", "PPOSAT", "NN", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Was thut das Rathhau\u00df nicht nun jetzt sein Eckstein f\u00e4llt?", "tokens": ["Was", "thut", "das", "Rath\u00b7hau\u00df", "nicht", "nun", "jetzt", "sein", "E\u00b7ckstein", "f\u00e4llt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "PTKNEG", "ADV", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Rufft nicht die B\u00fcrgerschafft wenn sie ihr Beyleid zeiget?", "tokens": ["Rufft", "nicht", "die", "B\u00fcr\u00b7ger\u00b7schafft", "wenn", "sie", "ihr", "Bey\u00b7leid", "zei\u00b7get", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "NN", "KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Die Mauren dieser Stadt sind durch den Rie\u00df zersch\u00e4llt.", "tokens": ["Die", "Mau\u00b7ren", "die\u00b7ser", "Stadt", "sind", "durch", "den", "Rie\u00df", "zer\u00b7sch\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "NN", "VAFIN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Die Taffel der Gesetz\u2019 ist/ leider nur zerbrochen/", "tokens": ["Die", "Taf\u00b7fel", "der", "Ge\u00b7setz'", "ist", "/", "lei\u00b7der", "nur", "zer\u00b7bro\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "$(", "ADV", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Der F\u00fcrsten Kleynod hin/ der Armen Hoffnung tod/", "tokens": ["Der", "F\u00fcrs\u00b7ten", "Kleyn\u00b7od", "hin", "/", "der", "Ar\u00b7men", "Hoff\u00b7nung", "tod", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKVZ", "$(", "ART", "NN", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Und/ der Partheyen hat das rechte Recht gesprochen/", "tokens": ["Und", "/", "der", "Par\u00b7the\u00b7yen", "hat", "das", "rech\u00b7te", "Recht", "ge\u00b7spro\u00b7chen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "ART", "NN", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.20": {"text": "Kan nicht ent\u00e4usert seyn der Sterbligkeit Gebot.", "tokens": ["Kan", "nicht", "en\u00b7t\u00e4u\u00b7sert", "seyn", "der", "Ster\u00b7blig\u00b7keit", "Ge\u00b7bot", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "VVPP", "VAINF", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Unsch\u00e4tzbarer Verlust! du Ulpian der Zeiten/", "tokens": ["Un\u00b7sch\u00e4tz\u00b7ba\u00b7rer", "Ver\u00b7lust", "!", "du", "Ul\u00b7pi\u00b7an", "der", "Zei\u00b7ten", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "PPER", "NE", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Den Themisstets geliebt/ und Suada Sohn genant/", "tokens": ["Den", "The\u00b7miss\u00b7tets", "ge\u00b7liebt", "/", "und", "Sua\u00b7da", "Sohn", "ge\u00b7nant", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$(", "KON", "NE", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "So l\u00e4st du uns allhier mit Recht und Unrecht streiten/", "tokens": ["So", "l\u00e4st", "du", "uns", "all\u00b7hier", "mit", "Recht", "und", "Un\u00b7recht", "strei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "APPR", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und suchst die wahre Ruh in jenem Vaterland?", "tokens": ["Und", "suchst", "die", "wah\u00b7re", "Ruh", "in", "je\u00b7nem", "Va\u00b7ter\u00b7land", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Wie wol ist dir geschehn! wie sehr wird dich doch missen", "tokens": ["Wie", "wol", "ist", "dir", "ge\u00b7schehn", "!", "wie", "sehr", "wird", "dich", "doch", "mis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VAFIN", "PPER", "VVPP", "$.", "PWAV", "ADV", "VAFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Das gantze Schlesien/ so deinen Witz geehrt.", "tokens": ["Das", "gant\u00b7ze", "Schle\u00b7si\u00b7en", "/", "so", "dei\u00b7nen", "Witz", "ge\u00b7ehrt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Was unter tausenden die Kl\u00fcgsten auch kaum wissen", "tokens": ["Was", "un\u00b7ter", "tau\u00b7sen\u00b7den", "die", "Kl\u00fcgs\u00b7ten", "auch", "kaum", "wis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "ADJA", "ART", "NN", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Hat in verwirrtem Stand uns offt dein Mund gelehrt.", "tokens": ["Hat", "in", "ver\u00b7wirr\u00b7tem", "Stand", "uns", "offt", "dein", "Mund", "ge\u00b7lehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ADJA", "NN", "PPER", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Dir war das Alterthum des Landes unverborgen/", "tokens": ["Dir", "war", "das", "Al\u00b7ter\u00b7thum", "des", "Lan\u00b7des", "un\u00b7ver\u00b7bor\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Desselben Policey Erfindung wol bewust.", "tokens": ["Des\u00b7sel\u00b7ben", "Po\u00b7li\u00b7cey", "Er\u00b7fin\u00b7dung", "wol", "be\u00b7wust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Der nimmer-lasse Flei\u00df/ und unersch\u00f6pffte Sorgen", "tokens": ["Der", "nim\u00b7mer\u00b7las\u00b7se", "Flei\u00df", "/", "und", "un\u00b7er\u00b7sch\u00f6pff\u00b7te", "Sor\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$(", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Die legten einen Schatz der Br\u00e4uch\u2019 in deine Brust.", "tokens": ["Die", "leg\u00b7ten", "ei\u00b7nen", "Schatz", "der", "Br\u00e4uch'", "in", "dei\u00b7ne", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Es haben F\u00fcrsten offt in zweiffelhafften Dingen", "tokens": ["Es", "ha\u00b7ben", "F\u00fcrs\u00b7ten", "offt", "in", "zweif\u00b7fel\u00b7haff\u00b7ten", "Din\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "NN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Den Gordens-Knoten dir zu l\u00f6sen f\u00fcrgelegt/", "tokens": ["Den", "Gor\u00b7dens\u00b7Kno\u00b7ten", "dir", "zu", "l\u00f6\u00b7sen", "f\u00fcr\u00b7ge\u00b7legt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "PTKZU", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Dein tieffgesch\u00e4rffter Witz war m\u00e4chtig durch zu dringen", "tokens": ["Dein", "tieff\u00b7ge\u00b7sch\u00e4rff\u00b7ter", "Witz", "war", "m\u00e4ch\u00b7tig", "durch", "zu", "drin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ADJD", "APPR", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Und hat verh\u00e4rtete zur Lindigkeit bewegt.", "tokens": ["Und", "hat", "ver\u00b7h\u00e4r\u00b7te\u00b7te", "zur", "Lin\u00b7dig\u00b7keit", "be\u00b7wegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "VVFIN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Astr", "tokens": ["Astr"], "token_info": ["word"], "pos": ["NE"], "meter": "+", "measure": "single.up"}, "line.38": {"text": "Wenns die Gerechtigkeit recht abzuw\u00e4gen kam.", "tokens": ["Wenns", "die", "Ge\u00b7rech\u00b7tig\u00b7keit", "recht", "ab\u00b7zu\u00b7w\u00e4\u00b7gen", "kam", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "VVIZU", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "In F\u00e4llen da wol offt den meisten hat gegrauet/", "tokens": ["In", "F\u00e4l\u00b7len", "da", "wol", "offt", "den", "meis\u00b7ten", "hat", "ge\u00b7grau\u00b7et", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "ADV", "ADV", "ART", "PIS", "VAFIN", "VVPP", "$("], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.40": {"text": "Sah\u2019 man doch deinen Muth/ der alles unternahm.", "tokens": ["Sah'", "man", "doch", "dei\u00b7nen", "Muth", "/", "der", "al\u00b7les", "un\u00b7ter\u00b7nahm", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "PPOSAT", "NN", "$(", "ART", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Es kan des Solons Ernst/ des Catons Tapfferkeiten", "tokens": ["Es", "kan", "des", "So\u00b7lons", "Ernst", "/", "des", "Ca\u00b7tons", "Tapf\u00b7fer\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "NN", "NE", "$(", "ART", "NE", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Mit so viel Spr\u00fcchen nicht der Nach-Welt Mund erh\u00f6hn/", "tokens": ["Mit", "so", "viel", "Spr\u00fc\u00b7chen", "nicht", "der", "Nach\u00b7Welt", "Mund", "er\u00b7h\u00f6hn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PIAT", "NN", "PTKNEG", "ART", "NN", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Als dich mit Tugenden gekr\u00f6nt von allen Seiten", "tokens": ["Als", "dich", "mit", "Tu\u