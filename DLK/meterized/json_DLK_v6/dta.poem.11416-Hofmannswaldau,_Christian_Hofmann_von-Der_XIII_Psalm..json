{"dta.poem.11416": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der  XIII  Psalm.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hjlff HErr! das reine gold ist mehrentheils verschmoltzen,", "tokens": ["Hjlff", "Herr", "!", "das", "rei\u00b7ne", "gold", "ist", "meh\u00b7ren\u00b7theils", "ver\u00b7schmolt\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "ART", "ADJA", "NN", "VAFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und dein getreues volck nimmt augenscheinlich ab:", "tokens": ["Und", "dein", "ge\u00b7treu\u00b7es", "volck", "nimmt", "au\u00b7gen\u00b7schein\u00b7lich", "ab", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das los f\u00e4llt in der welt nur immer vor die stoltzen,", "tokens": ["Das", "los", "f\u00e4llt", "in", "der", "welt", "nur", "im\u00b7mer", "vor", "die", "stolt\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKVZ", "VVFIN", "APPR", "ART", "NN", "ADV", "ADV", "APPR", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und wer nicht mitte macht, mu\u00df an den bettel-stab.", "tokens": ["Und", "wer", "nicht", "mit\u00b7te", "macht", ",", "mu\u00df", "an", "den", "bet\u00b7tel\u00b7stab", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PTKNEG", "ADV", "VVFIN", "$,", "VMFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die priester zancken sich um abgeschmackte grillen:", "tokens": ["Die", "pries\u00b7ter", "zan\u00b7cken", "sich", "um", "ab\u00b7ge\u00b7schmack\u00b7te", "gril\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Jhr gantzes Christenthum ist lauter heucheley:", "tokens": ["Ihr", "gant\u00b7zes", "Chris\u00b7ten\u00b7thum", "ist", "lau\u00b7ter", "heu\u00b7che\u00b7ley", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Sie wollen lehrer seyn, und dein gesetz erf\u00fcllen;", "tokens": ["Sie", "wol\u00b7len", "leh\u00b7rer", "seyn", ",", "und", "dein", "ge\u00b7setz", "er\u00b7f\u00fcl\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "VAINF", "$,", "KON", "PPOSAT", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Doch ihre predigten sind nur ein leer geschrey.", "tokens": ["Doch", "ih\u00b7re", "pre\u00b7dig\u00b7ten", "sind", "nur", "ein", "leer", "ge\u00b7schrey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Denn ihre lehre quillt aus keinem s\u00fcssen brunnen,", "tokens": ["Denn", "ih\u00b7re", "leh\u00b7re", "quillt", "aus", "kei\u00b7nem", "s\u00fcs\u00b7sen", "brun\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Weil in dem hertzen nichts, als gall und bo\u00dfheit, steckt.", "tokens": ["Weil", "in", "dem", "hert\u00b7zen", "nichts", ",", "als", "gall", "und", "bo\u00df\u00b7heit", ",", "steckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "PIS", "$,", "KOUS", "NN", "KON", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Aus faulen pf\u00fctzen kommt kein lebens-strohm gerunnen:", "tokens": ["Aus", "fau\u00b7len", "pf\u00fct\u00b7zen", "kommt", "kein", "le\u00b7bens\u00b7strohm", "ge\u00b7run\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Dein wort kennt keinen geist, den s\u00fcnd und schuld befleckt,", "tokens": ["Dein", "wort", "kennt", "kei\u00b7nen", "geist", ",", "den", "s\u00fcnd", "und", "schuld", "be\u00b7fleckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PIAT", "NN", "$,", "PRELS", "ADJD", "KON", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Und gleichwohl m\u00fcssen wir und deine diener schweigen.", "tokens": ["Und", "gleich\u00b7wohl", "m\u00fcs\u00b7sen", "wir", "und", "dei\u00b7ne", "die\u00b7ner", "schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "KON", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Herr! rotte diesen schwarm und alle heuchler aus,", "tokens": ["Herr", "!", "rot\u00b7te", "die\u00b7sen", "schwarm", "und", "al\u00b7le", "heuch\u00b7ler", "aus", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "PDAT", "ADJD", "KON", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Die dir und deinem volck itzund zu kopffe steigen,", "tokens": ["Die", "dir", "und", "dei\u00b7nem", "volck", "it\u00b7zund", "zu", "kopf\u00b7fe", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "KON", "PPOSAT", "NN", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und immer stehn und schreyn: Hier ist des HErren haus!", "tokens": ["Und", "im\u00b7mer", "stehn", "und", "schreyn", ":", "Hier", "ist", "des", "Her\u00b7ren", "haus", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVINF", "KON", "ADJD", "$.", "ADV", "VAFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Erf\u00fclle deinen bund, und rette deine kinder,", "tokens": ["Er\u00b7f\u00fcl\u00b7le", "dei\u00b7nen", "bund", ",", "und", "ret\u00b7te", "dei\u00b7ne", "kin\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Die Babels tyranney durch ihre pfaffen dr\u00fcckt!", "tokens": ["Die", "Ba\u00b7bels", "ty\u00b7ran\u00b7ney", "durch", "ih\u00b7re", "pfaf\u00b7fen", "dr\u00fcckt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Und schick uns lehrer zu, durch deren mund der s\u00fcnder", "tokens": ["Und", "schick", "uns", "leh\u00b7rer", "zu", ",", "durch", "de\u00b7ren", "mund", "der", "s\u00fcn\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "PPER", "NN", "PTKVZ", "$,", "APPR", "PRELAT", "NN", "ART", "ADJA"], "meter": "-+-+---+-+-+-", "measure": "unknown.measure.penta"}, "line.20": {"text": "Von seinem wege l\u00e4\u00dft und sich zur busse schickt!", "tokens": ["Von", "sei\u00b7nem", "we\u00b7ge", "l\u00e4\u00dft", "und", "sich", "zur", "bus\u00b7se", "schickt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "KON", "PRF", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "La\u00df keinen miedling mehr bey deiner heerde w\u00fctten,", "tokens": ["La\u00df", "kei\u00b7nen", "mied\u00b7ling", "mehr", "bey", "dei\u00b7ner", "heer\u00b7de", "w\u00fct\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PIAT", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Und stell uns hirten vor, die recht beruffen seyn,", "tokens": ["Und", "stell", "uns", "hir\u00b7ten", "vor", ",", "die", "recht", "be\u00b7ruf\u00b7fen", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPER", "VVFIN", "PTKVZ", "$,", "PRELS", "ADJD", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Die balsam auf das hertz erschrockner seelen sch\u00fctten,", "tokens": ["Die", "bal\u00b7sam", "auf", "das", "hertz", "er\u00b7schrock\u00b7ner", "see\u00b7len", "sch\u00fct\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und der verruchten schaar mit deinem donner dr\u00e4un!", "tokens": ["Und", "der", "ver\u00b7ruch\u00b7ten", "schaar", "mit", "dei\u00b7nem", "don\u00b7ner", "dr\u00e4un", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Ja, HErr! du wirst es thun. Denn was dein mund versprochen,", "tokens": ["Ja", ",", "Herr", "!", "du", "wirst", "es", "thun", ".", "Denn", "was", "dein", "mund", "ver\u00b7spro\u00b7chen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NN", "$.", "PPER", "VAFIN", "PPER", "VVINF", "$.", "KON", "PWS", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Ist nicht ein menschen-wort, das leichtlich fehlen kan.", "tokens": ["Ist", "nicht", "ein", "men\u00b7schen\u00b7wort", ",", "das", "leicht\u00b7lich", "feh\u00b7len", "kan", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ART", "NN", "$,", "PRELS", "ADJD", "VVINF", "VMFIN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.27": {"text": "Mag doch die ketzer-rott indessen galle kochen;", "tokens": ["Mag", "doch", "die", "ket\u00b7zer\u00b7rott", "in\u00b7des\u00b7sen", "gal\u00b7le", "ko\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "ADV", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Sie feuren doch dadurch nur deinen eyfer an.", "tokens": ["Sie", "feu\u00b7ren", "doch", "da\u00b7durch", "nur", "dei\u00b7nen", "ey\u00b7fer", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PAV", "ADV", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Du wirst uns starck genung vor dem geschmeisse sch\u00fctzen,", "tokens": ["Du", "wirst", "uns", "starck", "ge\u00b7nung", "vor", "dem", "ge\u00b7schmeis\u00b7se", "sch\u00fct\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Das unertr\u00e4glich herrscht und unsre freyheit hemmt;", "tokens": ["Das", "un\u00b7er\u00b7tr\u00e4g\u00b7lich", "herrscht", "und", "uns\u00b7re", "frey\u00b7heit", "hemmt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "VVPP", "KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Herr! la\u00df durch deinen arm den bau der kirche st\u00fctzen,", "tokens": ["Herr", "!", "la\u00df", "durch", "dei\u00b7nen", "arm", "den", "bau", "der", "kir\u00b7che", "st\u00fct\u00b7zen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVIMP", "APPR", "PPOSAT", "ADJD", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Damit des satans reich nicht alles \u00fcberschwemmt!", "tokens": ["Da\u00b7mit", "des", "sa\u00b7tans", "reich", "nicht", "al\u00b7les", "\u00fc\u00b7bersc\u00b7hwemmt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "ADJD", "PTKNEG", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}