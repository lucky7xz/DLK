{"textgrid.poem.35479": {"metadata": {"author": {"name": "Conradi, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "In schlafloser Nacht", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich liege schlaflos. \u2013 Die Gedanken kreisen", "tokens": ["Ich", "lie\u00b7ge", "schlaf\u00b7los", ".", "\u2013", "Die", "Ge\u00b7dan\u00b7ken", "krei\u00b7sen"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "$.", "$(", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In alten und in neuen Geleisen.", "tokens": ["In", "al\u00b7ten", "und", "in", "neu\u00b7en", "Ge\u00b7lei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "KON", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Die Enge dr\u00fcckt mich \u2013 es dr\u00fcckt mich die Nacht \u2013", "tokens": ["Die", "En\u00b7ge", "dr\u00fcckt", "mich", "\u2013", "es", "dr\u00fcckt", "mich", "die", "Nacht", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$(", "PPER", "VVFIN", "PPER", "ART", "NN", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wehe dem Armen, der einsam wacht!", "tokens": ["We\u00b7he", "dem", "Ar\u00b7men", ",", "der", "ein\u00b7sam", "wacht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Wehe dem Armen, der einsam b\u00fc\u00dft,", "tokens": ["We\u00b7he", "dem", "Ar\u00b7men", ",", "der", "ein\u00b7sam", "b\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Dem nichts den Wermut der Reue vers\u00fc\u00dft!", "tokens": ["Dem", "nichts", "den", "Wer\u00b7mut", "der", "Reu\u00b7e", "ver\u00b7s\u00fc\u00dft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Ich liege schlaflos ... und alles still ...", "tokens": ["Ich", "lie\u00b7ge", "schlaf\u00b7los", "...", "und", "al\u00b7les", "still", "..."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$(", "KON", "PIS", "ADJD", "$("], "meter": "-+----+-+", "measure": "dactylic.init"}, "line.2": {"text": "Es atmet die Nacht, die vergeben nicht will ...", "tokens": ["Es", "at\u00b7met", "die", "Nacht", ",", "die", "ver\u00b7ge\u00b7ben", "nicht", "will", "..."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "VVFIN", "PTKNEG", "VMFIN", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.5": {"line.1": {"text": "Da klappert ein Schritt die Stra\u00dfe heran ...", "tokens": ["Da", "klap\u00b7pert", "ein", "Schritt", "die", "Stra\u00b7\u00dfe", "he\u00b7ran", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$("], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ein leiser Gang ... Und er schwillt an ...", "tokens": ["Ein", "lei\u00b7ser", "Gang", "...", "Und", "er", "schwillt", "an", "..."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "KON", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Und in mein einsam K\u00e4mmerlein", "tokens": ["Und", "in", "mein", "ein\u00b7sam", "K\u00e4m\u00b7mer\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Flutet ein Lied der Sehnsucht hinein ...", "tokens": ["Flu\u00b7tet", "ein", "Lied", "der", "Sehn\u00b7sucht", "hin\u00b7ein", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}}, "stanza.7": {"line.1": {"text": "Ein Lied so ergreifend, so mild und so schwer ...", "tokens": ["Ein", "Lied", "so", "er\u00b7grei\u00b7fend", ",", "so", "mild", "und", "so", "schwer", "..."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "$,", "ADV", "ADJD", "KON", "ADV", "ADJD", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "An Entsagung so voll ... an Entz\u00fccken so leer ...", "tokens": ["An", "Ent\u00b7sa\u00b7gung", "so", "voll", "...", "an", "Ent\u00b7z\u00fc\u00b7cken", "so", "leer", "..."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "ADJD", "$(", "APPR", "NN", "ADV", "ADJD", "$("], "meter": "+-+---+-+--+", "measure": "iambic.penta.chol"}}, "stanza.8": {"line.1": {"text": "Da fa\u00dft es mich j\u00e4h \u2013 ich walle empor ...", "tokens": ["Da", "fa\u00dft", "es", "mich", "j\u00e4h", "\u2013", "ich", "wal\u00b7le", "em\u00b7por", "..."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADJD", "$(", "PPER", "VVFIN", "PTKVZ", "$("], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "T\u00f6nt in mir ein brausender Engelchor?", "tokens": ["T\u00f6nt", "in", "mir", "ein", "brau\u00b7sen\u00b7der", "En\u00b7gel\u00b7chor", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.9": {"line.1": {"text": "Ich hebe mich auf \u2013 ich atme bang \u2013", "tokens": ["Ich", "he\u00b7be", "mich", "auf", "\u2013", "ich", "at\u00b7me", "bang", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "$(", "PPER", "VVFIN", "ADJD", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und mich bezwingt unheimlicher Drang ...", "tokens": ["Und", "mich", "be\u00b7zwingt", "un\u00b7heim\u00b7li\u00b7cher", "Drang", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJA", "NN", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.10": {"line.1": {"text": "Oh! K\u00f6nnt' ich dich an die Brust wild rei\u00dfen \u2013", "tokens": ["Oh", "!", "K\u00f6nnt'", "ich", "dich", "an", "die", "Brust", "wild", "rei\u00b7\u00dfen", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VMFIN", "PPER", "PRF", "APPR", "ART", "NN", "ADJD", "VVFIN", "$("], "meter": "+---+-+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Dich, die ich habe gehen hei\u00dfen!", "tokens": ["Dich", ",", "die", "ich", "ha\u00b7be", "ge\u00b7hen", "hei\u00b7\u00dfen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "VAFIN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Ich hielte dich sicher \u2013 und du vergibst \u2013", "tokens": ["Ich", "hiel\u00b7te", "dich", "si\u00b7cher", "\u2013", "und", "du", "ver\u00b7gibst", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$(", "KON", "PPER", "VVFIN", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Und du sagst mir noch einmal, da\u00df du mich liebst!", "tokens": ["Und", "du", "sagst", "mir", "noch", "ein\u00b7mal", ",", "da\u00df", "du", "mich", "liebst", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV", "ADV", "$,", "KOUS", "PPER", "PRF", "ADJD", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.12": {"line.1": {"text": "Die Schritte verhallen ... Es schweigt der Gesang ...", "tokens": ["Die", "Schrit\u00b7te", "ver\u00b7hal\u00b7len", "...", "Es", "schweigt", "der", "Ge\u00b7sang", "..."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$(", "PPER", "VVFIN", "ART", "NN", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Es br\u00f6ckelt meiner Seele d\u00e4monischer Drang ...", "tokens": ["Es", "br\u00f6\u00b7ckelt", "mei\u00b7ner", "See\u00b7le", "d\u00e4\u00b7mo\u00b7ni\u00b7scher", "Drang", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.13": {"line.1": {"text": "Nun wieder Stille ... Es atmet die Nacht.", "tokens": ["Nun", "wie\u00b7der", "Stil\u00b7le", "...", "Es", "at\u00b7met", "die", "Nacht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "$(", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Wehe dem Armen, der einsam wacht!", "tokens": ["We\u00b7he", "dem", "Ar\u00b7men", ",", "der", "ein\u00b7sam", "wacht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.14": {"line.1": {"text": "Der einsam nach Verlorenem sp\u00fcrt ...", "tokens": ["Der", "ein\u00b7sam", "nach", "Ver\u00b7lo\u00b7re\u00b7nem", "sp\u00fcrt", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Es atmet die Nacht \u2013 schicksalkn\u00fcpfend und unger\u00fchrt.", "tokens": ["Es", "at\u00b7met", "die", "Nacht", "\u2013", "schick\u00b7sal\u00b7kn\u00fcp\u00b7fend", "und", "un\u00b7ge\u00b7r\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$(", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+--+--+-+", "measure": "amphibrach.tetra.plus"}}}}}