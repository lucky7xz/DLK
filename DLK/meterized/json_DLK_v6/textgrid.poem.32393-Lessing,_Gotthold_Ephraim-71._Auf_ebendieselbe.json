{"textgrid.poem.32393": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "71. Auf ebendieselbe", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kommt diesem Amor nicht zu nah,", "tokens": ["Kommt", "die\u00b7sem", "A\u00b7mor", "nicht", "zu", "nah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "NE", "PTKNEG", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und st\u00f6rt ihn nicht in seinem Staunen!", "tokens": ["Und", "st\u00f6rt", "ihn", "nicht", "in", "sei\u00b7nem", "Stau\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Noch steht er so, in Einem s\u00fc\u00dfen Staunen,", "tokens": ["Noch", "steht", "er", "so", ",", "in", "Ei\u00b7nem", "s\u00fc\u00b7\u00dfen", "Stau\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Seit er Philinden sah.", "tokens": ["Seit", "er", "Phi\u00b7lin\u00b7den", "sah", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}