{"textgrid.poem.56985": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbich bin ein einsamer Schaukelstuhl", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbich bin ein einsamer Schaukelstuhl", "tokens": ["\u00bb", "ich", "bin", "ein", "ein\u00b7sa\u00b7mer", "Schau\u00b7kel\u00b7stuhl"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "und wackel im Winde, im Winde.", "tokens": ["und", "wa\u00b7ckel", "im", "Win\u00b7de", ",", "im", "Win\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "NN", "$,", "APPRART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Auf der Terrasse, da ist es kuhl,", "tokens": ["Auf", "der", "Ter\u00b7ras\u00b7se", ",", "da", "ist", "es", "kuhl", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADV", "VAFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "und ich wackel im Winde, im Winde.", "tokens": ["und", "ich", "wa\u00b7ckel", "im", "Win\u00b7de", ",", "im", "Win\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJD", "APPRART", "NN", "$,", "APPRART", "NN", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}}, "stanza.3": {"line.1": {"text": "Und ich wackel und nackel den ganzen Tag.", "tokens": ["Und", "ich", "wa\u00b7ckel", "und", "na\u00b7ckel", "den", "gan\u00b7zen", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJD", "KON", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Und es nackelt und rackelt die Linde.", "tokens": ["Und", "es", "na\u00b7ckelt", "und", "ra\u00b7ckelt", "die", "Lin\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVPP", "KON", "VVFIN", "ART", "NE", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Wer wei\u00df, was sonst wohl noch wackeln mag", "tokens": ["Wer", "wei\u00df", ",", "was", "sonst", "wohl", "noch", "wa\u00b7ckeln", "mag"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "$,", "PRELS", "ADV", "ADV", "ADV", "VVINF", "VMFIN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "im Winde, im Winde, im Winde.\u00ab", "tokens": ["im", "Win\u00b7de", ",", "im", "Win\u00b7de", ",", "im", "Win\u00b7de", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "$,", "APPRART", "NN", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}