{"textgrid.poem.59736": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Rausche durch den Wald, rausche durch das Herz,", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Rausche durch den Wald, rausche durch das Herz,", "tokens": ["Rau\u00b7sche", "durch", "den", "Wald", ",", "rau\u00b7sche", "durch", "das", "Herz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Tr\u00e4nenzorn, du frischer Lebenswind!", "tokens": ["Tr\u00e4\u00b7nen\u00b7zorn", ",", "du", "fri\u00b7scher", "Le\u00b7bens\u00b7wind", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Schweige nicht das Wort, schweige nicht den Schmerz,", "tokens": ["Schwei\u00b7ge", "nicht", "das", "Wort", ",", "schwei\u00b7ge", "nicht", "den", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ART", "NN", "$,", "VVFIN", "PTKNEG", "ART", "NN", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Rausche, du des Muts erstgebornes Kind!", "tokens": ["Rau\u00b7sche", ",", "du", "des", "Muts", "erst\u00b7ge\u00b7bor\u00b7nes", "Kind", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Rausche, brause frisch! Klinge, schalle k\u00fchn!", "tokens": ["Rau\u00b7sche", ",", "brau\u00b7se", "frisch", "!", "Klin\u00b7ge", ",", "schal\u00b7le", "k\u00fchn", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ADJD", "$.", "NN", "$,", "VVFIN", "ADJD", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "K\u00fchner, weil der Feigheit Pestilenz,", "tokens": ["K\u00fch\u00b7ner", ",", "weil", "der", "Feig\u00b7heit", "Pes\u00b7ti\u00b7lenz", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Deutsche Pest, uns leirt Welken und Verbl\u00fchn,", "tokens": ["Deut\u00b7sche", "Pest", ",", "uns", "leirt", "Wel\u00b7ken", "und", "Ver\u00b7bl\u00fchn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Winterfrost und Tod vor dem deutschen Lenz.", "tokens": ["Win\u00b7ter\u00b7frost", "und", "Tod", "vor", "dem", "deut\u00b7schen", "Lenz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "\u00bbwo ist Babel heut? Wo das alte Rom?", "tokens": ["\u00bb", "wo", "ist", "Ba\u00b7bel", "heut", "?", "Wo", "das", "al\u00b7te", "Rom", "?"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "VAFIN", "NE", "ADV", "$.", "PWAV", "ART", "ADJA", "NE", "$."], "meter": "-+---+-+-+", "measure": "dactylic.init"}, "line.2": {"text": "Welche Fahnen wehn heut vom Kapitol?", "tokens": ["Wel\u00b7che", "Fah\u00b7nen", "wehn", "heut", "vom", "Ka\u00b7pi\u00b7tol", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "VVFIN", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Wie kein Tropfen flie\u00dft je hinauf den Strom,", "tokens": ["Wie", "kein", "Trop\u00b7fen", "flie\u00dft", "je", "hin\u00b7auf", "den", "Strom", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "VVFIN", "ADV", "ADV", "ART", "NN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Find't erloschner Stern nimmer neuen Pol.\u00ab", "tokens": ["Find't", "er\u00b7lo\u00b7schner", "Stern", "nim\u00b7mer", "neu\u00b7en", "Pol", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "ADJA", "NN", "ADV", "ADJA", "NN", "$.", "$("], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Leiertest du so mit, verschneiter Greis?", "tokens": ["Lei\u00b7er\u00b7test", "du", "so", "mit", ",", "ver\u00b7schnei\u00b7ter", "Greis", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKVZ", "$,", "ADJA", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Tod und Nacht, die deutsche Greisennacht,", "tokens": ["Tod", "und", "Nacht", ",", "die", "deut\u00b7sche", "Grei\u00b7sen\u00b7nacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Weil kein Kaiser kommt, welcher weist und wei\u00df,", "tokens": ["Weil", "kein", "Kai\u00b7ser", "kommt", ",", "wel\u00b7cher", "weist", "und", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "VVFIN", "$,", "PRELS", "ADJD", "KON", "VVFIN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Was den deutschen Mut stark und fr\u00f6hlich macht?", "tokens": ["Was", "den", "deut\u00b7schen", "Mut", "stark", "und", "fr\u00f6h\u00b7lich", "macht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "ADJD", "KON", "ADJD", "VVFIN", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Feiger Memmen Klang t\u00f6nest du so nach,", "tokens": ["Fei\u00b7ger", "Mem\u00b7men", "Klang", "t\u00f6\u00b7nest", "du", "so", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "VVFIN", "PPER", "ADV", "APPR", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Weiberhoffen, Weiberzagen nach,", "tokens": ["Wei\u00b7ber\u00b7hof\u00b7fen", ",", "Wei\u00b7ber\u00b7za\u00b7gen", "nach", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Weil noch immer kein Adlerfl\u00fcgelschlag", "tokens": ["Weil", "noch", "im\u00b7mer", "kein", "Ad\u00b7ler\u00b7fl\u00fc\u00b7gel\u00b7schlag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Klingt den langen Schlaf Barbarossas wach?", "tokens": ["Klingt", "den", "lan\u00b7gen", "Schlaf", "Bar\u00b7ba\u00b7ros\u00b7sas", "wach", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "NE", "PTKVZ", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.6": {"line.1": {"text": "Nicht also mit dir! Nimm dir deutschen Schwung,", "tokens": ["Nicht", "al\u00b7so", "mit", "dir", "!", "Nimm", "dir", "deut\u00b7schen", "Schwung", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPR", "PPER", "$.", "NE", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Deutscher! Nimm einmal dir den deutschen Stolz", "tokens": ["Deut\u00b7scher", "!", "Nimm", "ein\u00b7mal", "dir", "den", "deut\u00b7schen", "Stolz"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "NE", "ADV", "PPER", "ART", "ADJA", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "F\u00fcr dein gro\u00dfes Volk, unter Greisen jung,", "tokens": ["F\u00fcr", "dein", "gro\u00b7\u00dfes", "Volk", ",", "un\u00b7ter", "Grei\u00b7sen", "jung", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,", "APPR", "NN", "ADJD", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Gr\u00fcn wie seines Waldes gr\u00fcnstes Eichenholz.", "tokens": ["Gr\u00fcn", "wie", "sei\u00b7nes", "Wal\u00b7des", "gr\u00fcns\u00b7tes", "Ei\u00b7chen\u00b7holz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.7": {"line.1": {"text": "Nicht also mit dir! Rausche durch den Wald!", "tokens": ["Nicht", "al\u00b7so", "mit", "dir", "!", "Rau\u00b7sche", "durch", "den", "Wald", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPR", "PPER", "$.", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Rausche, brause, Zorn, durch Stein und Bein!", "tokens": ["Rau\u00b7sche", ",", "brau\u00b7se", ",", "Zorn", ",", "durch", "Stein", "und", "Bein", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "NN", "$,", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Brause, deutscher Mut, Gottes Zorngewalt!", "tokens": ["Brau\u00b7se", ",", "deut\u00b7scher", "Mut", ",", "Got\u00b7tes", "Zorn\u00b7ge\u00b7walt", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,", "NN", "NN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Greif die Adler dir, la\u00df die Kr\u00e4hen schrein.", "tokens": ["Greif", "die", "Ad\u00b7ler", "dir", ",", "la\u00df", "die", "Kr\u00e4\u00b7hen", "schrein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "PPER", "$,", "VVIMP", "ART", "NN", "PTKVZ", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}}}}}