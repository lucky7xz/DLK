{"textgrid.poem.56191": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "4", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Denke, denke mein Geliebter,", "tokens": ["Den\u00b7ke", ",", "den\u00b7ke", "mein", "Ge\u00b7lieb\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meiner alten Lieb und Treue,", "tokens": ["Mei\u00b7ner", "al\u00b7ten", "Lieb", "und", "Treu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Denke, wie aus freud'gem Herzen,", "tokens": ["Den\u00b7ke", ",", "wie", "aus", "freu\u00b7d'\u00b7gem", "Her\u00b7zen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PWAV", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Sonder Harm und sonder Reue,", "tokens": ["Son\u00b7der", "Harm", "und", "son\u00b7der", "Reu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Frei das Wort ich dir gegeben,", "tokens": ["Frei", "das", "Wort", "ich", "dir", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PPER", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dich zu lieben, dir zu leben \u2013", "tokens": ["Dich", "zu", "lie\u00b7ben", ",", "dir", "zu", "le\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "PPER", "PTKZU", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Suche dir ein andres Lieb!", "tokens": ["Su\u00b7che", "dir", "ein", "and\u00b7res", "Lieb", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ach! er kam, besah die Felder", "tokens": ["Ach", "!", "er", "kam", ",", "be\u00b7sah", "die", "Fel\u00b7der"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "$.", "PPER", "VVFIN", "$,", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und das Haus, der Mutter Erbe,", "tokens": ["Und", "das", "Haus", ",", "der", "Mut\u00b7ter", "Er\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sprach und feilschte mit dem Vater", "tokens": ["Sprach", "und", "feilschte", "mit", "dem", "Va\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Der befahl gestreng und herbe. \u2013", "tokens": ["Der", "be\u00b7fahl", "ge\u00b7streng", "und", "her\u00b7be", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "KON", "VVFIN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Eitel war das Wort gesprochen,", "tokens": ["Ei\u00b7tel", "war", "das", "Wort", "ge\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Herz und Treue sind gebrochen \u2013", "tokens": ["Herz", "und", "Treu\u00b7e", "sind", "ge\u00b7bro\u00b7chen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Suche dir ein andres Lieb!", "tokens": ["Su\u00b7che", "dir", "ein", "and\u00b7res", "Lieb", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und der Priester mit dem Munde", "tokens": ["Und", "der", "Pries\u00b7ter", "mit", "dem", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sprach den Segen unverdrossen,", "tokens": ["Sprach", "den", "Se\u00b7gen", "un\u00b7ver\u00b7dros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Unerh\u00f6ret, einem Bunde,", "tokens": ["Un\u00b7er\u00b7h\u00f6\u00b7ret", ",", "ei\u00b7nem", "Bun\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der im Himmel nicht geschlossen. \u2013", "tokens": ["Der", "im", "Him\u00b7mel", "nicht", "ge\u00b7schlos\u00b7sen", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "APPRART", "NN", "PTKNEG", "VVPP", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Zieh von hinnen! zieh von hinnen!", "tokens": ["Zieh", "von", "hin\u00b7nen", "!", "zieh", "von", "hin\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADV", "$.", "VVFIN", "APPR", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Andres Gl\u00fcck dir zu gewinnen,", "tokens": ["And\u00b7res", "Gl\u00fcck", "dir", "zu", "ge\u00b7win\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Suche dir ein andres Lieb!", "tokens": ["Su\u00b7che", "dir", "ein", "and\u00b7res", "Lieb", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}