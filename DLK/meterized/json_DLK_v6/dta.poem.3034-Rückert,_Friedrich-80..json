{"dta.poem.3034": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "80.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Ewigkeit umfa\u00dft die Ewigkeit allein;", "tokens": ["Die", "E\u00b7wig\u00b7keit", "um\u00b7fa\u00dft", "die", "E\u00b7wig\u00b7keit", "al\u00b7lein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was in dir Ew'ges denkt, das mu\u00df unsterblich seyn.", "tokens": ["Was", "in", "dir", "Ew'\u00b7ges", "denkt", ",", "das", "mu\u00df", "uns\u00b7terb\u00b7lich", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPER", "ADJA", "VVFIN", "$,", "PDS", "VMFIN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Unsterblichkeitsgef\u00fchl im Menschen war erwacht,", "tokens": ["U\u00b7nsterb\u00b7lich\u00b7keits\u00b7ge\u00b7f\u00fchl", "im", "Men\u00b7schen", "war", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VAFIN", "VVPP", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Sobald nur seinen Gott unsterblich er gedacht.", "tokens": ["So\u00b7bald", "nur", "sei\u00b7nen", "Gott", "uns\u00b7terb\u00b7lich", "er", "ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Mocht' er im Gegensatz zum Gott sich sterblich nennen,", "tokens": ["Mocht'", "er", "im", "Ge\u00b7gen\u00b7satz", "zum", "Gott", "sich", "sterb\u00b7lich", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPRART", "NN", "APPRART", "NN", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sein eignes G\u00f6ttliches konnt' er vom Gott nicht trennen.", "tokens": ["Sein", "eig\u00b7nes", "G\u00f6tt\u00b7li\u00b7ches", "konnt'", "er", "vom", "Gott", "nicht", "tren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VMFIN", "PPER", "APPRART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+---+-+-", "measure": "unknown.measure.penta"}}, "stanza.4": {"line.1": {"text": "Doch als den G\u00f6ttern er Gestalt und Leib gegeben,", "tokens": ["Doch", "als", "den", "G\u00f6t\u00b7tern", "er", "Ge\u00b7stalt", "und", "Leib", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PPER", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu Menschen sie gemacht, die nur viel l\u00e4nger leben;", "tokens": ["Zu", "Men\u00b7schen", "sie", "ge\u00b7macht", ",", "die", "nur", "viel", "l\u00e4n\u00b7ger", "le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "VVPP", "$,", "PRELS", "ADV", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Da war Unsterblichkeitsgef\u00fchl ihm selbst entschwunden,", "tokens": ["Da", "war", "U\u00b7nsterb\u00b7lich\u00b7keits\u00b7ge\u00b7f\u00fchl", "ihm", "selbst", "ent\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "PPER", "ADV", "VVINF", "$,"], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Mit k\u00f6rperlosem Gott erst wieder klar empfunden.", "tokens": ["Mit", "k\u00f6r\u00b7per\u00b7lo\u00b7sem", "Gott", "erst", "wie\u00b7der", "klar", "emp\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}