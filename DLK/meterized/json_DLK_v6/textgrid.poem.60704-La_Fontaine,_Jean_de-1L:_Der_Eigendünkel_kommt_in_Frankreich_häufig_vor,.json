{"textgrid.poem.60704": {"metadata": {"author": {"name": "La Fontaine, Jean de", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Eigend\u00fcnkel kommt in Frankreich h\u00e4ufig vor,", "genre": "verse", "period": "N.A.", "pub_year": 1658, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Eigend\u00fcnkel kommt in Frankreich h\u00e4ufig vor,", "tokens": ["Der", "Ei\u00b7gen\u00b7d\u00fcn\u00b7kel", "kommt", "in", "Fran\u00b7kreich", "h\u00e4u\u00b7fig", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NE", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Man spielt den einflu\u00dfreichen Mann", "tokens": ["Man", "spielt", "den", "ein\u00b7flu\u00df\u00b7rei\u00b7chen", "Mann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und ist doch nur ein B\u00fcrger, nur ein Tor,", "tokens": ["Und", "ist", "doch", "nur", "ein", "B\u00fcr\u00b7ger", ",", "nur", "ein", "Tor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "ART", "NN", "$,", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der seiner Eitelkeit nicht widerstehen kann.", "tokens": ["Der", "sei\u00b7ner", "Ei\u00b7tel\u00b7keit", "nicht", "wi\u00b7der\u00b7ste\u00b7hen", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Eitelkeit ist auch dem Spanier eigen,", "tokens": ["Die", "Ei\u00b7tel\u00b7keit", "ist", "auch", "dem", "Spa\u00b7nier", "ei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Doch pflegt er sie nicht so zu zeigen;", "tokens": ["Doch", "pflegt", "er", "sie", "nicht", "so", "zu", "zei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "PTKNEG", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Sein D\u00fcnkel scheint mir n\u00e4rrischer zu sein,", "tokens": ["Sein", "D\u00fcn\u00b7kel", "scheint", "mir", "n\u00e4r\u00b7ri\u00b7scher", "zu", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADJD", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Doch nicht so dumm und nicht so kindisch klein.", "tokens": ["Doch", "nicht", "so", "dumm", "und", "nicht", "so", "kin\u00b7disch", "klein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADV", "ADJD", "KON", "PTKNEG", "ADV", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Den unsrigen soll euch ein Beispiel geben,", "tokens": ["Den", "uns\u00b7ri\u00b7gen", "soll", "euch", "ein", "Bei\u00b7spiel", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.10": {"text": "Das ich mit Flei\u00df gezeichnet nach dem Leben.", "tokens": ["Das", "ich", "mit", "Flei\u00df", "ge\u00b7zeich\u00b7net", "nach", "dem", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "APPR", "NN", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die allerkleinste Ratte sah einmal", "tokens": ["Die", "al\u00b7ler\u00b7kleins\u00b7te", "Rat\u00b7te", "sah", "ein\u00b7mal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Den allergr\u00f6\u00dften Elefanten gehen,", "tokens": ["Den", "al\u00b7ler\u00b7gr\u00f6\u00df\u00b7ten", "E\u00b7lef\u00b7an\u00b7ten", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und h\u00f6hnte laut, es w\u00e4re ein Skandal,", "tokens": ["Und", "h\u00f6hn\u00b7te", "laut", ",", "es", "w\u00e4\u00b7re", "ein", "Skan\u00b7dal", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$,", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Den Schneckengang des Riesen anzusehen.", "tokens": ["Den", "Schne\u00b7cken\u00b7gang", "des", "Rie\u00b7sen", "an\u00b7zu\u00b7se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Der Elefant war feierlich geschm\u00fcckt,", "tokens": ["Der", "E\u00b7le\u00b7fant", "war", "fei\u00b7er\u00b7lich", "ge\u00b7schm\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und lang und breit mit schwerer Last begl\u00fcckt:", "tokens": ["Und", "lang", "und", "breit", "mit", "schwe\u00b7rer", "Last", "be\u00b7gl\u00fcckt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "ADJD", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Des Sultans edles Weib", "tokens": ["Des", "Sul\u00b7tans", "ed\u00b7les", "Weib"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Nebst ihrem liebsten Zeitvertreib,", "tokens": ["Nebst", "ih\u00b7rem", "liebs\u00b7ten", "Zeit\u00b7ver\u00b7treib", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Dem H\u00fcndchen, \u00c4ffchen und der Katz,", "tokens": ["Dem", "H\u00fcnd\u00b7chen", ",", "\u00c4ff\u00b7chen", "und", "der", "Katz", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Der Magd und allem Putz und Tand,", "tokens": ["Der", "Magd", "und", "al\u00b7lem", "Putz", "und", "Tand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PIS", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Ja selbst dem Papagei noch auf der Hand,", "tokens": ["Ja", "selbst", "dem", "Pa\u00b7pa\u00b7gei", "noch", "auf", "der", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "ART", "NN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Fand auf des Tieres breitem R\u00fccken Platz", "tokens": ["Fand", "auf", "des", "Tie\u00b7res", "brei\u00b7tem", "R\u00fc\u00b7cken", "Platz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Zu einer fernen Pilgerreise.", "tokens": ["Zu", "ei\u00b7ner", "fer\u00b7nen", "Pil\u00b7ger\u00b7rei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Die Ratte h\u00f6hnte die dem\u00fctige Weise,", "tokens": ["Die", "Rat\u00b7te", "h\u00f6hn\u00b7te", "die", "de\u00b7m\u00fc\u00b7ti\u00b7ge", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "Mit der das Volk zur Seite wich.", "tokens": ["Mit", "der", "das", "Volk", "zur", "Sei\u00b7te", "wich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "\u00bbes scheint,\u00ab rief sie, \u00bbman achtet den am meisten,", "tokens": ["\u00bb", "es", "scheint", ",", "\u00ab", "rief", "sie", ",", "\u00bb", "man", "ach\u00b7tet", "den", "am", "meis\u00b7ten", ","], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "$,", "$(", "VVFIN", "PPER", "$,", "$(", "PIS", "VVFIN", "ART", "PTKA", "PIS", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Der r\u00fccksichtslos f\u00fcr sich", "tokens": ["Der", "r\u00fcck\u00b7sichts\u00b7los", "f\u00fcr", "sich"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJD", "APPR", "PRF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.18": {"text": "Den gr\u00f6\u00dften Raum in Anspruch nimmt.", "tokens": ["Den", "gr\u00f6\u00df\u00b7ten", "Raum", "in", "An\u00b7spruch", "nimmt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Ist's denn die Masse, die den Wert bestimmt?", "tokens": ["Ist's", "denn", "die", "Mas\u00b7se", ",", "die", "den", "Wert", "be\u00b7stimmt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Kommt's mehr nicht darauf an, etwas zu leisten?", "tokens": ["Kommt's", "mehr", "nicht", "da\u00b7rauf", "an", ",", "et\u00b7was", "zu", "leis\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PTKNEG", "PAV", "PTKVZ", "$,", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.21": {"text": "Den Elefanten f\u00fcrchtet jedes Kind,", "tokens": ["Den", "E\u00b7lef\u00b7an\u00b7ten", "f\u00fcrch\u00b7tet", "je\u00b7des", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Doch wir, die wir viel kleiner sind,", "tokens": ["Doch", "wir", ",", "die", "wir", "viel", "klei\u00b7ner", "sind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PRELS", "PPER", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Wir stehn an Kraftbewu\u00dftsein ihm nicht nach!\u00ab", "tokens": ["Wir", "stehn", "an", "Kraft\u00b7be\u00b7wu\u00df\u00b7tsein", "ihm", "nicht", "nach", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "PPER", "PTKNEG", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Das war es, was die tief emp\u00f6rte Ratte sprach.", "tokens": ["Das", "war", "es", ",", "was", "die", "tief", "em\u00b7p\u00f6r\u00b7te", "Rat\u00b7te", "sprach", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "$,", "PRELS", "ART", "ADJD", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Sie h\u00e4tte wohl noch mehr gesprochen,", "tokens": ["Sie", "h\u00e4t\u00b7te", "wohl", "noch", "mehr", "ge\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Doch eine Katze, die herbeigekrochen,", "tokens": ["Doch", "ei\u00b7ne", "Kat\u00b7ze", ",", "die", "her\u00b7bei\u00b7ge\u00b7kro\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Bewies der eitlen Kleinen kurzerhand,", "tokens": ["Be\u00b7wies", "der", "eit\u00b7len", "Klei\u00b7nen", "kur\u00b7zer\u00b7hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Da\u00df eine Ratte schw\u00e4cher als ein Elefant.", "tokens": ["Da\u00df", "ei\u00b7ne", "Rat\u00b7te", "schw\u00e4\u00b7cher", "als", "ein", "E\u00b7le\u00b7fant", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}