{"textgrid.poem.25045": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ihr fragt, was ewig aufs neue", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr fragt, was ewig aufs neue", "tokens": ["Ihr", "fragt", ",", "was", "e\u00b7wig", "aufs", "neu\u00b7e"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "ADJD", "APPRART", "ADJA"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Zu ihr zur\u00fcck mich zieht:", "tokens": ["Zu", "ihr", "zu\u00b7r\u00fcck", "mich", "zieht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKVZ", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ist's ihres Auges Bl\u00e4ue?", "tokens": ["Ist's", "ih\u00b7res", "Au\u00b7ges", "Bl\u00e4u\u00b7e", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Lippe Zauberlied?", "tokens": ["Der", "Lip\u00b7pe", "Zau\u00b7berl\u00b7ied", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Fragt, wer dem Schmetterlinge", "tokens": ["Fragt", ",", "wer", "dem", "Schmet\u00b7ter\u00b7lin\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "PWS", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den Weg um die Rose weist,", "tokens": ["Den", "Weg", "um", "die", "Ro\u00b7se", "weist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da\u00df er mit flatternder Schwinge", "tokens": ["Da\u00df", "er", "mit", "flat\u00b7tern\u00b7der", "Schwin\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Den duftenden Kelch umkreist!", "tokens": ["Den", "duf\u00b7ten\u00b7den", "Kelch", "um\u00b7kreist", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Fragt, wer die brandende Welle", "tokens": ["Fragt", ",", "wer", "die", "bran\u00b7den\u00b7de", "Wel\u00b7le"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PWS", "ART", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Den Meerpfad kennen lehrt,", "tokens": ["Den", "Me\u00b7er\u00b7pfad", "ken\u00b7nen", "lehrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Da\u00df stets zu der Uferstelle,", "tokens": ["Da\u00df", "stets", "zu", "der", "Uf\u00b7er\u00b7stel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Der teuren, sie wiederkehrt!", "tokens": ["Der", "teu\u00b7ren", ",", "sie", "wie\u00b7der\u00b7kehrt", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PPER", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Wie's in den Sternen geschrieben,", "tokens": ["Wie's", "in", "den", "Ster\u00b7nen", "ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Werden sie unbewu\u00dft", "tokens": ["Wer\u00b7den", "sie", "un\u00b7be\u00b7wu\u00dft"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "PPER", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zur Rose, zur K\u00fcste getrieben,", "tokens": ["Zur", "Ro\u00b7se", ",", "zur", "K\u00fcs\u00b7te", "ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und ich an ihre Brust.", "tokens": ["Und", "ich", "an", "ih\u00b7re", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}