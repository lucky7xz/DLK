{"textgrid.poem.44417": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[leb wohl, o Weimar, gutes Land]", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Leb wohl, o Weimar, gutes Land,", "tokens": ["Leb", "wohl", ",", "o", "Wei\u00b7mar", ",", "gu\u00b7tes", "Land", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "FM", "NE", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Pferde sind zur Fahrt gespannt,", "tokens": ["Die", "Pfer\u00b7de", "sind", "zur", "Fahrt", "ge\u00b7spannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Fort mu\u00df ich, heimatw\u00e4rts.", "tokens": ["Fort", "mu\u00df", "ich", ",", "hei\u00b7mat\u00b7w\u00e4rts", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "$,", "ADV", "$."], "meter": "+-+++-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Noch einmal, eh es ganz entschwand,", "tokens": ["Noch", "ein\u00b7mal", ",", "eh", "es", "ganz", "ent\u00b7schwand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Begr\u00fc\u00dft dich winkend diese Hand", "tokens": ["Be\u00b7gr\u00fc\u00dft", "dich", "win\u00b7kend", "die\u00b7se", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und klopfend dieses Herz.", "tokens": ["Und", "klop\u00b7fend", "die\u00b7ses", "Herz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "...", "tokens": ["..."], "token_info": ["punct"], "pos": ["$("]}}}}}