{"textgrid.poem.66843": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Graue Gespenster verd\u00e4mmernder Zeiten!", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Graue Gespenster verd\u00e4mmernder Zeiten!", "tokens": ["Grau\u00b7e", "Ge\u00b7spens\u00b7ter", "ver\u00b7d\u00e4m\u00b7mern\u00b7der", "Zei\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Schwellende Sch\u00f6ne der gr\u00fc\u00dfenden Welt!", "tokens": ["Schwel\u00b7len\u00b7de", "Sch\u00f6\u00b7ne", "der", "gr\u00fc\u00b7\u00dfen\u00b7den", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "N\u00e4her und n\u00e4her seh ich sie schreiten,", "tokens": ["N\u00e4\u00b7her", "und", "n\u00e4\u00b7her", "seh", "ich", "sie", "schrei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VVFIN", "PPER", "PPER", "VVFIN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Die das Gewaltreich der Knechtschaft zerschellt.", "tokens": ["Die", "das", "Ge\u00b7walt\u00b7reich", "der", "Knecht\u00b7schaft", "zer\u00b7schellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.5": {"text": "Tief im Moraste des Mammons verloren", "tokens": ["Tief", "im", "Mo\u00b7ras\u00b7te", "des", "Mam\u00b7mons", "ver\u00b7lo\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPRART", "NN", "ART", "NN", "VVPP"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.6": {"text": "Ging uns des Lebens begl\u00fcckender Mut,", "tokens": ["Ging", "uns", "des", "Le\u00b7bens", "be\u00b7gl\u00fc\u00b7cken\u00b7der", "Mut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.7": {"text": "Bl\u00fchende Anmut der Liebe geboren", "tokens": ["Bl\u00fc\u00b7hen\u00b7de", "An\u00b7mut", "der", "Lie\u00b7be", "ge\u00b7bo\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "ART", "NN", "VVPP"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.8": {"text": "Wird aus der Freiheit erl\u00f6sender Flut.", "tokens": ["Wird", "aus", "der", "Frei\u00b7heit", "er\u00b7l\u00f6\u00b7sen\u00b7der", "Flut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.2": {"line.1": {"text": "Massen, die hungernd im Elend geschmachtet,", "tokens": ["Mas\u00b7sen", ",", "die", "hun\u00b7gernd", "im", "E\u00b7lend", "ge\u00b7schmach\u00b7tet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADJD", "APPRART", "NN", "VVPP", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Wimmernd im Frondienst der Not sich gem\u00fcht,", "tokens": ["Wim\u00b7mernd", "im", "Fron\u00b7di\u00b7enst", "der", "Not", "sich", "ge\u00b7m\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "ART", "NN", "PRF", "VVPP", "$,"], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Von ihren Peinigern schamlos verachtet,", "tokens": ["Von", "ih\u00b7ren", "Pei\u00b7ni\u00b7gern", "scham\u00b7los", "ver\u00b7ach\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Welk und gebrochen, noch eh sie gebl\u00fcht.", "tokens": ["Welk", "und", "ge\u00b7bro\u00b7chen", ",", "noch", "eh", "sie", "ge\u00b7bl\u00fcht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVPP", "$,", "ADV", "KOUS", "PPER", "VVPP", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.5": {"text": "Freudeberaubt und versto\u00dfen vom Sch\u00f6nen,", "tokens": ["Freu\u00b7de\u00b7be\u00b7raubt", "und", "ver\u00b7sto\u00b7\u00dfen", "vom", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "APPRART", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.6": {"text": "Fern von den H\u00f6hen des Lebens verbannt,", "tokens": ["Fern", "von", "den", "H\u00f6\u00b7hen", "des", "Le\u00b7bens", "ver\u00b7bannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.7": {"text": "M\u00fctter mit T\u00f6chtern und V\u00e4ter mit S\u00f6hnen", "tokens": ["M\u00fct\u00b7ter", "mit", "T\u00f6ch\u00b7tern", "und", "V\u00e4\u00b7ter", "mit", "S\u00f6h\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "KON", "NN", "APPR", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.8": {"text": "Eisern ins Joch der Entbehrung gespannt.", "tokens": ["Ei\u00b7sern", "ins", "Joch", "der", "Ent\u00b7beh\u00b7rung", "ge\u00b7spannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "NN", "VVPP", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.3": {"line.1": {"text": "An die Maschine zeitlebens geschmiedet,", "tokens": ["An", "die", "Ma\u00b7schi\u00b7ne", "zeit\u00b7le\u00b7bens", "ge\u00b7schmie\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Beute dem nimmersatten Kolo\u00df,", "tokens": ["Beu\u00b7te", "dem", "nim\u00b7mer\u00b7sat\u00b7ten", "Ko\u00b7lo\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "W\u00fctend umschmettert, umzischt und umsiedet,", "tokens": ["W\u00fc\u00b7tend", "um\u00b7schmet\u00b7tert", ",", "um\u00b7zischt", "und", "um\u00b7sie\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Keuchten sie dumpf, ein betrogener Tro\u00df.", "tokens": ["Keuch\u00b7ten", "sie", "dumpf", ",", "ein", "be\u00b7tro\u00b7ge\u00b7ner", "Tro\u00df", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$,", "ART", "ADJA", "NN", "$."], "meter": "+--++-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "Darbend erzeugten sie drohende F\u00fclle,", "tokens": ["Dar\u00b7bend", "er\u00b7zeug\u00b7ten", "sie", "dro\u00b7hen\u00b7de", "F\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.6": {"text": "Weh! und von \u00dcberflu\u00df strotzte die Welt \u2013", "tokens": ["Weh", "!", "und", "von", "\u00dc\u00b7berf\u00b7lu\u00df", "strotz\u00b7te", "die", "Welt", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "KON", "APPR", "NN", "VVFIN", "ART", "NN", "$("], "meter": "+--+-++--+", "measure": "iambic.penta.invert"}, "line.7": {"text": "Aber ein \u00fcbergewaltiger Wille", "tokens": ["A\u00b7ber", "ein", "\u00fc\u00b7ber\u00b7ge\u00b7wal\u00b7ti\u00b7ger", "Wil\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.8": {"text": "Schneidet die Klauen dem gierigen Geld.", "tokens": ["Schnei\u00b7det", "die", "Klau\u00b7en", "dem", "gie\u00b7ri\u00b7gen", "Geld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.4": {"line.1": {"text": "H\u00f6rt ihr die Fl\u00fcsse, die Meerwasser brausen?", "tokens": ["H\u00f6rt", "ihr", "die", "Fl\u00fcs\u00b7se", ",", "die", "Meer\u00b7was\u00b7ser", "brau\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Seht die geschmeidigen Metalle geh\u00e4uft?", "tokens": ["Seht", "die", "ge\u00b7schmei\u00b7di\u00b7gen", "Me\u00b7tal\u00b7le", "ge\u00b7h\u00e4uft", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Sp\u00fcrt die elektrischen Funken ihr sausen?", "tokens": ["Sp\u00fcrt", "die", "e\u00b7lekt\u00b7ri\u00b7schen", "Fun\u00b7ken", "ihr", "sau\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "F\u00fchlt, wie der Segen des Weltalls tr\u00e4uft?", "tokens": ["F\u00fchlt", ",", "wie", "der", "Se\u00b7gen", "des", "Welt\u00b7alls", "tr\u00e4uft", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Hilfreiche Kr\u00e4fte zermalmen den Schrecken,", "tokens": ["Hilf\u00b7rei\u00b7che", "Kr\u00e4f\u00b7te", "zer\u00b7mal\u00b7men", "den", "Schre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.6": {"text": "Fluch der Vergangenheitsarbeit verdr\u00f6hnt,", "tokens": ["Fluch", "der", "Ver\u00b7gan\u00b7gen\u00b7heits\u00b7ar\u00b7beit", "ver\u00b7dr\u00f6hnt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVPP", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.7": {"text": "Wonne wird unser Schaffen erwecken,", "tokens": ["Won\u00b7ne", "wird", "un\u00b7ser", "Schaf\u00b7fen", "er\u00b7we\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Freude, die liebliche, l\u00e4chelt vers\u00f6hnt.", "tokens": ["Freu\u00b7de", ",", "die", "lieb\u00b7li\u00b7che", ",", "l\u00e4\u00b7chelt", "ver\u00b7s\u00f6hnt", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "ADJA", "$,", "VVFIN", "VVPP", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}}}}