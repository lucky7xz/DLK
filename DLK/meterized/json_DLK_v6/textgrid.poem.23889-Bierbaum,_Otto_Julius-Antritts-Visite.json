{"textgrid.poem.23889": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Antritts-Visite", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Welch Gegl\u00f6ckel, welch Gebimmel", "tokens": ["Welch", "Ge\u00b7gl\u00f6\u00b7ckel", ",", "welch", "Ge\u00b7bim\u00b7mel"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PIAT", "NN", "$,", "PWAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Klingelt meinen Berg heran?", "tokens": ["Klin\u00b7gelt", "mei\u00b7nen", "Berg", "he\u00b7ran", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kommt der brave Schellenschimmel", "tokens": ["Kommt", "der", "bra\u00b7ve", "Schel\u00b7len\u00b7schim\u00b7mel"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jener guten Fee Morgan?", "tokens": ["Je\u00b7ner", "gu\u00b7ten", "Fee", "Mor\u00b7gan", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und der Himmel! Nein, der Himmel!", "tokens": ["Und", "der", "Him\u00b7mel", "!", "Nein", ",", "der", "Him\u00b7mel", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$.", "PTKANT", "$,", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Seht doch nur den Himmel an!", "tokens": ["Seht", "doch", "nur", "den", "Him\u00b7mel", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "War er grau nicht noch soeben?", "tokens": ["War", "er", "grau", "nicht", "noch", "soe\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "PTKNEG", "ADV", "VVINF", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Und jetzt ist er gl\u00fch und klar!", "tokens": ["Und", "jetzt", "ist", "er", "gl\u00fch", "und", "klar", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sollt es heut noch Wunder geben?", "tokens": ["Sollt", "es", "heut", "noch", "Wun\u00b7der", "ge\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nein, das ist nicht wunderbar:", "tokens": ["Nein", ",", "das", "ist", "nicht", "wun\u00b7der\u00b7bar", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PDS", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Durch die schwanken jungen Reben", "tokens": ["Durch", "die", "schwan\u00b7ken", "jun\u00b7gen", "Re\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Kommt ein junges Ehepaar.", "tokens": ["Kommt", "ein", "jun\u00b7ges", "E\u00b7he\u00b7paar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}