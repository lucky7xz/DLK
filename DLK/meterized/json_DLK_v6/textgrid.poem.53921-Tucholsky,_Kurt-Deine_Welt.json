{"textgrid.poem.53921": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Deine Welt", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Trudele dahin! Verkehre bei Ingenieuren!", "tokens": ["Tru\u00b7de\u00b7le", "da\u00b7hin", "!", "Ver\u00b7keh\u00b7re", "bei", "In\u00b7ge\u00b7ni\u00b7eu\u00b7ren", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PAV", "$.", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "La\u00df dich als Redakteur von Staatsanw\u00e4lten verh\u00f6ren!", "tokens": ["La\u00df", "dich", "als", "Re\u00b7dak\u00b7teur", "von", "Staats\u00b7an\u00b7w\u00e4l\u00b7ten", "ver\u00b7h\u00f6\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "KOUS", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Sei eingeladen bei Snobs, die wichtigtuende Diplomaten", "tokens": ["Sei", "ein\u00b7ge\u00b7la\u00b7den", "bei", "Snobs", ",", "die", "wich\u00b7tig\u00b7tu\u00b7en\u00b7de", "Dip\u00b7lo\u00b7ma\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "VVPP", "APPR", "NE", "$,", "ART", "ADJA", "NN"], "meter": "-+-+--+-+-+--+-+-", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "schnurrend umschleichen, besonders die aus den kleinern Staaten!", "tokens": ["schnur\u00b7rend", "um\u00b7schlei\u00b7chen", ",", "be\u00b7son\u00b7ders", "die", "aus", "den", "klei\u00b7nern", "Staa\u00b7ten", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "$,", "ADV", "ART", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+-+-+-", "measure": "elegiambus"}, "line.5": {"text": "Entflieh der Familie! Rutsch die soziale Leiter hinauf und hinab \u2013:", "tokens": ["Ent\u00b7flieh", "der", "Fa\u00b7mi\u00b7lie", "!", "Rutsch", "die", "so\u00b7zi\u00b7a\u00b7le", "Lei\u00b7ter", "hin\u00b7auf", "und", "hin\u00b7ab", "\u2013", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ART", "NN", "$.", "NE", "ART", "ADJA", "NN", "PTKVZ", "KON", "ADV", "$(", "$."], "meter": "---+--+--+--+--+--+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "es spielt sich alles unter zweihundert Menschen ab.", "tokens": ["es", "spielt", "sich", "al\u00b7les", "un\u00b7ter", "zwei\u00b7hun\u00b7dert", "Men\u00b7schen", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PIS", "APPR", "CARD", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Wohn an der Weser, der Oder, der Weichsel, der Elbe \u2013", "tokens": ["Wohn", "an", "der", "We\u00b7ser", ",", "der", "O\u00b7der", ",", "der", "Weich\u00b7sel", ",", "der", "El\u00b7be", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "ART", "NE", "$,", "ART", "NN", "$,", "ART", "NE", "$("], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}, "line.2": {"text": "deine Gesellschaft bleibt immer, immer dieselbe.", "tokens": ["dei\u00b7ne", "Ge\u00b7sell\u00b7schaft", "bleibt", "im\u00b7mer", ",", "im\u00b7mer", "die\u00b7sel\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "$,", "ADV", "PDAT", "$."], "meter": "+--+--+-+--+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Immer dieselben Fahrt- und Leidensgenossen,", "tokens": ["Im\u00b7mer", "die\u00b7sel\u00b7ben", "Fahr\u00b7t", "und", "Lei\u00b7dens\u00b7ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "TRUNC", "KON", "NN", "$,"], "meter": "+--+-+--+--+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "wie mit Gittern sind dir die andern G\u00e4rten verschlossen.", "tokens": ["wie", "mit", "Git\u00b7tern", "sind", "dir", "die", "an\u00b7dern", "G\u00e4r\u00b7ten", "ver\u00b7schlos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "VAFIN", "PPER", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Freunde sind Schicksal, aber nicht zu knapp.", "tokens": ["Freun\u00b7de", "sind", "Schick\u00b7sal", ",", "a\u00b7ber", "nicht", "zu", "knapp", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "$,", "ADV", "PTKNEG", "PTKA", "ADJD", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.6": {"text": "Es spielt sich alles unter zweihundert Menschen ab.", "tokens": ["Es", "spielt", "sich", "al\u00b7les", "un\u00b7ter", "zwei\u00b7hun\u00b7dert", "Men\u00b7schen", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PIS", "APPR", "CARD", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Fahr nach Amerika! Wer steht im Hotel auf den Herrentoiletten?", "tokens": ["Fahr", "nach", "A\u00b7me\u00b7ri\u00b7ka", "!", "Wer", "steht", "im", "Ho\u00b7tel", "auf", "den", "Her\u00b7ren\u00b7to\u00b7i\u00b7let\u00b7ten", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "$.", "PWS", "VVFIN", "APPRART", "NN", "APPR", "ART", "NN", "$."], "meter": "+----+-+-+-+-+-+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Rosenfeld. Und er spricht: \u00bbWas machen Sie in Manhattan?\u00ab", "tokens": ["Ro\u00b7sen\u00b7feld", ".", "Und", "er", "spricht", ":", "\u00bb", "Was", "ma\u00b7chen", "Sie", "in", "Man\u00b7hat\u00b7tan", "?", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "$.", "KON", "PPER", "VVFIN", "$.", "$(", "PWS", "VVFIN", "PPER", "APPR", "NE", "$.", "$("], "meter": "+-+--+-+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Flieh zu den Eskimos, in des Eises kreischende Masse:", "tokens": ["Flieh", "zu", "den", "Es\u00b7ki\u00b7mos", ",", "in", "des", "Ei\u00b7ses", "krei\u00b7schen\u00b7de", "Mas\u00b7se", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "--+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "der Dicke im Pelz ist bestimmt ein Kind deiner Klasse.", "tokens": ["der", "Di\u00b7cke", "im", "Pelz", "ist", "be\u00b7stimmt", "ein", "Kind", "dei\u00b7ner", "Klas\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VAFIN", "VVPP", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+--+--+-+--+-", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Jag durch die Welt vom n\u00f6rdlichen bis zum s\u00fcdlichen Kap \u2013:", "tokens": ["Jag", "durch", "die", "Welt", "vom", "n\u00f6rd\u00b7li\u00b7chen", "bis", "zum", "s\u00fcd\u00b7li\u00b7chen", "Kap", "\u2013", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "APPR", "ART", "NN", "APPRART", "ADJA", "APPR", "APPRART", "ADJA", "NN", "$(", "$."], "meter": "-+-+-+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "es spielt sich alles unter zweihundert Menschen ab.", "tokens": ["es", "spielt", "sich", "al\u00b7les", "un\u00b7ter", "zwei\u00b7hun\u00b7dert", "Men\u00b7schen", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PIS", "APPR", "CARD", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Unsere Welt ist so klein. Dies mu\u00dft du wissen:", "tokens": ["Un\u00b7se\u00b7re", "Welt", "ist", "so", "klein", ".", "Dies", "mu\u00dft", "du", "wis\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "$.", "PDS", "VMFIN", "PPER", "VVINF", "$."], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Ganze Klassen und V\u00f6lker sind nur deines Lebens Kulissen;", "tokens": ["Gan\u00b7ze", "Klas\u00b7sen", "und", "V\u00f6l\u00b7ker", "sind", "nur", "dei\u00b7nes", "Le\u00b7bens", "Ku\u00b7lis\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "VAFIN", "ADV", "PPOSAT", "NN", "NN", "$."], "meter": "+-+--+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "du wei\u00dft, da\u00df sie sind. Aber sei nicht verwundert:", "tokens": ["du", "wei\u00dft", ",", "da\u00df", "sie", "sind", ".", "A\u00b7ber", "sei", "nicht", "ver\u00b7wun\u00b7dert", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "VAFIN", "$.", "KON", "VAFIN", "PTKNEG", "VVFIN", "$."], "meter": "-+---+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "du lebst ja doch nur inmitten deiner zweihundert.", "tokens": ["du", "lebst", "ja", "doch", "nur", "in\u00b7mit\u00b7ten", "dei\u00b7ner", "zwei\u00b7hun\u00b7dert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADV", "APPR", "PPOSAT", "CARD", "$."], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.5": {"text": "Und h\u00f6rst du auch fremde L\u00e4nder und Kontinente erklingen:", "tokens": ["Und", "h\u00f6rst", "du", "auch", "frem\u00b7de", "L\u00e4n\u00b7der", "und", "Kon\u00b7ti\u00b7nen\u00b7te", "er\u00b7klin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADJA", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+--+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "du kannst ja gar nicht aus deinem Kreise springen!", "tokens": ["du", "kannst", "ja", "gar", "nicht", "aus", "dei\u00b7nem", "Krei\u00b7se", "sprin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "PTKNEG", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.7": {"text": "Von Stund an, wo sie dich pudern, bis zum gemieteten Grab", "tokens": ["Von", "Stund", "an", ",", "wo", "sie", "dich", "pu\u00b7dern", ",", "bis", "zum", "ge\u00b7mie\u00b7te\u00b7ten", "Grab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "PTKVZ", "$,", "PWAV", "PPER", "PRF", "VVINF", "$,", "KOUS", "APPRART", "ADJA", "NN"], "meter": "-+-+--+----+--+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "spielt sich alles und alles und alles unter zweihundert Menschen ab.", "tokens": ["spielt", "sich", "al\u00b7les", "und", "al\u00b7les", "und", "al\u00b7les", "un\u00b7ter", "zwei\u00b7hun\u00b7dert", "Men\u00b7schen", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PIS", "KON", "PIS", "KON", "PIS", "APPR", "CARD", "NN", "PTKVZ", "$."], "meter": "+-+--+--+-+--+-+-+", "measure": "trochaic.octa.plus.relaxed"}}}}}