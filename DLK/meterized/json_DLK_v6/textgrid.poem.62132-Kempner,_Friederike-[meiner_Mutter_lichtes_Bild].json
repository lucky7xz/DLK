{"textgrid.poem.62132": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "[meiner Mutter lichtes Bild]", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Meiner Mutter lichtes Bild,", "tokens": ["Mei\u00b7ner", "Mut\u00b7ter", "lich\u00b7tes", "Bild", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Meiner Mutter sanft Gesicht,", "tokens": ["Mei\u00b7ner", "Mut\u00b7ter", "sanft", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Meiner Mutter braune Augen,", "tokens": ["Mei\u00b7ner", "Mut\u00b7ter", "brau\u00b7ne", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alles dieses seh' ich nicht.", "tokens": ["Al\u00b7les", "die\u00b7ses", "seh'", "ich", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PDAT", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Aber tief im Busen lebt,", "tokens": ["A\u00b7ber", "tief", "im", "Bu\u00b7sen", "lebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "All der unversehrte Glanz,", "tokens": ["All", "der", "un\u00b7ver\u00b7sehr\u00b7te", "Glanz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihres Wesens Sch\u00f6nheit schwebt", "tokens": ["Ih\u00b7res", "We\u00b7sens", "Sch\u00f6n\u00b7heit", "schwebt"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ueber mir im Himmel ganz!", "tokens": ["Ue\u00b7ber", "mir", "im", "Him\u00b7mel", "ganz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPRART", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}