{"textgrid.poem.63859": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Beata solitudo", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In diesen linden L\u00fcften", "tokens": ["In", "die\u00b7sen", "lin\u00b7den", "L\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie ruht es sich so gut,", "tokens": ["Wie", "ruht", "es", "sich", "so", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Umhaucht von leisen D\u00fcften", "tokens": ["Um\u00b7haucht", "von", "lei\u00b7sen", "D\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der jungen Veilchenbrut!", "tokens": ["Der", "jun\u00b7gen", "Veil\u00b7chen\u00b7brut", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Kein Laut der Tiefe dringet", "tokens": ["Kein", "Laut", "der", "Tie\u00b7fe", "drin\u00b7get"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Hier st\u00f6rend zu mir hin,", "tokens": ["Hier", "st\u00f6\u00b7rend", "zu", "mir", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und tr\u00f6stlich immer klinget", "tokens": ["Und", "tr\u00f6st\u00b7lich", "im\u00b7mer", "klin\u00b7get"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der Spruch mir durch den Sinn:", "tokens": ["Der", "Spruch", "mir", "durch", "den", "Sinn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ein Schifflein kommt gegangen", "tokens": ["Ein", "Schif\u00b7flein", "kommt", "ge\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Tief unten auf der Flut.", "tokens": ["Tief", "un\u00b7ten", "auf", "der", "Flut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Segel niederhangen,", "tokens": ["Die", "Se\u00b7gel", "nie\u00b7der\u00b7han\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da jeder Fahrwind ruht.", "tokens": ["Da", "je\u00b7der", "Fahr\u00b7wind", "ruht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "So spielt mit meinem Herzen", "tokens": ["So", "spielt", "mit", "mei\u00b7nem", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ein windstill s\u00fc\u00dfer Traum;", "tokens": ["Ein", "wind\u00b7still", "s\u00fc\u00b7\u00dfer", "Traum", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der tausend alten Schmerzen", "tokens": ["Der", "tau\u00b7send", "al\u00b7ten", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "CARD", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und Freuden denk' ich kaum.", "tokens": ["Und", "Freu\u00b7den", "denk'", "ich", "kaum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich habe lang mein Leben", "tokens": ["Ich", "ha\u00b7be", "lang", "mein", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gesch\u00e4ftig durchgest\u00fcrmt,", "tokens": ["Ge\u00b7sch\u00e4f\u00b7tig", "durch\u00b7ge\u00b7st\u00fcrmt", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gar oft in Furcht und Beben,", "tokens": ["Gar", "oft", "in", "Furcht", "und", "Be\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wenn Wolken sich get\u00fcrmt.", "tokens": ["Wenn", "Wol\u00b7ken", "sich", "ge\u00b7t\u00fcrmt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nun, da ich hier geschieden", "tokens": ["Nun", ",", "da", "ich", "hier", "ge\u00b7schie\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Vom Weltget\u00fcmmel bin,", "tokens": ["Vom", "Welt\u00b7ge\u00b7t\u00fcm\u00b7mel", "bin", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Sing' ich in sel'gem Frieden", "tokens": ["Sing'", "ich", "in", "sel'\u00b7gem", "Frie\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mein Spr\u00fcchlein vor mich hin:", "tokens": ["Mein", "Spr\u00fcch\u00b7lein", "vor", "mich", "hin", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}