{"textgrid.poem.42189": {"metadata": {"author": {"name": "Wedekind, Frank", "birth": "N.A.", "death": "N.A."}, "title": "An die Kritik", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mir mu\u00df die Kritik sich wahrlich", "tokens": ["Mir", "mu\u00df", "die", "Kri\u00b7tik", "sich", "wahr\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "NN", "PRF", "ADV"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Von den sch\u00f6nsten Seiten zeigen;", "tokens": ["Von", "den", "sch\u00f6ns\u00b7ten", "Sei\u00b7ten", "zei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Zwanzig Jahr war sie beharrlich", "tokens": ["Zwan\u00b7zig", "Jahr", "war", "sie", "be\u00b7harr\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "VAFIN", "PPER", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Drauf erpicht, mich totzuschweigen.", "tokens": ["Drauf", "er\u00b7picht", ",", "mich", "tot\u00b7zu\u00b7schwei\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "VVPP", "$,", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Jetzt, nachdem ich, totgeschwiegen,", "tokens": ["Jetzt", ",", "nach\u00b7dem", "ich", ",", "tot\u00b7ge\u00b7schwie\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mich zum Trotz ans Licht gerungen,", "tokens": ["Mich", "zum", "Trotz", "ans", "Licht", "ge\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Speit sie rastlos ", "tokens": ["Speit", "sie", "rast\u00b7los"], "token_info": ["word", "word", "word"], "pos": ["NN", "PPER", "ADJD"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Unversieglich ha\u00dfdurchdrungen.", "tokens": ["Un\u00b7ver\u00b7sieg\u00b7lich", "ha\u00df\u00b7durch\u00b7drun\u00b7gen", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Einmal wird sie doch verzichten", "tokens": ["Ein\u00b7mal", "wird", "sie", "doch", "ver\u00b7zich\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die kl\u00fcgere Richtung w\u00e4hlen:", "tokens": ["Und", "die", "kl\u00fc\u00b7ge\u00b7re", "Rich\u00b7tung", "w\u00e4h\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Hilft ihr nichts, mich zu vernichten,", "tokens": ["Hilft", "ihr", "nichts", ",", "mich", "zu", "ver\u00b7nich\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIS", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie wird sie mich dann \u2013 bestehlen!", "tokens": ["Wie", "wird", "sie", "mich", "dann", "\u2013", "be\u00b7steh\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PRF", "ADV", "$(", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}