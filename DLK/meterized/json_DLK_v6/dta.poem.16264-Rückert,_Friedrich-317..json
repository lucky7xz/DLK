{"dta.poem.16264": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "317.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was sucht der Geist? das was als Widerspruch betiteln", "tokens": ["Was", "sucht", "der", "Geist", "?", "das", "was", "als", "Wi\u00b7der\u00b7spruch", "be\u00b7ti\u00b7teln"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ART", "NN", "$.", "PDS", "PIS", "KOKOM", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Sinne, suchet er erg\u00e4nzend zu ermitteln.", "tokens": ["Die", "Sin\u00b7ne", ",", "su\u00b7chet", "er", "er\u00b7g\u00e4n\u00b7zend", "zu", "er\u00b7mit\u00b7teln", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "PPER", "VVPP", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Des Menschen H\u00f6chstes ist des Streitenden Verbindung,", "tokens": ["Des", "Men\u00b7schen", "H\u00f6chs\u00b7tes", "ist", "des", "Strei\u00b7ten\u00b7den", "Ver\u00b7bin\u00b7dung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit der Erkenntnis Frucht die Bl\u00fcte der Empfindung.", "tokens": ["Mit", "der", "Er\u00b7kennt\u00b7nis", "Frucht", "die", "Bl\u00fc\u00b7te", "der", "Emp\u00b7fin\u00b7dung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Als hohes Vorbild sei der Baum dir eingepr\u00e4gt,", "tokens": ["Als", "ho\u00b7hes", "Vor\u00b7bild", "sei", "der", "Baum", "dir", "ein\u00b7ge\u00b7pr\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "VAFIN", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der hier im Garten Frucht zugleich und Bl\u00fcte tr\u00e4gt.", "tokens": ["Der", "hier", "im", "Gar\u00b7ten", "Frucht", "zu\u00b7gleich", "und", "Bl\u00fc\u00b7te", "tr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPRART", "NN", "NN", "ADV", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}