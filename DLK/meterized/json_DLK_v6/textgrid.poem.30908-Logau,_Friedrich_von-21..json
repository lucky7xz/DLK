{"textgrid.poem.30908": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "21.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Reichthum soll man zwar nicht lieben, mag es, wann es k\u00fcmt, doch fassen,", "tokens": ["Reicht\u00b7hum", "soll", "man", "zwar", "nicht", "lie\u00b7ben", ",", "mag", "es", ",", "wann", "es", "k\u00fcmt", ",", "doch", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PIS", "ADV", "PTKNEG", "VVINF", "$,", "VMFIN", "PPER", "$,", "PWAV", "PPER", "VVFIN", "$,", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Mag es in sein Haus zwar nehmen, aber nicht ins Hertze lassen,", "tokens": ["Mag", "es", "in", "sein", "Haus", "zwar", "neh\u00b7men", ",", "a\u00b7ber", "nicht", "ins", "Hert\u00b7ze", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PPOSAT", "NN", "ADV", "VVINF", "$,", "ADV", "PTKNEG", "APPRART", "VVFIN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Mag es, wann mans hat, behalten, darff es nicht von sich verjagen,", "tokens": ["Mag", "es", ",", "wann", "mans", "hat", ",", "be\u00b7hal\u00b7ten", ",", "darff", "es", "nicht", "von", "sich", "ver\u00b7ja\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "PWAV", "PIS", "VAFIN", "$,", "VVINF", "$,", "VMFIN", "PPER", "PTKNEG", "APPR", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Mag es ein in sein Behaltn\u00fc\u00df, sich nur nicht in seines tragen.", "tokens": ["Mag", "es", "ein", "in", "sein", "Be\u00b7halt\u00b7n\u00fc\u00df", ",", "sich", "nur", "nicht", "in", "sei\u00b7nes", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "APPR", "PPOSAT", "NN", "$,", "PRF", "ADV", "PTKNEG", "APPR", "PPOSAT", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}}}}