{"textgrid.poem.42456": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wir waren gestern unter uns,", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir waren gestern unter uns,", "tokens": ["Wir", "wa\u00b7ren", "ge\u00b7stern", "un\u00b7ter", "uns", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Beim Grafen von der Wisch,", "tokens": ["Beim", "Gra\u00b7fen", "von", "der", "Wisch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der gesamte Adel der Provinz,", "tokens": ["Der", "ge\u00b7sam\u00b7te", "A\u00b7del", "der", "Pro\u00b7vinz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Zu Gejaid und Tanz und Tisch.", "tokens": ["Zu", "Ge\u00b7jaid", "und", "Tanz", "und", "Tisch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Am kleinen Bahnhof warten wir jetzt", "tokens": ["Am", "klei\u00b7nen", "Bahn\u00b7hof", "war\u00b7ten", "wir", "jetzt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Und wollen nach S\u00fcd und Nord,", "tokens": ["Und", "wol\u00b7len", "nach", "S\u00fcd", "und", "Nord", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "NN", "KON", "NE", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ein jeder auf sein Schlo\u00df und Gut,", "tokens": ["Ein", "je\u00b7der", "auf", "sein", "Schlo\u00df", "und", "Gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "APPR", "PPOSAT", "NN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der n\u00e4chste Zug bringt uns fort.", "tokens": ["Der", "n\u00e4chs\u00b7te", "Zug", "bringt", "uns", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Mit Habichtsnasen und langem Bart", "tokens": ["Mit", "Ha\u00b7bichts\u00b7na\u00b7sen", "und", "lan\u00b7gem", "Bart"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Steht hier die Ritterschaft,", "tokens": ["Steht", "hier", "die", "Rit\u00b7ter\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit Mark in den Knochen, in hohem Wuchs,", "tokens": ["Mit", "Mark", "in", "den", "Kno\u00b7chen", ",", "in", "ho\u00b7hem", "Wuchs", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "In alter Herrenkraft.", "tokens": ["In", "al\u00b7ter", "Her\u00b7ren\u00b7kraft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ihr Sprechen ist etwas absonderlich,", "tokens": ["Ihr", "Spre\u00b7chen", "ist", "et\u00b7was", "ab\u00b7son\u00b7der\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Statt Ja sagen sie J\u00e4.", "tokens": ["Statt", "Ja", "sa\u00b7gen", "sie", "J\u00e4", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "PPER", "NE", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Ich unterhalte im Kreise mich", "tokens": ["Ich", "un\u00b7ter\u00b7hal\u00b7te", "im", "Krei\u00b7se", "mich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PPER"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mit \u00d6llegaard Westensee.", "tokens": ["Mit", "\u00d6l\u00b7le\u00b7ga\u00b7ard", "Wes\u00b7ten\u00b7see", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wie hab' ich getanzt mit der sch\u00f6nen Komte\u00df,", "tokens": ["Wie", "hab'", "ich", "ge\u00b7tanzt", "mit", "der", "sch\u00f6\u00b7nen", "Kom\u00b7te\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "VVPP", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Mein Herz schlug st\u00fcrmisch und wild.", "tokens": ["Mein", "Herz", "schlug", "st\u00fcr\u00b7misch", "und", "wild", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Deine schwarzen Augen, dein Zigeunerhaar,", "tokens": ["Dei\u00b7ne", "schwar\u00b7zen", "Au\u00b7gen", ",", "dein", "Zi\u00b7geu\u00b7ner\u00b7haar", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Niemals vergess' ich dein Bild.", "tokens": ["Nie\u00b7mals", "ver\u00b7ge\u00b7ss'", "ich", "dein", "Bild", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.6": {"line.1": {"text": "Komte\u00df, bleib' hier. Sieh dich um nach West,", "tokens": ["Kom\u00b7te\u00df", ",", "bleib'", "hier", ".", "Sieh", "dich", "um", "nach", "West", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ADV", "$.", "NE", "PRF", "APPR", "APPR", "NE", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Haide liegt weit gestreckt.", "tokens": ["Die", "Hai\u00b7de", "liegt", "weit", "ge\u00b7streckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "ADJD", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Auf die Reigerbeize dort ziehn wir hin,", "tokens": ["Auf", "die", "Rei\u00b7ger\u00b7bei\u00b7ze", "dort", "ziehn", "wir", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Das Silberhorn hat uns geweckt.", "tokens": ["Das", "Sil\u00b7ber\u00b7horn", "hat", "uns", "ge\u00b7weckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Komte\u00df, bleib' hier. Sieh dich um nach Ost,", "tokens": ["Kom\u00b7te\u00df", ",", "bleib'", "hier", ".", "Sieh", "dich", "um", "nach", "Ost", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ADV", "$.", "NE", "PRF", "APPR", "APPR", "NE", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Wald liegt weit gestreckt.", "tokens": ["Der", "Wald", "liegt", "weit", "ge\u00b7streckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auf die Wolfsjagd wollen dorthin wir ziehn,", "tokens": ["Auf", "die", "Wolfs\u00b7jagd", "wol\u00b7len", "dor\u00b7thin", "wir", "ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "ADV", "PPER", "VVINF", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Das R\u00fcdenhorn hat uns geweckt.", "tokens": ["Das", "R\u00fc\u00b7den\u00b7horn", "hat", "uns", "ge\u00b7weckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Ich liebe dich, \u00d6llegaard, weil du noch viel,", "tokens": ["Ich", "lie\u00b7be", "dich", ",", "\u00d6l\u00b7le\u00b7ga\u00b7ard", ",", "weil", "du", "noch", "viel", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "NE", "$,", "KOUS", "PPER", "ADV", "ADV", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Viel hochm\u00fctiger als die andern schaust,", "tokens": ["Viel", "hoch\u00b7m\u00fc\u00b7ti\u00b7ger", "als", "die", "an\u00b7dern", "schaust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ART", "ADJA", "VVFIN", "$,"], "meter": "-++--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Weil du kein Blondhaar hast, kein wei\u00dfrotes Gesicht,", "tokens": ["Weil", "du", "kein", "Blond\u00b7haar", "hast", ",", "kein", "wei\u00df\u00b7ro\u00b7tes", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "VAFIN", "$,", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-++-+-+--+", "measure": "iambic.hexa.chol"}, "line.4": {"text": "Weil du mir trotzt und vertraust.", "tokens": ["Weil", "du", "mir", "trotzt", "und", "ver\u00b7traust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.9": {"line.1": {"text": "Wie das nasse Gras unsre Hengste umschl\u00e4gt,", "tokens": ["Wie", "das", "nas\u00b7se", "Gras", "uns\u00b7re", "Hengs\u00b7te", "um\u00b7schl\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der letzte Stern ging aus.", "tokens": ["Der", "letz\u00b7te", "Stern", "ging", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auf deinem gelben Stulpen hockt hoch", "tokens": ["Auf", "dei\u00b7nem", "gel\u00b7ben", "Stul\u00b7pen", "hockt", "hoch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Der Islandfalke zum Strau\u00df.", "tokens": ["Der", "Is\u00b7land\u00b7fal\u00b7ke", "zum", "Strau\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NE", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.10": {"line.1": {"text": "Die Sonne blitzt auf, aus Weiden und Schilf", "tokens": ["Die", "Son\u00b7ne", "blitzt", "auf", ",", "aus", "Wei\u00b7den", "und", "Schilf"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "APPR", "NN", "KON", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Streicht schwerf\u00e4llig ein Reiher ab.", "tokens": ["Streicht", "schwer\u00b7f\u00e4l\u00b7lig", "ein", "Rei\u00b7her", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ART", "NN", "PTKVZ", "$."], "meter": "-++--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die Haube los! Wie der Herrliche steigt!", "tokens": ["Die", "Hau\u00b7be", "los", "!", "Wie", "der", "Herr\u00b7li\u00b7che", "steigt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$.", "PWAV", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Dein Falke holt ihn herab.", "tokens": ["Dein", "Fal\u00b7ke", "holt", "ihn", "her\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.11": {"line.1": {"text": "O wundervolles Morgenspiel,", "tokens": ["O", "wun\u00b7der\u00b7vol\u00b7les", "Mor\u00b7gen\u00b7spiel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In L\u00fcften Kampf und Krieg,", "tokens": ["In", "L\u00fcf\u00b7ten", "Kampf", "und", "Krieg", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Reiher st\u00fcrzt, seine Feder ist dein,", "tokens": ["Der", "Rei\u00b7her", "st\u00fcrzt", ",", "sei\u00b7ne", "Fe\u00b7der", "ist", "dein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPOSAT", "NN", "VAFIN", "PPOSAT", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Im Haidedampf leuchtet der Sieg.", "tokens": ["Im", "Hai\u00b7de\u00b7dampf", "leuch\u00b7tet", "der", "Sieg", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.12": {"line.1": {"text": "Ich halte den m\u00e4chtigen Vogel fest,", "tokens": ["Ich", "hal\u00b7te", "den", "m\u00e4ch\u00b7ti\u00b7gen", "Vo\u00b7gel", "fest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Bis du dem Edeling", "tokens": ["Bis", "du", "dem", "E\u00b7de\u00b7ling"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Um den widerspenstigen Hals gelegt", "tokens": ["Um", "den", "wi\u00b7der\u00b7spens\u00b7ti\u00b7gen", "Hals", "ge\u00b7legt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "ADJA", "NN", "VVPP"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Den goldnen Sklavenring.", "tokens": ["Den", "gold\u00b7nen", "Skla\u00b7ven\u00b7ring", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Vierhundert Leibeigne umstellen den Wald,", "tokens": ["Vier\u00b7hun\u00b7dert", "Leib\u00b7eig\u00b7ne", "um\u00b7stel\u00b7len", "den", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+--+---+--+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Freund Wolf, fl\u00fcchte dein Flie\u00df.", "tokens": ["Freund", "Wolf", ",", "fl\u00fcch\u00b7te", "dein", "Flie\u00df", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Da trottet er, der magre, sch\u00e4bige Gesell,", "tokens": ["Da", "trot\u00b7tet", "er", ",", "der", "mag\u00b7re", ",", "sch\u00e4\u00b7bi\u00b7ge", "Ge\u00b7sell", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Schnell, Herrin, wirf den Spie\u00df.", "tokens": ["Schnell", ",", "Her\u00b7rin", ",", "wirf", "den", "Spie\u00df", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "NN", "$,", "VVIMP", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Der traf doch? Sitz' ab. Ich sto\u00df' in's Horn.", "tokens": ["Der", "traf", "doch", "?", "Sitz'", "ab", ".", "Ich", "sto\u00df'", "in's", "Horn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "$.", "NN", "PTKVZ", "$.", "PPER", "VVFIN"