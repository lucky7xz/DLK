{"dta.poem.7416": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Ii.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192430", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Seht ihr den Geist der Freiheit schreiten", "tokens": ["Seht", "ihr", "den", "Geist", "der", "Frei\u00b7heit", "schrei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auf Blumensohlen durch das Land?", "tokens": ["Auf", "Blu\u00b7men\u00b7soh\u00b7len", "durch", "das", "Land", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zum stillen Segen liebend breiten", "tokens": ["Zum", "stil\u00b7len", "Se\u00b7gen", "lie\u00b7bend", "brei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ADJD", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die schwertgewohnte G\u00f6tterhand?", "tokens": ["Die", "schwert\u00b7ge\u00b7wohn\u00b7te", "G\u00f6t\u00b7ter\u00b7hand", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Auf hohem Berg, im tiefsten Thale,", "tokens": ["Auf", "ho\u00b7hem", "Berg", ",", "im", "tiefs\u00b7ten", "Tha\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So freudig rauscht's, so wundersam;", "tokens": ["So", "freu\u00b7dig", "rauscht's", ",", "so", "wun\u00b7der\u00b7sam", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Freiheit weint zum vierten Male,", "tokens": ["Die", "Frei\u00b7heit", "weint", "zum", "vier\u00b7ten", "Ma\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zum vierten Male nicht aus Gram.", "tokens": ["Zum", "vier\u00b7ten", "Ma\u00b7le", "nicht", "aus", "Gram", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PTKNEG", "APPR", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Denn V\u00f6lker knieen am Altare,", "tokens": ["Denn", "V\u00f6l\u00b7ker", "kni\u00b7e\u00b7en", "am", "Al\u00b7ta\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Den ihrem Sohn man auferbaut,", "tokens": ["Den", "ih\u00b7rem", "Sohn", "man", "au\u00b7fer\u00b7baut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Opfer sind vierhundert Jahre,", "tokens": ["Das", "Op\u00b7fer", "sind", "vier\u00b7hun\u00b7dert", "Jah\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "CARD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Ewigkeit ist seine Braut.", "tokens": ["Die", "E\u00b7wig\u00b7keit", "ist", "sei\u00b7ne", "Braut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Vierhundert Jahre sind erschlagen,", "tokens": ["Vier\u00b7hun\u00b7dert", "Jah\u00b7re", "sind", "er\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "VVPP", "$,"], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vierhundert Feinde liegen tot,", "tokens": ["Vier\u00b7hun\u00b7dert", "Fein\u00b7de", "lie\u00b7gen", "tot", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bald wird er frei die Waffen tragen,", "tokens": ["Bald", "wird", "er", "frei", "die", "Waf\u00b7fen", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die ihm die freie Mutter bot.", "tokens": ["Die", "ihm", "die", "frei\u00b7e", "Mut\u00b7ter", "bot", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Bald wird er schleudern frei die Blitze", "tokens": ["Bald", "wird", "er", "schleu\u00b7dern", "frei", "die", "Blit\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "VVFIN", "ADJD", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In des Verbrechens d\u00fcstres Haus,", "tokens": ["In", "des", "Ver\u00b7bre\u00b7chens", "d\u00fcst\u00b7res", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und dann auf ihrem Lottersitze", "tokens": ["Und", "dann", "auf", "ih\u00b7rem", "Lot\u00b7ter\u00b7sit\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des Volkes Feinde sp\u00e4hen aus.", "tokens": ["Des", "Vol\u00b7kes", "Fein\u00b7de", "sp\u00e4\u00b7hen", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}