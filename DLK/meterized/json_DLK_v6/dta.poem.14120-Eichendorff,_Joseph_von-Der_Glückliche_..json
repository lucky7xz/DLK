{"dta.poem.14120": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der Gl\u00fcckliche .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Wie vom Gebirge in's Meer zu schauen,", "tokens": ["Wie", "vom", "Ge\u00b7bir\u00b7ge", "in's", "Meer", "zu", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Wie wenn der Seefalk, hangend im Blauen,", "tokens": ["Wie", "wenn", "der", "See\u00b7falk", ",", "han\u00b7gend", "im", "Blau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "ART", "NN", "$,", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Zuruft der d\u00e4mmerndern Erd', wo sie blieb? \u2014", "tokens": ["Zu\u00b7ruft", "der", "d\u00e4m\u00b7mern\u00b7dern", "Erd'", ",", "wo", "sie", "blieb", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,", "PWAV", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "So unerme\u00dflich ist rechte Lieb'!", "tokens": ["So", "un\u00b7er\u00b7me\u00df\u00b7lich", "ist", "rech\u00b7te", "Lieb'", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}}}}