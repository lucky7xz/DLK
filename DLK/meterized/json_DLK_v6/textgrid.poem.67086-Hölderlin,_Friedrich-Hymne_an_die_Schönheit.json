{"textgrid.poem.67086": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Hymne an die Sch\u00f6nheit", "genre": "verse", "period": "N.A.", "pub_year": 1791, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hab ich vor der G\u00f6tter Ohren,", "tokens": ["Hab", "ich", "vor", "der", "G\u00f6t\u00b7ter", "Oh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zauberische Muse, dir", "tokens": ["Zau\u00b7be\u00b7ri\u00b7sche", "Mu\u00b7se", ",", "dir"], "token_info": ["word", "word", "punct", "word"], "pos": ["ADJA", "NN", "$,", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lieb und Treue nicht geschworen?", "tokens": ["Lieb", "und", "Treu\u00b7e", "nicht", "ge\u00b7schwo\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PTKNEG", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sankst du nicht in Lust verloren", "tokens": ["Sankst", "du", "nicht", "in", "Lust", "ver\u00b7lo\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "APPR", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Gl\u00fchend in die Arme mir? \u2013", "tokens": ["Gl\u00fc\u00b7hend", "in", "die", "Ar\u00b7me", "mir", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "PPER", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ha! so wall ich ohne Zagen,", "tokens": ["Ha", "!", "so", "wall", "ich", "oh\u00b7ne", "Za\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Durch die Liebe froh und k\u00fchn,", "tokens": ["Durch", "die", "Lie\u00b7be", "froh", "und", "k\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "L\u00e4chelnd zu den H\u00f6hen hin,", "tokens": ["L\u00e4\u00b7chelnd", "zu", "den", "H\u00f6\u00b7hen", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Wo die letzten N\u00e4chte tagen,", "tokens": ["Wo", "die", "letz\u00b7ten", "N\u00e4ch\u00b7te", "ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Wo der Sonnen letzte schien.", "tokens": ["Wo", "der", "Son\u00b7nen", "letz\u00b7te", "schien", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJA", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Waltend \u00fcber Orionen,", "tokens": ["Wal\u00b7tend", "\u00fc\u00b7ber", "O\u00b7rio\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Wo der Sterne Klang verhallt,", "tokens": ["Wo", "der", "Ster\u00b7ne", "Klang", "ver\u00b7hallt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "L\u00e4chelt, opfernden D\u00e4monen", "tokens": ["L\u00e4\u00b7chelt", ",", "op\u00b7fern\u00b7den", "D\u00e4\u00b7mo\u00b7nen"], "token_info": ["word", "punct", "word", "word"], "pos": ["VVFIN", "$,", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Mit der Liebe Blick zu lohnen,", "tokens": ["Mit", "der", "Lie\u00b7be", "Blick", "zu", "loh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Dort dem hohen G\u00f6tterglanze", "tokens": ["Dort", "dem", "ho\u00b7hen", "G\u00f6t\u00b7ter\u00b7glan\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Der Gebieterin zu nahn,", "tokens": ["Der", "Ge\u00b7bie\u00b7te\u00b7rin", "zu", "nahn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Flammet Lieb und Stolz mich an,", "tokens": ["Flam\u00b7met", "Lieb", "und", "Stolz", "mich", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "NN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Denn mit hellem Siegeskranze", "tokens": ["Denn", "mit", "hel\u00b7lem", "Sie\u00b7ges\u00b7kran\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Lohnet sie die k\u00fchne Bahn.", "tokens": ["Loh\u00b7net", "sie", "die", "k\u00fch\u00b7ne", "Bahn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Reinere Begeisterungen", "tokens": ["Rei\u00b7ne\u00b7re", "Be\u00b7geis\u00b7te\u00b7run\u00b7gen"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Trinkt die freie Seele schon,", "tokens": ["Trinkt", "die", "frei\u00b7e", "See\u00b7le", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Meines Lebens Peinigungen", "tokens": ["Mei\u00b7nes", "Le\u00b7bens", "Pei\u00b7ni\u00b7gun\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hat die neue Lust verschlungen,", "tokens": ["Hat", "die", "neu\u00b7e", "Lust", "ver\u00b7schlun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nacht und Wolke sind entflohn;", "tokens": ["Nacht", "und", "Wol\u00b7ke", "sind", "ent\u00b7flohn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wann im schreckenden Gerichte", "tokens": ["Wann", "im", "schre\u00b7cken\u00b7den", "Ge\u00b7rich\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Schnell der Welten Achse bricht,", "tokens": ["Schnell", "der", "Wel\u00b7ten", "A\u00b7chse", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Hier erbebt die Liebe nicht,", "tokens": ["Hier", "er\u00b7bebt", "die", "Lie\u00b7be", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Wo von ", "tokens": ["Wo", "von"], "token_info": ["word", "word"], "pos": ["PWAV", "APPR"], "meter": "+-", "measure": "trochaic.single"}, "line.10": {"text": "Lieb und G\u00f6ttergr\u00f6\u00dfe spricht.", "tokens": ["Lieb", "und", "G\u00f6t\u00b7ter\u00b7gr\u00f6\u00b7\u00dfe", "spricht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Stiegst du so zur Erde nieder,", "tokens": ["Stiegst", "du", "so", "zur", "Er\u00b7de", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hohe s\u00fc\u00dfe Zauberin!", "tokens": ["Ho\u00b7he", "s\u00fc\u00b7\u00dfe", "Zau\u00b7be\u00b7rin", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ha! der Staub erwachte wieder", "tokens": ["Ha", "!", "der", "Staub", "er\u00b7wach\u00b7te", "wie\u00b7der"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "ART", "NN", "VVFIN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und des Kummers morsche Glieder", "tokens": ["Und", "des", "Kum\u00b7mers", "mor\u00b7sche", "Glie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "H\u00fcpften \u00fcppig vor dir hin;", "tokens": ["H\u00fcpf\u00b7ten", "\u00fcp\u00b7pig", "vor", "dir", "hin", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Von der Liebe Blick betroffen", "tokens": ["Von", "der", "Lie\u00b7be", "Blick", "be\u00b7trof\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Bebt' und k\u00fc\u00dfte br\u00fcderlich", "tokens": ["Bebt'", "und", "k\u00fc\u00df\u00b7te", "br\u00fc\u00b7der\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "VVFIN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Groll und wilder Hader sich,", "tokens": ["Groll", "und", "wil\u00b7der", "Ha\u00b7der", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJA", "NN", "PRF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Wie der Himmel, hell und offen", "tokens": ["Wie", "der", "Him\u00b7mel", ",", "hell", "und", "of\u00b7fen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "$,", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Gr\u00fc\u00dften Wahn und Irre dich.", "tokens": ["Gr\u00fc\u00df\u00b7ten", "Wahn", "und", "Ir\u00b7re", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Schon im gr\u00fcnen Erdenrunde", "tokens": ["Schon", "im", "gr\u00fc\u00b7nen", "Er\u00b7den\u00b7run\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schmeckt ich hohen Vorgenu\u00df,", "tokens": ["Schmeckt", "ich", "ho\u00b7hen", "Vor\u00b7ge\u00b7nu\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bebend dir am G\u00f6ttermunde", "tokens": ["Be\u00b7bend", "dir", "am", "G\u00f6t\u00b7ter\u00b7mun\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "PPER", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Trank ich fr\u00fch der Weihestunde", "tokens": ["Trank", "ich", "fr\u00fch", "der", "Wei\u00b7hes\u00b7tun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ADJD", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "S\u00fc\u00dfen m\u00fctterlichen Ku\u00df;", "tokens": ["S\u00fc\u00b7\u00dfen", "m\u00fct\u00b7ter\u00b7li\u00b7chen", "Ku\u00df", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Fremde meinem Kindersinne", "tokens": ["Frem\u00b7de", "mei\u00b7nem", "Kin\u00b7der\u00b7sin\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Folgte mir zu Wies und Wald", "tokens": ["Folg\u00b7te", "mir", "zu", "Wies", "und", "Wald"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "NE", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Die arkadische Gestalt.", "tokens": ["Die", "ar\u00b7ka\u00b7di\u00b7sche", "Ge\u00b7stalt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.9": {"text": "Ha! und staunend ward ich inne", "tokens": ["Ha", "!", "und", "stau\u00b7nend", "ward", "ich", "in\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "KON", "ADJD", "VAFIN", "PPER", "PTKVZ"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Ihres Zaubers Allgewalt.", "tokens": ["Ih\u00b7res", "Zau\u00b7bers", "All\u00b7ge\u00b7walt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "In den Tiefen und den H\u00f6hen", "tokens": ["In", "den", "Tie\u00b7fen", "und", "den", "H\u00f6\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "KON", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der erfreuenden Natur", "tokens": ["Der", "er\u00b7freu\u00b7en\u00b7den", "Na\u00b7tur"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fand ich, Wonne zu ersp\u00e4hen", "tokens": ["Fand", "ich", ",", "Won\u00b7ne", "zu", "er\u00b7sp\u00e4\u00b7hen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Von der Holdin ausersehen,", "tokens": ["Von", "der", "Hol\u00b7din", "au\u00b7ser\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVIZU", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Liebetrunken ihre Spur;", "tokens": ["Lie\u00b7be\u00b7trun\u00b7ken", "ih\u00b7re", "Spur", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wo das Tal der Blumenh\u00fcgel", "tokens": ["Wo", "das", "Tal", "der", "Blu\u00b7men\u00b7h\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Freundlich in die Arme schlo\u00df,", "tokens": ["Freund\u00b7lich", "in", "die", "Ar\u00b7me", "schlo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Wo die Quelle niederflo\u00df", "tokens": ["Wo", "die", "Quel\u00b7le", "nie\u00b7der\u00b7flo\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "In den klaren Wasserspiegel,", "tokens": ["In", "den", "kla\u00b7ren", "Was\u00b7ser\u00b7spie\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Fand ich Spuren, hold und gro\u00df!", "tokens": ["Fand", "ich", "Spu\u00b7ren", ",", "hold", "und", "gro\u00df", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Gl\u00fchend an der Purpurwange", "tokens": ["Gl\u00fc\u00b7hend", "an", "der", "Pur\u00b7pur\u00b7wan\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sanft ber\u00fchrt vom Lockenhaar,", "tokens": ["Sanft", "be\u00b7r\u00fchrt", "vom", "Lo\u00b7cken\u00b7haar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Von der Lippe, s\u00fc\u00df und bange", "tokens": ["Von", "der", "Lip\u00b7pe", ",", "s\u00fc\u00df", "und", "ban\u00b7ge"], "token_info": ["word", "word", 