{"textgrid.poem.46636": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[wie dauert mich der Freund, so oft ich ihm begegne]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie dauert mich der Freund, so oft ich ihm begegne,", "tokens": ["Wie", "dau\u00b7ert", "mich", "der", "Freund", ",", "so", "oft", "ich", "ihm", "be\u00b7geg\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "NN", "$,", "ADV", "ADV", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie er sein arm Gesicht mi\u00dfhandelt, das verlegne.", "tokens": ["Wie", "er", "sein", "arm", "Ge\u00b7sicht", "mi\u00df\u00b7han\u00b7delt", ",", "das", "ver\u00b7leg\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "ADJA", "NN", "VVPP", "$,", "PDS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Er meint, um Mitgef\u00fchl des Leids mir auszudr\u00fccken,", "tokens": ["Er", "meint", ",", "um", "Mit\u00b7ge\u00b7f\u00fchl", "des", "Leids", "mir", "aus\u00b7zu\u00b7dr\u00fc\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUI", "NN", "ART", "NN", "PPER", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "M\u00fc\u00df' er auch jede Spur von Frohheit unterdr\u00fccken.", "tokens": ["M\u00fc\u00df'", "er", "auch", "je\u00b7de", "Spur", "von", "Froh\u00b7heit", "un\u00b7ter\u00b7dr\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "PIAT", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es macht ihm, soviel Weh im Angesicht zu zeigen,", "tokens": ["Es", "macht", "ihm", ",", "so\u00b7viel", "Weh", "im", "An\u00b7ge\u00b7sicht", "zu", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PIAT", "NN", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Gewi\u00df mehr Weh, als mir, mein eignes zu verschweigen.", "tokens": ["Ge\u00b7wi\u00df", "mehr", "Weh", ",", "als", "mir", ",", "mein", "eig\u00b7nes", "zu", "ver\u00b7schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$,", "KOUS", "PPER", "$,", "PPOSAT", "ADJA", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}