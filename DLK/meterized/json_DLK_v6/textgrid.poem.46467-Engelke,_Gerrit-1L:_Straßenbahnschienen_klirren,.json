{"textgrid.poem.46467": {"metadata": {"author": {"name": "Engelke, Gerrit", "birth": "N.A.", "death": "N.A."}, "title": "1L: Stra\u00dfenbahnschienen klirren,", "genre": "verse", "period": "N.A.", "pub_year": 1913, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Stra\u00dfenbahnschienen klirren,", "tokens": ["Stra\u00b7\u00dfen\u00b7bahn\u00b7schie\u00b7nen", "klir\u00b7ren", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Hundert Sensen umschwirren,", "tokens": ["Hun\u00b7dert", "Sen\u00b7sen", "um\u00b7schwir\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVINF", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Fabriken umrauchen dich,", "tokens": ["Fab\u00b7ri\u00b7ken", "um\u00b7rau\u00b7chen", "dich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Im Ohre gellt dir: \u2013 Messerstich,", "tokens": ["Im", "Oh\u00b7re", "gellt", "dir", ":", "\u2013", "Mes\u00b7ser\u00b7stich", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "$.", "$(", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Gesch\u00e4ft, Diebstahl, Geld, Brand \u2013", "tokens": ["Ge\u00b7sch\u00e4ft", ",", "Dieb\u00b7stahl", ",", "Geld", ",", "Brand", "\u2013"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "W\u00e4nde st\u00fcrzen \u00fcber dir ein:", "tokens": ["W\u00e4n\u00b7de", "st\u00fcr\u00b7zen", "\u00fc\u00b7ber", "dir", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Du verk\u00fcmmerst, wirst klein und gemein \u2013", "tokens": ["Du", "ver\u00b7k\u00fcm\u00b7merst", ",", "wirst", "klein", "und", "ge\u00b7mein", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VAFIN", "ADJD", "KON", "ADJD", "$("], "meter": "+-+-++--+", "measure": "iambic.penta.chol"}, "line.8": {"text": "Hinaus!", "tokens": ["Hin\u00b7aus", "!"], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "-+", "measure": "iambic.single"}, "line.9": {"text": "Hinaus auf's Land!", "tokens": ["Hin\u00b7aus", "auf's", "Land", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}