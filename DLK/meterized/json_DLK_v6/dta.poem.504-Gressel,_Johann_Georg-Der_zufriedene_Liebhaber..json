{"dta.poem.504": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Der zufriedene Liebhaber.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Es sind zwar einige den Crispa nicht gef\u00e4llt;", "tokens": ["Es", "sind", "zwar", "ei\u00b7ni\u00b7ge", "den", "Cris\u00b7pa", "nicht", "ge\u00b7f\u00e4llt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PIS", "ART", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mir aber ist sie sch\u00f6n/ wie es mein Sinn vermeynet/", "tokens": ["Mir", "a\u00b7ber", "ist", "sie", "sch\u00f6n", "/", "wie", "es", "mein", "Sinn", "ver\u00b7mey\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "PPER", "ADJD", "$(", "PWAV", "PPER", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Denn weil die Eyversucht zur Liebe sich gesellt/", "tokens": ["Denn", "weil", "die", "Ey\u00b7ver\u00b7sucht", "zur", "Lie\u00b7be", "sich", "ge\u00b7sellt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "APPRART", "NN", "PRF", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So seh\u2019 ich auch nicht gern/ da\u00df allen sch\u00f6n sie scheinet.", "tokens": ["So", "seh'", "ich", "auch", "nicht", "gern", "/", "da\u00df", "al\u00b7len", "sch\u00f6n", "sie", "schei\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKNEG", "ADV", "$(", "KOUS", "PIS", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}