{"textgrid.poem.40565": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbmein Nam' ist Colibri, Mann von Hofe,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbmein Nam' ist Colibri, Mann von Hofe,", "tokens": ["\u00bb", "mein", "Nam'", "ist", "Co\u00b7lib\u00b7ri", ",", "Mann", "von", "Ho\u00b7fe", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "VAFIN", "NE", "$,", "NN", "APPR", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "An Liebreiz ein klein Ungeheuer,", "tokens": ["An", "Lieb\u00b7reiz", "ein", "klein", "Un\u00b7ge\u00b7heu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der K\u00f6nigin Rose und ihrer Zofe,", "tokens": ["Der", "K\u00f6\u00b7ni\u00b7gin", "Ro\u00b7se", "und", "ih\u00b7rer", "Zo\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "KON", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Dem sch\u00f6nen Haider\u00f6slein, gleich theuer.", "tokens": ["Dem", "sch\u00f6\u00b7nen", "Hai\u00b7de\u00b7r\u00f6s\u00b7lein", ",", "gleich", "theu\u00b7er", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Ich summe Sonette zu ihrem Preise,", "tokens": ["Ich", "sum\u00b7me", "So\u00b7net\u00b7te", "zu", "ih\u00b7rem", "Prei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Umschwebe sie artig und dienstbeflissen;", "tokens": ["Um\u00b7schwe\u00b7be", "sie", "ar\u00b7tig", "und", "dienst\u00b7be\u00b7flis\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "KON", "PDS", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Wer sich bewegt in so feinem Kreise,", "tokens": ["Wer", "sich", "be\u00b7wegt", "in", "so", "fei\u00b7nem", "Krei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "VVFIN", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Darf Anstand und fein Gewand nicht missen.", "tokens": ["Darf", "An\u00b7stand", "und", "fein", "Ge\u00b7wand", "nicht", "mis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "KON", "ADJD", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Ich trag' ein Barett demantenflimmernd", "tokens": ["Ich", "trag'", "ein", "Ba\u00b7rett", "de\u00b7man\u00b7ten\u00b7flim\u00b7mernd"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVPP"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Staatsweste, H\u00f6slein goldbrokaten,", "tokens": ["Staats\u00b7wes\u00b7te", ",", "H\u00f6s\u00b7lein", "gold\u00b7bro\u00b7ka\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Den Frack von gr\u00fcner Seide schimmernd", "tokens": ["Den", "Frack", "von", "gr\u00fc\u00b7ner", "Sei\u00b7de", "schim\u00b7mernd"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und ausgen\u00e4ht mit bunten Nahten.", "tokens": ["Und", "aus\u00b7ge\u00b7n\u00e4ht", "mit", "bun\u00b7ten", "Nah\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Mein Schn\u00e4blein ist mein Galadegen,", "tokens": ["Mein", "Schn\u00e4b\u00b7lein", "ist", "mein", "Ga\u00b7la\u00b7de\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mein Z\u00fcnglein beweglich ist die Klinge;", "tokens": ["Mein", "Z\u00fcn\u00b7glein", "be\u00b7weg\u00b7lich", "ist", "die", "Klin\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Was ich mit jenem nicht darf erlegen,", "tokens": ["Was", "ich", "mit", "je\u00b7nem", "nicht", "darf", "er\u00b7le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "PDAT", "PTKNEG", "VMFIN", "VVPP", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mit dieser ich's sicherlich bezwinge.", "tokens": ["Mit", "die\u00b7ser", "ich's", "si\u00b7cher\u00b7lich", "be\u00b7zwin\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "PIS", "PRF", "VVFIN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Man sagt, ich sei treulos und fl\u00fcchtig", "tokens": ["Man", "sagt", ",", "ich", "sei", "treu\u00b7los", "und", "fl\u00fcch\u00b7tig"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "$,", "PPER", "VAFIN", "ADJD", "KON", "ADJD"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Und meine Huldigung wetterwendig;", "tokens": ["Und", "mei\u00b7ne", "Hul\u00b7di\u00b7gung", "wet\u00b7ter\u00b7wen\u00b7dig", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+----+-+-", "measure": "dactylic.init"}, "line.3": {"text": "Untreu der einzlen Blume, die nichtig,", "tokens": ["Un\u00b7treu", "der", "einz\u00b7len", "Blu\u00b7me", ",", "die", "nich\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PRELS", "ADJD", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Bin treu ich der Lenzmacht, die best\u00e4ndig!", "tokens": ["Bin", "treu", "ich", "der", "Lenz\u00b7macht", ",", "die", "be\u00b7st\u00e4n\u00b7dig", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "PPER", "ART", "NN", "$,", "PRELS", "ADJD", "$."], "meter": "++--+-+-+-", "measure": "trochaic.penta.relaxed"}}, "stanza.6": {"line.1": {"text": "Ob sich die Meuter auch all' verschworen,", "tokens": ["Ob", "sich", "die", "Meu\u00b7ter", "auch", "all'", "ver\u00b7schwo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "ADV", "PIS", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Den milden Zepter der Rose werden,", "tokens": ["Den", "mil\u00b7den", "Zep\u00b7ter", "der", "Ro\u00b7se", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VAINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ich wei\u00df es, nimmer zerbrechen die Thoren,", "tokens": ["Ich", "wei\u00df", "es", ",", "nim\u00b7mer", "zer\u00b7bre\u00b7chen", "die", "Tho\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Das Reich des Lenzes nimmer gef\u00e4hrden.", "tokens": ["Das", "Reich", "des", "Len\u00b7zes", "nim\u00b7mer", "ge\u00b7f\u00e4hr\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Da schie\u00dft der Hagel mit silbernen Pfeilen,", "tokens": ["Da", "schie\u00dft", "der", "Ha\u00b7gel", "mit", "sil\u00b7ber\u00b7nen", "Pfei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da st\u00fcrmt mit kristall'nen Lanzen der Regen,", "tokens": ["Da", "st\u00fcrmt", "mit", "kris\u00b7tall'\u00b7nen", "Lan\u00b7zen", "der", "Re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Da seht ihr den grimmen Winter eilen,", "tokens": ["Da", "seht", "ihr", "den", "grim\u00b7men", "Win\u00b7ter", "ei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Des Reiches Farben hinwegzufegen.", "tokens": ["Des", "Rei\u00b7ches", "Far\u00b7ben", "hin\u00b7weg\u00b7zu\u00b7fe\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Da rei\u00dft der Sturm, ein gemeiner Scherge,", "tokens": ["Da", "rei\u00dft", "der", "Sturm", ",", "ein", "ge\u00b7mei\u00b7ner", "Scher\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Rose den Purpurmantel vom Leibe;", "tokens": ["Der", "Ro\u00b7se", "den", "Pur\u00b7pur\u00b7man\u00b7tel", "vom", "Lei\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+-++-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Sie wei\u00df, da\u00df, ob sie im Tod sich berge,", "tokens": ["Sie", "wei\u00df", ",", "da\u00df", ",", "ob", "sie", "im", "Tod", "sich", "ber\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "$,", "KOUS", "PPER", "APPRART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ihr Stamm doch frischere Sprossen treibe.", "tokens": ["Ihr", "Stamm", "doch", "fri\u00b7sche\u00b7re", "Spros\u00b7sen", "trei\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.9": {"line.1": {"text": "Besudelt mir nicht des Hofkleids Stoffe", "tokens": ["Be\u00b7su\u00b7delt", "mir", "nicht", "des", "Hof\u00b7kleids", "Stof\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "ART", "NN", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Im Tr\u00fcmmerfall, im Kampfgetose!", "tokens": ["Im", "Tr\u00fcm\u00b7mer\u00b7fall", ",", "im", "Kampf\u00b7ge\u00b7to\u00b7se", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Ausgang aber wird gut, ich hoffe,", "tokens": ["Der", "Aus\u00b7gang", "a\u00b7ber", "wird", "gut", ",", "ich", "hof\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "ADJD", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die Rose ist todt, es lebe die Rose!\u00ab", "tokens": ["Die", "Ro\u00b7se", "ist", "todt", ",", "es", "le\u00b7be", "die", "Ro\u00b7se", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "PPER", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}}}}}