{"textgrid.poem.46265": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Ein rundum", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein kleine weil, als ohn gef\u00e4hr", "tokens": ["Ein", "klei\u00b7ne", "weil", ",", "als", "ohn", "ge\u00b7f\u00e4hr"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "KOUS", "$,", "KOUS", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "ich euch in einem saal gefunden,", "tokens": ["ich", "euch", "in", "ei\u00b7nem", "saal", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "sah ich euch an, bald mehr und mehr", "tokens": ["sah", "ich", "euch", "an", ",", "bald", "mehr", "und", "mehr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "PTKVZ", "$,", "ADV", "ADV", "KON", "ADV"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "hat euer haar mein herz verbunden:", "tokens": ["hat", "eu\u00b7er", "haar", "mein", "herz", "ver\u00b7bun\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ihr auch lieb\u00e4ugelten mir sehr,", "tokens": ["Ihr", "auch", "lie\u00b7b\u00e4u\u00b7gel\u00b7ten", "mir", "sehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "ADV", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "dadurch ich, wei\u00df nicht was, empfunden,", "tokens": ["da\u00b7durch", "ich", ",", "wei\u00df", "nicht", "was", ",", "emp\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PAV", "PPER", "$,", "VVFIN", "PTKNEG", "PIS", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "das meinem geist, dan leicht, dan schwer,", "tokens": ["das", "mei\u00b7nem", "geist", ",", "dan", "leicht", ",", "dan", "schwer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "aus lieb und leid alsbald geschwunden", "tokens": ["aus", "lieb", "und", "leid", "als\u00b7bald", "ge\u00b7schwun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJD", "KON", "ADJD", "ADV", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "ein kleine weil;", "tokens": ["ein", "klei\u00b7ne", "weil", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KOUS", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "bis ich von eurer augen lehr", "tokens": ["bis", "ich", "von", "eu\u00b7rer", "au\u00b7gen", "lehr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "und ihr von meiner seufzen m\u00e4r", "tokens": ["und", "ihr", "von", "mei\u00b7ner", "seuf\u00b7zen", "m\u00e4r"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "die schuldigkeit der lieb verstunden,", "tokens": ["die", "schul\u00b7dig\u00b7keit", "der", "lieb", "ver\u00b7stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "darauf wir heimlich, ohn unehr,", "tokens": ["da\u00b7rauf", "wir", "heim\u00b7lich", ",", "ohn", "un\u00b7ehr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "PPER", "ADJD", "$,", "KOUI", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "einander fr\u00f6lich \u00fcberwunden", "tokens": ["ein\u00b7an\u00b7der", "fr\u00f6\u00b7lich", "\u00fc\u00b7berw\u00b7un\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "ein kleine weil.", "tokens": ["ein", "klei\u00b7ne", "weil", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KOUS", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}