{"textgrid.poem.34758": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "9.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schattenk\u00fcsse, Schattenliebe,", "tokens": ["Schat\u00b7ten\u00b7k\u00fcs\u00b7se", ",", "Schat\u00b7ten\u00b7lie\u00b7be", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schattenleben, wunderbar!", "tokens": ["Schat\u00b7ten\u00b7le\u00b7ben", ",", "wun\u00b7der\u00b7bar", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Glaubst du, N\u00e4rrin, alles bliebe", "tokens": ["Glaubst", "du", ",", "N\u00e4r\u00b7rin", ",", "al\u00b7les", "blie\u00b7be"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "NN", "$,", "PIS", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unver\u00e4ndert, ewig wahr?", "tokens": ["Un\u00b7ver\u00b7\u00e4n\u00b7dert", ",", "e\u00b7wig", "wahr", "?"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Was wir lieblich fest besessen,", "tokens": ["Was", "wir", "lieb\u00b7lich", "fest", "be\u00b7ses\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADJD", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schwindet hin, wie Tr\u00e4umerein,", "tokens": ["Schwin\u00b7det", "hin", ",", "wie", "Tr\u00e4u\u00b7me\u00b7rein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "PWAV", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die Herzen, die vergessen,", "tokens": ["Und", "die", "Her\u00b7zen", ",", "die", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die Augen schlafen ein.", "tokens": ["Und", "die", "Au\u00b7gen", "schla\u00b7fen", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}