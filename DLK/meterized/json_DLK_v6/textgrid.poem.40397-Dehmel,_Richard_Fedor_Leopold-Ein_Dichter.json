{"textgrid.poem.40397": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Ein Dichter", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich kann ein Dichtersmann werden,", "tokens": ["Ich", "kann", "ein", "Dich\u00b7ters\u00b7mann", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VAINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "ich wei\u00df schon, was das hei\u00dft;", "tokens": ["ich", "wei\u00df", "schon", ",", "was", "das", "hei\u00dft", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PWS", "PDS", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "das ist ein Mensch auf Erden", "tokens": ["das", "ist", "ein", "Mensch", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "mit einem himmlischen Geist,", "tokens": ["mit", "ei\u00b7nem", "himm\u00b7li\u00b7schen", "Geist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "und der auf Leben und Tod pfeift.", "tokens": ["und", "der", "auf", "Le\u00b7ben", "und", "Tod", "pfeift", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Er pfeift: mir lacht das Leben,", "tokens": ["Er", "pfeift", ":", "mir", "lacht", "das", "Le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "weil ich unsterblich bin!", "tokens": ["weil", "ich", "uns\u00b7terb\u00b7lich", "bin", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er pfeift: ich lache aufs Sterben,", "tokens": ["Er", "pfeift", ":", "ich", "la\u00b7che", "aufs", "Ster\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "mir lebt ein Lied im Sinn,", "tokens": ["mir", "lebt", "ein", "Lied", "im", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "das geht so weit wie die Welt!", "tokens": ["das", "geht", "so", "weit", "wie", "die", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "So einen Dichter kenn ich;", "tokens": ["So", "ei\u00b7nen", "Dich\u00b7ter", "kenn", "ich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "er streicht mir manchmal die Stirn,", "tokens": ["er", "streicht", "mir", "manch\u00b7mal", "die", "Stirn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "und wie ein Fernrohr r\u00fchrt sein Blick", "tokens": ["und", "wie", "ein", "Fern\u00b7rohr", "r\u00fchrt", "sein", "Blick"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "hell an mein Gehirn,", "tokens": ["hell", "an", "mein", "Ge\u00b7hirn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "dann seh ich den Himmel offen.", "tokens": ["dann", "seh", "ich", "den", "Him\u00b7mel", "of\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Da tanzen die Sterne und singen:", "tokens": ["Da", "tan\u00b7zen", "die", "Ster\u00b7ne", "und", "sin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "KON", "VVFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "\u00bbnur wen wir auserw\u00e4hlt,", "tokens": ["\u00bb", "nur", "wen", "wir", "au\u00b7ser\u00b7w\u00e4hlt", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "dem kann das Lied gelingen!", "tokens": ["dem", "kann", "das", "Lied", "ge\u00b7lin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wird er's wohl fertig bringen,", "tokens": ["Wird", "er's", "wohl", "fer\u00b7tig", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "unser kleiner Held?\u00ab", "tokens": ["un\u00b7ser", "klei\u00b7ner", "Held", "?", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}