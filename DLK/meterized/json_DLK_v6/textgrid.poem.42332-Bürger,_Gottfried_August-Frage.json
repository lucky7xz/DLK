{"textgrid.poem.42332": {"metadata": {"author": {"name": "B\u00fcrger, Gottfried August", "birth": "N.A.", "death": "N.A."}, "title": "Frage", "genre": "verse", "period": "N.A.", "pub_year": 1786, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie? Sollt' es denn nicht besser lassen,", "tokens": ["Wie", "?", "Sollt'", "es", "denn", "nicht", "bes\u00b7ser", "las\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "VMFIN", "PPER", "ADV", "PTKNEG", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein sch\u00f6nes Bild im Musenhain,", "tokens": ["Ein", "sch\u00f6\u00b7nes", "Bild", "im", "Mu\u00b7sen\u00b7hain", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als Pfahl nur, oder Pflasterstein,", "tokens": ["Als", "Pfahl", "nur", ",", "o\u00b7der", "Pflas\u00b7ter\u00b7stein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "$,", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kaum gut genug f\u00fcr Z\u00e4un' und Gassen,", "tokens": ["Kaum", "gut", "ge\u00b7nug", "f\u00fcr", "Z\u00e4un'", "und", "Gas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "In dieser besten Welt zu sein?", "tokens": ["In", "die\u00b7ser", "bes\u00b7ten", "Welt", "zu", "sein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}