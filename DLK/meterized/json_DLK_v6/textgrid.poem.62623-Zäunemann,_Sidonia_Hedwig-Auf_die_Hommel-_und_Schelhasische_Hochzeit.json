{"textgrid.poem.62623": {"metadata": {"author": {"name": "Z\u00e4unemann, Sidonia Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "Auf die Hommel- und Schelhasische Hochzeit", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Liebe lag und schlief, da kam ihr kleiner Sohn", "tokens": ["Die", "Lie\u00b7be", "lag", "und", "schlief", ",", "da", "kam", "ihr", "klei\u00b7ner", "Sohn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "$,", "ADV", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und weckte sie vom Schlaf, und sagte: Weist dus schon,", "tokens": ["Und", "weck\u00b7te", "sie", "vom", "Schlaf", ",", "und", "sag\u00b7te", ":", "Weist", "dus", "schon", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "$,", "KON", "VVFIN", "$.", "NN", "NE", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es sind vor kurzer Zeit zwey muntre Advocaten", "tokens": ["Es", "sind", "vor", "kur\u00b7zer", "Zeit", "zwey", "mun\u00b7tre", "Ad\u00b7vo\u00b7ca\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN", "CARD", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "In einen Feder Streit, und zwar sehr scharf gerathen?", "tokens": ["In", "ei\u00b7nen", "Fe\u00b7der", "Streit", ",", "und", "zwar", "sehr", "scharf", "ge\u00b7ra\u00b7then", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,", "KON", "ADV", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Man hat ", "tokens": ["Man", "hat"], "token_info": ["word", "word"], "pos": ["PIS", "VAFIN"], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "Ob man ein Jungfer-Bild von zwanzig und mehr Jahren", "tokens": ["Ob", "man", "ein", "Jung\u00b7fer\u00b7Bild", "von", "zwan\u00b7zig", "und", "mehr", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ART", "NN", "APPR", "CARD", "KON", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Statt einer, welche schon mit reifen Silber-Haaren", "tokens": ["Statt", "ei\u00b7ner", ",", "wel\u00b7che", "schon", "mit", "rei\u00b7fen", "Sil\u00b7ber\u00b7Haa\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "$,", "PRELS", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Auf ihrem Haupte prangt, zur Liebsten suchen soll:", "tokens": ["Auf", "ih\u00b7rem", "Haup\u00b7te", "prangt", ",", "zur", "Liebs\u00b7ten", "su\u00b7chen", "soll", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,", "APPRART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ob man mit Witwen nicht vielmehr Vergn\u00fcgens-voll", "tokens": ["Ob", "man", "mit", "Wit\u00b7wen", "nicht", "viel\u00b7mehr", "Ver\u00b7gn\u00fc\u00b7gens\u00b7voll"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "APPR", "NN", "PTKNEG", "ADV", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ich mu\u00df dir alles die\u00df, Frau Mutter! recht erzehlen.", "tokens": ["Ich", "mu\u00df", "dir", "al\u00b7les", "die\u00df", ",", "Frau", "Mut\u00b7ter", "!", "recht", "er\u00b7zeh\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PIS", "PDS", "$,", "NN", "NN", "$.", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die Schriften hab ich hier; so liegt mirs auch im Sinn,", "tokens": ["Die", "Schrif\u00b7ten", "hab", "ich", "hier", ";", "so", "liegt", "mirs", "auch", "im", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "$.", "ADV", "VVFIN", "NE", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Weil ich im Sch\u00f6ppen-Stuhl der ", "tokens": ["Weil", "ich", "im", "Sch\u00f6p\u00b7pen\u00b7Stuhl", "der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Du wirst, wie dirs geh\u00f6rt, dich auch darzu bequehmen,", "tokens": ["Du", "wirst", ",", "wie", "dirs", "ge\u00b7h\u00f6rt", ",", "dich", "auch", "dar\u00b7zu", "be\u00b7queh\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PWAV", "PIS", "VVFIN", "$,", "PPER", "ADV", "PAV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Das Urtheil und den Spruch mit n\u00e4chsten vorzunehmen,", "tokens": ["Das", "Ur\u00b7theil", "und", "den", "Spruch", "mit", "n\u00e4chs\u00b7ten", "vor\u00b7zu\u00b7neh\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "APPR", "ADJA", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Er band die Acten auf. Nach diesen las er ihr", "tokens": ["Er", "band", "die", "Ac\u00b7ten", "auf", ".", "Nach", "die\u00b7sen", "las", "er", "ihr"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$.", "APPR", "PDAT", "VVFIN", "PPER", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wie kan ein Gelehrter wohl durch das Freyen gl\u00fccklich heisen?", "tokens": ["Wie", "kan", "ein", "Ge\u00b7lehr\u00b7ter", "wohl", "durch", "das", "Frey\u00b7en", "gl\u00fcck\u00b7lich", "hei\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ART", "NN", "ADV", "APPR", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+--+--+-+-+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Was er sonst vor B\u00fccher gab, mu\u00df er nun auf Trank und Speisen,", "tokens": ["Was", "er", "sonst", "vor", "B\u00fc\u00b7cher", "gab", ",", "mu\u00df", "er", "nun", "auf", "Trank", "und", "Spei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "APPR", "NN", "VVFIN", "$,", "VMFIN", "PPER", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Und auf Frauen-Hoffart wenden. Fehlt nicht ohne Unterla\u00df,", "tokens": ["Und", "auf", "Frau\u00b7en\u00b7Hoff\u00b7art", "wen\u00b7den", ".", "Fehlt", "nicht", "oh\u00b7ne", "Un\u00b7ter\u00b7la\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NE", "VVINF", "$.", "VVFIN", "PTKNEG", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Heute die\u00df in K\u00fcch und Keller, und dann morgen wieder das?", "tokens": ["Heu\u00b7te", "die\u00df", "in", "K\u00fcch", "und", "Kel\u00b7ler", ",", "und", "dann", "mor\u00b7gen", "wie\u00b7der", "das", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "APPR", "NN", "KON", "NN", "$,", "KON", "ADV", "ADV", "ADV", "PDS", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Will er nicht von seiner Frau das Gebrumm und Knattern wissen,", "tokens": ["Will", "er", "nicht", "von", "sei\u00b7ner", "Frau", "das", "Ge\u00b7brumm", "und", "Knat\u00b7tern", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "APPR", "PPOSAT", "NN", "ART", "NN", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Mu\u00df er von den B\u00fcchern gehn, und dieselbe \u00f6fters k\u00fcssen.", "tokens": ["Mu\u00df", "er", "von", "den", "B\u00fc\u00b7chern", "gehn", ",", "und", "die\u00b7sel\u00b7be", "\u00f6f\u00b7ters", "k\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$,", "KON", "PDAT", "ADV", "VVINF", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.7": {"text": "Mein! was vor ein scheel Gesichte macht sie ihm, wofern er schreibt,", "tokens": ["Mein", "!", "was", "vor", "ein", "scheel", "Ge\u00b7sich\u00b7te", "macht", "sie", "ihm", ",", "wo\u00b7fern", "er", "schreibt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "$.", "PWS", "APPR", "ART", "ADJA", "NN", "VVFIN", "PPER", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Und nicht immer bey ihr sitzet, und mit ihr die Zeit vertreibt!", "tokens": ["Und", "nicht", "im\u00b7mer", "bey", "ihr", "sit\u00b7zet", ",", "und", "mit", "ihr", "die", "Zeit", "ver\u00b7treibt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADV", "APPR", "PPER", "VVFIN", "$,", "KON", "APPR", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.9": {"text": "O! wie ist ihm das verha\u00dft, wenn die Neubegierd der Frauen", "tokens": ["O", "!", "wie", "ist", "ihm", "das", "ver\u00b7ha\u00dft", ",", "wenn", "die", "Neu\u00b7be\u00b7gierd", "der", "Frau\u00b7en"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "PWAV", "VAFIN", "PPER", "PDS", "VVFIN", "$,", "KOUS", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.10": {"text": "Alles sehn und h\u00f6ren will! ja was macht ihm das vor Grauen,", "tokens": ["Al\u00b7les", "sehn", "und", "h\u00f6\u00b7ren", "will", "!", "ja", "was", "macht", "ihm", "das", "vor", "Grau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVINF", "KON", "VVINF", "VMFIN", "$.", "ADV", "PWS", "VVFIN", "PPER", "PDS", "APPR", "NN", "$,"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.11": {"text": "Und vor Unruh und Beschwerde, wenn die Kinder um ihn seyn!", "tokens": ["Und", "vor", "Un\u00b7ruh", "und", "Be\u00b7schwer\u00b7de", ",", "wenn", "die", "Kin\u00b7der", "um", "ihn", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "KON", "NN", "$,", "KOUS", "ART", "NN", "APPR", "PPER", "VAINF", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.12": {"text": "Ihn nur in Studiren hindern, und stets in die Ohren schreyn!", "tokens": ["Ihn", "nur", "in", "Stu\u00b7di\u00b7ren", "hin\u00b7dern", ",", "und", "stets", "in", "die", "Oh\u00b7ren", "schreyn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "NN", "VVINF", "$,", "KON", "ADV", "APPR", "ART", "NN", "VAINF", "$."], "meter": "-+-+--+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "Was vor Sorge, was vor M\u00fch bringet ihm ihr Auferziehen.", "tokens": ["Was", "vor", "Sor\u00b7ge", ",", "was", "vor", "M\u00fch", "brin\u00b7get", "ihm", "ihr", "Auf\u00b7er\u00b7zie\u00b7hen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "--+-+-++-+-+-+-", "measure": "anapaest.init"}, "line.14": {"text": "Und besonders, wenn sie sich nicht um Kunst und Flei\u00df bem\u00fchen.", "tokens": ["Und", "be\u00b7son\u00b7ders", ",", "wenn", "sie", "sich", "nicht", "um", "Kunst", "und", "Flei\u00df", "be\u00b7m\u00fc\u00b7hen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "KOUS", "PPER", "PRF", "PTKNEG", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "--+--+-+-+-+-+-", "measure": "anapaest.di.plus"}, "line.15": {"text": "Darum lebt er viel vergn\u00fcgter, viel begl\u00fcckter, wenn er nicht", "tokens": ["Da\u00b7rum", "lebt", "er", "viel", "ver\u00b7gn\u00fcg\u00b7ter", ",", "viel", "be\u00b7gl\u00fcck\u00b7ter", ",", "wenn", "er", "nicht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,", "KOUS", "PPER", "PTKNEG"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.16": {"text": "An das Heyraths-Werk gedenket, und der Liebe wiederspricht.", "tokens": ["An", "das", "Hey\u00b7raths\u00b7Werk", "ge\u00b7den\u00b7ket", ",", "und", "der", "Lie\u00b7be", "wie\u00b7der\u00b7spricht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,", "KON", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.17": {"text": "Weiter: welcher ist denn wohl der nicht alte Thaler liebet,", "tokens": ["Wei\u00b7ter", ":", "wel\u00b7cher", "ist", "denn", "wohl", "der", "nicht", "al\u00b7te", "Tha\u00b7ler", "lie\u00b7bet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PWAT", "VAFIN", "ADV", "ADV", "ART", "PTKNEG", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.18": {"text": "Und dem lang gelegnen Wein vor dem neuen Vorzug giebet?", "tokens": ["Und", "dem", "lang", "ge\u00b7leg\u00b7nen", "Wein", "vor", "dem", "neu\u00b7en", "Vor\u00b7zug", "gie\u00b7bet", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJD", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.19": {"text": "Altes Bier hat keine Hefen, seine Reinlichkeit macht froh;", "tokens": ["Al\u00b7tes", "Bier", "hat", "kei\u00b7ne", "He\u00b7fen", ",", "sei\u00b7ne", "Rein\u00b7lich\u00b7keit", "macht", "froh", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "PIAT", "NN", "$,", "PPOSAT", "NN", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.20": {"text": "Warum w\u00e4rs mit alten Jungfern und mit jungen nicht auch so?", "tokens": ["Wa\u00b7rum", "w\u00e4rs", "mit", "al\u00b7ten", "Jung\u00b7fern", "und", "mit", "jun\u00b7gen", "nicht", "auch", "so", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "APPR", "ADJA", "NN", "KON", "APPR", "ADJA", "PTKNEG", "ADV", "ADV", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.21": {"text": "Graue Jungfern haben schon Glut und Vorwitz abgeleget,", "tokens": ["Grau\u00b7e", "Jung\u00b7fern", "ha\u00b7ben", "schon", "Glut", "und", "Vor\u00b7witz", "ab\u00b7ge\u00b7le\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "ADV", "NN", "KON", "NN", "VVPP", "$,"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.22": {"text": "Und davor die Sitsamkeit in die edle Brust gepr\u00e4get.", "tokens": ["Und", "da\u00b7vor", "die", "Sit\u00b7sam\u00b7keit", "in", "die", "ed\u00b7le", "Brust", "ge\u00b7pr\u00e4\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "ART", "NN", "APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.23": {"text": "Wer sie freyet darf nicht sorgen, da\u00df er k\u00fchne Schw\u00e4ger kriegt,", "tokens": ["Wer", "sie", "fre\u00b7yet", "darf", "nicht", "sor\u00b7gen", ",", "da\u00df", "er", "k\u00fch\u00b7ne", "Schw\u00e4\u00b7ger", "kriegt", ","], "token_info": ["word", "word", "word", "word",