{"textgrid.poem.48838": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "63. Als er wieder mit ihr ausges\u00f6net war", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Nebel ist vorbei, die Sonne scheinet wieder.", "tokens": ["Der", "Ne\u00b7bel", "ist", "vor\u00b7bei", ",", "die", "Son\u00b7ne", "schei\u00b7net", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$,", "ART", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mein Lieb, das zornig war, das lacht mich freundlich an,", "tokens": ["Mein", "Lieb", ",", "das", "zor\u00b7nig", "war", ",", "das", "lacht", "mich", "freund\u00b7lich", "an", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "ADJD", "VAFIN", "$,", "PDS", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "so da\u00df ich von sonst Nichts als Freude sagen kan.", "tokens": ["so", "da\u00df", "ich", "von", "sonst", "Nichts", "als", "Freu\u00b7de", "sa\u00b7gen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "APPR", "ADV", "PIS", "KOKOM", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich f\u00fchle noch den Tod durch alle meine Glieder,", "tokens": ["Ich", "f\u00fch\u00b7le", "noch", "den", "Tod", "durch", "al\u00b7le", "mei\u00b7ne", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "APPR", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "die Wangen wurden bla\u00df, die Augen sunken nieder,", "tokens": ["die", "Wan\u00b7gen", "wur\u00b7den", "bla\u00df", ",", "die", "Au\u00b7gen", "sun\u00b7ken", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "das Herze ward mir Blei. Nun denk' ich zwar daran,", "tokens": ["das", "Her\u00b7ze", "ward", "mir", "Blei", ".", "Nun", "denk'", "ich", "zwar", "da\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "VAFIN", "PPER", "NN", "$.", "ADV", "VVFIN", "PPER", "ADV", "PAV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "doch bin ich zwiefach froh, da\u00df dieses ist getan,", "tokens": ["doch", "bin", "ich", "zwie\u00b7fach", "froh", ",", "da\u00df", "die\u00b7ses", "ist", "ge\u00b7tan", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJD", "$,", "KOUS", "PDS", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "von altem Trauren matt, von neuen Freuden m\u00fcder.", "tokens": ["von", "al\u00b7tem", "Trau\u00b7ren", "matt", ",", "von", "neu\u00b7en", "Freu\u00b7den", "m\u00fc\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$,", "APPR", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Der Zucker meiner Not, das Labsal meiner Pein", "tokens": ["Der", "Zu\u00b7cker", "mei\u00b7ner", "Not", ",", "das", "Lab\u00b7sal", "mei\u00b7ner", "Pein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "und was dem Kranken sonst pflegt recht gesund zu sein,", "tokens": ["und", "was", "dem", "Kran\u00b7ken", "sonst", "pflegt", "recht", "ge\u00b7sund", "zu", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "ADV", "VVFIN", "ADV", "ADJD", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "das Alles ist mir, Schatz, dein g\u00fcldnes Angesichte.", "tokens": ["das", "Al\u00b7les", "ist", "mir", ",", "Schatz", ",", "dein", "g\u00fcld\u00b7nes", "An\u00b7ge\u00b7sich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VAFIN", "PPER", "$,", "NN", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "O Sonne meiner Lust, schein' ewig so, wie itzt.", "tokens": ["O", "Son\u00b7ne", "mei\u00b7ner", "Lust", ",", "schein'", "e\u00b7wig", "so", ",", "wie", "itzt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "PPOSAT", "NN", "$,", "VVFIN", "ADJD", "ADV", "$,", "PWAV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du bist die s\u00fc\u00dfe Glut, die meinen Geist erhitzt,", "tokens": ["Du", "bist", "die", "s\u00fc\u00b7\u00dfe", "Glut", ",", "die", "mei\u00b7nen", "Geist", "er\u00b7hitzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "von dir, Glanz, nehm' ich Schein, von dir, Licht, werd' ich lichte.", "tokens": ["von", "dir", ",", "Glanz", ",", "nehm'", "ich", "Schein", ",", "von", "dir", ",", "Licht", ",", "werd'", "ich", "lich\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "NN", "$,", "VVFIN", "PPER", "NN", "$,", "APPR", "PPER", "$,", "NN", "$,", "VAFIN", "PPER", "VVFIN", "$."], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}}}}}