{"textgrid.poem.58116": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "154. Abgeschieden, mit Gott zufrieden", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was Gott mir gibt und nimm und tut,", "tokens": ["Was", "Gott", "mir", "gibt", "und", "nimm", "und", "tut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "PPER", "VVFIN", "KON", "VVIMP", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das hei\u00df' ich immer alles gut,", "tokens": ["Das", "hei\u00df'", "ich", "im\u00b7mer", "al\u00b7les", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "PIS", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich lass' mich ihm. Herr, nur dein Wille!", "tokens": ["Ich", "lass'", "mich", "ihm", ".", "Herr", ",", "nur", "dein", "Wil\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPER", "$.", "NN", "$,", "ADV", "PPOSAT", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Drin bleib' ich eingekehrt und stille.", "tokens": ["Drin", "bleib'", "ich", "ein\u00b7ge\u00b7kehrt", "und", "stil\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVPP", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}