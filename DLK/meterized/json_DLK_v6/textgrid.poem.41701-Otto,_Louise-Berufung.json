{"textgrid.poem.41701": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "Berufung", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie sch\u00f6n war meine Kinderzeit verflossen,", "tokens": ["Wie", "sch\u00f6n", "war", "mei\u00b7ne", "Kin\u00b7der\u00b7zeit", "ver\u00b7flos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie hab' ich da im trauten Vaterhaus", "tokens": ["Wie", "hab'", "ich", "da", "im", "trau\u00b7ten", "Va\u00b7ter\u00b7haus"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Elternliebe Segen ganz genossen.", "tokens": ["Der", "El\u00b7tern\u00b7lie\u00b7be", "Se\u00b7gen", "ganz", "ge\u00b7nos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wie spielt ich froh mit V\u00f6geln und mit Bl\u00fcten", "tokens": ["Wie", "spielt", "ich", "froh", "mit", "V\u00f6\u00b7geln", "und", "mit", "Bl\u00fc\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "ADJD", "APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im Garten und im Weinberg frei umher,", "tokens": ["Im", "Gar\u00b7ten", "und", "im", "Wein\u00b7berg", "frei", "um\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPRART", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und lernte gern sie pflegen und beh\u00fcten.", "tokens": ["Und", "lern\u00b7te", "gern", "sie", "pfle\u00b7gen", "und", "be\u00b7h\u00fc\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PPER", "VVINF", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wie war es sch\u00f6n mit jenen auch zu singen,", "tokens": ["Wie", "war", "es", "sch\u00f6n", "mit", "je\u00b7nen", "auch", "zu", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADJD", "APPR", "PDAT", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ganz leis, da\u00df es kein fremdes Ohr geh\u00f6rt,", "tokens": ["Ganz", "leis", ",", "da\u00df", "es", "kein", "frem\u00b7des", "Ohr", "ge\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "PPER", "PIAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und in das Reich der Dichtung mich zu schwingen!", "tokens": ["Und", "in", "das", "Reich", "der", "Dich\u00b7tung", "mich", "zu", "schwin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch ach, es ging das s\u00fc\u00dfe Gl\u00fcck zu Ende:", "tokens": ["Doch", "ach", ",", "es", "ging", "das", "s\u00fc\u00b7\u00dfe", "Gl\u00fcck", "zu", "En\u00b7de", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$,", "PPER", "VVFIN", "ART", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bald raubte beide Eltern mir der Tod \u2013", "tokens": ["Bald", "raub\u00b7te", "bei\u00b7de", "El\u00b7tern", "mir", "der", "Tod", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "PPER", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ob ihren Gr\u00e4bern rang ich bang die H\u00e4nde.", "tokens": ["Ob", "ih\u00b7ren", "Gr\u00e4\u00b7bern", "rang", "ich", "bang", "die", "H\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN", "PPER", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Dem\u00fctig beugt ich mich in Gottes Willen \u2013", "tokens": ["De\u00b7m\u00fc\u00b7tig", "beugt", "ich", "mich", "in", "Got\u00b7tes", "Wil\u00b7len", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PRF", "APPR", "NN", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und fragte doch: warum er das gethan?", "tokens": ["Und", "frag\u00b7te", "doch", ":", "wa\u00b7rum", "er", "das", "ge\u00b7than", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$.", "PWAV", "PPER", "PDS", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mit Liedern suchte ich den Schmerz zu stillen.", "tokens": ["Mit", "Lie\u00b7dern", "such\u00b7te", "ich", "den", "Schmerz", "zu", "stil\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "In Liedern sucht ich selbst mich zu erheben", "tokens": ["In", "Lie\u00b7dern", "sucht", "ich", "selbst", "mich", "zu", "er\u00b7he\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ADV", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und fragte mich und fragte die Natur:", "tokens": ["Und", "frag\u00b7te", "mich", "und", "frag\u00b7te", "die", "Na\u00b7tur", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ward nicht von oben mir Gesang gegeben?!", "tokens": ["Ward", "nicht", "von", "o\u00b7ben", "mir", "Ge\u00b7sang", "ge\u00b7ge\u00b7ben", "?!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "APPR", "ADV", "PPER", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Und was ich Anfangs nur als Spiel erlesen:", "tokens": ["Und", "was", "ich", "An\u00b7fangs", "nur", "als", "Spiel", "er\u00b7le\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "ADV", "KOUS", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ist's nicht ein Ruf der mich von oben trifft,", "tokens": ["Ist's", "nicht", "ein", "Ruf", "der", "mich", "von", "o\u00b7ben", "trifft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "ART", "NN", "ART", "PPER", "APPR", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "F\u00fcllt mit Begeisterung mein ganzes Wesen?", "tokens": ["F\u00fcllt", "mit", "Be\u00b7geis\u00b7te\u00b7rung", "mein", "gan\u00b7zes", "We\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.8": {"line.1": {"text": "So nah ich mich des Dichtertempels Stufen.", "tokens": ["So", "nah", "ich", "mich", "des", "Dich\u00b7ter\u00b7tem\u00b7pels", "Stu\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "PRF", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Will einzig mich dem Dienst der Muse weihn,", "tokens": ["Will", "ein\u00b7zig", "mich", "dem", "Dienst", "der", "Mu\u00b7se", "weihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "PRF", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ihr bleib ich treu, denn ich vernahm ihr Rufen!", "tokens": ["Ihr", "bleib", "ich", "treu", ",", "denn", "ich", "ver\u00b7nahm", "ihr", "Ru\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$,", "KON", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}