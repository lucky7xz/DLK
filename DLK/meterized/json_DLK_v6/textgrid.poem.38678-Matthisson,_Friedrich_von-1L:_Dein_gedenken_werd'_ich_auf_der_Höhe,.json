{"textgrid.poem.38678": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dein gedenken werd' ich auf der H\u00f6he,", "genre": "verse", "period": "N.A.", "pub_year": 1792, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein gedenken werd' ich auf der H\u00f6he,", "tokens": ["Dein", "ge\u00b7den\u00b7ken", "werd'", "ich", "auf", "der", "H\u00f6\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVINF", "VAFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wo durch Birken die Ruine blickt,", "tokens": ["Wo", "durch", "Bir\u00b7ken", "die", "Ru\u00b7i\u00b7ne", "blickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "--+-++--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Dein, wenn ich des Berges Tannen sehe,", "tokens": ["Dein", ",", "wenn", "ich", "des", "Ber\u00b7ges", "Tan\u00b7nen", "se\u00b7he", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "$,", "KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wo du mir den Strau\u00df gepfl\u00fcckt!", "tokens": ["Wo", "du", "mir", "den", "Strau\u00df", "ge\u00b7pfl\u00fcckt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "ART", "NE", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wallst du, fern von diesem Paradiese,", "tokens": ["Wallst", "du", ",", "fern", "von", "die\u00b7sem", "Pa\u00b7ra\u00b7die\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "ADJD", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Heimathw\u00e4rts im ernsten Abendlicht,", "tokens": ["Hei\u00b7mat\u00b7hw\u00e4rts", "im", "erns\u00b7ten", "A\u00b7bend\u00b7licht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Dann vergi\u00df des Labequells der Wiese,", "tokens": ["Dann", "ver\u00b7gi\u00df", "des", "La\u00b7be\u00b7quells", "der", "Wie\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Dann des Pappelw\u00e4ldchens nicht!", "tokens": ["Dann", "des", "Pap\u00b7pel\u00b7w\u00e4ld\u00b7chens", "nicht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Heute schimmert uns im Alpenthale", "tokens": ["Heu\u00b7te", "schim\u00b7mert", "uns", "im", "Al\u00b7pen\u00b7tha\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Noch durch Bl\u00fcthenlaub der Berge Schnee,", "tokens": ["Noch", "durch", "Bl\u00fc\u00b7then\u00b7laub", "der", "Ber\u00b7ge", "Schnee", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Heute gr\u00fc\u00dft, bei Hespers goldnem Strale,", "tokens": ["Heu\u00b7te", "gr\u00fc\u00dft", ",", "bei", "Hes\u00b7pers", "gold\u00b7nem", "Stra\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "APPR", "NE", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Philomel' uns noch am See:", "tokens": ["Phi\u00b7lo\u00b7mel'", "uns", "noch", "am", "See", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Morgen h\u00fcllt dich schon die Nebelferne", "tokens": ["Mor\u00b7gen", "h\u00fcllt", "dich", "schon", "die", "Ne\u00b7bel\u00b7fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Jenseits jener dunklen Fichtenh\u00f6hn.", "tokens": ["Jen\u00b7seits", "je\u00b7ner", "dunk\u00b7len", "Fich\u00b7ten\u00b7h\u00f6hn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Ach! in welchem Land', auf welchem Sterne", "tokens": ["Ach", "!", "in", "wel\u00b7chem", "Land'", ",", "auf", "wel\u00b7chem", "Ster\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "$.", "APPR", "PWAT", "NN", "$,", "APPR", "PWAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Werden wir uns wiedersehn?", "tokens": ["Wer\u00b7den", "wir", "uns", "wie\u00b7der\u00b7sehn", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}