{"textgrid.poem.28224": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "39.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie ietzt die Zeiten sind, so waren vor die Zeiten;", "tokens": ["Wie", "ietzt", "die", "Zei\u00b7ten", "sind", ",", "so", "wa\u00b7ren", "vor", "die", "Zei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "VAFIN", "$,", "ADV", "VAFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dann Salomo sah auch auff Pferden Knechte reiten", "tokens": ["Dann", "Sa\u00b7lo\u00b7mo", "sah", "auch", "auff", "Pfer\u00b7den", "Knech\u00b7te", "rei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "NE", "VVFIN", "ADV", "APPR", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Hingegen F\u00fcrsten-Volck zu Fusse gehn wie Knechte,", "tokens": ["Hin\u00b7ge\u00b7gen", "F\u00fcrs\u00b7ten\u00b7Volck", "zu", "Fus\u00b7se", "gehn", "wie", "Knech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "NN", "VVINF", "KOKOM", "NN", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Nur da\u00df die Grube noch nicht ist gemacht zu rechte.", "tokens": ["Nur", "da\u00df", "die", "Gru\u00b7be", "noch", "nicht", "ist", "ge\u00b7macht", "zu", "rech\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "ADV", "PTKNEG", "VAFIN", "VVPP", "PTKZU", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "al