{"dta.poem.16191": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "245.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.85", "nl:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zum Himmel blick' empor, er ist voll heller Kerzen;", "tokens": ["Zum", "Him\u00b7mel", "blick'", "em\u00b7por", ",", "er", "ist", "voll", "hel\u00b7ler", "Ker\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PTKVZ", "$,", "PPER", "VAFIN", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kind, freudig habe Gott vor Augen und im Herzen.", "tokens": ["Kind", ",", "freu\u00b7dig", "ha\u00b7be", "Gott", "vor", "Au\u00b7gen", "und", "im", "Her\u00b7zen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "VAFIN", "NN", "APPR", "NN", "KON", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "In jedem Augenblick sollst du ihm angeh\u00f6ren,", "tokens": ["In", "je\u00b7dem", "Au\u00b7gen\u00b7blick", "sollst", "du", "ihm", "an\u00b7ge\u00b7h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VMFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das will er, doch dich nicht in deiner Freude st\u00f6ren.", "tokens": ["Das", "will", "er", ",", "doch", "dich", "nicht", "in", "dei\u00b7ner", "Freu\u00b7de", "st\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "$,", "KON", "PPER", "PTKNEG", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Er will nicht, da\u00df du sollst in stetem Bangen schweben,", "tokens": ["Er", "will", "nicht", ",", "da\u00df", "du", "sollst", "in", "ste\u00b7tem", "Ban\u00b7gen", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "$,", "KOUS", "PPER", "VMFIN", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn er ist nicht der Tod, er ist das ew'ge Leben.", "tokens": ["Denn", "er", "ist", "nicht", "der", "Tod", ",", "er", "ist", "das", "ew'\u00b7ge", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PTKNEG", "ART", "NN", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+---+-+-+-+-", "measure": "dactylic.init"}}, "stanza.4": {"line.1": {"text": "Verschlie\u00dfest du dich ihm, so dringt er doch herein,", "tokens": ["Ver\u00b7schlie\u00b7\u00dfest", "du", "dich", "ihm", ",", "so", "dringt", "er", "doch", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "PPER", "$,", "ADV", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und macht mit seinem Blitz zunicht den falschen Schein.", "tokens": ["Und", "macht", "mit", "sei\u00b7nem", "Blitz", "zu\u00b7nicht", "den", "fal\u00b7schen", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Doch nimmst du selbst ihn ein, wird er mit Lust dich n\u00e4hren,", "tokens": ["Doch", "nimmst", "du", "selbst", "ihn", "ein", ",", "wird", "er", "mit", "Lust", "dich", "n\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PPER", "PTKVZ", "$,", "VAFIN", "PPER", "APPR", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+---+-+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Und nicht dein Irdisches vernichten, nur verkl\u00e4ren.", "tokens": ["Und", "nicht", "dein", "Ir\u00b7di\u00b7sches", "ver\u00b7nich\u00b7ten", ",", "nur", "ver\u00b7kl\u00e4\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "PPOSAT", "NN", "VVINF", "$,", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Entweichen kannst du nicht, er wird dich \u00fcberschleichen;", "tokens": ["Ent\u00b7wei\u00b7chen", "kannst", "du", "nicht", ",", "er", "wird", "dich", "\u00fc\u00b7bersc\u00b7hlei\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PTKNEG", "$,", "PPER", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vergleichen mu\u00dft du dich, die Hand zum Bund ihm reichen.", "tokens": ["Ver\u00b7glei\u00b7chen", "mu\u00dft", "du", "dich", ",", "die", "Hand", "zum", "Bund", "ihm", "rei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PRF", "$,", "ART", "NN", "APPRART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Mit ihm im Kampfe, bist du nie mit dir im Frieden!", "tokens": ["Mit", "ihm", "im", "Kamp\u00b7fe", ",", "bist", "du", "nie", "mit", "dir", "im", "Frie\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPRART", "NN", "$,", "VAFIN", "PPER", "ADV", "APPR", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Im Frieden sei mit ihm, so ist der Kampf geschieden.", "tokens": ["Im", "Frie\u00b7den", "sei", "mit", "ihm", ",", "so", "ist", "der", "Kampf", "ge\u00b7schie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "APPR", "PPER", "$,", "ADV", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}