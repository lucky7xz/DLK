{"textgrid.poem.49860": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Gefunden", "genre": "verse", "period": "N.A.", "pub_year": 1845, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zeus, um die Mitte zu finden vom Erdkreis, den er beherrschte,", "tokens": ["Zeus", ",", "um", "die", "Mit\u00b7te", "zu", "fin\u00b7den", "vom", "Erd\u00b7kreis", ",", "den", "er", "be\u00b7herrschte", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUI", "ART", "NN", "PTKZU", "VVINF", "APPRART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+--+--+--+--+-+", "measure": "diphilius"}, "line.2": {"text": "Wu\u00dfte den sinnigsten Rat: kindliche Dichtung erz\u00e4hlt's:", "tokens": ["Wu\u00df\u00b7te", "den", "sin\u00b7nigs\u00b7ten", "Rat", ":", "kind\u00b7li\u00b7che", "Dich\u00b7tung", "er\u00b7z\u00e4hlt's", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$.", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Adler, ein Paar, von Morgen den einen, den andern von Abend,", "tokens": ["Ad\u00b7ler", ",", "ein", "Paar", ",", "von", "Mor\u00b7gen", "den", "ei\u00b7nen", ",", "den", "an\u00b7dern", "von", "A\u00b7bend", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "$,", "APPR", "NN", "ART", "ART", "$,", "ART", "ADJA", "APPR", "NN", "$,"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Lie\u00df er fliegen, zugleich, gegeneinander gekehrt.", "tokens": ["Lie\u00df", "er", "flie\u00b7gen", ",", "zu\u00b7gleich", ",", "ge\u00b7gen\u00b7ein\u00b7an\u00b7der", "ge\u00b7kehrt", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVINF", "$,", "ADV", "$,", "ADV", "VVPP", "$."], "meter": "+-+--+---+--+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Wo sie alsdann, gleichm\u00e4\u00dfiger Kraft mit den Fittigen strebend,", "tokens": ["Wo", "sie", "als\u00b7dann", ",", "gleich\u00b7m\u00e4\u00b7\u00dfi\u00b7ger", "Kraft", "mit", "den", "Fit\u00b7ti\u00b7gen", "stre\u00b7bend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "$,", "ADJA", "NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.6": {"text": "Trafen zusammen, da fand, was er verlangte, der Gott.", "tokens": ["Tra\u00b7fen", "zu\u00b7sam\u00b7men", ",", "da", "fand", ",", "was", "er", "ver\u00b7lang\u00b7te", ",", "der", "Gott", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "ADV", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$,", "ART", "NN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.7": {"text": "So, wo die Weisheit sich und die Sch\u00f6nheit werden begegnen,", "tokens": ["So", ",", "wo", "die", "Weis\u00b7heit", "sich", "und", "die", "Sch\u00f6n\u00b7heit", "wer\u00b7den", "be\u00b7geg\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ART", "NN", "PRF", "KON", "ART", "NN", "VAFIN", "VVINF", "$,"], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Stellet den Dreifu\u00df keck, bauet den Tempel nur auf!", "tokens": ["Stel\u00b7let", "den", "Drei\u00b7fu\u00df", "keck", ",", "bau\u00b7et", "den", "Tem\u00b7pel", "nur", "auf", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "$,", "VVFIN", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}}}}}