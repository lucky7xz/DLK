{"textgrid.poem.55469": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "50", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer m\u00fcht sich wohl im Garten dort", "tokens": ["Wer", "m\u00fcht", "sich", "wohl", "im", "Gar\u00b7ten", "dort"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "PRF", "ADV", "APPRART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und mustert jedes Beet?", "tokens": ["Und", "mus\u00b7tert", "je\u00b7des", "Beet", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er pflanzt und gie\u00dft und spricht kein", "tokens": ["Er", "pflanzt", "und", "gie\u00dft", "und", "spricht", "kein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "PIAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wort, So sch\u00f6n auch alles steht.", "tokens": ["Wort", ",", "So", "sch\u00f6n", "auch", "al\u00b7les", "steht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "ADJD", "ADV", "PIS", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Das er gepfropft und okuliert", "tokens": ["Das", "er", "ge\u00b7pfropft", "und", "o\u00b7ku\u00b7liert"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "PPER", "VVPP", "KON", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit sichrer, kluger Hand,", "tokens": ["Mit", "sich\u00b7rer", ",", "klu\u00b7ger", "Hand", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das B\u00e4umchen zart ist anspaliert", "tokens": ["Das", "B\u00e4um\u00b7chen", "zart", "ist", "ans\u00b7pa\u00b7liert"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "VAFIN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nach Ordnung und Verstand.", "tokens": ["Nach", "Ord\u00b7nung", "und", "Ver\u00b7stand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Doch sagt mir, was es hei\u00dfen soll?", "tokens": ["Doch", "sagt", "mir", ",", "was", "es", "hei\u00b7\u00dfen", "soll", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "PWS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Warum ist er so still?", "tokens": ["Wa\u00b7rum", "ist", "er", "so", "still", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-----+", "measure": "unknown.measure.single"}, "line.3": {"text": "Man sieht, ihm ist der Kopf so voll,", "tokens": ["Man", "sieht", ",", "ihm", "ist", "der", "Kopf", "so", "voll", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PPER", "VAFIN", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df er was andres will.", "tokens": ["Da\u00df", "er", "was", "and\u00b7res", "will", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "PIS", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Genug, ihm wird nicht wohl dahier,", "tokens": ["Ge\u00b7nug", ",", "ihm", "wird", "nicht", "wohl", "da\u00b7hier", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "VAFIN", "PTKNEG", "ADV", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich f\u00fcrcht, er will davon;", "tokens": ["Ich", "f\u00fcrcht", ",", "er", "will", "da\u00b7von", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VMFIN", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Er schreitet nach der Gartent\u00fcr,", "tokens": ["Er", "schrei\u00b7tet", "nach", "der", "Gar\u00b7ten\u00b7t\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und drau\u00dfen ist er schon.", "tokens": ["Und", "drau\u00b7\u00dfen", "ist", "er", "schon", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Im Felde gibt's genug zu tun,", "tokens": ["Im", "Fel\u00b7de", "gibt's", "ge\u00b7nug", "zu", "tun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo der Befreite schweift;", "tokens": ["Wo", "der", "Be\u00b7frei\u00b7te", "schweift", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Er schaut, studiert und kann nicht ruhn,", "tokens": ["Er", "schaut", ",", "stu\u00b7diert", "und", "kann", "nicht", "ruhn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "KON", "VMFIN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bis es im Kopfe reift.", "tokens": ["Bis", "es", "im", "Kop\u00b7fe", "reift", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Auf einmal hat's der Biedre los,", "tokens": ["Auf", "ein\u00b7mal", "hat's", "der", "Bied\u00b7re", "los", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VAFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie er das Beste kann:", "tokens": ["Wie", "er", "das", "Bes\u00b7te", "kann", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Der Boden r\u00fchrt sich unges\u00e4umt", "tokens": ["Der", "Bo\u00b7den", "r\u00fchrt", "sich", "un\u00b7ge\u00b7s\u00e4umt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Wechsel jedes Jahr,", "tokens": ["Im", "Wech\u00b7sel", "je\u00b7des", "Jahr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein Feld so nach dem andern keimt", "tokens": ["Ein", "Feld", "so", "nach", "dem", "an\u00b7dern", "keimt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "APPR", "ART", "ADJA", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und reift und fruchtet bar;", "tokens": ["Und", "reift", "und", "fruch\u00b7tet", "bar", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "So fruchtet's auch von Geist zu Geist", "tokens": ["So", "fruch\u00b7tet's", "auch", "von", "Geist", "zu", "Geist"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und nutzt von Ort zu Ort.", "tokens": ["Und", "nutzt", "von", "Ort", "zu", "Ort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Gewi\u00df, ihr fragt nicht, wie er hei\u00dft,", "tokens": ["Ge\u00b7wi\u00df", ",", "ihr", "fragt", "nicht", ",", "wie", "er", "hei\u00dft", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "PTKNEG", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sein Name lebe fort!", "tokens": ["Sein", "Na\u00b7me", "le\u00b7be", "fort", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}}}}