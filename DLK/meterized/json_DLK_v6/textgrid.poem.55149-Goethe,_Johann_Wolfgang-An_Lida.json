{"textgrid.poem.55149": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "An Lida", "genre": "verse", "period": "N.A.", "pub_year": 1781, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Den Einzigen, Lida, welchen du lieben kannst,", "tokens": ["Den", "Ein\u00b7zi\u00b7gen", ",", "Li\u00b7da", ",", "wel\u00b7chen", "du", "lie\u00b7ben", "kannst", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NE", "$,", "PWAT", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Forderst du ganz f\u00fcr dich, und mit Recht.", "tokens": ["For\u00b7derst", "du", "ganz", "f\u00fcr", "dich", ",", "und", "mit", "Recht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPER", "$,", "KON", "APPR", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Auch ist er einzig dein.", "tokens": ["Auch", "ist", "er", "ein\u00b7zig", "dein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Denn seit ich von dir bin,", "tokens": ["Denn", "seit", "ich", "von", "dir", "bin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "PPER", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Scheint mir des schnellsten Lebens", "tokens": ["Scheint", "mir", "des", "schnells\u00b7ten", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "L\u00e4rmende Bewegung", "tokens": ["L\u00e4r\u00b7men\u00b7de", "Be\u00b7we\u00b7gung"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+---+-", "measure": "dactylic.init"}, "line.7": {"text": "Nur ein leichter Flor, durch den ich deine Gestalt", "tokens": ["Nur", "ein", "leich\u00b7ter", "Flor", ",", "durch", "den", "ich", "dei\u00b7ne", "Ge\u00b7stalt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "$,", "APPR", "PRELS", "PPER", "PPOSAT", "NN"], "meter": "--+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.8": {"text": "Immerfort wie in Wolken erblicke:", "tokens": ["Im\u00b7mer\u00b7fort", "wie", "in", "Wol\u00b7ken", "er\u00b7bli\u00b7cke", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "APPR", "NN", "VVFIN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.9": {"text": "Sie leuchtet mir freundlich und treu,", "tokens": ["Sie", "leuch\u00b7tet", "mir", "freund\u00b7lich", "und", "treu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.10": {"text": "Wie durch des Nordlichts bewegliche Strahlen", "tokens": ["Wie", "durch", "des", "Nord\u00b7lichts", "be\u00b7weg\u00b7li\u00b7che", "Strah\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.11": {"text": "Ewige Sterne schimmern.", "tokens": ["E\u00b7wi\u00b7ge", "Ster\u00b7ne", "schim\u00b7mern", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}}}}