{"textgrid.poem.47362": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Der mitleidige Himmel", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht t\u00e4glich darf ich es wohl wagen,", "tokens": ["Nicht", "t\u00e4g\u00b7lich", "darf", "ich", "es", "wohl", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VMFIN", "PPER", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zu meinen Schwesterlein zu gehn;", "tokens": ["Zu", "mei\u00b7nen", "Schwes\u00b7ter\u00b7lein", "zu", "gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was w\u00fcrden auch die Leute sagen,", "tokens": ["Was", "w\u00fcr\u00b7den", "auch", "die", "Leu\u00b7te", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn sie mich t\u00e4glich kommen s\u00e4hn?", "tokens": ["Wenn", "sie", "mich", "t\u00e4g\u00b7lich", "kom\u00b7men", "s\u00e4hn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "So mu\u00df nach jedem Tag der Freuden", "tokens": ["So", "mu\u00df", "nach", "je\u00b7dem", "Tag", "der", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "APPR", "PIAT", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sich einen langen Trauertag", "tokens": ["Sich", "ei\u00b7nen", "lan\u00b7gen", "Trau\u00b7er\u00b7tag"], "token_info": ["word", "word", "word", "word"], "pos": ["PRF", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mein Herzchen an Erinn'rung weiden,", "tokens": ["Mein", "Herz\u00b7chen", "an", "Er\u00b7inn'\u00b7rung", "wei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was es dazu auch sagen mag.", "tokens": ["Was", "es", "da\u00b7zu", "auch", "sa\u00b7gen", "mag."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["PWS", "PPER", "PAV", "ADV", "VVFIN", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Doch da\u00df es still sein Schicksal trage,", "tokens": ["Doch", "da\u00df", "es", "still", "sein", "Schick\u00b7sal", "tra\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hilft ihm der Himmel mitleidsvoll", "tokens": ["Hilft", "ihm", "der", "Him\u00b7mel", "mit\u00b7le\u00b7ids\u00b7voll"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und macht zu einem Regentage", "tokens": ["Und", "macht", "zu", "ei\u00b7nem", "Re\u00b7gen\u00b7ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den Tag, wo ich nicht gehen soll.", "tokens": ["Den", "Tag", ",", "wo", "ich", "nicht", "ge\u00b7hen", "soll", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PPER", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ist dann der Freudentag gekehret,", "tokens": ["Ist", "dann", "der", "Freu\u00b7den\u00b7tag", "ge\u00b7keh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Schnell kehret auch der Sonnenschein", "tokens": ["Schnell", "keh\u00b7ret", "auch", "der", "Son\u00b7nen\u00b7schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und f\u00fchrt, von Lieb' und Lust verkl\u00e4ret,", "tokens": ["Und", "f\u00fchrt", ",", "von", "Lieb'", "und", "Lust", "ver\u00b7kl\u00e4\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mich nieder zu den Schwesterlein.", "tokens": ["Mich", "nie\u00b7der", "zu", "den", "Schwes\u00b7ter\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}