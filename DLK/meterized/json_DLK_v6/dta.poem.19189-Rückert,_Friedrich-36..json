{"dta.poem.19189": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "36.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Geschichte und Natur, zwei R\u00e4ume sind sie nur,", "tokens": ["Ge\u00b7schich\u00b7te", "und", "Na\u00b7tur", ",", "zwei", "R\u00e4u\u00b7me", "sind", "sie", "nur", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "CARD", "NN", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo \u00fcberall der Tod geht auf des Lebens Spur.", "tokens": ["Wo", "\u00fc\u00b7be\u00b7rall", "der", "Tod", "geht", "auf", "des", "Le\u00b7bens", "Spur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Du siehst, wohin du siehst, Zerst\u00fcckelung, Bruchst\u00fccke;", "tokens": ["Du", "siehst", ",", "wo\u00b7hin", "du", "siehst", ",", "Zer\u00b7st\u00fc\u00b7cke\u00b7lung", ",", "Bruchs\u00b7t\u00fc\u00b7cke", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das eine ist dahin, das andre noch zur\u00fccke.", "tokens": ["Das", "ei\u00b7ne", "ist", "da\u00b7hin", ",", "das", "and\u00b7re", "noch", "zu\u00b7r\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VAFIN", "PAV", "$,", "PRELS", "PIS", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Du siehst Verbindungen und f\u00fchlest eine L\u00fccke,", "tokens": ["Du", "siehst", "Ver\u00b7bin\u00b7dun\u00b7gen", "und", "f\u00fch\u00b7lest", "ei\u00b7ne", "L\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-++--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Suchest Zusammenhang und findest keine Br\u00fccke.", "tokens": ["Su\u00b7chest", "Zu\u00b7sam\u00b7men\u00b7hang", "und", "fin\u00b7dest", "kei\u00b7ne", "Br\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "VVFIN", "PIAT", "NN", "$."], "meter": "---+-+-+-+-+-", "measure": "unknown.measure.penta"}}, "stanza.4": {"line.1": {"text": "Blick' in die Sternwelt auf, damit dein Geist gesundet!", "tokens": ["Blick'", "in", "die", "Stern\u00b7welt", "auf", ",", "da\u00b7mit", "dein", "Geist", "ge\u00b7sun\u00b7det", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PTKVZ", "$,", "KOUS", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dort ist der ewige Kreis, der in sich selb sich rundet.", "tokens": ["Dort", "ist", "der", "e\u00b7wi\u00b7ge", "Kreis", ",", "der", "in", "sich", "selb", "sich", "run\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "$,", "PRELS", "APPR", "PRF", "ADJD", "PRF", "VVFIN", "$."], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "Die Ordnung droben ist, wo aufgehoben ist", "tokens": ["Die", "Ord\u00b7nung", "dro\u00b7ben", "ist", ",", "wo", "auf\u00b7ge\u00b7ho\u00b7ben", "ist"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VAFIN", "$,", "PWAV", "VVPP", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Wirrung, wo sich f\u00fcgt, was hier verschoben ist.", "tokens": ["Die", "Wir\u00b7rung", ",", "wo", "sich", "f\u00fcgt", ",", "was", "hier", "ver\u00b7scho\u00b7ben", "ist", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PRF", "VVFIN", "$,", "PRELS", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Freu dich in jeder Nacht, da\u00df Sterne niedergl\u00e4nzen", "tokens": ["Freu", "dich", "in", "je\u00b7der", "Nacht", ",", "da\u00df", "Ster\u00b7ne", "nie\u00b7der\u00b7gl\u00e4n\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "PIAT", "NN", "$,", "KOUS", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit h\u00f6hrer Hofnung Stral dein Daseyn zu erg\u00e4nzen.", "tokens": ["Mit", "h\u00f6h\u00b7rer", "Hof\u00b7nung", "Stral", "dein", "Da\u00b7seyn", "zu", "er\u00b7g\u00e4n\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}}}}}