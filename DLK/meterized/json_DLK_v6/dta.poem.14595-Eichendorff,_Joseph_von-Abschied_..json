{"dta.poem.14595": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Abschied .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Alles geht zu seiner Ruh,", "tokens": ["Al\u00b7les", "geht", "zu", "sei\u00b7ner", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wald und Welt versausen,", "tokens": ["Wald", "und", "Welt", "ver\u00b7sau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Schauernd h\u00f6rt der Wandrer zu,", "tokens": ["Schau\u00b7ernd", "h\u00f6rt", "der", "Wand\u00b7rer", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sehnt sich recht nach Hause,", "tokens": ["Sehnt", "sich", "recht", "nach", "Hau\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJD", "APPR", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Hier in Waldes gr\u00fcner Klause", "tokens": ["Hier", "in", "Wal\u00b7des", "gr\u00fc\u00b7ner", "Klau\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Herz, geh' endlich auch zur Ruh!", "tokens": ["Herz", ",", "geh'", "end\u00b7lich", "auch", "zur", "Ruh", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,",