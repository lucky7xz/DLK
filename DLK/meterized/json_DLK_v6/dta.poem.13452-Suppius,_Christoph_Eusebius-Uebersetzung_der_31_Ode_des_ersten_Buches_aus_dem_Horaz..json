{"dta.poem.13452": {"metadata": {"author": {"name": "Suppius, Christoph Eusebius", "birth": "N.A.", "death": "N.A."}, "title": "Uebersetzung  \n der 31 Ode des ersten Buches  \n aus dem Horaz.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1749", "urn": "urn:nbn:de:kobv:b4-20594-7", "language": ["de:0.99"], "booktitle": "Suppius, Christoph Eusebius: Oden und Lieder. Gotha, 1749."}, "poem": {"stanza.1": {"line.1": {"text": "Was fordert von Apollens G\u00f6ttlichkeit", "tokens": ["Was", "for\u00b7dert", "von", "A\u00b7pol\u00b7lens", "G\u00f6tt\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "APPR", "NE", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Dichter? was ists, das er bittet?", "tokens": ["Der", "Dich\u00b7ter", "?", "was", "ists", ",", "das", "er", "bit\u00b7tet", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "PWS", "VAFIN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Da er ihm neuen Wein geweiht,", "tokens": ["Da", "er", "ihm", "neu\u00b7en", "Wein", "ge\u00b7weiht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und solchen aus dem Becher sch\u00fcttet;", "tokens": ["Und", "sol\u00b7chen", "aus", "dem", "Be\u00b7cher", "sch\u00fct\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Nicht aus Sardiniens fruchtbarer L\u00e4nderey", "tokens": ["Nicht", "aus", "Sar\u00b7di\u00b7ni\u00b7ens", "frucht\u00b7ba\u00b7rer", "L\u00e4n\u00b7de\u00b7rey"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "APPR", "NE", "ADJA", "NN"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "Ein hundertf\u00e4ltiges Getraide", "tokens": ["Ein", "hun\u00b7dert\u00b7f\u00e4l\u00b7ti\u00b7ges", "Ge\u00b7trai\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Nicht angenehmes Vieh, das fett geworden sey,", "tokens": ["Nicht", "an\u00b7ge\u00b7neh\u00b7mes", "Vieh", ",", "das", "fett", "ge\u00b7wor\u00b7den", "sey", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJA", "NN", "$,", "PRELS", "ADJD", "VAPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Auf Calaberns erhitzter Waide", "tokens": ["Auf", "Ca\u00b7la\u00b7berns", "er\u00b7hitz\u00b7ter", "Wai\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Kein Geld noch indisch Elfenbein,", "tokens": ["Kein", "Geld", "noch", "in\u00b7disch", "El\u00b7fen\u00b7bein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Nicht Aecker, die der Lir sanft l\u00e4\u00dft gew\u00e4ssert seyn.", "tokens": ["Nicht", "A\u00b7e\u00b7cker", ",", "die", "der", "Lir", "sanft", "l\u00e4\u00dft", "ge\u00b7w\u00e4s\u00b7sert", "seyn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "$,", "PRELS", "ART", "NN", "ADJD", "VVFIN", "VVPP", "VAINF", "$."], "meter": "+---+-+-+-+-+", "measure": "dactylic.init"}}, "stanza.2": {"line.1": {"text": "Der, den das Gl\u00fcck den Weinwachs hat beschert,", "tokens": ["Der", ",", "den", "das", "Gl\u00fcck", "den", "Wein\u00b7wachs", "hat", "be\u00b7schert", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PRELS", "ART", "NN", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mag mit Calenschen Sicheln schneiden,", "tokens": ["Mag", "mit", "Ca\u00b7len\u00b7schen", "Si\u00b7cheln", "schnei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Die g\u00fcldnen Becher seyn geleert", "tokens": ["Die", "g\u00fcld\u00b7nen", "Be\u00b7cher", "seyn", "ge\u00b7leert"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von eines reichen Kaufmanns Freuden,", "tokens": ["Von", "ei\u00b7nes", "rei\u00b7chen", "Kauf\u00b7manns", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Vom Weine, welchen er vor syrisch Gut gekauft,", "tokens": ["Vom", "Wei\u00b7ne", ",", "wel\u00b7chen", "er", "vor", "sy\u00b7risch", "Gut", "ge\u00b7kauft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PWAT", "PPER", "APPR", "ADJD", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Er, den die G\u00f6tter selbst begnaden,", "tokens": ["Er", ",", "den", "die", "G\u00f6t\u00b7ter", "selbst", "be\u00b7gna\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Indem er vier f\u00fcnfmahl im Jahr zu Schiffe lauft,", "tokens": ["In\u00b7dem", "er", "vier", "f\u00fcnf\u00b7mahl", "im", "Jahr", "zu", "Schif\u00b7fe", "lauft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "CARD", "NN", "APPRART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Das Meer besuchet ohne Schaden,", "tokens": ["Das", "Meer", "be\u00b7su\u00b7chet", "oh\u00b7ne", "Scha\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Mich machen die Oliven satt,", "tokens": ["Mich", "ma\u00b7chen", "die", "O\u00b7li\u00b7ven", "satt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.10": {"text": "Endivien und ein erleichternder Sallat.", "tokens": ["En\u00b7di\u00b7vi\u00b7en", "und", "ein", "er\u00b7leich\u00b7tern\u00b7der", "Sal\u00b7lat", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.3": {"line.1": {"text": "So la\u00df mich denn, o Sohn der Latoe,", "tokens": ["So", "la\u00df", "mich", "denn", ",", "o", "Sohn", "der", "La\u00b7toe", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "ADV", "$,", "FM", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Zu meinem k\u00fcnftigen Erspriessen,", "tokens": ["Zu", "mei\u00b7nem", "k\u00fcnf\u00b7ti\u00b7gen", "Er\u00b7spries\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Was ich f\u00fcr mich bereitet seh,", "tokens": ["Was", "ich", "f\u00fcr", "mich", "be\u00b7rei\u00b7tet", "seh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "PPER", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich bitte dich darum geniessen!", "tokens": ["Ich", "bit\u00b7te", "dich", "da\u00b7rum", "ge\u00b7nies\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PAV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und zwar mit v\u00f6lligem und nicht geschw\u00e4chtem Geist;", "tokens": ["Und", "zwar", "mit", "v\u00f6l\u00b7li\u00b7gem", "und", "nicht", "ge\u00b7schw\u00e4ch\u00b7tem", "Geist", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ADJA", "KON", "PTKNEG", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Auch da\u00df du mich in alten Tagen", "tokens": ["Auch", "da\u00df", "du", "mich", "in", "al\u00b7ten", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPER", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Von dem, was sch\u00e4ndlich ist, was lasterhaft, befreyst,", "tokens": ["Von", "dem", ",", "was", "sch\u00e4nd\u00b7lich", "ist", ",", "was", "las\u00b7ter\u00b7haft", ",", "be\u00b7freyst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "$,", "PRELS", "ADJD", "VAFIN", "$,", "PRELS", "ADJD", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Denn la\u00df mich noch die Laute schlagen,", "tokens": ["Denn", "la\u00df", "mich", "noch", "die", "Lau\u00b7te", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Damit nicht vor des Lebens Schlu\u00df", "tokens": ["Da\u00b7mit", "nicht", "vor", "des", "Le\u00b7bens", "Schlu\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "PTKNEG", "APPR", "ART", "NN", "NN"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.10": {"text": "Ich dieses grosse Gl\u00fcck dereinst entbehren mu\u00df.", "tokens": ["Ich", "die\u00b7ses", "gros\u00b7se", "Gl\u00fcck", "de\u00b7reinst", "ent\u00b7beh\u00b7ren", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PDAT", "ADJA", "NN", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}