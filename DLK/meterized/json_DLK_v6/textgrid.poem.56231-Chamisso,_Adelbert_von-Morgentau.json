{"textgrid.poem.56231": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "Morgentau", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir wollten mit Kosen und Lieben", "tokens": ["Wir", "woll\u00b7ten", "mit", "Ko\u00b7sen", "und", "Lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "NN", "KON", "ADJA"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Genie\u00dfen der k\u00f6stlichen Nacht.", "tokens": ["Ge\u00b7nie\u00b7\u00dfen", "der", "k\u00f6st\u00b7li\u00b7chen", "Nacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Wo sind doch die Stunden geblieben?", "tokens": ["Wo", "sind", "doch", "die", "Stun\u00b7den", "ge\u00b7blie\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Es ist ja der Hahn schon erwacht.", "tokens": ["Es", "ist", "ja", "der", "Hahn", "schon", "er\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Die Sonne, die bringt viel Leiden,", "tokens": ["Die", "Son\u00b7ne", ",", "die", "bringt", "viel", "Lei\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Es weinet die scheidende Nacht;", "tokens": ["Es", "wei\u00b7net", "die", "schei\u00b7den\u00b7de", "Nacht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Ich also mu\u00df weinen und scheiden,", "tokens": ["Ich", "al\u00b7so", "mu\u00df", "wei\u00b7nen", "und", "schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "VVINF", "KON", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Es ist ja die Welt schon erwacht.", "tokens": ["Es", "ist", "ja", "die", "Welt", "schon", "er\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Ich wollt, es g\u00e4b keine Sonne,", "tokens": ["Ich", "wollt", ",", "es", "g\u00e4b", "kei\u00b7ne", "Son\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Als eben dein Auge so klar,", "tokens": ["Als", "e\u00b7ben", "dein", "Au\u00b7ge", "so", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "ADV", "ADJD", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Wir weilten in Tag und in Wonne,", "tokens": ["Wir", "weil\u00b7ten", "in", "Tag", "und", "in", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und schliefe die Welt immerdar.", "tokens": ["Und", "schlie\u00b7fe", "die", "Welt", "im\u00b7mer\u00b7dar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}