{"dta.poem.3114": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "159.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du sagst, und wei\u00dft nicht was du sagst: Vielg\u00f6tterei!", "tokens": ["Du", "sagst", ",", "und", "wei\u00dft", "nicht", "was", "du", "sagst", ":", "Viel\u00b7g\u00f6t\u00b7te\u00b7rei", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "VVFIN", "PTKNEG", "PWS", "PPER", "VVFIN", "$.", "CARD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Alsob nicht \u00fcberall ein Gott der G\u00f6tter sei.", "tokens": ["Al\u00b7sob", "nicht", "\u00fc\u00b7be\u00b7rall", "ein", "Gott", "der", "G\u00f6t\u00b7ter", "sei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ADV", "ART", "NN", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ein Gott, der \u00fcberall ist schweigend anerkannt,", "tokens": ["Ein", "Gott", ",", "der", "\u00fc\u00b7be\u00b7rall", "ist", "schwei\u00b7gend", "an\u00b7er\u00b7kannt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vorausgesetzt, wennauch mit Namen nicht benannt.", "tokens": ["Vor\u00b7aus\u00b7ge\u00b7setzt", ",", "wen\u00b7nauch", "mit", "Na\u00b7men", "nicht", "be\u00b7nannt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "APPR", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ein Gott, der still geahnt ruht hinter den Tapeten,", "tokens": ["Ein", "Gott", ",", "der", "still", "ge\u00b7ahnt", "ruht", "hin\u00b7ter", "den", "Ta\u00b7pe\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "VVPP", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Aus denen bunt hervor der G\u00f6tterchor getreten.", "tokens": ["Aus", "de\u00b7nen", "bunt", "her\u00b7vor", "der", "G\u00f6t\u00b7ter\u00b7chor", "ge\u00b7tre\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ADJD", "PTKVZ", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wie unabh\u00e4ngig auf der B\u00fchne vorn erscheine", "tokens": ["Wie", "un\u00b7ab\u00b7h\u00e4n\u00b7gig", "auf", "der", "B\u00fch\u00b7ne", "vorn", "er\u00b7schei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "APPR", "ART", "NN", "ADV", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Chor, vom Hintergrund hervor lenkt ihn der Eine.", "tokens": ["Der", "Chor", ",", "vom", "Hin\u00b7ter\u00b7grund", "her\u00b7vor", "lenkt", "ihn", "der", "Ei\u00b7ne", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPRART", "NN", "PTKVZ", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Befangen sei der Sinn von sinnlichen Gestalten,", "tokens": ["Be\u00b7fan\u00b7gen", "sei", "der", "Sinn", "von", "sinn\u00b7li\u00b7chen", "Ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch unbefangen f\u00fchlt der Geist des Geistes Walten.", "tokens": ["Doch", "un\u00b7be\u00b7fan\u00b7gen", "f\u00fchlt", "der", "Geist", "des", "Geis\u00b7tes", "Wal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Und selbst dem Geiste, der den h\u00f6chsten Geist nur ehrt,", "tokens": ["Und", "selbst", "dem", "Geis\u00b7te", ",", "der", "den", "h\u00f6chs\u00b7ten", "Geist", "nur", "ehrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$,", "PRELS", "ART", "ADJA", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erscheinen heilige Vermittler w\u00fcnschenswerth;", "tokens": ["Er\u00b7schei\u00b7nen", "hei\u00b7li\u00b7ge", "Ver\u00b7mitt\u00b7ler", "w\u00fcn\u00b7schens\u00b7werth", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Ob G\u00f6ttliches herab ins Menschliche von oben", "tokens": ["Ob", "G\u00f6tt\u00b7li\u00b7ches", "her\u00b7ab", "ins", "Menschli\u00b7che", "von", "o\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "ADV", "APPRART", "NN", "APPR", "ADV"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Entstiegen, oder dis zu jenem sich erhoben:", "tokens": ["Ent\u00b7stie\u00b7gen", ",", "o\u00b7der", "dis", "zu", "je\u00b7nem", "sich", "er\u00b7ho\u00b7ben", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "PDS", "APPR", "PDAT", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Es sei nur G\u00f6ttliches und Menschliches vermittelt;", "tokens": ["Es", "sei", "nur", "G\u00f6tt\u00b7li\u00b7ches", "und", "Menschli\u00b7ches", "ver\u00b7mit\u00b7telt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJA", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Nicht darauf kommt es an, wie es nun sei betitelt.", "tokens": ["Nicht", "da\u00b7rauf", "kommt", "es", "an", ",", "wie", "es", "nun", "sei", "be\u00b7ti\u00b7telt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PAV", "VVFIN", "PPER", "PTKVZ", "$,", "PWAV", "PPER", "ADV", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}