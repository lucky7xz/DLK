{"textgrid.poem.65692": {"metadata": {"author": {"name": "Drollinger, Carl Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Auf \u00fcberstandne Tageslast", "genre": "verse", "period": "N.A.", "pub_year": 1715, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf \u00fcberstandne Tageslast", "tokens": ["Auf", "\u00fc\u00b7bers\u00b7tand\u00b7ne", "Ta\u00b7ges\u00b7last"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Fand sich Sylvander einst begihrig nach der Rast,", "tokens": ["Fand", "sich", "Syl\u00b7van\u00b7der", "einst", "be\u00b7gih\u00b7rig", "nach", "der", "Rast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "NE", "ADV", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Erm\u00fcdet von der Arbeit M\u00e4nge;", "tokens": ["Er\u00b7m\u00fc\u00b7det", "von", "der", "Ar\u00b7beit", "M\u00e4n\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und wollte sich dem w\u00fchlenden Gedr\u00e4nge,", "tokens": ["Und", "woll\u00b7te", "sich", "dem", "w\u00fch\u00b7len\u00b7den", "Ge\u00b7dr\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Dem Kerker seiner Stadt entziehn.", "tokens": ["Dem", "Ker\u00b7ker", "sei\u00b7ner", "Stadt", "ent\u00b7ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Er floh den unmuhtsvollen Ort,", "tokens": ["Er", "floh", "den", "un\u00b7muhts\u00b7vol\u00b7len", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da uns die bange Luft beklemmet,", "tokens": ["Da", "uns", "die", "ban\u00b7ge", "Luft", "be\u00b7klem\u00b7met", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Da Maur und Wall uns unsre Freyheit hemmet,", "tokens": ["Da", "Maur", "und", "Wall", "uns", "uns\u00b7re", "Frey\u00b7heit", "hem\u00b7met", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NE", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Und satzte seinen Fu\u00df ins holde Gr\u00fcne fort.", "tokens": ["Und", "satz\u00b7te", "sei\u00b7nen", "Fu\u00df", "ins", "hol\u00b7de", "Gr\u00fc\u00b7ne", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Erst irrt er hin und her mit ungewissem Tritt,", "tokens": ["Erst", "irrt", "er", "hin", "und", "her", "mit", "un\u00b7ge\u00b7wis\u00b7sem", "Tritt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "KON", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Bis da\u00df sich seinem Aug ein sch\u00f6ner Vorwurf wiese.", "tokens": ["Bis", "da\u00df", "sich", "sei\u00b7nem", "Aug", "ein", "sch\u00f6\u00b7ner", "Vor\u00b7wurf", "wie\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PRF", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Doch f\u00fchret ihn zuletzt der unvermerkte Schritt", "tokens": ["Doch", "f\u00fch\u00b7ret", "ihn", "zu\u00b7letzt", "der", "un\u00b7ver\u00b7merk\u00b7te", "Schritt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Zu einem holden Platz, zu einem Paradise.", "tokens": ["Zu", "ei\u00b7nem", "hol\u00b7den", "Platz", ",", "zu", "ei\u00b7nem", "Pa\u00b7ra\u00b7di\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Zur Linken stund ein anmuhtsvoller Wald", "tokens": ["Zur", "Lin\u00b7ken", "stund", "ein", "an\u00b7muhts\u00b7vol\u00b7ler", "Wald"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In einer pr\u00e4chtigen Gestalt.", "tokens": ["In", "ei\u00b7ner", "pr\u00e4ch\u00b7ti\u00b7gen", "Ge\u00b7stalt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Man sah die Ba\u00fcme fast bis an die Wolken reichen.", "tokens": ["Man", "sah", "die", "Ba\u00fc\u00b7me", "fast", "bis", "an", "die", "Wol\u00b7ken", "rei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "ADV", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die etwas d\u00fcnne Zahl der gr\u00fcn-belaubten Eichen,", "tokens": ["Die", "et\u00b7was", "d\u00fcn\u00b7ne", "Zahl", "der", "gr\u00fcn\u00b7be\u00b7laub\u00b7ten", "Ei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Worauf der V\u00f6gel munters Heer", "tokens": ["Wo\u00b7rauf", "der", "V\u00f6\u00b7gel", "mun\u00b7ters", "Heer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit freyem Spielen flog, mit unbesorgtem Singen,", "tokens": ["Mit", "frey\u00b7em", "Spie\u00b7len", "flog", ",", "mit", "un\u00b7be\u00b7sorg\u00b7tem", "Sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Lie\u00df seinen Blick je mehr und mehr", "tokens": ["Lie\u00df", "sei\u00b7nen", "Blick", "je", "mehr", "und", "mehr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "ADV", "KON", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "In eine dunkle Ferne dringen,", "tokens": ["In", "ei\u00b7ne", "dunk\u00b7le", "Fer\u00b7ne", "drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "In eine schattenreiche Kluft.", "tokens": ["In", "ei\u00b7ne", "schat\u00b7ten\u00b7rei\u00b7che", "Kluft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Dort stellte sich in offner Luft", "tokens": ["Dort", "stell\u00b7te", "sich", "in", "off\u00b7ner", "Luft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Ein sch\u00f6ner H\u00fcgel dar, bedeckt mit schlanken Reben.", "tokens": ["Ein", "sch\u00f6\u00b7ner", "H\u00fc\u00b7gel", "dar", ",", "be\u00b7deckt", "mit", "schlan\u00b7ken", "Re\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Es schien, als w\u00e4r er selbst bedacht,", "tokens": ["Es", "schien", ",", "als", "w\u00e4r", "er", "selbst", "be\u00b7dacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOKOM", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Sein stolzes Haupt emporzuheben,", "tokens": ["Sein", "stol\u00b7zes", "Haupt", "em\u00b7por\u00b7zu\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Und wollte seiner Sch\u00e4tze Pracht", "tokens": ["Und", "woll\u00b7te", "sei\u00b7ner", "Sch\u00e4t\u00b7ze", "Pracht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Der Welt zu schaun und zu bewundern geben.", "tokens": ["Der", "Welt", "zu", "schaun", "und", "zu", "be\u00b7wun\u00b7dern", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "An diesen stie\u00df ein buntes Feld,", "tokens": ["An", "die\u00b7sen", "stie\u00df", "ein", "bun\u00b7tes", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Das teils mit saftigen, und teils schon reiffen Saaten", "tokens": ["Das", "teils", "mit", "saf\u00b7ti\u00b7gen", ",", "und", "teils", "schon", "reif\u00b7fen", "Saa\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "APPR", "VVINF", "$,", "KON", "ADV", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Ein pr\u00e4chtig Schauspiel vorgestellt,", "tokens": ["Ein", "pr\u00e4ch\u00b7tig", "Schau\u00b7spiel", "vor\u00b7ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Das einem Teppich \u00e4hnlich schien,", "tokens": ["Das", "ei\u00b7nem", "Tep\u00b7pich", "\u00e4hn\u00b7lich", "schien", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Worauf ein schimmernd Gold aus gr\u00fcnemTafte strahlte,", "tokens": ["Wo\u00b7rauf", "ein", "schim\u00b7mernd", "Gold", "aus", "gr\u00fc\u00b7nem", "Taf\u00b7te", "strahl\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJD", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Den hier und dar der B\u00e4ume dunkler Gr\u00fcn", "tokens": ["Den", "hier", "und", "dar", "der", "B\u00e4u\u00b7me", "dunk\u00b7ler", "Gr\u00fcn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "KON", "PAV", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Mit lieblicher Schattirung mahlte.", "tokens": ["Mit", "lieb\u00b7li\u00b7cher", "Schat\u00b7ti\u00b7rung", "mahl\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.23": {"text": "Darzwischen lief ein Tahl, in dessen Mitten", "tokens": ["Darz\u00b7wi\u00b7schen", "lief", "ein", "Tahl", ",", "in", "des\u00b7sen", "Mit\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ART", "NN", "$,", "APPR", "PRELAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Ein D\u00f6rfchen lag, voll angenemer H\u00fctten,", "tokens": ["Ein", "D\u00f6rf\u00b7chen", "lag", ",", "voll", "an\u00b7ge\u00b7ne\u00b7mer", "H\u00fct\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Die nicht nach eitler Kunst bereit,", "tokens": ["Die", "nicht", "nach", "eit\u00b7ler", "Kunst", "be\u00b7reit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "APPR", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Nicht nach gezwungnem Ma\u00df des Zirkels eingefangen;", "tokens": ["Nicht", "nach", "ge\u00b7zwung\u00b7nem", "Ma\u00df", "des", "Zir\u00b7kels", "ein\u00b7ge\u00b7fan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ADJA", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Nein, sondern hin und her zerstre\u00fct,", "tokens": ["Nein", ",", "son\u00b7dern", "hin", "und", "her", "zer\u00b7stre\u00fct", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KON", "ADV", "KON", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Wie der bequeme Brauch und Nutzen es verlangen,", "tokens": ["Wie", "der", "be\u00b7que\u00b7me", "Brauch", "und", "Nut\u00b7zen", "es", "ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "KON", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "In einer wilden Zierlichkeit.", "tokens": ["In", "ei\u00b7ner", "wil\u00b7den", "Zier\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Sylvander sah entz\u00fcckt so viele Lieblichkeit.", "tokens": ["Syl\u00b7van\u00b7der", "sah", "ent\u00b7z\u00fcckt", "so", "vie\u00b7le", "Lieb\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "VVFIN", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er legte seine m\u00fcden Glider", "tokens": ["Er", "leg\u00b7te", "sei\u00b7ne", "m\u00fc\u00b7den", "Gli\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Bey einem dicken Busche nider,", "tokens": ["Bey", "ei\u00b7nem", "di\u00b7cken", "Bu\u00b7sche", "ni\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bedeckt vor Hitz und Sonnenschein;", "tokens": ["Be\u00b7deckt", "vor", "Hitz", "und", "Son\u00b7nen\u00b7schein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und sog allda mit langen Z\u00fcgen,", "tokens": ["Und", "sog", "all\u00b7da", "mit", "lan\u00b7gen", "Z\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.6": {"text": "Zu seinem innigsten Vergn\u00fcgen,", "tokens": ["Zu", "sei\u00b7nem", "in\u00b7nigs\u00b7ten", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Des Himmels reinen Hauch, der Blumen Balsam ein.", "tokens": ["Des", "Him\u00b7mels", "rei\u00b7nen", "Hauch", ",", "der", "Blu\u00b7men", "Bal\u00b7sam", "ein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "ART", "NN", "NE", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die Gegend reizet ihn zu andachtsvollem Triebe.", "tokens": ["Die", "Ge\u00b7gend", "rei\u00b7zet", "ihn", "zu", "an\u00b7dachts\u00b7vol\u00b7lem", "Trie\u00b7be", "."], "to