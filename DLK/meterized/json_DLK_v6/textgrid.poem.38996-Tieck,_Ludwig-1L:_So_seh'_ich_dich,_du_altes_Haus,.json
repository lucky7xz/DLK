{"textgrid.poem.38996": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: So seh' ich dich, du altes Haus,", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So seh' ich dich, du altes Haus,", "tokens": ["So", "seh'", "ich", "dich", ",", "du", "al\u00b7tes", "Haus", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In dem Saal und Zimmer und Hof", "tokens": ["In", "dem", "Saal", "und", "Zim\u00b7mer", "und", "Hof"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "KON", "NN", "KON", "NN"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Ja jeder Stein", "tokens": ["Ja", "je\u00b7der", "Stein"], "token_info": ["word", "word", "word"], "pos": ["PTKANT", "PIAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Uns Geschichte lehrt:", "tokens": ["Uns", "Ge\u00b7schich\u00b7te", "lehrt", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Du alter Pallast,", "tokens": ["Du", "al\u00b7ter", "Pal\u00b7last", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Zeuge so vieler Thaten,", "tokens": ["Zeu\u00b7ge", "so", "vie\u00b7ler", "Tha\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PIAT", "NN", "$,"], "meter": "+-++-+-", "measure": "unknown.measure.tetra"}, "line.7": {"text": "So vieler Gr\u00e4uel,", "tokens": ["So", "vie\u00b7ler", "Gr\u00e4u\u00b7el", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "In dessem Zirk", "tokens": ["In", "des\u00b7sem", "Zirk"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Die edlen B\u00fcrger,", "tokens": ["Die", "ed\u00b7len", "B\u00fcr\u00b7ger", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "Die feinen F\u00fcrsten", "tokens": ["Die", "fei\u00b7nen", "F\u00fcrs\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "Gewandelt und gesprochen.", "tokens": ["Ge\u00b7wan\u00b7delt", "und", "ge\u00b7spro\u00b7chen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und Buonarotti's Werk", "tokens": ["Und", "Buo\u00b7na\u00b7rot\u00b7ti's", "Werk"], "token_info": ["word", "word", "word"], "pos": ["KON", "NE", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Mit Bandinellis Riesen", "tokens": ["Mit", "Ban\u00b7di\u00b7nel\u00b7lis", "Rie\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NE", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "H\u00e4lt draussen Wacht:", "tokens": ["H\u00e4lt", "draus\u00b7sen", "Wacht", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Dort in der Halle", "tokens": ["Dort", "in", "der", "Hal\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Prahlet der Perseus", "tokens": ["Prah\u00b7let", "der", "Per\u00b7seus"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Des wunderlichen Abentheurers,", "tokens": ["Des", "wun\u00b7der\u00b7li\u00b7chen", "A\u00b7bent\u00b7heu\u00b7rers", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Des Fechters und K\u00fcnstlers,", "tokens": ["Des", "Fech\u00b7ters", "und", "K\u00fcnst\u00b7lers", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Benvenuto Cellini.", "tokens": ["Ben\u00b7ve\u00b7nu\u00b7to", "Cel\u00b7li\u00b7ni", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Die vielbewandelte Gasse,", "tokens": ["Die", "viel\u00b7be\u00b7wan\u00b7del\u00b7te", "Gas\u00b7se", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "An ", "tokens": ["An"], "token_info": ["word"], "pos": ["APPR"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "F\u00fchrt mich zum weltber\u00fchmten Dom,", "tokens": ["F\u00fchrt", "mich", "zum", "welt\u00b7be\u00b7r\u00fchm\u00b7ten", "Dom", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Brunelleschi Denkmal.", "tokens": ["Des", "Bru\u00b7nel\u00b7le\u00b7schi", "Denk\u00b7mal", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-++-++-", "measure": "unknown.measure.tetra"}, "line.5": {"text": "Dort die erznen Thore", "tokens": ["Dort", "die", "erz\u00b7nen", "Tho\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Wundervoller Kunst.", "tokens": ["Wun\u00b7der\u00b7vol\u00b7ler", "Kunst", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "In welcher Gasse,", "tokens": ["In", "wel\u00b7cher", "Gas\u00b7se", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Vor welchem Kloster,", "tokens": ["Vor", "wel\u00b7chem", "Klos\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "In welcher Villa", "tokens": ["In", "wel\u00b7cher", "Vil\u00b7la"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PWAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Ist es stumm,", "tokens": ["Ist", "es", "stumm", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Da\u00df nicht laut die Kunst", "tokens": ["Da\u00df", "nicht", "laut", "die", "Kunst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "APPR", "ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Mit allen Stimmen riefe?", "tokens": ["Mit", "al\u00b7len", "Stim\u00b7men", "rie\u00b7fe", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wohin ich blicke", "tokens": ["Wo\u00b7hin", "ich", "bli\u00b7cke"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "PPER", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Tritt die Erinnrung auf mich zu", "tokens": ["Tritt", "die", "E\u00b7rinn\u00b7rung", "auf", "mich", "zu"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPR", "PPER", "PTKZU"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.9": {"text": "Holden und ernsten Angesichts.", "tokens": ["Hol\u00b7den", "und", "erns\u00b7ten", "An\u00b7ge\u00b7sichts", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.10": {"text": "Und wie ich den Kreis", "tokens": ["Und", "wie", "ich", "den", "Kreis"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "ART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.11": {"text": "Der Thaten und M\u00e4nner,", "tokens": ["Der", "Tha\u00b7ten", "und", "M\u00e4n\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.12": {"text": "Der geliebten K\u00fcnstler", "tokens": ["Der", "ge\u00b7lieb\u00b7ten", "K\u00fcnst\u00b7ler"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.13": {"text": "Sinnend \u00fcberschaue,", "tokens": ["Sin\u00b7nend", "\u00fc\u00b7bersc\u00b7hau\u00b7e", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.14": {"text": "Reiht sich der gro\u00dfe Dante", "tokens": ["Reiht", "sich", "der", "gro\u00b7\u00dfe", "Dan\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Dem Zuge an,", "tokens": ["Dem", "Zu\u00b7ge", "an", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.16": {"text": "Und alle blicken voll Ehrfurcht", "tokens": ["Und", "al\u00b7le", "bli\u00b7cken", "voll", "Ehr\u00b7furcht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Auf den greisen Alten,", "tokens": ["Auf", "den", "grei\u00b7sen", "Al\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.18": {"text": "Der alle belehrte,", "tokens": ["Der", "al\u00b7le", "be\u00b7lehr\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.19": {"text": "Der sie alle entz\u00fcckte,", "tokens": ["Der", "sie", "al\u00b7le", "ent\u00b7z\u00fcck\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PIS", "VVFIN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.20": {"text": "Und die Begeistrung vom Himmel rief,", "tokens": ["Und", "die", "Be\u00b7geis\u00b7trung", "vom", "Him\u00b7mel", "rief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.21": {"text": "In Beatrice's Gestalt zu wandeln.", "tokens": ["In", "Bea\u00b7tri\u00b7ce's", "Ge\u00b7stalt", "zu", "wan\u00b7deln", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "PTKZU", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}