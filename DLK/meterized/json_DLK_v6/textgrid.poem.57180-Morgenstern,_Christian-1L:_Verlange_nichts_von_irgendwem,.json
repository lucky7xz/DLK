{"textgrid.poem.57180": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Verlange nichts von irgendwem,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verlange nichts von irgendwem,", "tokens": ["Ver\u00b7lan\u00b7ge", "nichts", "von", "ir\u00b7gend\u00b7wem", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "PIS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "la\u00df jedermann sein Wesen,", "tokens": ["la\u00df", "je\u00b7der\u00b7mann", "sein", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PIS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "du bist von irgendwelcher Fehm", "tokens": ["du", "bist", "von", "ir\u00b7gend\u00b7wel\u00b7cher", "Fehm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "zum Richter nicht erlesen.", "tokens": ["zum", "Rich\u00b7ter", "nicht", "er\u00b7le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Tu still dein Werk und gib der Welt", "tokens": ["Tu", "still", "dein", "Werk", "und", "gib", "der", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADJD", "PPOSAT", "NN", "KON", "VVIMP", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "allein von deinem Frieden,", "tokens": ["al\u00b7lein", "von", "dei\u00b7nem", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "und hab dein Sach auf nichts gestellt", "tokens": ["und", "hab", "dein", "Sach", "auf", "nichts", "ge\u00b7stellt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "APPR", "PIS", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und niemanden hienieden.", "tokens": ["und", "nie\u00b7man\u00b7den", "hien\u00b7ie\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}