{"textgrid.poem.44844": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[als strenger Richter tilgst du aus]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als strenger Richter tilgst du aus,", "tokens": ["Als", "stren\u00b7ger", "Rich\u00b7ter", "tilgst", "du", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was schlecht und hinderlich,", "tokens": ["Was", "schlecht", "und", "hin\u00b7der\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So wiederhole denn den Streich", "tokens": ["So", "wie\u00b7der\u00b7ho\u00b7le", "denn", "den", "Streich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und tilg das Schlechtste \u2013 dich.", "tokens": ["Und", "tilg", "das", "Schlechts\u00b7te", "\u2013", "dich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "NE", "ART", "NN", "$(", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}