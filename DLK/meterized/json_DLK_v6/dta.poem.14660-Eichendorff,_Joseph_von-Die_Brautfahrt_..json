{"dta.poem.14660": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die Brautfahrt .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Alle fa\u00dft ein freud'ges Beben \u2014", "tokens": ["Al\u00b7le", "fa\u00dft", "ein", "freu\u00b7d'\u00b7ges", "Be\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Aber dunkler rauscht das Meer,", "tokens": ["A\u00b7ber", "dunk\u00b7ler", "rauscht", "das", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schwarze Wetter schwer sich heben,", "tokens": ["Schwar\u00b7ze", "Wet\u00b7ter", "schwer", "sich", "he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Stille wird es ringsumher,", "tokens": ["Stil\u00b7le", "wird", "es", "rings\u00b7um\u00b7her", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und nur freudiger und treuer", "tokens": ["Und", "nur", "freu\u00b7di\u00b7ger", "und", "treu\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "KON", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Steht der Ritter an dem Steuer.", "tokens": ["Steht", "der", "Rit\u00b7ter", "an", "dem", "Steu\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}