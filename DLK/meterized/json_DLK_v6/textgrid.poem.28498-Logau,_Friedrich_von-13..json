{"textgrid.poem.28498": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "13.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein innerliches Weib, ein \u00e4usserlicher Mann", "tokens": ["Ein", "in\u00b7ner\u00b7li\u00b7ches", "Weib", ",", "ein", "\u00e4us\u00b7ser\u00b7li\u00b7cher", "Mann"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist Cordicunnus; ey, obs wahr, obs seyn auch kan?", "tokens": ["Ist", "Cor\u00b7di\u00b7cun\u00b7nus", ";", "ey", ",", "obs", "wahr", ",", "obs", "seyn", "auch", "kan", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "$.", "PTKVZ", "$,", "KOUS", "ADJD", "$,", "KOUS", "PPOSAT", "ADV", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Im Hertzen steht ihm ja, was Weibern unten an.", "tokens": ["Im", "Hert\u00b7zen", "steht", "ihm", "ja", ",", "was", "Wei\u00b7bern", "un\u00b7ten", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADV", "$,", "PRELS", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}