{"textgrid.poem.37284": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "[du warst noch so ein kleines M\u00e4dchen]", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du warst noch so ein kleines M\u00e4dchen", "tokens": ["Du", "warst", "noch", "so", "ein", "klei\u00b7nes", "M\u00e4d\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von acht, neun Jahren ungef\u00e4hr,", "tokens": ["Von", "acht", ",", "neun", "Jah\u00b7ren", "un\u00b7ge\u00b7f\u00e4hr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "$,", "CARD", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da fragtest du mich vertraut und wichtig:", "tokens": ["Da", "frag\u00b7test", "du", "mich", "ver\u00b7traut", "und", "wich\u00b7tig", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wo kommen die kleinen Kinder her?", "tokens": ["Wo", "kom\u00b7men", "die", "klei\u00b7nen", "Kin\u00b7der", "her", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Als ich nach Jahren dich besuchte,", "tokens": ["Als", "ich", "nach", "Jah\u00b7ren", "dich", "be\u00b7such\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da warst du schon \u00fcber den Fall belehrt,", "tokens": ["Da", "warst", "du", "schon", "\u00fc\u00b7ber", "den", "Fall", "be\u00b7lehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Du hattest die alte vertrauliche Frage", "tokens": ["Du", "hat\u00b7test", "die", "al\u00b7te", "ver\u00b7trau\u00b7li\u00b7che", "Fra\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "ADJA", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "H\u00fcbsch praktisch gel\u00f6st und aufgekl\u00e4rt.", "tokens": ["H\u00fcbsch", "prak\u00b7tisch", "ge\u00b7l\u00f6st", "und", "auf\u00b7ge\u00b7kl\u00e4rt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "VVPP", "KON", "VVPP", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Und wieder ist die Zeit vergangen.", "tokens": ["Und", "wie\u00b7der", "ist", "die", "Zeit", "ver\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hohl ist der Zahn und ernst der Sinn.", "tokens": ["Hohl", "ist", "der", "Zahn", "und", "ernst", "der", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "KON", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nun kommt die zweite wichtige Frage:", "tokens": ["Nun", "kommt", "die", "zwei\u00b7te", "wich\u00b7ti\u00b7ge", "Fra\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wo gehen die alten Leute hin?", "tokens": ["Wo", "ge\u00b7hen", "die", "al\u00b7ten", "Leu\u00b7te", "hin", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Madam, ich habe mal vernommen,", "tokens": ["Ma\u00b7dam", ",", "ich", "ha\u00b7be", "mal", "ver\u00b7nom\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich wei\u00df nicht mehr so recht von wem:", "tokens": ["Ich", "wei\u00df", "nicht", "mehr", "so", "recht", "von", "wem", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "ADV", "ADJD", "APPR", "PWS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die praktische L\u00f6sung dieser Frage", "tokens": ["Die", "prak\u00b7ti\u00b7sche", "L\u00f6\u00b7sung", "die\u00b7ser", "Fra\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PDAT", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sei eigentlich recht unbequem.", "tokens": ["Sei", "ei\u00b7gent\u00b7lich", "recht", "un\u00b7be\u00b7quem", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}