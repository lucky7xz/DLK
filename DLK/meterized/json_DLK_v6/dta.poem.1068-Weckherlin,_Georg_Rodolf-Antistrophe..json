{"dta.poem.1068": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Antistrophe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1618", "urn": "urn:nbn:de:kobv:b4-20628-9", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Djse hat dich in jhren schutz", "tokens": ["Djse", "hat", "dich", "in", "jhren", "schutz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "PRF", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Genommen alsbald du geboren/", "tokens": ["Ge\u00b7nom\u00b7men", "als\u00b7bald", "du", "ge\u00b7bo\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KOUS", "PPER", "VVPP", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Du de\u00dfgleichen der welt zu nutz", "tokens": ["Du", "de\u00df\u00b7glei\u00b7chen", "der", "welt", "zu", "nutz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "ART", "NN", "APPR", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Sie f\u00fcr deinen glaitsman erkoren:", "tokens": ["Sie", "f\u00fcr", "dei\u00b7nen", "glaits\u00b7man", "er\u00b7ko\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Dise hat zu meyden die aa\u00df", "tokens": ["Di\u00b7se", "hat", "zu", "mey\u00b7den", "die", "aa\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PTKZU", "VVFIN", "ART", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Des kurtzen wollusts dich beraittet/", "tokens": ["Des", "kurt\u00b7zen", "wol\u00b7lusts", "dich", "be\u00b7rait\u00b7tet", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Vnd raisend auf der engen stra\u00df", "tokens": ["Vnd", "rai\u00b7send", "auf", "der", "en\u00b7gen", "stra\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Durch mehr st\u00e4t vnd l\u00e4nder belaittet/", "tokens": ["Durch", "mehr", "st\u00e4t", "vnd", "l\u00e4n\u00b7der", "be\u00b7lait\u00b7tet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "KON", "ADV", "VVPP", "$("], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.9": {"text": "Dan der Griech an wei\u00dfheit sehr gro\u00df", "tokens": ["Dan", "der", "Griech", "an", "wei\u00df\u00b7heit", "sehr", "gro\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "APPR", "NN", "ADV", "ADJD"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.10": {"text": "(von dessen zung der honig flo\u00df/", "tokens": ["(", "von", "des\u00b7sen", "zung", "der", "ho\u00b7nig", "flo\u00df", "/"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PRELAT", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Wie die schnee fr\u00fclingszeit zugehen", "tokens": ["Wie", "die", "schnee", "fr\u00fc\u00b7lings\u00b7zeit", "zu\u00b7ge\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Von dem geb\u00fcrg) jemahls gesehen.", "tokens": ["Von", "dem", "ge\u00b7b\u00fcrg", ")", "je\u00b7mahls", "ge\u00b7se\u00b7hen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$(", "ADV", "VVPP", "$."], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}}}}}