{"textgrid.poem.28624": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "39.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Braut und Br\u00e4utgam ward getraut; eh ein iedes nun empfing", "tokens": ["Braut", "und", "Br\u00e4ut\u00b7gam", "ward", "ge\u00b7traut", ";", "eh", "ein", "ie\u00b7des", "nun", "emp\u00b7fing"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NE", "VAFIN", "VVPP", "$.", "KOUS", "ART", "PIS", "ADV", "VVFIN"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Ihrer Pflichten offnes Pfand, wie gebr\u00e4uchlich, einen Ring,", "tokens": ["Ih\u00b7rer", "Pflich\u00b7ten", "off\u00b7nes", "Pfand", ",", "wie", "ge\u00b7br\u00e4uch\u00b7lich", ",", "ei\u00b7nen", "Ring", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,", "PWAV", "ADJD", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Ward die Braut noch fertig eh, als damit der Br\u00e4utigam war;", "tokens": ["Ward", "die", "Braut", "noch", "fer\u00b7tig", "eh", ",", "als", "da\u00b7mit", "der", "Br\u00e4u\u00b7ti\u00b7gam", "war", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "ADJD", "KOUS", "$,", "KOUS", "PAV", "ART", "NE", "VAFIN", "$."], "meter": "--+-+-+-++-+--+", "measure": "iambic.septa.chol"}, "line.4": {"text": "Dann der Ring war etwas eng, und der Finger dicke gar.", "tokens": ["Dann", "der", "Ring", "war", "et\u00b7was", "eng", ",", "und", "der", "Fin\u00b7ger", "di\u00b7cke", "gar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "ADV", "ADJD", "$,", "KON", "ART", "NN", "VVFIN", "ADV", "$."], "meter": "--+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Tugendsame Jungfer Braut, haltet euer Zeichen an,", "tokens": ["Tu\u00b7gend\u00b7sa\u00b7me", "Jung\u00b7fer", "Braut", ",", "hal\u00b7tet", "eu\u00b7er", "Zei\u00b7chen", "an", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$,", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Bi\u00df der Br\u00e4utgam, sprach der Pfarr, seines fertig haben kan.", "tokens": ["Bi\u00df", "der", "Br\u00e4ut\u00b7gam", ",", "sprach", "der", "Pfarr", ",", "sei\u00b7nes", "fer\u00b7tig", "ha\u00b7ben", "kan", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "$,", "VVFIN", "ART", "NN", "$,", "PPOSAT", "ADJD", "VAINF", "VMFIN", "$."], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}}}}}