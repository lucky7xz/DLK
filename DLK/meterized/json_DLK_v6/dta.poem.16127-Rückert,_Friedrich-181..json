{"dta.poem.16127": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "181.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das Leben magst du wohl vergleichen einem Feste,", "tokens": ["Das", "Le\u00b7ben", "magst", "du", "wohl", "ver\u00b7glei\u00b7chen", "ei\u00b7nem", "Fes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "ADV", "VVINF", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch nicht zur Freude sind geladen alle G\u00e4ste.", "tokens": ["Doch", "nicht", "zur", "Freu\u00b7de", "sind", "ge\u00b7la\u00b7den", "al\u00b7le", "G\u00e4s\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPRART", "NN", "VAFIN", "VVPP", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Gar manchen, scheint es, lud man nur, um die Beschwerde", "tokens": ["Gar", "man\u00b7chen", ",", "scheint", "es", ",", "lud", "man", "nur", ",", "um", "die", "Be\u00b7schwer\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVINF", "$,", "VVFIN", "PPER", "$,", "VVFIN", "PIS", "ADV", "$,", "KOUI", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu \u00fcbertragen, da\u00df die Lust den andern werde.", "tokens": ["Zu", "\u00fc\u00b7ber\u00b7tra\u00b7gen", ",", "da\u00df", "die", "Lust", "den", "an\u00b7dern", "wer\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "KOUS", "ART", "NN", "ART", "ADJA", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Den Esel lud man einst zu einem Hochzeitschmause,", "tokens": ["Den", "E\u00b7sel", "lud", "man", "einst", "zu", "ei\u00b7nem", "Hoch\u00b7zeit\u00b7schmau\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil es zu tragen Holz und Wasser gab im Hause.", "tokens": ["Weil", "es", "zu", "tra\u00b7gen", "Holz", "und", "Was\u00b7ser", "gab", "im", "Hau\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKZU", "VVINF", "NN", "KON", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Der Esel dachte stolz, geladen bin ich auch.", "tokens": ["Der", "E\u00b7sel", "dach\u00b7te", "stolz", ",", "ge\u00b7la\u00b7den", "bin", "ich", "auch", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,", "VVPP", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ja wohl, beladen mit dem Tragref und dem Schlauch.", "tokens": ["Ja", "wohl", ",", "be\u00b7la\u00b7den", "mit", "dem", "Trag\u00b7ref", "und", "dem", "Schlauch", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "$,", "VVPP", "APPR", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}