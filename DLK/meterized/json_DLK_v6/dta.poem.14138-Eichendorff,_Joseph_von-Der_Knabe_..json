{"dta.poem.14138": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der Knabe .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Nun mu\u00df der Knabe einsam geh'n,", "tokens": ["Nun", "mu\u00df", "der", "Kna\u00b7be", "ein\u00b7sam", "geh'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Klagt \u00fcber Thal und H\u00fcgel:", "tokens": ["Klagt", "\u00fc\u00b7ber", "Thal", "und", "H\u00fc\u00b7gel", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u201es\u00fc\u00df' Lieb', s\u00fc\u00df' Lieb', wie bist Du sch\u00f6n:", "tokens": ["\u201e", "s\u00fc\u00df'", "Lieb'", ",", "s\u00fc\u00df'", "Lieb'", ",", "wie", "bist", "Du", "sch\u00f6n", ":"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "NN", "$,", "ADJA", "NN", "$,", "PWAV", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ach, h\u00e4ttst Du keine Fl\u00fcgel!\u201c \u2014", "tokens": ["Ach", ",", "h\u00e4ttst", "Du", "kei\u00b7ne", "Fl\u00fc\u00b7gel", "!", "\u201c"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ITJ", "$,", "VAFIN", "PPER", "PIAT", "NN", "$.", "$(", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}