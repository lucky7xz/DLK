{"dta.poem.9754": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Eines schusters.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich konte ziehen lang das leder mit den z\u00e4hnen/", "tokens": ["Ich", "kon\u00b7te", "zie\u00b7hen", "lang", "das", "le\u00b7der", "mit", "den", "z\u00e4h\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "ADJD", "ART", "ADJA", "APPR", "ART", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn in der arbeit es nicht langen wolte zu;", "tokens": ["Wenn", "in", "der", "ar\u00b7beit", "es", "nicht", "lan\u00b7gen", "wol\u00b7te", "zu", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "PPER", "PTKNEG", "VVINF", "VMFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das leben aber nicht mir etwas l\u00e4nger dehnen/", "tokens": ["Das", "le\u00b7ben", "a\u00b7ber", "nicht", "mir", "et\u00b7was", "l\u00e4n\u00b7ger", "deh\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "PTKNEG", "PPER", "ADV", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da mirs doch lieber war/ als alle meine schuh.", "tokens": ["Da", "mirs", "doch", "lie\u00b7ber", "war", "/", "als", "al\u00b7le", "mei\u00b7ne", "schuh", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "ADV", "ADV", "VAFIN", "$(", "KOUS", "PIS", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}