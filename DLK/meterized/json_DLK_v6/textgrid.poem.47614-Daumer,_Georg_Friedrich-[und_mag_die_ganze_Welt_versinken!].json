{"textgrid.poem.47614": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[und mag die ganze Welt versinken!]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.85", "af:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und mag die ganze Welt versinken!", "tokens": ["Und", "mag", "die", "gan\u00b7ze", "Welt", "ver\u00b7sin\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hafis, mit dir, mit dir allein", "tokens": ["Ha\u00b7fis", ",", "mit", "dir", ",", "mit", "dir", "al\u00b7lein"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "APPR", "PPER", "$,", "APPR", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Will ich wetteifern; Lust und Pein", "tokens": ["Will", "ich", "wett\u00b7ei\u00b7fern", ";", "Lust", "und", "Pein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "PPER", "VVINF", "$.", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sei uns, den Zwillingen, gemein!", "tokens": ["Sei", "uns", ",", "den", "Zwil\u00b7lin\u00b7gen", ",", "ge\u00b7mein", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "ART", "NN", "$,", "ADJD", "$."], "meter": "-+-++--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Wie du, zu lieben und zu trinken,", "tokens": ["Wie", "du", ",", "zu", "lie\u00b7ben", "und", "zu", "trin\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das soll mein Stolz, mein Leben sein.", "tokens": ["Das", "soll", "mein", "Stolz", ",", "mein", "Le\u00b7ben", "sein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}