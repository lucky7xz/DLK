{"textgrid.poem.33021": {"metadata": {"author": {"name": "Flaischlen, C\u00e4sar", "birth": "N.A.", "death": "N.A."}, "title": "1L: Als \u00dcberwinder, nicht als Leider,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als \u00dcberwinder, nicht als Leider,", "tokens": ["Als", "\u00dc\u00b7berw\u00b7in\u00b7der", ",", "nicht", "als", "Lei\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "PTKNEG", "KOUS", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "als Vollender, nicht als Streiter,", "tokens": ["als", "Voll\u00b7en\u00b7der", ",", "nicht", "als", "Strei\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "PTKNEG", "KOUS", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "K\u00f6nig, die Krone auf dem Haupt,", "tokens": ["K\u00f6\u00b7nig", ",", "die", "Kro\u00b7ne", "auf", "dem", "Haupt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "als Sieger mu\u00df kommen", "tokens": ["als", "Sie\u00b7ger", "mu\u00df", "kom\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "NN", "VMFIN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "mit flatternden Fahnen ...", "tokens": ["mit", "flat\u00b7tern\u00b7den", "Fah\u00b7nen", "..."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "wer uns erl\u00f6sen will und befrein!", "tokens": ["wer", "uns", "er\u00b7l\u00f6\u00b7sen", "will", "und", "be\u00b7fr\u00b7ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVINF", "VMFIN", "KON", "ADJD", "$."], "meter": "---+-+--+-", "measure": "iambic.tri.relaxed"}}}}}