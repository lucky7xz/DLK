{"textgrid.poem.55084": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Ritter Kurts Brautfahrt", "genre": "verse", "period": "N.A.", "pub_year": 1802, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit des Br\u00e4utigams Behagen", "tokens": ["Mit", "des", "Br\u00e4u\u00b7ti\u00b7gams", "Be\u00b7ha\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NE", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schwingt sich Ritter Kurt aufs Ro\u00df;", "tokens": ["Schwingt", "sich", "Rit\u00b7ter", "Kurt", "aufs", "Ro\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "NE", "NE", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Zu der Trauung soll's ihn tragen,", "tokens": ["Zu", "der", "Trau\u00b7ung", "soll's", "ihn", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf der edlen Liebsten Schlo\u00df:", "tokens": ["Auf", "der", "ed\u00b7len", "Liebs\u00b7ten", "Schlo\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Als am \u00f6den Felsenorte", "tokens": ["Als", "am", "\u00f6\u00b7den", "Fel\u00b7sen\u00b7or\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Drohend sich ein Gegner naht;", "tokens": ["Dro\u00b7hend", "sich", "ein", "Geg\u00b7ner", "naht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ohne Z\u00f6gern, ohne Worte", "tokens": ["Oh\u00b7ne", "Z\u00f6\u00b7gern", ",", "oh\u00b7ne", "Wor\u00b7te"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "$,", "KOUI", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Schreiten sie zu rascher Tat.", "tokens": ["Schrei\u00b7ten", "sie", "zu", "ra\u00b7scher", "Tat", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Lange schwankt des Kampfes Welle,", "tokens": ["Lan\u00b7ge", "schwankt", "des", "Kamp\u00b7fes", "Wel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bis sich Kurt im Siege freut;", "tokens": ["Bis", "sich", "Kurt", "im", "Sie\u00b7ge", "freut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "NE", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Er entfernt sich von der Stelle,", "tokens": ["Er", "ent\u00b7fernt", "sich", "von", "der", "Stel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00dcberwinder und gebleut.", "tokens": ["\u00dc\u00b7berw\u00b7in\u00b7der", "und", "ge\u00b7bleut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "KON", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Aber was er bald gewahret", "tokens": ["A\u00b7ber", "was", "er", "bald", "ge\u00b7wah\u00b7ret"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "PPER", "ADV", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "In des Busches Zitterschein!", "tokens": ["In", "des", "Bu\u00b7sches", "Zit\u00b7ter\u00b7schein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Mit dem S\u00e4ugling still gepaaret", "tokens": ["Mit", "dem", "S\u00e4ug\u00b7ling", "still", "ge\u00b7paa\u00b7ret"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJD", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Schleicht ein Liebchen durch den Hain.", "tokens": ["Schleicht", "ein", "Lieb\u00b7chen", "durch", "den", "Hain", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und sie winkt ihm auf das Pl\u00e4tzchen:", "tokens": ["Und", "sie", "winkt", "ihm", "auf", "das", "Pl\u00e4tz\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00bblieber Herr, nicht so geschwind!", "tokens": ["\u00bb", "lie\u00b7ber", "Herr", ",", "nicht", "so", "ge\u00b7schwind", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "NN", "$,", "PTKNEG", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Habt Ihr nichts an Euer Sch\u00e4tzchen,", "tokens": ["Habt", "Ihr", "nichts", "an", "Eu\u00b7er", "Sch\u00e4tz\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PIS", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Habt Ihr nichts f\u00fcr Euer Kind?\u00ab", "tokens": ["Habt", "Ihr", "nichts", "f\u00fcr", "Eu\u00b7er", "Kind", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPER", "PIS", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ihn durchgl\u00fchet s\u00fc\u00dfe Flamme,", "tokens": ["Ihn", "durch\u00b7gl\u00fc\u00b7het", "s\u00fc\u00b7\u00dfe", "Flam\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df er nicht vorbeibegehrt,", "tokens": ["Da\u00df", "er", "nicht", "vor\u00b7bei\u00b7be\u00b7gehrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und er findet nun die Amme,", "tokens": ["Und", "er", "fin\u00b7det", "nun", "die", "Am\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wie die Jungfrau, liebenswert.", "tokens": ["Wie", "die", "Jung\u00b7frau", ",", "lie\u00b7bens\u00b7wert", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch er h\u00f6rt die Diener blasen,", "tokens": ["Doch", "er", "h\u00f6rt", "die", "Die\u00b7ner", "bla\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Denket nun der hohen Braut,", "tokens": ["Den\u00b7ket", "nun", "der", "ho\u00b7hen", "Braut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und nun wird auf seinen Stra\u00dfen", "tokens": ["Und", "nun", "wird", "auf", "sei\u00b7nen", "Stra\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jahresfest und Markt so laut,", "tokens": ["Jah\u00b7res\u00b7fest", "und", "Markt", "so", "laut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und er w\u00e4hlet in den Buden", "tokens": ["Und", "er", "w\u00e4h\u00b7let", "in", "den", "Bu\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Manches Pfand zu Lieb und Huld;", "tokens": ["Man\u00b7ches", "Pfand", "zu", "Lieb", "und", "Huld", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Aber ach! da kommen Juden", "tokens": ["A\u00b7ber", "ach", "!", "da", "kom\u00b7men", "Ju\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "$.", "ADV", "VVFIN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Mit dem Schein vertagter Schuld.", "tokens": ["Mit", "dem", "Schein", "ver\u00b7tag\u00b7ter", "Schuld", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Und nun halten die Gerichte", "tokens": ["Und", "nun", "hal\u00b7ten", "die", "Ge\u00b7rich\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Den behenden Ritter auf.", "tokens": ["Den", "be\u00b7hen\u00b7den", "Rit\u00b7ter", "auf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "O verteufelte Geschichte!", "tokens": ["O", "ver\u00b7teu\u00b7fel\u00b7te", "Ge\u00b7schich\u00b7te", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.4": {"text": "Heldenhafter Lebenslauf!", "tokens": ["Hel\u00b7den\u00b7haf\u00b7ter", "Le\u00b7bens\u00b7lauf", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Soll ich heute mich gedulden?", "tokens": ["Soll", "ich", "heu\u00b7te", "mich", "ge\u00b7dul\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die Verlegenheit ist gro\u00df.", "tokens": ["Die", "Ver\u00b7le\u00b7gen\u00b7heit", "ist", "gro\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Widersacher, Weiber, Schulden,", "tokens": ["Wi\u00b7der\u00b7sa\u00b7cher", ",", "Wei\u00b7ber", ",", "Schul\u00b7den", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ach! kein Ritter wird sie los.", "tokens": ["Ach", "!", "kein", "Rit\u00b7ter", "wird", "sie", "los", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PIAT", "NN", "VAFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}