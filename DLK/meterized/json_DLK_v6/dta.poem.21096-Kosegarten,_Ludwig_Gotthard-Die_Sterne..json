{"dta.poem.21096": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Die Sterne.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193300", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie wohl ist mir im Dunkeln!               ", "tokens": ["Wie", "wohl", "ist", "mir", "im", "Dun\u00b7keln", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie weht die laue Nacht!", "tokens": ["Wie", "weht", "die", "lau\u00b7e", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Sterne Gottes funkeln", "tokens": ["Die", "Ster\u00b7ne", "Got\u00b7tes", "fun\u00b7keln"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In feyerlicher Pracht!", "tokens": ["In", "fey\u00b7er\u00b7li\u00b7cher", "Pracht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Komm, Ida, komm ins Freye,", "tokens": ["Komm", ",", "I\u00b7da", ",", "komm", "ins", "Frey\u00b7e", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NE", "$,", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und lass in jene Bl\u00e4ue", "tokens": ["Und", "lass", "in", "je\u00b7ne", "Bl\u00e4u\u00b7e"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und lass zu jenen H\u00f6hn", "tokens": ["Und", "lass", "zu", "je\u00b7nen", "H\u00f6hn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Uns staunend aufw\u00e4rts sehn.", "tokens": ["Uns", "stau\u00b7nend", "auf\u00b7w\u00e4rts", "sehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADV", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sieh, wie die ", "tokens": ["Sieh", ",", "wie", "die"], "token_info": ["word", "punct", "word", "word"], "pos": ["NE", "$,", "PWAV", "ART"], "meter": "+--", "measure": "dactylic.init"}, "line.2": {"text": "Sieh, wie der ", "tokens": ["Sieh", ",", "wie", "der"], "token_info": ["word", "punct", "word", "word"], "pos": ["NE", "$,", "PWAV", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Sieh, wie die ", "tokens": ["Sieh", ",", "wie", "die"], "token_info": ["word", "punct", "word", "word"], "pos": ["NE", "$,", "PWAV", "ART"], "meter": "+--", "measure": "dactylic.init"}, "line.4": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Die hellen ", "tokens": ["Die", "hel\u00b7len"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Die goldnen ", "tokens": ["Die", "gold\u00b7nen"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Und stolz durchschwimmt der ", "tokens": ["Und", "stolz", "durch\u00b7schwimmt", "der"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Den blauen Ocean.", "tokens": ["Den", "blau\u00b7en", "O\u00b7cean", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "O Sterne Gottes, Zeugen", "tokens": ["O", "Ster\u00b7ne", "Got\u00b7tes", ",", "Zeu\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["NE", "NN", "NN", "$,", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und Boten bessrer Welt,", "tokens": ["Und", "Bo\u00b7ten", "bess\u00b7rer", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr heisst den Aufruhr schweigen,", "tokens": ["Ihr", "heisst", "den", "Auf\u00b7ruhr", "schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der unsern Busen schwellt.", "tokens": ["Der", "un\u00b7sern", "Bu\u00b7sen", "schwellt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich seh' hinauf, ihr Hehren,", "tokens": ["Ich", "seh'", "hin\u00b7auf", ",", "ihr", "Heh\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Zu euren lichten Sph\u00e4ren,", "tokens": ["Zu", "eu\u00b7ren", "lich\u00b7ten", "Sph\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und Ahnung bessrer Lust", "tokens": ["Und", "Ah\u00b7nung", "bess\u00b7rer", "Lust"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Stillt die emp\u00f6rte Brust.", "tokens": ["Stillt", "die", "em\u00b7p\u00f6r\u00b7te", "Brust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "O Ida, wenn die Schwermuth", "tokens": ["O", "I\u00b7da", ",", "wenn", "die", "Schwer\u00b7muth"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NE", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dein sanftes Auge h\u00fcllt,", "tokens": ["Dein", "sanf\u00b7tes", "Au\u00b7ge", "h\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn dir die Welt mit Wermuth", "tokens": ["Wenn", "dir", "die", "Welt", "mit", "Wer\u00b7muth"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Den Lebensbecher f\u00fcllt;", "tokens": ["Den", "Le\u00b7bens\u00b7be\u00b7cher", "f\u00fcllt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "So geh hinaus im Dunkeln,", "tokens": ["So", "geh", "hin\u00b7aus", "im", "Dun\u00b7keln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "APPRART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und sieh die Sterne funkeln,", "tokens": ["Und", "sieh", "die", "Ster\u00b7ne", "fun\u00b7keln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und leiser wird dein Schmerz,", "tokens": ["Und", "lei\u00b7ser", "wird", "dein", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Und freyer schl\u00e4gt dein Herz.", "tokens": ["Und", "frey\u00b7er", "schl\u00e4gt", "dein", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und wenn im \u00f6den Staube", "tokens": ["Und", "wenn", "im", "\u00f6\u00b7den", "Stau\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der irre Geist erkrankt;", "tokens": ["Der", "ir\u00b7re", "Geist", "er\u00b7krankt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn tief in dir der Glaube", "tokens": ["Wenn", "tief", "in", "dir", "der", "Glau\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "APPR", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "An Gott und Zukunft schwankt;", "tokens": ["An", "Gott", "und", "Zu\u00b7kunft", "schwankt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Schau auf zu jenen Fernen", "tokens": ["Schau", "auf", "zu", "je\u00b7nen", "Fer\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "APPR", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Zu jenen ewgen Sternen!", "tokens": ["Zu", "je\u00b7nen", "ew\u00b7gen", "Ster\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Schau auf und glaub' an Gott,", "tokens": ["Schau", "auf", "und", "glaub'", "an", "Gott", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "KON", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Und segne Grab und Tod.", "tokens": ["Und", "seg\u00b7ne", "Grab", "und", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "O Ida, wenn die Strenge", "tokens": ["O", "I\u00b7da", ",", "wenn", "die", "Stren\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NE", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Des Schicksals einst uns trennt,", "tokens": ["Des", "Schick\u00b7sals", "einst", "uns", "trennt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wenn das Weltgedr\u00e4nge", "tokens": ["Und", "wenn", "das", "Welt\u00b7ge\u00b7dr\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nicht Blick noch Kuss uns g\u00f6nnt;", "tokens": ["Nicht", "Blick", "noch", "Kuss", "uns", "g\u00f6nnt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "ADV", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "So schau hinauf ins Freye,", "tokens": ["So", "schau", "hin\u00b7auf", "ins", "Frey\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "In jene weite Bl\u00e4ue!", "tokens": ["In", "je\u00b7ne", "wei\u00b7te", "Bl\u00e4u\u00b7e", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "In jenen lichten H\u00f6hn,", "tokens": ["In", "je\u00b7nen", "lich\u00b7ten", "H\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Dort, dort ist Wiedersehn!", "tokens": ["Dort", ",", "dort", "ist", "Wie\u00b7der\u00b7sehn", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VAFIN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Und wenn ich einst, o Theure,", "tokens": ["Und", "wenn", "ich", "einst", ",", "o", "Theu\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "$,", "FM", "FM", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von allem Kampf und Krieg,", "tokens": ["Von", "al\u00b7lem", "Kampf", "und", "Krieg", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Im stillen Grabe feyre,", "tokens": ["Im", "stil\u00b7len", "Gra\u00b7be", "fey\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So schau' empor und sprich:", "tokens": ["So", "schau'", "em\u00b7por", "und", "sprich", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKVZ", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "\u201ein jenen hohen Fernen,", "tokens": ["\u201e", "in", "je\u00b7nen", "ho\u00b7hen", "Fer\u00b7nen", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PDAT", "ADJA", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "\u201eauf jenen goldnen Sternen,", "tokens": ["\u201e", "auf", "je\u00b7nen", "gold\u00b7nen", "Ster\u00b7nen", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "\u201edort, wo's am hellsten blitzt,", "tokens": ["\u201e", "dort", ",", "wo's", "am", "hells\u00b7ten", "blitzt", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$,", "NE", "APPRART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "\u201ewallt mein Verlorner itzt.\u201c", "tokens": ["\u201e", "wallt", "mein", "Ver\u00b7lor\u00b7ner", "itzt", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "PPOSAT", "NN", "ADV", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "O Sterne Gottes, Boten", "tokens": ["O", "Ster\u00b7ne", "Got\u00b7tes", ",", "Bo\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["NE", "NN", "NN", "$,", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und B\u00fcrger bessrer Welt,", "tokens": ["Und", "B\u00fcr\u00b7ger", "bess\u00b7rer", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die ihr die Nacht der Todten", "tokens": ["Die", "ihr", "die", "Nacht", "der", "Tod\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu milder D\u00e4mmrung hellt!", "tokens": ["Zu", "mil\u00b7der", "D\u00e4mm\u00b7rung", "hellt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Umschimmert sanft die St\u00e4tte,", "tokens": ["Um\u00b7schim\u00b7mert", "sanft", "die", "St\u00e4t\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wo ich aus stillem Bette", "tokens": ["Wo", "ich", "aus", "stil\u00b7lem", "Bet\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und s\u00fcssem Schlaf erwach", "tokens": ["Und", "s\u00fcs\u00b7sem", "Schlaf", "er\u00b7wach"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Zu Edens sch\u00f6nerm Tag!", "tokens": ["Zu", "E\u00b7dens", "sch\u00f6\u00b7nerm", "Tag", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}