{"textgrid.poem.42798": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Antwort auf einen Brief des Malers Oskar Coester", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Wort auf das, was du gesprochen.", "tokens": ["Ein", "Wort", "auf", "das", ",", "was", "du", "ge\u00b7spro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "$,", "PWS", "PPER", "VVPP", "$."], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.2": {"text": "St\u00fctz guten Kopf in gute Hand", "tokens": ["St\u00fctz", "gu\u00b7ten", "Kopf", "in", "gu\u00b7te", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und la\u00df dein Herz ans Weinglas pochen:", "tokens": ["Und", "la\u00df", "dein", "Herz", "ans", "Wein\u00b7glas", "po\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPOSAT", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Heimat ist kein begrenztes Land.", "tokens": ["Hei\u00b7mat", "ist", "kein", "be\u00b7grenz\u00b7tes", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PIAT", "ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Auch wo man Muttersprache spricht,", "tokens": ["Auch", "wo", "man", "Mut\u00b7ter\u00b7spra\u00b7che", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "PIS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ist Heimat nicht.", "tokens": ["Ist", "Hei\u00b7mat", "nicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PTKNEG", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Mich deucht, es will auch nichts besagen,", "tokens": ["Mich", "deucht", ",", "es", "will", "auch", "nichts", "be\u00b7sa\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VMFIN", "ADV", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ob einer seine Heimat kennt.", "tokens": ["Ob", "ei\u00b7ner", "sei\u00b7ne", "Hei\u00b7mat", "kennt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Denn L\u00fcge ist, was auf Befragen", "tokens": ["Denn", "L\u00fc\u00b7ge", "ist", ",", "was", "auf", "Be\u00b7fra\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "NN", "VAFIN", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das Heimweh uns als Heimat nennt.", "tokens": ["Das", "Heim\u00b7weh", "uns", "als", "Hei\u00b7mat", "nennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "KOUS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ein schmutzig Loch kann r\u00fchrend sich verkneifen,", "tokens": ["Ein", "schmut\u00b7zig", "Loch", "kann", "r\u00fch\u00b7rend", "sich", "ver\u00b7knei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VMFIN", "VVPP", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und h\u00f6chste W\u00fcrde kann zur Blase reifen.", "tokens": ["Und", "h\u00f6chs\u00b7te", "W\u00fcr\u00b7de", "kann", "zur", "Bla\u00b7se", "rei\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VMFIN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Stich fest in das Humorische!", "tokens": ["Stich", "fest", "in", "das", "Hu\u00b7mo\u00b7ri\u00b7sche", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Heimat? Wir alle finden keine,", "tokens": ["Hei\u00b7mat", "?", "Wir", "al\u00b7le", "fin\u00b7den", "kei\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "PIS", "VVFIN", "PIAT", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Oder \u2013 und allerh\u00f6chstens \u2013 eine", "tokens": ["O\u00b7der", "\u2013", "und", "al\u00b7ler\u00b7h\u00f6chs\u00b7tens", "\u2013", "ei\u00b7ne"], "token_info": ["word", "punct", "word", "word", "punct", "word"], "pos": ["KON", "$(", "KON", "ADV", "$(", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Improvisatorische.", "tokens": ["Im\u00b7pro\u00b7vi\u00b7sa\u00b7to\u00b7ri\u00b7sche", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.4": {"text": "Es kommt auch gar nicht darauf an. \u2013 \u2013", "tokens": ["Es", "kommt", "auch", "gar", "nicht", "da\u00b7rauf", "an", ".", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "PTKNEG", "PAV", "PTKVZ", "$.", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Ich danke dir f\u00fcr den Vergleich", "tokens": ["Ich", "dan\u00b7ke", "dir", "f\u00fcr", "den", "Ver\u00b7gleich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Mit einem braven Reitersmann.", "tokens": ["Mit", "ei\u00b7nem", "bra\u00b7ven", "Rei\u00b7ters\u00b7mann", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Man tue m\u00f6glichst, was man kann.", "tokens": ["Man", "tue", "m\u00f6g\u00b7lichst", ",", "was", "man", "kann", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "$,", "PRELS", "PIS", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Coester, du bist von Gott aus reich.", "tokens": ["Coes\u00b7ter", ",", "du", "bist", "von", "Gott", "aus", "reich", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "APPR", "NN", "APPR", "ADJD", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Sch\u00e4um aus, was du zu schenken hast;", "tokens": ["Sch\u00e4um", "aus", ",", "was", "du", "zu", "schen\u00b7ken", "hast", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "PWS", "PPER", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Letzte w\u00e4re dir noch Last.", "tokens": ["Das", "Letz\u00b7te", "w\u00e4\u00b7re", "dir", "noch", "Last", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "PPER", "ADV", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und warte frech, doch fromm auf Leiden.", "tokens": ["Und", "war\u00b7te", "frech", ",", "doch", "fromm", "auf", "Lei\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$,", "ADV", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Denn du w\u00e4chst neben dem Jahrhundert.", "tokens": ["Denn", "du", "w\u00e4chst", "ne\u00b7ben", "dem", "Jahr\u00b7hun\u00b7dert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-++-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Du bist der gr\u00f6\u00dfre von uns beiden.", "tokens": ["Du", "bist", "der", "gr\u00f6\u00df\u00b7re", "von", "uns", "bei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "APPR", "PPER", "PIAT", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich habe dich so oft bewundert. \u2013", "tokens": ["Ich", "ha\u00b7be", "dich", "so", "oft", "be\u00b7wun\u00b7dert", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADV", "VVPP", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie kl\u00e4glich ist es zu beneiden. \u2013", "tokens": ["Wie", "kl\u00e4g\u00b7lich", "ist", "es", "zu", "be\u00b7nei\u00b7den", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "PTKZU", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Du wurdest leider mir von fern", "tokens": ["Du", "wur\u00b7dest", "lei\u00b7der", "mir", "von", "fern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "PPER", "APPR", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Noch lieber, als du warst im Nahen.", "tokens": ["Noch", "lie\u00b7ber", ",", "als", "du", "warst", "im", "Na\u00b7hen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUS", "PPER", "VAFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nun, da wir lange uns nicht sahen,", "tokens": ["Nun", ",", "da", "wir", "lan\u00b7ge", "uns", "nicht", "sa\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "ADV", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bild ich mir ein: Du hast mich gern.", "tokens": ["Bild", "ich", "mir", "ein", ":", "Du", "hast", "mich", "gern", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "PTKVZ", "$.", "PPER", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ach bitte komme bald zur\u00fcck", "tokens": ["Ach", "bit\u00b7te", "kom\u00b7me", "bald", "zu\u00b7r\u00fcck"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ITJ", "ADV", "VVFIN", "ADV", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit offnem, unverwitzeltem Vertraun.", "tokens": ["Mit", "off\u00b7nem", ",", "un\u00b7ver\u00b7wit\u00b7zel\u00b7tem", "Ver\u00b7traun", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Ich w\u00fcnsche dir f\u00fcrs neue Jahr viel Gl\u00fcck,", "tokens": ["Ich", "w\u00fcn\u00b7sche", "dir", "f\u00fcrs", "neu\u00b7e", "Jahr", "viel", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Eine Frau (zur Hochzeit mich einladend)", "tokens": ["Ei\u00b7ne", "Frau", "(", "zur", "Hoch\u00b7zeit", "mich", "ein\u00b7la\u00b7dend", ")"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "APPRART", "NN", "PPER", "VVPP", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und andre gro\u00dfe Nebenfraun", "tokens": ["Und", "and\u00b7re", "gro\u00b7\u00dfe", "Ne\u00b7ben\u00b7fraun"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und was du sonstens wichtig brauchst.", "tokens": ["Und", "was", "du", "sons\u00b7tens", "wich\u00b7tig", "brauchst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df du nie anders, als wie badend,", "tokens": ["Da\u00df", "du", "nie", "an\u00b7ders", ",", "als", "wie", "ba\u00b7dend", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "$,", "KOUS", "KOKOM", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Auch f\u00fcr Minuten nur untertauchst.", "tokens": ["Auch", "f\u00fcr", "Mi\u00b7nu\u00b7ten", "nur", "un\u00b7ter\u00b7tauchst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "ADV", "ADV", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}}}}