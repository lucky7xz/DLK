{"textgrid.poem.34532": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "1L: In der nagelneuen Hose", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In der nagelneuen Hose", "tokens": ["In", "der", "na\u00b7gel\u00b7neu\u00b7en", "Ho\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "mit der senkrecht steifen Falte,", "tokens": ["mit", "der", "sen\u00b7krecht", "stei\u00b7fen", "Fal\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "w\u00fcrdevoll und dennoch sch\u00fcchtern", "tokens": ["w\u00fcr\u00b7de\u00b7voll", "und", "den\u00b7noch", "sch\u00fcch\u00b7tern"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "KON", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "naht Pierrot der Schwiegermutter.", "tokens": ["naht", "Pier\u00b7rot", "der", "Schwie\u00b7ger\u00b7mut\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Stolz hebt er die sch\u00f6nen Augen", "tokens": ["Stolz", "hebt", "er", "die", "sch\u00f6\u00b7nen", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "auf, doch zittern ihm die Beine", "tokens": ["auf", ",", "doch", "zit\u00b7tern", "ihm", "die", "Bei\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKVZ", "$,", "ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "in der nagelneuen Hose", "tokens": ["in", "der", "na\u00b7gel\u00b7neu\u00b7en", "Ho\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "mit der senkrecht steifen Falte.", "tokens": ["mit", "der", "sen\u00b7krecht", "stei\u00b7fen", "Fal\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "\u2013 N\u00f6rgelnd klagt die Graubehaarte:", "tokens": ["\u2013", "N\u00f6r\u00b7gelnd", "klagt", "die", "Grau\u00b7be\u00b7haar\u00b7te", ":"], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVPP", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Colombinchen sei zu jung noch,", "tokens": ["Co\u00b7lom\u00b7bin\u00b7chen", "sei", "zu", "jung", "noch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PTKA", "ADJD", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "und ihr Sinn noch gar zu kindlich ...", "tokens": ["und", "ihr", "Sinn", "noch", "gar", "zu", "kind\u00b7lich", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "ADV", "PTKA", "ADJD", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "W\u00fcthend ballt Pierrot die F\u00e4uste", "tokens": ["W\u00fct\u00b7hend", "ballt", "Pier\u00b7rot", "die", "F\u00e4us\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "in der nagelneuen Hose.", "tokens": ["in", "der", "na\u00b7gel\u00b7neu\u00b7en", "Ho\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}