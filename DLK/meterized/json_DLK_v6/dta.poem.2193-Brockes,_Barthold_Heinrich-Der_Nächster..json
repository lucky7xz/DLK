{"dta.poem.2193": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der N\u00e4chster.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Aus Sorgen f\u00fcr ein k\u00fcnftig Uebel k\u00f6nnt' uns sogar", "tokens": ["Aus", "Sor\u00b7gen", "f\u00fcr", "ein", "k\u00fcnf\u00b7tig", "Ue\u00b7bel", "k\u00f6nnt'", "uns", "so\u00b7gar"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "ART", "ADJD", "NN", "VMFIN", "PPER", "ADV"], "meter": "-+---+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "ein Nutz entsprie\u00dfen,", "tokens": ["ein", "Nutz", "ent\u00b7sprie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Gebrauchte man nur die Vernunft. Man wird von ihr", "tokens": ["Ge\u00b7brauch\u00b7te", "man", "nur", "die", "Ver\u00b7nunft", ".", "Man", "wird", "von", "ihr"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "ADV", "ART", "NN", "$.", "PIS", "VAFIN", "APPR", "PPOSAT"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "mit Recht belehrt,", "tokens": ["mit", "Recht", "be\u00b7lehrt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "(da uns das Gegenw\u00e4rtige von unsrer Zeit nur zugeh\u00f6rt)", "tokens": ["(", "da", "uns", "das", "Ge\u00b7gen\u00b7w\u00e4r\u00b7ti\u00b7ge", "von", "uns\u00b7rer", "Zeit", "nur", "zu\u00b7ge\u00b7h\u00f6rt", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.6": {"text": "Bey einem noch entfernten B\u00f6sen das gegenw\u00e4rtge Gut", "tokens": ["Bey", "ei\u00b7nem", "noch", "ent\u00b7fern\u00b7ten", "B\u00f6\u00b7sen", "das", "ge\u00b7gen\u00b7w\u00e4rt\u00b7ge", "Gut"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADV", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.7": {"text": "genie\u00dfen,", "tokens": ["ge\u00b7nie\u00b7\u00dfen", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.8": {"text": "Um so viel mehr als wir erfahren, und auch von andern", "tokens": ["Um", "so", "viel", "mehr", "als", "wir", "er\u00b7fah\u00b7ren", ",", "und", "auch", "von", "an\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUI", "ADV", "ADV", "ADV", "KOUS", "PPER", "VVINF", "$,", "KON", "ADV", "APPR", "ADJA"], "meter": "--+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "oft gelesen,", "tokens": ["oft", "ge\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.10": {"text": "Da\u00df viele Sorgen \u00fcberfl\u00fc\u00dfig, unn\u00f6thig und umsonst", "tokens": ["Da\u00df", "vie\u00b7le", "Sor\u00b7gen", "\u00fc\u00b7berf\u00b7l\u00fc\u00b7\u00dfig", ",", "un\u00b7n\u00f6\u00b7thig", "und", "um\u00b7sonst"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PIAT", "NN", "ADJD", "$,", "ADJD", "KON", "ADV"], "meter": "-+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.11": {"text": "gewesen.", "tokens": ["ge\u00b7we\u00b7sen", "."], "token_info": ["word", "punct"], "pos": ["VAPP", "$."], "meter": "-+-", "measure": "amphibrach.single"}}}}}