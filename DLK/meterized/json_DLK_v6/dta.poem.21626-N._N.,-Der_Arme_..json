{"dta.poem.21626": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Der Arme .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Die Armuth gab ihm dieses Leben", "tokens": ["Die", "Ar\u00b7muth", "gab", "ihm", "die\u00b7ses", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zur Armuth und zur blassen Pein \u2014", "tokens": ["Zur", "Ar\u00b7muth", "und", "zur", "blas\u00b7sen", "Pein"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPRART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im Kothe war einst seine Wiege,", "tokens": ["Im", "Ko\u00b7the", "war", "einst", "sei\u00b7ne", "Wie\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und wird sein Sterbebett auch sein.", "tokens": ["Und", "wird", "sein", "Ster\u00b7be\u00b7bett", "auch", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "ADV", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vom ersten Schrei verdammt zur Dummheit", "tokens": ["Vom", "ers\u00b7ten", "Schrei", "ver\u00b7dammt", "zur", "Dumm\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und ausgeschlossen von dem Licht \u2014", "tokens": ["Und", "aus\u00b7ge\u00b7schlos\u00b7sen", "von", "dem", "Licht"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "F\u00fcr ihn erschien ja der Erl\u00f6ser,", "tokens": ["F\u00fcr", "ihn", "er\u00b7schien", "ja", "der", "Er\u00b7l\u00f6\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der milde Gott der K\u00fcnste, nicht.", "tokens": ["Der", "mil\u00b7de", "Gott", "der", "K\u00fcns\u00b7te", ",", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Mit Stumpfheit durft\u2019 er nur verkehren,", "tokens": ["Mit", "Stumpf\u00b7heit", "durft'", "er", "nur", "ver\u00b7keh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "An Rohheit war er festgebannt,", "tokens": ["An", "Roh\u00b7heit", "war", "er", "fest\u00b7ge\u00b7bannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Er stank nach Schnaps und kaute Tabak \u2014", "tokens": ["Er", "stank", "nach", "Schnaps", "und", "kau\u00b7te", "Ta\u00b7bak"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "VVFIN", "NN", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.12": {"text": "Roh wie sein Kittel der Verstand.", "tokens": ["Roh", "wie", "sein", "Kit\u00b7tel", "der", "Ver\u00b7stand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Und seine Lippen lernten Fluchen,", "tokens": ["Und", "sei\u00b7ne", "Lip\u00b7pen", "lern\u00b7ten", "Flu\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Stets blieb er stumpf, stets blieb er dumm \u2014", "tokens": ["Stets", "blieb", "er", "stumpf", ",", "stets", "blieb", "er", "dumm"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$,", "ADV", "VVFIN", "PPER", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Die langen Jahre hast\u2019ger Arbeit,", "tokens": ["Die", "lan\u00b7gen", "Jah\u00b7re", "hast'\u00b7ger", "Ar\u00b7beit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Die dr\u00fcckten seinen R\u00fccken krumm;", "tokens": ["Die", "dr\u00fcck\u00b7ten", "sei\u00b7nen", "R\u00fc\u00b7cken", "krumm", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Und kraftlos wurden seine H\u00e4nde", "tokens": ["Und", "kraft\u00b7los", "wur\u00b7den", "sei\u00b7ne", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Und betteln mu\u00dft\u2019 der arme Mann \u2014 \u2014", "tokens": ["Und", "bet\u00b7teln", "mu\u00dft'", "der", "ar\u00b7me", "Mann"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVINF", "VMFIN", "ART", "ADJA", "NN", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Da\u00df selbst ein ganzes ems\u2019ges Leben", "tokens": ["Da\u00df", "selbst", "ein", "gan\u00b7zes", "ems'\u00b7ges", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Kein ruhiges Sterben geben kann!", "tokens": ["Kein", "ru\u00b7hi\u00b7ges", "Ster\u00b7ben", "ge\u00b7ben", "kann", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}