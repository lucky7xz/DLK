{"textgrid.poem.35800": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die in Thr\u00e4nen hier sich sehnen,", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die in Thr\u00e4nen hier sich sehnen,", "tokens": ["Die", "in", "Thr\u00e4\u00b7nen", "hier", "sich", "seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "ADV", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Finden sich im Vaterland;", "tokens": ["Fin\u00b7den", "sich", "im", "Va\u00b7ter\u00b7land", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was hienieden bang geschieden,", "tokens": ["Was", "hien\u00b7ie\u00b7den", "bang", "ge\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Geht dort selig Hand in Hand.", "tokens": ["Geht", "dort", "se\u00b7lig", "Hand", "in", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "NN", "APPR", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Ob in Leiden, ob in Freuden", "tokens": ["Ob", "in", "Lei\u00b7den", ",", "ob", "in", "Freu\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "APPR", "NN", "$,", "KOUS", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hier dein Herz auch hat gelebt,", "tokens": ["Hier", "dein", "Herz", "auch", "hat", "ge\u00b7lebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "ADV", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist dann eines; dort ist keines,", "tokens": ["Ist", "dann", "ei\u00b7nes", ";", "dort", "ist", "kei\u00b7nes", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIS", "$.", "ADV", "VAFIN", "PIS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das in Schmerz und Jammer bebt.", "tokens": ["Das", "in", "Schmerz", "und", "Jam\u00b7mer", "bebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Was wir lieben, folgt uns dr\u00fcben", "tokens": ["Was", "wir", "lie\u00b7ben", ",", "folgt", "uns", "dr\u00fc\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zu der Engel sel'gem Chor.", "tokens": ["Zu", "der", "En\u00b7gel", "sel'\u00b7gem", "Chor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sei nur stille: bricht die H\u00fclle,", "tokens": ["Sei", "nur", "stil\u00b7le", ":", "bricht", "die", "H\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVFIN", "$.", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schwebt der Schmetterling empor.", "tokens": ["Schwebt", "der", "Schmet\u00b7ter\u00b7ling", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und es kommen alle Frommen,", "tokens": ["Und", "es", "kom\u00b7men", "al\u00b7le", "From\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die ihm hier geeilt voraus,", "tokens": ["Die", "ihm", "hier", "ge\u00b7eilt", "vo\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVPP", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihm mit Segen froh entgegen,", "tokens": ["Ihm", "mit", "Se\u00b7gen", "froh", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fchren ihn in's Vaterhaus.", "tokens": ["F\u00fch\u00b7ren", "ihn", "in's", "Va\u00b7ter\u00b7haus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Hin zum Throne, wo die Krone", "tokens": ["Hin", "zum", "Thro\u00b7ne", ",", "wo", "die", "Kro\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "APPRART", "NN", "$,", "PWAV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Er f\u00fcr treuen Kampf erh\u00e4lt,", "tokens": ["Er", "f\u00fcr", "treu\u00b7en", "Kampf", "er\u00b7h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und es neigen sich die Reigen", "tokens": ["Und", "es", "nei\u00b7gen", "sich", "die", "Rei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PRF", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Vor dem neugekr\u00f6nten Held.", "tokens": ["Vor", "dem", "neu\u00b7ge\u00b7kr\u00f6n\u00b7ten", "Held", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}