{"textgrid.poem.63666": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Das Schwerste", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nichts wird dem Herzen so leicht,", "tokens": ["Nichts", "wird", "dem", "Her\u00b7zen", "so", "leicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Als zu vergessen des Schweren,", "tokens": ["Als", "zu", "ver\u00b7ges\u00b7sen", "des", "Schwe\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKZU", "VVINF", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Wie durch den Schleier der Z\u00e4hren", "tokens": ["Wie", "durch", "den", "Schlei\u00b7er", "der", "Z\u00e4h\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Pl\u00f6tzlich ein L\u00e4cheln sich schleicht.", "tokens": ["Pl\u00f6tz\u00b7lich", "ein", "L\u00e4\u00b7cheln", "sich", "schleicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Schwerer vergi\u00dft sich das Gl\u00fcck,", "tokens": ["Schwe\u00b7rer", "ver\u00b7gi\u00dft", "sich", "das", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Sp\u00e4ter das Labende, S\u00fc\u00dfe;", "tokens": ["Sp\u00e4\u00b7ter", "das", "La\u00b7ben\u00b7de", ",", "S\u00fc\u00b7\u00dfe", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$,", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Sehnende Seufzer und Gr\u00fc\u00dfe", "tokens": ["Seh\u00b7nen\u00b7de", "Seuf\u00b7zer", "und", "Gr\u00fc\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Rufen es oft noch zur\u00fcck.", "tokens": ["Ru\u00b7fen", "es", "oft", "noch", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "PTKVZ", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "Aber die reizende Lust,", "tokens": ["A\u00b7ber", "die", "rei\u00b7zen\u00b7de", "Lust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Wenn sie mit schaudernder K\u00e4lte", "tokens": ["Wenn", "sie", "mit", "schau\u00b7dern\u00b7der", "K\u00e4l\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Pl\u00f6tzlich ein Gott uns verg\u00e4llte,", "tokens": ["Pl\u00f6tz\u00b7lich", "ein", "Gott", "uns", "ver\u00b7g\u00e4ll\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Nimmer verschmerzt sie die Brust.", "tokens": ["Nim\u00b7mer", "ver\u00b7schmerzt", "sie", "die", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.4": {"line.1": {"text": "Ach, wer verwindet das Heil,", "tokens": ["Ach", ",", "wer", "ver\u00b7win\u00b7det", "das", "Heil", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWS", "VVFIN", "ART", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Das sich zum Unheil gewendet?", "tokens": ["Das", "sich", "zum", "Un\u00b7heil", "ge\u00b7wen\u00b7det", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Erst wenn das Leben sich endet,", "tokens": ["Erst", "wenn", "das", "Le\u00b7ben", "sich", "en\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Schw\u00e4rt aus der Wunde der Pfeil.", "tokens": ["Schw\u00e4rt", "aus", "der", "Wun\u00b7de", "der", "Pfeil", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}}}}