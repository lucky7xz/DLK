{"dta.poem.3517": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Nachtanz .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Schau gut Gesell, was f\u00fchr ich allhier,               ", "tokens": ["Schau", "gut", "Ge\u00b7sell", ",", "was", "f\u00fchr", "ich", "all\u00b7hier", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "NN", "$,", "PWS", "VVFIN", "PPER", "ADV", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Schau, was f\u00fchre ich allhier,", "tokens": ["Schau", ",", "was", "f\u00fch\u00b7re", "ich", "all\u00b7hier", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ein fein braun M\u00e4gdlein,", "tokens": ["Ein", "fein", "braun", "M\u00e4gd\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJD", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Guter Ding nach meinem Begier,", "tokens": ["Gu\u00b7ter", "Ding", "nach", "mei\u00b7nem", "Be\u00b7gier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Wir wollen fein miteinander seyn.", "tokens": ["Wir", "wol\u00b7len", "fein", "mi\u00b7tein\u00b7an\u00b7der", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "ADV", "VAINF", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Drum sing mir bald ein kleines T\u00e4nzlein,", "tokens": ["Drum", "sing", "mir", "bald", "ein", "klei\u00b7nes", "T\u00e4nz\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ja ein kleines T\u00e4nzlein,", "tokens": ["Ja", "ein", "klei\u00b7nes", "T\u00e4nz\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ich will dirs lohnen,", "tokens": ["Ich", "will", "dirs", "loh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Und dir bringen das Jungfr\u00e4ulein,", "tokens": ["Und", "dir", "brin\u00b7gen", "das", "Jung\u00b7fr\u00e4u\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVINF", "ART", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Du mu\u00dft ihr aber wohl verschonen.", "tokens": ["Du", "mu\u00dft", "ihr", "a\u00b7ber", "wohl", "ver\u00b7scho\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Gleich wie ein hurtig R\u00f6sselein trabt,", "tokens": ["Gleich", "wie", "ein", "hur\u00b7tig", "R\u00f6s\u00b7sel\u00b7ein", "trabt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Ja ein R\u00f6sselein trabt,", "tokens": ["Ja", "ein", "R\u00f6s\u00b7sel\u00b7ein", "trabt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Leis unbeschlagen,", "tokens": ["Leis", "un\u00b7be\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADJD", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Also dies M\u00e4gdlein zu Tanz gaht,", "tokens": ["Al\u00b7so", "dies", "M\u00e4gd\u00b7lein", "zu", "Tanz", "gaht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.5": {"text": "Und springet, h\u00fcpfet ohn Verzagen.", "tokens": ["Und", "sprin\u00b7get", ",", "h\u00fcp\u00b7fet", "ohn", "Ver\u00b7za\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Schau wie er trabt, der wackre Gaul,", "tokens": ["Schau", "wie", "er", "trabt", ",", "der", "wack\u00b7re", "Gaul", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "PPER", "VVFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Auf scharfe Sporn thut er nit harren;", "tokens": ["Auf", "schar\u00b7fe", "Sporn", "thut", "er", "nit", "har\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Stroh, Heu, dient gar nit f\u00fcr sein Maul,", "tokens": ["Stroh", ",", "Heu", ",", "dient", "gar", "nit", "f\u00fcr", "sein", "Maul", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "VVFIN", "ADV", "PTKNEG", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Bei ihm kann man das alles sparen.", "tokens": ["Bei", "ihm", "kann", "man", "das", "al\u00b7les", "spa\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VMFIN", "PIS", "ART", "PIS", "VVINF", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}, "stanza.5": {"line.1": {"text": "Ein reichen Herrn mu\u00df es traun han,", "tokens": ["Ein", "rei\u00b7chen", "Herrn", "mu\u00df", "es", "traun", "han", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PPER", "VVINF", "VAFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der es allzeit so wohl mag warten,", "tokens": ["Der", "es", "all\u00b7zeit", "so", "wohl", "mag", "war\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der dies R\u00f6\u00dflein fein z\u00e4umen kann,", "tokens": ["Der", "dies", "R\u00f6\u00df\u00b7lein", "fein", "z\u00e4u\u00b7men", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PDS", "NN", "ADJD", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu reiten es in seim Lustgarten.", "tokens": ["Zu", "rei\u00b7ten", "es", "in", "seim", "Lust\u00b7gar\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}