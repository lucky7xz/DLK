{"dta.poem.10921": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Damm-Hirsch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So nat\u00fcrlich hat der Damm-Hirsch den herabgezognen Ast,", "tokens": ["So", "na\u00b7t\u00fcr\u00b7lich", "hat", "der", "Dam\u00b7mHirsch", "den", "her\u00b7ab\u00b7ge\u00b7zog\u00b7nen", "Ast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "--+-+-+---+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Vom belaubten Eich-Baum, hier, mit den Z\u00e4hnen, an-", "tokens": ["Vom", "be\u00b7laub\u00b7ten", "Eich\u00b7Baum", ",", "hier", ",", "mit", "den", "Z\u00e4h\u00b7nen", ",", "an"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["APPRART", "ADJA", "NN", "$,", "ADV", "$,", "APPR", "ART", "NN", "$,", "TRUNC"], "meter": "+-+-++-+-+-+", "measure": "unknown.measure.septa"}, "line.3": {"text": "Da\u00df mich deucht, ich seh den Zweig, da der scharfe Bi\u00df geschehn,", "tokens": ["Da\u00df", "mich", "deucht", ",", "ich", "seh", "den", "Zweig", ",", "da", "der", "schar\u00b7fe", "Bi\u00df", "ge\u00b7schehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "$,", "KOUS", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Mit geschwind - und schnellem Ruck, wieder in die H\u00f6he gehn.", "tokens": ["Mit", "ge\u00b7schwind", "und", "schnel\u00b7lem", "Ruck", ",", "wie\u00b7der", "in", "die", "H\u00f6\u00b7he", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "$(", "KON", "ADJA", "NN", "$,", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.5": {"text": "Da\u00df im \u00fcbrigen, auf Erden, die nie m\u00fc\u00dfige Natur", "tokens": ["Da\u00df", "im", "\u00fcb\u00b7ri\u00b7gen", ",", "auf", "Er\u00b7den", ",", "die", "nie", "m\u00fc\u00b7\u00dfi\u00b7ge", "Na\u00b7tur"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "APPRART", "ADJA", "$,", "APPR", "NN", "$,", "PRELS", "ADV", "ADJA", "NN"], "meter": "--+-+-+--+-+--+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Unersch\u00f6pflich im Formiren, davon zeigt das Damm-Wild hier", "tokens": ["Un\u00b7er\u00b7sch\u00f6pf\u00b7lich", "im", "For\u00b7mi\u00b7ren", ",", "da\u00b7von", "zeigt", "das", "Dam\u00b7mWild", "hier"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPRART", "NN", "$,", "PAV", "VVFIN", "ART", "NN", "ADV"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.7": {"text": "Uns von neuen eine Prob, und ein\u2019 unleugbare Spur.", "tokens": ["Uns", "von", "neu\u00b7en", "ei\u00b7ne", "Prob", ",", "und", "ein'", "un\u00b7leug\u00b7ba\u00b7re", "Spur", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "ART", "NN", "$,", "KON", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.8": {"text": "Von dem andern rothen Wilde, weiset sich die\u00df bunte Thier,", "tokens": ["Von", "dem", "an\u00b7dern", "ro\u00b7then", "Wil\u00b7de", ",", "wei\u00b7set", "sich", "die\u00df", "bun\u00b7te", "Thier", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$,", "VVFIN", "PRF", "PDS", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.9": {"text": "So an Farb als Form, verschiedlich. Ausser den vier Augen-", "tokens": ["So", "an", "Farb", "als", "Form", ",", "ver\u00b7schied\u00b7lich", ".", "Aus\u00b7ser", "den", "vier", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "KOUS", "NN", "$,", "ADJD", "$.", "NN", "ART", "CARD", "TRUNC"], "meter": "--+-+-+-+-+-+-", "measure": "anapaest.init"}, "line.10": {"text": "Gleicht fast sein Geweih den Schaufeln; seine meistens weisse", "tokens": ["Gleicht", "fast", "sein", "Ge\u00b7weih", "den", "Schau\u00b7feln", ";", "sei\u00b7ne", "meis\u00b7tens", "weis\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "ART", "NN", "$.", "PPOSAT", "ADV", "VVFIN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.11": {"text": "Wird, zu unsrer Augenlust, \u00f6fters sch\u00f6n gefleckt geschaut.", "tokens": ["Wird", ",", "zu", "uns\u00b7rer", "Au\u00b7gen\u00b7lust", ",", "\u00f6f\u00b7ters", "sch\u00f6n", "ge\u00b7fleckt", "ge\u00b7schaut", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "APPR", "PPOSAT", "NN", "$,", "ADV", "ADJD", "VVPP", "VVPP", "$."], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.12": {"text": "Und sein noch viel zarter Fleisch wird mit mehrer Lust genossen.", "tokens": ["Und", "sein", "noch", "viel", "zar\u00b7ter", "Fleisch", "wird", "mit", "meh\u00b7rer", "Lust", "ge\u00b7nos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADV", "PIAT", "ADJA", "NN", "VAFIN", "APPR", "PIAT", "NN", "VVPP", "$."], "meter": "-+--+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "Wenn wir denn an diesem Thier, nebst dem Gaum, das Auge", "tokens": ["Wenn", "wir", "denn", "an", "die\u00b7sem", "Thier", ",", "nebst", "dem", "Gaum", ",", "das", "Au\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PDAT", "NN", "$,", "APPR", "ART", "NN", "$,", "ART", "NN"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.14": {"text": "So verge\u00dft nicht, dem zu danken, dem wir es zu danken haben.", "tokens": ["So", "ver\u00b7ge\u00dft", "nicht", ",", "dem", "zu", "dan\u00b7ken", ",", "dem", "wir", "es", "zu", "dan\u00b7ken", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKNEG", "$,", "PRELS", "PTKZU", "VVINF", "$,", "PRELS", "PPER", "PPER", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}}}}