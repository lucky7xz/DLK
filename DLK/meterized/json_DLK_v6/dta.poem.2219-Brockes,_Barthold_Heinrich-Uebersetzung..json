{"dta.poem.2219": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Uebersetzung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das R\u00fchmen machet den nicht stolz, der w\u00fcrdig ist,", "tokens": ["Das", "R\u00fch\u00b7men", "ma\u00b7chet", "den", "nicht", "stolz", ",", "der", "w\u00fcr\u00b7dig", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "PTKNEG", "ADJD", "$,", "PRELS", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "da\u00df wir ihn ehren:", "tokens": ["da\u00df", "wir", "ihn", "eh\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Das Loben bl\u00e4het den nur auf, der nicht gewohnt, sein", "tokens": ["Das", "Lo\u00b7ben", "bl\u00e4\u00b7het", "den", "nur", "auf", ",", "der", "nicht", "ge\u00b7wohnt", ",", "sein"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "ADV", "PTKVZ", "$,", "PRELS", "PTKNEG", "VVPP", "$,", "PPOSAT"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Lob zu h\u00f6ren.", "tokens": ["Lob", "zu", "h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}