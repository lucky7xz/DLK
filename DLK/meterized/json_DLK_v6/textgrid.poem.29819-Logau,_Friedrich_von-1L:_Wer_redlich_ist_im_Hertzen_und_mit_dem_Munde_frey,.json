{"textgrid.poem.29819": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wer redlich ist im Hertzen und mit dem Munde frey,", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer redlich ist im Hertzen und mit dem Munde frey,", "tokens": ["Wer", "red\u00b7lich", "ist", "im", "Hert\u00b7zen", "und", "mit", "dem", "Mun\u00b7de", "frey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "APPRART", "NN", "KON", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Der wisse, da\u00df bey Hofe beh\u00e4glich er nicht sey.", "tokens": ["Der", "wis\u00b7se", ",", "da\u00df", "bey", "Ho\u00b7fe", "be\u00b7h\u00e4g\u00b7lich", "er", "nicht", "sey", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "KOUS", "APPR", "NN", "ADJD", "PPER", "PTKNEG", "VAFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Wie man ihm vorgesaget, so sagt der Papagey;", "tokens": ["Wie", "man", "ihm", "vor\u00b7ge\u00b7sa\u00b7get", ",", "so", "sagt", "der", "Pa\u00b7pa\u00b7gey", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Drum wer daselbst wil gelten, der trete diesem bey.", "tokens": ["Drum", "wer", "da\u00b7selbst", "wil", "gel\u00b7ten", ",", "der", "tre\u00b7te", "die\u00b7sem", "bey", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PWS", "PAV", "VMFIN", "VVINF", "$,", "PRELS", "VVFIN", "PDAT", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}}}}