{"textgrid.poem.45645": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[wir haben zusammen gestanden]", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir haben zusammen gestanden,", "tokens": ["Wir", "ha\u00b7ben", "zu\u00b7sam\u00b7men", "ge\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Wir haben zusammen gesiegt,", "tokens": ["Wir", "ha\u00b7ben", "zu\u00b7sam\u00b7men", "ge\u00b7siegt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Und wurden wir je \u00fcberwunden,", "tokens": ["Und", "wur\u00b7den", "wir", "je", "\u00fc\u00b7berw\u00b7un\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du stehst, wo der Dichter erliegt.", "tokens": ["Du", "stehst", ",", "wo", "der", "Dich\u00b7ter", "er\u00b7liegt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}