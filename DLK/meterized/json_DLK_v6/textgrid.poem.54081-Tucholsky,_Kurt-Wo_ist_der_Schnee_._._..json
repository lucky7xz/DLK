{"textgrid.poem.54081": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Wo ist der Schnee . . .", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo ist der Schnee vom vergangenen Jahr,", "tokens": ["Wo", "ist", "der", "Schnee", "vom", "ver\u00b7gan\u00b7ge\u00b7nen", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Anna-Susanna?", "tokens": ["An\u00b7na\u00b7Su\u00b7san\u00b7na", "?"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "-----", "measure": "unknown.measure.zero"}, "line.3": {"text": "Wei\u00dft du noch, was damals Mode war,", "tokens": ["Wei\u00dft", "du", "noch", ",", "was", "da\u00b7mals", "Mo\u00b7de", "war", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "PRELS", "ADV", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Anna-Susanna?", "tokens": ["An\u00b7na\u00b7Su\u00b7san\u00b7na", "?"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "-----", "measure": "unknown.measure.zero"}, "line.5": {"text": "Die Literatur trug man vorne gerafft,", "tokens": ["Die", "Li\u00b7te\u00b7ra\u00b7tur", "trug", "man", "vor\u00b7ne", "ge\u00b7rafft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.6": {"text": "jede Woche gabs ein Genie.", "tokens": ["je\u00b7de", "Wo\u00b7che", "gabs", "ein", "Ge\u00b7nie", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Und alles murmelte: \u00bbFaaabelhaft!", "tokens": ["Und", "al\u00b7les", "mur\u00b7mel\u00b7te", ":", "\u00bb", "Fa\u00b7aa\u00b7bel\u00b7haft", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "$.", "$(", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Rein menschlich . . . irgendwie . . . !\u00ab", "tokens": ["Rein", "menschlich", ".", ".", ".", "ir\u00b7gend\u00b7wie", ".", ".", ".", "!", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "punct", "word", "punct", "punct", "punct", "punct", "punct"], "pos": ["NN", "ADJD", "$.", "$.", "$.", "FM.la", "$.", "$.", "$.", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Wo sind die Blumen vom letzten Lenz,", "tokens": ["Wo", "sind", "die", "Blu\u00b7men", "vom", "letz\u00b7ten", "Lenz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Anna-Susanna?", "tokens": ["An\u00b7na\u00b7Su\u00b7san\u00b7na", "?"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "-----", "measure": "unknown.measure.zero"}, "line.3": {"text": "Die Betonung des kosmischen B\u00fchnen-Akzents,", "tokens": ["Die", "Be\u00b7to\u00b7nung", "des", "kos\u00b7mi\u00b7schen", "B\u00fch\u00b7nen\u00b7Ak\u00b7zents", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.4": {"text": "Anna-Susanna?", "tokens": ["An\u00b7na\u00b7Su\u00b7san\u00b7na", "?"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "-----", "measure": "unknown.measure.zero"}, "line.5": {"text": "Das gebildete Publikum lief zuhauf", "tokens": ["Das", "ge\u00b7bil\u00b7de\u00b7te", "Pub\u00b7li\u00b7kum", "lief", "zu\u00b7hauf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.6": {"text": "mit der Kritiker-Artillerie.", "tokens": ["mit", "der", "Kri\u00b7ti\u00b7ker\u00b7Ar\u00b7til\u00b7le\u00b7rie", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "--+---+-+", "measure": "anapaest.init"}, "line.7": {"text": "Und die St\u00fccke f\u00fchrt kein Mensch mehr auf,", "tokens": ["Und", "die", "St\u00fc\u00b7cke", "f\u00fchrt", "kein", "Mensch", "mehr", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PIAT", "NN", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "rein menschlich irgendwie.", "tokens": ["rein", "menschlich", "ir\u00b7gend\u00b7wie", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Wo ist der Schnee vom vergangenen Jahr,", "tokens": ["Wo", "ist", "der", "Schnee", "vom", "ver\u00b7gan\u00b7ge\u00b7nen", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Anna-Susanna?", "tokens": ["An\u00b7na\u00b7Su\u00b7san\u00b7na", "?"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "-----", "measure": "unknown.measure.zero"}, "line.3": {"text": "Brecht wird sein, was Sudermann war,", "tokens": ["Brecht", "wird", "sein", ",", "was", "Su\u00b7der\u00b7mann", "war", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "VAINF", "$,", "PRELS", "NE", "VAFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Anna-Susanna.", "tokens": ["An\u00b7na\u00b7Su\u00b7san\u00b7na", "."], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "-----", "measure": "unknown.measure.zero"}, "line.5": {"text": "Sie br\u00fcllen sich hoch, die Reklame schreit,", "tokens": ["Sie", "br\u00fcl\u00b7len", "sich", "hoch", ",", "die", "Re\u00b7kla\u00b7me", "schreit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "das ist eine Industrie.", "tokens": ["das", "ist", "ei\u00b7ne", "In\u00b7dust\u00b7rie", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Pro Mann einen Monat Unsterblichkeit", "tokens": ["Pro", "Mann", "ei\u00b7nen", "Mo\u00b7nat", "U\u00b7nsterb\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NN", "ART", "NN", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "\u2013 Anna-Susanna \u2013", "tokens": ["\u2013", "An\u00b7na\u00b7Su\u00b7san\u00b7na", "\u2013"], "token_info": ["punct", "word", "punct"], "pos": ["$(", "NE", "$("], "meter": "-----", "measure": "unknown.measure.zero"}, "line.9": {"text": "rein menschlich irgendwie.", "tokens": ["rein", "menschlich", "ir\u00b7gend\u00b7wie", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}