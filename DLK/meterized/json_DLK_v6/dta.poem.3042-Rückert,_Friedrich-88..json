{"dta.poem.3042": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "88.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Den Menschen gn\u00fcget nie, was Menschen wissen k\u00f6nnen,", "tokens": ["Den", "Men\u00b7schen", "gn\u00fc\u00b7get", "nie", ",", "was", "Men\u00b7schen", "wis\u00b7sen", "k\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,", "PWS", "NN", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kein Vorrecht wollen sie darin den G\u00f6ttern g\u00f6nnen.", "tokens": ["Kein", "Vor\u00b7recht", "wol\u00b7len", "sie", "da\u00b7rin", "den", "G\u00f6t\u00b7tern", "g\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "PPER", "PAV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch hat solch Wissen nie sie g\u00f6ttergleich gemacht,", "tokens": ["Doch", "hat", "solch", "Wis\u00b7sen", "nie", "sie", "g\u00f6t\u00b7ter\u00b7gleich", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PIAT", "NN", "ADV", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Um ihren menschlichen Verstand nur oft gebracht.", "tokens": ["Um", "ih\u00b7ren", "menschli\u00b7chen", "Ver\u00b7stand", "nur", "oft", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "La\u00df uns, was vor uns steht, gewahren und erfahren,", "tokens": ["La\u00df", "uns", ",", "was", "vor", "uns", "steht", ",", "ge\u00b7wah\u00b7ren", "und", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$,", "ADJA", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und was dar\u00fcber geht, auf dahin\u00fcber sparen.", "tokens": ["Und", "was", "da\u00b7r\u00fc\u00b7ber", "geht", ",", "auf", "da\u00b7hin\u00b7\u00fc\u00b7ber", "spa\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PAV", "VVFIN", "$,", "APPR", "PAV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Es ist ja gut da\u00df uns bleib' etwas vorbehalten,", "tokens": ["Es", "ist", "ja", "gut", "da\u00df", "uns", "bleib'", "et\u00b7was", "vor\u00b7be\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "KOUS", "PPER", "VVFIN", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das wir zu seiner Zeit mit neuer Lust entfalten.", "tokens": ["Das", "wir", "zu", "sei\u00b7ner", "Zeit", "mit", "neu\u00b7er", "Lust", "ent\u00b7fal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Ich sage dir auch nicht, du sollst dich gar nicht schwingen", "tokens": ["Ich", "sa\u00b7ge", "dir", "auch", "nicht", ",", "du", "sollst", "dich", "gar", "nicht", "schwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKNEG", "$,", "PPER", "VMFIN", "PPER", "ADV", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hinan, hin\u00fcber nur mit Hals und Kopf nicht springen.", "tokens": ["Hi\u00b7nan", ",", "hin\u00b7\u00fc\u00b7ber", "nur", "mit", "Hals", "und", "Kopf", "nicht", "sprin\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "ADV", "APPR", "NN", "KON", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Es ist ein Unterschied, ob man hin\u00fcber blicke,", "tokens": ["Es", "ist", "ein", "Un\u00b7ter\u00b7schied", ",", "ob", "man", "hin\u00b7\u00fc\u00b7ber", "bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "KOUS", "PIS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ob man hin\u00fcberspring' und breche das Genicke.", "tokens": ["Ob", "man", "hin\u00b7\u00fcber\u00b7spring'", "und", "bre\u00b7che", "das", "Ge\u00b7ni\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PTKVZ", "KON", "VVFIN", "ART", "NN", "$."], "meter": "----+-+-+-+-", "measure": "unknown.measure.tetra"}}, "stanza.7": {"line.1": {"text": "Schwing dich empor und hol' herab von dort die Ahnung,", "tokens": ["Schwing", "dich", "em\u00b7por", "und", "hol'", "her\u00b7ab", "von", "dort", "die", "Ah\u00b7nung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "ADV", "APPR", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die gn\u00fcgt zur Mahnung dir, die gn\u00fcget dir zur Bahnung,", "tokens": ["Die", "gn\u00fcgt", "zur", "Mah\u00b7nung", "dir", ",", "die", "gn\u00fc\u00b7get", "dir", "zur", "Bah\u00b7nung", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPRART", "NN", "PPER", "$,", "PRELS", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Zur Mahnung deines Wegs, da\u00df du nicht sinkst in Ruh,", "tokens": ["Zur", "Mah\u00b7nung", "dei\u00b7nes", "Wegs", ",", "da\u00df", "du", "nicht", "sinkst", "in", "Ruh", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "$,", "KOUS", "PPER", "PTKNEG", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zur Bahnung eines Stegs dem h\u00f6hern Ziele zu.", "tokens": ["Zur", "Bah\u00b7nung", "ei\u00b7nes", "Stegs", "dem", "h\u00f6\u00b7hern", "Zie\u00b7le", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Inzwischen, wenn du wei\u00dft, du bist im Weg zum Ziel,", "tokens": ["I\u00b7nzwi\u00b7schen", ",", "wenn", "du", "wei\u00dft", ",", "du", "bist", "im", "Weg", "zum", "Ziel", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "VVFIN", "$,", "PPER", "VAFIN", "APPRART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sieh rechts und links dich um! auf Reisen sieht man viel.", "tokens": ["Sieh", "rechts", "und", "links", "dich", "um", "!", "auf", "Rei\u00b7sen", "sieht", "man", "viel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "KON", "ADV", "PPER", "PTKVZ", "$.", "APPR", "NN", "VVFIN", "PIS", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Die dumpf verrannten sinds, die nur im Auge haben", "tokens": ["Die", "dumpf", "ver\u00b7rann\u00b7ten", "sinds", ",", "die", "nur", "im", "Au\u00b7ge", "ha\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "VVPP", "VAFIN", "$,", "PRELS", "ADV", "APPRART", "NN", "VAFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Ziel, und unbeschaut die sch\u00f6ne Welt durchtraben.", "tokens": ["Das", "Ziel", ",", "und", "un\u00b7be\u00b7schaut", "die", "sch\u00f6\u00b7ne", "Welt", "durch\u00b7tra\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "ADJD", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Kurzsichtige, die sich als gar fernsichtige preisen;", "tokens": ["Kurz\u00b7sich\u00b7ti\u00b7ge", ",", "die", "sich", "als", "gar", "fern\u00b7sich\u00b7ti\u00b7ge", "prei\u00b7sen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PRF", "KOUS", "ADV", "ADJA", "NN", "$."], "meter": "-+--+--+-+--+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Denn nur aufs Ziel zu sehn, verdirbt die Lust am Reisen.", "tokens": ["Denn", "nur", "aufs", "Ziel", "zu", "sehn", ",", "ver\u00b7dirbt", "die", "Lust", "am", "Rei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "PTKZU", "VVINF", "$,", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}