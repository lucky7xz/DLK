{"dta.poem.17603": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "225.  Die ungekreutzigten Kreutze.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Viel sind der Welt ein Kreutz/ die Welt ist aber jhnen/", "tokens": ["Viel", "sind", "der", "Welt", "ein", "Kreutz", "/", "die", "Welt", "ist", "a\u00b7ber", "jh\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NN", "$(", "ART", "NN", "VAFIN", "ADV", "PPER", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht dieses widerumb: weil sie die noch bedienen.", "tokens": ["Nicht", "die\u00b7ses", "wi\u00b7de\u00b7rumb", ":", "weil", "sie", "die", "noch", "be\u00b7die\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PDAT", "ADV", "$.", "KOUS", "PPER", "ART", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}