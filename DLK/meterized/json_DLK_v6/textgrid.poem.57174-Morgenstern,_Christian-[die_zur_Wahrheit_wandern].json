{"textgrid.poem.57174": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "[die zur Wahrheit wandern]", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die zur Wahrheit wandern,", "tokens": ["Die", "zur", "Wahr\u00b7heit", "wan\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "wandern allein,", "tokens": ["wan\u00b7dern", "al\u00b7lein", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "keiner kann dem andern", "tokens": ["kei\u00b7ner", "kann", "dem", "an\u00b7dern"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "ART", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Wegbruder sein.", "tokens": ["Weg\u00b7bru\u00b7der", "sein", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VAINF", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Eine Spanne gehn wir,", "tokens": ["Ei\u00b7ne", "Span\u00b7ne", "gehn", "wir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "scheint es, im Chor ...", "tokens": ["scheint", "es", ",", "im", "Chor", "..."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "APPRART", "NN", "$("], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "bis zuletzt sich, sehn wir,", "tokens": ["bis", "zu\u00b7letzt", "sich", ",", "sehn", "wir", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "PRF", "$,", "VVFIN", "PPER", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "jeder verlor.", "tokens": ["je\u00b7der", "ver\u00b7lor", "."], "token_info": ["word", "word", "punct"], "pos": ["PIS", "VVFIN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Selbst der Liebste ringet", "tokens": ["Selbst", "der", "Liebs\u00b7te", "rin\u00b7get"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "irgendwo fern;", "tokens": ["ir\u00b7gend\u00b7wo", "fern", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "doch wer's ganz vollbringet,", "tokens": ["doch", "wer's", "ganz", "voll\u00b7brin\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "siegt sich zum Stern,", "tokens": ["siegt", "sich", "zum", "Stern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPRART", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "schafft, sein selbst Durchchrister,", "tokens": ["schafft", ",", "sein", "selbst", "Durchc\u00b7hris\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "ADV", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Neugottesgrund \u2013", "tokens": ["Neu\u00b7got\u00b7tes\u00b7grund", "\u2013"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "und ihn gr\u00fc\u00dft Geschwister", "tokens": ["und", "ihn", "gr\u00fc\u00dft", "Ge\u00b7schwis\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Ewiger Bund.", "tokens": ["E\u00b7wi\u00b7ger", "Bund", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}}}}