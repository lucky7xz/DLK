{"dta.poem.18800": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "Hatem .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "An des lust\u2019gen Brunnens Rand", "tokens": ["An", "des", "lust'\u00b7gen", "Brun\u00b7nens", "Rand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Der in Wasserf\u00e4den spielt", "tokens": ["Der", "in", "Was\u00b7ser\u00b7f\u00e4\u00b7den", "spielt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wusst ich nicht was fest mich hielt;", "tokens": ["Wusst", "ich", "nicht", "was", "fest", "mich", "hielt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "PWS", "VVFIN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch da war von deiner Hand", "tokens": ["Doch", "da", "war", "von", "dei\u00b7ner", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Meine Chiffer leis\u2019 gezogen,", "tokens": ["Mei\u00b7ne", "Chif\u00b7fer", "leis'", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nieder blickt\u2019 ich dir gewogen.", "tokens": ["Nie\u00b7der", "blickt'", "ich", "dir", "ge\u00b7wo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Hier am Ende des Canals", "tokens": ["Hier", "am", "En\u00b7de", "des", "Ca\u00b7nals"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Der gereihten Hauptallee", "tokens": ["Der", "ge\u00b7reih\u00b7ten", "Haupt\u00b7al\u00b7lee"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Blick\u2019 ich wieder in die H\u00f6h,", "tokens": ["Blick'", "ich", "wie\u00b7der", "in", "die", "H\u00f6h", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und da seh\u2019 ich abermals", "tokens": ["Und", "da", "seh'", "ich", "a\u00b7ber\u00b7mals"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.5": {"text": "Meine Lettern fein gezogen.", "tokens": ["Mei\u00b7ne", "Let\u00b7tern", "fein", "ge\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bleibe! bleibe mir gewogen!", "tokens": ["Blei\u00b7be", "!", "blei\u00b7be", "mir", "ge\u00b7wo\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "M\u00f6ge Wasser springend, wallend,", "tokens": ["M\u00f6\u00b7ge", "Was\u00b7ser", "sprin\u00b7gend", ",", "wal\u00b7lend", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$,", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die Cypressen dir gestehn:", "tokens": ["Die", "Cyp\u00b7res\u00b7sen", "dir", "ge\u00b7stehn", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Von Suleika zu Suleika", "tokens": ["Von", "Sul\u00b7ei\u00b7ka", "zu", "Sul\u00b7ei\u00b7ka"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "APPR", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist mein Kommen und mein Gehn.", "tokens": ["Ist", "mein", "Kom\u00b7men", "und", "mein", "Gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}