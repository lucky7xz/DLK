{"textgrid.poem.65101": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "Unterscheidung", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ja! selig sind die friedlichen Gem\u00fcther,", "tokens": ["Ja", "!", "se\u00b7lig", "sind", "die", "fried\u00b7li\u00b7chen", "Ge\u00b7m\u00fc\u00b7ther", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "ADJD", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die nie am Abgrund finstern Zweifels schwanken,", "tokens": ["Die", "nie", "am", "Ab\u00b7grund", "fins\u00b7tern", "Zwei\u00b7fels", "schwan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPRART", "NN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die sich umschirmt mit fester Satzung Schranken,", "tokens": ["Die", "sich", "um\u00b7schirmt", "mit", "fes\u00b7ter", "Sat\u00b7zung", "Schran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "VVFIN", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und leben als des Glaubensschatzes H\u00fcther.", "tokens": ["Und", "le\u00b7ben", "als", "des", "Glau\u00b7bens\u00b7schat\u00b7zes", "H\u00fct\u00b7her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOKOM", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch heilig nenne ich die Schaar ergl\u00fchter,", "tokens": ["Doch", "hei\u00b7lig", "nen\u00b7ne", "ich", "die", "Schaar", "er\u00b7gl\u00fch\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Furchtloser Geister, deren Lichtgedanken,", "tokens": ["Furcht\u00b7lo\u00b7ser", "Geis\u00b7ter", ",", "de\u00b7ren", "Licht\u00b7ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELAT", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Statt um die fremde St\u00fctze sich zu ranken,", "tokens": ["Statt", "um", "die", "frem\u00b7de", "St\u00fct\u00b7ze", "sich", "zu", "ran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Selbander k\u00e4mpfen um das Gut der G\u00fcter.", "tokens": ["Sel\u00b7ban\u00b7der", "k\u00e4mp\u00b7fen", "um", "das", "Gut", "der", "G\u00fc\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Denn also br\u00fcnstig ist ihr Gottverlangen,", "tokens": ["Denn", "al\u00b7so", "br\u00fcns\u00b7tig", "ist", "ihr", "Gott\u00b7ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df keine Lehre fernher \u00fcberkommen", "tokens": ["Da\u00df", "kei\u00b7ne", "Leh\u00b7re", "fern\u00b7her", "\u00fc\u00b7ber\u00b7kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIAT", "NN", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Befried'gen kann, der Seele Sehnsuchtbangen.", "tokens": ["Be\u00b7frie\u00b7d'\u00b7gen", "kann", ",", "der", "See\u00b7le", "Sehn\u00b7sucht\u00b7ban\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVINF", "VMFIN", "$,", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}, "stanza.4": {"line.1": {"text": "Drum selig, wem des Glaubens Licht erglommen.", "tokens": ["Drum", "se\u00b7lig", ",", "wem", "des", "Glau\u00b7bens", "Licht", "er\u00b7glom\u00b7men", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADJD", "$,", "PWS", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch, die im Kampf' zu Gott empor sich rangen,", "tokens": ["Doch", ",", "die", "im", "Kampf'", "zu", "Gott", "em\u00b7por", "sich", "ran\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PRELS", "APPRART", "NN", "APPR", "NN", "PTKVZ", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das sind die Heiligen, die Heldenfrommen.", "tokens": ["Das", "sind", "die", "Hei\u00b7li\u00b7gen", ",", "die", "Hel\u00b7den\u00b7from\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}