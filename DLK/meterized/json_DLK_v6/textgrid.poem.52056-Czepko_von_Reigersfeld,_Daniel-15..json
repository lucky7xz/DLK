{"textgrid.poem.52056": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "15.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es kam ein Handels Mann nechst einem Fleischer bey,", "tokens": ["Es", "kam", "ein", "Han\u00b7dels", "Mann", "nechst", "ei\u00b7nem", "Flei\u00b7scher", "bey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die zugen sich nun auff, und schertzten beyde frey:", "tokens": ["Die", "zu\u00b7gen", "sich", "nun", "auff", ",", "und", "schertz\u00b7ten", "bey\u00b7de", "frey", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PRF", "ADV", "PTKVZ", "$,", "KON", "VVFIN", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der von dem Beile sprach: La\u00df mich, sonst ruff ich dir,", "tokens": ["Der", "von", "dem", "Bei\u00b7le", "sprach", ":", "La\u00df", "mich", ",", "sonst", "ruff", "ich", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$.", "VVIMP", "PPER", "$,", "ADV", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Knecht kom in Stall und halt allda die Ochsen mir:", "tokens": ["Knecht", "kom", "in", "Stall", "und", "halt", "all\u00b7da", "die", "Och\u00b7sen", "mir", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "NN", "KON", "VVFIN", "ADV", "ART", "NN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der Handelsmann erwuscht ihn stracks und sprach: Wie nu?", "tokens": ["Der", "Han\u00b7dels\u00b7mann", "er\u00b7wuscht", "ihn", "stracks", "und", "sprach", ":", "Wie", "nu", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "KON", "VVFIN", "$.", "PWAV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ich halt ihn schon, steht auff Ihr G\u00e4st und schlaget zu.", "tokens": ["Ich", "halt", "ihn", "schon", ",", "steht", "auff", "Ihr", "G\u00e4st", "und", "schla\u00b7get", "zu", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "VVFIN", "APPR", "PPOSAT", "NN", "KON", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}