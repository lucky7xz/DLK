{"textgrid.poem.62296": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "Am 23. Juli 1894", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie, die ich so hei\u00df geliebt,", "tokens": ["Sie", ",", "die", "ich", "so", "hei\u00df", "ge\u00b7liebt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "ADV", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sie, die niemals mich betr\u00fcbt,", "tokens": ["Sie", ",", "die", "nie\u00b7mals", "mich", "be\u00b7tr\u00fcbt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ADV", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sie, die meiner Augen Licht,", "tokens": ["Sie", ",", "die", "mei\u00b7ner", "Au\u00b7gen", "Licht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach, ich sehe sie doch nicht!", "tokens": ["Ach", ",", "ich", "se\u00b7he", "sie", "doch", "nicht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "PPER", "ADV", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sah't ihr jemals ein Gesicht,", "tokens": ["Sah't", "ihr", "je\u00b7mals", "ein", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Das die Himmelssprache spricht,", "tokens": ["Das", "die", "Him\u00b7mels\u00b7spra\u00b7che", "spricht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sah't ihr jemals einen Blick,", "tokens": ["Sah't", "ihr", "je\u00b7mals", "ei\u00b7nen", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Liebe strahlend, G\u00fcte, Gl\u00fcck:", "tokens": ["Lie\u00b7be", "strah\u00b7lend", ",", "G\u00fc\u00b7te", ",", "Gl\u00fcck", ":"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "VVPP", "$,", "NN", "$,", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ach, dann habt ihr sie gesehen,", "tokens": ["Ach", ",", "dann", "habt", "ihr", "sie", "ge\u00b7se\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "VAFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meine Mutter in den H\u00f6hen,", "tokens": ["Mei\u00b7ne", "Mut\u00b7ter", "in", "den", "H\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo die Engel aufw\u00e4rts gehen,", "tokens": ["Wo", "die", "En\u00b7gel", "auf\u00b7w\u00e4rts", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Denn die Engel reichen nicht", "tokens": ["Denn", "die", "En\u00b7gel", "rei\u00b7chen", "nicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "PTKNEG"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Bis hinauf zu ihrem Licht,", "tokens": ["Bis", "hin\u00b7auf", "zu", "ih\u00b7rem", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Das ist Wahrheit, nicht Gedicht!", "tokens": ["Das", "ist", "Wahr\u00b7heit", ",", "nicht", "Ge\u00b7dicht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NN", "$,", "PTKNEG", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}