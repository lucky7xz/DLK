{"textgrid.poem.51221": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "10. Das G\u00e4rtlein dicht verschlossen", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das G\u00e4rtlein dicht verschlossen", "tokens": ["Das", "G\u00e4rt\u00b7lein", "dicht", "ver\u00b7schlos\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "H\u00e4ltst wohl du, frommes Kind,", "tokens": ["H\u00e4ltst", "wohl", "du", ",", "from\u00b7mes", "Kind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da diese Heckensprossen", "tokens": ["Da", "die\u00b7se", "He\u00b7ckens\u00b7pros\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So eng verwachsen sind?", "tokens": ["So", "eng", "ver\u00b7wach\u00b7sen", "sind", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Doch bl\u00fcht die Unschuld immer", "tokens": ["Doch", "bl\u00fcht", "die", "Un\u00b7schuld", "im\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Darin, soviel ich seh;", "tokens": ["Da\u00b7rin", ",", "so\u00b7viel", "ich", "seh", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sonst war es Lilienschimmer,", "tokens": ["Sonst", "war", "es", "Li\u00b7li\u00b7en\u00b7schim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Nun ist es wei\u00dfer Schnee!", "tokens": ["Nun", "ist", "es", "wei\u00b7\u00dfer", "Schnee", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Als h\u00e4tt der gnadenreichen", "tokens": ["Als", "h\u00e4tt", "der", "gna\u00b7den\u00b7rei\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "VAFIN", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Maria reinste Hand", "tokens": ["Ma\u00b7ria", "reins\u00b7te", "Hand"], "token_info": ["word", "word", "word"], "pos": ["NE", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Im Sonnenschein zum Bleichen", "tokens": ["Im", "Son\u00b7nen\u00b7schein", "zum", "Blei\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ihr Hemdlein ausgespannt.", "tokens": ["Ihr", "Hemd\u00b7lein", "aus\u00b7ge\u00b7spannt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}