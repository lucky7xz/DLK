{"textgrid.poem.60867": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Pindar", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht auf irdischer Flur hast solchen Gesang du gelernt je,", "tokens": ["Nicht", "auf", "ir\u00b7di\u00b7scher", "Flur", "hast", "sol\u00b7chen", "Ge\u00b7sang", "du", "ge\u00b7lernt", "je", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ADJA", "NN", "VAFIN", "PIAT", "NN", "PPER", "VVPP", "ADV", "$,"], "meter": "-+---+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Pindaros! Jegliche Nacht stiegst zum Olymp du hinauf,", "tokens": ["Pin\u00b7da\u00b7ros", "!", "Jeg\u00b7li\u00b7che", "Nacht", "stiegst", "zum", "O\u00b7lymp", "du", "hin\u00b7auf", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PIAT", "NN", "VVFIN", "APPRART", "NN", "PPER", "PTKVZ", "$,"], "meter": "+--+--+--+-+-+", "measure": "dactylic.tri.plus"}, "line.3": {"text": "Lauschend unsterblichem Lied, und erwachend am Morgen erhubst du", "tokens": ["Lau\u00b7schend", "uns\u00b7terb\u00b7li\u00b7chem", "Lied", ",", "und", "er\u00b7wa\u00b7chend", "am", "Mor\u00b7gen", "er\u00b7hubst", "du"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "ADJA", "NN", "$,", "KON", "VVPP", "APPRART", "NN", "VVFIN", "PPER"], "meter": "+--+--+--+--+--++", "measure": "dactylic.tetra.plus"}, "line.4": {"text": "Hymnen, und sch\u00f6nere noch, als in dem Traum du vernahmst.", "tokens": ["Hym\u00b7nen", ",", "und", "sch\u00f6\u00b7ne\u00b7re", "noch", ",", "als", "in", "dem", "Traum", "du", "ver\u00b7nahmst", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "VVFIN", "ADV", "$,", "KOUS", "APPR", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}}}}}