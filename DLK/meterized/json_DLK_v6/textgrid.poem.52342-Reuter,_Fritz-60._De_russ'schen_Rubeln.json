{"textgrid.poem.52342": {"metadata": {"author": {"name": "Reuter, Fritz", "birth": "N.A.", "death": "N.A."}, "title": "60. De russ'schen Rubeln", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "De Fuhrmann Matz, de was mal einen Juden", "tokens": ["De", "Fuhr\u00b7mann", "Matz", ",", "de", "was", "mal", "ei\u00b7nen", "Ju\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "NE", "$,", "NE", "PWS", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Fiw Daler s\u00f6\u00dfteihn Gr\u00f6schen sch\u00fcllig", "tokens": ["Fiw", "Da\u00b7ler", "s\u00f6\u00df\u00b7teihn", "Gr\u00f6\u00b7schen", "sch\u00fcl\u00b7lig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NE", "NE", "NN", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Un was ok tau betahlen willig;", "tokens": ["Un", "was", "ok", "tau", "be\u00b7tah\u00b7len", "wil\u00b7lig", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "VVFIN", "ADJD", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Doch ob em dat tau lang w\u00fcrd duren", "tokens": ["Doch", "ob", "em", "dat", "tau", "lang", "w\u00fcrd", "du\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PIS", "ART", "NN", "ADJD", "VAFIN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Un ob hei gl\u00f6wt, dat hei nicks kreg,", "tokens": ["Un", "ob", "hei", "gl\u00f6wt", ",", "dat", "hei", "nicks", "kreg", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "NE", "VVFIN", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "De Jud, de lep nah't Rathus rup,", "tokens": ["De", "Jud", ",", "de", "lep", "nah't", "Ra\u00b7thus", "rup", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wo hei denn sine Klag v\u00f6rdr\u00f6g,", "tokens": ["Wo", "hei", "denn", "si\u00b7ne", "Klag", "v\u00f6r\u00b7dr\u00f6g", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "KON", "PPOSAT", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Un Matz, de s\u00fcll betahlen nu de ganze Supp", "tokens": ["Un", "Matz", ",", "de", "s\u00fcll", "be\u00b7tah\u00b7len", "nu", "de", "gan\u00b7ze", "Supp"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "$,", "FM.da", "FM.da", "FM.da", "FM.da", "FM.da", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Mit H\u00fctt un M\u00fctt un all de Kosten.", "tokens": ["Mit", "H\u00fctt", "un", "M\u00fctt", "un", "all", "de", "Kos\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "FM", "FM", "FM", "PIAT", "ADJA", "NN", "$."], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Dat was denn nu en sch\u00f6nen Posten,", "tokens": ["Dat", "was", "denn", "nu", "en", "sch\u00f6\u00b7nen", "Pos\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ADV", "ADV", "FM", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Un argern ded't em ganz gef\u00e4hrlich;", "tokens": ["Un", "ar\u00b7gern", "ded't", "em", "ganz", "ge\u00b7f\u00e4hr\u00b7lich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Denn dat s\u00fcll gliksten afmakt sin.", "tokens": ["Denn", "dat", "s\u00fcll", "gliks\u00b7ten", "af\u00b7makt", "sin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Geld tau verdeinen was beswerlich,", "tokens": ["Geld", "tau", "ver\u00b7dei\u00b7nen", "was", "bes\u00b7wer\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "PWS", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Un in sin Taschen was nicks in.", "tokens": ["Un", "in", "sin", "Ta\u00b7schen", "was", "nicks", "in", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "NN", "PWS", "PIS", "APPR", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.15": {"text": "Dat was dunnmals de slimme Tid,", "tokens": ["Dat", "was", "dunn\u00b7mals", "de", "slim\u00b7me", "Tid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.16": {"text": "As Krig was \u00fcm uns sid un wid", "tokens": ["As", "Krig", "was", "\u00fcm", "uns", "sid", "un", "wid"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["FM", "FM", "FM", "FM", "FM", "FM", "FM", "FM"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Un as as Fr\u00fcn'n de Russen kemen", "tokens": ["Un", "as", "as", "Fr\u00fcn'n", "de", "Rus\u00b7sen", "ke\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["FM", "FM", "FM", "NN", "NE", "NE", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Un allens, wat wi hadden, nemen", "tokens": ["Un", "al\u00b7lens", ",", "wat", "wi", "had\u00b7den", ",", "ne\u00b7men"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["NE", "ADV", "$,", "FM.la", "FM.la", "FM.la", "$,", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Un rottenkal un arm uns freten", "tokens": ["Un", "rot\u00b7ten\u00b7kal", "un", "arm", "uns", "fre\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADJD", "FM", "ADJD", "PPER", "VVFIN"], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.20": {"text": "Un stats Betahlung L\u00fcs' uns leten.", "tokens": ["Un", "stats", "Be\u00b7tah\u00b7lung", "L\u00fcs'", "uns", "le\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "NE", "PPER", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.21": {"text": "As Matz nu von dat Rathus g\u00fcng tau Hus,", "tokens": ["As", "Matz", "nu", "von", "dat", "Ra\u00b7thus", "g\u00fcng", "tau", "Hus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADV", "APPR", "ART", "NN", "NE", "NE", "NE", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.22": {"text": "Dunn knep un bet em ok 'ne Lus;", "tokens": ["Dunn", "knep", "un", "bet", "em", "ok", "'ne", "Lus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.23": {"text": "Em j\u00e4kt dat Fell, hei kratzt un seggt:", "tokens": ["Em", "j\u00e4kt", "dat", "Fell", ",", "hei", "kratzt", "un", "seggt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$,", "FM", "FM", "FM", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.24": {"text": "\u00bbt\u00e4uw, du oll Sliker, Moses Schmuhl,", "tokens": ["\u00bb", "t\u00e4uw", ",", "du", "oll", "Sli\u00b7ker", ",", "Mo\u00b7ses", "Schmuhl", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "NE", "$,", "PPER", "ADJD", "NN", "$,", "NE", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Ick strak di doch noch \u00e4wer't Mul.\u00ab", "tokens": ["Ick", "strak", "di", "doch", "noch", "\u00e4\u00b7wer't", "Mul", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "NE", "ADV", "ADV", "ADJD", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Un as hei dor noch \u00e4wer gr\u00fcweln deiht, dunn dr\u00f6ppt", "tokens": ["Un", "as", "hei", "dor", "noch", "\u00e4\u00b7wer", "gr\u00fc\u00b7weln", "deiht", ",", "dunn", "dr\u00f6ppt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["FM", "FM", "FM", "FM", "ADV", "ADJD", "VVINF", "VVFIN", "$,", "ADJA", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.27": {"text": "Sick dat, dat buten einer fr\u00f6ggt:", "tokens": ["Sick", "dat", ",", "dat", "bu\u00b7ten", "ei\u00b7ner", "fr\u00f6ggt", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "ART", "NN", "ART", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "\u00bbkamrad, is dies die Fuhrmann Matzen Haus?\u00ab", "tokens": ["\u00bb", "kam\u00b7rad", ",", "is", "dies", "die", "Fuhr\u00b7mann", "Mat\u00b7zen", "Haus", "?", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "$,", "FM", "PDS", "ART", "NN", "NN", "NN", "$.", "$("], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.29": {"text": "\u00bbja\u00ab, seggt de anner, un de irst, de r\u00f6ppt:", "tokens": ["\u00bb", "ja", "\u00ab", ",", "seggt", "de", "an\u00b7ner", ",", "un", "de", "irst", ",", "de", "r\u00f6ppt", ":"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PTKANT", "$(", "$,", "FM.fr", "FM.fr", "FM.fr", "$,", "FM", "FM", "FM", "$,", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "\u00bbdu, Fuhrmann Matz, steck Fenster Kopp heraus!\u00ab", "tokens": ["\u00bb", "du", ",", "Fuhr\u00b7mann", "Matz", ",", "steck", "Fens\u00b7ter", "Kopp", "he\u00b7raus", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "$,", "NE", "NE", "$,", "ADJD", "NN", "NE", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "Un Matz, de kikt denn d\u00f6rch de Ruten", "tokens": ["Un", "Matz", ",", "de", "kikt", "denn", "d\u00f6rch", "de", "Ru\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "$,", "NE", "VVFIN", "ADV", "APPR", "NE", "NE"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Un s\u00fcht en Russen stahn dor buten,", "tokens": ["Un", "s\u00fcht", "en", "Rus\u00b7sen", "stahn", "dor", "bu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.33": {"text": "So'n rechten ollen Unteroffezierer", "tokens": ["So'n", "rech\u00b7ten", "ol\u00b7len", "Un\u00b7te\u00b7rof\u00b7fe\u00b7zie\u00b7rer"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.34": {"text": "Mit Degen, Kantschu un so wider,", "tokens": ["Mit", "De\u00b7gen", ",", "Kant\u00b7schu", "un", "so", "wi\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "FM", "FM", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.35": {"text": "Un de steiht dor un r\u00f6ppt em tau:", "tokens": ["Un", "de", "steiht", "dor", "un", "r\u00f6ppt", "em", "tau", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.36": {"text": "\u00bbdu, Fuhrmann Matz, pa\u00df, was ich sag, genau:", "tokens": ["\u00bb", "du", ",", "Fuhr\u00b7mann", "Matz", ",", "pa\u00df", ",", "was", "ich", "sag", ",", "ge\u00b7nau", ":"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "PPER", "$,", "NE", "NE", "$,", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$,", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.37": {"text": "Punkt morgen fr\u00fch, Punkt Klock Schlag acht Uhr,", "tokens": ["Punkt", "mor\u00b7gen", "fr\u00fch", ",", "Punkt", "Klock", "Schlag", "acht", "Uhr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "$,", "NN", "NE", "NN", "CARD", "NN", "$,"], "meter": "+---+-+-+", "measure": "dactylic.init"}, "line.38": {"text": "F\u00fchrst Pferd mit zwei auf Markt du vur", "tokens": ["F\u00fchrst", "Pferd", "mit", "zwei", "auf", "Markt", "du", "vur"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "APPR", "CARD", "APPR", "NE", "NE", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Die Leuchtmont, Geldgebel un Kaptain", "tokens": ["Die", "Leucht\u00b7mont", ",", "Geld\u00b7ge\u00b7bel", "un", "Kap\u00b7ta\u00b7in"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "NN", "FM", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.40": {"text": "Un f\u00e4hrst in Nowo-Strelitz rein,", "tokens": ["Un", "f\u00e4hrst", "in", "No\u00b7wo\u00b7Stre\u00b7litz", "rein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "NE", "ADJD", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.41": {"text": "Sie blank dir f\u00fcnfe Rubel biet.", "tokens": ["Sie", "blank", "dir", "f\u00fcn\u00b7fe", "Ru\u00b7bel", "biet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "NE", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Un wenn ich sag', was nich geschieht ...\u00ab", "tokens": ["Un", "wenn", "ich", "sag'", ",", "was", "nich", "ge\u00b7schieht", "...", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NE", "KOUS", "PPER", "VVFIN", "$,", "PRELS", "PTKNEG", "VVFIN", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.43": {"text": "Hir langt hei nah den Kantschu mit de Hand,", "tokens": ["Hir", "langt", "hei", "nah", "den", "Kant\u00b7schu", "mit", "de", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ADJD", "ART", "NN", "APPR", "NE", "NN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.44": {"text": "\u00bbrosomi? Sag', hast mein verstand?\u00ab", "tokens": ["\u00bb", "ro\u00b7so\u00b7mi", "?", "Sag'", ",", "hast", "mein", "ver\u00b7stand", "?", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "NE", "$.", "NN", "$,", "VAFIN", "PPOSAT", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.45": {"text": "Wat sull hei dauhn? \u2013 Den annern Morgen fr\u00fch", "tokens": ["Wat", "sull", "hei", "dauhn", "?", "\u2013", "Den", "an\u00b7nern", "Mor\u00b7gen", "fr\u00fch"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$.", "$(", "ART", "ADJA", "NN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.46": {"text": "Is Matz all up den Mark, un de oll Rosomi,", "tokens": ["Is", "Matz", "all", "up", "den", "Mark", ",", "un", "de", "oll", "Ro\u00b7so\u00b7mi", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PIAT", "NN", "ART", "NN", "$,", "FM", "FM", "FM", "FM", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.47": {"text": "De stiggt nu up den Wagen rupper", "tokens": ["De", "stiggt", "nu", "up", "den", "Wa\u00b7gen", "rup\u00b7per"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ADV", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.48": {"text": "Un giwwt em af un an en Swupper,", "tokens": ["Un", "giwwt", "em", "af", "un", "an", "en", "Swup\u00b7per", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "---+-+-+-", "measure": "unknown.measure.tri"}, "line.49": {"text"