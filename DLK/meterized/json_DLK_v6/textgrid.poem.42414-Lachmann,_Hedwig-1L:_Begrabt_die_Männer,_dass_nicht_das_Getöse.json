{"textgrid.poem.42414": {"metadata": {"author": {"name": "Lachmann, Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Begrabt die M\u00e4nner, dass nicht das Get\u00f6se", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Begrabt die M\u00e4nner, dass nicht das Get\u00f6se", "tokens": ["Be\u00b7grabt", "die", "M\u00e4n\u00b7ner", ",", "dass", "nicht", "das", "Ge\u00b7t\u00f6\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "KOUS", "PTKNEG", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Schlachtgemenges l\u00e4nger sie umschallt,", "tokens": ["Des", "Schlacht\u00b7ge\u00b7men\u00b7ges", "l\u00e4n\u00b7ger", "sie", "um\u00b7schallt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und dass vom Todeskrampf, der sie umkrallt,", "tokens": ["Und", "dass", "vom", "To\u00b7des\u00b7krampf", ",", "der", "sie", "um\u00b7krallt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPRART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Erde ihre starren Glieder l\u00f6se.", "tokens": ["Die", "Er\u00b7de", "ih\u00b7re", "star\u00b7ren", "Glie\u00b7der", "l\u00f6\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "O traure, traure, Herz, an den Gebeinen", "tokens": ["O", "trau\u00b7re", ",", "trau\u00b7re", ",", "Herz", ",", "an", "den", "Ge\u00b7bei\u00b7nen"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "$,", "VVFIN", "$,", "NN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Der Mannheit, die dem rohen Schwert erlag,", "tokens": ["Der", "Mann\u00b7heit", ",", "die", "dem", "ro\u00b7hen", "Schwert", "er\u00b7lag", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zehntausend starben dir an einem Tag,", "tokens": ["Zehn\u00b7tau\u00b7send", "star\u00b7ben", "dir", "an", "ei\u00b7nem", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Beweine sie, als weintest du um einen!", "tokens": ["Be\u00b7wei\u00b7ne", "sie", ",", "als", "wein\u00b7test", "du", "um", "ei\u00b7nen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "VVFIN", "PPER", "APPR", "ART", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Auf fahlen \u00c4ckern stockt in breiten Spuren", "tokens": ["Auf", "fah\u00b7len", "\u00c4\u00b7ckern", "stockt", "in", "brei\u00b7ten", "Spu\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das frisch vergossene, noch warme Blut;", "tokens": ["Das", "frisch", "ver\u00b7gos\u00b7se\u00b7ne", ",", "noch", "war\u00b7me", "Blut", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "ADJA", "$,", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Vergeudet, wie ein allzufr\u00fches Gut,", "tokens": ["Ver\u00b7geu\u00b7det", ",", "wie", "ein", "all\u00b7zu\u00b7fr\u00fc\u00b7hes", "Gut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PWAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Verwest die Frucht der M\u00fctter auf den Fluren.", "tokens": ["Ver\u00b7west", "die", "Frucht", "der", "M\u00fct\u00b7ter", "auf", "den", "Flu\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Mit Dunkel \u00fcberziehend ihre Namen,", "tokens": ["Mit", "Dun\u00b7kel", "\u00fc\u00b7ber\u00b7zie\u00b7hend", "ih\u00b7re", "Na\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sprengt \u00fcber sie der erzbeschlagne Tross,", "tokens": ["Sprengt", "\u00fc\u00b7ber", "sie", "der", "erz\u00b7be\u00b7schlag\u00b7ne", "Tross", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dicht Mann bei Mann, erl\u00f6schen Stamm und Spross,", "tokens": ["Dicht", "Mann", "bei", "Mann", ",", "er\u00b7l\u00f6\u00b7schen", "Stamm", "und", "Spross", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "APPR", "NN", "$,", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und auf verheertem Grund zerf\u00e4llt ihr Samen.", "tokens": ["Und", "auf", "ver\u00b7heer\u00b7tem", "Grund", "zer\u00b7f\u00e4llt", "ihr", "Sa\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Begrabt die M\u00e4nner, dass nicht das Get\u00f6se", "tokens": ["Be\u00b7grabt", "die", "M\u00e4n\u00b7ner", ",", "dass", "nicht", "das", "Ge\u00b7t\u00f6\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "KOUS", "PTKNEG", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Schlachtgemenges l\u00e4nger sie umschallt,", "tokens": ["Des", "Schlacht\u00b7ge\u00b7men\u00b7ges", "l\u00e4n\u00b7ger", "sie", "um\u00b7schallt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und dass vom Todeskrampf, der sie umkrallt,", "tokens": ["Und", "dass", "vom", "To\u00b7des\u00b7krampf", ",", "der", "sie", "um\u00b7krallt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPRART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Erde ihre starren Glieder l\u00f6se.", "tokens": ["Die", "Er\u00b7de", "ih\u00b7re", "star\u00b7ren", "Glie\u00b7der", "l\u00f6\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}