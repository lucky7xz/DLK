{"dta.poem.6679": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Considerations sur l\u2019Ou\u00efe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Durch einen muntern Klang, der Lerm und Ruhm begreifft,", "tokens": ["Durch", "ei\u00b7nen", "mun\u00b7tern", "Klang", ",", "der", "Lerm", "und", "Ruhm", "be\u00b7greifft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "ART", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "F\u00fchlt Alexander sich bewegen,", "tokens": ["F\u00fchlt", "A\u00b7lex\u00b7an\u00b7der", "sich", "be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und greifft, gantz ausser sich, vor Wuth,", "tokens": ["Und", "greifft", ",", "gantz", "aus\u00b7ser", "sich", ",", "vor", "Wuth", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ADV", "APPR", "PRF", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bey einer Gasterey zum Degen.", "tokens": ["Bey", "ei\u00b7ner", "Gas\u00b7te\u00b7rey", "zum", "De\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Hingegen siehet man das Blut", "tokens": ["Hin\u00b7ge\u00b7gen", "sie\u00b7het", "man", "das", "Blut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "ART", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "Durch sanfte T\u00f6ne sich bes\u00e4nftigen, sich legen.", "tokens": ["Durch", "sanf\u00b7te", "T\u00f6\u00b7ne", "sich", "be\u00b7s\u00e4nf\u00b7ti\u00b7gen", ",", "sich", "le\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PRF", "VVPP", "$,", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der Friede folgt auf Zorn und W\u00fcten. Saul wird frey", "tokens": ["Der", "Frie\u00b7de", "folgt", "auf", "Zorn", "und", "W\u00fc\u00b7ten", ".", "Saul", "wird", "frey"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$.", "NE", "VAFIN", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Von seiner Noth; es weicht der Geist der Raserey", "tokens": ["Von", "sei\u00b7ner", "Noth", ";", "es", "weicht", "der", "Geist", "der", "Ra\u00b7se\u00b7rey"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Den Augenblick, wann die entbrannte Gall\u2019", "tokens": ["Den", "Au\u00b7gen\u00b7blick", ",", "wann", "die", "ent\u00b7brann\u00b7te", "Gall'"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "In ihm bes\u00e4nftigt wird durch s\u00fcssen Saiten-Schall.", "tokens": ["In", "ihm", "be\u00b7s\u00e4nf\u00b7tigt", "wird", "durch", "s\u00fcs\u00b7sen", "Sai\u00b7ten\u00b7Schall", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "VAFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}