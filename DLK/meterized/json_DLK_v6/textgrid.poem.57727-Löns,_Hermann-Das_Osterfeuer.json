{"textgrid.poem.57727": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Das Osterfeuer", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00dcber die Heide ging ich, die Heide so weit und so breit,", "tokens": ["\u00dc\u00b7ber", "die", "Hei\u00b7de", "ging", "ich", ",", "die", "Hei\u00b7de", "so", "weit", "und", "so", "breit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "VVFIN", "PPER", "$,", "ART", "NN", "ADV", "ADJD", "KON", "ADV", "ADJD", "$,"], "meter": "+--+-+--+--+--+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "M\u00fcrrische Worte raunte ins Ohr mir die Einsamkeit.", "tokens": ["M\u00fcr\u00b7ri\u00b7sche", "Wor\u00b7te", "raun\u00b7te", "ins", "Ohr", "mir", "die", "Ein\u00b7sam\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPRART", "NN", "PPER", "ART", "NN", "$."], "meter": "+--+-+--+--+-+", "measure": "iambic.hexa.invert"}}, "stanza.2": {"line.1": {"text": "Raunte von toten Zeiten, da hier noch der Urstier zog,", "tokens": ["Raun\u00b7te", "von", "to\u00b7ten", "Zei\u00b7ten", ",", "da", "hier", "noch", "der", "Ur\u00b7stier", "zog", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,", "KOUS", "ADV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-+-+-+", "measure": "iambic.septa.invert"}, "line.2": {"text": "\u00dcber dem Bruche der Adler himmelhoch kreisend flog;", "tokens": ["\u00dc\u00b7ber", "dem", "Bru\u00b7che", "der", "Ad\u00b7ler", "him\u00b7mel\u00b7hoch", "krei\u00b7send", "flog", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "ADV", "ADJD", "VVFIN", "$."], "meter": "+--+--+-+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Da der Grauhund, der grimme, Mordrunen lie\u00df im Sand,", "tokens": ["Da", "der", "Grau\u00b7hund", ",", "der", "grim\u00b7me", ",", "Mor\u00b7dru\u00b7nen", "lie\u00df", "im", "Sand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "ART", "ADJA", "$,", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "--+--+--+-+-+", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Da noch das Elch, das starke, fiel von des J\u00e4gers Hand.", "tokens": ["Da", "noch", "das", "Elch", ",", "das", "star\u00b7ke", ",", "fiel", "von", "des", "J\u00e4\u00b7gers", "Hand", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$,", "PDS", "VVFIN", "$,", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Da noch nicht welsche Weise Gut in B\u00f6se verkehrt,", "tokens": ["Da", "noch", "nicht", "wel\u00b7sche", "Wei\u00b7se", "Gut", "in", "B\u00f6\u00b7se", "ver\u00b7kehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKNEG", "PWAT", "NN", "ADJD", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.2": {"text": "Wode und Frigga, die Hehren, standen hochgeehrt;", "tokens": ["Wo\u00b7de", "und", "Frig\u00b7ga", ",", "die", "Heh\u00b7ren", ",", "stan\u00b7den", "hoch\u00b7geehrt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "KON", "NE", "$,", "ART", "NN", "$,", "VVFIN", "ADJD", "$."], "meter": "---+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Da noch Mannesmut galt und nicht allein das Geld,", "tokens": ["Da", "noch", "Man\u00b7nes\u00b7mut", "galt", "und", "nicht", "al\u00b7lein", "das", "Geld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "VVFIN", "KON", "PTKNEG", "ADV", "ART", "NN", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Da mit dem blanken Schwert wahrte sein Recht der Held;", "tokens": ["Da", "mit", "dem", "blan\u00b7ken", "Schwert", "wahr\u00b7te", "sein", "Recht", "der", "Held", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+---+-+", "measure": "unknown.measure.penta"}}, "stanza.4": {"line.1": {"text": "Nicht mit feigem Worte, und nicht mit billigem Eid;", "tokens": ["Nicht", "mit", "fei\u00b7gem", "Wor\u00b7te", ",", "und", "nicht", "mit", "bil\u00b7li\u00b7gem", "Eid", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ADJA", "NN", "$,", "KON", "PTKNEG", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+--+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Also lehrte mich heimlich die Toteneinsamkeit.", "tokens": ["Al\u00b7so", "lehr\u00b7te", "mich", "heim\u00b7lich", "die", "To\u00b7ten\u00b7ein\u00b7sam\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ADJD", "ART", "NN", "$."], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "Unsere G\u00f6tter die hie\u00dfen einstmals Liebe und Kraft,", "tokens": ["Un\u00b7se\u00b7re", "G\u00f6t\u00b7ter", "die", "hie\u00b7\u00dfen", "einst\u00b7mals", "Lie\u00b7be", "und", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "VVFIN", "ADV", "NN", "KON", "NN", "$,"], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Kraft, die Leben erzeugt, Liebe, die Wonnen schafft.", "tokens": ["Kraft", ",", "die", "Le\u00b7ben", "er\u00b7zeugt", ",", "Lie\u00b7be", ",", "die", "Won\u00b7nen", "schafft", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "VVPP", "$,", "NN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "+-+--++--+-+", "measure": "asklepiade"}}, "stanza.6": {"line.1": {"text": "Unser Gesetz war kurz, unser Gesetz war das:", "tokens": ["Un\u00b7ser", "Ge\u00b7setz", "war", "kurz", ",", "un\u00b7ser", "Ge\u00b7setz", "war", "das", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "PPOSAT", "NN", "VAFIN", "PDS", "$."], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.2": {"text": "Liebe um Liebe, aber auch Ha\u00df um Ha\u00df.", "tokens": ["Lie\u00b7be", "um", "Lie\u00b7be", ",", "a\u00b7ber", "auch", "Ha\u00df", "um", "Ha\u00df", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "ADV", "ADV", "NN", "APPR", "NN", "$."], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}}, "stanza.7": {"line.1": {"text": "Treuhand jedwedem Mann, der sich erwies als Freund,", "tokens": ["Treu\u00b7hand", "jed\u00b7we\u00b7dem", "Mann", ",", "der", "sich", "er\u00b7wies", "als", "Freund", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$,", "PRELS", "PRF", "VVFIN", "KOUS", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bluthand dagegen dem Wicht, so sich da nahte als Feind.", "tokens": ["Blut\u00b7hand", "da\u00b7ge\u00b7gen", "dem", "Wicht", ",", "so", "sich", "da", "nah\u00b7te", "als", "Feind", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PAV", "ART", "NN", "$,", "ADV", "PRF", "ADV", "VVFIN", "KOUS", "NN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}}, "stanza.8": {"line.1": {"text": "Andere Zeiten zogen \u00fcber das Heideland,", "tokens": ["An\u00b7de\u00b7re", "Zei\u00b7ten", "zo\u00b7gen", "\u00fc\u00b7ber", "das", "Hei\u00b7de\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Vor der t\u00fcckischen Axt Wodes Lobewald schwand;", "tokens": ["Vor", "der", "t\u00fc\u00b7cki\u00b7schen", "Axt", "Wo\u00b7des", "Lo\u00b7be\u00b7wald", "schwand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NE", "NE", "VVFIN", "$."], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}}, "stanza.9": {"line.1": {"text": "Frigga die freundliche Fraue wurde zur Hexe verkehrt,", "tokens": ["Frig\u00b7ga", "die", "freund\u00b7li\u00b7che", "Frau\u00b7e", "wur\u00b7de", "zur", "He\u00b7xe", "ver\u00b7kehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "VAFIN", "APPRART", "NN", "VVPP", "$,"], "meter": "+--+--+-+-+-+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Jeglicher heilige Ort zur Greuelst\u00e4tte entehrt;", "tokens": ["Jeg\u00b7li\u00b7cher", "hei\u00b7li\u00b7ge", "Ort", "zur", "Greu\u00b7el\u00b7st\u00e4t\u00b7te", "ent\u00b7ehrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}}, "stanza.10": {"line.1": {"text": "Wodes edles Gefl\u00fcgel hie\u00df Galgenvogel nun,", "tokens": ["Wo\u00b7des", "ed\u00b7les", "Ge\u00b7fl\u00fc\u00b7gel", "hie\u00df", "Gal\u00b7gen\u00b7vo\u00b7gel", "nun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "VVFIN", "NE", "ADV", "$,"], "meter": "--+--+--+-+-+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Friggas schelmisches Eulchen schimpften sie Leichenhuhn;", "tokens": ["Frig\u00b7gas", "schel\u00b7mi\u00b7sches", "Eul\u00b7chen", "schimpf\u00b7ten", "sie", "Lei\u00b7chen\u00b7huhn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "VVFIN", "PPER", "NN", "$."], "meter": "+-+--+-+--+-+", "measure": "trochaic.hexa.relaxed"}}, "stanza.11": {"line.1": {"text": "Und die Dreizehn, die hohe Geheimniszahl,", "tokens": ["Und", "die", "Drei\u00b7zehn", ",", "die", "ho\u00b7he", "Ge\u00b7heim\u00b7nis\u00b7zahl", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "CARD", "$,", "ART", "ADJA", "NN", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Ungl\u00fccks- und Angstnummer wurd' sie mit einemmal.", "tokens": ["Un\u00b7gl\u00fccks", "und", "Angst\u00b7num\u00b7mer", "wurd'", "sie", "mit", "ei\u00b7nem\u00b7mal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["TRUNC", "KON", "NN", "VAFIN", "PPER", "APPR", "ADV", "$."], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}}, "stanza.12": {"line.1": {"text": "Zwischen Eichen erhob sich ein einsames Strohdachhaus,", "tokens": ["Zwi\u00b7schen", "Ei\u00b7chen", "er\u00b7hob", "sich", "ein", "ein\u00b7sa\u00b7mes", "Stroh\u00b7dach\u00b7haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "M\u00e4hrenh\u00e4upter reckte der moosige Giebel heraus;", "tokens": ["M\u00e4h\u00b7ren\u00b7h\u00e4up\u00b7ter", "reck\u00b7te", "der", "moo\u00b7si\u00b7ge", "Gie\u00b7bel", "he\u00b7raus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+--+--+--+", "measure": "trochaic.hexa.relaxed"}}, "stanza.13": {"line.1": {"text": "Unter ihm aber nach freundlicher Altsitte noch", "tokens": ["Un\u00b7ter", "ihm", "a\u00b7ber", "nach", "freund\u00b7li\u00b7cher", "Alt\u00b7sit\u00b7te", "noch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ADV", "APPR", "ADJA", "NN", "ADV"], "meter": "+--+--+-+-+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Eingeschnitten als Herz starrte das Ulenloch.", "tokens": ["Ein\u00b7ge\u00b7schnit\u00b7ten", "als", "Herz", "starr\u00b7te", "das", "U\u00b7len\u00b7loch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "KOKOM", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+--++--+-+", "measure": "asklepiade"}}, "stanza.14": {"line.1": {"text": "An dem Misset\u00fcrbalken, dem grauen, nach alter Weis'", "tokens": ["An", "dem", "Mis\u00b7se\u00b7t\u00fcr\u00b7bal\u00b7ken", ",", "dem", "grau\u00b7en", ",", "nach", "al\u00b7ter", "Weis'"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "ART", "ADJA", "$,", "APPR", "ADJA", "NN"], "meter": "--+--+--+--+-+", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Eingehauen und bunt prangte der heilige Kreis,", "tokens": ["Ein\u00b7ge\u00b7hau\u00b7en", "und", "bunt", "prang\u00b7te", "der", "hei\u00b7li\u00b7ge", "Kreis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+---+--+", "measure": "trochaic.penta.relaxed"}}, "stanza.15": {"line.1": {"text": "Und die Sonnenrune, die gute, daneben auch,", "tokens": ["Und", "die", "Son\u00b7nen\u00b7ru\u00b7ne", ",", "die", "gu\u00b7te", ",", "da\u00b7ne\u00b7ben", "auch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ART", "ADJA", "$,", "PAV", "ADV", "$,"], "meter": "--+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Nach der Urvorv\u00e4ter ernsthaft beharrlichem Brauch.", "tokens": ["Nach", "der", "Ur\u00b7vor\u00b7v\u00e4\u00b7ter", "ernst\u00b7haft", "be\u00b7harr\u00b7li\u00b7chem", "Brauch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}}, "stanza.16": {"line.1": {"text": "Rechts und links von der schwarzblanken Feuerwand", "tokens": ["Rechts", "und", "links", "von", "der", "schwarz\u00b7blan\u00b7ken", "Feu\u00b7er\u00b7wand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KON", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+++-+-+", "measure": "unknown.measure.septa"}, "line.2": {"text": "Wodes Schlachtro\u00df mutig sich b\u00e4umend stand;", "tokens": ["Wo\u00b7des", "Schlach\u00b7tro\u00df", "mu\u00b7tig", "sich", "b\u00e4u\u00b7mend", "stand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJD", "PRF", "ADJD", "VVFIN", "$."], "meter": "--++---+-+", "measure": "anapaest.init"}}, "stanza.17": {"line.1": {"text": "Gleich als wollte es lauthals mir wiehern zu:", "tokens": ["Gleich", "als", "woll\u00b7te", "es", "laut\u00b7hals", "mir", "wie\u00b7hern", "zu", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "VMFIN", "PPER", "ADV", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Noch trage Wode ich, Freund, noch trauest Frigga du.", "tokens": ["Noch", "tra\u00b7ge", "Wo\u00b7de", "ich", ",", "Freund", ",", "noch", "trau\u00b7est", "Frig\u00b7ga", "du", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "PPER", "$,", "NN", "$,", "ADV", "VVFIN", "NE", "NE", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.18": {"line.1": {"text": "Weiter ging ich \u00fcber das d\u00e4mmernde Land,", "tokens": ["Wei\u00b7ter", "ging", "ich", "\u00fc\u00b7ber", "das", "d\u00e4m\u00b7mern\u00b7de", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Hinter dem rund und rot das gute Gestirn verschwand;", "tokens": ["Hin\u00b7ter", "dem", "rund", "und", "rot", "das", "gu\u00b7te", "Ge\u00b7stirn", "ver\u00b7schwand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "KON", "ADJD", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+-+-+--+-+", "measure": "iambic.hexa.invert"}}, "stanza.19": {"line.1": {"text": "Ihm gegen\u00fcber weit hinter dem br\u00e4unlichen Bruch", "tokens": ["Ihm", "ge\u00b7gen\u00b7\u00fc\u00b7ber", "weit", "hin\u00b7ter", "dem", "br\u00e4un\u00b7li\u00b7chen", "Bruch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-++--+--+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Eine gl\u00fchrote Flamm