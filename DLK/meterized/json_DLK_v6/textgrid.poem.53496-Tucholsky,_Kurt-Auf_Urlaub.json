{"textgrid.poem.53496": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Auf Urlaub", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.57", "af:0.28", "fr:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Residenz!", "tokens": ["Die", "Re\u00b7si\u00b7denz", "!"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Gu'n Tag, du Metropole!", "tokens": ["Gu'n", "Tag", ",", "du", "Me\u00b7tro\u00b7po\u00b7le", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da ist auch schon der Alexanderplatz . . .", "tokens": ["Da", "ist", "auch", "schon", "der", "A\u00b7lex\u00b7an\u00b7der\u00b7platz", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADV", "ART", "NN", "$.", "$.", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Verstatte, da\u00df ich mich das Schneuztuch hole,", "tokens": ["Ver\u00b7stat\u00b7te", ",", "da\u00df", "ich", "mich", "das", "Schneuz\u00b7tuch", "ho\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "das Herz schl\u00e4gt st\u00fcrmisch unterm Busenlatz.", "tokens": ["das", "Herz", "schl\u00e4gt", "st\u00fcr\u00b7misch", "un\u00b7term", "Bu\u00b7sen\u00b7latz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Du gute Spree mit dem geduldigen R\u00fccken,", "tokens": ["Du", "gu\u00b7te", "Spree", "mit", "dem", "ge\u00b7dul\u00b7di\u00b7gen", "R\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "der Ruderklubs und der Mamsells Entz\u00fccken \u2013", "tokens": ["der", "Ru\u00b7der\u00b7klubs", "und", "der", "Mam\u00b7sells", "Ent\u00b7z\u00fc\u00b7cken", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "ich seh dich still und m\u00e4chtig dreckig ziehn . . .", "tokens": ["ich", "seh", "dich", "still", "und", "m\u00e4ch\u00b7tig", "dre\u00b7ckig", "ziehn", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "ADJD", "VVINF", "$.", "$.", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Berlin!", "tokens": ["Ber\u00b7lin", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.2": {"line.1": {"text": "Die Weiche knackt. Der Zug zischt an den Hallen", "tokens": ["Die", "Wei\u00b7che", "knackt", ".", "Der", "Zug", "zischt", "an", "den", "Hal\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "der Stadtbahn lang. Da liegt der dicke Dom.", "tokens": ["der", "Stadt\u00b7bahn", "lang", ".", "Da", "liegt", "der", "di\u00b7cke", "Dom", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$.", "ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die pfui! die Friedrichstra\u00dfe will mir recht gefallen,", "tokens": ["Die", "pfui", "!", "die", "Fried\u00b7rich\u00b7stra\u00b7\u00dfe", "will", "mir", "recht", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ITJ", "$.", "ART", "NN", "VMFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "am Charit\u00e9haus gr\u00fcnt ein Appelboom.", "tokens": ["am", "Cha\u00b7rit\u00e9haus", "gr\u00fcnt", "ein", "Ap\u00b7pel\u00b7boom", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die V\u00f6lker auf den Stra\u00dfen sind nicht ohne:", "tokens": ["Die", "V\u00f6l\u00b7ker", "auf", "den", "Stra\u00b7\u00dfen", "sind", "nicht", "oh\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VAFIN", "PTKNEG", "APPR", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "dem Gang nach lauter Jrafens und Barone.", "tokens": ["dem", "Gang", "nach", "lau\u00b7ter", "Jra\u00b7fens", "und", "Ba\u00b7ro\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PIAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Es riecht nach Geld. Prozente, Mensch, verdien!", "tokens": ["Es", "riecht", "nach", "Geld", ".", "Pro\u00b7zen\u00b7te", ",", "Mensch", ",", "ver\u00b7di\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$.", "NN", "$,", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Berlin!", "tokens": ["Ber\u00b7lin", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.3": {"line.1": {"text": "Charlottenburg. Da steht die lange Claire,", "tokens": ["Char\u00b7lot\u00b7ten\u00b7burg", ".", "Da", "steht", "die", "lan\u00b7ge", "Clai\u00b7re", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "den Bastard meiner Liebe an der Hand.", "tokens": ["den", "Bas\u00b7tard", "mei\u00b7ner", "Lie\u00b7be", "an", "der", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ob auch die Rationierung an uns zehre \u2013", "tokens": ["Ob", "auch", "die", "Ra\u00b7ti\u00b7o\u00b7nie\u00b7rung", "an", "uns", "zeh\u00b7re", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "APPR", "PPER", "VVFIN", "$("], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "der Knochenbau h\u00e4lt allen Feinden stand.", "tokens": ["der", "Kno\u00b7chen\u00b7bau", "h\u00e4lt", "al\u00b7len", "Fein\u00b7den", "stand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Das wird die rechte Wiedersehensfeier!", "tokens": ["Das", "wird", "die", "rech\u00b7te", "Wie\u00b7der\u00b7se\u00b7hen\u00b7sfei\u00b7er", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ich hab (im Rucksack) f\u00fcnfundsiebzig Eier \u2013", "tokens": ["Ich", "hab", "(", "im", "Ruck\u00b7sack", ")", "f\u00fcn\u00b7fund\u00b7sieb\u00b7zig", "Ei\u00b7er", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$(", "APPRART", "NN", "$(", "CARD", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Da h\u00e4lt der Zug! Die K\u00fcmmernisse fliehn . . .", "tokens": ["Da", "h\u00e4lt", "der", "Zug", "!", "Die", "K\u00fcm\u00b7mer\u00b7nis\u00b7se", "fliehn", ".", ".", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "ART", "NN", "VVINF", "$.", "$.", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Berlin! Berlin!", "tokens": ["Ber\u00b7lin", "!", "Ber\u00b7lin", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$.", "NE", "$."], "meter": "-OOO", "measure": "unknown.measure.zero"}}}}}