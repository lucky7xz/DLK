{"dta.poem.5404": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "J rdisches  \n  V ergn\u00fcgen  \n in  \n  GOTT .  \n  F \u00fcnfter  T heil.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die noch ein ander Blatt,", "tokens": ["Die", "noch", "ein", "an\u00b7der", "Blatt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Das von des Ueberflusses Horn", "tokens": ["Das", "von", "des", "Ue\u00b7ber\u00b7flus\u00b7ses", "Horn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "APPR", "ART", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die eigentliche Bildung hat,", "tokens": ["Die", "ei\u00b7gent\u00b7li\u00b7che", "Bil\u00b7dung", "hat", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Erblicken l\u00e4\u00dft,", "tokens": ["Er\u00b7bli\u00b7cken", "l\u00e4\u00dft", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "In dessen \u00e4usserm Theil, wo sich das H\u00f6rnchen windet,", "tokens": ["In", "des\u00b7sen", "\u00e4us\u00b7serm", "Theil", ",", "wo", "sich", "das", "H\u00f6rn\u00b7chen", "win\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "ADJA", "NN", "$,", "PWAV", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Man eine S\u00fc\u00dfigkeit gesammlet findet.", "tokens": ["Man", "ei\u00b7ne", "S\u00fc\u00b7\u00dfig\u00b7keit", "ge\u00b7samm\u00b7let", "fin\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "NN", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Die\u00df hohle Bl\u00e4ttchen, das am rothen Stengel fest,", "tokens": ["Die\u00df", "hoh\u00b7le", "Bl\u00e4tt\u00b7chen", ",", "das", "am", "ro\u00b7then", "Sten\u00b7gel", "fest", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJA", "NN", "$,", "PRELS", "APPRART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Scheint nicht allein mit seiner gr\u00fcnen Spitzen", "tokens": ["Scheint", "nicht", "al\u00b7lein", "mit", "sei\u00b7ner", "gr\u00fc\u00b7nen", "Spit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "ADV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Der breiten Bl\u00e4tter Par zu st\u00fctzen;", "tokens": ["Der", "brei\u00b7ten", "Bl\u00e4t\u00b7ter", "Par", "zu", "st\u00fct\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NE", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Es fa\u00dft es recht, als wie in einer holen Schalen,", "tokens": ["Es", "fa\u00dft", "es", "recht", ",", "als", "wie", "in", "ei\u00b7ner", "ho\u00b7len", "Scha\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$,", "KOUS", "KOKOM", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "In welcher die Natur", "tokens": ["In", "wel\u00b7cher", "die", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PRELS", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Noch eine zierliche Figur,", "tokens": ["Noch", "ei\u00b7ne", "zier\u00b7li\u00b7che", "Fi\u00b7gur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Ein g\u00fcldnes Hertz, zu mahlen", "tokens": ["Ein", "g\u00fcld\u00b7nes", "Hertz", ",", "zu", "mah\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Sich stets besch\u00e4ftiget.", "tokens": ["Sich", "stets", "be\u00b7sch\u00e4f\u00b7ti\u00b7get", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "ADV", "VVFIN", "$."], "meter": "++-+-+", "measure": "iambic.tri"}, "line.15": {"text": "Die\u00df Hertzgen so die\u00df hohle Bl\u00e4tgen schm\u00fcckt,", "tokens": ["Die\u00df", "Hertz\u00b7gen", "so", "die\u00df", "hoh\u00b7le", "Bl\u00e4t\u00b7gen", "schm\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "PDS", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Wird durch die Oefnungen der Bl\u00e4ttergen erblickt,", "tokens": ["Wird", "durch", "die", "Oef\u00b7nun\u00b7gen", "der", "Bl\u00e4t\u00b7ter\u00b7gen", "er\u00b7blickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Die, wie gesagt, so sonderlich formirt.", "tokens": ["Die", ",", "wie", "ge\u00b7sagt", ",", "so", "son\u00b7der\u00b7lich", "for\u00b7mirt", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PWAV", "VVPP", "$,", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "An aller dieser Bl\u00e4tter Fu\u00df", "tokens": ["An", "al\u00b7ler", "die\u00b7ser", "Bl\u00e4t\u00b7ter", "Fu\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "PDAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Sieht man ein gr\u00fcnes K\u00f6lbgen sitzen,", "tokens": ["Sieht", "man", "ein", "gr\u00fc\u00b7nes", "K\u00f6lb\u00b7gen", "sit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "De\u00df ich annoch erwehnen mu\u00df.", "tokens": ["De\u00df", "ich", "an\u00b7noch", "er\u00b7weh\u00b7nen", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Die\u00df ist von l\u00e4nglichter Figur,", "tokens": ["Die\u00df", "ist", "von", "l\u00e4ng\u00b7lich\u00b7ter", "Fi\u00b7gur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.22": {"text": "Und hat viel tausend weisse Spitzen.", "tokens": ["Und", "hat", "viel", "tau\u00b7send", "weis\u00b7se", "Spit\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "CARD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Hier hat die sich erhaltende Natur", "tokens": ["Hier", "hat", "die", "sich", "er\u00b7hal\u00b7ten\u00b7de", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "PRF", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Den Schatz des Saamens eingeleget,", "tokens": ["Den", "Schatz", "des", "Saa\u00b7mens", "ein\u00b7ge\u00b7le\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Den es nicht nur, als ein Beh\u00e4lter, heget,", "tokens": ["Den", "es", "nicht", "nur", ",", "als", "ein", "Be\u00b7h\u00e4l\u00b7ter", ",", "he\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PPER", "PTKNEG", "ADV", "$,", "KOUS", "ART", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Nein den es gar, so bald der Saame reifft,", "tokens": ["Nein", "den", "es", "gar", ",", "so", "bald", "der", "Saa\u00b7me", "reifft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ART", "PPER", "ADV", "$,", "ADV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Indem es sich so dann sehr schnell zusammen streifft,", "tokens": ["In\u00b7dem", "es", "sich", "so", "dann", "sehr", "schnell", "zu\u00b7sam\u00b7men", "streifft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "ADV", "ADV", "ADJD", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Recht als mit einem Schu\u00df gewaltig von sich streuet,", "tokens": ["Recht", "als", "mit", "ei\u00b7nem", "Schu\u00df", "ge\u00b7wal\u00b7tig", "von", "sich", "streu\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "APPR", "ART", "NN", "ADJD", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Da\u00df jedermann", "tokens": ["Da\u00df", "je\u00b7der\u00b7mann"], "token_info": ["word", "word"], "pos": ["KOUS", "PIS"], "meter": "-+-+", "measure": "iambic.di"}, "line.30": {"text": "Sich nicht genug darob verwundern kann,", "tokens": ["Sich", "nicht", "ge\u00b7nug", "da\u00b7rob", "ver\u00b7wun\u00b7dern", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PTKNEG", "ADV", "PAV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "Und, wenn ers recht erwegt, mit Recht sich dr\u00fcber freuet:", "tokens": ["Und", ",", "wenn", "ers", "recht", "er\u00b7wegt", ",", "mit", "Recht", "sich", "dr\u00fc\u00b7ber", "freu\u00b7et", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PIS", "ADV", "VVPP", "$,", "APPR", "NN", "PRF", "PAV", "VVFIN", "$."], "meter": "-+---+-+-+-+-", "measure": "dactylic.init"}}}}}