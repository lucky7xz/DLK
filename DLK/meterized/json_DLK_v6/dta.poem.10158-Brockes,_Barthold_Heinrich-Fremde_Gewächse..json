{"dta.poem.10158": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Fremde Gew\u00e4chse.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Will iemand, in der Bluhm- und in der Pflantzen\nHeere,", "tokens": ["Will", "ie\u00b7mand", ",", "in", "der", "Bluhm", "und", "in", "der", "Pflant\u00b7zen", "Hee\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "$,", "APPR", "ART", "TRUNC", "KON", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des Sch\u00f6pfers Finger deutlich sehn;", "tokens": ["Des", "Sch\u00f6p\u00b7fers", "Fin\u00b7ger", "deut\u00b7lich", "sehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der mu\u00df in deinen Garten gehn,", "tokens": ["Der", "mu\u00df", "in", "dei\u00b7nen", "Gar\u00b7ten", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von Deiner Vater-Stadt; der Du aus Asia,", "tokens": ["Von", "Dei\u00b7ner", "Va\u00b7ter\u00b7Stadt", ";", "der", "Du", "aus", "A\u00b7sia", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$.", "PRELS", "PPER", "APPR", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Aus Africa, wie auch America", "tokens": ["Aus", "A\u00b7fri\u00b7ca", ",", "wie", "auch", "A\u00b7me\u00b7ri\u00b7ca"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NE", "$,", "PWAV", "ADV", "NE"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Viel tausend seltene Gew\u00e4chse zeigst und n\u00e4hrest;", "tokens": ["Viel", "tau\u00b7send", "sel\u00b7te\u00b7ne", "Ge\u00b7w\u00e4ch\u00b7se", "zeigst", "und", "n\u00e4h\u00b7rest", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJA", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wodurch Du Dein- und ihren Sch\u00f6pfer ehrest.", "tokens": ["Wo\u00b7durch", "Du", "Dein", "und", "ih\u00b7ren", "Sch\u00f6p\u00b7fer", "eh\u00b7rest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "TRUNC", "KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wie ich so grosse Meng\u2019 erblickte,", "tokens": ["Wie", "ich", "so", "gros\u00b7se", "Meng'", "er\u00b7blick\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit welcher bunten Zierlichkeit,", "tokens": ["Mit", "wel\u00b7cher", "bun\u00b7ten", "Zier\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit welcher Farben Glantz und Schein,", "tokens": ["Mit", "wel\u00b7cher", "Far\u00b7ben", "Glantz", "und", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit welcher Bildung Unterscheid", "tokens": ["Mit", "wel\u00b7cher", "Bil\u00b7dung", "Un\u00b7ter\u00b7scheid"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PWAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie die Natur so unnachahmbar schm\u00fcckte;", "tokens": ["Sie", "die", "Na\u00b7tur", "so", "un\u00b7nac\u00b7hahm\u00b7bar", "schm\u00fcck\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Nahm mich f\u00fcrwahr ein heiligs Schaudern ein.", "tokens": ["Nahm", "mich", "f\u00fcr\u00b7wahr", "ein", "hei\u00b7ligs", "Schau\u00b7dern", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die Seele ward auf eine neue Weise,", "tokens": ["Die", "See\u00b7le", "ward", "auf", "ei\u00b7ne", "neu\u00b7e", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dem Sch\u00f6pfer der Natur zum Preise,", "tokens": ["Dem", "Sch\u00f6p\u00b7fer", "der", "Na\u00b7tur", "zum", "Prei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Indem sie auf einmahl so viele Wunder sp\u00fchret,", "tokens": ["In\u00b7dem", "sie", "auf", "ein\u00b7mahl", "so", "vie\u00b7le", "Wun\u00b7der", "sp\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADV", "ADV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie sie bisher noch nie ger\u00fchret war, ger\u00fchret.", "tokens": ["Wie", "sie", "bis\u00b7her", "noch", "nie", "ge\u00b7r\u00fch\u00b7ret", "war", ",", "ge\u00b7r\u00fch\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADV", "ADV", "VVPP", "VAFIN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mein gantzes Wesen ward beweget:", "tokens": ["Mein", "gant\u00b7zes", "We\u00b7sen", "ward", "be\u00b7we\u00b7get", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Es drang sich durch mein Aug, in tausendfachem Schein,", "tokens": ["Es", "drang", "sich", "durch", "mein", "Aug", ",", "in", "tau\u00b7send\u00b7fa\u00b7chem", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Solch eine Menge Zierlichkeiten,", "tokens": ["Solch", "ei\u00b7ne", "Men\u00b7ge", "Zier\u00b7lich\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Solch eine grosse Zahl Vollkommenheiten", "tokens": ["Solch", "ei\u00b7ne", "gros\u00b7se", "Zahl", "Voll\u00b7kom\u00b7men\u00b7hei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Auf einmahl in die Seel\u2019, als wie ein Strahl, hinein,", "tokens": ["Auf", "ein\u00b7mahl", "in", "die", "Seel'", ",", "als", "wie", "ein", "Strahl", ",", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADV", "APPR", "ART", "NN", "$,", "KOUS", "KOKOM", "ART", "NN", "$,", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Befeurete mein Blut, da\u00df es dadurch erreget,", "tokens": ["Be\u00b7fe\u00b7u\u00b7re\u00b7te", "mein", "Blut", ",", "da\u00df", "es", "da\u00b7durch", "er\u00b7re\u00b7get", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "KOUS", "PPER", "PAV", "VVFIN", "$,"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "In seinem Circkel-Lauff, f\u00fcr Anmuth, schneller flo\u00df,", "tokens": ["In", "sei\u00b7nem", "Cir\u00b7ckel\u00b7Lauff", ",", "f\u00fcr", "An\u00b7muth", ",", "schnel\u00b7ler", "flo\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "APPR", "NN", "$,", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und, mit erneuter Krafft, durch seine G\u00e4nge scho\u00df.", "tokens": ["Und", ",", "mit", "er\u00b7neu\u00b7ter", "Krafft", ",", "durch", "sei\u00b7ne", "G\u00e4n\u00b7ge", "scho\u00df", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "ADJA", "NN", "$,", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Der Nerven-Safft fing alle Sehnen,", "tokens": ["Der", "Ner\u00b7ven\u00b7\u00b7S\u00b7afft", "fing", "al\u00b7le", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Mit einem sanften Druck, vergn\u00fcglich auszudehnen", "tokens": ["Mit", "ei\u00b7nem", "sanf\u00b7ten", "Druck", ",", "ver\u00b7gn\u00fcg\u00b7lich", "aus\u00b7zu\u00b7deh\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und zu bewegen an. Es f\u00fcllte meine Brust,", "tokens": ["Und", "zu", "be\u00b7we\u00b7gen", "an", ".", "Es", "f\u00fcll\u00b7te", "mei\u00b7ne", "Brust", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKZU", "VVINF", "PTKVZ", "$.", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "So wol als mein Gehirn, gantz eine neue Lust.", "tokens": ["So", "wol", "als", "mein", "Ge\u00b7hirn", ",", "gantz", "ei\u00b7ne", "neu\u00b7e", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KOKOM", "PPOSAT", "NN", "$,", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Die Seel\u2019, entflammet durch die\u00df Sehnen,", "tokens": ["Die", "Seel'", ",", "ent\u00b7flam\u00b7met", "durch", "die\u00df", "Seh\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "APPR", "PDS", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Entz\u00fcndet durch den Strahl des ewgen Weisheit-Lichts,", "tokens": ["Ent\u00b7z\u00fcn\u00b7det", "durch", "den", "Strahl", "des", "ew\u00b7gen", "Weis\u00b7heit\u00b7Lichts", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Erzeuget\u2019 anders nichts,", "tokens": ["Er\u00b7zeu\u00b7get'", "an\u00b7ders", "nichts", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PIS", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Als fast verhimmelnde Gedancken und Jdeen.", "tokens": ["Als", "fast", "ver\u00b7him\u00b7meln\u00b7de", "Ge\u00b7dan\u00b7cken", "und", "Jdeen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+---+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Denn ieder Vorwurff zeigt\u2019 ihr auf so manche Art,", "tokens": ["Denn", "ie\u00b7der", "Vor\u00b7wurff", "zeigt'", "ihr", "auf", "so", "man\u00b7che", "Art", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PPER", "APPR", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "In seiner Wercke Pracht, des Sch\u00f6pfers Gegenwart.", "tokens": ["In", "sei\u00b7ner", "Wer\u00b7cke", "Pracht", ",", "des", "Sch\u00f6p\u00b7fers", "Ge\u00b7gen\u00b7wart", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Daher sie Jhm ihr Andachts-Opfer brachte", "tokens": ["Da\u00b7her", "sie", "Jhm", "ihr", "An\u00b7dachts\u00b7Op\u00b7fer", "brach\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "PPER", "PPER", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und voller Lust und Ehrfurcht dieses dachte:", "tokens": ["Und", "vol\u00b7ler", "Lust", "und", "Ehr\u00b7furcht", "die\u00b7ses", "dach\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "KON", "NN", "PDS", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "\u201eallm\u00e4chtiger, und Wunder-reicher GOTT,", "tokens": ["\u201e", "all\u00b7m\u00e4ch\u00b7ti\u00b7ger", ",", "und", "Wun\u00b7der\u00b7rei\u00b7cher", "GoTT", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$,", "KON", "NN", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}