{"textgrid.poem.43824": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Still stand die Wiege, das Kind schlief fest,", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Still stand die Wiege, das Kind schlief fest,", "tokens": ["Still", "stand", "die", "Wie\u00b7ge", ",", "das", "Kind", "schlief", "fest", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$,", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ein M\u00e4uselein sich blicken l\u00e4\u00dft,", "tokens": ["Ein", "M\u00e4u\u00b7sel\u00b7ein", "sich", "bli\u00b7cken", "l\u00e4\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wippelndes, trippelndes Ding!", "tokens": ["Wip\u00b7peln\u00b7des", ",", "trip\u00b7peln\u00b7des", "Ding", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "Es dreht das K\u00f6pfchen hin und her,", "tokens": ["Es", "dreht", "das", "K\u00f6pf\u00b7chen", "hin", "und", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es h\u00fcpft und t\u00e4nzelt kreuz und quer,", "tokens": ["Es", "h\u00fcpft", "und", "t\u00e4n\u00b7zelt", "kreuz", "und", "quer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "NN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Hin und her, kreuz und quer.", "tokens": ["Hin", "und", "her", ",", "kreuz", "und", "quer", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "PTKVZ", "$,", "NN", "KON", "ADJD", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Mach' kein Ger\u00e4usch, o M\u00e4uselein!", "tokens": ["Mach'", "kein", "Ge\u00b7r\u00e4usch", ",", "o", "M\u00e4u\u00b7sel\u00b7ein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$,", "FM", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sonst f\u00e4ngt mein Kind gleich an zu schrei'n.", "tokens": ["Sonst", "f\u00e4ngt", "mein", "Kind", "gleich", "an", "zu", "schrei'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ADV", "PTKVZ", "PTKZU", "VVFIN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Das M\u00e4uselein nimmt sich wohl in Acht,", "tokens": ["Das", "M\u00e4u\u00b7sel\u00b7ein", "nimmt", "sich", "wohl", "in", "Acht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADV", "APPR", "CARD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Es schleicht zur Wiege sacht, ganz sacht,", "tokens": ["Es", "schleicht", "zur", "Wie\u00b7ge", "sacht", ",", "ganz", "sacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "VVFIN", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wippelndes, trippelndes Ding!", "tokens": ["Wip\u00b7peln\u00b7des", ",", "trip\u00b7peln\u00b7des", "Ding", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "Es schaut und schnuppert rings umher,", "tokens": ["Es", "schaut", "und", "schnup\u00b7pert", "rings", "um\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wo wohl ein Kr\u00fcmchen Kuchen w\u00e4r',", "tokens": ["Wo", "wohl", "ein", "Kr\u00fcm\u00b7chen", "Ku\u00b7chen", "w\u00e4r'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Kuchen w\u00e4r', Kuchen w\u00e4r',", "tokens": ["Ku\u00b7chen", "w\u00e4r'", ",", "Ku\u00b7chen", "w\u00e4r'", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "NN", "VAFIN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Mach' kein Ger\u00e4usch, o M\u00e4uselein!", "tokens": ["Mach'", "kein", "Ge\u00b7r\u00e4usch", ",", "o", "M\u00e4u\u00b7sel\u00b7ein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$,", "FM", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sonst f\u00e4ngt mein Kind gleich an zu schrei'n.", "tokens": ["Sonst", "f\u00e4ngt", "mein", "Kind", "gleich", "an", "zu", "schrei'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ADV", "PTKVZ", "PTKZU", "VVFIN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und alle Kr\u00fcmchen, die da sind,", "tokens": ["Und", "al\u00b7le", "Kr\u00fcm\u00b7chen", ",", "die", "da", "sind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "PRELS", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die g\u00f6nnt dir alle gern mein Kind,", "tokens": ["Die", "g\u00f6nnt", "dir", "al\u00b7le", "gern", "mein", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PIS", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wippelndes, trippelndes Ding!", "tokens": ["Wip\u00b7peln\u00b7des", ",", "trip\u00b7peln\u00b7des", "Ding", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "Wie hat's geschmeckt dem M\u00e4uslein doch!", "tokens": ["Wie", "hat's", "ge\u00b7schmeckt", "dem", "M\u00e4us\u00b7lein", "doch", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "VVPP", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "'s Ist eins zwei drei in seinem Loch,", "tokens": ["'s", "Ist", "eins", "zwei", "drei", "in", "sei\u00b7nem", "Loch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "CARD", "CARD", "CARD", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Seinem Loch, seinem Loch.", "tokens": ["Sei\u00b7nem", "Loch", ",", "sei\u00b7nem", "Loch", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "M\u00e4uselein, hast es gut gemacht:", "tokens": ["M\u00e4u\u00b7sel\u00b7ein", ",", "hast", "es", "gut", "ge\u00b7macht", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mein Kindlein schl\u00e4ft, nun gute Nacht.", "tokens": ["Mein", "Kin\u00b7dlein", "schl\u00e4ft", ",", "nun", "gu\u00b7te", "Nacht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}