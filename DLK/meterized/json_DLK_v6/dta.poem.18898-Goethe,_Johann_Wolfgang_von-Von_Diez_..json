{"dta.poem.18898": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "Von Diez .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Wie man mit Vorsicht auf der Erde wandelt,", "tokens": ["Wie", "man", "mit", "Vor\u00b7sicht", "auf", "der", "Er\u00b7de", "wan\u00b7delt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "APPR", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es sey bergauf, es sey hinab vom Thron,", "tokens": ["Es", "sey", "ber\u00b7gauf", ",", "es", "sey", "hin\u00b7ab", "vom", "Thron", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKVZ", "$,", "PPER", "VAFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und wie man Menschen, wie man Pferde handelt", "tokens": ["Und", "wie", "man", "Men\u00b7schen", ",", "wie", "man", "Pfer\u00b7de", "han\u00b7delt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PIS", "NN", "$,", "PWAV", "PIS", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das alles lehrt der K\u00f6nig seinen Sohn.", "tokens": ["Das", "al\u00b7les", "lehrt", "der", "K\u00f6\u00b7nig", "sei\u00b7nen", "Sohn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wir wissens nun, durch Dich der uns beschenkte;", "tokens": ["Wir", "wis\u00b7sens", "nun", ",", "durch", "Dich", "der", "uns", "be\u00b7schenk\u00b7te", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "APPR", "PPER", "ART", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Jetzt f\u00fcgest du der Tulpe Flor daran,", "tokens": ["Jetzt", "f\u00fc\u00b7gest", "du", "der", "Tul\u00b7pe", "Flor", "da\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "NN", "PAV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und wenn mich nicht der goldne Rahm beschr\u00e4nkte", "tokens": ["Und", "wenn", "mich", "nicht", "der", "gold\u00b7ne", "Rahm", "be\u00b7schr\u00e4nk\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "PTKNEG", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wo endete was Du f\u00fcr uns gethan.", "tokens": ["Wo", "en\u00b7de\u00b7te", "was", "Du", "f\u00fcr", "uns", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PWS", "PPER", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}