{"textgrid.poem.41648": {"metadata": {"author": {"name": "Baudelaire, Charles", "birth": "N.A.", "death": "N.A."}, "title": "1L: Bei dem Himmel bleich und zerrissen,", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bei dem Himmel bleich und zerrissen,", "tokens": ["Bei", "dem", "Him\u00b7mel", "bleich", "und", "zer\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "KON", "VVPP", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Den das Unheil foltert wie dich,", "tokens": ["Den", "das", "Un\u00b7heil", "fol\u00b7tert", "wie", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "KOKOM", "PPER", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Was f\u00fchlst in den Finsternissen", "tokens": ["Was", "f\u00fchlst", "in", "den", "Fins\u00b7ter\u00b7nis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "APPR", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Deiner Brust du? L\u00fcstling, sprich!", "tokens": ["Dei\u00b7ner", "Brust", "du", "?", "L\u00fcst\u00b7ling", ",", "sprich", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "$.", "NE", "$,", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Voller Gier nach dem Ungewissen,", "tokens": ["Vol\u00b7ler", "Gier", "nach", "dem", "Un\u00b7ge\u00b7wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Werde niemals mit Tr\u00e4nen ich", "tokens": ["Wer\u00b7de", "nie\u00b7mals", "mit", "Tr\u00e4\u00b7nen", "ich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "APPR", "NN", "PPER"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Die Paradiese vermissen,", "tokens": ["Die", "Pa\u00b7ra\u00b7die\u00b7se", "ver\u00b7mis\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wie Ovid, als von Rom er wich.", "tokens": ["Wie", "O\u00b7vid", ",", "als", "von", "Rom", "er", "wich", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "$,", "KOUS", "APPR", "NE", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ihr verw\u00fcsteten Himmelsr\u00e4ume,", "tokens": ["Ihr", "ver\u00b7w\u00fcs\u00b7te\u00b7ten", "Him\u00b7mels\u00b7r\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Zu euch schaut mein Stolz empor!", "tokens": ["Zu", "euch", "schaut", "mein", "Stolz", "em\u00b7por", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Eurer Wolken trauernder Flor", "tokens": ["Eu\u00b7rer", "Wol\u00b7ken", "trau\u00b7ern\u00b7der", "Flor"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJA", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.4": {"line.1": {"text": "Gleicht dem Leichenzug meiner Tr\u00e4ume,", "tokens": ["Gleicht", "dem", "Lei\u00b7chen\u00b7zug", "mei\u00b7ner", "Tr\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Und der H\u00f6lle entstammt euer Schein,", "tokens": ["Und", "der", "H\u00f6l\u00b7le", "ent\u00b7stammt", "eu\u00b7er", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Wo mein Herz sich wohl f\u00fchlt allein!", "tokens": ["Wo", "mein", "Herz", "sich", "wohl", "f\u00fchlt", "al\u00b7lein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "PRF", "ADV", "VVFIN", "ADV", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}}}}