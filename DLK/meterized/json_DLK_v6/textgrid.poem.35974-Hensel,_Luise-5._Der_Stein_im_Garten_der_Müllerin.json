{"textgrid.poem.35974": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "5. Der Stein im Garten der M\u00fcllerin", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Guten Morgen, ihr Leute! ich gr\u00fc\u00df' euch sch\u00f6n,", "tokens": ["Gu\u00b7ten", "Mor\u00b7gen", ",", "ihr", "Leu\u00b7te", "!", "ich", "gr\u00fc\u00df'", "euch", "sch\u00f6n", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Ihr habt wohl lange keinen Stein gesehen?", "tokens": ["Ihr", "habt", "wohl", "lan\u00b7ge", "kei\u00b7nen", "Stein", "ge\u00b7se\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie machen gro\u00dfe Augen und wundern sich sehr,", "tokens": ["Sie", "ma\u00b7chen", "gro\u00b7\u00dfe", "Au\u00b7gen", "und", "wun\u00b7dern", "sich", "sehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "KON", "VVFIN", "PRF", "ADV", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Als wollten sie fragen, was will denn der?", "tokens": ["Als", "woll\u00b7ten", "sie", "fra\u00b7gen", ",", "was", "will", "denn", "der", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "PPER", "VVINF", "$,", "PWS", "VMFIN", "ADV", "ART", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "In Blumen will ich liegen, die M\u00fcllerin will ich sehn.", "tokens": ["In", "Blu\u00b7men", "will", "ich", "lie\u00b7gen", ",", "die", "M\u00fcl\u00b7le\u00b7rin", "will", "ich", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PPER", "VVFIN", "$,", "ART", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Wenn ihr euch genug verwundert, k\u00f6nnt' ihr nach Hause gehn.", "tokens": ["Wenn", "ihr", "euch", "ge\u00b7nug", "ver\u00b7wun\u00b7dert", ",", "k\u00f6nnt'", "ihr", "nach", "Hau\u00b7se", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "VVFIN", "$,", "VMFIN", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "--+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}}}}