{"textgrid.poem.33083": {"metadata": {"author": {"name": "B\u00fcchner, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Im Griechenlande, bei den gro\u00dfen Alten,", "genre": "verse", "period": "N.A.", "pub_year": 1849, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Griechenlande, bei den gro\u00dfen Alten,", "tokens": ["Im", "Grie\u00b7chen\u00b7lan\u00b7de", ",", "bei", "den", "gro\u00b7\u00dfen", "Al\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den geistig freien, pries man als begl\u00fcckt", "tokens": ["Den", "geis\u00b7tig", "frei\u00b7en", ",", "pries", "man", "als", "be\u00b7gl\u00fcckt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADJA", "$,", "VVFIN", "PIS", "KOKOM", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Den Mann, dem von des Schicksals ernstem Walten", "tokens": ["Den", "Mann", ",", "dem", "von", "des", "Schick\u00b7sals", "erns\u00b7tem", "Wal\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein Leid voll Segen ward auf's Haupt gedr\u00fcckt.", "tokens": ["Ein", "Leid", "voll", "Se\u00b7gen", "ward", "auf's", "Haupt", "ge\u00b7dr\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "VAFIN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Nicht war dies kranke Lust an Schmerz und Wunden \u2013", "tokens": ["Nicht", "war", "dies", "kran\u00b7ke", "Lust", "an", "Schmerz", "und", "Wun\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VAFIN", "PDS", "ADJA", "NN", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wo bl\u00fchte sch\u00f6ner heitrer Sinn und Geist?", "tokens": ["Wo", "bl\u00fch\u00b7te", "sch\u00f6\u00b7ner", "hei\u00b7trer", "Sinn", "und", "Geist", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADJA", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Nein, Weisheit war es, welche tief empfunden,", "tokens": ["Nein", ",", "Weis\u00b7heit", "war", "es", ",", "wel\u00b7che", "tief", "emp\u00b7fun\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NN", "VAFIN", "PPER", "$,", "PRELS", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wie ernst, bedeutsam, was da Leben hei\u00dft.", "tokens": ["Wie", "ernst", ",", "be\u00b7deut\u00b7sam", ",", "was", "da", "Le\u00b7ben", "hei\u00dft", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "ADJD", "$,", "PRELS", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nicht feig erliegen, selbstbewu\u00dft es tragen,", "tokens": ["Nicht", "feig", "er\u00b7lie\u00b7gen", ",", "selbst\u00b7be\u00b7wu\u00dft", "es", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VVINF", "$,", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie eine Freude nach der andern weicht,", "tokens": ["Wie", "ei\u00b7ne", "Freu\u00b7de", "nach", "der", "an\u00b7dern", "weicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Kann er's, der nie ge\u00fcbt sich im Entsagen,", "tokens": ["Kann", "er's", ",", "der", "nie", "ge\u00b7\u00fcbt", "sich", "im", "Ent\u00b7sa\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "$,", "PRELS", "ADV", "VVPP", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dem Bl\u00fcthen nur das Gl\u00fcck stets dargereicht?", "tokens": ["Dem", "Bl\u00fc\u00b7then", "nur", "das", "Gl\u00fcck", "stets", "dar\u00b7ge\u00b7reicht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ein hoher Segen aber ruht auf Schmerzen,", "tokens": ["Ein", "ho\u00b7her", "Se\u00b7gen", "a\u00b7ber", "ruht", "auf", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und, wie die Perl' im dunklen Meeresschacht", "tokens": ["Und", ",", "wie", "die", "Perl'", "im", "dunk\u00b7len", "Mee\u00b7res\u00b7schacht"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "PWAV", "ART", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Sich formt und bildet, w\u00e4chst im Menschenherzen", "tokens": ["Sich", "formt", "und", "bil\u00b7det", ",", "w\u00e4chst", "im", "Men\u00b7schen\u00b7her\u00b7zen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PRF", "VVPP", "KON", "VVFIN", "$,", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ein edler Schatz aus finstrer Leidensnacht.", "tokens": ["Ein", "ed\u00b7ler", "Schatz", "aus", "finst\u00b7rer", "Lei\u00b7dens\u00b7nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Der Seele Ruhe, die sich still begn\u00fcgend", "tokens": ["Der", "See\u00b7le", "Ru\u00b7he", ",", "die", "sich", "still", "be\u00b7gn\u00fc\u00b7gend"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "PRELS", "PRF", "ADJD", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nicht mehr, als ihr beschiednes Theil begehrt,", "tokens": ["Nicht", "mehr", ",", "als", "ihr", "be\u00b7schied\u00b7nes", "Theil", "be\u00b7gehrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "$,", "KOUS", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der freie Geist, der nie sich selbst bel\u00fcgend,", "tokens": ["Der", "frei\u00b7e", "Geist", ",", "der", "nie", "sich", "selbst", "be\u00b7l\u00fc\u00b7gend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADV", "PRF", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein jedes Ding ermi\u00dft nach \u00e4chtem Werth,", "tokens": ["Ein", "je\u00b7des", "Ding", "er\u00b7mi\u00dft", "nach", "\u00e4ch\u00b7tem", "Werth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und auch ein Herz voll Demuth und voll Liebe", "tokens": ["Und", "auch", "ein", "Herz", "voll", "De\u00b7muth", "und", "voll", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "ADJD", "NN", "KON", "ADJD", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Und voll Geduld f\u00fcr sie, die schw\u00e4cher sind,", "tokens": ["Und", "voll", "Ge\u00b7duld", "f\u00fcr", "sie", ",", "die", "schw\u00e4\u00b7cher", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "NN", "APPR", "PPER", "$,", "PRELS", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "O, Perle reinster Menschlichkeit, wer bliebe", "tokens": ["O", ",", "Per\u00b7le", "reins\u00b7ter", "Menschlich\u00b7keit", ",", "wer", "blie\u00b7be"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "$,", "NN", "ADJA", "NN", "$,", "PWS", "VVFIN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Gern frei von Leid, wenn so man dich gewinnt?", "tokens": ["Gern", "frei", "von", "Leid", ",", "wenn", "so", "man", "dich", "ge\u00b7winnt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "NN", "$,", "KOUS", "ADV", "PIS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "O, Allen diesen Trost, die schwere Stunden", "tokens": ["O", ",", "Al\u00b7len", "die\u00b7sen", "Trost", ",", "die", "schwe\u00b7re", "Stun\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "NE", "PDAT", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zu den Gesellen ", "tokens": ["Zu", "den", "Ge\u00b7sel\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Wie manches Herz hat sich zur\u00fcckgefunden", "tokens": ["Wie", "man\u00b7ches", "Herz", "hat", "sich", "zu\u00b7r\u00fcck\u00b7ge\u00b7fun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIAT", "NN", "VAFIN", "PRF", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Aus lautem Tag in stiller Leidensnacht.", "tokens": ["Aus", "lau\u00b7tem", "Tag", "in", "stil\u00b7ler", "Lei\u00b7dens\u00b7nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Von allen Kronen, die die Erde schm\u00fccken", "tokens": ["Von", "al\u00b7len", "Kro\u00b7nen", ",", "die", "die", "Er\u00b7de", "schm\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "$,", "PRELS", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ist eine einz'ge nur von \u00e4chtem Werth,", "tokens": ["Ist", "ei\u00b7ne", "einz'\u00b7ge", "nur", "von", "\u00e4ch\u00b7tem", "Werth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Lass' sie geduldig auf das Haupt dir dr\u00fccken,", "tokens": ["Lass'", "sie", "ge\u00b7dul\u00b7dig", "auf", "das", "Haupt", "dir", "dr\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "APPR", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die Dornenkrone, die im Schmerz verkl\u00e4rt!", "tokens": ["Die", "Dor\u00b7nen\u00b7kro\u00b7ne", ",", "die", "im", "Schmerz", "ver\u00b7kl\u00e4rt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}