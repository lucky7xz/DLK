{"textgrid.poem.45699": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Unsre \u00c4rzte", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seit ihr so eifrig im Studieren,", "tokens": ["Seit", "ihr", "so", "eif\u00b7rig", "im", "Stu\u00b7die\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Mu\u00df meine Hoffnung auf Genesung scheitern,", "tokens": ["Mu\u00df", "mei\u00b7ne", "Hoff\u00b7nung", "auf", "Ge\u00b7ne\u00b7sung", "schei\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ihr wollt nicht eure Kranken kurieren,", "tokens": ["Ihr", "wollt", "nicht", "eu\u00b7re", "Kran\u00b7ken", "ku\u00b7rie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sondern nur eure Wissenschaft erweitern.", "tokens": ["Son\u00b7dern", "nur", "eu\u00b7re", "Wis\u00b7sen\u00b7schaft", "er\u00b7wei\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}}}}