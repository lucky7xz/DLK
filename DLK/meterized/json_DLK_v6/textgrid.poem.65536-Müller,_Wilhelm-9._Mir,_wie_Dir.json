{"textgrid.poem.65536": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "9. Mir, wie Dir", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer sich nicht selbst verspotten kann,", "tokens": ["Wer", "sich", "nicht", "selbst", "ver\u00b7spot\u00b7ten", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "PTKNEG", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der fang' es nicht mit Andern an.", "tokens": ["Der", "fang'", "es", "nicht", "mit", "An\u00b7dern", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PTKNEG", "APPR", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Narr, hudle nur den eignen Herrn,", "tokens": ["Narr", ",", "hud\u00b7le", "nur", "den", "eig\u00b7nen", "Herrn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So haben dich die G\u00e4ste gern.", "tokens": ["So", "ha\u00b7ben", "dich", "die", "G\u00e4s\u00b7te", "gern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}