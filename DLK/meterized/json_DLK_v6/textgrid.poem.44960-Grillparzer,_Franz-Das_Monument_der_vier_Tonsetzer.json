{"textgrid.poem.44960": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Das Monument der vier Tonsetzer", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sparsam zu sein ist unsre Liebe,", "tokens": ["Spar\u00b7sam", "zu", "sein", "ist", "uns\u00b7re", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKZU", "VAINF", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "In Zwecken gro\u00df, in Mitteln klein,", "tokens": ["In", "Zwe\u00b7cken", "gro\u00df", ",", "in", "Mit\u00b7teln", "klein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "$,", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie einen Galgen auf vier Diebe,", "tokens": ["Wie", "ei\u00b7nen", "Gal\u00b7gen", "auf", "vier", "Die\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vier Meistern einen Leichenstein.", "tokens": ["Vier", "Meis\u00b7tern", "ei\u00b7nen", "Lei\u00b7chen\u00b7stein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}