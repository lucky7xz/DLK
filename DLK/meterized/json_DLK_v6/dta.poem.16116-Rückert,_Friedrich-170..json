{"dta.poem.16116": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "170.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Lieb' ist vielerlei: es liebt das Allgemeine", "tokens": ["Die", "Lieb'", "ist", "vie\u00b7ler\u00b7lei", ":", "es", "liebt", "das", "All\u00b7ge\u00b7mei\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "$.", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sich selber, Gott mit sich im ew'gen Lustvereine.", "tokens": ["Sich", "sel\u00b7ber", ",", "Gott", "mit", "sich", "im", "ew'\u00b7gen", "Lust\u00b7ver\u00b7ei\u00b7ne", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "$,", "NN", "APPR", "PRF", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Das Allgemeine dann liebt das Besondre auch,", "tokens": ["Das", "All\u00b7ge\u00b7mei\u00b7ne", "dann", "liebt", "das", "Be\u00b7sond\u00b7re", "auch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die ganze Welt durchdringt von Gott ein Liebeshauch.", "tokens": ["Die", "gan\u00b7ze", "Welt", "durch\u00b7dringt", "von", "Gott", "ein", "Lie\u00b7bes\u00b7hauch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und das Besondre liebt das Allgemeine dann,", "tokens": ["Und", "das", "Be\u00b7sond\u00b7re", "liebt", "das", "All\u00b7ge\u00b7mei\u00b7ne", "dann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "ADJA", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das ist soviel ein Mensch, o Gott, dich lieben kann.", "tokens": ["Das", "ist", "so\u00b7viel", "ein", "Mensch", ",", "o", "Gott", ",", "dich", "lie\u00b7ben", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ART", "NN", "$,", "FM", "NN", "$,", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Nur das Besondre kann ganz das Besondre lieben,", "tokens": ["Nur", "das", "Be\u00b7sond\u00b7re", "kann", "ganz", "das", "Be\u00b7sond\u00b7re", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VMFIN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Liebe zu dir selbst hat mich zur Welt getrieben.", "tokens": ["Die", "Lie\u00b7be", "zu", "dir", "selbst", "hat", "mich", "zur", "Welt", "ge\u00b7trie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "ADV", "VAFIN", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Ich bin ein Blumenstaub und will auf Blumen stieben.", "tokens": ["Ich", "bin", "ein", "Blu\u00b7men\u00b7staub", "und", "will", "auf", "Blu\u00b7men", "stie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "KON", "VMFIN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}