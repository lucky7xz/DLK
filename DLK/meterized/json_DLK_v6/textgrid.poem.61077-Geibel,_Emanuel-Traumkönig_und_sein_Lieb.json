{"textgrid.poem.61077": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Traumk\u00f6nig und sein Lieb", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "S\u00fc\u00df schlummert das M\u00e4dchen im K\u00e4mmerlein,", "tokens": ["S\u00fc\u00df", "schlum\u00b7mert", "das", "M\u00e4d\u00b7chen", "im", "K\u00e4m\u00b7mer\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Gebettet auf reinlichem Pf\u00fchle;", "tokens": ["Ge\u00b7bet\u00b7tet", "auf", "rein\u00b7li\u00b7chem", "Pf\u00fch\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Die Sommernacht haucht w\u00fcrzig herein", "tokens": ["Die", "Som\u00b7mer\u00b7nacht", "haucht", "w\u00fcr\u00b7zig", "her\u00b7ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PTKVZ"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Mit ihrer erquickenden K\u00fchle.", "tokens": ["Mit", "ih\u00b7rer", "er\u00b7qui\u00b7cken\u00b7den", "K\u00fch\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Am Fenster bl\u00fchn die Rosen zumal,", "tokens": ["Am", "Fens\u00b7ter", "bl\u00fchn", "die", "Ro\u00b7sen", "zu\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Es duften so s\u00fc\u00df die Linden,", "tokens": ["Es", "duf\u00b7ten", "so", "s\u00fc\u00df", "die", "Lin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "ART", "NE", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Kaum mag des Mondes goldner Strahl", "tokens": ["Kaum", "mag", "des", "Mon\u00b7des", "gold\u00b7ner", "Strahl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durchs Laub den Eingang finden.", "tokens": ["Durchs", "Laub", "den", "Ein\u00b7gang", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Doch pl\u00f6tzlich st\u00e4rker wird der Duft,", "tokens": ["Doch", "pl\u00f6tz\u00b7lich", "st\u00e4r\u00b7ker", "wird", "der", "Duft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gl\u00fchw\u00fcrmchen weben und flimmen,", "tokens": ["Gl\u00fch\u00b7w\u00fcrm\u00b7chen", "we\u00b7ben", "und", "flim\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "KON", "VVINF", "$,"], "meter": "++-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Es rauschen die Bl\u00e4tter, es klingt die Luft", "tokens": ["Es", "rau\u00b7schen", "die", "Bl\u00e4t\u00b7ter", ",", "es", "klingt", "die", "Luft"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Von leisen melodischen Stimmen:", "tokens": ["Von", "lei\u00b7sen", "me\u00b7lo\u00b7di\u00b7schen", "Stim\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "\u00bbs\u00fc\u00df Lieb, s\u00fc\u00df Lieb, und wiege dich fein", "tokens": ["\u00bb", "s\u00fc\u00df", "Lieb", ",", "s\u00fc\u00df", "Lieb", ",", "und", "wie\u00b7ge", "dich", "fein"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "ADJD", "NN", "$,", "ADJD", "NN", "$,", "KON", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Auf stillen Schlummerwogen!", "tokens": ["Auf", "stil\u00b7len", "Schlum\u00b7mer\u00b7wo\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Traumk\u00f6nig will dein Liebster sein,", "tokens": ["Traum\u00b7k\u00f6\u00b7nig", "will", "dein", "Liebs\u00b7ter", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPOSAT", "NN", "VAINF", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Traumk\u00f6nig kommt gezogen.\u00ab", "tokens": ["Traum\u00b7k\u00f6\u00b7nig", "kommt", "ge\u00b7zo\u00b7gen", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["NN", "VVFIN", "VVPP", "$.", "$("], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.5": {"line.1": {"text": "Da steht der Elf zu H\u00e4upten ihr;", "tokens": ["Da", "steht", "der", "Elf", "zu", "H\u00e4up\u00b7ten", "ihr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NN", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er sch\u00fcttelt die Locken, die dunkeln,", "tokens": ["Er", "sch\u00fct\u00b7telt", "die", "Lo\u00b7cken", ",", "die", "dun\u00b7keln", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Da\u00df hell an seiner Krone Zier", "tokens": ["Da\u00df", "hell", "an", "sei\u00b7ner", "Kro\u00b7ne", "Zier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Edelsteine funkeln.", "tokens": ["Die", "E\u00b7del\u00b7stei\u00b7ne", "fun\u00b7keln", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Dann beugt er sich sanft auf die Holde herab,", "tokens": ["Dann", "beugt", "er", "sich", "sanft", "auf", "die", "Hol\u00b7de", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADJD", "APPR", "ART", "NN", "ADV", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "K\u00fc\u00dft Stirn und Lippen ihr leise", "tokens": ["K\u00fc\u00dft", "Stirn", "und", "Lip\u00b7pen", "ihr", "lei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "NN", "KON", "NN", "PPOSAT", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und zieht mit goldenem Zauberstab", "tokens": ["Und", "zieht", "mit", "gol\u00b7de\u00b7nem", "Zau\u00b7ber\u00b7stab"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Umher viele luftige Kreise.", "tokens": ["Um\u00b7her", "vie\u00b7le", "luf\u00b7ti\u00b7ge", "Krei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.7": {"line.1": {"text": "Und wie er sie weiter und weiter schlingt,", "tokens": ["Und", "wie", "er", "sie", "wei\u00b7ter", "und", "wei\u00b7ter", "schlingt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "PPER", "ADV", "KON", "ADV", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Da wird zum Palaste das St\u00fcbchen,", "tokens": ["Da", "wird", "zum", "Pa\u00b7las\u00b7te", "das", "St\u00fcb\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Drin ruhn, von f\u00fcrstlichem Glanz umringt,", "tokens": ["Drin", "ruhn", ",", "von", "f\u00fcrst\u00b7li\u00b7chem", "Glanz", "um\u00b7ringt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "$,", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Traumk\u00f6nig und sein Liebchen.", "tokens": ["Traum\u00b7k\u00f6\u00b7nig", "und", "sein", "Lieb\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "PPOSAT", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.8": {"line.1": {"text": "Aus purpurnen Polstern bereitet schwillt", "tokens": ["Aus", "pur\u00b7pur\u00b7nen", "Pols\u00b7tern", "be\u00b7rei\u00b7tet", "schwillt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "VVFIN"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die pr\u00e4chtige Lagerst\u00e4tte;", "tokens": ["Die", "pr\u00e4ch\u00b7ti\u00b7ge", "La\u00b7ger\u00b7st\u00e4t\u00b7te", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Von ferne d\u00e4mmert die Lampe mild.", "tokens": ["Von", "fer\u00b7ne", "d\u00e4m\u00b7mert", "die", "Lam\u00b7pe", "mild", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Zwei Pagen knieen am Bette.", "tokens": ["Zwei", "Pa\u00b7gen", "kni\u00b7e\u00b7en", "am", "Bet\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.9": {"line.1": {"text": "Und dr\u00fcber in silbernem Reifen schwingt", "tokens": ["Und", "dr\u00fc\u00b7ber", "in", "sil\u00b7ber\u00b7nem", "Rei\u00b7fen", "schwingt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PAV", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Ein Vogel sein farbig Gefieder,", "tokens": ["Ein", "Vo\u00b7gel", "sein", "far\u00b7big", "Ge\u00b7fie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Er schaukelt sich sacht wie im Schlaf und singt", "tokens": ["Er", "schau\u00b7kelt", "sich", "sacht", "wie", "im", "Schlaf", "und", "singt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "KOKOM", "APPRART", "NN", "KON", "VVFIN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Ein Brautlied schmelzend hernieder.", "tokens": ["Ein", "Braut\u00b7lied", "schmel\u00b7zend", "her\u00b7nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.10": {"line.1": {"text": "So ruht Traumk\u00f6nig beim Liebchen fein", "tokens": ["So", "ruht", "Traum\u00b7k\u00f6\u00b7nig", "beim", "Lieb\u00b7chen", "fein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "APPRART", "NN", "ADJD"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "In traulichem K\u00fcssen und Kosen,", "tokens": ["In", "trau\u00b7li\u00b7chem", "K\u00fcs\u00b7sen", "und", "Ko\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Bis hell das Lager der Morgenschein", "tokens": ["Bis", "hell", "das", "La\u00b7ger", "der", "Mor\u00b7gen\u00b7schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJD", "ART", "NN", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Bekr\u00e4nzt mit leuchtenden Rosen.", "tokens": ["Be\u00b7kr\u00e4nzt", "mit", "leuch\u00b7ten\u00b7den", "Ro\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.11": {"line.1": {"text": "Dann schwindet der Elfe von dannen sacht,", "tokens": ["Dann", "schwin\u00b7det", "der", "El\u00b7fe", "von", "dan\u00b7nen", "sacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ADV", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Rings ist der Zauber zerflossen,", "tokens": ["Rings", "ist", "der", "Zau\u00b7ber", "zer\u00b7flos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Und auch das M\u00e4dchen, das holde, erwacht,", "tokens": ["Und", "auch", "das", "M\u00e4d\u00b7chen", ",", "das", "hol\u00b7de", ",", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$,", "ART", "ADJA", "$,", "VVPP", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Von lieblicher Scham \u00fcbergossen.", "tokens": ["Von", "lieb\u00b7li\u00b7cher", "Scham", "\u00fc\u00b7ber\u00b7gos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Doch als sie empor nun die Augen schl\u00e4gt,", "tokens": ["Doch", "als", "sie", "em\u00b7por", "nun", "die", "Au\u00b7gen", "schl\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PTKVZ", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Von langen Wimpern ums\u00e4umet,", "tokens": ["Von", "lan\u00b7gen", "Wim\u00b7pern", "um\u00b7s\u00e4u\u00b7met", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da seufzt sie, da pre\u00dft sie das Herz bewegt:", "tokens": ["Da", "seufzt", "sie", ",", "da", "pre\u00dft", "sie", "das", "Herz", "be\u00b7wegt", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADV", "VVFIN", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "\u00bbach, war denn mein Gl\u00fcck nur getr\u00e4umet?\u00ab", "tokens": ["\u00bb", "ach", ",", "war", "denn", "mein", "Gl\u00fcck", "nur", "ge\u00b7tr\u00e4u\u00b7met", "?", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ITJ", "$,", "VAFIN", "ADV", "PPOSAT", "NN", "ADV", "VVPP", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}