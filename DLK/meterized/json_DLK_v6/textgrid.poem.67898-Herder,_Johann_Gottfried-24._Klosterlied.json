{"textgrid.poem.67898": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "24. Klosterlied", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kein' sch\u00f6nre Freud auf Erden ist", "tokens": ["Kein'", "sch\u00f6n\u00b7re", "Freud", "auf", "Er\u00b7den", "ist"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "APPR", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als in das Kloster zu ziehn.", "tokens": ["Als", "in", "das", "Klos\u00b7ter", "zu", "ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Ich hab mich drein ergeben,", "tokens": ["Ich", "hab", "mich", "drein", "er\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu f\u00fchren ein geistlich Leben;", "tokens": ["Zu", "f\u00fch\u00b7ren", "ein", "geist\u00b7lich", "Le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ART", "ADJD", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "O Liebe, was hab ich gethan!", "tokens": ["O", "Lie\u00b7be", ",", "was", "hab", "ich", "ge\u00b7than", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWS", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "O Liebe etc.", "tokens": ["O", "Lie\u00b7be", "et\u00b7c."], "token_info": ["word", "word", "abbreviation"], "pos": ["NE", "NN", "ADV"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.2": {"line.1": {"text": "Des Morgens, wenn ich in die Kirche geh", "tokens": ["Des", "Mor\u00b7gens", ",", "wenn", "ich", "in", "die", "Kir\u00b7che", "geh"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "$,", "KOUS", "PPER", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mu\u00df singen die Mess alleine;", "tokens": ["Mu\u00df", "sin\u00b7gen", "die", "Mess", "al\u00b7lei\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und wenn ich das Gloria patri sing',", "tokens": ["Und", "wenn", "ich", "das", "Glo\u00b7ria", "pa\u00b7tri", "sing'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ART", "FM", "FM", "FM", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "So liegt mir mein Liebchen immer im Sinn,", "tokens": ["So", "liegt", "mir", "mein", "Lieb\u00b7chen", "im\u00b7mer", "im", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "O Liebe, was hab ich gethan!", "tokens": ["O", "Lie\u00b7be", ",", "was", "hab", "ich", "ge\u00b7than", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWS", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "O Liebe etc.", "tokens": ["O", "Lie\u00b7be", "et\u00b7c."], "token_info": ["word", "word", "abbreviation"], "pos": ["NE", "NN", "ADV"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.3": {"line.1": {"text": "Da k\u00f6mmt mein Vater und Mutter her,", "tokens": ["Da", "k\u00f6mmt", "mein", "Va\u00b7ter", "und", "Mut\u00b7ter", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sie beten f\u00fcr sich alleine;", "tokens": ["Sie", "be\u00b7ten", "f\u00fcr", "sich", "al\u00b7lei\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PRF", "ADV", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sie haben sch\u00f6ne Kleider an,", "tokens": ["Sie", "ha\u00b7ben", "sch\u00f6\u00b7ne", "Klei\u00b7der", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich aber mu\u00df in der Kutten stahn;", "tokens": ["Ich", "a\u00b7ber", "mu\u00df", "in", "der", "Kut\u00b7ten", "stahn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "O Liebe, was hab ich gethan!", "tokens": ["O", "Lie\u00b7be", ",", "was", "hab", "ich", "ge\u00b7than", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWS", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "O Liebe etc.", "tokens": ["O", "Lie\u00b7be", "et\u00b7c."], "token_info": ["word", "word", "abbreviation"], "pos": ["NE", "NN", "ADV"], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.4": {"line.1": {"text": "Des Abends, wenn ich schlafen geh,", "tokens": ["Des", "A\u00b7bends", ",", "wenn", "ich", "schla\u00b7fen", "geh", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So find ich mein Bettchen alleine;", "tokens": ["So", "find", "ich", "mein", "Bett\u00b7chen", "al\u00b7lei\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "ADV", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "So denk ich denn, das Gott erbarm!", "tokens": ["So", "denk", "ich", "denn", ",", "das", "Gott", "er\u00b7barm", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ach h\u00e4tt' ich mein Liebchen in dem Arm,", "tokens": ["Ach", "h\u00e4tt'", "ich", "mein", "Lieb\u00b7chen", "in", "dem", "Arm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VAFIN", "PPER", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "O Liebe, was hab ich gethan!", "tokens": ["O", "Lie\u00b7be", ",", "was", "hab", "ich", "ge\u00b7than", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWS", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "O Liebe etc.", "tokens": ["O", "Lie\u00b7be", "et\u00b7c."], "token_info": ["word", "word", "abbreviation"], "pos": ["NE", "NN", "ADV"], "meter": "-+-", "measure": "amphibrach.single"}}}}}