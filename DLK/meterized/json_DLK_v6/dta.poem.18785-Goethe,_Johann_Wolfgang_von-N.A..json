{"dta.poem.18785": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Lieb\u2019 um Liebe, Stund\u2019 um Stunde,", "tokens": ["Lieb'", "um", "Lie\u00b7be", ",", "Stund'", "um", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wort um Wort und Blick um Blick;", "tokens": ["Wort", "um", "Wort", "und", "Blick", "um", "Blick", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kuss um Kuss, vom treusten Munde,", "tokens": ["Kuss", "um", "Kuss", ",", "vom", "treus\u00b7ten", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hauch um Hauch und Gl\u00fcck um Gl\u00fcck.", "tokens": ["Hauch", "um", "Hauch", "und", "Gl\u00fcck", "um", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "So am Abend, so am Morgen!", "tokens": ["So", "am", "A\u00b7bend", ",", "so", "am", "Mor\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,", "ADV", "APPRART", "NN", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "Doch du f\u00fchlst an meinen Liedern", "tokens": ["Doch", "du", "f\u00fchlst", "an", "mei\u00b7nen", "Lie\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Immer noch geheime Sorgen;", "tokens": ["Im\u00b7mer", "noch", "ge\u00b7hei\u00b7me", "Sor\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Jussufs Reize m\u00f6cht\u2019 ich borgen", "tokens": ["Jus\u00b7sufs", "Rei\u00b7ze", "m\u00f6cht'", "ich", "bor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Deine Sch\u00f6nheit zu erwiedern.", "tokens": ["Dei\u00b7ne", "Sch\u00f6n\u00b7heit", "zu", "er\u00b7wie\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}