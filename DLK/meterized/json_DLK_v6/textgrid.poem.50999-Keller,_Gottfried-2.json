{"textgrid.poem.50999": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "2", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich liege beschaulich", "tokens": ["Ich", "lie\u00b7ge", "be\u00b7schau\u00b7lich"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "An klingender Quelle", "tokens": ["An", "klin\u00b7gen\u00b7der", "Quel\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Und senke vertraulich", "tokens": ["Und", "sen\u00b7ke", "ver\u00b7trau\u00b7lich"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "ADJD"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Den Blick in die Welle;", "tokens": ["Den", "Blick", "in", "die", "Wel\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Ich such in den Sch\u00e4umen,", "tokens": ["Ich", "such", "in", "den", "Sch\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Wei\u00df selbst nicht, wonach?", "tokens": ["Wei\u00df", "selbst", "nicht", ",", "wo\u00b7nach", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKNEG", "$,", "PWAV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Verschollenes Tr\u00e4umen", "tokens": ["Ver\u00b7schol\u00b7le\u00b7nes", "Tr\u00e4u\u00b7men"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Wird in mir wach.", "tokens": ["Wird", "in", "mir", "wach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Da kommt es gefahren", "tokens": ["Da", "kommt", "es", "ge\u00b7fah\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "VVPP"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Mit l\u00e4chelndem Munde", "tokens": ["Mit", "l\u00e4\u00b7cheln\u00b7dem", "Mun\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Vor\u00fcber im klaren", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "im", "kla\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Kristallenen Grunde", "tokens": ["Kris\u00b7tal\u00b7le\u00b7nen", "Grun\u00b7de"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Das alte vertraute,", "tokens": ["Das", "al\u00b7te", "ver\u00b7trau\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Das Weltangesicht!", "tokens": ["Das", "Wel\u00b7tan\u00b7ge\u00b7sicht", "!"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Sein Aug auf mich schaute", "tokens": ["Sein", "Aug", "auf", "mich", "schau\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "PPER", "VVFIN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Mit \u00e4th'rischem Licht.", "tokens": ["Mit", "\u00e4th'\u00b7ri\u00b7schem", "Licht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Wohin ist's geschwommen", "tokens": ["Wo\u00b7hin", "ist's", "ge\u00b7schwom\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "VAFIN", "VVPP"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Im Wellengewimmel?", "tokens": ["Im", "Wel\u00b7len\u00b7ge\u00b7wim\u00b7mel", "?"], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Woher ist's gekommen?", "tokens": ["Wo\u00b7her", "ist's", "ge\u00b7kom\u00b7men", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "VVPP", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Vom blauenden Himmel!", "tokens": ["Vom", "blau\u00b7en\u00b7den", "Him\u00b7mel", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Denn als ich ins Weben", "tokens": ["Denn", "als", "ich", "ins", "We\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "APPRART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Der Wolken gesehn,", "tokens": ["Der", "Wol\u00b7ken", "ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Da sah ich noch eben", "tokens": ["Da", "sah", "ich", "noch", "e\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Es dort vergehn.", "tokens": ["Es", "dort", "ver\u00b7gehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVINF", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Ich seh es fast immer,", "tokens": ["Ich", "seh", "es", "fast", "im\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wenn's windstill und heiter,", "tokens": ["Wenn's", "wind\u00b7still", "und", "hei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "KON", "ADJD", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und stets macht sein Schimmer", "tokens": ["Und", "stets", "macht", "sein", "Schim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Die Brust mir dann weiter;", "tokens": ["Die", "Brust", "mir", "dann", "wei\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADV", "ADV", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Doch wenn sein Begegnen", "tokens": ["Doch", "wenn", "sein", "Be\u00b7geg\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Der Seele Bedarf,", "tokens": ["Der", "See\u00b7le", "Be\u00b7darf", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Im St\u00fcrmen und Regnen", "tokens": ["Im", "St\u00fcr\u00b7men", "und", "Reg\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "KON", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Auch seh ich es scharf.", "tokens": ["Auch", "seh", "ich", "es", "scharf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}