{"textgrid.poem.54852": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "66. Auff Alcippen", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Alcipp' ist hin. Auff! Auff! und wickelt die Gebein/", "tokens": ["Al\u00b7cipp'", "ist", "hin", ".", "Auff", "!", "Auff", "!", "und", "wi\u00b7ckelt", "die", "Ge\u00b7bein", "/"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "$.", "NN", "$.", "NN", "$.", "KON", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In Wermut/ Aloen/ und Hartz und Myrrhen ein;", "tokens": ["In", "Wer\u00b7mut", "/", "A\u00b7loen", "/", "und", "Hartz", "und", "Myr\u00b7rhen", "ein", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$(", "NE", "$(", "KON", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "--+-+-+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Denn weil sie lebend kont' ein gantzes Land beflecken/", "tokens": ["Denn", "weil", "sie", "le\u00b7bend", "kont'", "ein", "gant\u00b7zes", "Land", "be\u00b7fle\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "VMFIN", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So wird der Todten Stanck die grosse Welt anstecken.", "tokens": ["So", "wird", "der", "Tod\u00b7ten", "Stanck", "die", "gros\u00b7se", "Welt", "an\u00b7ste\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}