{"textgrid.poem.61553": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Gudruns Klage", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun geht in grauer Fr\u00fche", "tokens": ["Nun", "geht", "in", "grau\u00b7er", "Fr\u00fc\u00b7he"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der scharfe M\u00e4rzenwind,", "tokens": ["Der", "schar\u00b7fe", "M\u00e4r\u00b7zen\u00b7wind", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und meiner Qual und M\u00fche", "tokens": ["Und", "mei\u00b7ner", "Qual", "und", "M\u00fc\u00b7he"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein neuer Tag beginnt.", "tokens": ["Ein", "neu\u00b7er", "Tag", "be\u00b7ginnt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich wall' hinab zum Strande", "tokens": ["Ich", "wall'", "hin\u00b7ab", "zum", "Stran\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Durch Reif und Dornen hin,", "tokens": ["Durch", "Reif", "und", "Dor\u00b7nen", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Zu waschen die Gewande", "tokens": ["Zu", "wa\u00b7schen", "die", "Ge\u00b7wan\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der grimmen K\u00f6nigin.", "tokens": ["Der", "grim\u00b7men", "K\u00f6\u00b7ni\u00b7gin", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Das Meer ist tief und herbe,", "tokens": ["Das", "Meer", "ist", "tief", "und", "her\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Doch tiefer ist die Pein,", "tokens": ["Doch", "tie\u00b7fer", "ist", "die", "Pein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Von Freund und Heimatserbe", "tokens": ["Von", "Freund", "und", "Hei\u00b7mat\u00b7ser\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Allzeit geschieden sein;", "tokens": ["All\u00b7zeit", "ge\u00b7schie\u00b7den", "sein", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Doch herber ist's, zu dienen", "tokens": ["Doch", "her\u00b7ber", "ist's", ",", "zu", "die\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "$,", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "In fremder M\u00e4gde Schar,", "tokens": ["In", "frem\u00b7der", "M\u00e4g\u00b7de", "Schar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und hat mir einst geschienen", "tokens": ["Und", "hat", "mir", "einst", "ge\u00b7schie\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die g\u00fcldne Kron' im Haar.", "tokens": ["Die", "g\u00fcld\u00b7ne", "Kron'", "im", "Haar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Mir ward kein guter Morgen,", "tokens": ["Mir", "ward", "kein", "gu\u00b7ter", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Seit ich dem Feind verfiel:", "tokens": ["Seit", "ich", "dem", "Feind", "ver\u00b7fiel", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mein Speis' und Trank sind Sorgen,", "tokens": ["Mein", "Speis'", "und", "Trank", "sind", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "VAFIN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und Kummer mein Gespiel.", "tokens": ["Und", "Kum\u00b7mer", "mein", "Ge\u00b7spiel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Doch berg' ich meine Tr\u00e4nen", "tokens": ["Doch", "ber\u00b7g'", "ich", "mei\u00b7ne", "Tr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "In stolzer Einsamkeit;", "tokens": ["In", "stol\u00b7zer", "Ein\u00b7sam\u00b7keit", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Am Strand den wilden Schw\u00e4nen", "tokens": ["Am", "Strand", "den", "wil\u00b7den", "Schw\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Allein sing' ich mein Leid.", "tokens": ["Al\u00b7lein", "sing'", "ich", "mein", "Leid", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Kein Dr\u00e4uen soll mir beugen", "tokens": ["Kein", "Dr\u00e4u\u00b7en", "soll", "mir", "beu\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den hochgemuten Sinn;", "tokens": ["Den", "hoch\u00b7ge\u00b7mu\u00b7ten", "Sinn", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ausduldend will ich zeugen,", "tokens": ["Aus\u00b7dul\u00b7dend", "will", "ich", "zeu\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von welchem Stamm ich bin.", "tokens": ["Von", "wel\u00b7chem", "Stamm", "ich", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "PPER", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und so sie hold gebaren,", "tokens": ["Und", "so", "sie", "hold", "ge\u00b7ba\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wie Spinnweb acht' ich's nur;", "tokens": ["Wie", "Spinn\u00b7web", "acht'", "ich's", "nur", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "PIS", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ich will getreu bewahren", "tokens": ["Ich", "will", "ge\u00b7treu", "be\u00b7wah\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mein Herz und meinen Schwur.", "tokens": ["Mein", "Herz", "und", "mei\u00b7nen", "Schwur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "O Ortwin, trauter Bruder,", "tokens": ["O", "Ort\u00b7win", ",", "trau\u00b7ter", "Bru\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "O Herwig! Buhle wert,", "tokens": ["O", "Her\u00b7wig", "!", "Buh\u00b7le", "wert", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was rauscht nicht euer Ruder,", "tokens": ["Was", "rauscht", "nicht", "eu\u00b7er", "Ru\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PTKNEG", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was klingt nicht euer Schwert!", "tokens": ["Was", "klingt", "nicht", "eu\u00b7er", "Schwert", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Umsonst zur Meeresw\u00fcste", "tokens": ["Um\u00b7sonst", "zur", "Mee\u00b7res\u00b7w\u00fcs\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Hinsp\u00e4h' ich jede Stund':", "tokens": ["Hin\u00b7sp\u00e4h'", "ich", "je\u00b7de", "Stund'", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Doch naht sich dieser K\u00fcste", "tokens": ["Doch", "naht", "sich", "die\u00b7ser", "K\u00fcs\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Kein Wimpel, das mir kund.", "tokens": ["Kein", "Wim\u00b7pel", ",", "das", "mir", "kund", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ich wei\u00df es: Nicht vergessen", "tokens": ["Ich", "wei\u00df", "es", ":", "Nicht", "ver\u00b7ges\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$.", "PTKNEG", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Habt ihr der armen Maid;", "tokens": ["Habt", "ihr", "der", "ar\u00b7men", "Maid", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch ist nur kurz gemessen", "tokens": ["Doch", "ist", "nur", "kurz", "ge\u00b7mes\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ADV", "ADJD", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dem steten Gram die Zeit.", "tokens": ["Dem", "ste\u00b7ten", "Gram", "die", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wohl kommt ihr einst, zu s\u00fchnen; \u2013", "tokens": ["Wohl", "kommt", "ihr", "einst", ",", "zu", "s\u00fch\u00b7nen", ";", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "PTKZU", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Zu retten, ach, zu sp\u00e4t,", "tokens": ["Zu", "ret\u00b7ten", ",", "ach", ",", "zu", "sp\u00e4t", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "ITJ", "$,", "PTKA", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wann schon der Sand der D\u00fcnen", "tokens": ["Wann", "schon", "der", "Sand", "der", "D\u00fc\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Um meinen H\u00fcgel weht.", "tokens": ["Um", "mei\u00b7nen", "H\u00fc\u00b7gel", "weht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Es dr\u00f6hnt mit dumpfem Schlage", "tokens": ["Es", "dr\u00f6hnt", "mit", "dum\u00b7pfem", "Schla\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Brandung in mein Wort;", "tokens": ["Die", "Bran\u00b7dung", "in", "mein", "Wort", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Sturm zerrei\u00dft die Klage", "tokens": ["Der", "Sturm", "zer\u00b7rei\u00dft", "die", "Kla\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und tr\u00e4gt beschwingt sie fort.", "tokens": ["Und", "tr\u00e4gt", "be\u00b7schwingt", "sie", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "O m\u00f6cht' er brausend schweben", "tokens": ["O", "m\u00f6cht'", "er", "brau\u00b7send", "schwe\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VMFIN", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und geben euch Bericht:", "tokens": ["Und", "ge\u00b7ben", "euch", "Be\u00b7richt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "\u00bbwohl lass' ich hier das Leben,", "tokens": ["\u00bb", "wohl", "lass'", "ich", "hier", "das", "Le\u00b7ben", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Treue lass' ich nicht!\u00ab", "tokens": ["Treu\u00b7e", "lass'", "ich", "nicht", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKNEG", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}