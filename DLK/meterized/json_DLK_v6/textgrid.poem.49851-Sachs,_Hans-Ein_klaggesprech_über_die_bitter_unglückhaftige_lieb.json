{"textgrid.poem.49851": {"metadata": {"author": {"name": "Sachs, Hans", "birth": "N.A.", "death": "N.A."}, "title": "Ein klaggesprech \u00fcber die bitter ungl\u00fcckhaftige lieb", "genre": "verse", "period": "N.A.", "pub_year": 1568, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eins morgens gieng ich aus spazieren,", "tokens": ["Eins", "mor\u00b7gens", "gieng", "ich", "aus", "spa\u00b7zie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "PPER", "APPR", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "f\u00fcr einen gr\u00fcnen walt refieren,", "tokens": ["f\u00fcr", "ei\u00b7nen", "gr\u00fc\u00b7nen", "walt", "re\u00b7fie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "da h\u00f6rt ich ein heimlich gesprech,", "tokens": ["da", "h\u00f6rt", "ich", "ein", "heim\u00b7lich", "ge\u00b7sprech", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJD", "ADJD", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "in einem busche in der nech", "tokens": ["in", "ei\u00b7nem", "bu\u00b7sche", "in", "der", "nech"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "tet ich durch das gestreus nein schauen,", "tokens": ["tet", "ich", "durch", "das", "ge\u00b7streus", "nein", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "PTKANT", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "da sa\u00df ein gsell bei einer frauen;", "tokens": ["da", "sa\u00df", "ein", "gsell", "bei", "ei\u00b7ner", "frau\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "ich lost zu irem freuntlichn sagen,", "tokens": ["ich", "lost", "zu", "i\u00b7rem", "freunt\u00b7lichn", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "da war es nichts den bitter klagen.", "tokens": ["da", "war", "es", "nichts", "den", "bit\u00b7ter", "kla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIS", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}