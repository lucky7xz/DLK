{"textgrid.poem.57730": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Des Hanebuth Lied", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Tau liegt auf der Heide,", "tokens": ["Der", "Tau", "liegt", "auf", "der", "Hei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das wird ein sch\u00f6ner Tag,", "tokens": ["Das", "wird", "ein", "sch\u00f6\u00b7ner", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein V\u00f6gelein mit Freude", "tokens": ["Ein", "V\u00f6\u00b7ge\u00b7lein", "mit", "Freu\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Erhebet seinen Schlag;", "tokens": ["Er\u00b7he\u00b7bet", "sei\u00b7nen", "Schlag", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Taschen sind mir leer,", "tokens": ["Die", "Ta\u00b7schen", "sind", "mir", "leer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Der Magen noch viel mehr;", "tokens": ["Der", "Ma\u00b7gen", "noch", "viel", "mehr", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ich armer Heidel\u00e4ufer,", "tokens": ["Ich", "ar\u00b7mer", "Hei\u00b7de\u00b7l\u00e4u\u00b7fer", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mich dauert meiner sehr.", "tokens": ["Mich", "dau\u00b7ert", "mei\u00b7ner", "sehr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die W\u00fcrfel und die Frauen", "tokens": ["Die", "W\u00fcr\u00b7fel", "und", "die", "Frau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sind voller Trug und T\u00fcck,", "tokens": ["Sind", "vol\u00b7ler", "Trug", "und", "T\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wer ihnen mochte trauen,", "tokens": ["Wer", "ih\u00b7nen", "moch\u00b7te", "trau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dem ward ein gl\u00e4sern Gl\u00fcck;", "tokens": ["Dem", "ward", "ein", "gl\u00e4\u00b7sern", "Gl\u00fcck", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dazu der Branntewein,", "tokens": ["Da\u00b7zu", "der", "Brann\u00b7te\u00b7wein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Erst l\u00e4uft er lieblich ein;", "tokens": ["Erst", "l\u00e4uft", "er", "lieb\u00b7lich", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ist man des Rausches ledig,", "tokens": ["Ist", "man", "des", "Rau\u00b7sches", "le\u00b7dig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ART", "NN", "ADJD", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.8": {"text": "So tr\u00e4gt man schwere Pein.", "tokens": ["So", "tr\u00e4gt", "man", "schwe\u00b7re", "Pein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich hab' die drei genossen", "tokens": ["Ich", "hab'", "die", "drei", "ge\u00b7nos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "CARD", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zu Bothfeld in dem Krug,", "tokens": ["Zu", "Both\u00b7feld", "in", "dem", "Krug", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mein Geld, das ist zerflossen,", "tokens": ["Mein", "Geld", ",", "das", "ist", "zer\u00b7flos\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PDS", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hab' dessen nie genug;", "tokens": ["Hab'", "des\u00b7sen", "nie", "ge\u00b7nug", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Pfennige sind hin,", "tokens": ["Die", "Pfen\u00b7ni\u00b7ge", "sind", "hin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Das Weib war falsch von Sinn;", "tokens": ["Das", "Weib", "war", "falsch", "von", "Sinn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der Branntwein macht mir Kopfweh,", "tokens": ["Der", "Brannt\u00b7wein", "macht", "mir", "Kopf\u00b7weh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Das ist nun mein Gewinn.", "tokens": ["Das", "ist", "nun", "mein", "Ge\u00b7winn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Da\u00df ich ein Kaufherr w\u00e4re,", "tokens": ["Da\u00df", "ich", "ein", "Kauf\u00b7herr", "w\u00e4\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit vielem Gut und Geld,", "tokens": ["Mit", "vie\u00b7lem", "Gut", "und", "Geld", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ADJD", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich st\u00e4nde hoch in Ehre,", "tokens": ["Ich", "st\u00e4n\u00b7de", "hoch", "in", "Eh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "H\u00e4tt' wohl mein' Seel' bestellt;", "tokens": ["H\u00e4tt'", "wohl", "mein'", "Seel'", "be\u00b7stellt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nun streif' ich armer Fant", "tokens": ["Nun", "streif'", "ich", "ar\u00b7mer", "Fant"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Im magren Heideland;", "tokens": ["Im", "mag\u00b7ren", "Hei\u00b7de\u00b7land", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Da flie\u00dft die braune Wietze,", "tokens": ["Da", "flie\u00dft", "die", "brau\u00b7ne", "Wiet\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Da fliegt der gelbe Sand.", "tokens": ["Da", "fliegt", "der", "gel\u00b7be", "Sand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Was seh' ich dr\u00fcben reiten?", "tokens": ["Was", "seh'", "ich", "dr\u00fc\u00b7ben", "rei\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Kr\u00e4mer hochgemut,", "tokens": ["Ein", "Kr\u00e4\u00b7mer", "hoch\u00b7ge\u00b7mut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich wittre fette Zeiten,", "tokens": ["Ich", "witt\u00b7re", "fet\u00b7te", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Meine Kugel trifft ihn gut;", "tokens": ["Mei\u00b7ne", "Ku\u00b7gel", "trifft", "ihn", "gut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Schnell Pulver auf die Pfann',", "tokens": ["Schnell", "Pul\u00b7ver", "auf", "die", "Pfann'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Du lieber Reitersmann,", "tokens": ["Du", "lie\u00b7ber", "Rei\u00b7ters\u00b7mann", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Jetzt k\u00fcssest du die Erden,", "tokens": ["Jetzt", "k\u00fcs\u00b7sest", "du", "die", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Dieweil ich fahren kann.", "tokens": ["Die\u00b7weil", "ich", "fah\u00b7ren", "kann", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Der Taler hat er drei\u00dfig", "tokens": ["Der", "Ta\u00b7ler", "hat", "er", "drei\u00b7\u00dfig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPER", "CARD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In seinem Sacke drin,", "tokens": ["In", "sei\u00b7nem", "Sa\u00b7cke", "drin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der frohe Jasper hei\u00df ich,", "tokens": ["Der", "fro\u00b7he", "Jas\u00b7per", "hei\u00df", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nach Bothfeld steht mein Sinn;", "tokens": ["Nach", "Both\u00b7feld", "steht", "mein", "Sinn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Zu Fu\u00dfe zog ich aus,", "tokens": ["Zu", "Fu\u00b7\u00dfe", "zog", "ich", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Jetzt fahr' ich vor das Haus;", "tokens": ["Jetzt", "fahr'", "ich", "vor", "das", "Haus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Herr Wirt und auch Frau Wirtin,", "tokens": ["Herr", "Wirt", "und", "auch", "Frau", "Wir\u00b7tin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "ADV", "NN", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "So richtet her den Schmaus.", "tokens": ["So", "rich\u00b7tet", "her", "den", "Schmaus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Und zapft vom Brannteweine", "tokens": ["Und", "zapft", "vom", "Brann\u00b7te\u00b7wei\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und von dem braunen Bier,", "tokens": ["Und", "von", "dem", "brau\u00b7nen", "Bier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du H\u00fcbsche und du Feine,", "tokens": ["Du", "H\u00fcb\u00b7sche", "und", "du", "Fei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "KON", "PPER", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein Liedchen singen wir;", "tokens": ["Ein", "Lied\u00b7chen", "sin\u00b7gen", "wir", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Geh' her und tu' Bescheid;", "tokens": ["Geh'", "her", "und", "tu'", "Be\u00b7scheid", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Soll werden dir nicht leid;", "tokens": ["Soll", "wer\u00b7den", "dir", "nicht", "leid", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VAFIN", "PPER", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ein T\u00fcchlein sollst du haben,", "tokens": ["Ein", "T\u00fcch\u00b7lein", "sollst", "du", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Desgleichen ein Geschmeid'.", "tokens": ["Des\u00b7glei\u00b7chen", "ein", "Ge\u00b7schmeid'", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Was fliegen da die Raben", "tokens": ["Was", "flie\u00b7gen", "da", "die", "Ra\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und heben an zu schrei'n?", "tokens": ["Und", "he\u00b7ben", "an", "zu", "schrei'n", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es soll mich frommen Knaben", "tokens": ["Es", "soll", "mich", "from\u00b7men", "Kna\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Des Werkes nicht gereu'n;", "tokens": ["Des", "Wer\u00b7kes", "nicht", "ge\u00b7reu'n", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es n\u00e4hrt sich jeder Mann", "tokens": ["Es", "n\u00e4hrt", "sich", "je\u00b7der", "Mann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "So wacker wie er kann;", "tokens": ["So", "wa\u00b7cker", "wie", "er", "kann", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "PPER", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Herr Wirt, schlie\u00dft zu die Laden,", "tokens": ["Herr", "Wirt", ",", "schlie\u00dft", "zu", "die", "La\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.8": {"text": "Der Abend naht heran.", "tokens": ["Der", "A\u00b7bend", "naht", "he\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "La\u00dft uns das Liedlein singen,", "tokens": ["La\u00dft", "uns", "das", "Lied\u00b7lein", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vom Brummelbeerbusch das Lied,", "tokens": ["Vom", "Brum\u00b7mel\u00b7beer\u00b7busch", "das", "Lied", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Das gibt ein trefflich Klingen", "tokens": ["Das", "gibt", "ein", "treff\u00b7lich", "Klin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "ADJD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wohl f\u00fcr mein treu' Gem\u00fct;", "tokens": ["Wohl", "f\u00fcr", "mein", "treu'", "Ge\u00b7m\u00fct", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der schwarzen Raben Schrei'n", "tokens": ["Der", "schwar\u00b7zen", "Ra\u00b7ben", "Schrei'n"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Geht mir nicht glimpflich ein;", "tokens": ["Geht", "mir", "nicht", "glimpf\u00b7lich", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Es soll mich heut' nicht st\u00f6ren,", "tokens": ["Es", "soll", "mich", "heut'", "nicht", "st\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Gar lustig will ich sein.", "tokens": ["Gar", "lus\u00b7tig", "will", "ich", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VMFIN", "PPER", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Das Liedlein ist gesungen,", "tokens": ["Das", "Lied\u00b7lein", "ist", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das Kr\u00fcglein, das ist leer,", "tokens": ["Das", "Kr\u00fcg\u00b7lein", ",", "das", "ist", "leer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PDS", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Gl\u00e4slein ist zersprungen,", "tokens": ["Das", "Gl\u00e4s\u00b7lein", "ist", "zer\u00b7sprun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mich freut kein Tr\u00fcnklein mehr;", "tokens": ["Mich", "freut", "kein", "Tr\u00fcn\u00b7klein", "mehr", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der Wein sieht aus wie Blut,", "tokens": ["Der", "Wein", "sieht", "aus", "wie", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "KOKOM", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Der Tabak schmeckt nicht gut,", "tokens": ["Der", "Ta\u00b7bak", "schmeckt", "nicht", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Mein Lachen wird zum Weinen,", "tokens": ["Mein", "La\u00b7chen", "wird", "zum", "Wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Das Herz mir wehe tut.", "tokens": ["Das", "Herz", "mir", "we\u00b7he", "tut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Hannover an der Leine", "tokens": ["Han\u00b7no\u00b7ver", "an", "der", "Lei\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ist eine sch\u00f6ne Stadt,", "tokens": ["Ist", "ei\u00b7ne", "sch\u00f6\u00b7ne", "Stadt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Am Steintor auf dem Raine,", "tokens": ["Am", "Stein\u00b7tor", "auf", "dem", "Rai\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da steht das gute Rad;", "tokens": ["Da", "steht", "das", "gu\u00b7te", "Rad", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dabei so steif und stolz", "tokens": ["Da\u00b7bei", "so", "steif", "und", "stolz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "ADV", "ADJD", "KON", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Das edle Galgenholz;", "tokens": ["Das", "ed\u00b7le", "Gal\u00b7gen\u00b7holz", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der Himmel mocht's nicht leiden,", "tokens": ["Der", "Him\u00b7mel", "mocht's", "nicht", "lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der Teufel aber wollt's.", "tokens": ["Der", "Teu\u00b7fel", "a\u00b7ber", "wollt'", "s."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["ART", "NN", "ADV", "VMFIN", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Ich glaub' nicht, was der Pfaffe", "tokens": ["Ich", "glaub'", "nicht", ",", "was", "der", "Pfaf\u00b7fe"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In seiner Kirche spricht,", "tokens": ["In", "sei\u00b7ner", "Kir\u00b7che", "spricht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Lamm hat keine Waffe,", "tokens": ["Das", "Lamm", "hat", "kei\u00b7ne", "Waf\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sonst grasete es nicht;", "tokens": ["Sonst", "gra\u00b7se\u00b7te", "es", "nicht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die bitterb\u00f6se Not,", "tokens": ["Die", "bit\u00b7ter\u00b7b\u00f6\u00b7se", "Not", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Die kennet kein Gebot;", "tokens": ["Die", "ken\u00b7net", "kein", "Ge\u00b7bot", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der Wolf hat seine Z\u00e4hne", "tokens": ["Der", "Wolf", "hat", "sei\u00b7ne", "Z\u00e4h\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NE", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und ich hab' Kraut und Lot.", "tokens": ["Und", "ich", "hab'", "Kraut", "und", "Lot", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Das R\u00f6\u00dflein hat gefressen,", "tokens": ["Das", "R\u00f6\u00df\u00b7lein", "hat", "ge\u00b7fres\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Es scharret in dem Sand,", "tokens": ["Es", "schar\u00b7ret", "in", "dem", "Sand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nun hei\u00dft es aufgesessen,", "tokens": ["Nun", "hei\u00dft", "es", "auf\u00b7ge\u00b7ses\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wir reiten \u00fcber Land;", "tokens": ["Wir", "rei\u00b7ten", "\u00fc\u00b7ber", "Land", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Reicht mir das Faustrohr her,", "tokens": ["Reicht", "mir", "das", "Faust\u00b7rohr", "her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Und auch die gro\u00dfe Wehr;", "tokens": ["Und", "auch", "die", "gro\u00b7\u00dfe", "Wehr", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der Wind pfeift auf der Heide,", "tokens": ["Der", "Wind", "pfeift", "auf", "der", "Hei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Das Herze wird mir schwer.", "tokens": ["Das", "Her\u00b7ze", "wird", "mir", "schwer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}