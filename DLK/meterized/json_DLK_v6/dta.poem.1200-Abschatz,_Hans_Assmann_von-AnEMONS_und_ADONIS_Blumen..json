{"dta.poem.1200": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "AnEMONS  \n und  \n  ADONIS  \n Blumen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "An diesem wilden Ort/ auff dieser rauhen Spitze/", "tokens": ["An", "die\u00b7sem", "wil\u00b7den", "Ort", "/", "auff", "die\u00b7ser", "rau\u00b7hen", "Spit\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$(", "APPR", "PDAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo stille Lufft/ wo Sonn und Sommer G\u00e4ste seyn/", "tokens": ["Wo", "stil\u00b7le", "Lufft", "/", "wo", "Sonn", "und", "Som\u00b7mer", "G\u00e4s\u00b7te", "seyn", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$(", "PWAV", "NN", "KON", "NN", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wo ich f\u00fcr Frost halb todt bey lauher Asche sitze/", "tokens": ["Wo", "ich", "f\u00fcr", "Frost", "halb", "todt", "bey", "lau\u00b7her", "A\u00b7sche", "sit\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "NN", "ADJD", "ADJD", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Begeh ich doch mit Lust des werthen Tages Schein.", "tokens": ["Be\u00b7geh", "ich", "doch", "mit", "Lust", "des", "wert\u00b7hen", "Ta\u00b7ges", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "NN", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ein Lied/ ein schlechter Reim soll meine Nimphe binden:", "tokens": ["Ein", "Lied", "/", "ein", "schlech\u00b7ter", "Reim", "soll", "mei\u00b7ne", "Nim\u00b7phe", "bin\u00b7den", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "ADJA", "NN", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Geschencke/ die ihr werth/ sind um kein Geld zu finden.", "tokens": ["Ge\u00b7schen\u00b7cke", "/", "die", "ihr", "werth", "/", "sind", "um", "kein", "Geld", "zu", "fin\u00b7den", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PRELS", "PPER", "ADJD", "$(", "VAFIN", "APPR", "PIAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Verzeihe mir/ im Fall nicht gutte Reimen fl\u00fcssen/", "tokens": ["Ver\u00b7zei\u00b7he", "mir", "/", "im", "Fall", "nicht", "gut\u00b7te", "Rei\u00b7men", "fl\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "APPRART", "NN", "PTKNEG", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein grobes Holtz vertritt der zarten Feder Amt/", "tokens": ["Ein", "gro\u00b7bes", "Holtz", "ver\u00b7tritt", "der", "zar\u00b7ten", "Fe\u00b7der", "Amt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Schnee ist mein Papir/ doch zeuget mein Gewissen/", "tokens": ["Der", "Schnee", "ist", "mein", "Pa\u00b7pir", "/", "doch", "zeu\u00b7get", "mein", "Ge\u00b7wis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$(", "ADV", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df dieser kurtze Wunsch aus reinem Hertzen stammt.", "tokens": ["Da\u00df", "die\u00b7ser", "kurt\u00b7ze", "Wunsch", "aus", "rei\u00b7nem", "Hert\u00b7zen", "stammt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Des Himmels Gunst la\u00df ihn im Winter auch bekleiben/", "tokens": ["Des", "Him\u00b7mels", "Gunst", "la\u00df", "ihn", "im", "Win\u00b7ter", "auch", "be\u00b7klei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVIMP", "PPER", "APPRART", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und einen gutten Wind zu deiner Wohnung treiben!", "tokens": ["Und", "ei\u00b7nen", "gut\u00b7ten", "Wind", "zu", "dei\u00b7ner", "Woh\u00b7nung", "trei\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Es m\u00fcsse so viel Lust dein edles Hertz erfreuen/", "tokens": ["Es", "m\u00fcs\u00b7se", "so", "viel", "Lust", "dein", "ed\u00b7les", "Hertz", "er\u00b7freu\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PIAT", "NN", "PPOSAT", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als mein Gem\u00fctte Schmertz und Trauren in sich hegt!", "tokens": ["Als", "mein", "Ge\u00b7m\u00fct\u00b7te", "Schmertz", "und", "Trau\u00b7ren", "in", "sich", "hegt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "KON", "NN", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es m\u00fcsse so viel Gl\u00fcck und Wohlfart dich beschneyen/", "tokens": ["Es", "m\u00fcs\u00b7se", "so", "viel", "Gl\u00fcck", "und", "Wohl\u00b7fart", "dich", "be\u00b7schne\u00b7yen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PIAT", "NN", "KON", "NN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als dieser hohe Berg gefrorne Tropffen tr\u00e4gt.", "tokens": ["Als", "die\u00b7ser", "ho\u00b7he", "Berg", "ge\u00b7fror\u00b7ne", "Tropf\u00b7fen", "tr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "ADJA", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es kan dir nimmermehr so wohl und gl\u00fccklich gehen/", "tokens": ["Es", "kan", "dir", "nim\u00b7mer\u00b7mehr", "so", "wohl", "und", "gl\u00fcck\u00b7lich", "ge\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "ADV", "ADV", "KON", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df mein getreuer Wunsch dabey wird stille stehen.", "tokens": ["Da\u00df", "mein", "ge\u00b7treu\u00b7er", "Wunsch", "da\u00b7bey", "wird", "stil\u00b7le", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "PAV", "VAFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}