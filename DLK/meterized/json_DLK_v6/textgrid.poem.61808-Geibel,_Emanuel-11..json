{"textgrid.poem.61808": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "11.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sanft verglimmt des Tages Helle,", "tokens": ["Sanft", "ver\u00b7glimmt", "des", "Ta\u00b7ges", "Hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und, vom letzten Strahl gek\u00fc\u00dft,", "tokens": ["Und", ",", "vom", "letz\u00b7ten", "Strahl", "ge\u00b7k\u00fc\u00dft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Liegt die glatte Meereswelle", "tokens": ["Liegt", "die", "glat\u00b7te", "Mee\u00b7res\u00b7wel\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie geschmolzner Amethyst.", "tokens": ["Wie", "ge\u00b7schmolz\u00b7ner", "A\u00b7met\u00b7hyst", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Kaum ein L\u00fcftchen r\u00fchrt die Schwingen,", "tokens": ["Kaum", "ein", "L\u00fcft\u00b7chen", "r\u00fchrt", "die", "Schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schweigen rings und Abendglut!", "tokens": ["Schwei\u00b7gen", "rings", "und", "A\u00b7bend\u00b7glut", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur der Fischer leises Singen", "tokens": ["Nur", "der", "Fi\u00b7scher", "lei\u00b7ses", "Sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schwebt verhallend auf der Flut.", "tokens": ["Schwebt", "ver\u00b7hal\u00b7lend", "auf", "der", "Flut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VVPP", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Jetzt erstirbt's; ihr Nachen gleitet", "tokens": ["Jetzt", "e\u00b7rstirbt's", ";", "ihr", "Na\u00b7chen", "glei\u00b7tet"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$.", "PPOSAT", "NN", "VVFIN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ohne Laut dem Hafen zu,", "tokens": ["Oh\u00b7ne", "Laut", "dem", "Ha\u00b7fen", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und um meine Seele breitet", "tokens": ["Und", "um", "mei\u00b7ne", "See\u00b7le", "brei\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich dein Zauber, Meeresruh'.", "tokens": ["Sich", "dein", "Zau\u00b7ber", ",", "Mee\u00b7res\u00b7ruh'", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PRF", "PPOSAT", "NN", "$,", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}