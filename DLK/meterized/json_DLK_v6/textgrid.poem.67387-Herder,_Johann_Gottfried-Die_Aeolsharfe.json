{"textgrid.poem.67387": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Die Aeolsharfe", "genre": "verse", "period": "N.A.", "pub_year": 1795, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kommt, \u00e4therische Wesen,", "tokens": ["Kommt", ",", "\u00e4t\u00b7he\u00b7ri\u00b7sche", "We\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Luftbewohner, die Ihr \u00fcber der Menschheit Loos", "tokens": ["Luft\u00b7be\u00b7woh\u00b7ner", ",", "die", "Ihr", "\u00fc\u00b7ber", "der", "Menschheit", "Loos"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "PPER", "APPR", "ART", "NN", "NE"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Euch betr\u00fcbt und erfreuet!", "tokens": ["Euch", "be\u00b7tr\u00fcbt", "und", "er\u00b7freu\u00b7et", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "KON", "VVFIN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Aeol's Saiten erwarten Euch. \u2013", "tokens": ["A\u00b7eol's", "Sai\u00b7ten", "er\u00b7war\u00b7ten", "Euch", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "NN", "VVFIN", "PPER", "$.", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Horch, sie kommen unsichtbar. \u2013", "tokens": ["Horch", ",", "sie", "kom\u00b7men", "un\u00b7sicht\u00b7bar", ".", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NE", "$,", "PPER", "VVFIN", "ADJD", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Diesen traurigen Ton, sang ihn ein Liebender,", "tokens": ["Die\u00b7sen", "trau\u00b7ri\u00b7gen", "Ton", ",", "sang", "ihn", "ein", "Lie\u00b7ben\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$,", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.3": {"text": "Der zum Tod in die Schlacht zog? \u2013", "tokens": ["Der", "zum", "Tod", "in", "die", "Schlacht", "zog", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "APPRART", "NN", "APPR", "ART", "NN", "VVFIN", "$.", "$("], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Jenen z\u00e4rteren, sanftern Laut,", "tokens": ["Je\u00b7nen", "z\u00e4r\u00b7te\u00b7ren", ",", "sanf\u00b7tern", "Laut", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVINF", "$,", "VVFIN", "APPR", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.3": {"line.1": {"text": "Diesen Seufzer verhauchte", "tokens": ["Die\u00b7sen", "Seuf\u00b7zer", "ver\u00b7hauch\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["PDAT", "NN", "VVFIN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Braut und Mutter? \u2013 Erklang diesen ein flehender", "tokens": ["Braut", "und", "Mut\u00b7ter", "?", "\u2013", "Er\u00b7klang", "die\u00b7sen", "ein", "fle\u00b7hen\u00b7der"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "$.", "$(", "NN", "PDAT", "ART", "NN"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Greis, der unter der Knechtschaft", "tokens": ["Greis", ",", "der", "un\u00b7ter", "der", "Knecht\u00b7schaft"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Harten Fessel daniedersank? \u2013", "tokens": ["Har\u00b7ten", "Fes\u00b7sel", "da\u00b7nie\u00b7der\u00b7sank", "?", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "$.", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.4": {"line.1": {"text": "S\u00fc\u00dfe T\u00f6ne beginnen.", "tokens": ["S\u00fc\u00b7\u00dfe", "T\u00f6\u00b7ne", "be\u00b7gin\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Seid Ihr Kindesgelall? oder der S\u00e4uglinge", "tokens": ["Seid", "Ihr", "Kin\u00b7des\u00b7ge\u00b7lall", "?", "o\u00b7der", "der", "S\u00e4ug\u00b7lin\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAIMP", "PPOSAT", "NN", "$.", "KON", "ART", "NN"], "meter": "+-+--+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Und des Knaben und M\u00e4dchens", "tokens": ["Und", "des", "Kna\u00b7ben", "und", "M\u00e4d\u00b7chens"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "KON", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Erste Freuden? O weilet, weilt! \u2013", "tokens": ["Ers\u00b7te", "Freu\u00b7den", "?", "O", "wei\u00b7let", ",", "weilt", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "punct"], "pos": ["ADJA", "NN", "$.", "NE", "VVFIN", "$,", "VVFIN", "$.", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.5": {"line.1": {"text": "Weilt auch Ihr, die Ihr wieder-", "tokens": ["Weilt", "auch", "Ihr", ",", "die", "Ihr", "wie\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PPER", "$,", "PRELS", "PPOSAT", "TRUNC"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Kehret, Seufzer des Manns, die Ihr den letzten Hauch", "tokens": ["Keh\u00b7ret", ",", "Seuf\u00b7zer", "des", "Manns", ",", "die", "Ihr", "den", "letz\u00b7ten", "Hauch"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "NN", "ART", "NN", "$,", "PRELS", "PPER", "ART", "ADJA", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Seines brechenden Herzens", "tokens": ["Sei\u00b7nes", "bre\u00b7chen\u00b7den", "Her\u00b7zens"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Einem f\u00fchlenden Weltgeist gabt!", "tokens": ["Ei\u00b7nem", "f\u00fch\u00b7len\u00b7den", "Welt\u00b7geist", "gabt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.6": {"line.1": {"text": "Horch! In tieferem Tone", "tokens": ["Horch", "!", "In", "tie\u00b7fe\u00b7rem", "To\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$.", "APPR", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Bebt die Saite; wer ist's? Eines Hermiten Ton,", "tokens": ["Bebt", "die", "Sai\u00b7te", ";", "wer", "ist's", "?", "Ei\u00b7nes", "Her\u00b7mi\u00b7ten", "Ton", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$.", "PWS", "VAFIN", "$.", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.3": {"text": "Der, ein heiliger Barde,", "tokens": ["Der", ",", "ein", "hei\u00b7li\u00b7ger", "Bar\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Sich beseufzt und das Vaterland.", "tokens": ["Sich", "be\u00b7seufzt", "und", "das", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "KON", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.7": {"line.1": {"text": "Horch! An Babylon's Weiden", "tokens": ["Horch", "!", "An", "Ba\u00b7by\u00b7lon's", "Wei\u00b7den"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$.", "APPR", "NE", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Klang die Harfe so dumpf, und so erhaben jetzt,", "tokens": ["Klang", "die", "Har\u00b7fe", "so", "dumpf", ",", "und", "so", "er\u00b7ha\u00b7ben", "jetzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "ADJD", "$,", "KON", "ADV", "ADJD", "ADV", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Da sie Freuden der Zukunft,", "tokens": ["Da", "sie", "Freu\u00b7den", "der", "Zu\u00b7kunft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "ART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Hell in T\u00f6nen, frohlockend singt.", "tokens": ["Hell", "in", "T\u00f6\u00b7nen", ",", "froh\u00b7lo\u00b7ckend", "singt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "$,", "ADJD", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.8": {"line.1": {"text": "Horch! So klinget die Harfe", "tokens": ["Horch", "!", "So", "klin\u00b7get", "die", "Har\u00b7fe"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$.", "ADV", "VVFIN", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Eines Engels im Chor himmlischer Geister, wenn", "tokens": ["Ei\u00b7nes", "En\u00b7gels", "im", "Chor", "himm\u00b7li\u00b7scher", "Geis\u00b7ter", ",", "wenn"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "APPRART", "NN", "ADJA", "NN", "$,", "KOUS"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.3": {"text": "Sich die l\u00f6sende Seele", "tokens": ["Sich", "die", "l\u00f6\u00b7sen\u00b7de", "See\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["PRF", "ART", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Sanft von Athem zu Athem hebt,", "tokens": ["Sanft", "von", "A\u00b7them", "zu", "A\u00b7them", "hebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.9": {"line.1": {"text": "Bis allm\u00e4chtig erklinget", "tokens": ["Bis", "all\u00b7m\u00e4ch\u00b7tig", "er\u00b7klin\u00b7get"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJD", "VVFIN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Aller Seligen Chor, aller Befreieten,", "tokens": ["Al\u00b7ler", "Se\u00b7li\u00b7gen", "Chor", ",", "al\u00b7ler", "Be\u00b7frei\u00b7e\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$,", "PIAT", "NN", "$,"], "meter": "--+--+---+--", "measure": "anapaest.di.plus"}, "line.3": {"text": "Die, der dr\u00fcckenden Bande", "tokens": ["Die", ",", "der", "dr\u00fc\u00b7cken\u00b7den", "Ban\u00b7de"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ART", "$,", "ART", "ADJA", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Los, beginnen den Weltaccord.", "tokens": ["Los", ",", "be\u00b7gin\u00b7nen", "den", "Wel\u00b7tac\u00b7cord", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.10": {"line.1": {"text": "Singt, Ihr Hauche des Weltalls,", "tokens": ["Singt", ",", "Ihr", "Hau\u00b7che", "des", "Welt\u00b7alls", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Wandernde Stimmen, singt Eure phantastischen", "tokens": ["Wan\u00b7dern\u00b7de", "Stim\u00b7men", ",", "singt", "Eu\u00b7re", "phan\u00b7tas\u00b7ti\u00b7schen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PPOSAT", "ADJA"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "T\u00f6ne, denen erwartend", "tokens": ["T\u00f6\u00b7ne", ",", "de\u00b7nen", "er\u00b7war\u00b7tend"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "PRELS", "VVPP"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Meine k\u00fcnstliche Leyer schweigt.", "tokens": ["Mei\u00b7ne", "k\u00fcnst\u00b7li\u00b7che", "Le\u00b7yer", "schweigt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}}}}