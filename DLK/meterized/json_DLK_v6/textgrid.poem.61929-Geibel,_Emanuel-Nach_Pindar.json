tetra"}, "line.3": {"text": "Angekr\u00e4chzt in hohlem Grimme", "tokens": ["An\u00b7ge\u00b7kr\u00e4chzt", "in", "hoh\u00b7lem", "Grim\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wider Zeus erlauchten Aar.", "tokens": ["Wi\u00b7der", "Zeus", "er\u00b7lauch\u00b7ten", "Aar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}