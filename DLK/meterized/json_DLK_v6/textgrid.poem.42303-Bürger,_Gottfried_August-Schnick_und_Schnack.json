{"textgrid.poem.42303": {"metadata": {"author": {"name": "B\u00fcrger, Gottfried August", "birth": "N.A.", "death": "N.A."}, "title": "Schnick und Schnack", "genre": "verse", "period": "N.A.", "pub_year": 1784, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verbreite du vor Hack und Mack", "tokens": ["Ver\u00b7brei\u00b7te", "du", "vor", "Hack", "und", "Mack"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Duft der besten Thaten!", "tokens": ["Den", "Duft", "der", "bes\u00b7ten", "Tha\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Kaum wird Frau Schnick und kaum Herr Schnack", "tokens": ["Kaum", "wird", "Frau", "Schnick", "und", "kaum", "Herr", "Schnack"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "NN", "NN", "KON", "ADV", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihn merken und verraten.", "tokens": ["Ihn", "mer\u00b7ken", "und", "ver\u00b7ra\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "KON", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Mach' aber Einen schwachen Streich \u2013", "tokens": ["Mach'", "a\u00b7ber", "Ei\u00b7nen", "schwa\u00b7chen", "Streich", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wer kann dem immer wehren? \u2013", "tokens": ["Wer", "kann", "dem", "im\u00b7mer", "weh\u00b7ren", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VMFIN", "ART", "ADV", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ganz heimlich! \u2013 O so wirst du gleich", "tokens": ["Ganz", "heim\u00b7lich", "!", "\u2013", "O", "so", "wirst", "du", "gleich"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "$.", "$(", "NE", "ADV", "VAFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dein blaues Wunder h\u00f6ren!", "tokens": ["Dein", "blau\u00b7es", "Wun\u00b7der", "h\u00f6\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Umsonst, umsonst bem\u00fchst du dich,", "tokens": ["Um\u00b7sonst", ",", "um\u00b7sonst", "be\u00b7m\u00fchst", "du", "dich", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VVFIN", "PPER", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihn halb nur zu verstecken.", "tokens": ["Ihn", "halb", "nur", "zu", "ver\u00b7ste\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Vom Liebesmantel findet sich", "tokens": ["Vom", "Lie\u00b7bes\u00b7man\u00b7tel", "fin\u00b7det", "sich"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kein L\u00e4ppchen, ihn zu decken.", "tokens": ["Kein", "L\u00e4pp\u00b7chen", ",", "ihn", "zu", "de\u00b7cken", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Beging'st du ihn im Keller gleich,", "tokens": ["Be\u00b7ging'st", "du", "ihn", "im", "Kel\u00b7ler", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "APPRART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Tief in der Nacht der Erde:", "tokens": ["Tief", "in", "der", "Nacht", "der", "Er\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "++-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Hervor mu\u00df er, der matte Streich,", "tokens": ["Her\u00b7vor", "mu\u00df", "er", ",", "der", "mat\u00b7te", "Streich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.4": {"text": "Da\u00df er beschnickschnackt werde!", "tokens": ["Da\u00df", "er", "be\u00b7schnick\u00b7schnackt", "wer\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Du fragst umsonst: Wie hat das Pack", "tokens": ["Du", "fragst", "um\u00b7sonst", ":", "Wie", "hat", "das", "Pack"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$.", "PWAV", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das bi\u00dfchen Streich erfahren? \u2013", "tokens": ["Das", "bi\u00df\u00b7chen", "Streich", "er\u00b7fah\u00b7ren", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Auch Klag' und Fluch auf Schnick und Schnack", "tokens": ["Auch", "Klag'", "und", "Fluch", "auf", "Schnick", "und", "Schnack"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "KON", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kannst du gem\u00e4chlich sparen.", "tokens": ["Kannst", "du", "ge\u00b7m\u00e4ch\u00b7lich", "spa\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Sie borgen dann die List vom Fuchs;", "tokens": ["Sie", "bor\u00b7gen", "dann", "die", "List", "vom", "Fuchs", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "APPRART", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vom Sp\u00fcrhund ihre Nasen;", "tokens": ["Vom", "Sp\u00fcr\u00b7hund", "ih\u00b7re", "Na\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die gluhen Augen von dem Luchs;", "tokens": ["Die", "glu\u00b7hen", "Au\u00b7gen", "von", "dem", "Luchs", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Ohren von dem Hasen.", "tokens": ["Die", "Oh\u00b7ren", "von", "dem", "Ha\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Und sp\u00fcren und verschonen nie,", "tokens": ["Und", "sp\u00fc\u00b7ren", "und", "ver\u00b7scho\u00b7nen", "nie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "KON", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht Bruder, Schwester, Base.", "tokens": ["Nicht", "Bru\u00b7der", ",", "Schwes\u00b7ter", ",", "Ba\u00b7se", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PTKNEG", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wie Galgenraben schw\u00e4rmen sie", "tokens": ["Wie", "Gal\u00b7gen\u00b7ra\u00b7ben", "schw\u00e4r\u00b7men", "sie"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Am liebsten nach dem Aase.", "tokens": ["Am", "liebs\u00b7ten", "nach", "dem", "Aa\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}