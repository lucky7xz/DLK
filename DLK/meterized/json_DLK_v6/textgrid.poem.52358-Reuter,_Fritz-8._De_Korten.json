{"textgrid.poem.52358": {"metadata": {"author": {"name": "Reuter, Fritz", "birth": "N.A.", "death": "N.A."}, "title": "8. De Korten", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Baron von Sprudelwitz, de b\u00e4ngt", "tokens": ["Ba\u00b7ron", "von", "Spru\u00b7del\u00b7witz", ",", "de", "b\u00e4ngt"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "APPR", "NE", "$,", "NE", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sick sine gelen Hanschen an,", "tokens": ["Sick", "si\u00b7ne", "ge\u00b7len", "Han\u00b7schen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Un achter em steiht sin Jehann,", "tokens": ["Un", "ach\u00b7ter", "em", "steiht", "sin", "Je\u00b7hann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dat hei den Pelz em \u00e4wer h\u00e4ngt.", "tokens": ["Dat", "hei", "den", "Pelz", "em", "\u00e4\u00b7wer", "h\u00e4ngt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "\u00bbabscheulich\u00ab, seggt de Herr Baron tau sick,", "tokens": ["\u00bb", "ab\u00b7scheu\u00b7lich", "\u00ab", ",", "seggt", "de", "Herr", "Ba\u00b7ron", "tau", "sick", ","], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$(", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.6": {"text": "\u00bbabscheulich heut pressiert, auf gage!", "tokens": ["\u00bb", "ab\u00b7scheu\u00b7lich", "heut", "pres\u00b7siert", ",", "auf", "ga\u00b7ge", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ADJD", "ADV", "VVFIN", "$,", "APPR", "NN", "$."], "meter": "+---+-+-+", "measure": "dactylic.init"}, "line.7": {"text": "Noblesse oblige! \u2013 Wie f\u00fcrchterlich", "tokens": ["Nob\u00b7les\u00b7se", "ob\u00b7li\u00b7ge", "!", "\u2013", "Wie", "f\u00fcrch\u00b7ter\u00b7lich"], "token_info": ["word", "word", "punct", "punct", "word", "word"], "pos": ["NN", "ADJA", "$.", "$(", "PWAV", "ADJD"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.8": {"text": "Setzt heut mich dies Wort en rage!", "tokens": ["Setzt", "heut", "mich", "dies", "Wort", "en", "ra\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "PDS", "NN", "NE", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Ja, w\u00fcnschte, wahrlich, ja! auf taille,", "tokens": ["Ja", ",", "w\u00fcnschte", ",", "wahr\u00b7lich", ",", "ja", "!", "auf", "tail\u00b7le", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "$,", "ADV", "$,", "ADV", "$.", "APPR", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Ich h\u00f6rte heute zur canaille", "tokens": ["Ich", "h\u00f6r\u00b7te", "heu\u00b7te", "zur", "ca\u00b7nail\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Und braucht Visiten nicht zu schneiden", "tokens": ["Und", "braucht", "Vi\u00b7si\u00b7ten", "nicht", "zu", "schnei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "NN", "PTKNEG", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Und k\u00f6nnt' Pepita sehn, superbe!", "tokens": ["Und", "k\u00f6nnt'", "Pe\u00b7pi\u00b7ta", "sehn", ",", "su\u00b7per\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VMFIN", "NE", "VVINF", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Oh, grausam Schicksal, bitter, herbe,", "tokens": ["Oh", ",", "grau\u00b7sam", "Schick\u00b7sal", ",", "bit\u00b7ter", ",", "her\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ITJ", "$,", "ADJD", "NN", "$,", "ADJD", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Wie h\u00e4ufst du auf mein armes Haupt die Leiden!", "tokens": ["Wie", "h\u00e4ufst", "du", "auf", "mein", "ar\u00b7mes", "Haupt", "die", "Lei\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Doch halt mal, ja! So k\u00f6nnt es passen \u2013", "tokens": ["Doch", "halt", "mal", ",", "ja", "!", "So", "k\u00f6nnt", "es", "pas\u00b7sen", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$,", "ADV", "$.", "ADV", "VVFIN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Die Karten blo\u00df abgeben lassen", "tokens": ["Die", "Kar\u00b7ten", "blo\u00df", "ab\u00b7ge\u00b7ben", "las\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVINF", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.17": {"text": "An jeder T\u00fcr, und dann, und dann ...!\u00ab", "tokens": ["An", "je\u00b7der", "T\u00fcr", ",", "und", "dann", ",", "und", "dann", "...", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "KON", "ADV", "$,", "KON", "ADV", "$(", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Un dreiht sick \u00fcm un seggt: \u00bbJohann,", "tokens": ["Un", "dreiht", "sick", "\u00fcm", "un", "seggt", ":", "\u00bb", "Jo\u00b7hann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "VVFIN", "$.", "$(", "NE", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.19": {"text": "Vorfahren! Christoph, der soll warten!", "tokens": ["Vor\u00b7fah\u00b7ren", "!", "Chris\u00b7toph", ",", "der", "soll", "war\u00b7ten", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NE", "$,", "PRELS", "VMFIN", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.20": {"text": "Und du die Karten, wei\u00dft du? \u2013 Karten!\u00ab", "tokens": ["Und", "du", "die", "Kar\u00b7ten", ",", "wei\u00dft", "du", "?", "\u2013", "Kar\u00b7ten", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct", "word", "punct", "punct"], "pos": ["KON", "PPER", "ART", "NN", "$,", "VVFIN", "PPER", "$.", "$(", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "\u00bbja\u00ab, seggt Jehann, \u00bbde weit ick, Herr Baron,", "tokens": ["\u00bb", "ja", "\u00ab", ",", "seggt", "Je\u00b7hann", ",", "\u00bb", "de", "weit", "ick", ",", "Herr", "Ba\u00b7ron", ","], "token_info": ["punct", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PTKANT", "$(", "$,", "VVFIN", "NE", "$,", "$(", "NE", "ADJD", "PPER", "$,", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Sei liggen linkschen in dat Schapp.\u00ab", "tokens": ["Sei", "lig\u00b7gen", "link\u00b7schen", "in", "dat", "Schapp", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAIMP", "ADJD", "ADJA", "APPR", "ART", "NN", "$.", "$("], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.23": {"text": "\u00bbah, bon! Da ist ja Christoph schon!", "tokens": ["\u00bb", "ah", ",", "bon", "!", "Da", "ist", "ja", "Chris\u00b7toph", "schon", "!"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$,", "ADV", "$.", "ADV", "VAFIN", "ADV", "NE", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Nun, Christoph, immer schlanken Trab!", "tokens": ["Nun", ",", "Chris\u00b7toph", ",", "im\u00b7mer", "schlan\u00b7ken", "Trab", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NE", "$,", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Und du, Johann, nun rasch hinab!", "tokens": ["Und", "du", ",", "Jo\u00b7hann", ",", "nun", "rasch", "hin\u00b7ab", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "NE", "$,", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Nicht lange fragen, ob zu Haus!", "tokens": ["Nicht", "lan\u00b7ge", "fra\u00b7gen", ",", "ob", "zu", "Haus", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVINF", "$,", "KOUS", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Gleich wieder 'raus;", "tokens": ["Gleich", "wie\u00b7der", "'raus", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NE", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.28": {"text": "Gibst immer blo\u00df 'ne Karte ab!\u00ab", "tokens": ["Gibst", "im\u00b7mer", "blo\u00df", "'ne", "Kar\u00b7te", "ab", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADV", "ADV", "ART", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "So jagen sei nu d\u00f6rch Swerin", "tokens": ["So", "ja\u00b7gen", "sei", "nu", "d\u00f6rch", "Swe\u00b7rin"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVINF", "VAFIN", "ADV", "PAV", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Un ward'n dor bald mit farig sin.", "tokens": ["Un", "ward'n", "dor", "bald", "mit", "fa\u00b7rig", "sin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "ADV", "APPR", "ADJD", "VAINF", "$."], "meter": "+----+-+", "measure": "dactylic.init"}, "line.31": {"text": "Jehann sitt \u00fcmmer up den Schupp,", "tokens": ["Je\u00b7hann", "sitt", "\u00fcm\u00b7mer", "up", "den", "Schupp", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Nu rasch herun, nu fix herup!", "tokens": ["Nu", "rasch", "he\u00b7run", ",", "nu", "fix", "he\u00b7rup", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVINF", "$,", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "Na, ", "tokens": ["Na", ","], "token_info": ["word", "punct"], "pos": ["ITJ", "$,"], "meter": "-", "measure": "single.down"}, "line.34": {"text": "Denn s\u00fcnd sei d\u00f6rch mit ehre Turen.", "tokens": ["Denn", "s\u00fcnd", "sei", "d\u00f6rch", "mit", "eh\u00b7re", "Tu\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PAV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.35": {"text": "De Herr Baron, de fr\u00f6ggt: \u00bbJohann!", "tokens": ["De", "Herr", "Ba\u00b7ron", ",", "de", "fr\u00f6ggt", ":", "\u00bb", "Jo\u00b7hann", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct", "word", "punct"], "pos": ["NE", "NN", "NN", "$,", "NE", "VVFIN", "$.", "$(", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Du hast doch wohl 'ne Karte noch?\u00ab", "tokens": ["Du", "hast", "doch", "wohl", "'ne", "Kar\u00b7te", "noch", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ART", "NN", "ADV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "\u00bbja\u00ab, seggt Jehann, \u00bbHerr, Rutenburen.\u00ab", "tokens": ["\u00bb", "ja", "\u00ab", ",", "seggt", "Je\u00b7hann", ",", "\u00bb", "Herr", ",", "Ru\u00b7ten\u00b7bu\u00b7ren", ".", "\u00ab"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["$(", "PTKANT", "$(", "$,", "VVFIN", "NE", "$,", "$(", "NN", "$,", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}