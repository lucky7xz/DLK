{"dta.poem.8220": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Das M\u00e4dchen mit den hellen Augen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1852", "urn": "urn:nbn:de:kobv:b4-200905196098", "language": ["de:0.85", "af:0.14"], "booktitle": "Storm, Theodor: Gedichte. Kiel, 1852."}, "poem": {"stanza.1": {"line.1": {"text": "Die wollte Keines Liebste sein;", "tokens": ["Die", "woll\u00b7te", "Kei\u00b7nes", "Liebs\u00b7te", "sein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PIAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie sprang und lie\u00df die Z\u00f6pfe fliegen,", "tokens": ["Sie", "sprang", "und", "lie\u00df", "die", "Z\u00f6p\u00b7fe", "flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Freier schauten hinterdrein.", "tokens": ["Die", "Frei\u00b7er", "schau\u00b7ten", "hin\u00b7ter\u00b7drein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Freier standen ganz von Ferne", "tokens": ["Die", "Frei\u00b7er", "stan\u00b7den", "ganz", "von", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In blanken R\u00f6cklein lobesam.", "tokens": ["In", "blan\u00b7ken", "R\u00f6ck\u00b7lein", "lo\u00b7be\u00b7sam", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u201efrau Mutter, ach, so sprecht ein W\u00f6rtchen,", "tokens": ["\u201e", "frau", "Mut\u00b7ter", ",", "ach", ",", "so", "sprecht", "ein", "W\u00f6rt\u00b7chen", ","], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "NN", "$,", "ITJ", "$,", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und macht das liebe Kindlein zahm!\u201c", "tokens": ["Und", "macht", "das", "lie\u00b7be", "Kin\u00b7dlein", "zahm", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die Mutter schlug die H\u00e4nd' zusammen,", "tokens": ["Die", "Mut\u00b7ter", "schlug", "die", "H\u00e4nd'", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Mutter rief: \u201eDu th\u00f6richt Kind,", "tokens": ["Die", "Mut\u00b7ter", "rief", ":", "\u201e", "Du", "th\u00f6\u00b7richt", "Kind", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "PPER", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Greif zu, greif zu! Die Jahre kommen,", "tokens": ["Greif", "zu", ",", "greif", "zu", "!", "Die", "Jah\u00b7re", "kom\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "ADJD", "PTKVZ", "$.", "ART", "NN", "VVINF", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Die Freier gehen gar geschwind!\u201c", "tokens": ["Die", "Frei\u00b7er", "ge\u00b7hen", "gar", "ge\u00b7schwind", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Sie aber lie\u00df die Z\u00f6pfe fliegen,", "tokens": ["Sie", "a\u00b7ber", "lie\u00df", "die", "Z\u00f6p\u00b7fe", "flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und lachte alle Weisheit aus;", "tokens": ["Und", "lach\u00b7te", "al\u00b7le", "Weis\u00b7heit", "aus", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da sprang durch die erschrockenen Freier", "tokens": ["Da", "sprang", "durch", "die", "er\u00b7schro\u00b7cke\u00b7nen", "Frei\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ein toller Knabe in das Haus.", "tokens": ["Ein", "tol\u00b7ler", "Kna\u00b7be", "in", "das", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und wie sie bog das wilde K\u00f6pfchen,", "tokens": ["Und", "wie", "sie", "bog", "das", "wil\u00b7de", "K\u00f6pf\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und wie ihr F\u00fc\u00dfchen schlug den Grund,", "tokens": ["Und", "wie", "ihr", "F\u00fc\u00df\u00b7chen", "schlug", "den", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er schlo\u00df sie fest in seine Arme", "tokens": ["Er", "schlo\u00df", "sie", "fest", "in", "sei\u00b7ne", "Ar\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und k\u00fc\u00dfte ihren rothen Mund.", "tokens": ["Und", "k\u00fc\u00df\u00b7te", "ih\u00b7ren", "ro\u00b7then", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Die Freier standen ganz von Ferne,", "tokens": ["Die", "Frei\u00b7er", "stan\u00b7den", "ganz", "von", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Mutter rief vor Staunen schier:", "tokens": ["Die", "Mut\u00b7ter", "rief", "vor", "Stau\u00b7nen", "schier", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u201egott sch\u00fctz' dich vor dem ungeschlachten,", "tokens": ["\u201e", "gott", "sch\u00fctz'", "dich", "vor", "dem", "un\u00b7ge\u00b7schlach\u00b7ten", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "VVFIN", "PRF", "APPR", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ohn' Maa\u00dfen groben Cavalier!\u201c", "tokens": ["Ohn'", "Maa\u00b7\u00dfen", "gro\u00b7ben", "Ca\u00b7va\u00b7lier", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}