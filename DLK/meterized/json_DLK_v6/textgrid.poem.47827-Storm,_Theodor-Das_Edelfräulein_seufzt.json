{"textgrid.poem.47827": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Das Edelfr\u00e4ulein seufzt", "genre": "verse", "period": "N.A.", "pub_year": 1863, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist wohl wahr,", "tokens": ["Es", "ist", "wohl", "wahr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Die Menschen stammen von einem Paar!", "tokens": ["Die", "Men\u00b7schen", "stam\u00b7men", "von", "ei\u00b7nem", "Paar", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Der doppelte Adam, so s\u00fc\u00df er w\u00e4re,", "tokens": ["Der", "dop\u00b7pel\u00b7te", "A\u00b7dam", ",", "so", "s\u00fc\u00df", "er", "w\u00e4\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "ADJD", "PPER", "VAFIN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Ich halte ihn dennoch f\u00fcr eine Schim\u00e4re!", "tokens": ["Ich", "hal\u00b7te", "ihn", "den\u00b7noch", "f\u00fcr", "ei\u00b7ne", "Schi\u00b7m\u00e4\u00b7re", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}}}}}