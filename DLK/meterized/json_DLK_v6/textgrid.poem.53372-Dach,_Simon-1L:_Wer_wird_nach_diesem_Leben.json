{"textgrid.poem.53372": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wer wird nach diesem Leben", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer wird nach diesem Leben", "tokens": ["Wer", "wird", "nach", "die\u00b7sem", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In Glantz und Herrlicheit", "tokens": ["In", "Glantz", "und", "Herr\u00b7li\u00b7cheit"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bey Gott ohn Ende schweben", "tokens": ["Bey", "Gott", "ohn", "En\u00b7de", "schwe\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In Christo hocherfrewt,", "tokens": ["In", "Chris\u00b7to", "ho\u00b7cher\u00b7frewt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wer wird in Gottes Hand", "tokens": ["Wer", "wird", "in", "Got\u00b7tes", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "APPR", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Der Gn\u00fcge f\u00e4hig werden,", "tokens": ["Der", "Gn\u00fc\u00b7ge", "f\u00e4\u00b7hig", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Die noch kein Sinn auff Erden", "tokens": ["Die", "noch", "kein", "Sinn", "auff", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "PIAT", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Kein Ohr noch Aug erkant?", "tokens": ["Kein", "Ohr", "noch", "Aug", "er\u00b7kant", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der, weil er hie mu\u00df wallen", "tokens": ["Der", ",", "weil", "er", "hie", "mu\u00df", "wal\u00b7len"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "$,", "KOUS", "PPER", "ADV", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zugleich in Lieb und Pein", "tokens": ["Zu\u00b7gleich", "in", "Lieb", "und", "Pein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "GoTT suchet zu gefallen", "tokens": ["GoTT", "su\u00b7chet", "zu", "ge\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vnd dienen ihm allein,", "tokens": ["Vnd", "die\u00b7nen", "ihm", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sich ihm nur schuldig h\u00e4lt", "tokens": ["Sich", "ihm", "nur", "schul\u00b7dig", "h\u00e4lt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PRF", "PPER", "ADV", "ADJD", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Mit Gutt Leib und Gem\u00fcte,", "tokens": ["Mit", "Gutt", "Leib", "und", "Ge\u00b7m\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "NN", "KON", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.7": {"text": "Als der durch seine G\u00fcte", "tokens": ["Als", "der", "durch", "sei\u00b7ne", "G\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Gebeut die gantze Welt.", "tokens": ["Ge\u00b7beut", "die", "gant\u00b7ze", "Welt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der seinen Neben-Christen", "tokens": ["Der", "sei\u00b7nen", "Ne\u00b7ben\u00b7Chris\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ART", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So wie sich selber liebt", "tokens": ["So", "wie", "sich", "sel\u00b7ber", "liebt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "PRF", "ADV", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und niemals ihn mit Listen", "tokens": ["Und", "nie\u00b7mals", "ihn", "mit", "Lis\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PPER", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Noch offentlich betr\u00fcbt,", "tokens": ["Noch", "of\u00b7fent\u00b7lich", "be\u00b7tr\u00fcbt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dem Hochmuth widerstrebt,", "tokens": ["Dem", "Hoch\u00b7muth", "wi\u00b7der\u00b7strebt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Das Vnrecht nicht kan leiden", "tokens": ["Das", "Vn\u00b7recht", "nicht", "kan", "lei\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PTKNEG", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Sucht Geitz und Zorn zu meiden", "tokens": ["Sucht", "Geitz", "und", "Zorn", "zu", "mei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "NN", "KON", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Auch keusch und heilig lebt.", "tokens": ["Auch", "keusch", "und", "hei\u00b7lig", "lebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Vnd ist es wo versehen", "tokens": ["Vnd", "ist", "es", "wo", "ver\u00b7se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "PWAV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sein Hertz ihm wird beschwert", "tokens": ["Sein", "Hertz", "ihm", "wird", "be\u00b7schwert"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PPER", "VAFIN", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sich stracks mit Buss' und Flehen", "tokens": ["Sich", "stracks", "mit", "Buss'", "und", "Fle\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PRF", "ADV", "APPR", "NE", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu seinem Gott bekehrt,", "tokens": ["Zu", "sei\u00b7nem", "Gott", "be\u00b7kehrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ihm hei\u00dfe Thr\u00e4nen schenckt,", "tokens": ["Ihm", "hei\u00b7\u00dfe", "Thr\u00e4\u00b7nen", "schenckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Die allzeit ihm gefallen", "tokens": ["Die", "all\u00b7zeit", "ihm", "ge\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADV", "PPER", "VVPP"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.7": {"text": "F\u00fcr andern Opffern allen,", "tokens": ["F\u00fcr", "an\u00b7dern", "Opf\u00b7fern", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PIAT", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Da\u00df er sich zu uns lenckt.", "tokens": ["Da\u00df", "er", "sich", "zu", "uns", "lenckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Vorau\u00df der auff sein Ende", "tokens": ["Vor\u00b7au\u00df", "der", "auff", "sein", "En\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In allen Dingen sieht,", "tokens": ["In", "al\u00b7len", "Din\u00b7gen", "sieht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vnd, wenn das kompt, behende", "tokens": ["Vnd", ",", "wenn", "das", "kompt", ",", "be\u00b7hen\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KON", "$,", "KOUS", "PDS", "VVFIN", "$,", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sich hin zu Christo zieht,", "tokens": ["Sich", "hin", "zu", "Chris\u00b7to", "zieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "APPR", "NE", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dem rechten Gnaden-Thron", "tokens": ["Dem", "rech\u00b7ten", "Gna\u00b7den\u00b7Thron"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Und wird in seinen Wunden", "tokens": ["Und", "wird", "in", "sei\u00b7nen", "Wun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Der Leibes-Last entbunden", "tokens": ["Der", "Lei\u00b7bes\u00b7Last", "ent\u00b7bun\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und stirbt wie Simeon.", "tokens": ["Und", "stirbt", "wie", "Si\u00b7me\u00b7on", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOKOM", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Der wird nach diesem Leben", "tokens": ["Der", "wird", "nach", "die\u00b7sem", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In Glantz und Herrlicheit", "tokens": ["In", "Glantz", "und", "Herr\u00b7li\u00b7cheit"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bey Gott ohn Ende schweben", "tokens": ["Bey", "Gott", "ohn", "En\u00b7de", "schwe\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In Christo hocherfrewt,", "tokens": ["In", "Chris\u00b7to", "ho\u00b7cher\u00b7frewt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der wird in Gottes Hand", "tokens": ["Der", "wird", "in", "Got\u00b7tes", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "APPR", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Der Gn\u00fcge f\u00e4hig werden", "tokens": ["Der", "Gn\u00fc\u00b7ge", "f\u00e4\u00b7hig", "wer\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "VAINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Die noch kein Sinn auff Erden", "tokens": ["Die", "noch", "kein", "Sinn", "auff", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "PIAT", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Kein Ohr noch Aug erkant.", "tokens": ["Kein", "Ohr", "noch", "Aug", "er\u00b7kant", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}