{"dta.poem.11013": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Gr\u00f6\u00dfe eines Punkts.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie viel an einem einzgen Punkt, w\u00e4r er auch noch so\nklein, gelegen:", "tokens": ["Wie", "viel", "an", "ei\u00b7nem", "einz\u00b7gen", "Punkt", ",", "w\u00e4r", "er", "auch", "noch", "so", "klein", ",", "ge\u00b7le\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "ADV", "APPR", "ART", "ADJA", "NN", "$,", "VAFIN", "PPER", "ADV", "ADV", "ADV", "ADJD", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.2": {"text": "Giebt die Geometrie uns deutlich, wenn man drauf achtet, zu", "tokens": ["Giebt", "die", "Geo\u00b7me\u00b7trie", "uns", "deut\u00b7lich", ",", "wenn", "man", "drauf", "ach\u00b7tet", ",", "zu"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "ART", "NN", "PPER", "ADJD", "$,", "KOUS", "PIS", "PAV", "VVFIN", "$,", "APPR"], "meter": "+-+-+-+-++-+-+", "measure": "unknown.measure.octa.plus"}, "line.3": {"text": "Da nemlich, wenn zwo Linien aus ihm gezogen sind und fliessen,", "tokens": ["Da", "nem\u00b7lich", ",", "wenn", "zwo", "Li\u00b7ni\u00b7en", "aus", "ihm", "ge\u00b7zo\u00b7gen", "sind", "und", "flies\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUS", "CARD", "NN", "APPR", "PPER", "VVPP", "VAFIN", "KON", "VVINF", "$,"], "meter": "-----+--+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Wie oder (wie mans rechnet) sich, nach ihm gezogen., in ihm", "tokens": ["Wie", "o\u00b7der", "(", "wie", "mans", "rech\u00b7net", ")", "sich", ",", "nach", "ihm", "ge\u00b7zo\u00b7gen", ".", ",", "in", "ihm"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["PWAV", "KON", "$(", "PWAV", "PIS", "VVFIN", "$(", "PRF", "$,", "APPR", "PPER", "VVPP", "$.", "$,", "APPR", "PPER"], "meter": "-+-+-+--+--+--+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Die Winkel, die der Punkt regiert, den Inhalt ungeheurer", "tokens": ["Die", "Win\u00b7kel", ",", "die", "der", "Punkt", "re\u00b7giert", ",", "den", "In\u00b7halt", "un\u00b7ge\u00b7heu\u00b7rer"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,", "ART", "NN", "ADJA"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "Die alle Zahlen \u00fcbersteigen, durch einen Theil vom Zirkel,", "tokens": ["Die", "al\u00b7le", "Zah\u00b7len", "\u00fc\u00b7bers\u00b7tei\u00b7gen", ",", "durch", "ei\u00b7nen", "Theil", "vom", "Zir\u00b7kel", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVPP", "$,", "APPR", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.7": {"text": "Ich habe dieses oft bewundert; und kam ein solches Cen-", "tokens": ["Ich", "ha\u00b7be", "die\u00b7ses", "oft", "be\u00b7wun\u00b7dert", ";", "und", "kam", "ein", "sol\u00b7ches", "Cen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PDAT", "ADV", "VVPP", "$.", "KON", "VVFIN", "ART", "PIAT", "TRUNC"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.8": {"text": "Als wie ein Schattenbild der Gottheit, aus welchem alle Din-", "tokens": ["Als", "wie", "ein", "Schat\u00b7ten\u00b7bild", "der", "Got\u00b7theit", ",", "aus", "wel\u00b7chem", "al\u00b7le", "Din"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "KOKOM", "ART", "NN", "ART", "NN", "$,", "APPR", "PRELS", "PIAT", "TRUNC"], "meter": "-+-+-+--+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.9": {"text": "In welchem alle Dinge wieder, wie hier, die Linien zusammen,", "tokens": ["In", "wel\u00b7chem", "al\u00b7le", "Din\u00b7ge", "wie\u00b7der", ",", "wie", "hier", ",", "die", "Li\u00b7ni\u00b7en", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "PIAT", "NN", "ADV", "$,", "PWAV", "ADV", "$,", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+--+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.10": {"text": "Als ihrem ersten Ursprung laufen, auch wieder sich vereinen, f\u00fcr,", "tokens": ["Als", "ih\u00b7rem", "ers\u00b7ten", "Ur\u00b7sprung", "lau\u00b7fen", ",", "auch", "wie\u00b7der", "sich", "ver\u00b7ei\u00b7nen", ",", "f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "VVINF", "$,", "ADV", "ADV", "PRF", "VVINF", "$,", "APPR", "$,"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.11": {"text": "Nur mit dem Unterscheid allein,", "tokens": ["Nur", "mit", "dem", "Un\u00b7ter\u00b7scheid", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADV", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.12": {"text": "Da\u00df so, wie dieser Mittelpunkt, ein Punkt im Zirkel, der so", "tokens": ["Da\u00df", "so", ",", "wie", "die\u00b7ser", "Mit\u00b7tel\u00b7punkt", ",", "ein", "Punkt", "im", "Zir\u00b7kel", ",", "der", "so"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "ADV", "$,", "PWAV", "PDAT", "NN", "$,", "ART", "NN", "APPRART", "NN", "$,", "PRELS", "ADV"], "meter": "-+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.13": {"text": "Der Gottheit Wesen solch ein Cirkel, wovon in allen Abgrund", "tokens": ["Der", "Got\u00b7theit", "We\u00b7sen", "solch", "ein", "Cir\u00b7kel", ",", "wo\u00b7von", "in", "al\u00b7len", "Ab\u00b7grund"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "PIAT", "ART", "NN", "$,", "PWAV", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.14": {"text": "Der unerforschten Ewigkeit, die Grenz-und Schranken nicht", "tokens": ["Der", "un\u00b7er\u00b7forschten", "E\u00b7wig\u00b7keit", ",", "die", "Grenz\u00b7\u00b7und", "Schran\u00b7ken", "nicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "NN", "PTKNEG"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.15": {"text": "Den Stralen, die unendlich, f\u00fcllen, den kein erschaffner Geist", "tokens": ["Den", "Stra\u00b7len", ",", "die", "un\u00b7end\u00b7lich", ",", "f\u00fcl\u00b7len", ",", "den", "kein", "er\u00b7schaff\u00b7ner", "Geist"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "$,", "VVFIN", "$,", "PRELS", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.16": {"text": "Wovon das Centrum allenthalben, der Umkreis aber nirgend ist.", "tokens": ["Wo\u00b7von", "das", "Cen\u00b7trum", "al\u00b7len\u00b7thal\u00b7ben", ",", "der", "Um\u00b7kreis", "a\u00b7ber", "nir\u00b7gend", "ist", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "$,", "ART", "NN", "ADV", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}}}}}