{"textgrid.poem.42373": {"metadata": {"author": {"name": "Lachmann, Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Seit du zerflossen bist in Dunst und Geist,", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seit du zerflossen bist in Dunst und Geist,", "tokens": ["Seit", "du", "zer\u00b7flos\u00b7sen", "bist", "in", "Dunst", "und", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Verf\u00e4ngt sich liebend und wie nie vertraut", "tokens": ["Ver\u00b7f\u00e4ngt", "sich", "lie\u00b7bend", "und", "wie", "nie", "ver\u00b7traut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ADJD", "KON", "PWAV", "ADV", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mein Blick im Raum, wenn das Gew\u00f6lk zerreisst", "tokens": ["Mein", "Blick", "im", "Raum", ",", "wenn", "das", "Ge\u00b7w\u00f6lk", "zer\u00b7reisst"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "$,", "KOUS", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und in den \u00c4ther seine Pforten baut.", "tokens": ["Und", "in", "den", "\u00c4\u00b7ther", "sei\u00b7ne", "Pfor\u00b7ten", "baut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da, in dem tief gegrabenen Gefild", "tokens": ["Da", ",", "in", "dem", "tief", "ge\u00b7gra\u00b7be\u00b7nen", "Ge\u00b7fild"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "APPR", "ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Des klarsten, wolkenlosen Flecks Azur", "tokens": ["Des", "klars\u00b7ten", ",", "wol\u00b7ken\u00b7lo\u00b7sen", "Flecks", "A\u00b7zur"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "NN"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Seh ich dich ohne Umriss, ohne Bild,", "tokens": ["Seh", "ich", "dich", "oh\u00b7ne", "Um\u00b7riss", ",", "oh\u00b7ne", "Bild", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "NN", "$,", "KOUI", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Als Schein und Schatten, Hauch und Seelenspur.", "tokens": ["Als", "Schein", "und", "Schat\u00b7ten", ",", "Hauch", "und", "See\u00b7len\u00b7spur", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Um einen Schimmer, der als Abglanz blieb,", "tokens": ["Um", "ei\u00b7nen", "Schim\u00b7mer", ",", "der", "als", "Ab\u00b7glanz", "blieb", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "$,", "PRELS", "KOUS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Kreist aller Traum. An jede Schwungkraft schliesst", "tokens": ["Kreist", "al\u00b7ler", "Traum", ".", "An", "je\u00b7de", "Schwung\u00b7kraft", "schliesst"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PIAT", "NN", "$.", "APPR", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sich unsrer Seele namenloser Trieb,", "tokens": ["Sich", "uns\u00b7rer", "See\u00b7le", "na\u00b7men\u00b7lo\u00b7ser", "Trieb", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Bis er mit einem Punkt des Alls verfliesst.", "tokens": ["Bis", "er", "mit", "ei\u00b7nem", "Punkt", "des", "Alls", "ver\u00b7fliesst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}