{"dta.poem.15991": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "45.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.85", "af:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du sagst: \u201edie Tugend darbt, indem das Laster prasset.\u201c", "tokens": ["Du", "sagst", ":", "\u201e", "die", "Tu\u00b7gend", "darbt", ",", "in\u00b7dem", "das", "Las\u00b7ter", "pras\u00b7set", ".", "\u201c"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "ART", "NN", "VVFIN", "$,", "KOUS", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hast du der Tugend Werth so niedrig aufgefasset?", "tokens": ["Hast", "du", "der", "Tu\u00b7gend", "Werth", "so", "nied\u00b7rig", "auf\u00b7ge\u00b7fas\u00b7set", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "NN", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ist Ueberflu\u00df ihr Lohn? der Lohn ist \u00fcberfl\u00fcssig.", "tokens": ["Ist", "Ue\u00b7berf\u00b7lu\u00df", "ihr", "Lohn", "?", "der", "Lohn", "ist", "\u00fc\u00b7berf\u00b7l\u00fcs\u00b7sig", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PPOSAT", "NN", "$.", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Tugend aber darbt mit Recht, wenn sie ist m\u00fc\u00dfig.", "tokens": ["Die", "Tu\u00b7gend", "a\u00b7ber", "darbt", "mit", "Recht", ",", "wenn", "sie", "ist", "m\u00fc\u00b7\u00dfig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "APPR", "NN", "$,", "KOUS", "PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Den Lohn der Arbeit, Brot, verdient der B\u00f6sewicht,", "tokens": ["Den", "Lohn", "der", "Ar\u00b7beit", ",", "Brot", ",", "ver\u00b7dient", "der", "B\u00f6\u00b7se\u00b7wicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn er die Meerflut pfl\u00fcgt, wenn er das Feld umbricht.", "tokens": ["Wenn", "er", "die", "Meer\u00b7flut", "pfl\u00fcgt", ",", "wenn", "er", "das", "Feld", "um\u00b7bricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Willst du ihn, frommer Mann, verdienen, reg dich frisch!", "tokens": ["Willst", "du", "ihn", ",", "from\u00b7mer", "Mann", ",", "ver\u00b7die\u00b7nen", ",", "reg", "dich", "frisch", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "$,", "ADJA", "NN", "$,", "VVFIN", "$,", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo nicht, so nimm f\u00fcrlieb mit Duft vom G\u00f6ttertisch.", "tokens": ["Wo", "nicht", ",", "so", "nimm", "f\u00fcr\u00b7lieb", "mit", "Duft", "vom", "G\u00f6t\u00b7ter\u00b7tisch", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "$,", "ADV", "VVIMP", "ADV", "APPR", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}