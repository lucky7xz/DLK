{"textgrid.poem.26262": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Sind je die Zeiten trauriger gewesen?", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sind je die Zeiten trauriger gewesen?", "tokens": ["Sind", "je", "die", "Zei\u00b7ten", "trau\u00b7ri\u00b7ger", "ge\u00b7we\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ADJD", "VAPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Menschheit hat die Br\u00fcderschaft vergessen.", "tokens": ["Die", "Menschheit", "hat", "die", "Br\u00fc\u00b7der\u00b7schaft", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Die Stundenreihen in dem Ha\u00df versinken.", "tokens": ["Die", "Stun\u00b7den\u00b7rei\u00b7hen", "in", "dem", "Ha\u00df", "ver\u00b7sin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Erde mu\u00df mehr Blut als Regen trinken.", "tokens": ["Die", "Er\u00b7de", "mu\u00df", "mehr", "Blut", "als", "Re\u00b7gen", "trin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PIAT", "NN", "KOUS", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Im Westen dort, das ist nicht Abendglut, \u2013", "tokens": ["Im", "Wes\u00b7ten", "dort", ",", "das", "ist", "nicht", "A\u00b7bend\u00b7glut", ",", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "ADV", "$,", "PDS", "VAFIN", "PTKNEG", "NN", "$,", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Erde bricht zum Himmel aus das Blut.", "tokens": ["Die", "Er\u00b7de", "bricht", "zum", "Him\u00b7mel", "aus", "das", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}