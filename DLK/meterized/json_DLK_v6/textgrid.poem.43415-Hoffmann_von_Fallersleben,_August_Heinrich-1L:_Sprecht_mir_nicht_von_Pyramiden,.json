{"textgrid.poem.43415": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sprecht mir nicht von Pyramiden,", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sprecht mir nicht von Pyramiden,", "tokens": ["Sprecht", "mir", "nicht", "von", "Py\u00b7ra\u00b7mi\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "APPR", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mammuththierversteinerung!", "tokens": ["Mam\u00b7muth\u00b7thier\u00b7ver\u00b7stei\u00b7ne\u00b7rung", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "Was dem Dichter ist beschieden,", "tokens": ["Was", "dem", "Dich\u00b7ter", "ist", "be\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lebt und athmet ewig jung.", "tokens": ["Lebt", "und", "ath\u00b7met", "e\u00b7wig", "jung", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "ADJD", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Nicht antikenvolle R\u00e4ume,", "tokens": ["Nicht", "an\u00b7ti\u00b7ken\u00b7vol\u00b7le", "R\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Nicht Papyrus, nicht Membran", "tokens": ["Nicht", "Pa\u00b7py\u00b7rus", ",", "nicht", "Memb\u00b7ran"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PTKNEG", "NE", "$,", "PTKNEG", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "K\u00f6nnen ihn und seine Tr\u00e4ume", "tokens": ["K\u00f6n\u00b7nen", "ihn", "und", "sei\u00b7ne", "Tr\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "KON", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Freud- und Lebensvoll umfah'n.", "tokens": ["Freu\u00b7d", "und", "Le\u00b7bens\u00b7voll", "um\u00b7fah'", "n."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["TRUNC", "KON", "ADJD", "VVFIN", "NE"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "Nicht der Moder in den Gr\u00fcften,", "tokens": ["Nicht", "der", "Mo\u00b7der", "in", "den", "Gr\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Klosterzell' und Burgverlie\u00df", "tokens": ["Klos\u00b7ter\u00b7zell'", "und", "Burg\u00b7ver\u00b7lie\u00df"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Taugen zu den reinen L\u00fcften,", "tokens": ["Tau\u00b7gen", "zu", "den", "rei\u00b7nen", "L\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Drin erbl\u00fcht sein Paradies.", "tokens": ["Drin", "er\u00b7bl\u00fcht", "sein", "Pa\u00b7ra\u00b7dies", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Weg mit allem Todten, Alten", "tokens": ["Weg", "mit", "al\u00b7lem", "Tod\u00b7ten", ",", "Al\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["NN", "APPR", "PIS", "NN", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und was sich ihm zugesellt!", "tokens": ["Und", "was", "sich", "ihm", "zu\u00b7ge\u00b7sellt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PRF", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ich mu\u00df bilden und gestalten", "tokens": ["Ich", "mu\u00df", "bil\u00b7den", "und", "ge\u00b7stal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "VVINF", "KON", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aus dem Leben meine Welt.", "tokens": ["Aus", "dem", "Le\u00b7ben", "mei\u00b7ne", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Alter Todter, dich nur ehr' ich,", "tokens": ["Al\u00b7ter", "Tod\u00b7ter", ",", "dich", "nur", "ehr'", "ich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PPER", "ADV", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bremer Rathsherrnkellerwein!", "tokens": ["Bre\u00b7mer", "Raths\u00b7herrn\u00b7kel\u00b7ler\u00b7wein", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Niemals spott' ich, niemals wehr' ich,", "tokens": ["Nie\u00b7mals", "spott'", "ich", ",", "nie\u00b7mals", "wehr'", "ich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADV", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Stellest du dich bei mir ein.", "tokens": ["Stel\u00b7lest", "du", "dich", "bei", "mir", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}