{"textgrid.poem.37695": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Einerlei", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr Mund ist stets derselbe,", "tokens": ["Ihr", "Mund", "ist", "stets", "der\u00b7sel\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "PDAT", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sein Ku\u00df mir immer neu,", "tokens": ["Sein", "Ku\u00df", "mir", "im\u00b7mer", "neu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr Auge noch dasselbe,", "tokens": ["Ihr", "Au\u00b7ge", "noch", "das\u00b7sel\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "PDAT", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sein freier Blick mir treu;", "tokens": ["Sein", "frei\u00b7er", "Blick", "mir", "treu", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "O du liebes Einerlei,", "tokens": ["O", "du", "lie\u00b7bes", "Ei\u00b7ner\u00b7lei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie wird aus dir so mancherlei!", "tokens": ["Wie", "wird", "aus", "dir", "so", "man\u00b7cher\u00b7lei", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "APPR", "PPER", "ADV", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}