{"textgrid.poem.26244": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es springen Ziegen am Stra\u00dfenrand,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es springen Ziegen am Stra\u00dfenrand,", "tokens": ["Es", "sprin\u00b7gen", "Zie\u00b7gen", "am", "Stra\u00b7\u00dfen\u00b7rand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPRART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und Bauern, die Reisb\u00fcndel in der Hand,", "tokens": ["Und", "Bau\u00b7ern", ",", "die", "Reis\u00b7b\u00fcn\u00b7del", "in", "der", "Hand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Ziehn unter Mandelb\u00e4umen hin.", "tokens": ["Ziehn", "un\u00b7ter", "Man\u00b7del\u00b7b\u00e4u\u00b7men", "hin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Tag hat sonnigen Arbeitssinn.", "tokens": ["Der", "Tag", "hat", "son\u00b7ni\u00b7gen", "Ar\u00b7beits\u00b7sinn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Es hocken Verk\u00e4uferinnen am Weg", "tokens": ["Es", "ho\u00b7cken", "Ver\u00b7k\u00e4u\u00b7fe\u00b7rin\u00b7nen", "am", "Weg"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "APPRART", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mit K\u00e4ufern, versunken in Handelsgespr\u00e4ch.", "tokens": ["Mit", "K\u00e4u\u00b7fern", ",", "ver\u00b7sun\u00b7ken", "in", "Han\u00b7dels\u00b7ge\u00b7spr\u00e4ch", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "VVFIN", "APPR", "NE", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Und Bambus schattet mit hohem Strau\u00df,", "tokens": ["Und", "Bam\u00b7bus", "schat\u00b7tet", "mit", "ho\u00b7hem", "Strau\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "APPR", "ADJA", "NE", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und K\u00e4fige schaukeln am Strohmattenhaus,", "tokens": ["Und", "K\u00e4\u00b7fi\u00b7ge", "schau\u00b7keln", "am", "Stroh\u00b7mat\u00b7ten\u00b7haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+--+--++-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Und Kinder spielen am Treppenstein.", "tokens": ["Und", "Kin\u00b7der", "spie\u00b7len", "am", "Trep\u00b7pen\u00b7stein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Vom nahen Reisfeld gl\u00e4nzt Spiegelschein", "tokens": ["Vom", "na\u00b7hen", "Reis\u00b7feld", "gl\u00e4nzt", "Spie\u00b7gel\u00b7schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Des Wassers, das um die Reis\u00e4hre steht.", "tokens": ["Des", "Was\u00b7sers", ",", "das", "um", "die", "Rei\u00b7s\u00e4h\u00b7re", "steht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Und eine Kokospalme weht", "tokens": ["Und", "ei\u00b7ne", "Ko\u00b7ko\u00b7spal\u00b7me", "weht"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und winkt ins blaue Licht hinaus.", "tokens": ["Und", "winkt", "ins", "blau\u00b7e", "Licht", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Sie alle sind warm und wohl zu Haus.", "tokens": ["Sie", "al\u00b7le", "sind", "warm", "und", "wohl", "zu", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VAFIN", "ADJD", "KON", "ADV", "APPR", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Nur ich schau' zu mit fremdem Blick", "tokens": ["Nur", "ich", "schau'", "zu", "mit", "frem\u00b7dem", "Blick"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VVFIN", "APPR", "APPR", "ADJA", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Und trage die Fremde als Stein im Genick.", "tokens": ["Und", "tra\u00b7ge", "die", "Frem\u00b7de", "als", "Stein", "im", "Ge\u00b7nick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "KOUS", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}}}}