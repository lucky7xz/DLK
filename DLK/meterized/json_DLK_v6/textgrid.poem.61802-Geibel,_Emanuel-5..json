{"textgrid.poem.61802": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ist das Spiel des Wassermanns", "tokens": ["Ist", "das", "Spiel", "des", "Was\u00b7ser\u00b7manns"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Gestern aus der Flut erklungen,", "tokens": ["Ge\u00b7stern", "aus", "der", "Flut", "er\u00b7klun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Oder war es nur der Wind,", "tokens": ["O\u00b7der", "war", "es", "nur", "der", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Der so wunderbar gesungen?", "tokens": ["Der", "so", "wun\u00b7der\u00b7bar", "ge\u00b7sun\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Bald wie ferner Orgelschall,", "tokens": ["Bald", "wie", "fer\u00b7ner", "Or\u00b7gel\u00b7schall", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ADV", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Bald wie \u00c4olsharfen t\u00f6nen,", "tokens": ["Bald", "wie", "\u00c4\u00b7ols\u00b7har\u00b7fen", "t\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Flo\u00df die Weise durch die Nacht,", "tokens": ["Flo\u00df", "die", "Wei\u00b7se", "durch", "die", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Jauchzend nun und nun mit St\u00f6hnen;", "tokens": ["Jauch\u00b7zend", "nun", "und", "nun", "mit", "St\u00f6h\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "KON", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie wenn tiefe Schwermut singt", "tokens": ["Wie", "wenn", "tie\u00b7fe", "Schwer\u00b7mut", "singt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOKOM", "KOUS", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Von vergangnen sel'gen Stunden,", "tokens": ["Von", "ver\u00b7gang\u00b7nen", "sel'\u00b7gen", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie wenn Inbrunst sich zu Tod", "tokens": ["Wie", "wenn", "In\u00b7brunst", "sich", "zu", "Tod"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOKOM", "KOUS", "NN", "PRF", "APPR", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Bluten will aus s\u00fc\u00dfen Wunden.", "tokens": ["Blu\u00b7ten", "will", "aus", "s\u00fc\u00b7\u00dfen", "Wun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und ich lag und dachte dein,", "tokens": ["Und", "ich", "lag", "und", "dach\u00b7te", "dein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "KON", "VVFIN", "PPOSAT", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und zum Traumbild ward mein Sehnen:", "tokens": ["Und", "zum", "Traum\u00b7bild", "ward", "mein", "Seh\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00dcbers wilde Meer zu dir", "tokens": ["\u00dc\u00b7bers", "wil\u00b7de", "Meer", "zu", "dir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "APPR", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Flog ich mit den ziehnden Schw\u00e4nen.", "tokens": ["Flog", "ich", "mit", "den", "ziehn\u00b7den", "Schw\u00e4\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}