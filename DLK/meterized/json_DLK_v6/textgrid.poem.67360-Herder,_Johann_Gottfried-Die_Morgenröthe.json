{"textgrid.poem.67360": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Die Morgenr\u00f6the", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Deines Lebens sch\u00f6nste Blumen,", "tokens": ["Dei\u00b7nes", "Le\u00b7bens", "sch\u00f6ns\u00b7te", "Blu\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sammle sie am Morgen fr\u00fch!", "tokens": ["Samm\u00b7le", "sie", "am", "Mor\u00b7gen", "fr\u00fch", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn je mehr die Sonne steiget,", "tokens": ["Denn", "je", "mehr", "die", "Son\u00b7ne", "stei\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Welken sie.", "tokens": ["Wel\u00b7ken", "sie", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "PPER", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Sieh, die Morgenr\u00f6the", "tokens": ["Sieh", ",", "die", "Mor\u00b7gen\u00b7r\u00f6\u00b7the"], "token_info": ["word", "punct", "word", "word"], "pos": ["NE", "$,", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Und des Hirten Fl\u00f6te", "tokens": ["Und", "des", "Hir\u00b7ten", "Fl\u00f6\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.7": {"text": "Wecket schon die W\u00e4lder,", "tokens": ["We\u00b7cket", "schon", "die", "W\u00e4l\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Schm\u00fccket schon die Felder.", "tokens": ["Schm\u00fc\u00b7cket", "schon", "die", "Fel\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "Willst Du Blumen pfl\u00fccken,", "tokens": ["Willst", "Du", "Blu\u00b7men", "pfl\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.10": {"text": "M\u00e4dchen, zu entz\u00fccken", "tokens": ["M\u00e4d\u00b7chen", ",", "zu", "ent\u00b7z\u00fc\u00b7cken"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "PTKZU", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.11": {"text": "In der Freude T\u00e4nzen,", "tokens": ["In", "der", "Freu\u00b7de", "T\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.12": {"text": "Mit der Unschuld Kr\u00e4nzen,", "tokens": ["Mit", "der", "Un\u00b7schuld", "Kr\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.13": {"text": "Den damit zu kr\u00f6nen,", "tokens": ["Den", "da\u00b7mit", "zu", "kr\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PAV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.14": {"text": "Dem ja alle Sch\u00f6nen", "tokens": ["Dem", "ja", "al\u00b7le", "Sch\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "ADV", "PIAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.15": {"text": "Gerne schmeicheln: fr\u00fch,", "tokens": ["Ger\u00b7ne", "schmei\u00b7cheln", ":", "fr\u00fch", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVINF", "$.", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.16": {"text": "M\u00e4dchen, pfl\u00fccke sie!", "tokens": ["M\u00e4d\u00b7chen", ",", "pfl\u00fc\u00b7cke", "sie", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.17": {"text": "Sieh der Liebe Rose,", "tokens": ["Sieh", "der", "Lie\u00b7be", "Ro\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "NE", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.18": {"text": "Die auf gr\u00fcnem Klo\u00dfe", "tokens": ["Die", "auf", "gr\u00fc\u00b7nem", "Klo\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.19": {"text": "Unter Dornen stehet", "tokens": ["Un\u00b7ter", "Dor\u00b7nen", "ste\u00b7het"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.20": {"text": "Und so bald vergehet!", "tokens": ["Und", "so", "bald", "ver\u00b7ge\u00b7het", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.21": {"text": "Sieh halb aufgegangen", "tokens": ["Sieh", "halb", "auf\u00b7ge\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["NE", "ADJD", "VVPP"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.22": {"text": "Hier ein Kn\u00f6spchen prangen,", "tokens": ["Hier", "ein", "Kn\u00f6spc\u00b7hen", "pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.23": {"text": "Dort die Nelke winken,", "tokens": ["Dort", "die", "Nel\u00b7ke", "win\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.24": {"text": "Hier ein Veilchen blinken,", "tokens": ["Hier", "ein", "Veil\u00b7chen", "blin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.25": {"text": "Und der Unschuld Sehnen", "tokens": ["Und", "der", "Un\u00b7schuld", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.26": {"text": "In der Lilje Thr\u00e4nen.", "tokens": ["In", "der", "Lil\u00b7je", "Thr\u00e4\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.27": {"text": "Huld und Anmuth, fr\u00fch,", "tokens": ["Huld", "und", "An\u00b7muth", ",", "fr\u00fch", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.28": {"text": "M\u00e4dchen, sammle sie!", "tokens": ["M\u00e4d\u00b7chen", ",", "samm\u00b7le", "sie", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.29": {"text": "Anmuth, Lieb' und Freuden", "tokens": ["An\u00b7muth", ",", "Lieb'", "und", "Freu\u00b7den"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.30": {"text": "Welken hin und scheiden,", "tokens": ["Wel\u00b7ken", "hin", "und", "schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "KON", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.31": {"text": "Wie das L\u00fcftchen streichet,", "tokens": ["Wie", "das", "L\u00fcft\u00b7chen", "strei\u00b7chet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.32": {"text": "Wie die Welle schleichet,", "tokens": ["Wie", "die", "Wel\u00b7le", "schlei\u00b7chet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.33": {"text": "Und auf allen Auen", "tokens": ["Und", "auf", "al\u00b7len", "Au\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.34": {"text": "Kannst Du Thr\u00e4nen schauen,", "tokens": ["Kannst", "Du", "Thr\u00e4\u00b7nen", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.35": {"text": "Thr\u00e4nen, die Aurora", "tokens": ["Thr\u00e4\u00b7nen", ",", "die", "Au\u00b7ro\u00b7ra"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "PRELS", "NE"], "meter": "+--+--", "measure": "dactylic.di.plus"}, "line.36": {"text": "In den Schoo\u00df der Flora", "tokens": ["In", "den", "Schoo\u00df", "der", "Flo\u00b7ra"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.37": {"text": "Weint'. Ach, ihre Stunden", "tokens": ["Weint'", ".", "Ach", ",", "ih\u00b7re", "Stun\u00b7den"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["VVFIN", "$.", "ITJ", "$,", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.38": {"text": "Sind so bald verschwunden.", "tokens": ["Sind", "so", "bald", "ver\u00b7schwun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}