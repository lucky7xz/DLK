{"dta.poem.23652": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1. An Vlaemsch\u2013 Belgien.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Suche nicht das Heil im Westen!", "tokens": ["Su\u00b7che", "nicht", "das", "Heil", "im", "Wes\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In der Fremde wohnt kein Gl\u00fcck \u2014", "tokens": ["In", "der", "Frem\u00b7de", "wohnt", "kein", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PIAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Suchst du deines G\u00fcckes Besten,", "tokens": ["Suchst", "du", "dei\u00b7nes", "G\u00fc\u00b7ckes", "Bes\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kehre in dich selbst zur\u00fcck!", "tokens": ["Keh\u00b7re", "in", "dich", "selbst", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Aus der Tugend deiner Ahnen", "tokens": ["Aus", "der", "Tu\u00b7gend", "dei\u00b7ner", "Ah\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Musst du deine Burgen baun,", "tokens": ["Musst", "du", "dei\u00b7ne", "Bur\u00b7gen", "baun", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der L\u00f6w' auf deinen Fahnen", "tokens": ["Und", "der", "L\u00f6\u00b7w'", "auf", "dei\u00b7nen", "Fah\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Lehre dich dir selbst vertraun.", "tokens": ["Leh\u00b7re", "dich", "dir", "selbst", "ver\u00b7traun", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Treu bewahr in deiner Mitte", "tokens": ["Treu", "be\u00b7wahr", "in", "dei\u00b7ner", "Mit\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Vor dem w\u00e4lschen Uebermuth", "tokens": ["Vor", "dem", "w\u00e4l\u00b7schen", "Ue\u00b7ber\u00b7muth"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Deine Sprach' und deine Sitte,", "tokens": ["Dei\u00b7ne", "Sprach'", "und", "dei\u00b7ne", "Sit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deiner V\u00e4ter Gut und Blut.", "tokens": ["Dei\u00b7ner", "V\u00e4\u00b7ter", "Gut", "und", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Dann erst kannst du r&#252;hmend sagen,", "tokens": ["Dann", "erst", "kannst", "du", "r", "&#252;", "hmend", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "XML_entity", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "NE", "$(", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df du ", "tokens": ["Da\u00df", "du"], "token_info": ["word", "word"], "pos": ["KOUS", "PPER"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Da\u00df erbl\u00fcht in unsern Tagen", "tokens": ["Da\u00df", "er\u00b7bl\u00fcht", "in", "un\u00b7sern", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "VVPP", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deine alte Herrlichkeit.", "tokens": ["Dei\u00b7ne", "al\u00b7te", "Herr\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}