{"textgrid.poem.31919": {"metadata": {"author": {"name": "Angelus Silesius", "birth": "N.A.", "death": "N.A."}, "title": "200. Die Aufgegebenheit", "genre": "verse", "period": "N.A.", "pub_year": 1650, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer seine Seele hat verloren und vergeben,", "tokens": ["Wer", "sei\u00b7ne", "See\u00b7le", "hat", "ver\u00b7lo\u00b7ren", "und", "ver\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VAFIN", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der kann ganz seliglich mit Gott die Wette leben.", "tokens": ["Der", "kann", "ganz", "se\u00b7lig\u00b7lich", "mit", "Gott", "die", "Wet\u00b7te", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "ADJD", "APPR", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}