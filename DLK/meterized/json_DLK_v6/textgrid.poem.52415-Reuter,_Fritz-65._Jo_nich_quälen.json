{"textgrid.poem.52415": {"metadata": {"author": {"name": "Reuter, Fritz", "birth": "N.A.", "death": "N.A."}, "title": "65. Jo nich qu\u00e4len", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbmin leiwe Pr\u00f6hl\u00ab, segg ick tau Wewer Pr\u00f6hlen,", "tokens": ["\u00bb", "min", "lei\u00b7we", "Pr\u00f6hl", "\u00ab", ",", "segg", "ick", "tau", "We\u00b7wer", "Pr\u00f6h\u00b7len", ","], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "ADJA", "NN", "$(", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "\u00bbhir is de Hund, man mit em furt!", "tokens": ["\u00bb", "hir", "is", "de", "Hund", ",", "man", "mit", "em", "furt", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM.la", "FM.la", "FM.la", "FM.la", "$,", "PIS", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Un dat Sei em man jo nich fehlen,", "tokens": ["Un", "dat", "Sei", "em", "man", "jo", "nich", "feh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "FM", "FM", "FM", "NE", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Un dat sin Qual nich l\u00e4nger durt!\u00ab", "tokens": ["Un", "dat", "sin", "Qual", "nich", "l\u00e4n\u00b7ger", "durt", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "ART", "NN", "NN", "PTKNEG", "ADJD", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "\u00bbje, Herr\u00ab, seggt Pr\u00f6hl, \u00bbSei m\u00f6ten mit;", "tokens": ["\u00bb", "je", ",", "Herr", "\u00ab", ",", "seggt", "Pr\u00f6hl", ",", "\u00bb", "Sei", "m\u00f6\u00b7ten", "mit", ";"], "token_info": ["punct", "word", "punct", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$,", "NN", "$(", "$,", "VVFIN", "NN", "$,", "$(", "VAIMP", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mi folgt hei willig nich allein,", "tokens": ["Mi", "folgt", "hei", "wil\u00b7lig", "nich", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADJD", "ADJD", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Un wenn hei an den Strick so ritt,", "tokens": ["Un", "wenn", "hei", "an", "den", "Strick", "so", "ritt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "NE", "APPR", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.8": {"text": "Denn kann ick't ok nich gaud anseihn,", "tokens": ["Denn", "kann", "ick't", "ok", "nich", "gaud", "an\u00b7seihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.9": {"text": "Denn ick b\u00fcn ok man sihr weikm\u00e4udig.\u00ab", "tokens": ["Denn", "ick", "b\u00fcn", "ok", "man", "sihr", "weik\u00b7m\u00e4u\u00b7dig", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "FM", "FM", "FM", "FM", "FM", "ADJD", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Na, ik gah mit. \u00bbKumm, Wasser, kumm!\u00ab", "tokens": ["Na", ",", "ik", "gah", "mit", ".", "\u00bb", "Kumm", ",", "Was\u00b7ser", ",", "kumm", "!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct", "word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["ITJ", "$,", "NE", "VVFIN", "PTKVZ", "$.", "$(", "NE", "$,", "NN", "$,", "ADJD", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Un min oll Wasser, stiw un stumm,", "tokens": ["Un", "min", "oll", "Was\u00b7ser", ",", "stiw", "un", "stumm", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "ADJD", "NN", "$,", "FM.fr", "FM.fr", "FM.fr", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.12": {"text": "Half blind, de beiden Uhren bl\u00e4udig,", "tokens": ["Half", "blind", ",", "de", "bei\u00b7den", "Uh\u00b7ren", "bl\u00e4u\u00b7dig", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "$,", "NE", "PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Geiht tru un trurig an min Sid.", "tokens": ["Geiht", "tru", "un", "tru\u00b7rig", "an", "min", "Sid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.14": {"text": "\u00bbso\u00ab, segg ik, \u00bbnu man fix heran!", "tokens": ["\u00bb", "so", "\u00ab", ",", "segg", "ik", ",", "\u00bb", "nu", "man", "fix", "he\u00b7ran", "!"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$(", "$,", "NE", "NE", "$,", "$(", "ADV", "PIS", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Hir binn'n S' em sick man an de Wid", "tokens": ["Hir", "binn'n", "S'", "em", "sick", "man", "an", "de", "Wid"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["XY", "XY", "XY", "XY", "XY", "PIS", "APPR", "NE", "NE"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.16": {"text": "Fast, dat hei sick nich r\u00f6gen kann!", "tokens": ["Fast", ",", "dat", "hei", "sick", "nich", "r\u00f6\u00b7gen", "kann", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "FM.nl", "FM.nl", "FM.nl", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.17": {"text": "Un denn v\u00f6r'n Kopp, dat't fix v\u00f6rbi.\u00ab", "tokens": ["Un", "denn", "v\u00f6r'n", "Kopp", ",", "dat't", "fix", "v\u00f6r\u00b7bi", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NE", "KON", "ADJA", "NN", "$,", "FM.la", "FM.la", "FM.la", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Oll Wewer Pr\u00f6hl, de binnt em an", "tokens": ["Oll", "We\u00b7wer", "Pr\u00f6hl", ",", "de", "binnt", "em", "an"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NE", "NE", "$,", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Un dreiht sick \u00fcm un seggt tau mi:", "tokens": ["Un", "dreiht", "sick", "\u00fcm", "un", "seggt", "tau", "mi", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "FM", "FM", "FM", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "\u00bbherr, sall'ck em up dat Bladd nich scheiten?\u00ab", "tokens": ["\u00bb", "herr", ",", "sall'ck", "em", "up", "dat", "Bladd", "nich", "schei\u00b7ten", "?", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PTKVZ", "$,", "FM", "FM", "FM", "ART", "NN", "PTKNEG", "VVFIN", "$.", "$("], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.21": {"text": "\u00bbne\u00ab, segg ick, \u00bbne! Wat sall dat heiten?", "tokens": ["\u00bb", "ne", "\u00ab", ",", "segg", "ick", ",", "\u00bb", "ne", "!", "Wat", "sall", "dat", "hei\u00b7ten", "?"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "NE", "$(", "$,", "VVFIN", "PPER", "$,", "$(", "NE", "$.", "NN", "NE", "ART", "ADJA", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.22": {"text": "Grad v\u00f6r den Kopp! \u2013 Denn's't glik gescheihn\u00ab,", "tokens": ["Grad", "v\u00f6r", "den", "Kopp", "!", "\u2013", "Denn'\u00b7s't", "glik", "ge\u00b7scheihn", "\u00ab", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$.", "$(", "FM.la", "FM.la", "FM.la", "$(", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.23": {"text": "Un ward mi von em afwarts dreihn", "tokens": ["Un", "ward", "mi", "von", "em", "af\u00b7warts", "dreihn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "NE", "APPR", "ADJA", "ADV", "CARD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Un stah un lur nu up den Paff.", "tokens": ["Un", "stah", "un", "lur", "nu", "up", "den", "Paff", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "FM", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.25": {"text": "Oll Pr\u00f6hl leggt an, set't wedder af", "tokens": ["Oll", "Pr\u00f6hl", "leggt", "an", ",", "set't", "wed\u00b7der", "af"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NE", "VVFIN", "PTKVZ", "$,", "VVFIN", "KON", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Un seggt tau mi: '\u00bbHerr, weiten S' wat?", "tokens": ["Un", "seggt", "tau", "mi", ":", "'", "\u00bb", "Herr", ",", "wei\u00b7ten", "S", "'", "wat", "?"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$.", "$(", "$(", "NN", "$,", "ADJA", "XY", "$(", "PWS", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.27": {"text": "Ick scheit em richtig up dat Bladd.\u00ab", "tokens": ["Ick", "scheit", "em", "rich\u00b7tig", "up", "dat", "Bladd", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "\u00bbih, ne\u00ab, segg ick, \u00bbgrad v\u00f6r den Kopp!\u00ab", "tokens": ["\u00bb", "ih", ",", "ne", "\u00ab", ",", "segg", "ick", ",", "\u00bb", "grad", "v\u00f6r", "den", "Kopp", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "$,", "NE", "$(", "$,", "VVFIN", "PPER", "$,", "$(", "ADV", "APPR", "ART", "NN", "$.", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.29": {"text": "Hei nimmt sin Flint un b\u00f6hrt s' herup,", "tokens": ["Hei", "nimmt", "sin", "Flint", "un", "b\u00f6hrt", "s'", "he\u00b7rup", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.30": {"text": "Set't s' wedder af un r\u00f6ppt mi tau:", "tokens": ["Set't", "s'", "wed\u00b7der", "af", "un", "r\u00f6ppt", "mi", "tau", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+---+-+-+", "measure": "dactylic.init"}, "line.31": {"text": "\u00bbherr, wenn'ck oll Wassern scheiten sall,", "tokens": ["\u00bb", "herr", ",", "wenn'ck", "oll", "Was\u00b7sern", "schei\u00b7ten", "sall", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKVZ", "$,", "KOUS", "ADJD", "NN", "VVFIN", "ADJD", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.32": {"text": "Scheit ick \u2013 de D\u00fcwel hal! \u2013 up't Bladd.\u00ab", "tokens": ["Scheit", "ick", "\u2013", "de", "D\u00fc\u00b7wel", "hal", "!", "\u2013", "up't", "Bladd", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "punct", "punct"], "pos": ["NN", "PPER", "$(", "NE", "NE", "NE", "$.", "$(", "FM.la", "FM.la", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "\u00bbne, ne!\u00ab segg ick, \u00bbwat sall denn dat?", "tokens": ["\u00bb", "ne", ",", "ne", "!", "\u00ab", "segg", "ick", ",", "\u00bb", "wat", "sall", "denn", "dat", "?"], "token_info": ["punct", "word", "punct", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "NE", "$,", "NE", "$.", "$(", "VVIMP", "PPER", "$,", "$(", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.34": {"text": "V\u00f6r'n Kopp! Un nu man tau! Man ball!", "tokens": ["V\u00f6r'n", "Kopp", "!", "Un", "nu", "man", "tau", "!", "Man", "ball", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "NE", "ADV", "PIS", "PTKVZ", "$.", "PIS", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Up't Bladd, dor k\u00e4n'n Sei em verfehlen;", "tokens": ["Up't", "Bladd", ",", "dor", "k\u00e4n'n", "Sei", "em", "ver\u00b7feh\u00b7len", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.36": {"text": "Wotau den armen K\u00f6ter qu\u00e4len?\u00ab", "tokens": ["Wo\u00b7tau", "den", "ar\u00b7men", "K\u00f6\u00b7ter", "qu\u00e4\u00b7len", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.37": {"text": "\u00bbje, Herr\u00ab, seggt Pr\u00f6hl, \u00bbdat is dat eben:", "tokens": ["\u00bb", "je", ",", "Herr", "\u00ab", ",", "seggt", "Pr\u00f6hl", ",", "\u00bb", "dat", "is", "dat", "e\u00b7ben", ":"], "token_info": ["punct", "word", "punct", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$,", "NN", "$(", "$,", "VVFIN", "NN", "$,", "$(", "FM.nl", "FM.nl", "FM.nl", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.38": {"text": "V\u00f6r'n Kopp k\u00fcnn't em so'n Dr\u00e4hnung gewen.\u00ab", "tokens": ["V\u00f6r'n", "Kopp", "k\u00fcnn't", "em", "so'n", "Dr\u00e4h\u00b7nung", "ge\u00b7wen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$.", "$("], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}}}}}