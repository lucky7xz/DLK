{"textgrid.poem.62451": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sing' Heldenlieder, Preu\u00dfenvolk,", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sing' Heldenlieder, Preu\u00dfenvolk,", "tokens": ["Sing'", "Hel\u00b7den\u00b7lie\u00b7der", ",", "Preu\u00b7\u00dfen\u00b7volk", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NE", "NE", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df sich dein Krieger freut.", "tokens": ["Da\u00df", "sich", "dein", "Krie\u00b7ger", "freut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der K\u00f6nig steckt sein Banner auf", "tokens": ["Der", "K\u00f6\u00b7nig", "steckt", "sein", "Ban\u00b7ner", "auf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Alles l\u00e4uft den Heldenlauf,", "tokens": ["Und", "Al\u00b7les", "l\u00e4uft", "den", "Hel\u00b7den\u00b7lauf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zu streiten solchen Streit.", "tokens": ["Zu", "strei\u00b7ten", "sol\u00b7chen", "Streit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wir hoffen auf dein Heergebot,", "tokens": ["Wir", "hof\u00b7fen", "auf", "dein", "Heer\u00b7ge\u00b7bot", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O F\u00fcrst, voll Streitbegier;", "tokens": ["O", "F\u00fcrst", ",", "voll", "Streit\u00b7be\u00b7gier", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du sannst und wogst, du wogst und sannst,", "tokens": ["Du", "sannst", "und", "wogst", ",", "du", "wogst", "und", "sannst", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$,", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Eh' du den gro\u00dfen Kampf begannst,", "tokens": ["Eh'", "du", "den", "gro\u00b7\u00dfen", "Kampf", "be\u00b7gannst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das dankt die Menschheit dir.", "tokens": ["Das", "dankt", "die", "Menschheit", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "PPER", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Doch nun du stehst und rufst und winkst,", "tokens": ["Doch", "nun", "du", "stehst", "und", "rufst", "und", "winkst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Greift Alles zum Gewehr,", "tokens": ["Greift", "Al\u00b7les", "zum", "Ge\u00b7wehr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und Alles gl\u00fcht in Kampfeslust,", "tokens": ["Und", "Al\u00b7les", "gl\u00fcht", "in", "Kamp\u00b7fes\u00b7lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dich segnet jede Preu\u00dfenbrust,", "tokens": ["Dich", "seg\u00b7net", "je\u00b7de", "Preu\u00b7\u00dfen\u00b7brust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du guter frommer Herr.", "tokens": ["Du", "gu\u00b7ter", "from\u00b7mer", "Herr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wie Sparter gegen Xerxes Heer", "tokens": ["Wie", "Spar\u00b7ter", "ge\u00b7gen", "Xe\u00b7rxes", "Heer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "APPR", "NE", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In dicht gedr\u00e4ngten Reihn,", "tokens": ["In", "dicht", "ge\u00b7dr\u00e4ng\u00b7ten", "Reihn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Stehn Preu\u00dfens wackre Krieger da,", "tokens": ["Stehn", "Preu\u00b7\u00dfens", "wack\u00b7re", "Krie\u00b7ger", "da", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Am Grabe der Germania", "tokens": ["Am", "Gra\u00b7be", "der", "Ger\u00b7ma\u00b7nia"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Zu R\u00e4chern sich zu weihn.", "tokens": ["Zu", "R\u00e4\u00b7chern", "sich", "zu", "weihn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "So ziehet hin, ihr Br\u00fcder zieht,", "tokens": ["So", "zie\u00b7het", "hin", ",", "ihr", "Br\u00fc\u00b7der", "zieht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKVZ", "$,", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In den gerechten Krieg!", "tokens": ["In", "den", "ge\u00b7rech\u00b7ten", "Krieg", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wir liefen gern mit euch die Bahn,", "tokens": ["Wir", "lie\u00b7fen", "gern", "mit", "euch", "die", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Jeder thut, so viel er kann,", "tokens": ["Ein", "Je\u00b7der", "thut", ",", "so", "viel", "er", "kann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "$,", "ADV", "ADV", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und tr\u00e4umt von Schlacht und Sieg.", "tokens": ["Und", "tr\u00e4umt", "von", "Schlacht", "und", "Sieg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}