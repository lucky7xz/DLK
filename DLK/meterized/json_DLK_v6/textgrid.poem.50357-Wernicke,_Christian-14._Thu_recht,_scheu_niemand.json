{"textgrid.poem.50357": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "14. Thu recht, scheu niemand", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dass alles was misslingt dein K\u00f6nig auf dich schieb',", "tokens": ["Dass", "al\u00b7les", "was", "miss\u00b7lingt", "dein", "K\u00f6\u00b7nig", "auf", "dich", "schieb'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PWS", "VVFIN", "PPOSAT", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Klagst du, ob gleich du ihm gehorsam bist in allen:", "tokens": ["Klagst", "du", ",", "ob", "gleich", "du", "ihm", "ge\u00b7hor\u00b7sam", "bist", "in", "al\u00b7len", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "ADV", "PPER", "PPER", "ADJD", "VAFIN", "APPR", "PIAT", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wilst du ihm nicht,", "tokens": ["Wilst", "du", "ihm", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "PTKNEG", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "So thu' ihm niemals nicht, ", "tokens": ["So", "thu'", "ihm", "nie\u00b7mals", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKNEG", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}