{"textgrid.poem.40189": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Letzter Wunsch", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was mich br\u00fcnstig einst getrieben,", "tokens": ["Was", "mich", "br\u00fcns\u00b7tig", "einst", "ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADJD", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Allen es verkl\u00e4rt zu k\u00fcnden,", "tokens": ["Al\u00b7len", "es", "ver\u00b7kl\u00e4rt", "zu", "k\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "VVPP", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "all mein Streben, all mein Lieben,", "tokens": ["all", "mein", "Stre\u00b7ben", ",", "all", "mein", "Lie\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "$,", "PIAT", "PPOSAT", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "ruht nun still in seinen Gr\u00fcnden.", "tokens": ["ruht", "nun", "still", "in", "sei\u00b7nen", "Gr\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "F\u00fchle nur die Reine, Sch\u00f6ne,", "tokens": ["F\u00fch\u00b7le", "nur", "die", "Rei\u00b7ne", ",", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "die ich tiefst im Herzen trage;", "tokens": ["die", "ich", "tiefst", "im", "Her\u00b7zen", "tra\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRELS", "PPER", "ADJD", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "und mir sind so fremd die T\u00f6ne", "tokens": ["und", "mir", "sind", "so", "fremd", "die", "T\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "ADV", "ADJD", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "meiner J\u00fcnglingsthr\u00e4nenklage.", "tokens": ["mei\u00b7ner", "J\u00fcng\u00b7lings\u00b7thr\u00e4\u00b7nen\u00b7kla\u00b7ge", "."], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wage dennoch nicht zu singen,", "tokens": ["Wa\u00b7ge", "den\u00b7noch", "nicht", "zu", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "wie mein Gl\u00fcck sich mir enth\u00fcllte;", "tokens": ["wie", "mein", "Gl\u00fcck", "sich", "mir", "ent\u00b7h\u00fcll\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "PPOSAT", "NN", "PRF", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "m\u00f6cht' es doch zu arm gelingen,", "tokens": ["m\u00f6cht'", "es", "doch", "zu", "arm", "ge\u00b7lin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PTKA", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "was so reich sich mir erf\u00fcllte.", "tokens": ["was", "so", "reich", "sich", "mir", "er\u00b7f\u00fcll\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADJD", "PRF", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Reicher als erstrebt mein Lieben,", "tokens": ["Rei\u00b7cher", "als", "er\u00b7strebt", "mein", "Lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "VVFIN", "PPOSAT", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "liebereicher so mein Streben,", "tokens": ["lie\u00b7be\u00b7rei\u00b7cher", "so", "mein", "Stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "und nur Ein Wunsch ist geblieben:", "tokens": ["und", "nur", "Ein", "Wunsch", "ist", "ge\u00b7blie\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "da\u00df wir auch den ", "tokens": ["da\u00df", "wir", "auch", "den"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ART"], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.5": {"line.1": {"text": "da\u00df ein Wandel ohne Fehle", "tokens": ["da\u00df", "ein", "Wan\u00b7del", "oh\u00b7ne", "Feh\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "nun in ", "tokens": ["nun", "in"], "token_info": ["word", "word"], "pos": ["ADV", "APPR"], "meter": "++", "measure": "spondeus"}, "line.3": {"text": "wie dies Ruhen Seel' in Seele", "tokens": ["wie", "dies", "Ru\u00b7hen", "Seel'", "in", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PDS", "ADJA", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "uns den ", "tokens": ["uns", "den"], "token_info": ["word", "word"], "pos": ["PPER", "ART"], "meter": "+-", "measure": "trochaic.single"}}}}}