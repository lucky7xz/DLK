"punct"], "pos": ["$(", "XY", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "\u201each gebt mir einen Rath,", "tokens": ["\u201e", "ach", "gebt", "mir", "ei\u00b7nen", "Rath", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u201ees reitet mir alle Tage", "tokens": ["\u201e", "es", "rei\u00b7tet", "mir", "al\u00b7le", "Ta\u00b7ge"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "PPER", "PIAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "\u201eein hurtiger Ritter nach.\u201c", "tokens": ["\u201e", "ein", "hur\u00b7ti\u00b7ger", "Rit\u00b7ter", "nach", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "PTKVZ", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Ach Tochter! liebe Tochter!", "tokens": ["Ach", "Toch\u00b7ter", "!", "lie\u00b7be", "Toch\u00b7ter", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "NN", "$.", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den Rath, den geb ich dir,", "tokens": ["Den", "Rath", ",", "den", "geb", "ich", "dir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "La\u00df du den Reiter fahren,", "tokens": ["La\u00df", "du", "den", "Rei\u00b7ter", "fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bleib du das Jahr bey mir.", "tokens": ["Bleib", "du", "das", "Jahr", "bey", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "\u201each Mutter! liebe Mutter!", "tokens": ["\u201e", "ach", "Mut\u00b7ter", "!", "lie\u00b7be", "Mut\u00b7ter", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "XY", "NN", "$.", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "\u201eder Rath, der ist nicht gut,", "tokens": ["\u201e", "der", "Rath", ",", "der", "ist", "nicht", "gut", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$,", "PRELS", "VAFIN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u201eder Ritter ist mir lieber,", "tokens": ["\u201e", "der", "Rit\u00b7ter", "ist", "mir", "lie\u00b7ber", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "\u201eals all dein Hab und Gut.", "tokens": ["\u201e", "als", "all", "dein", "Hab", "und", "Gut", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PIAT", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Ist dir der Reiter lieber,", "tokens": ["Ist", "dir", "der", "Rei\u00b7ter", "lie\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Als all mein Hab und Gut,", "tokens": ["Als", "all", "mein", "Hab", "und", "Gut", ","], "token_info": ["word", "word", "word", "word", "wo