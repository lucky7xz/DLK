{"dta.poem.19658": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "W\u00e4r ich ein Knab geboren .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519168", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es wollt ein M\u00e4del grasen,               ", "tokens": ["Es", "wollt", "ein", "M\u00e4\u00b7del", "gra\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wollt grasen im gr\u00fcnen Klee,", "tokens": ["Wollt", "gra\u00b7sen", "im", "gr\u00fc\u00b7nen", "Klee", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Begegnets ihm ein Reiter,", "tokens": ["Be\u00b7geg\u00b7nets", "ihm", "ein", "Rei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wollts haben zu der Eh.", "tokens": ["Wollts", "ha\u00b7ben", "zu", "der", "Eh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ach komm, du hurtig M\u00e4del,", "tokens": ["Ach", "komm", ",", "du", "hur\u00b7tig", "M\u00e4\u00b7del", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "$,", "PPER", "ADJD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und setz dich zu mir her.", "tokens": ["Und", "setz", "dich", "zu", "mir", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u201each wollt ich d\u00fcrft mich setzen,", "tokens": ["\u201e", "ach", "wollt", "ich", "d\u00fcrft", "mich", "set\u00b7zen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VMFIN", "PPER", "VMFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "\u201ekein Gras hats Zicklein mehr.", "tokens": ["\u201e", "kein", "Gras", "hats", "Zic\u00b7klein", "mehr", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PIAT", "NN", "VAFIN", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der Reiter spreit den Mantel,", "tokens": ["Der", "Rei\u00b7ter", "spreit", "den", "Man\u00b7tel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wohl \u00fcber den gr\u00fcnen Klee:", "tokens": ["Wohl", "\u00fc\u00b7ber", "den", "gr\u00fc\u00b7nen", "Klee", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Komm du mein wackeres M\u00e4del,", "tokens": ["Komm", "du", "mein", "wa\u00b7cke\u00b7res", "M\u00e4\u00b7del", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und setz dich zu mir her.", "tokens": ["Und", "setz", "dich", "zu", "mir", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "\u2