{"dta.poem.17909": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "152.  GOtt ist unser Ziehl.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Was macht nicht GOtt au\u00df sich", "tokens": ["Was", "macht", "nicht", "Gott", "au\u00df", "sich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PTKNEG", "NN", "APPR", "PRF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ziel/", "tokens": ["Ziel", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Ich sch\u00fcsse st\u00e4ts nach ihm/ ich treff\u2019 ihn wenn ich wil.", "tokens": ["Ich", "sch\u00fcs\u00b7se", "st\u00e4ts", "nach", "ihm", "/", "ich", "treff'", "ihn", "wenn", "ich", "wil", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPER", "$(", "PPER", "VVFIN", "PPER", "KOUS", "PPER", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}