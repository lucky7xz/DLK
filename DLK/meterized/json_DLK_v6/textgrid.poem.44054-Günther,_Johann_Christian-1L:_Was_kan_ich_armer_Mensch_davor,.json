{"textgrid.poem.44054": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was kan ich armer Mensch davor,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was kan ich armer Mensch davor,", "tokens": ["Was", "kan", "ich", "ar\u00b7mer", "Mensch", "da\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADJA", "NN", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn Noth und Angst zur S\u00fcnde zwingen?", "tokens": ["Wenn", "Noth", "und", "Angst", "zur", "S\u00fcn\u00b7de", "zwin\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Herr, neige dein gerechtes Ohr,", "tokens": ["Herr", ",", "nei\u00b7ge", "dein", "ge\u00b7rech\u00b7tes", "Ohr", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich will ein kleines Opfer bringen.", "tokens": ["Ich", "will", "ein", "klei\u00b7nes", "Op\u00b7fer", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Es blutet weder Schaaf noch Rind,", "tokens": ["Es", "blu\u00b7tet", "we\u00b7der", "Schaaf", "noch", "Rind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "NN", "ADV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich habe Weihrauch angez\u00fcndt,", "tokens": ["Ich", "ha\u00b7be", "Weih\u00b7rauch", "an\u00b7ge\u00b7z\u00fcndt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Nicht Weihrauch, den die B\u00e4ume schwizen:", "tokens": ["Nicht", "Weih\u00b7rauch", ",", "den", "die", "B\u00e4u\u00b7me", "schwi\u00b7zen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "$,", "PRELS", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ein \u00e4ngstlich Herz und treu Gebeth,", "tokens": ["Ein", "\u00e4ngst\u00b7lich", "Herz", "und", "treu", "Ge\u00b7beth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "KON", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Du hast es ja noch nie verschm\u00e4ht,", "tokens": ["Du", "hast", "es", "ja", "noch", "nie", "ver\u00b7schm\u00e4ht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Soll wider Zorn und Rache sch\u00fczen.", "tokens": ["Soll", "wi\u00b7der", "Zorn", "und", "Ra\u00b7che", "sch\u00fc\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Gr\u00f6\u00dfe deiner Majest\u00e4t", "tokens": ["Die", "Gr\u00f6\u00b7\u00dfe", "dei\u00b7ner", "Ma\u00b7jes\u00b7t\u00e4t"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erkenn ich aus den kleinsten Dingen;", "tokens": ["Er\u00b7kenn", "ich", "aus", "den", "kleins\u00b7ten", "Din\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dein Arm, der \u00fcber alles geht,", "tokens": ["Dein", "Arm", ",", "der", "\u00fc\u00b7ber", "al\u00b7les", "geht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "APPR", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kan Wa\u00dfer aus den Felsen zwingen;", "tokens": ["Kan", "Wa\u00b7\u00dfer", "aus", "den", "Fel\u00b7sen", "zwin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Du sprichst ein Wort, so wird es Licht;", "tokens": ["Du", "sprichst", "ein", "Wort", ",", "so", "wird", "es", "Licht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ADV", "VAFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bedroh das Meer, es regt sich nicht;", "tokens": ["Be\u00b7droh", "das", "Meer", ",", "es", "regt", "sich", "nicht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$,", "PPER", "VVFIN", "PRF", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Befiehl, so wird die Fluth zu Flammen;", "tokens": ["Be\u00b7fiehl", ",", "so", "wird", "die", "Fluth", "zu", "Flam\u00b7men", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VAFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Du winckst, so steht der Sonnen Lauf,", "tokens": ["Du", "win\u00b7ckst", ",", "so", "steht", "der", "Son\u00b7nen", "Lauf", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADV", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "So thun sich Tief und Abgrund auf", "tokens": ["So", "thun", "sich", "Tief", "und", "Ab\u00b7grund", "auf"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ADJD", "KON", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und werfen Erd und Stern zusammen.", "tokens": ["Und", "wer\u00b7fen", "Erd", "und", "Stern", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Du z\u00fcrntest ehmahls, gro\u00dfer Gott,", "tokens": ["Du", "z\u00fcrn\u00b7test", "eh\u00b7mahls", ",", "gro\u00b7\u00dfer", "Gott", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da wuchs das Wa\u00dfer \u00fcber Berge,", "tokens": ["Da", "wuchs", "das", "Wa\u00b7\u00dfer", "\u00fc\u00b7ber", "Ber\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Starcken Hochmuth war dein Spott,", "tokens": ["Der", "Star\u00b7cken", "Hoch\u00b7muth", "war", "dein", "Spott", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auch Riesen fielen durch die Zwerge;", "tokens": ["Auch", "Rie\u00b7sen", "fie\u00b7len", "durch", "die", "Zwer\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Egypten trozt und st\u00e4rckt sein Heer,", "tokens": ["E\u00b7gyp\u00b7ten", "trozt", "und", "st\u00e4rckt", "sein", "Heer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Stock verjagt es in das Meer,", "tokens": ["Ein", "Stock", "ver\u00b7jagt", "es", "in", "das", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da schwamm Volck, K\u00f6nig, Ro\u00df und Wagen;", "tokens": ["Da", "schwamm", "Volck", ",", "K\u00f6\u00b7nig", ",", "Ro\u00df", "und", "Wa\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der Wind bringt Fleisch, die W\u00fcsten Brodt;", "tokens": ["Der", "Wind", "bringt", "Fleisch", ",", "die", "W\u00fcs\u00b7ten", "Brodt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Mana\u00dfe f\u00e4llt, du schickst ihm Noth,", "tokens": ["Ma\u00b7na\u00b7\u00dfe", "f\u00e4llt", ",", "du", "schickst", "ihm", "Noth", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Er weint, du \u00e4nderst Kett- und Klagen.", "tokens": ["Er", "weint", ",", "du", "\u00e4n\u00b7derst", "Ket\u00b7t", "und", "Kla\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "ADV", "TRUNC", "KON", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Dein Nahm und Weg ist wunderlich,", "tokens": ["Dein", "Nahm", "und", "Weg", "ist", "wun\u00b7der\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du wirst auch mir zu helfen wi\u00dfen;", "tokens": ["Du", "wirst", "auch", "mir", "zu", "hel\u00b7fen", "wi\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PPER", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich hof, ich trau dir, zeige dich,", "tokens": ["Ich", "hof", ",", "ich", "trau", "dir", ",", "zei\u00b7ge", "dich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein L\u00e4strer wird sich sch\u00e4men m\u00fc\u00dfen.", "tokens": ["Mein", "L\u00e4st\u00b7rer", "wird", "sich", "sch\u00e4\u00b7men", "m\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PRF", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Kein Ungl\u00fcck schl\u00e4gt die Zuversicht,", "tokens": ["Kein", "Un\u00b7gl\u00fcck", "schl\u00e4gt", "die", "Zu\u00b7ver\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Du kanst, du must, du l\u00e4st mich nicht,", "tokens": ["Du", "kanst", ",", "du", "must", ",", "du", "l\u00e4st", "mich", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PPER", "VMFIN", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die B\u00fc\u00df ist hier, der Trost schwebt oben.", "tokens": ["Die", "B\u00fc\u00df", "ist", "hier", ",", "der", "Trost", "schwebt", "o\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$,", "ART", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Kein menschlich Ansehn hebt die Pein;", "tokens": ["Kein", "menschlich", "An\u00b7sehn", "hebt", "die", "Pein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJD", "VVINF", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Getrost, mein Herz, so mu\u00df es seyn,", "tokens": ["Ge\u00b7trost", ",", "mein", "Herz", ",", "so", "mu\u00df", "es", "seyn", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,", "ADV", "VMFIN", "PPER", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Wir sollen blos den H\u00f6chsten loben.", "tokens": ["Wir", "sol\u00b7len", "blos", "den", "H\u00f6chs\u00b7ten", "lo\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}