{"dta.poem.1148": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "34. Die Sch\u00f6ne Listige.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Ich gebe dir nicht schuld/ du Ausbund schlimmer Frauen/ ", "tokens": ["Ich", "ge\u00b7be", "dir", "nicht", "schuld", "/", "du", "Aus\u00b7bund", "schlim\u00b7mer", "Frau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "ADJD", "$(", "PPER", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df du den Witz gebrauchst/ wozu dir n\u00f6thig scheint.", "tokens": ["Da\u00df", "du", "den", "Witz", "ge\u00b7brauchst", "/", "wo\u00b7zu", "dir", "n\u00f6\u00b7thig", "scheint", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$(", "PWAV", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Veracht dich iederman/ ich bin dir drum nicht feind;", "tokens": ["Ver\u00b7acht", "dich", "ie\u00b7der\u00b7man", "/", "ich", "bin", "dir", "drum", "nicht", "feind", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIS", "$(", "PPER", "VAFIN", "PPER", "PAV", "PTKNEG", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mein Leben wolt ich dir in deine Schos vertrauen.", "tokens": ["Mein", "Le\u00b7ben", "wolt", "ich", "dir", "in", "dei\u00b7ne", "Schos", "ver\u00b7trau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ulyssens kluge List halff ja bey Troja bauen/", "tokens": ["U\u00b7lys\u00b7sens", "klu\u00b7ge", "List", "halff", "ja", "bey", "Tro\u00b7ja", "bau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "VVFIN", "ADV", "APPR", "NE", "VVINF", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.6": {"text": "Warob der Grieche lacht und Hectors Erbe weint/", "tokens": ["Wa\u00b7rob", "der", "Grie\u00b7che", "lacht", "und", "Hec\u00b7tors", "Er\u00b7be", "weint", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "KON", "NE", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Was das gemeine Volck nicht recht zu seyn vermeint/", "tokens": ["Was", "das", "ge\u00b7mei\u00b7ne", "Volck", "nicht", "recht", "zu", "seyn", "ver\u00b7meint", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "PTKNEG", "ADJD", "PTKZU", "VAINF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Das mn\u00df man stets bey Hoff\u2019 in vollem Lauffe schauen.", "tokens": ["Das", "mn\u00df", "man", "stets", "bey", "Hoff'", "in", "vol\u00b7lem", "Lauf\u00b7fe", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PIS", "ADV", "APPR", "NE", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der Netze sind zu viel/ durch die man euch will bey/", "tokens": ["Der", "Net\u00b7ze", "sind", "zu", "viel", "/", "durch", "die", "man", "euch", "will", "bey", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "PIAT", "$(", "APPR", "PRELS", "PIS", "PPER", "VMFIN", "APPR", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Da\u00df/ wenn ihr nicht versteht/ wie einem Schalcke sey/", "tokens": ["Da\u00df", "/", "wenn", "ihr", "nicht", "ver\u00b7steht", "/", "wie", "ei\u00b7nem", "Schal\u00b7cke", "sey", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$(", "KOUS", "PPER", "PTKNEG", "VVFIN", "$(", "KOKOM", "ART", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und List mit List belohnt/ ihr schwerlich k\u00f6nt entrinnen.", "tokens": ["Und", "List", "mit", "List", "be\u00b7lohnt", "/", "ihr", "schwer\u00b7lich", "k\u00f6nt", "ent\u00b7rin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "VVPP", "$(", "PPER", "ADJD", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "So bleib nun wie du bist: denn/ red\u2019 ich dir gleich ein/", "tokens": ["So", "bleib", "nun", "wie", "du", "bist", ":", "denn", "/", "red'", "ich", "dir", "gleich", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "KOKOM", "PPER", "VAFIN", "$.", "KON", "$(", "VVFIN", "PPER", "PPER", "ADV", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "(wo du nicht anderst wilt mit Lust gefangen seyn:)", "tokens": ["(", "wo", "du", "nicht", "an\u00b7derst", "wilt", "mit", "Lust", "ge\u00b7fan\u00b7gen", "seyn", ":)"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "emoticon"], "pos": ["$(", "PWAV", "PPER", "PTKNEG", "ADV", "VMFIN", "APPR", "NN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Mau siehet doch den Fuchs nicht andre Haar gewinnen.", "tokens": ["Mau", "sie\u00b7het", "doch", "den", "Fuchs", "nicht", "and\u00b7re", "Haar", "ge\u00b7win\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "ART", "NE", "PTKNEG", "ADJA", "NN", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}}}}