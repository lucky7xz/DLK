{"dta.poem.8661": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Der Lenz .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Da kommt der Lenz, der sch\u00f6ne Junge,", "tokens": ["Da", "kommt", "der", "Lenz", ",", "der", "sch\u00f6\u00b7ne", "Jun\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den Alles lieben mu\u00df,", "tokens": ["Den", "Al\u00b7les", "lie\u00b7ben", "mu\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Herein mit einem Freudensprunge,", "tokens": ["Her\u00b7ein", "mit", "ei\u00b7nem", "Freu\u00b7den\u00b7sprun\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und l\u00e4chelt seinen Gru\u00df;", "tokens": ["Und", "l\u00e4\u00b7chelt", "sei\u00b7nen", "Gru\u00df", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und schickt sich gleich mit frohem Necken", "tokens": ["Und", "schickt", "sich", "gleich", "mit", "fro\u00b7hem", "Ne\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zu all' den Streichen an,", "tokens": ["Zu", "all'", "den", "Strei\u00b7chen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die er auch sonst dem alten Recken,", "tokens": ["Die", "er", "auch", "sonst", "dem", "al\u00b7ten", "Re\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Winter, angethan.", "tokens": ["Dem", "Win\u00b7ter", ",", "an\u00b7ge\u00b7than", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Er gibt sie frei die B\u00e4chlein alle,", "tokens": ["Er", "gibt", "sie", "frei", "die", "B\u00e4ch\u00b7lein", "al\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "ART", "NN", "PIS", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie auch der Alte schilt,", "tokens": ["Wie", "auch", "der", "Al\u00b7te", "schilt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die der in seiner Eisesfalle", "tokens": ["Die", "der", "in", "sei\u00b7ner", "Ei\u00b7ses\u00b7fal\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ART", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So streng gefangen hielt.", "tokens": ["So", "streng", "ge\u00b7fan\u00b7gen", "hielt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Schon zieh'n die Wellen flink von dannen", "tokens": ["Schon", "zieh'n", "die", "Wel\u00b7len", "flink", "von", "dan\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "VVFIN", "APPR", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit T\u00e4nzen und Geschw\u00e4tz,", "tokens": ["Mit", "T\u00e4n\u00b7zen", "und", "Ge\u00b7schw\u00e4tz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und sp\u00f6tteln \u00fcber des Tyrannen", "tokens": ["Und", "sp\u00f6t\u00b7teln", "\u00fc\u00b7ber", "des", "Ty\u00b7ran\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zerronnenes Gesetz.", "tokens": ["Zer\u00b7ron\u00b7ne\u00b7nes", "Ge\u00b7setz", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Den J\u00fcngling freut es, wie die raschen", "tokens": ["Den", "J\u00fcng\u00b7ling", "freut", "es", ",", "wie", "die", "ra\u00b7schen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "PWAV", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hinl\u00e4rmen durchs Gefild,", "tokens": ["Hin\u00b7l\u00e4r\u00b7men", "durchs", "Ge\u00b7fild", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und sich aus leichten Fingern haschen", "tokens": ["Und", "sich", "aus", "leich\u00b7ten", "Fin\u00b7gern", "ha\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PRF", "APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sein aufgebl\u00fchtes Bild.", "tokens": ["Sein", "auf\u00b7ge\u00b7bl\u00fch\u00b7tes", "Bild", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Froh l\u00e4chelt seine Mutter Erde", "tokens": ["Froh", "l\u00e4\u00b7chelt", "sei\u00b7ne", "Mut\u00b7ter", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nach ihrem langen Harm;", "tokens": ["Nach", "ih\u00b7rem", "lan\u00b7gen", "Harm", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie schlingt mit jubelnder Geberde", "tokens": ["Sie", "schlingt", "mit", "ju\u00b7beln\u00b7der", "Ge\u00b7ber\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.4": {"text": "Das S\u00f6hnlein in den Arm.", "tokens": ["Das", "S\u00f6hn\u00b7lein", "in", "den", "Arm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "In ihren Busen greift der Lose", "tokens": ["In", "ih\u00b7ren", "Bu\u00b7sen", "greift", "der", "Lo\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und zieht ihr schmeichelnd keck", "tokens": ["Und", "zieht", "ihr", "schmei\u00b7chelnd", "keck"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das sanfte Veilchen und die Rose", "tokens": ["Das", "sanf\u00b7te", "Veil\u00b7chen", "und", "die", "Ro\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hervor aus dem Versteck.", "tokens": ["Her\u00b7vor", "aus", "dem", "Ver\u00b7steck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Und sein geschmeidiges Gesinde", "tokens": ["Und", "sein", "ge\u00b7schmei\u00b7di\u00b7ges", "Ge\u00b7sin\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Schickt er zu Berg und Thal:", "tokens": ["Schickt", "er", "zu", "Berg", "und", "Thal", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u201esagt, da\u00df ich da bin, meine Winde,", "tokens": ["\u201e", "sagt", ",", "da\u00df", "ich", "da", "bin", ",", "mei\u00b7ne", "Win\u00b7de", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "KOUS", "PPER", "ADV", "VAFIN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Den Freunden allzumal!\u201c", "tokens": ["Den", "Freun\u00b7den", "all\u00b7zu\u00b7mal", "!", "\u201c"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADV", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Er zieht das Herz an Liebesketten", "tokens": ["Er", "zieht", "das", "Herz", "an", "Lie\u00b7bes\u00b7ket\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Rasch \u00fcber manche Kluft,", "tokens": ["Rasch", "\u00fc\u00b7ber", "man\u00b7che", "Kluft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und schleudert seine Singraketen,", "tokens": ["Und", "schleu\u00b7dert", "sei\u00b7ne", "Sin\u00b7gra\u00b7ke\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Lerchen, in die Luft.", "tokens": ["Die", "Ler\u00b7chen", ",", "in", "die", "Luft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}