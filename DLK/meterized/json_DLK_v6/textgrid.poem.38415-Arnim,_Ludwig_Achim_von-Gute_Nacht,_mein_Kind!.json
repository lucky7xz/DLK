{"textgrid.poem.38415": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Gute Nacht, mein Kind!", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Guten Abend, gute Nacht,", "tokens": ["Gu\u00b7ten", "A\u00b7bend", ",", "gu\u00b7te", "Nacht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit Rosen bedacht,", "tokens": ["Mit", "Ro\u00b7sen", "be\u00b7dacht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Mit N\u00e4glein besteckt,", "tokens": ["Mit", "N\u00e4g\u00b7lein", "be\u00b7steckt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Schlupf' unter die Deck,", "tokens": ["Schlupf'", "un\u00b7ter", "die", "Deck", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Morgen fr\u00fch, wenns Gott will,", "tokens": ["Mor\u00b7gen", "fr\u00fch", ",", "wenns", "Gott", "will", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "KOUS", "NN", "VMFIN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Wirst du wieder geweckt.", "tokens": ["Wirst", "du", "wie\u00b7der", "ge\u00b7weckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}}}}