{"dta.poem.7947": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Auf den Tod eines Landgeistlichen .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Bleibt abgeschiednen Geistern die Gewalt,", "tokens": ["Bleibt", "ab\u00b7ge\u00b7schied\u00b7nen", "Geis\u00b7tern", "die", "Ge\u00b7walt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Zu kehren nach dem ird\u2019schen Aufenthalt,", "tokens": ["Zu", "keh\u00b7ren", "nach", "dem", "ird'\u00b7schen", "Auf\u00b7ent\u00b7halt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So kehrest du nicht in der Mondennacht,", "tokens": ["So", "keh\u00b7rest", "du", "nicht", "in", "der", "Mon\u00b7den\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wann nur die Sehnsucht und die Schwermuth wacht.", "tokens": ["Wann", "nur", "die", "Sehn\u00b7sucht", "und", "die", "Schwer\u00b7muth", "wacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "KON", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Nein! wann ein Sommermorgen niedersteigt,", "tokens": ["Nein", "!", "wann", "ein", "Som\u00b7mer\u00b7mor\u00b7gen", "nie\u00b7ders\u00b7teigt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PWAV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Wo sich im weiten Blau kein W\u00f6lkchen zeigt,", "tokens": ["Wo", "sich", "im", "wei\u00b7ten", "Blau", "kein", "W\u00f6lk\u00b7chen", "zeigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "APPRART", "ADJA", "NN", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wo hoch und golden sich die Ernte hebt,", "tokens": ["Wo", "hoch", "und", "gol\u00b7den", "sich", "die", "Ern\u00b7te", "hebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "KON", "ADJA", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Mit rothen, blauen Blumen hell durchwebt,", "tokens": ["Mit", "ro\u00b7then", ",", "blau\u00b7en", "Blu\u00b7men", "hell", "durch\u00b7webt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Dann wandelst du, wie einst, durch das Gefild", "tokens": ["Dann", "wan\u00b7delst", "du", ",", "wie", "einst", ",", "durch", "das", "Ge\u00b7fild"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "ADV", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Und gr\u00fc\u00dfest jeden Schnitter freundlich mild.", "tokens": ["Und", "gr\u00fc\u00b7\u00dfest", "je\u00b7den", "Schnit\u00b7ter", "freund\u00b7lich", "mild", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}