{"textgrid.poem.45540": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Theologische Politik", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hat Humayan und Konkordat", "tokens": ["Hat", "Hu\u00b7ma\u00b7yan", "und", "Kon\u00b7kor\u00b7dat"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "NE", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ver\u00e4ndern nicht an Zahlenwert", "tokens": ["Ver\u00b7\u00e4n\u00b7dern", "nicht", "an", "Zah\u00b7len\u00b7wert"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das europ\u00e4ische Konzert.", "tokens": ["Das", "eu\u00b7ro\u00b7p\u00e4\u00b7i\u00b7sche", "Kon\u00b7zert", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Tritt ein der T\u00fcrke durch die erste T\u00fcr,", "tokens": ["Tritt", "ein", "der", "T\u00fcr\u00b7ke", "durch", "die", "ers\u00b7te", "T\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Tritt \u00d6streich durch die zweite aus daf\u00fcr.", "tokens": ["Tritt", "\u00d6s\u00b7treich", "durch", "die", "zwei\u00b7te", "aus", "da\u00b7f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "ADJA", "APPR", "PAV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}