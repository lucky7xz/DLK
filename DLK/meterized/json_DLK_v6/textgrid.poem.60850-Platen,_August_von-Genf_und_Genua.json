{"textgrid.poem.60850": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Genf und Genua", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zwei Freistaaten begrenzten den garstigen Staat, und sie sahn sich", "tokens": ["Zwei", "Frei\u00b7staa\u00b7ten", "be\u00b7grenz\u00b7ten", "den", "gars\u00b7ti\u00b7gen", "Staat", ",", "und", "sie", "sahn", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["CARD", "NN", "VVFIN", "ART", "ADJA", "NN", "$,", "KON", "PPER", "VVFIN", "PRF"], "meter": "-++--+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Durch die Despoten Turins bitter geha\u00dft und bek\u00e4mpft.", "tokens": ["Durch", "die", "Des\u00b7po\u00b7ten", "Tu\u00b7rins", "bit\u00b7ter", "ge\u00b7ha\u00dft", "und", "be\u00b7k\u00e4mpft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJD", "VVPP", "KON", "VVFIN", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Aber sie trotzten den t\u00fcckischen stets; blo\u00df Genua sank nun", "tokens": ["A\u00b7ber", "sie", "trotz\u00b7ten", "den", "t\u00fc\u00b7cki\u00b7schen", "stets", ";", "blo\u00df", "Ge\u00b7nua", "sank", "nun"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "ADV", "$.", "ADV", "NE", "VVFIN", "ADV"], "meter": "+--+--++-+--+-+", "measure": "diphilius"}, "line.4": {"text": "Unter das Joch schuldlos, dank dem bewu\u00dften Kongre\u00df!", "tokens": ["Un\u00b7ter", "das", "Joch", "schuld\u00b7los", ",", "dank", "dem", "be\u00b7wu\u00df\u00b7ten", "Kon\u00b7gre\u00df", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "$,", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}}}}}