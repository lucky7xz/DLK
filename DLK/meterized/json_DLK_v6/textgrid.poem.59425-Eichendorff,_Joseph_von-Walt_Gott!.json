{"textgrid.poem.59425": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Walt Gott!", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gestern st\u00fcrmt's noch, und am Morgen", "tokens": ["Ge\u00b7stern", "st\u00fcrmt's", "noch", ",", "und", "am", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADV", "$,", "KON", "APPRART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Bl\u00fchet schon das ganze Land \u2013", "tokens": ["Bl\u00fc\u00b7het", "schon", "das", "gan\u00b7ze", "Land", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Will auch nicht f\u00fcr morgen sorgen,", "tokens": ["Will", "auch", "nicht", "f\u00fcr", "mor\u00b7gen", "sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PTKNEG", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alles steht in Gottes Hand.", "tokens": ["Al\u00b7les", "steht", "in", "Got\u00b7tes", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Putz dich nur in Gold und Seiden:", "tokens": ["Putz", "dich", "nur", "in", "Gold", "und", "Sei\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In dem Felde \u00fcber Nacht", "tokens": ["In", "dem", "Fel\u00b7de", "\u00fc\u00b7ber", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Engel Gotts die Lilien kleiden,", "tokens": ["En\u00b7gel", "Gotts", "die", "Li\u00b7li\u00b7en", "klei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Sch\u00f6ner als du's je gedacht.", "tokens": ["Sch\u00f6\u00b7ner", "als", "du's", "je", "ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOUS", "PIS", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Sonn dich auf des Lebens Gipfeln:", "tokens": ["Sonn", "dich", "auf", "des", "Le\u00b7bens", "Gip\u00b7feln", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00dcber deinem stolzen Haus", "tokens": ["\u00dc\u00b7ber", "dei\u00b7nem", "stol\u00b7zen", "Haus"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Singt der Vogel in den Wipfeln,", "tokens": ["Singt", "der", "Vo\u00b7gel", "in", "den", "Wip\u00b7feln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schwingt sich \u00fcber dich hinaus!", "tokens": ["Schwingt", "sich", "\u00fc\u00b7ber", "dich", "hin\u00b7aus", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "V\u00f6gel nicht, noch Blumen sorgen,", "tokens": ["V\u00f6\u00b7gel", "nicht", ",", "noch", "Blu\u00b7men", "sor\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "$,", "ADV", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hat doch jedes sein Gewand \u2013", "tokens": ["Hat", "doch", "je\u00b7des", "sein", "Ge\u00b7wand", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PIAT", "PPOSAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie so fr\u00f6hlich rauscht der Morgen!", "tokens": ["Wie", "so", "fr\u00f6h\u00b7lich", "rauscht", "der", "Mor\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alles steht in Gottes Hand.", "tokens": ["Al\u00b7les", "steht", "in", "Got\u00b7tes", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}