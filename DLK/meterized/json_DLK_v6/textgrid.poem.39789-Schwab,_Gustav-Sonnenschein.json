{"textgrid.poem.39789": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Sonnenschein", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Alle Bl\u00fcte war verdorben", "tokens": ["Al\u00b7le", "Bl\u00fc\u00b7te", "war", "ver\u00b7dor\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "VAFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In der tr\u00fcben Regenzeit,", "tokens": ["In", "der", "tr\u00fc\u00b7ben", "Re\u00b7gen\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aller Sang war ausgestorben,", "tokens": ["Al\u00b7ler", "Sang", "war", "aus\u00b7ge\u00b7stor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Keine Freude weit und breit.", "tokens": ["Kei\u00b7ne", "Freu\u00b7de", "weit", "und", "breit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Was gespielt im Herzen munter,", "tokens": ["Was", "ge\u00b7spielt", "im", "Her\u00b7zen", "mun\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVPP", "APPRART", "NN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ganz erstarrt war Lieb' und Lust,", "tokens": ["Ganz", "er\u00b7starrt", "war", "Lieb'", "und", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Alle Lieder tauchten unter,", "tokens": ["Al\u00b7le", "Lie\u00b7der", "tauch\u00b7ten", "un\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "APPR", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wagten sich nicht aus der Brust.", "tokens": ["Wag\u00b7ten", "sich", "nicht", "aus", "der", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PTKNEG", "APPR", "ART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Auf der hohen Alpenkette,", "tokens": ["Auf", "der", "ho\u00b7hen", "Al\u00b7pen\u00b7ket\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo die sch\u00f6nen Burgen stehn,", "tokens": ["Wo", "die", "sch\u00f6\u00b7nen", "Bur\u00b7gen", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo die Sagen um die Wette", "tokens": ["Wo", "die", "Sa\u00b7gen", "um", "die", "Wet\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Durch die alten Tr\u00fcmmer gehn,", "tokens": ["Durch", "die", "al\u00b7ten", "Tr\u00fcm\u00b7mer", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sah man nur den Nebel heuer", "tokens": ["Sah", "man", "nur", "den", "Ne\u00b7bel", "heu\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "ADV", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und des Regens ew'gen Fall,", "tokens": ["Und", "des", "Re\u00b7gens", "ew'\u00b7gen", "Fall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "In dem moosigen Gem\u00e4uer", "tokens": ["In", "dem", "moo\u00b7si\u00b7gen", "Ge\u00b7m\u00e4u\u00b7er"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Schliefen die Geschichten all'.", "tokens": ["Schlie\u00b7fen", "die", "Ge\u00b7schich\u00b7ten", "all'", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PIAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Endlich scheint die Sommersonne", "tokens": ["End\u00b7lich", "scheint", "die", "Som\u00b7mer\u00b7son\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ueber ihr vergessnes Land,", "tokens": ["Ue\u00b7ber", "ihr", "ver\u00b7gess\u00b7nes", "Land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und zu ungewohnter Wonne", "tokens": ["Und", "zu", "un\u00b7ge\u00b7wohn\u00b7ter", "Won\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zieht es an sein Festgewand,", "tokens": ["Zieht", "es", "an", "sein", "Fest\u00b7ge\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Das mit Perlen ist bethauet,", "tokens": ["Das", "mit", "Per\u00b7len", "ist", "be\u00b7thau\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "NN", "VAFIN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Das mit Gold ist \u00fcberstickt;", "tokens": ["Das", "mit", "Gold", "ist", "\u00fc\u00b7bers\u00b7tickt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Weil der Himmel nicht mehr grauet,", "tokens": ["Weil", "der", "Him\u00b7mel", "nicht", "mehr", "grau\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Auch die Erde freundlich blickt.", "tokens": ["Auch", "die", "Er\u00b7de", "freund\u00b7lich", "blickt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Von den Burgen wehn die Sagen,", "tokens": ["Von", "den", "Bur\u00b7gen", "wehn", "die", "Sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In dem kleinen Liedermund", "tokens": ["In", "dem", "klei\u00b7nen", "Lie\u00b7der\u00b7mund"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bringt der Vogel sie getragen,", "tokens": ["Bringt", "der", "Vo\u00b7gel", "sie", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und der Dichter macht sie kund,", "tokens": ["Und", "der", "Dich\u00b7ter", "macht", "sie", "kund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Der die frischesten der Lieder", "tokens": ["Der", "die", "fri\u00b7sches\u00b7ten", "der", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "ART", "NN"], "meter": "---+--+-", "measure": "iambic.di.relaxed"}, "line.6": {"text": "Singt in seines Herzens Drang:", "tokens": ["Singt", "in", "sei\u00b7nes", "Her\u00b7zens", "Drang", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Was als Sonne scheinet nieder,", "tokens": ["Was", "als", "Son\u00b7ne", "schei\u00b7net", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "KOUS", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Keimet auf als Blum' und Klang.", "tokens": ["Kei\u00b7met", "auf", "als", "Blum'", "und", "Klang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "KOUS", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}