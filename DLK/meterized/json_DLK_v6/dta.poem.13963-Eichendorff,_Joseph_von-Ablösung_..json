{"dta.poem.13963": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Abl\u00f6sung .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Ge\u00f6ffnet bleibt ewig zum Feste", "tokens": ["Ge\u00b7\u00f6ff\u00b7net", "bleibt", "e\u00b7wig", "zum", "Fes\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADJD", "APPRART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Des Fr\u00fchlings lustiges Haus,", "tokens": ["Des", "Fr\u00fch\u00b7lings", "lus\u00b7ti\u00b7ges", "Haus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Es schw\u00e4rmen so wechselnd die G\u00e4ste", "tokens": ["Es", "schw\u00e4r\u00b7men", "so", "wech\u00b7selnd", "die", "G\u00e4s\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Da immer herein und heraus.", "tokens": ["Da", "im\u00b7mer", "her\u00b7ein", "und", "he\u00b7raus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}