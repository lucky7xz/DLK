{"textgrid.poem.65836": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "110", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach, wohl ist's wahr, ich schw\u00e4rmte her und hin,", "tokens": ["Ach", ",", "wohl", "ist's", "wahr", ",", "ich", "schw\u00e4rm\u00b7te", "her", "und", "hin", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "VAFIN", "ADJD", "$,", "PPER", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Bot mich der Welt zum Spielwerk; in die Seele", "tokens": ["Bot", "mich", "der", "Welt", "zum", "Spiel\u00b7werk", ";", "in", "die", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "$.", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Schnitt ich mir selbst, gab H\u00f6chstes wohlfeil hin;", "tokens": ["Schnitt", "ich", "mir", "selbst", ",", "gab", "H\u00f6chs\u00b7tes", "wohl\u00b7feil", "hin", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "$,", "VVFIN", "NE", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit neuen Trieben mehrt' ich alte Fehle.", "tokens": ["Mit", "neu\u00b7en", "Trie\u00b7ben", "mehrt'", "ich", "al\u00b7te", "Feh\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Sehr wahr ist's: fremd und schielend und bedingt", "tokens": ["Sehr", "wahr", "ist's", ":", "fremd", "und", "schie\u00b7lend", "und", "be\u00b7dingt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VAFIN", "$.", "ADJD", "KON", "ADJD", "KON", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Sah ich die Wahrheit. Doch, bei allen M\u00e4chten!", "tokens": ["Sah", "ich", "die", "Wahr\u00b7heit", ".", "Doch", ",", "bei", "al\u00b7len", "M\u00e4ch\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$.", "KON", "$,", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Dies Straucheln hat mein Herz mir nur verj\u00fcngt;", "tokens": ["Dies", "Strau\u00b7cheln", "hat", "mein", "Herz", "mir", "nur", "ver\u00b7j\u00fcngt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VAFIN", "PPOSAT", "NN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Dich besten Freund erprobt' ich unter Schlechten.", "tokens": ["Dich", "bes\u00b7ten", "Freund", "er\u00b7probt'", "ich", "un\u00b7ter", "Schlech\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Nun ist es alles, bis auf eins getan,", "tokens": ["Nun", "ist", "es", "al\u00b7les", ",", "bis", "auf", "eins", "ge\u00b7tan", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIS", "$,", "KOUS", "APPR", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Das ewig w\u00e4hrt. Nie kommt zu neuer Probe", "tokens": ["Das", "e\u00b7wig", "w\u00e4hrt", ".", "Nie", "kommt", "zu", "neu\u00b7er", "Pro\u00b7be"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADJD", "VVFIN", "$.", "ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Des alten Freundes mehr der Trieb mich an,", "tokens": ["Des", "al\u00b7ten", "Freun\u00b7des", "mehr", "der", "Trieb", "mich", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ART", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Des Liebesgottes, dem ich mich gelobe.", "tokens": ["Des", "Lie\u00b7bes\u00b7got\u00b7tes", ",", "dem", "ich", "mich", "ge\u00b7lo\u00b7be", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Gib n\u00e4chst dem Himmel denn die h\u00f6chste Lust,", "tokens": ["Gib", "n\u00e4chst", "dem", "Him\u00b7mel", "denn", "die", "h\u00f6chs\u00b7te", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "NN", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Den Willkomm mir an deiner liebsten Brust!", "tokens": ["Den", "Will\u00b7komm", "mir", "an", "dei\u00b7ner", "liebs\u00b7ten", "Brust", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}