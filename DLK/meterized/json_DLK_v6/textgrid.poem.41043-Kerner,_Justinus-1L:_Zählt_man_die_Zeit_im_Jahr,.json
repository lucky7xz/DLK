{"textgrid.poem.41043": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "1L: Z\u00e4hlt man die Zeit im Jahr,", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Z\u00e4hlt man die Zeit im Jahr,", "tokens": ["Z\u00e4hlt", "man", "die", "Zeit", "im", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Drin freudvoll war ein Herz,", "tokens": ["Drin", "freud\u00b7voll", "war", "ein", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sind's wen'ge Tage nur,", "tokens": ["Sin\u00b7d's", "wen'\u00b7ge", "Ta\u00b7ge", "nur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Die andern trug es Schmerz.", "tokens": ["Die", "an\u00b7dern", "trug", "es", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Z\u00e4hlt man die Zeit im Jahr,", "tokens": ["Z\u00e4hlt", "man", "die", "Zeit", "im", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Drin blau der Himmel blieb,", "tokens": ["Drin", "blau", "der", "Him\u00b7mel", "blieb", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sind's wen'ge Tage nur,", "tokens": ["Sin\u00b7d's", "wen'\u00b7ge", "Ta\u00b7ge", "nur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Die andern waren tr\u00fcb.", "tokens": ["Die", "an\u00b7dern", "wa\u00b7ren", "tr\u00fcb", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Drum, da der Himmel selbst", "tokens": ["Drum", ",", "da", "der", "Him\u00b7mel", "selbst"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PAV", "$,", "KOUS", "ART", "NN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "So oft in Tr\u00e4nen steht,", "tokens": ["So", "oft", "in", "Tr\u00e4\u00b7nen", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Klag' nimmer, Menschenherz,", "tokens": ["Klag'", "nim\u00b7mer", ",", "Men\u00b7schen\u00b7herz", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NN", "ADV", "$,", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df dir's nicht besser geht.", "tokens": ["Da\u00df", "dir's", "nicht", "bes\u00b7ser", "geht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PTKNEG", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}