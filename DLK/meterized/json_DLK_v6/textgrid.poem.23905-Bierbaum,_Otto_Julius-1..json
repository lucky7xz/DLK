{"textgrid.poem.23905": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was ist mein Schatz? \u2013 Eine Pl\u00e4ttmamsell.", "tokens": ["Was", "ist", "mein", "Schatz", "?", "\u2013", "Ei\u00b7ne", "Pl\u00e4tt\u00b7mam\u00b7sell", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPOSAT", "NN", "$.", "$(", "ART", "NN", "$."], "meter": "+--++-+-+", "measure": "dactylic.init"}, "line.2": {"text": "Wo wohnt sie? \u2013 Unten am Gries.", "tokens": ["Wo", "wohnt", "sie", "?", "\u2013", "Un\u00b7ten", "am", "Gries", "."], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "$.", "$(", "ADV", "APPRART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Wo die Isar rauscht, wo die Br\u00fccke steht,", "tokens": ["Wo", "die", "I\u00b7sar", "rauscht", ",", "wo", "die", "Br\u00fc\u00b7cke", "steht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NE", "VVFIN", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wo die Wiese von flatternden Hemden weht:", "tokens": ["Wo", "die", "Wie\u00b7se", "von", "flat\u00b7tern\u00b7den", "Hem\u00b7den", "weht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Da liegt mein Paradies.", "tokens": ["Da", "liegt", "mein", "Pa\u00b7ra\u00b7dies", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Im allerkleinsten Hause drin,", "tokens": ["Im", "al\u00b7ler\u00b7kleins\u00b7ten", "Hau\u00b7se", "drin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit den Fensterl\u00e4den gr\u00fcn,", "tokens": ["Mit", "den", "Fens\u00b7ter\u00b7l\u00e4\u00b7den", "gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da steht mein Schatz am B\u00fcgelbret;", "tokens": ["Da", "steht", "mein", "Schatz", "am", "B\u00fc\u00b7gel\u00b7bret", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hoiho, wie sie hurtig den B\u00fcgelstahl dreht,", "tokens": ["Hoi\u00b7ho", ",", "wie", "sie", "hur\u00b7tig", "den", "B\u00fc\u00b7gel\u00b7stahl", "dreht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "PPER", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "++--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Gott, wie die Backen gl\u00fchn.", "tokens": ["Gott", ",", "wie", "die", "Ba\u00b7cken", "gl\u00fchn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ART", "NN", "VVINF", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Im wei\u00dfen R\u00f6ckchen steht sie da,", "tokens": ["Im", "wei\u00b7\u00dfen", "R\u00f6ck\u00b7chen", "steht", "sie", "da", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihre Bluse ist blumig bunt;", "tokens": ["Ih\u00b7re", "Blu\u00b7se", "ist", "blu\u00b7mig", "bunt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "ADJD", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Kein Mieder schn\u00fcrt, was drunter sich regt,", "tokens": ["Kein", "Mie\u00b7der", "schn\u00fcrt", ",", "was", "drun\u00b7ter", "sich", "regt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$,", "PRELS", "PAV", "PRF", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Sich wellenwohlig weich bewegt,", "tokens": ["Sich", "wel\u00b7len\u00b7woh\u00b7lig", "weich", "be\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Br\u00fcste knospendes Rund.", "tokens": ["Der", "Br\u00fcs\u00b7te", "knos\u00b7pen\u00b7des", "Rund", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}