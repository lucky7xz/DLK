{"textgrid.poem.58195": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "233. Das lautere Gottmeinen", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer sich selber meint und liebet,", "tokens": ["Wer", "sich", "sel\u00b7ber", "meint", "und", "lie\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "ADV", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wird gest\u00f6ret, stets betr\u00fcbet", "tokens": ["Wird", "ge\u00b7st\u00f6\u00b7ret", ",", "stets", "be\u00b7tr\u00fc\u00b7bet"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["VAFIN", "VVPP", "$,", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und gedr\u00fcckt durch mancherlei;", "tokens": ["Und", "ge\u00b7dr\u00fcckt", "durch", "man\u00b7cher\u00b7lei", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "APPR", "PIS", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Meint dein Einfaltsaug' in allen", "tokens": ["Meint", "dein", "Ein\u00b7faltsaug'", "in", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "PIAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Nichts als Gott und sein Gefallen,", "tokens": ["Nichts", "als", "Gott", "und", "sein", "Ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "KOKOM", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bleibst du ruhig, froh und frei.", "tokens": ["Bleibst", "du", "ru\u00b7hig", ",", "froh", "und", "frei", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}