{"textgrid.poem.43028": {"metadata": {"author": {"name": "Aston, Louise", "birth": "N.A.", "death": "N.A."}, "title": "Den M\u00f6rdern Robert Blum's", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn einst der Freiheit Hymnen schallen,", "tokens": ["Wenn", "einst", "der", "Frei\u00b7heit", "Hym\u00b7nen", "schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Schwerter wieder rein von Blut;", "tokens": ["Die", "Schwer\u00b7ter", "wie\u00b7der", "rein", "von", "Blut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dann will ich zu der St\u00e4tte wallen,", "tokens": ["Dann", "will", "ich", "zu", "der", "St\u00e4t\u00b7te", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wo Robert Blum, der Edle, ruht;", "tokens": ["Wo", "Ro\u00b7bert", "Blum", ",", "der", "Ed\u00b7le", ",", "ruht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "NE", "NE", "$,", "ART", "ADJA", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dann schm\u00fcck' ich unter Thr\u00e4nen", "tokens": ["Dann", "schm\u00fcck", "ich", "un\u00b7ter", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "In einer stillen Nacht,", "tokens": ["In", "ei\u00b7ner", "stil\u00b7len", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wenn sie mich schlummernd w\u00e4hnen,", "tokens": ["Wenn", "sie", "mich", "schlum\u00b7mernd", "w\u00e4h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Sein Grab mit Blumenpracht.", "tokens": ["Sein", "Grab", "mit", "Blu\u00b7men\u00b7pracht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Was soll uns ", "tokens": ["Was", "soll", "uns"], "token_info": ["word", "word", "word"], "pos": ["PWS", "VMFIN", "PPER"], "meter": "++-", "measure": "unknown.measure.di"}, "line.2": {"text": "Mein Aug ist ", "tokens": ["Mein", "Aug", "ist"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Es wird der Tag des Kampfes kommen,", "tokens": ["Es", "wird", "der", "Tag", "des", "Kamp\u00b7fes", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Leier nicht \u2013 es gilt den Speer!", "tokens": ["Die", "Lei\u00b7er", "nicht", "\u2013", "es", "gilt", "den", "Speer", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "$(", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Aus jeder Todeswunde", "tokens": ["Aus", "je\u00b7der", "To\u00b7des\u00b7wun\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ein Gott der Rache spricht:", "tokens": ["Ein", "Gott", "der", "Ra\u00b7che", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Noch kennt ihr bis zur Stunde", "tokens": ["Noch", "kennt", "ihr", "bis", "zur", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Des Zornes Allmacht nicht!", "tokens": ["Des", "Zor\u00b7nes", "All\u00b7macht", "nicht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}