{"textgrid.poem.57535": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Heimatskl\u00e4nge", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Drei Kl\u00e4nge sind's vom Heimatsland,", "tokens": ["Drei", "Kl\u00e4n\u00b7ge", "sin\u00b7d's", "vom", "Hei\u00b7mats\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "APPRART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die ich schon lang nicht mehr geh\u00f6rt,", "tokens": ["Die", "ich", "schon", "lang", "nicht", "mehr", "ge\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Manch tr\u00fcbe Stunde schon entschwand,", "tokens": ["Manch", "tr\u00fc\u00b7be", "Stun\u00b7de", "schon", "ent\u00b7schwand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In der ich schmerzlich sie entbehrt;", "tokens": ["In", "der", "ich", "schmerz\u00b7lich", "sie", "ent\u00b7behrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Drei Kl\u00e4nge, s\u00fc\u00df wie Liebeslaut,", "tokens": ["Drei", "Kl\u00e4n\u00b7ge", ",", "s\u00fc\u00df", "wie", "Lie\u00b7bes\u00b7laut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "$,", "ADJD", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie sch\u00fcchtern Wort aus Kindermund \u2013", "tokens": ["Wie", "sch\u00fcch\u00b7tern", "Wort", "aus", "Kin\u00b7der\u00b7mund", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "APPR", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Bald wieder, wie zur n\u00e4cht'gen Stund,", "tokens": ["Bald", "wie\u00b7der", ",", "wie", "zur", "n\u00e4cht'\u00b7gen", "Stund", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "PWAV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das Wutgeheul der Sturmesbraut:", "tokens": ["Das", "Wut\u00b7ge\u00b7heul", "der", "Stur\u00b7mes\u00b7braut", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Du Rauschen in dem dunklen F\u00f6hr,", "tokens": ["Du", "Rau\u00b7schen", "in", "dem", "dunk\u00b7len", "F\u00f6hr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Du Wellenklang vom gr\u00fcnen See,", "tokens": ["Du", "Wel\u00b7len\u00b7klang", "vom", "gr\u00fc\u00b7nen", "See", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Du Lied aus Volksmund, wild und weh \u2013", "tokens": ["Du", "Lied", "aus", "Volks\u00b7mund", ",", "wild", "und", "weh", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPR", "NN", "$,", "ADJD", "KON", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Wer wei\u00df, ob ich euch nochmals h\u00f6r!", "tokens": ["Wer", "wei\u00df", ",", "ob", "ich", "euch", "noch\u00b7mals", "h\u00f6r", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "KOUS", "PPER", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "O Rauschen von dem Kiefernwald,", "tokens": ["O", "Rau\u00b7schen", "von", "dem", "Kie\u00b7fern\u00b7wald", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich hab' dich stets so lieb gehabt,", "tokens": ["Ich", "hab'", "dich", "stets", "so", "lieb", "ge\u00b7habt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "VAPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie hat's mein wildes Herz gelabt,", "tokens": ["Wie", "hat's", "mein", "wil\u00b7des", "Herz", "ge\u00b7labt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn des Piroles Fl\u00f6ten schallt,", "tokens": ["Wenn", "des", "Pi\u00b7ro\u00b7les", "Fl\u00f6\u00b7ten", "schallt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn ringsumher die Biene summt,", "tokens": ["Wenn", "rings\u00b7um\u00b7her", "die", "Bie\u00b7ne", "summt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sonst alles Leben ist verstummt \u2013", "tokens": ["Sonst", "al\u00b7les", "Le\u00b7ben", "ist", "ver\u00b7stummt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und andersmal beim Mondenschein,", "tokens": ["Und", "an\u00b7ders\u00b7mal", "beim", "Mon\u00b7den\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Bei Nachtwinds grellen Melodein,", "tokens": ["Bei", "Nacht\u00b7winds", "grel\u00b7len", "Me\u00b7lo\u00b7dein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Wenn's in den Kronen \u00e4chzt und kracht", "tokens": ["Wenn's", "in", "den", "Kro\u00b7nen", "\u00e4chzt", "und", "kracht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "VVFIN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und durchs Ge\u00e4st der Waldkauz lacht,", "tokens": ["Und", "durchs", "Ge\u00b7\u00e4st", "der", "Wald\u00b7kauz", "lacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "O Rauschen von dem dunklen F\u00f6hr,", "tokens": ["O", "Rau\u00b7schen", "von", "dem", "dunk\u00b7len", "F\u00f6hr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Wer wei\u00df, ob ich dich nochmals h\u00f6r!", "tokens": ["Wer", "wei\u00df", ",", "ob", "ich", "dich", "noch\u00b7mals", "h\u00f6r", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "KOUS", "PPER", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "O Wellenplaudern im Ger\u00f6hr,", "tokens": ["O", "Wel\u00b7len\u00b7plau\u00b7dern", "im", "Ge\u00b7r\u00f6hr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und Wogenklatschen an dem Strand!", "tokens": ["Und", "Wo\u00b7gen\u00b7klat\u00b7schen", "an", "dem", "Strand", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wer euch geh\u00f6rt, den l\u00e4\u00dft's nicht mehr,", "tokens": ["Wer", "euch", "ge\u00b7h\u00f6rt", ",", "den", "l\u00e4\u00dft's", "nicht", "mehr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "ART", "NE", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es h\u00e4lt f\u00fcr immer ihn gebannt.", "tokens": ["Es", "h\u00e4lt", "f\u00fcr", "im\u00b7mer", "ihn", "ge\u00b7bannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADV", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O traumhaft leises Abendlied,", "tokens": ["O", "traum\u00b7haft", "lei\u00b7ses", "A\u00b7ben\u00b7dlied", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie's murmelnd durch das R\u00f6hricht zieht,", "tokens": ["Wie's", "mur\u00b7melnd", "durch", "das", "R\u00f6h\u00b7richt", "zieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVPP", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Du liebes Lied der dunklen Flut,", "tokens": ["Du", "lie\u00b7bes", "Lied", "der", "dunk\u00b7len", "Flut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Begl\u00e4nzt von Abendsonnenglut \u2013", "tokens": ["Be\u00b7gl\u00e4nzt", "von", "A\u00b7bend\u00b7son\u00b7nen\u00b7glut", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Noch sch\u00f6ner, wenn die M\u00f6we gellt", "tokens": ["Noch", "sch\u00f6\u00b7ner", ",", "wenn", "die", "M\u00f6\u00b7we", "gellt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "KOUS", "ART", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und weiter Gischt am Strand zerschellt \u2013", "tokens": ["Und", "wei\u00b7ter", "Gischt", "am", "Strand", "zer\u00b7schellt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "APPRART", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "O Wellenrauschen, leis und schwer,", "tokens": ["O", "Wel\u00b7len\u00b7rau\u00b7schen", ",", "leis", "und", "schwer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Wer wei\u00df, ob ich dich nochmals h\u00f6r!", "tokens": ["Wer", "wei\u00df", ",", "ob", "ich", "dich", "noch\u00b7mals", "h\u00f6r", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "KOUS", "PPER", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "O Heimatslied aus Volkesmund,", "tokens": ["O", "Hei\u00b7mats\u00b7lied", "aus", "Vol\u00b7kes\u00b7mund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So schneidighell wie Schwerterklang,", "tokens": ["So", "schnei\u00b7dig\u00b7hell", "wie", "Schwer\u00b7ter\u00b7klang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So k\u00fchn, wie's je in hei\u00dfer Stund'", "tokens": ["So", "k\u00fchn", ",", "wie's", "je", "in", "hei\u00b7\u00dfer", "Stund'"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aus starken M\u00e4nnerkehlen drang;", "tokens": ["Aus", "star\u00b7ken", "M\u00e4n\u00b7ner\u00b7keh\u00b7len", "drang", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Bald zarter Liebe Leid und Lust,", "tokens": ["Bald", "zar\u00b7ter", "Lie\u00b7be", "Leid", "und", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der eignen Sch\u00f6nheit unbewu\u00dft,", "tokens": ["Der", "eig\u00b7nen", "Sch\u00f6n\u00b7heit", "un\u00b7be\u00b7wu\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Bald d\u00e4mmrigschauernd Ammenlied,", "tokens": ["Bald", "d\u00e4mm\u00b7rig\u00b7schau\u00b7ernd", "Am\u00b7men\u00b7lied", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So gleichbewegt wie Glockenton,", "tokens": ["So", "gleich\u00b7be\u00b7wegt", "wie", "Glo\u00b7cken\u00b7ton", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KOKOM", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Vom Abglanz alter Zeit durchgl\u00fcht,", "tokens": ["Vom", "Ab\u00b7glanz", "al\u00b7ter", "Zeit", "durch\u00b7gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Vom Volke fast vergessen schon \u2013", "tokens": ["Vom", "Vol\u00b7ke", "fast", "ver\u00b7ges\u00b7sen", "schon", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVFIN", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Ihr Lieder, wild und wehmutsschwer,", "tokens": ["Ihr", "Lie\u00b7der", ",", "wild", "und", "weh\u00b7muts\u00b7schwer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Wer wei\u00df, ob ich euch nochmals h\u00f6r!", "tokens": ["Wer", "wei\u00df", ",", "ob", "ich", "euch", "noch\u00b7mals", "h\u00f6r", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "KOUS", "PPER", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Nach Osten zieht's mich m\u00e4chtig hin,", "tokens": ["Nach", "Os\u00b7ten", "zieht's", "mich", "m\u00e4ch\u00b7tig", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbnach Hause\u00ab klingt's in meinem Sinn:", "tokens": ["\u00bb", "nach", "Hau\u00b7se", "\u00ab", "klingt's", "in", "mei\u00b7nem", "Sinn", ":"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "NN", "$(", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Drei Kl\u00e4nge sind's vom Heimatsland,", "tokens": ["Drei", "Kl\u00e4n\u00b7ge", "sin\u00b7d's", "vom", "Hei\u00b7mats\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "APPRART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die haben mir das Herz entwandt;", "tokens": ["Die", "ha\u00b7ben", "mir", "das", "Herz", "ent\u00b7wandt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es ist schon lange nicht mehr mein,", "tokens": ["Es", "ist", "schon", "lan\u00b7ge", "nicht", "mehr", "mein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "PTKNEG", "ADV", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Es findet nur zu Hause Ruh:", "tokens": ["Es", "fin\u00b7det", "nur", "zu", "Hau\u00b7se", "Ruh", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "\u00bbnur einmal in der Heim