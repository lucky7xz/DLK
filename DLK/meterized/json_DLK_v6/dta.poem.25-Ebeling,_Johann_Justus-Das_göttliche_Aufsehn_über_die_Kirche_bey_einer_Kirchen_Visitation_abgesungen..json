{"dta.poem.25": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Das  \n  g\u00f6ttliche Aufsehn  \n \u00fcber die Kirche  \n bey einer Kirchen Visitation abgesungen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198797", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Vorsicht Auge schlummert nicht", "tokens": ["Der", "Vor\u00b7sicht", "Au\u00b7ge", "schlum\u00b7mert", "nicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das \u00fcber Zion wacht;", "tokens": ["Das", "\u00fc\u00b7ber", "Zi\u00b7on", "wacht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "NE", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Weil es durch Finsternis und Nacht", "tokens": ["Weil", "es", "durch", "Fins\u00b7ter\u00b7nis", "und", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit seinen Strahlen bricht.", "tokens": ["Mit", "sei\u00b7nen", "Strah\u00b7len", "bricht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der H\u00f6chste will sein Zion stets erfreun,", "tokens": ["Der", "H\u00f6chs\u00b7te", "will", "sein", "Zi\u00b7on", "stets", "er\u00b7freun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPOSAT", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Er sucht es heim, bekr\u00f6net es mit gn\u00e4digen Gedein,", "tokens": ["Er", "sucht", "es", "heim", ",", "be\u00b7kr\u00f6\u00b7net", "es", "mit", "gn\u00e4\u00b7di\u00b7gen", "Ge\u00b7dein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Er l\u00e4sset pflanzen, und durch das Begiessen,", "tokens": ["Er", "l\u00e4s\u00b7set", "pflan\u00b7zen", ",", "und", "durch", "das", "Be\u00b7gies\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVINF", "$,", "KON", "APPR", "ART", "NN", "$,"], "meter": "-+-+--++-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Den Seegen spriessen.", "tokens": ["Den", "See\u00b7gen", "spries\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Soll die Kirch in Seegen bl\u00fchen;", "tokens": ["Soll", "die", "Kirch", "in", "See\u00b7gen", "bl\u00fc\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "So mu\u00df ", "tokens": ["So", "mu\u00df"], "token_info": ["word", "word"], "pos": ["ADV", "VMFIN"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Und bey seiner Vorsicht Walten", "tokens": ["Und", "bey", "sei\u00b7ner", "Vor\u00b7sicht", "Wal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Durch die Lehrer sie erhalten:", "tokens": ["Durch", "die", "Leh\u00b7rer", "sie", "er\u00b7hal\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn auf deren treu Bem\u00fchen", "tokens": ["Denn", "auf", "de\u00b7ren", "treu", "Be\u00b7m\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PRELAT", "ADJD", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Folgt noch immer das Gedein.", "tokens": ["Folgt", "noch", "im\u00b7mer", "das", "Ge\u00b7dein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Gott liebt die Seinen,", "tokens": ["Gott", "liebt", "die", "Sei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "PPOSS", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Und l\u00e4st sein Wort zur Lehr, zum Leben,", "tokens": ["Und", "l\u00e4st", "sein", "Wort", "zur", "Lehr", ",", "zum", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wie eine Leuchte scheinen;", "tokens": ["Wie", "ei\u00b7ne", "Leuch\u00b7te", "schei\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Er wird auch ferner geben", "tokens": ["Er", "wird", "auch", "fer\u00b7ner", "ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Von seinen heilgen H\u00f6hen", "tokens": ["Von", "sei\u00b7nen", "heil\u00b7gen", "H\u00f6\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Da\u00df wir davon noch reiche Fr\u00fcchte sehen.", "tokens": ["Da\u00df", "wir", "da\u00b7von", "noch", "rei\u00b7che", "Fr\u00fcch\u00b7te", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PAV", "ADV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Es bl\u00fcht zwar leider \u00fcberall", "tokens": ["Es", "bl\u00fcht", "zwar", "lei\u00b7der", "\u00fc\u00b7be\u00b7rall"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das Unkraut, das der Feind auf ", "tokens": ["Das", "Un\u00b7kraut", ",", "das", "der", "Feind", "auf"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "APPR"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Allein getrost! es liegt der H\u00f6llen Belial,", "tokens": ["Al\u00b7lein", "ge\u00b7trost", "!", "es", "liegt", "der", "H\u00f6l\u00b7len", "Be\u00b7li\u00b7al", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$.", "PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und JEsus der ihn st\u00fcrzt, der siegt", "tokens": ["Und", "Je\u00b7sus", "der", "ihn", "st\u00fcrzt", ",", "der", "siegt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "NE", "ART", "PPER", "VVFIN", "$,", "PRELS", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "In seiner Majest\u00e4t", "tokens": ["In", "sei\u00b7ner", "Ma\u00b7jes\u00b7t\u00e4t"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Und dadurch wird zulezt das Unkraut doch bekriegt", "tokens": ["Und", "da\u00b7durch", "wird", "zu\u00b7lezt", "das", "Un\u00b7kraut", "doch", "be\u00b7kriegt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PAV", "VAFIN", "ADV", "ART", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Die Erndte kommt, da wird der Weizen herrlich", "tokens": ["Die", "Ernd\u00b7te", "kommt", ",", "da", "wird", "der", "Wei\u00b7zen", "herr\u00b7lich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "ADV", "VAFIN", "ART", "NN", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Das Unkraut in dem Feur das ewig brennt, vergehen.", "tokens": ["Das", "Un\u00b7kraut", "in", "dem", "Feur", "das", "e\u00b7wig", "brennt", ",", "ver\u00b7ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "ART", "ADJD", "VVFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "O! segne Heiland deine Lehren,", "tokens": ["O", "!", "seg\u00b7ne", "Hei\u00b7land", "dei\u00b7ne", "Leh\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und gib zum Worte ferner Kraft;", "tokens": ["Und", "gib", "zum", "Wor\u00b7te", "fer\u00b7ner", "Kraft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "APPRART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O! la\u00df die Jugend, la\u00df die Alten,", "tokens": ["O", "!", "la\u00df", "die", "Ju\u00b7gend", ",", "la\u00df", "die", "Al\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VVIMP", "ART", "NN", "$,", "VVIMP", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sich woll in Christenthum verhalten;", "tokens": ["Sich", "woll", "in", "Chris\u00b7ten\u00b7thum", "ver\u00b7hal\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und gib da\u00df wir uns erst bekehren,", "tokens": ["Und", "gib", "da\u00df", "wir", "uns", "erst", "be\u00b7keh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "KOUS", "PPER", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Eh uns der Tod von hinnen raft!", "tokens": ["Eh", "uns", "der", "Tod", "von", "hin\u00b7nen", "raft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}