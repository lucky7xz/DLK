{"dta.poem.21488": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Am Morgen.  \n 1884.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Fahler Morgenglanz,", "tokens": ["Fah\u00b7ler", "Mor\u00b7gen\u00b7glanz", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Graues D\u00e4mmerlicht,", "tokens": ["Grau\u00b7es", "D\u00e4m\u00b7mer\u00b7licht", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Und im Spiegel dort", "tokens": ["Und", "im", "Spie\u00b7gel", "dort"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "ADV"], "meter": "--+-+", "measure": "anapaest.init"}, "line.4": {"text": "Starrt mein Angesicht.", "tokens": ["Starrt", "mein", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Von dem letzten Ku\u00df", "tokens": ["Von", "dem", "letz\u00b7ten", "Ku\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "--+-+", "measure": "anapaest.init"}, "line.2": {"text": "Bebt mein Mund noch bang,", "tokens": ["Bebt", "mein", "Mund", "noch", "bang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "ADV", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Horch, noch t\u00f6nt sein Schritt", "tokens": ["Horch", ",", "noch", "t\u00f6nt", "sein", "Schritt"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "ADV", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Dumpf hinab den Gang.", "tokens": ["Dumpf", "hin\u00b7ab", "den", "Gang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Auf der Treppe knirscht", "tokens": ["Auf", "der", "Trep\u00b7pe", "knirscht"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Leise noch sein Fu\u00df,", "tokens": ["Lei\u00b7se", "noch", "sein", "Fu\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Schwer die Th\u00fcre f\u00e4llt", "tokens": ["Schwer", "die", "Th\u00fc\u00b7re", "f\u00e4llt"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "ART", "NN", "VVFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Wie ein Todesgru\u00df.", "tokens": ["Wie", "ein", "To\u00b7des\u00b7gru\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Wie ein Todesgru\u00df!", "tokens": ["Wie", "ein", "To\u00b7des\u00b7gru\u00df", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Und der Traum zerrinnt \u2026", "tokens": ["Und", "der", "Traum", "zer\u00b7rinnt", "\u2026"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVPP", "$("], "meter": "--+-+", "measure": "anapaest.init"}, "line.3": {"text": "In die hei\u00dfe Nacht", "tokens": ["In", "die", "hei\u00b7\u00dfe", "Nacht"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "St\u00f6hnt der Morgenwind.", "tokens": ["St\u00f6hnt", "der", "Mor\u00b7gen\u00b7wind", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Eben noch so reich", "tokens": ["E\u00b7ben", "noch", "so", "reich"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADV", "ADJD"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "An verliebter Gluth,", "tokens": ["An", "ver\u00b7lieb\u00b7ter", "Gluth", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Jetzt so arm und leer,", "tokens": ["Jetzt", "so", "arm", "und", "leer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Und verst\u00f6rt mein Muth.", "tokens": ["Und", "ver\u00b7st\u00f6rt", "mein", "Muth", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Thr\u00e4nenna\u00df mein Blick,", "tokens": ["Thr\u00e4\u00b7nen\u00b7na\u00df", "mein", "Blick", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Und mein Kopf so schwer, \u2014", "tokens": ["Und", "mein", "Kopf", "so", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "ADJD", "$,", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Alles gab ich hin,", "tokens": ["Al\u00b7les", "gab", "ich", "hin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Und ich hab\u2019 nichts mehr.", "tokens": ["Und", "ich", "hab'", "nichts", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PIS", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Und bes\u00e4\u00df ich\u2019s noch,", "tokens": ["Und", "be\u00b7s\u00e4\u00df", "ich's", "noch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "ADV", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Wieder g\u00e4b\u2019 ich\u2019s dir,", "tokens": ["Wie\u00b7der", "g\u00e4b'", "ich's", "dir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PPER", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Tr\u00e4f dein Liebesku\u00df", "tokens": ["Tr\u00e4f", "dein", "Lie\u00b7bes\u00b7ku\u00df"], "token_info": ["word", "word", "word"], "pos": ["NN", "PPOSAT", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Mund und Seele mir.", "tokens": ["Mund", "und", "See\u00b7le", "mir", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Dennoch wei\u00df ich\u2019s wohl,", "tokens": ["Den\u00b7noch", "wei\u00df", "ich's", "wohl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Aus den Nebeln dort", "tokens": ["Aus", "den", "Ne\u00b7beln", "dort"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADV"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Webt in meinen Tag", "tokens": ["Webt", "in", "mei\u00b7nen", "Tag"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Tod und Schmach sich fort.", "tokens": ["Tod", "und", "Schmach", "sich", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PRF", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.9": {"line.1": {"text": "Finster starrt mich an", "tokens": ["Fins\u00b7ter", "starrt", "mich", "an"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "PTKVZ"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Ein Medusenhaupt,", "tokens": ["Ein", "Me\u00b7du\u00b7sen\u00b7haupt", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Meine Zukunft du,", "tokens": ["Mei\u00b7ne", "Zu\u00b7kunft", "du", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Schlangenwirrumlaubt.", "tokens": ["Schlan\u00b7gen\u00b7wir\u00b7rum\u00b7laubt", "."], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.10": {"line.1": {"text": "Zu so wenig Lust,", "tokens": ["Zu", "so", "we\u00b7nig", "Lust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PIAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "So viel Leid erkorn \u2014", "tokens": ["So", "viel", "Leid", "er\u00b7korn"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVINF", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Mutter, Fluch auf dich,", "tokens": ["Mut\u00b7ter", ",", "Fluch", "auf", "dich", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "APPR", "PPER", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Da\u00df du mich geborn!", "tokens": ["Da\u00df", "du", "mich", "ge\u00b7born", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.11": {"line.1": {"text": "Fluch auf dich, du Welt,", "tokens": ["Fluch", "auf", "dich", ",", "du", "Welt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "$,", "PPER", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Die so rasch verdammt,", "tokens": ["Die", "so", "rasch", "ver\u00b7dammt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Was durch die Natur", "tokens": ["Was", "durch", "die", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "APPR", "ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Ringsum gluthend flammt.", "tokens": ["Ring\u00b7sum", "glu\u00b7thend", "flammt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.12": {"line.1": {"text": "Liebe, du allein", "tokens": ["Lie\u00b7be", ",", "du", "al\u00b7lein"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "PPER", "ADV"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Rette du dein Kind,", "tokens": ["Ret\u00b7te", "du", "dein", "Kind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Streif mit deinem Mund", "tokens": ["Streif", "mit", "dei\u00b7nem", "Mund"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Meine Lippen lind.", "tokens": ["Mei\u00b7ne", "Lip\u00b7pen", "lind", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.13": {"line.1": {"text": "La\u00df mich einmal ruhen", "tokens": ["La\u00df", "mich", "ein\u00b7mal", "ru\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Noch in deinem Schoo\u00df,", "tokens": ["Noch", "in", "dei\u00b7nem", "Schoo\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Komme in mein Herz", "tokens": ["Kom\u00b7me", "in", "mein", "Herz"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Leuchtend, sch\u00f6n und gro\u00df.", "tokens": ["Leuch\u00b7tend", ",", "sch\u00f6n", "und", "gro\u00df", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.14": {"line.1": {"text": "Komme wie du willst,", "tokens": ["Kom\u00b7me", "wie", "du", "willst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "PPER", "VMFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Wie das Morgenroth,", "tokens": ["Wie", "das", "Mor\u00b7gen\u00b7roth", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Komm\u2019 in Nacht und Sturm", "tokens": ["Komm'", "in", "Nacht", "und", "Sturm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "APPR", "NN", "KON", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Gleich dem W\u00fcrger-Tod.", "tokens": ["Gleich", "dem", "W\u00fcr\u00b7ger\u00b7Tod", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.15": {"line.1": {"text": "Bleicht im Morgenglanz,", "tokens": ["Bleicht", "im", "Mor\u00b7gen\u00b7glanz", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Rothe Rosen ihr, \u2014", "tokens": ["Ro\u00b7the", "Ro\u00b7sen", "ihr", ","], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "PPER", "$,", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Liebe, bett\u2019 ein Grab", "tokens": ["Lie\u00b7be", ",", "bett'", "ein", "Grab"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "ART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Unter Rosen mir!", "tokens": ["Un\u00b7ter", "Ro\u00b7sen", "mir", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}