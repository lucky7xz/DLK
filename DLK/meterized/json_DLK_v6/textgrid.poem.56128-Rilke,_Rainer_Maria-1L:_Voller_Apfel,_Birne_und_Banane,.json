{"textgrid.poem.56128": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "1L: Voller Apfel, Birne und Banane,", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Voller Apfel, Birne und Banane,", "tokens": ["Vol\u00b7ler", "Ap\u00b7fel", ",", "Bir\u00b7ne", "und", "Ba\u00b7na\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Stachelbeere... Alles dieses spricht", "tokens": ["Sta\u00b7chel\u00b7bee\u00b7re", "...", "Al\u00b7les", "die\u00b7ses", "spricht"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$(", "PIS", "PDAT", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Tod und Leben in den Mund... Ich ahne...", "tokens": ["Tod", "und", "Le\u00b7ben", "in", "den", "Mund", "...", "Ich", "ah\u00b7ne", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "ART", "NN", "$(", "PPER", "VVFIN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Lest es einem Kind vom Angesicht,", "tokens": ["Lest", "es", "ei\u00b7nem", "Kind", "vom", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "wenn es sie erschmeckt. Dies kommt von weit.", "tokens": ["wenn", "es", "sie", "er\u00b7schmeckt", ".", "Dies", "kommt", "von", "weit", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVPP", "$.", "PDS", "VVFIN", "APPR", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Wird euch langsam namenlos im Munde?", "tokens": ["Wird", "euch", "lang\u00b7sam", "na\u00b7men\u00b7los", "im", "Mun\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "ADJD", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Wo sonst Worte waren, flie\u00dfen Funde,", "tokens": ["Wo", "sonst", "Wor\u00b7te", "wa\u00b7ren", ",", "flie\u00b7\u00dfen", "Fun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADV", "NN", "VAFIN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "aus dem Fruchtfleisch \u00fcberrascht befreit.", "tokens": ["aus", "dem", "Frucht\u00b7fleisch", "\u00fc\u00b7berr\u00b7ascht", "be\u00b7freit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Wagt zu sagen, was ihr Apfel nennt.", "tokens": ["Wagt", "zu", "sa\u00b7gen", ",", "was", "ihr", "Ap\u00b7fel", "nennt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKZU", "VVINF", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Diese S\u00fc\u00dfe, die sich erst verdichtet,", "tokens": ["Die\u00b7se", "S\u00fc\u00b7\u00dfe", ",", "die", "sich", "erst", "ver\u00b7dich\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "PRELS", "PRF", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "um, im Schmecken leise aufgerichtet,", "tokens": ["um", ",", "im", "Schme\u00b7cken", "lei\u00b7se", "auf\u00b7ge\u00b7rich\u00b7tet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "$,", "APPRART", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "klar zu werden, wach und transparent,", "tokens": ["klar", "zu", "wer\u00b7den", ",", "wach", "und", "trans\u00b7pa\u00b7rent", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKZU", "VAINF", "$,", "ADJD", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "doppeldeutig, sonnig, erdig, hiesig \u2013:", "tokens": ["dop\u00b7pel\u00b7deu\u00b7tig", ",", "son\u00b7nig", ",", "er\u00b7dig", ",", "hie\u00b7sig", "\u2013", ":"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,", "ADJD", "$,", "ADJD", "$(", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "O Erfahrung, F\u00fchlung, Freude \u2013, riesig!", "tokens": ["O", "Er\u00b7fah\u00b7rung", ",", "F\u00fch\u00b7lung", ",", "Freu\u00b7de", "\u2013", ",", "rie\u00b7sig", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "punct", "word", "punct"], "pos": ["NE", "NN", "$,", "NN", "$,", "NN", "$(", "$,", "ADJD", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}