{"dta.poem.21599": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Im Himmel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Gen Himmel f&#252;hlt' ich meine Seele schweben               ", "tokens": ["Gen", "Him\u00b7mel", "f", "&#252;", "hlt'", "ich", "mei\u00b7ne", "See\u00b7le", "schwe\u00b7ben"], "token_info": ["word", "word", "word", "XML_entity", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "NE", "$(", "VVFIN", "PPER", "PPOSAT", "NN", "VVINF"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und in das Reich der Engel ging ich ein.", "tokens": ["Und", "in", "das", "Reich", "der", "En\u00b7gel", "ging", "ich", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Geblendet wagt\u2019 ich kaum den Blick zu heben \u2014", "tokens": ["Ge\u00b7blen\u00b7det", "wagt'", "ich", "kaum", "den", "Blick", "zu", "he\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PPER", "ADV", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "O Glanz! O Gl\u00fcck! Das Alles war nun mein!", "tokens": ["O", "Glanz", "!", "O", "Gl\u00fcck", "!", "Das", "Al\u00b7les", "war", "nun", "mein", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "NE", "NN", "$.", "ART", "PIS", "VAFIN", "ADV", "PPOSAT", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und unwillk\u00fcrlich kam mir ein Gedanke,", "tokens": ["Und", "un\u00b7will\u00b7k\u00fcr\u00b7lich", "kam", "mir", "ein", "Ge\u00b7dan\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der ach! so sch\u00f6n, so gut, so menschlich war \u2014", "tokens": ["Der", "ach", "!", "so", "sch\u00f6n", ",", "so", "gut", ",", "so", "menschlich", "war"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "XY", "$.", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD", "VAFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Mir kam der Liebe seliger Gedanke \u2014 \u2014", "tokens": ["Mir", "kam", "der", "Lie\u00b7be", "se\u00b7li\u00b7ger", "Ge\u00b7dan\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "NN", "$(", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "O Du, o meiner Mutter Augenpaar!", "tokens": ["O", "Du", ",", "o", "mei\u00b7ner", "Mut\u00b7ter", "Au\u00b7gen\u00b7paar", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "FM", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Von Ferne schien sie l\u00e4chelnd mich zu gr\u00fc\u00dfen,", "tokens": ["Von", "Fer\u00b7ne", "schien", "sie", "l\u00e4\u00b7chelnd", "mich", "zu", "gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ADJD", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die meiner Jugend erste Freuden sah;", "tokens": ["Die", "mei\u00b7ner", "Ju\u00b7gend", "ers\u00b7te", "Freu\u00b7den", "sah", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und auch den Vater fand ich Gott zu F\u00fc\u00dfen:", "tokens": ["Und", "auch", "den", "Va\u00b7ter", "fand", "ich", "Gott", "zu", "F\u00fc\u00b7\u00dfen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "PPER", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Verkl\u00e4rten Angesichtes stand er da.", "tokens": ["Ver\u00b7kl\u00e4r\u00b7ten", "An\u00b7ge\u00b7sich\u00b7tes", "stand", "er", "da", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "\u201eso soll ich hier Euch wirklich alle finden,", "tokens": ["\u201e", "so", "soll", "ich", "hier", "Euch", "wirk\u00b7lich", "al\u00b7le", "fin\u00b7den", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VMFIN", "PPER", "ADV", "PPER", "ADJD", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die mich in Lieb\u2019 an ihre Brust gepre\u00dft?", "tokens": ["Die", "mich", "in", "Lieb'", "an", "ih\u00b7re", "Brust", "ge\u00b7pre\u00dft", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ich h\u00f6rt\u2019 es in der Jugend mir verk\u00fcnden,", "tokens": ["Ich", "h\u00f6rt'", "es", "in", "der", "Ju\u00b7gend", "mir", "ver\u00b7k\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und zweifelnd hielt ich an der Hoffnung fest!\u201c", "tokens": ["Und", "zwei\u00b7felnd", "hielt", "ich", "an", "der", "Hoff\u00b7nung", "fest", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich trat heran \u2014 ich wollte sie umarmen.", "tokens": ["Ich", "trat", "he\u00b7ran", "ich", "woll\u00b7te", "sie", "um\u00b7ar\u00b7men", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$(", "PPER", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "\u201emein Vater! Meine Mutter!\u201c rief ich laut \u2014", "tokens": ["\u201e", "mein", "Va\u00b7ter", "!", "Mei\u00b7ne", "Mut\u00b7ter", "!", "\u201c", "rief", "ich", "laut"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "$.", "PPOSAT", "NN", "$.", "$(", "VVFIN", "PPER", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "O la\u00dft an Eurer Brust mich hold erwarmen,", "tokens": ["O", "la\u00dft", "an", "Eu\u00b7rer", "Brust", "mich", "hold", "er\u00b7war\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "PPOSAT", "NN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Selig das Kind, das Euch nun wiederschaut!", "tokens": ["Se\u00b7lig", "das", "Kind", ",", "das", "Euch", "nun", "wie\u00b7der\u00b7schaut", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.5": {"text": "Wie? War es m\u00f6glich? Ihres einzigen Sohnes", "tokens": ["Wie", "?", "War", "es", "m\u00f6g\u00b7lich", "?", "Ih\u00b7res", "ein\u00b7zi\u00b7gen", "Soh\u00b7nes"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "$.", "VAFIN", "PPER", "ADJD", "$.", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Erinnert eine Mutter sich nicht mehr? \u2014", "tokens": ["E\u00b7rin\u00b7nert", "ei\u00b7ne", "Mut\u00b7ter", "sich", "nicht", "mehr", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ART", "NN", "PRF", "PTKNEG", "ADV", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ich fuhr empor: unwillig dumpfen Tones", "tokens": ["Ich", "fuhr", "em\u00b7por", ":", "un\u00b7wil\u00b7lig", "dum\u00b7pfen", "To\u00b7nes"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$.", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Lief ein Gemurmel durch der Engel Heer.", "tokens": ["Lief", "ein", "Ge\u00b7mur\u00b7mel", "durch", "der", "En\u00b7gel", "Heer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ich sah best\u00fcrzt umher im weiten Raume:", "tokens": ["Ich", "sah", "be\u00b7st\u00fcrzt", "um\u00b7her", "im", "wei\u00b7ten", "Rau\u00b7me", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PTKVZ", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Versunken war in Andacht Jedermann.", "tokens": ["Ver\u00b7sun\u00b7ken", "war", "in", "An\u00b7dacht", "Je\u00b7der\u00b7mann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "NN", "PIS", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Pl\u00f6tzlich erwachten wie aus tiefem Traume", "tokens": ["Pl\u00f6tz\u00b7lich", "er\u00b7wach\u00b7ten", "wie", "aus", "tie\u00b7fem", "Trau\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "KOKOM", "APPR", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Alle und blickten mich erschrocken an.", "tokens": ["Al\u00b7le", "und", "blick\u00b7ten", "mich", "er\u00b7schro\u00b7cken", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "KON", "VVFIN", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.5": {"text": "\u201eo meine Freunde! Meine s\u00fc\u00dfen Schwestern!", "tokens": ["\u201e", "o", "mei\u00b7ne", "Freun\u00b7de", "!", "Mei\u00b7ne", "s\u00fc\u00b7\u00dfen", "Schwes\u00b7tern", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "FM", "PPOSAT", "NN", "$.", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Euch dr\u00fcck\u2019 ich liebend wieder an mein Herz!\u201c \u2014", "tokens": ["Euch", "dr\u00fcck", "ich", "lie\u00b7bend", "wie\u00b7der", "an", "mein", "Herz", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "ADV", "APPR", "PPOSAT", "NN", "$.", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "\u201e\u201eH\u00f6r\u2019 endlich auf, den gro\u00dfen ", "tokens": ["\u201e", "\u201e", "H\u00f6r'", "end\u00b7lich", "auf", ",", "den", "gro\u00b7\u00dfen"], "token_info": ["punct", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "$(", "VVIMP", "ADV", "PTKVZ", "$,", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Durch meinen Busen zog ein tiefer Schmerz.", "tokens": ["Durch", "mei\u00b7nen", "Bu\u00b7sen", "zog", "ein", "tie\u00b7fer", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "\u201enein, noch ist alle Hoffnung nicht geschwunden,", "tokens": ["\u201e", "nein", ",", "noch", "ist", "al\u00b7le", "Hoff\u00b7nung", "nicht", "ge\u00b7schwun\u00b7den", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKANT", "$,", "ADV", "VAFIN", "PIAT", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich sahe ", "tokens": ["Ich", "sa\u00b7he"], "token_info": ["word", "word"], "pos": ["PPER", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "An deren Brust ich Paradieses-Stunden", "tokens": ["An", "de\u00b7ren", "Brust", "ich", "Pa\u00b7ra\u00b7die\u00b7ses\u00b7Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PRELAT", "NN", "PPER", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In weicher, warmer Sommernacht geruht.\u201c", "tokens": ["In", "wei\u00b7cher", ",", "war\u00b7mer", "Som\u00b7mer\u00b7nacht", "ge\u00b7ruht", ".", "\u201c"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und durch allm\u00e4chtiges Gef\u00fchl getrieben,", "tokens": ["Und", "durch", "all\u00b7m\u00e4ch\u00b7ti\u00b7ges", "Ge\u00b7f\u00fchl", "ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Eilt\u2019 ich auf ihre s\u00fc\u00dfen Reize zu.", "tokens": ["Eilt'", "ich", "auf", "ih\u00b7re", "s\u00fc\u00b7\u00dfen", "Rei\u00b7ze", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "\u201edu,\u201c rief ich aus,\u201c Du mu\u00dft mich ewig lieben \u2014", "tokens": ["\u201e", "du", ",", "\u201c", "rief", "ich", "aus", ",", "\u201c", "Du", "mu\u00dft", "mich", "e\u00b7wig", "lie\u00b7ben"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "$,", "$(", "VVFIN", "PPER", "PTKVZ", "$,", "$(", "PPER", "VMFIN", "PPER", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Denn meines Lebens Liebe warst ja Du!\u201c", "tokens": ["Denn", "mei\u00b7nes", "Le\u00b7bens", "Lie\u00b7be", "warst", "ja", "Du", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "VAFIN", "ADV", "PPER", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Sie sah mich an. Ihr Haupt erhob sich freier \u2014", "tokens": ["Sie", "sah", "mich", "an", ".", "Ihr", "Haupt", "er\u00b7hob", "sich", "frei\u00b7er"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$.", "PPOSAT", "NN", "VVFIN", "PRF", "ADJD", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie war so sch\u00f6n, so keusch, so engelrein!", "tokens": ["Sie", "war", "so", "sch\u00f6n", ",", "so", "keusch", ",", "so", "en\u00b7gel\u00b7rein", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Geh\u00fcllt in einen leichten, wei\u00dfen Schleier", "tokens": ["Ge\u00b7h\u00fcllt", "in", "ei\u00b7nen", "leich\u00b7ten", ",", "wei\u00b7\u00dfen", "Schlei\u00b7er"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Lud ihres Busens holde Pracht mich ein.", "tokens": ["Lud", "ih\u00b7res", "Bu\u00b7sens", "hol\u00b7de", "Pracht", "mich", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "ADJA", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "\u201eo blicke dorthin! Beuge Deine Glieder!", "tokens": ["\u201e", "o", "bli\u00b7cke", "dor\u00b7thin", "!", "Beu\u00b7ge", "Dei\u00b7ne", "Glie\u00b7der", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "FM", "VVFIN", "ADV", "$.", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und bete Gott, den Allerbarmer an!\u201c", "tokens": ["Und", "be\u00b7te", "Gott", ",", "den", "Al\u00b7ler\u00b7bar\u00b7mer", "an", "!", "\u201c"], "toke