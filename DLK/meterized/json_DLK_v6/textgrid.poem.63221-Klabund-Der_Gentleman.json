{"textgrid.poem.63221": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Der Gentleman", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun ist Ihr Schritt aus diesem Haus entschwunden,", "tokens": ["Nun", "ist", "Ihr", "Schritt", "aus", "die\u00b7sem", "Haus", "ent\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "APPR", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Ledersessel stehen leer und stumm.", "tokens": ["Die", "Le\u00b7der\u00b7ses\u00b7sel", "ste\u00b7hen", "leer", "und", "stumm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich rufe nach den gelben Rosenstunden", "tokens": ["Ich", "ru\u00b7fe", "nach", "den", "gel\u00b7ben", "Ro\u00b7sen\u00b7stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und nach des Ragtimes zartem Unikum.", "tokens": ["Und", "nach", "des", "Rag\u00b7ti\u00b7mes", "zar\u00b7tem", "U\u00b7ni\u00b7kum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Mir ist, als ob ich immer jenen Lord seh,", "tokens": ["Mir", "ist", ",", "als", "ob", "ich", "im\u00b7mer", "je\u00b7nen", "Lord", "seh", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOKOM", "KOUS", "PPER", "ADV", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Der einst vor meiner Eifersucht sich barg.", "tokens": ["Der", "einst", "vor", "mei\u00b7ner", "Ei\u00b7fer\u00b7sucht", "sich", "barg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PPOSAT", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie schweben schon im Dampfer auf der Nordsee,", "tokens": ["Sie", "schwe\u00b7ben", "schon", "im", "Damp\u00b7fer", "auf", "der", "Nord\u00b7see", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPRART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und aus den Masten steigt der Hydepark.", "tokens": ["Und", "aus", "den", "Mas\u00b7ten", "steigt", "der", "Hy\u00b7de\u00b7park", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ein Pastor predigt Sonntags fr\u00fch den Frommen", "tokens": ["Ein", "Pas\u00b7tor", "pre\u00b7digt", "Sonn\u00b7tags", "fr\u00fch", "den", "From\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und warnt sie vor des Whiskytrinkers Los.", "tokens": ["Und", "warnt", "sie", "vor", "des", "Whis\u00b7ky\u00b7trin\u00b7kers", "Los", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Whitechapel l\u00e4chelt heiter und verkommen.", "tokens": ["Whi\u00b7te\u00b7cha\u00b7pel", "l\u00e4\u00b7chelt", "hei\u00b7ter", "und", "ver\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "KON", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Der Mond betr\u00e4ufelt Neger und Matros.", "tokens": ["Der", "Mond", "be\u00b7tr\u00e4u\u00b7felt", "Ne\u00b7ger", "und", "Mat\u00b7ros", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "KON", "NE", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Und w\u00e4hrend unsere Br\u00fcder sich zerfleischen:", "tokens": ["Und", "w\u00e4h\u00b7rend", "un\u00b7se\u00b7re", "Br\u00fc\u00b7der", "sich", "zer\u00b7flei\u00b7schen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "PRF", "VVINF", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Das U-Boot zischt, auf London sinkt der Zepp \u2013", "tokens": ["Das", "U\u00b7\u00b7Boot", "zischt", ",", "auf", "Lon\u00b7don", "sinkt", "der", "Zepp", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "APPR", "NE", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zerfliessen wir in geigenden Ger\u00e4uschen", "tokens": ["Zer\u00b7flies\u00b7sen", "wir", "in", "gei\u00b7gen\u00b7den", "Ge\u00b7r\u00e4u\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und wippen leicht im Brasilianerstep.", "tokens": ["Und", "wip\u00b7pen", "leicht", "im", "Bra\u00b7si\u00b7li\u00b7a\u00b7ners\u00b7tep", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Mich warf die Leidenschaft an Ihre K\u00fcste.", "tokens": ["Mich", "warf", "die", "Lei\u00b7den\u00b7schaft", "an", "Ih\u00b7re", "K\u00fcs\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "W\u00e4r ich Barbar! so wagte ich mich ganz!", "tokens": ["W\u00e4r", "ich", "Bar\u00b7bar", "!", "so", "wag\u00b7te", "ich", "mich", "ganz", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "$.", "ADV", "VVFIN", "PPER", "PRF", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "O neigen Sie die Aprikosenbr\u00fcste", "tokens": ["O", "nei\u00b7gen", "Sie", "die", "Ap\u00b7ri\u00b7ko\u00b7sen\u00b7br\u00fcs\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im Angesicht des doch geliebten Manns.", "tokens": ["Im", "An\u00b7ge\u00b7sicht", "des", "doch", "ge\u00b7lieb\u00b7ten", "Manns", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Ich blute vor den Fliegerpfeilen Ihrer", "tokens": ["Ich", "blu\u00b7te", "vor", "den", "Flie\u00b7ger\u00b7pfei\u00b7len", "Ih\u00b7rer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PPOSAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Entbrannten Augen, braune Marjorie.", "tokens": ["Ent\u00b7brann\u00b7ten", "Au\u00b7gen", ",", "brau\u00b7ne", "Mar\u00b7jo\u00b7rie", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Einst siege ich \u2013 vielleicht ... im Rennbootvierer", "tokens": ["Einst", "sie\u00b7ge", "ich", "\u2013", "viel\u00b7leicht", "...", "im", "Renn\u00b7boot\u00b7vie\u00b7rer"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$(", "ADV", "$(", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im Glanz der ewigen Kameraderie ...", "tokens": ["Im", "Glanz", "der", "e\u00b7wi\u00b7gen", "Ka\u00b7me\u00b7ra\u00b7de\u00b7rie", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}}}}