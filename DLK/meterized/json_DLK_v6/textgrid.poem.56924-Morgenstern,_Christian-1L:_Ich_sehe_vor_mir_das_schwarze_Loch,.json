{"textgrid.poem.56924": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich sehe vor mir das schwarze Loch,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich sehe vor mir das schwarze Loch,", "tokens": ["Ich", "se\u00b7he", "vor", "mir", "das", "schwar\u00b7ze", "Loch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "das tiefe, abgr\u00fcndige Loch,", "tokens": ["das", "tie\u00b7fe", ",", "ab\u00b7gr\u00fcn\u00b7di\u00b7ge", "Loch", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+---+", "measure": "unknown.measure.tri"}, "line.3": {"text": "in das ich tausend und abertausend", "tokens": ["in", "das", "ich", "tau\u00b7send", "und", "a\u00b7bert\u00b7au\u00b7send"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "ADJD", "KON", "ADJD"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Gedanken hinuntergeworfen,", "tokens": ["Ge\u00b7dan\u00b7ken", "hin\u00b7un\u00b7ter\u00b7ge\u00b7wor\u00b7fen", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "goldene Gedanken", "tokens": ["gol\u00b7de\u00b7ne", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "zu Menschenlust und Vorteil,", "tokens": ["zu", "Men\u00b7schen\u00b7lust", "und", "Vor\u00b7teil", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "die niemand wollte,", "tokens": ["die", "nie\u00b7mand", "woll\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PIS", "VMFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "denen niemand Gestalt lieh.", "tokens": ["de\u00b7nen", "nie\u00b7mand", "Ge\u00b7stalt", "lieh", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRELS", "PIS", "NN", "VVFIN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.9": {"text": "Und doch warens", "tokens": ["Und", "doch", "wa\u00b7rens"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADV", "NE"], "meter": "+-+-", "measure": "trochaic.di"}, "line.10": {"text": "Sch\u00f6pfergedanken,", "tokens": ["Sch\u00f6p\u00b7fer\u00b7ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "oh glaubt mir,", "tokens": ["oh", "glaubt", "mir", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.12": {"text": "des Lichtes wert.", "tokens": ["des", "Lich\u00b7tes", "wert", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.13": {"text": "Was sollt ich mich br\u00fcsten?", "tokens": ["Was", "sollt", "ich", "mich", "br\u00fcs\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.14": {"text": "Wer so viel", "tokens": ["Wer", "so", "viel"], "token_info": ["word", "word", "word"], "pos": ["PWS", "ADV", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.15": {"text": "in Jahren und Jahren", "tokens": ["in", "Jah\u00b7ren", "und", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.16": {"text": "versinken sah,", "tokens": ["ver\u00b7sin\u00b7ken", "sah", ","], "token_info": ["word", "word", "punct"], "pos": ["VVINF", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.17": {"text": "wer so viel Fr\u00fchlinge", "tokens": ["wer", "so", "viel", "Fr\u00fch\u00b7lin\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "ADV", "PIAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.18": {"text": "ungeschaut opferte,", "tokens": ["un\u00b7ge\u00b7schaut", "op\u00b7fer\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.19": {"text": "ihm ist das Herz", "tokens": ["ihm", "ist", "das", "Herz"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.20": {"text": "nicht mehr danach,", "tokens": ["nicht", "mehr", "da\u00b7nach", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "PAV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.21": {"text": "sich vor Menschen zu br\u00fcsten.", "tokens": ["sich", "vor", "Men\u00b7schen", "zu", "br\u00fcs\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.22": {"text": "Er sieht nur mit starrem Aug", "tokens": ["Er", "sieht", "nur", "mit", "star\u00b7rem", "Aug"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.23": {"text": "und zuckendem Mund", "tokens": ["und", "zu\u00b7cken\u00b7dem", "Mund"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.24": {"text": "auf den Abgrund Vergessenheit,", "tokens": ["auf", "den", "Ab\u00b7grund", "Ver\u00b7ges\u00b7sen\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.25": {"text": "der ihm zu viel verschlang.", "tokens": ["der", "ihm", "zu", "viel", "ver\u00b7schlang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PTKA", "PIS", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}