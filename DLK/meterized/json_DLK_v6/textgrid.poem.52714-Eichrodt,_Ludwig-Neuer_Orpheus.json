{"textgrid.poem.52714": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Neuer Orpheus", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bl\u00e4tter fallen, bunte welke,", "tokens": ["Bl\u00e4t\u00b7ter", "fal\u00b7len", ",", "bun\u00b7te", "wel\u00b7ke", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "jeder Schmuck zergeht,", "tokens": ["je\u00b7der", "Schmuck", "zer\u00b7geht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Wo der blaue Himmel lachte,", "tokens": ["Wo", "der", "blau\u00b7e", "Him\u00b7mel", "lach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "grauer Nebel steht;", "tokens": ["grau\u00b7er", "Ne\u00b7bel", "steht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Nimmer gl\u00fcht im Abend reiner", "tokens": ["Nim\u00b7mer", "gl\u00fcht", "im", "A\u00b7bend", "rei\u00b7ner"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Purpurlohe Brand,", "tokens": ["Pur\u00b7pur\u00b7lo\u00b7he", "Brand", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "W\u00e4rmelose Strahlen fliehen", "tokens": ["W\u00e4r\u00b7me\u00b7lo\u00b7se", "Strah\u00b7len", "flie\u00b7hen"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "\u00fcber \u00f6des Land.", "tokens": ["\u00fc\u00b7ber", "\u00f6\u00b7des", "Land", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Und auch dieser sch\u00f6ne Sommer", "tokens": ["Und", "auch", "die\u00b7ser", "sch\u00f6\u00b7ne", "Som\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PDAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "mu\u00dfte untergehn?", "tokens": ["mu\u00df\u00b7te", "un\u00b7ter\u00b7gehn", "?"], "token_info": ["word", "word", "punct"], "pos": ["VMFIN", "VVINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Dieser Reize Pracht und F\u00fclle", "tokens": ["Die\u00b7ser", "Rei\u00b7ze", "Pracht", "und", "F\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "mu\u00df ich welken sehn?", "tokens": ["mu\u00df", "ich", "wel\u00b7ken", "sehn", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "VVINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Diese Sonne, glutenm\u00e4chtig,", "tokens": ["Die\u00b7se", "Son\u00b7ne", ",", "glu\u00b7ten\u00b7m\u00e4ch\u00b7tig", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PDAT", "NN", "$,", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "hat der Herbst entthront,", "tokens": ["hat", "der", "Herbst", "ent\u00b7thront", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Die am hohen First des Himmels", "tokens": ["Die", "am", "ho\u00b7hen", "First", "des", "Him\u00b7mels"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPRART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "gnadenreich gewohnt!", "tokens": ["gna\u00b7den\u00b7reich", "ge\u00b7wohnt", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Ein erloschnes Feuerauge", "tokens": ["Ein", "er\u00b7lo\u00b7schnes", "Feu\u00b7er\u00b7au\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "schaut sie drein die Welt;", "tokens": ["schaut", "sie", "drein", "die", "Welt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Eine B\u00fchne ists, in welche", "tokens": ["Ei\u00b7ne", "B\u00fch\u00b7ne", "ists", ",", "in", "wel\u00b7che"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VAFIN", "$,", "APPR", "PRELS"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "falber Morgen f\u00e4llt.", "tokens": ["fal\u00b7ber", "Mor\u00b7gen", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Wie nach einer pr\u00e4chtigen Tafel", "tokens": ["Wie", "nach", "ei\u00b7ner", "pr\u00e4ch\u00b7ti\u00b7gen", "Ta\u00b7fel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "wenn die G\u00e4ste fort,", "tokens": ["wenn", "die", "G\u00e4s\u00b7te", "fort", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Oder wie die Prunkgem\u00e4cher", "tokens": ["O\u00b7der", "wie", "die", "Prunk\u00b7ge\u00b7m\u00e4\u00b7cher"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "nach dem K\u00f6nigsmord \u2013!", "tokens": ["nach", "dem", "K\u00f6\u00b7nigs\u00b7mord", "\u2013", "!"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "$(", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "W\u00fcst und widrig diese Erde", "tokens": ["W\u00fcst", "und", "wid\u00b7rig", "die\u00b7se", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "KON", "ADJD", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "schaurig mir und kalt.", "tokens": ["schau\u00b7rig", "mir", "und", "kalt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "KON", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Ach, wie sch\u00f6n ist sie gewesen", "tokens": ["Ach", ",", "wie", "sch\u00f6n", "ist", "sie", "ge\u00b7we\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PWAV", "ADJD", "VAFIN", "PPER", "VAPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "wonnig von Gestalt!", "tokens": ["won\u00b7nig", "von", "Ge\u00b7stalt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Dieser Reize Pracht und F\u00fclle", "tokens": ["Die\u00b7ser", "Rei\u00b7ze", "Pracht", "und", "F\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "mu\u00dfte untergehn,", "tokens": ["mu\u00df\u00b7te", "un\u00b7ter\u00b7gehn", ","], "token_info": ["word", "word", "punct"], "pos": ["VMFIN", "VVINF", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Und auch diesen sch\u00f6nen Sommer", "tokens": ["Und", "auch", "die\u00b7sen", "sch\u00f6\u00b7nen", "Som\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PDAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "soll ich welken sehn!", "tokens": ["soll", "ich", "wel\u00b7ken", "sehn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "VVINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "\u00bbwie? du trauerst, weich empfindsam,", "tokens": ["\u00bb", "wie", "?", "du", "trau\u00b7erst", ",", "weich", "emp\u00b7find\u00b7sam", ","], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PWAV", "$.", "PPER", "VVFIN", "$,", "ADJD", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "da\u00df der Sommer stirbt,", "tokens": ["da\u00df", "der", "Som\u00b7mer", "stirbt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "W\u00e4hrend dir in dunkler Erde", "tokens": ["W\u00e4h\u00b7rend", "dir", "in", "dunk\u00b7ler", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "bessrer Reiz verdirbt?", "tokens": ["bess\u00b7rer", "Reiz", "ver\u00b7dirbt", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Willst du klagen, o so klage,", "tokens": ["Willst", "du", "kla\u00b7gen", ",", "o", "so", "kla\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$,", "FM", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "jammere, weil du mu\u00dft", "tokens": ["jam\u00b7me\u00b7re", ",", "weil", "du", "mu\u00dft"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "VMFIN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Aber greife nach dem Kummer", "tokens": ["A\u00b7ber", "grei\u00b7fe", "nach", "dem", "Kum\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "deiner tiefsten Brust!\u00ab", "tokens": ["dei\u00b7ner", "tiefs\u00b7ten", "Brust", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Ha! was r\u00fcttelst du der Schmerzen", "tokens": ["Ha", "!", "was", "r\u00fct\u00b7telst", "du", "der", "Schmer\u00b7zen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "PWS", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "w\u00fcthendsten mir auf,", "tokens": ["w\u00fct\u00b7hends\u00b7ten", "mir", "auf", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Der ich sanft poetscher Trauer", "tokens": ["Der", "ich", "sanft", "poet\u00b7scher", "Trau\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADJD", "ADJA", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "lie\u00df den holden Lauf!", "tokens": ["lie\u00df", "den", "hol\u00b7den", "Lauf", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Freilich, n\u00e4rrisch ist die Klage", "tokens": ["Frei\u00b7lich", ",", "n\u00e4r\u00b7risch", "ist", "die", "Kla\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "ADJD", "VAFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "um den Schmuck der Erd,", "tokens": ["um", "den", "Schmuck", "der", "Erd", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "--+-+", "measure": "anapaest.init"}, "line.7": {"text": "Der in wenig Monden sch\u00f6ner,", "tokens": ["Der", "in", "we\u00b7nig", "Mon\u00b7den", "sch\u00f6\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PIAT", "NN", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "sicher wieberkehrt.", "tokens": ["si\u00b7cher", "wie\u00b7ber\u00b7kehrt", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "L\u00e4ppisch nur im Mannesauge", "tokens": ["L\u00e4p\u00b7pisch", "nur", "im", "Man\u00b7nes\u00b7au\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "solche Thr\u00e4ne scheint,", "tokens": ["sol\u00b7che", "Thr\u00e4\u00b7ne", "scheint", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Die ich einem bl\u00fcthevollen", "tokens": ["Die", "ich", "ei\u00b7nem", "bl\u00fc\u00b7the\u00b7vol\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sommer nachgeweint.", "tokens": ["Som\u00b7mer", "nach\u00b7ge\u00b7weint", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "O beschw\u00f6re andre Thr\u00e4nen,", "tokens": ["O", "be\u00b7schw\u00f6\u00b7re", "and\u00b7re", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "andre Klagen nicht!", "tokens": ["and\u00b7re", "Kla\u00b7gen", "nicht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKNEG", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Die da rythmisch flie\u00dfen, halten", "tokens": ["Die", "da", "ryth\u00b7misch", "flie\u00b7\u00dfen", ",", "hal\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ART", "ADV", "ADJD", "VVINF", "$,", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "mich im Gleichgewicht.", "tokens": ["mich", "im", "Gleich\u00b7ge\u00b7wicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Jene Klage la\u00df mich meiden,", "tokens": ["Je\u00b7ne", "Kla\u00b7ge", "la\u00df", "mich", "mei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVIMP", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "die den Schmerz beschreibt,", "tokens": ["die", "den", "Schmerz", "be\u00b7schreibt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Der, wenn ich ihn nicht bet\u00e4ube,", "tokens": ["Der", ",", "wenn", "ich", "ihn", "nicht", "be\u00b7t\u00e4u\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "PPER", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "selber mich bet\u00e4ubt.", "tokens": ["sel\u00b7ber", "mich", "be\u00b7t\u00e4ubt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Fort! mich kann der Wahnsinn fassen", "tokens": ["Fort", "!", "mich", "kann", "der", "Wahn\u00b7sinn", "fas\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "PPER", "VMFIN", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "jeden Augenblick \u2013", "tokens": ["je\u00b7den", "Au\u00b7gen\u00b7blick", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Eine todte theure Gattin", "tokens": ["Ei\u00b7ne", "tod\u00b7te", "theu\u00b7re", "Gat\u00b7tin"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "kehrt nicht mehr zur\u00fcck!", "tokens": ["kehrt", "nicht", "mehr", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ADV", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}