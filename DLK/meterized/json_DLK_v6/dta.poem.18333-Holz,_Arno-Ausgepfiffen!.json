{"dta.poem.18333": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "Ausgepfiffen!", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Und geht oft \u00fcber den Spa\u00df", "tokens": ["Und", "geht", "oft", "\u00fc\u00b7ber", "den", "Spa\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Und gleicht dann jener Trag\u00f6die,", "tokens": ["Und", "gleicht", "dann", "je\u00b7ner", "Tra\u00b7g\u00f6\u00b7die", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PDAT", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "In der Einer den Andern fra\u00df.", "tokens": ["In", "der", "Ei\u00b7ner", "den", "An\u00b7dern", "fra\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIS", "ART", "ADJA", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Und wenn wir's auch nicht wollen,", "tokens": ["Und", "wenn", "wir's", "auch", "nicht", "wol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "ADV", "PTKNEG", "VMFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wir kommen doch alle drin vor", "tokens": ["Wir", "kom\u00b7men", "doch", "al\u00b7le", "drin", "vor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PIS", "ADV", "APPR"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Und spielen die n\u00f6thigen Rollen", "tokens": ["Und", "spie\u00b7len", "die", "n\u00f6\u00b7thi\u00b7gen", "Rol\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Vom Jean bis zum Heldentenor.", "tokens": ["Vom", "Jean", "bis", "zum", "Hel\u00b7den\u00b7te\u00b7nor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NE", "APPR", "APPRART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Und wer mit seiner Visage", "tokens": ["Und", "wer", "mit", "sei\u00b7ner", "Vi\u00b7sa\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Am besten zu gaunern gelernt,", "tokens": ["Am", "bes\u00b7ten", "zu", "gau\u00b7nern", "ge\u00b7lernt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "PTKZU", "VVINF", "VVPP", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Erh\u00e4lt die nobelste Gage", "tokens": ["Er\u00b7h\u00e4lt", "die", "no\u00b7bels\u00b7te", "Ga\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und wird auch mitunter besternt.", "tokens": ["Und", "wird", "auch", "mi\u00b7tun\u00b7ter", "bes\u00b7ternt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "Ich studirte mir manche Falte", "tokens": ["Ich", "stu\u00b7dir\u00b7te", "mir", "man\u00b7che", "Fal\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und trat vor das volle Haus,", "tokens": ["Und", "trat", "vor", "das", "vol\u00b7le", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch blieb ich immer der Alte \u2014", "tokens": ["Doch", "blieb", "ich", "im\u00b7mer", "der", "Al\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "NN", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Drum pfiff mich das Publikum aus!", "tokens": ["Drum", "pfiff", "mich", "das", "Pub\u00b7li\u00b7kum", "aus", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}