{"dta.poem.18048": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "V.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198462", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Weih stritt mit dem Volck der Dauben.", "tokens": ["Der", "Weih", "stritt", "mit", "dem", "Volck", "der", "Dau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Er griff es feindlich an mit morden und mit rauben.", "tokens": ["Er", "griff", "es", "feind\u00b7lich", "an", "mit", "mor\u00b7den", "und", "mit", "rau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "APPR", "APPR", "ADJA", "KON", "APPR", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Dauben sahen das mit Schmertzen,", "tokens": ["Die", "Dau\u00b7ben", "sa\u00b7hen", "das", "mit", "Schmert\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und plagten sich in ihrem Hertzen.", "tokens": ["Und", "plag\u00b7ten", "sich", "in", "ih\u00b7rem", "Hert\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sie hielten einen Rath, und kamen \u00fcberein,", "tokens": ["Sie", "hiel\u00b7ten", "ei\u00b7nen", "Rath", ",", "und", "ka\u00b7men", "\u00fc\u00b7be\u00b7re\u00b7in", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "KON", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.6": {"text": "Da\u00df sie ohn\u2019 einen Vogt nicht sicher m\u00f6gten seyn;", "tokens": ["Da\u00df", "sie", "ohn'", "ei\u00b7nen", "Vogt", "nicht", "si\u00b7cher", "m\u00f6g\u00b7ten", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "PTKNEG", "ADJD", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Zu diesem wollten sie den Habicht nehmen,", "tokens": ["Zu", "die\u00b7sem", "woll\u00b7ten", "sie", "den", "Ha\u00b7bicht", "neh\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Der w\u00e4re starck genug, des Weihen Grimm zu z\u00e4hmen.", "tokens": ["Der", "w\u00e4\u00b7re", "starck", "ge\u00b7nug", ",", "des", "Wei\u00b7hen", "Grimm", "zu", "z\u00e4h\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJD", "ADV", "$,", "ART", "NN", "NE", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Da dieser nun ihr Pfleger ward,", "tokens": ["Da", "die\u00b7ser", "nun", "ihr", "Pfle\u00b7ger", "ward", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "ADV", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Verjagt er zwar den Weih, doch lie\u00df er nicht von Art.", "tokens": ["Ver\u00b7jagt", "er", "zwar", "den", "Weih", ",", "doch", "lie\u00df", "er", "nicht", "von", "Art", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$,", "ADV", "VVFIN", "PPER", "PTKNEG", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Da er der Dauben Freund", "tokens": ["Da", "er", "der", "Dau\u00b7ben", "Freund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Und Schirmer sollte seyn, war er ihr gr\u00f6ster Feind.", "tokens": ["Und", "Schir\u00b7mer", "soll\u00b7te", "seyn", ",", "war", "er", "ihr", "gr\u00f6s\u00b7ter", "Feind", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VMFIN", "VAINF", "$,", "VAFIN", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}