{"textgrid.poem.52442": {"metadata": {"author": {"name": "Salis-Seewis, Johann Gaudenz von", "birth": "N.A.", "death": "N.A."}, "title": "24. Die Einsiedelei", "genre": "verse", "period": "N.A.", "pub_year": 1798, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es rieselt, klar und wehend,", "tokens": ["Es", "rie\u00b7selt", ",", "klar", "und", "we\u00b7hend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Quell im Eichenwald;", "tokens": ["Ein", "Quell", "im", "Ei\u00b7chen\u00b7wald", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da w\u00e4hl' ich einsam gehend", "tokens": ["Da", "w\u00e4hl'", "ich", "ein\u00b7sam", "ge\u00b7hend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mir meinen Aufenthalt.", "tokens": ["Mir", "mei\u00b7nen", "Auf\u00b7ent\u00b7halt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mir dienet zur Kapelle", "tokens": ["Mir", "die\u00b7net", "zur", "Ka\u00b7pel\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN"], "meter": "-++--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Ein Gr\u00f6ttchen, duftigfrisch;", "tokens": ["Ein", "Gr\u00f6tt\u00b7chen", ",", "duf\u00b7tig\u00b7frisch", ";"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Zu meiner Klausnerzelle", "tokens": ["Zu", "mei\u00b7ner", "Klaus\u00b7ner\u00b7zel\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Verschlungenes Geb\u00fcsch.", "tokens": ["Ver\u00b7schlun\u00b7ge\u00b7nes", "Ge\u00b7b\u00fcsch", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Zwar d\u00fcster ist und tr\u00fcber", "tokens": ["Zwar", "d\u00fcs\u00b7ter", "ist", "und", "tr\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VAFIN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die nahe W\u00fcstenei,", "tokens": ["Die", "na\u00b7he", "W\u00fcs\u00b7te\u00b7nei", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Allein nur desto lieber", "tokens": ["Al\u00b7lein", "nur", "des\u00b7to", "lie\u00b7ber"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADV", "ADV"], "meter": "---+-+-", "measure": "unknown.measure.di"}, "line.4": {"text": "Der stillen Phantasei.", "tokens": ["Der", "stil\u00b7len", "Phan\u00b7ta\u00b7sei", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da ruh' ich oft im dichten,", "tokens": ["Da", "ruh'", "ich", "oft", "im", "dich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPRART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Bebl\u00fcmten Heidekraut;", "tokens": ["Be\u00b7bl\u00fcm\u00b7ten", "Hei\u00b7de\u00b7kraut", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Hoch wehn die schlanken Fichten", "tokens": ["Hoch", "wehn", "die", "schlan\u00b7ken", "Fich\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und st\u00f6hnen Seufzerlaut'.", "tokens": ["Und", "st\u00f6h\u00b7nen", "Seuf\u00b7zer\u00b7laut'", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wo von Wachholderstr\u00e4uchen", "tokens": ["Wo", "von", "Wach\u00b7hol\u00b7der\u00b7str\u00e4u\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "APPR", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Den Kieselsteig hinan", "tokens": ["Den", "Kie\u00b7sel\u00b7steig", "hi\u00b7nan"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "PTKVZ"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Verworrne Ranken schleichen,", "tokens": ["Ver\u00b7worr\u00b7ne", "Ran\u00b7ken", "schlei\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da brech' ich mir die Bahn;", "tokens": ["Da", "brech'", "ich", "mir", "die", "Bahn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Durch des Gehaues Stumpen,", "tokens": ["Durch", "des", "Ge\u00b7hau\u00b7es", "Stum\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wo wilde Erdbeern stehn,", "tokens": ["Wo", "wil\u00b7de", "Erd\u00b7beern", "stehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Klimm' ich auf Felsenklumpen,", "tokens": ["Klimm'", "ich", "auf", "Fel\u00b7sen\u00b7klum\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Das Land umher zu sehn.", "tokens": ["Das", "Land", "um\u00b7her", "zu", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Nichts unterbricht das Schweigen", "tokens": ["Nichts", "un\u00b7ter\u00b7bricht", "das", "Schwei\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Wildnis weit und breit,", "tokens": ["Der", "Wild\u00b7nis", "weit", "und", "breit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als wenn auf d\u00fcrren Zweigen", "tokens": ["Als", "wenn", "auf", "d\u00fcr\u00b7ren", "Zwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein Gr\u00fcnspecht hackt und schreit,", "tokens": ["Ein", "Gr\u00fcn\u00b7specht", "hackt", "und", "schreit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ein Rab' auf hoher Spitze", "tokens": ["Ein", "Rab'", "auf", "ho\u00b7her", "Spit\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Bemooster Tannen kr\u00e4chzt,", "tokens": ["Be\u00b7moos\u00b7ter", "Tan\u00b7nen", "kr\u00e4chzt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und in der Felsenritze", "tokens": ["Und", "in", "der", "Fel\u00b7sen\u00b7rit\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Ein Ringelt\u00e4ubchen \u00e4chzt.", "tokens": ["Ein", "Rin\u00b7gel\u00b7t\u00e4ub\u00b7chen", "\u00e4chzt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wie sich das Herz erweitert", "tokens": ["Wie", "sich", "das", "Herz", "er\u00b7wei\u00b7tert"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PRF", "ART", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im engen, dichten Wald!", "tokens": ["Im", "en\u00b7gen", ",", "dich\u00b7ten", "Wald", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den \u00f6den Tr\u00fcbsinn heitert", "tokens": ["Den", "\u00f6\u00b7den", "Tr\u00fcb\u00b7sinn", "hei\u00b7tert"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der traute Schatten bald.", "tokens": ["Der", "trau\u00b7te", "Schat\u00b7ten", "bald", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Kein \u00fcberlegner Sp\u00e4her", "tokens": ["Kein", "\u00fc\u00b7berl\u00b7eg\u00b7ner", "Sp\u00e4\u00b7her"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Erforscht hier meine Spur;", "tokens": ["Er\u00b7forscht", "hier", "mei\u00b7ne", "Spur", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Hier bin ich frei und n\u00e4her", "tokens": ["Hier", "bin", "ich", "frei", "und", "n\u00e4\u00b7her"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der Einfalt und Natur.", "tokens": ["Der", "Ein\u00b7falt", "und", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "O blieb' ich von den Ketten", "tokens": ["O", "blieb'", "ich", "von", "den", "Ket\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Des Weltgewirres frei!", "tokens": ["Des", "Welt\u00b7ge\u00b7wir\u00b7res", "frei", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "K\u00f6nnt' ich zu dir mich retten,", "tokens": ["K\u00f6nnt'", "ich", "zu", "dir", "mich", "ret\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du traute Siedelei!", "tokens": ["Du", "trau\u00b7te", "Sie\u00b7de\u00b7lei", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Froh, da\u00df ich dem Gebrause", "tokens": ["Froh", ",", "da\u00df", "ich", "dem", "Ge\u00b7brau\u00b7se"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "KOUS", "PPER", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Des Menschenschwarms entwich,", "tokens": ["Des", "Men\u00b7schen\u00b7schwarms", "ent\u00b7wich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Baut' ich hier eine Klause", "tokens": ["Baut'", "ich", "hier", "ei\u00b7ne", "Klau\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "F\u00fcr Liebchen und f\u00fcr mich.", "tokens": ["F\u00fcr", "Lieb\u00b7chen", "und", "f\u00fcr", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}