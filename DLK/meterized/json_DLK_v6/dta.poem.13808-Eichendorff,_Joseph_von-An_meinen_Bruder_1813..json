{"dta.poem.13808": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "An meinen Bruder 1813.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Mich irrte manches Sch\u00f6ne,", "tokens": ["Mich", "irr\u00b7te", "man\u00b7ches", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Viel reizte mich und viel mu\u00dft' ich vermissen.", "tokens": ["Viel", "reiz\u00b7te", "mich", "und", "viel", "mu\u00dft'", "ich", "ver\u00b7mis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "KON", "ADV", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Von Lust und Schmerz zerrissen,", "tokens": ["Von", "Lust", "und", "Schmerz", "zer\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was so mein Herz hinausgestr\u00f6mt in T\u00f6ne:", "tokens": ["Was", "so", "mein", "Herz", "hin\u00b7aus\u00b7ge\u00b7str\u00f6mt", "in", "T\u00f6\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PPOSAT", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Es waren Wiederspiele", "tokens": ["Es", "wa\u00b7ren", "Wie\u00b7der\u00b7spie\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Von Deines Busens ewigem Gef\u00fchle.", "tokens": ["Von", "Dei\u00b7nes", "Bu\u00b7sens", "e\u00b7wi\u00b7gem", "Ge\u00b7f\u00fch\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}