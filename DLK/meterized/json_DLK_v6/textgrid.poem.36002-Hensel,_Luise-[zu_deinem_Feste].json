{"textgrid.poem.36002": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "[zu deinem Feste]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu deinem Feste", "tokens": ["Zu", "dei\u00b7nem", "Fes\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Nimm hin das Beste:", "tokens": ["Nimm", "hin", "das", "Bes\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Schneegl\u00f6ckchen luftig", "tokens": ["Schnee\u00b7gl\u00f6ck\u00b7chen", "luf\u00b7tig"], "token_info": ["word", "word"], "pos": ["NN", "ADJD"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Und Veilchen duftig,", "tokens": ["Und", "Veil\u00b7chen", "duf\u00b7tig", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJD", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Doch eine Zwiebel", "tokens": ["Doch", "ei\u00b7ne", "Zwie\u00b7bel"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Ist auch nicht \u00fcbel.", "tokens": ["Ist", "auch", "nicht", "\u00fc\u00b7bel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PTKNEG", "ADJD", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Schneeglock' und Veilchen", "tokens": ["Schnee\u00b7glock'", "und", "Veil\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Bl\u00fch'n nur ein Weilchen,", "tokens": ["Bl\u00fch'n", "nur", "ein", "Weil\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Sind bald vergessen,", "tokens": ["Sind", "bald", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Kannst sie nicht essen,", "tokens": ["Kannst", "sie", "nicht", "es\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Aber dem Bollen", "tokens": ["A\u00b7ber", "dem", "Bol\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Wirst du nicht grollen.", "tokens": ["Wirst", "du", "nicht", "grol\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Gib uns in Gnaden", "tokens": ["Gib", "uns", "in", "Gna\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPR", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Dem Koch zum Braten,", "tokens": ["Dem", "Koch", "zum", "Bra\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "APPRART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Wir rufen alle,", "tokens": ["Wir", "ru\u00b7fen", "al\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Mit frohem Schalle,", "tokens": ["Mit", "fro\u00b7hem", "Schal\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Aus einem Munde:", "tokens": ["Aus", "ei\u00b7nem", "Mun\u00b7de", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Hoch Cunigunde!", "tokens": ["Hoch", "Cu\u00b7ni\u00b7gun\u00b7de", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}