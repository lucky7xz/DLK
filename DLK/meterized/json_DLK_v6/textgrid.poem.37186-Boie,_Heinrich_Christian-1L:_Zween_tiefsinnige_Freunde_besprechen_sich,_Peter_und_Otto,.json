{"textgrid.poem.37186": {"metadata": {"author": {"name": "Boie, Heinrich Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zween tiefsinnige Freunde besprechen sich, Peter und Otto,", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zween tiefsinnige Freunde besprechen sich, Peter und Otto,", "tokens": ["Zween", "tief\u00b7sin\u00b7ni\u00b7ge", "Freun\u00b7de", "be\u00b7spre\u00b7chen", "sich", ",", "Pe\u00b7ter", "und", "Ot\u00b7to", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "VVFIN", "PRF", "$,", "NE", "KON", "NE", "$,"], "meter": "-++--+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und in Gedanken krazt Otto den Peter am Arm.", "tokens": ["Und", "in", "Ge\u00b7dan\u00b7ken", "krazt", "Ot\u00b7to", "den", "Pe\u00b7ter", "am", "Arm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "NE", "ART", "NE", "APPRART", "NN", "$."], "meter": "---+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Peter fragt in Gedanken: \u00bbWas kratzest Du?\u00ab Kratzend erwidert", "tokens": ["Pe\u00b7ter", "fragt", "in", "Ge\u00b7dan\u00b7ken", ":", "\u00bb", "Was", "krat\u00b7zest", "Du", "?", "\u00ab", "Krat\u00b7zend", "er\u00b7wi\u00b7dert"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "NN", "$.", "$(", "PWS", "VVFIN", "PPER", "$.", "$(", "NN", "VVPP"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Otto: Mir juckte der Arm. Peter versezte \u00bbJe so!\u00ab", "tokens": ["Ot\u00b7to", ":", "Mir", "juck\u00b7te", "der", "Arm", ".", "Pe\u00b7ter", "ver\u00b7sez\u00b7te", "\u00bb", "Je", "so", "!", "\u00a