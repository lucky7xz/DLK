{"textgrid.poem.48419": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "11. Auf Herrn Ilgens Leichbestattung", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer sagts, geehrter Man, itzt neuer Himmelsb\u00fcrger,", "tokens": ["Wer", "sagts", ",", "ge\u00b7ehr\u00b7ter", "Man", ",", "itzt", "neu\u00b7er", "Him\u00b7mels\u00b7b\u00fcr\u00b7ger", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "ADJA", "PIS", "$,", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "da\u00df euch sei Leid geschehn, indem der wilde W\u00fcrger,", "tokens": ["da\u00df", "euch", "sei", "Leid", "ge\u00b7schehn", ",", "in\u00b7dem", "der", "wil\u00b7de", "W\u00fcr\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "NN", "VVPP", "$,", "KOUS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "dem euch Gott zahm hie\u00df sein, sich auch an euch gemacht", "tokens": ["dem", "euch", "Gott", "zahm", "hie\u00df", "sein", ",", "sich", "auch", "an", "euch", "ge\u00b7macht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "NN", "ADJD", "VVFIN", "VAINF", "$,", "PRF", "ADV", "APPR", "PPER", "VVPP"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "und durch sein scharfes Recht, wie Alles, umgebracht?", "tokens": ["und", "durch", "sein", "schar\u00b7fes", "Recht", ",", "wie", "Al\u00b7les", ",", "um\u00b7ge\u00b7bracht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN", "$,", "PWAV", "PIS", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wer sagts, euch sei nicht wol, als etwan eure Lieben,", "tokens": ["Wer", "sagts", ",", "euch", "sei", "nicht", "wol", ",", "als", "et\u00b7wan", "eu\u00b7re", "Lie\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "PPER", "VAFIN", "PTKNEG", "ADV", "$,", "KOUS", "ADV", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "die \u00fcber euren Fall sich billich hoch betr\u00fcben", "tokens": ["die", "\u00fc\u00b7ber", "eu\u00b7ren", "Fall", "sich", "bil\u00b7lich", "hoch", "be\u00b7tr\u00fc\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PPOSAT", "NN", "PRF", "ADJD", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "und ernstlich traurig sein? Wir Andern, die wir euch", "tokens": ["und", "ernst\u00b7lich", "trau\u00b7rig", "sein", "?", "Wir", "An\u00b7dern", ",", "die", "wir", "euch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJD", "ADJD", "VAINF", "$.", "PPER", "ADJA", "$,", "PRELS", "PPER", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "am Blute nicht verwandt, doch nach der Liebe gleich,", "tokens": ["am", "Blu\u00b7te", "nicht", "ver\u00b7wandt", ",", "doch", "nach", "der", "Lie\u00b7be", "gleich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKNEG", "VVPP", "$,", "ADV", "APPR", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "die uns gesampt verkn\u00fcpft, erkennen euer Gl\u00fccke", "tokens": ["die", "uns", "ge\u00b7sampt", "ver\u00b7kn\u00fcpft", ",", "er\u00b7ken\u00b7nen", "eu\u00b7er", "Gl\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PPER", "ADJD", "VVPP", "$,", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "und h\u00f6chste Seligkeit. Ihr habt die Welt zur\u00fccke", "tokens": ["und", "h\u00f6chs\u00b7te", "Se\u00b7lig\u00b7keit", ".", "Ihr", "habt", "die", "Welt", "zu\u00b7r\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "$.", "PPER", "VAFIN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "und Alles, was sie ist. Die Erde la\u00dft ihr stehn", "tokens": ["und", "Al\u00b7les", ",", "was", "sie", "ist", ".", "Die", "Er\u00b7de", "la\u00dft", "ihr", "stehn"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "$,", "PRELS", "PPER", "VAFIN", "$.", "ART", "NN", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "und k\u00f6nnt mit sicherm Fu\u00df itzt auf den Wolken gehn,", "tokens": ["und", "k\u00f6nnt", "mit", "si\u00b7cherm", "Fu\u00df", "itzt", "auf", "den", "Wol\u00b7ken", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "die sich euch unterstreun. Ihr selbsten w\u00fcrdet sagen,", "tokens": ["die", "sich", "euch", "un\u00b7ter\u00b7streun", ".", "Ihr", "selbs\u00b7ten", "w\u00fcr\u00b7det", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRELS", "PRF", "PPER", "VVINF", "$.", "PPOSAT", "ADJA", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "wenn eine solche Wahl euch w\u00fcrde f\u00fcrgeschlagen,", "tokens": ["wenn", "ei\u00b7ne", "sol\u00b7che", "Wahl", "euch", "w\u00fcr\u00b7de", "f\u00fcr\u00b7ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "PIAT", "NN", "PPER", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "ihr soltet kehren um: Bewahre mich mein Gott,", "tokens": ["ihr", "sol\u00b7tet", "keh\u00b7ren", "um", ":", "Be\u00b7wah\u00b7re", "mich", "mein", "Gott", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "KOUI", "$.", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "da\u00df ich aus Freud' in Leid, aus Leben in den Tod,", "tokens": ["da\u00df", "ich", "aus", "Freud'", "in", "Leid", ",", "aus", "Le\u00b7ben", "in", "den", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "APPR", "NN", "$,", "APPR", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "aus Ruh' in St\u00fcrme z\u00f6g'! ach! allzuwahr, in St\u00fcrme.", "tokens": ["aus", "Ruh'", "in", "St\u00fcr\u00b7me", "z\u00f6g'", "!", "ach", "!", "all\u00b7zu\u00b7wahr", ",", "in", "St\u00fcr\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "VVFIN", "$.", "XY", "$.", "ADV", "$,", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Was ist es seit der Zeit, da\u00df sch\u00e4dliche Geschw\u00fcrme,", "tokens": ["Was", "ist", "es", "seit", "der", "Zeit", ",", "da\u00df", "sch\u00e4d\u00b7li\u00b7che", "Ge\u00b7schw\u00fcr\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "APPR", "ART", "NN", "$,", "KOUS", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "die Krieger, unser Land mit sich auch angesteckt,", "tokens": ["die", "Krie\u00b7ger", ",", "un\u00b7ser", "Land", "mit", "sich", "auch", "an\u00b7ge\u00b7steckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPOSAT", "NN", "APPR", "PRF", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "da immer eines noch in tausent Junge heckt", "tokens": ["da", "im\u00b7mer", "ei\u00b7nes", "noch", "in", "tau\u00b7sent", "Jun\u00b7ge", "heckt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "ADV", "APPR", "CARD", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "und hat sich wol besaamt? Was ist es, soll ich sprechen,", "tokens": ["und", "hat", "sich", "wol", "be\u00b7saamt", "?", "Was", "ist", "es", ",", "soll", "ich", "spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PRF", "ADV", "VVPP", "$.", "PWS", "VAFIN", "PPER", "$,", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "wol anders seit der Zeit, als wenn die Klippen brechen,", "tokens": ["wol", "an\u00b7ders", "seit", "der", "Zeit", ",", "als", "wenn", "die", "Klip\u00b7pen", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "$,", "KOKOM", "KOUS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "die \u00c4olus verwahrt, die Winde rei\u00dfen aus", "tokens": ["die", "\u00c4\u00b7o\u00b7lus", "ver\u00b7wahrt", ",", "die", "Win\u00b7de", "rei\u00b7\u00dfen", "aus"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "$,", "ART", "NN", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "und brausen durch die Welt? Da krachet manches Haus,", "tokens": ["und", "brau\u00b7sen", "durch", "die", "Welt", "?", "Da", "kra\u00b7chet", "man\u00b7ches", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$.", "ADV", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "manch edler Bau zerbricht. Wir haben es gesehen,", "tokens": ["manch", "ed\u00b7ler", "Bau", "zer\u00b7bricht", ".", "Wir", "ha\u00b7ben", "es", "ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "$.", "PPER", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "ach leider! allzusehr, wie uns bisher geschehen,", "tokens": ["ach", "lei\u00b7der", "!", "all\u00b7zu\u00b7sehr", ",", "wie", "uns", "bis\u00b7her", "ge\u00b7sche\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$.", "ADV", "$,", "PWAV", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "wie uns der Kriegessturm hat hin und her verweht,", "tokens": ["wie", "uns", "der", "Krie\u00b7ges\u00b7sturm", "hat", "hin", "und", "her", "ver\u00b7weht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VAFIN", "ADV", "KON", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "die St\u00e4dte durchgesaust, die D\u00f6rfer umgedreht,", "tokens": ["die", "St\u00e4d\u00b7te", "durch\u00b7ge\u00b7saust", ",", "die", "D\u00f6r\u00b7fer", "um\u00b7ge\u00b7dreht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "da\u00df Nichts ihm \u00e4hnlich ist. Zumitten in dem Wesen,", "tokens": ["da\u00df", "Nichts", "ihm", "\u00e4hn\u00b7lich", "ist", ".", "Zu\u00b7mit\u00b7ten", "in", "dem", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "ADJD", "VAFIN", "$.", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "da es am \u00e4rgsten war, seid, Vater, ihr genesen;", "tokens": ["da", "es", "am", "\u00e4rgs\u00b7ten", "war", ",", "seid", ",", "Va\u00b7ter", ",", "ihr", "ge\u00b7ne\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKA", "ADJD", "VAFIN", "$,", "VAFIN", "$,", "NN", "$,", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "genesen seid ihr nun und denkt nicht einmal dran,", "tokens": ["ge\u00b7ne\u00b7sen", "seid", "ihr", "nun", "und", "denkt", "nicht", "ein\u00b7mal", "dran", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "ADV", "KON", "VVFIN", "PTKNEG", "ADV", "PAV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "was euch der arge Feind f\u00fcr Dampf hat angetan,", "tokens": ["was", "euch", "der", "ar\u00b7ge", "Feind", "f\u00fcr", "Dampf", "hat", "an\u00b7ge\u00b7tan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "ADJA", "NN", "APPR", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "dar\u00fcber ihr erlagt. Der Eidam ist erfreuet,", "tokens": ["da\u00b7r\u00fc\u00b7ber", "ihr", "er\u00b7lagt", ".", "Der", "Ei\u00b7dam", "ist", "er\u00b7freu\u00b7et", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "VVFIN", "$.", "ART", "NN", "VAFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "den ihr euch schicktet vor. Die Tochter springt und schreiet:", "tokens": ["den", "ihr", "euch", "schick\u00b7tet", "vor", ".", "Die", "Toch\u00b7ter", "springt", "und", "schrei\u00b7et", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "VVFIN", "PTKVZ", "$.", "ART", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "komt Vater, Vater komt! Das liebe junge Paar", "tokens": ["komt", "Va\u00b7ter", ",", "Va\u00b7ter", "komt", "!", "Das", "lie\u00b7be", "jun\u00b7ge", "Paar"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "NN", "$,", "NN", "VVFIN", "$.", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "empf\u00e4ngt euch, wie es soll, sampt aller Geister Schaar,", "tokens": ["emp\u00b7f\u00e4ngt", "euch", ",", "wie", "es", "soll", ",", "sampt", "al\u00b7ler", "Geis\u00b7ter", "Schaar", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "PPER", "VMFIN", "$,", "APPR", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "die Gott stets um sich hat. Wir w\u00fcndschen uns ingleichen,", "tokens": ["die", "Gott", "stets", "um", "sich", "hat", ".", "Wir", "w\u00fcnd\u00b7schen", "uns", "in\u00b7glei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "PRF", "VAFIN", "$.", "PPER", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "da\u00df wir doch an den Ort auch m\u00f6gen bald gereichen,", "tokens": ["da\u00df", "wir", "doch", "an", "den", "Ort", "auch", "m\u00f6\u00b7gen", "bald", "ge\u00b7rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ART", "NN", "ADV", "VMFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "da keine Furcht mehr ist, da wir in Wahrheit sehn,", "tokens": ["da", "kei\u00b7ne", "Furcht", "mehr", "ist", ",", "da", "wir", "in", "Wahr\u00b7heit", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "ADV", "VAFIN", "$,", "KOUS", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "es sei auch uns, wie euch, in Allem wol geschehn.", "tokens": ["es", "sei", "auch", "uns", ",", "wie", "euch", ",", "in", "Al\u00b7lem", "wol", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PPER", "$,", "PWAV", "PPER", "$,", "APPR", "PIS", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}