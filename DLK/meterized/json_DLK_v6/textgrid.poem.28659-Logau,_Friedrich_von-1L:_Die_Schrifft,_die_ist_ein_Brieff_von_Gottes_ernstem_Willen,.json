{"textgrid.poem.28659": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Schrifft, die ist ein Brieff von Gottes ernstem Willen,", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Schrifft, die ist ein Brieff von Gottes ernstem Willen,", "tokens": ["Die", "Schrifft", ",", "die", "ist", "ein", "Brieff", "von", "Got\u00b7tes", "erns\u00b7tem", "Wil\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VAFIN", "ART", "NN", "APPR", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Geschrieben an die Welt, denselben zu erv\u00f6llen;", "tokens": ["Ge\u00b7schrie\u00b7ben", "an", "die", "Welt", ",", "den\u00b7sel\u00b7ben", "zu", "er\u00b7v\u00f6l\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "PDS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie da\u00df er liegen bleibt, und niemand an ihn nimmt?", "tokens": ["Wie", "da\u00df", "er", "lie\u00b7gen", "bleibt", ",", "und", "nie\u00b7mand", "an", "ihn", "nimmt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "PPER", "VVINF", "VVFIN", "$,", "KON", "PIS", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil seinen Titul er nach Willen nicht bek\u00fcmmt.", "tokens": ["Weil", "sei\u00b7nen", "Ti\u00b7tul", "er", "nach", "Wil\u00b7len", "nicht", "be\u00b7k\u00fcmmt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word",