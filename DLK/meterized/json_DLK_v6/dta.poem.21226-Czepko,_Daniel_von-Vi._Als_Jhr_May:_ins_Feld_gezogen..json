{"dta.poem.21226": {"metadata": {"author": {"name": "Czepko, Daniel von", "birth": "N.A.", "death": "N.A."}, "title": "Vi.  \n  Als Jhr May: ins Feld  \n gezogen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20871-8", "language": ["de:0.99"], "booktitle": "Czepko, Daniel von: Triumph Bogen. Breslau, 1641."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr Feinde steht vnd schawt den Grossen K\u00f6nig an/", "tokens": ["Ihr", "Fein\u00b7de", "steht", "vnd", "schawt", "den", "Gros\u00b7sen", "K\u00f6\u00b7nig", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "KON", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So siht die ", "tokens": ["So", "siht", "die"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Vnd schwiege diese Hand/ w\u00fcrd' Euch die sache straffen/", "tokens": ["Vnd", "schwie\u00b7ge", "die\u00b7se", "Hand", "/", "w\u00fcrd'", "Euch", "die", "sa\u00b7che", "straf\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PDAT", "NN", "$(", "VAFIN", "PPER", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sie z\u00f6rnt vor seine G\u00fctt vnd bricht dem Siege bahn.", "tokens": ["Sie", "z\u00f6rnt", "vor", "sei\u00b7ne", "G\u00fctt", "vnd", "bricht", "dem", "Sie\u00b7ge", "bahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "KON", "VVFIN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Jhr ", "tokens": ["Ihr"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Euch Schlacht vnd Feld vnd Sieg. Kein Arm kan Euch verletzen:", "tokens": ["Euch", "Schlacht", "vnd", "Feld", "vnd", "Sieg", ".", "Kein", "Arm", "kan", "Euch", "ver\u00b7let\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "KON", "NN", "KON", "NN", "$.", "PIAT", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Jhr m\u00fcst durch Trew den Sinn/ durch Muth den Degen wetzen:", "tokens": ["Ihr", "m\u00fcst", "durch", "Trew", "den", "Sinn", "/", "durch", "Muth", "den", "De\u00b7gen", "wet\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "ART", "NN", "$(", "APPR", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nur fort: die Losung heist: ", "tokens": ["Nur", "fort", ":", "die", "Lo\u00b7sung", "heist", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$.", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "O Wien/ du Andres Rom/ wie bist du so entbrant:", "tokens": ["O", "Wi\u00b7en", "/", "du", "And\u00b7res", "Rom", "/", "wie", "bist", "du", "so", "ent\u00b7brant", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$(", "PPER", "PIS", "NE", "$(", "KOKOM", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Er zeucht als K\u00f6nig hin/ vnd wird als Kayser kommen.", "tokens": ["Er", "zeucht", "als", "K\u00f6\u00b7nig", "hin", "/", "vnd", "wird", "als", "Kay\u00b7ser", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOUS", "NN", "PTKVZ", "$(", "KON", "VAFIN", "KOKOM", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Kein ", "tokens": ["Kein"], "token_info": ["word"], "pos": ["PIAT"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Dem auch ", "tokens": ["Dem", "auch"], "token_info": ["word", "word"], "pos": ["PDS", "ADV"], "meter": "+-", "measure": "trochaic.single"}}}}}