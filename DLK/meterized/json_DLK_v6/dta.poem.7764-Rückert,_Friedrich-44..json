{"dta.poem.7764": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "44.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Den Rosenzweig benagt ein L\u00e4mmchen auf der Weide,", "tokens": ["Den", "Ro\u00b7sen\u00b7zweig", "be\u00b7nagt", "ein", "L\u00e4mm\u00b7chen", "auf", "der", "Wei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es thuts nur sich zur Luft, es thuts nicht ihm zu Leide.", "tokens": ["Es", "thuts", "nur", "sich", "zur", "Luft", ",", "es", "thuts", "nicht", "ihm", "zu", "Lei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PRF", "APPRART", "NN", "$,", "PPER", "VVFIN", "PTKNEG", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Daf\u00fcr hat Rosendorn dem L\u00e4mmchen abgezwackt", "tokens": ["Da\u00b7f\u00fcr", "hat", "Ro\u00b7sen\u00b7dorn", "dem", "L\u00e4mm\u00b7chen", "ab\u00b7ge\u00b7zwackt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "NE", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Fl\u00f6ckchen Wolle nur, es ward davon nicht nackt.", "tokens": ["Ein", "Fl\u00f6ck\u00b7chen", "Wol\u00b7le", "nur", ",", "es", "ward", "da\u00b7von", "nicht", "nackt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "ADV", "$,", "PPER", "VAFIN", "PAV", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Das Fl\u00f6ckchen hielt der Dorn in scharfen Fingern fest;", "tokens": ["Das", "Fl\u00f6ck\u00b7chen", "hielt", "der", "Dorn", "in", "schar\u00b7fen", "Fin\u00b7gern", "fest", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da kam die Nachtigall und wollte baun ihr Nest.", "tokens": ["Da", "kam", "die", "Nach\u00b7ti\u00b7gall", "und", "woll\u00b7te", "baun", "ihr", "Nest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "KON", "VMFIN", "VVINF", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Sie sprach: Thu auf die Hand, und gib das Fl\u00f6ckchen mir,", "tokens": ["Sie", "sprach", ":", "Thu", "auf", "die", "Hand", ",", "und", "gib", "das", "Fl\u00f6ck\u00b7chen", "mir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "VVFIN", "APPR", "ART", "NN", "$,", "KON", "VVIMP", "ART", "NN", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und ist mein Nest gebaut, sing' ich zum Danke dir.", "tokens": ["Und", "ist", "mein", "Nest", "ge\u00b7baut", ",", "sing'", "ich", "zum", "Dan\u00b7ke", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "VVPP", "$,", "VVFIN", "PPER", "APPRART", "PTKANT", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Er gab, sie nahm und baut', und als sie nun gesungen,", "tokens": ["Er", "gab", ",", "sie", "nahm", "und", "baut'", ",", "und", "als", "sie", "nun", "ge\u00b7sun\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "KON", "VVFIN", "$,", "KON", "KOUS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da ist am Rosendorn vor Luft die Ros' entsprungen.", "tokens": ["Da", "ist", "am", "Ro\u00b7sen\u00b7dorn", "vor", "Luft", "die", "Ros'", "ent\u00b7sprun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPRART", "NN", "APPR", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}