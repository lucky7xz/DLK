{"textgrid.poem.33651": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Winterritt", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "An meines Rosses Brust und M\u00e4hne", "tokens": ["An", "mei\u00b7nes", "Ros\u00b7ses", "Brust", "und", "M\u00e4h\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gefriert der Hauch zu Duft im Schnee,", "tokens": ["Ge\u00b7friert", "der", "Hauch", "zu", "Duft", "im", "Schnee", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In meinem Auge quillt die Tr\u00e4ne,", "tokens": ["In", "mei\u00b7nem", "Au\u00b7ge", "quillt", "die", "Tr\u00e4\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich dacht' an dich heut' mehr als je.", "tokens": ["Ich", "dacht'", "an", "dich", "heut'", "mehr", "als", "je", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ADV", "PIAT", "KOKOM", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Mir klang's heut' fr\u00fch wie Sonntagsl\u00e4uten", "tokens": ["Mir", "klang's", "heut'", "fr\u00fch", "wie", "Sonn\u00b7tags\u00b7l\u00e4u\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "KOKOM", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Durch Berg und Tal in stiller Nacht;", "tokens": ["Durch", "Berg", "und", "Tal", "in", "stil\u00b7ler", "Nacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich sah dich da mit andern Br\u00e4uten,", "tokens": ["Ich", "sah", "dich", "da", "mit", "an\u00b7dern", "Br\u00e4u\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Kirchent\u00fcr' war aufgemacht.", "tokens": ["Die", "Kir\u00b7chen\u00b7t\u00fcr'", "war", "auf\u00b7ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die andern trugen Myrtenkr\u00f6nlein,", "tokens": ["Die", "an\u00b7dern", "tru\u00b7gen", "Myr\u00b7ten\u00b7kr\u00f6n\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Du trugst ein Schleierlein im Haar;", "tokens": ["Du", "trugst", "ein", "Schlei\u00b7er\u00b7lein", "im", "Haar", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du hattest auf dem Arm ein S\u00f6hnlein,", "tokens": ["Du", "hat\u00b7test", "auf", "dem", "Arm", "ein", "S\u00f6hn\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Grabstein war der Traualtar.", "tokens": ["Ein", "Grab\u00b7stein", "war", "der", "Trau\u00b7al\u00b7tar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}