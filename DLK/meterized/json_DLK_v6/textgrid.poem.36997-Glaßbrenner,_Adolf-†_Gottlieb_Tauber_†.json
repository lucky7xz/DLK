{"textgrid.poem.36997": {"metadata": {"author": {"name": "Gla\u00dfbrenner, Adolf", "birth": "N.A.", "death": "N.A."}, "title": "\u2020 Gottlieb Tauber \u2020", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf den J\u00fcngling, der hier ruht,", "tokens": ["Auf", "den", "J\u00fcng\u00b7ling", ",", "der", "hier", "ruht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hab' ich eine grimmige Wuth!", "tokens": ["Hab'", "ich", "ei\u00b7ne", "grim\u00b7mi\u00b7ge", "Wuth", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Wand'rer, denk' Dir, er ist Sieben", "tokens": ["Wan\u00b7d'\u00b7rer", ",", "denk'", "Dir", ",", "er", "ist", "Sie\u00b7ben"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "PPER", "$,", "PPER", "VAFIN", "NN"], "meter": "+---+--+-", "measure": "trochaic.tri.relaxed"}, "line.4": {"text": "Skudi's bei mir Rest geblieben!", "tokens": ["Sku\u00b7di's", "bei", "mir", "Rest", "ge\u00b7blie\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPER", "NN", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}