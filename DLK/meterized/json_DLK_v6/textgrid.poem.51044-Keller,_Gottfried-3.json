{"textgrid.poem.51044": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "3", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich sah j\u00fcngst einen Schwarm von frischen Knaben,", "tokens": ["Ich", "sah", "j\u00fcngst", "ei\u00b7nen", "Schwarm", "von", "fri\u00b7schen", "Kna\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gekoppelt und gez\u00e4umt wie ein Zug Pferde;", "tokens": ["Ge\u00b7kop\u00b7pelt", "und", "ge\u00b7z\u00e4umt", "wie", "ein", "Zug", "Pfer\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVPP", "KOKOM", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+++-", "measure": "unknown.measure.hexa"}, "line.3": {"text": "Sie wieherten und scharrten an der Erde", "tokens": ["Sie", "wie\u00b7her\u00b7ten", "und", "scharr\u00b7ten", "an", "der", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPR", "ART", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Und taten sonst, was Pferde an sich haben.", "tokens": ["Und", "ta\u00b7ten", "sonst", ",", "was", "Pfer\u00b7de", "an", "sich", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$,", "PWS", "NN", "APPR", "PRF", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und mehr noch; was sonst diesen ist Beschwerde,", "tokens": ["Und", "mehr", "noch", ";", "was", "sonst", "die\u00b7sen", "ist", "Be\u00b7schwer\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "$.", "PWS", "ADV", "PDS", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das schien die Buben k\u00f6stlich zu erlaben;", "tokens": ["Das", "schien", "die", "Bu\u00b7ben", "k\u00f6st\u00b7lich", "zu", "er\u00b7la\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Denn lustig sah ich durch die Gasse traben", "tokens": ["Denn", "lus\u00b7tig", "sah", "ich", "durch", "die", "Gas\u00b7se", "tra\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auf einen Peitschenknall die ganze Herde.", "tokens": ["Auf", "ei\u00b7nen", "Peit\u00b7schen\u00b7knall", "die", "gan\u00b7ze", "Her\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Das Leitseil war in eines Knirpses H\u00e4nden,", "tokens": ["Das", "Leit\u00b7seil", "war", "in", "ei\u00b7nes", "Knirp\u00b7ses", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der, klein und schwach, nicht sparte seine Hiebe", "tokens": ["Der", ",", "klein", "und", "schwach", ",", "nicht", "spar\u00b7te", "sei\u00b7ne", "Hie\u00b7be"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "$,", "ADJD", "KON", "ADJD", "$,", "PTKNEG", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und launisch das Gespann lie\u00df gehn und wenden.", "tokens": ["Und", "lau\u00b7nisch", "das", "Ge\u00b7spann", "lie\u00df", "gehn", "und", "wen\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "VVFIN", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wenn nur dies fr\u00fche Sinnbild niedrer Triebe,", "tokens": ["Wenn", "nur", "dies", "fr\u00fc\u00b7he", "Sinn\u00b7bild", "nie\u00b7drer", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PDS", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Anstatt mit schlimmer Wirklichkeit zu enden,", "tokens": ["An\u00b7statt", "mit", "schlim\u00b7mer", "Wirk\u00b7lich\u00b7keit", "zu", "en\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Einst mit den Kinderschuhn verloren bliebe!", "tokens": ["Einst", "mit", "den", "Kin\u00b7der\u00b7schuhn", "ver\u00b7lo\u00b7ren", "blie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVPP", "VVFIN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}}}}