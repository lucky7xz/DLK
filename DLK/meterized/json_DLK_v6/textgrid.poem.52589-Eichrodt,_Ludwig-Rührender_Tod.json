{"textgrid.poem.52589": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "R\u00fchrender Tod", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tief im Gebirg auf sonnigem Grund", "tokens": ["Tief", "im", "Ge\u00b7birg", "auf", "son\u00b7ni\u00b7gem", "Grund"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPRART", "NN", "APPR", "ADJA", "NN"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Da liegen zwei Genossen,", "tokens": ["Da", "lie\u00b7gen", "zwei", "Ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "CARD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Alle Beide auf den Tod verwundt,", "tokens": ["Al\u00b7le", "Bei\u00b7de", "auf", "den", "Tod", "ver\u00b7wundt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PIS", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Alle Beid ins Herz geschossen.", "tokens": ["Al\u00b7le", "Beid", "ins", "Herz", "ge\u00b7schos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Von Ferne toset das Gefecht", "tokens": ["Von", "Fer\u00b7ne", "to\u00b7set", "das", "Ge\u00b7fecht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.2": {"text": "Herauf zum gr\u00fcnen Walde,", "tokens": ["Her\u00b7auf", "zum", "gr\u00fc\u00b7nen", "Wal\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Sch\u00fcsse knattern so regelrecht", "tokens": ["Die", "Sch\u00fcs\u00b7se", "knat\u00b7tern", "so", "re\u00b7gel\u00b7recht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "VVFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und s\u00e4ubern Trift und Halde.", "tokens": ["Und", "s\u00e4u\u00b7bern", "Trift", "und", "Hal\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die Beiden aber lagen im Moos", "tokens": ["Die", "Bei\u00b7den", "a\u00b7ber", "la\u00b7gen", "im", "Moos"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "ADV", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Und schauten, treuen Blickes,", "tokens": ["Und", "schau\u00b7ten", ",", "treu\u00b7en", "Bli\u00b7ckes", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In des Himmels dunkelblauen Schoos", "tokens": ["In", "des", "Him\u00b7mels", "dun\u00b7kel\u00b7blau\u00b7en", "Schoos"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Und harrten ihres Geschickes.", "tokens": ["Und", "harr\u00b7ten", "ih\u00b7res", "Ge\u00b7schi\u00b7ckes", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Sie liegen viele Schritte fern \u2013", "tokens": ["Sie", "lie\u00b7gen", "vie\u00b7le", "Schrit\u00b7te", "fern", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das schmerzet mehr als die Wunde,", "tokens": ["Das", "schmer\u00b7zet", "mehr", "als", "die", "Wun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIS", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sie w\u00e4ren bei einander so gern", "tokens": ["Sie", "w\u00e4\u00b7ren", "bei", "ein\u00b7an\u00b7der", "so", "gern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PRF", "ADV", "ADV"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "In der bittern Todesstunde!", "tokens": ["In", "der", "bit\u00b7tern", "To\u00b7dess\u00b7tun\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Und mit unendlicher Liebesm\u00fch", "tokens": ["Und", "mit", "un\u00b7end\u00b7li\u00b7cher", "Lie\u00b7bes\u00b7m\u00fch"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "R\u00fccken sie n\u00e4her und n\u00e4her;", "tokens": ["R\u00fc\u00b7cken", "sie", "n\u00e4\u00b7her", "und", "n\u00e4\u00b7her", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "O Bruder, stirb mir nicht zu fr\u00fch,", "tokens": ["O", "Bru\u00b7der", ",", "stirb", "mir", "nicht", "zu", "fr\u00fch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVIMP", "PPER", "PTKNEG", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich sterbe sonst so eher.", "tokens": ["Ich", "ster\u00b7be", "sonst", "so", "e\u00b7her", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Sie haben sich mit stiller Glut", "tokens": ["Sie", "ha\u00b7ben", "sich", "mit", "stil\u00b7ler", "Glut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In ihre Arme geschlossen,", "tokens": ["In", "ih\u00b7re", "Ar\u00b7me", "ge\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und ihre Thr\u00e4nen und ihr Blut", "tokens": ["Und", "ih\u00b7re", "Thr\u00e4\u00b7nen", "und", "ihr", "Blut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In Eins zusammen flossen.", "tokens": ["In", "Eins", "zu\u00b7sam\u00b7men", "flos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Sie k\u00fcssen sich und schaun sich an,", "tokens": ["Sie", "k\u00fcs\u00b7sen", "sich", "und", "schaun", "sich", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "KON", "VVFIN", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Eine und der Ander,", "tokens": ["Der", "Ei\u00b7ne", "und", "der", "An\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und l\u00e4cheln freundlich dann und wann,", "tokens": ["Und", "l\u00e4\u00b7cheln", "freund\u00b7lich", "dann", "und", "wann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ADV", "KON", "PWAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und sterben mit einander.", "tokens": ["Und", "ster\u00b7ben", "mit", "ein\u00b7an\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PRF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}