{"textgrid.poem.59568": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Der Mann", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Brauset, Winde! Sch\u00e4ume, Meer!", "tokens": ["Brau\u00b7set", ",", "Win\u00b7de", "!", "Sch\u00e4u\u00b7me", ",", "Meer", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$.", "NN", "$,", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mir im Herzen braust es mehr;", "tokens": ["Mir", "im", "Her\u00b7zen", "braust", "es", "mehr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schlage, Ungl\u00fcckswetter, ein!", "tokens": ["Schla\u00b7ge", ",", "Un\u00b7gl\u00fccks\u00b7wet\u00b7ter", ",", "ein", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mut will trotzig oben sein.", "tokens": ["Mut", "will", "trot\u00b7zig", "o\u00b7ben", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ADJD", "ADV", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Schwillt die Flut ins Himmelshaus,", "tokens": ["Schwillt", "die", "Flut", "ins", "Him\u00b7mels\u00b7haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Keine Anker wirft er aus;", "tokens": ["Kei\u00b7ne", "An\u00b7ker", "wirft", "er", "aus", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schmettern Blitze h\u00f6llentief,", "tokens": ["Schmet\u00b7tern", "Blit\u00b7ze", "h\u00f6l\u00b7len\u00b7tief", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Blickt sein freies Aug' nicht schief.", "tokens": ["Blickt", "sein", "frei\u00b7es", "Aug'", "nicht", "schief", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Freudig schie\u00dft er auf Gefahr,", "tokens": ["Freu\u00b7dig", "schie\u00dft", "er", "auf", "Ge\u00b7fahr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie auf Raub der Sonnenaar,", "tokens": ["Wie", "auf", "Raub", "der", "Son\u00b7ne\u00b7naar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "St\u00fcrzt mit Wangen frisch und rot", "tokens": ["St\u00fcrzt", "mit", "Wan\u00b7gen", "frisch", "und", "rot"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "K\u00fchn hinein in tiefsten Tod.", "tokens": ["K\u00fchn", "hin\u00b7ein", "in", "tiefs\u00b7ten", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Froh f\u00fcr Recht und Vaterland", "tokens": ["Froh", "f\u00fcr", "Recht", "und", "Va\u00b7ter\u00b7land"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Fa\u00dft das Eisen seine Hand,", "tokens": ["Fa\u00dft", "das", "Ei\u00b7sen", "sei\u00b7ne", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "F\u00fcr das Laster feig und feil", "tokens": ["F\u00fcr", "das", "Las\u00b7ter", "feig", "und", "feil"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJD", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird sein Mund ein Donnerkeil.", "tokens": ["Wird", "sein", "Mund", "ein", "Don\u00b7ner\u00b7keil", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Seine Losung hei\u00dfet Gott,", "tokens": ["Sei\u00b7ne", "Lo\u00b7sung", "hei\u00b7\u00dfet", "Gott", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Darum ist die Welt ihm Spott;", "tokens": ["Da\u00b7rum", "ist", "die", "Welt", "ihm", "Spott", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "ART", "NN", "PPER", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Darum ha\u00dft er Tyrannei.", "tokens": ["Da\u00b7rum", "ha\u00dft", "er", "Ty\u00b7ran\u00b7nei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.6": {"line.1": {"text": "O wie selig ist der Mann,", "tokens": ["O", "wie", "se\u00b7lig", "ist", "der", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Der in Wahrheit sagen kann:", "tokens": ["Der", "in", "Wahr\u00b7heit", "sa\u00b7gen", "kann", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Du, Gefahr, bist meine Braut,", "tokens": ["Du", ",", "Ge\u00b7fahr", ",", "bist", "mei\u00b7ne", "Braut", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Treue, du mir angetraut!", "tokens": ["Treu\u00b7e", ",", "du", "mir", "an\u00b7ge\u00b7traut", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "O wie selig ist der Mann,", "tokens": ["O", "wie", "se\u00b7lig", "ist", "der", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Der in Wahrheit sagen kann:", "tokens": ["Der", "in", "Wahr\u00b7heit", "sa\u00b7gen", "kann", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mut, du bist mir Sonnenschein,", "tokens": ["Mut", ",", "du", "bist", "mir", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mut, du bist mir edler Wein!", "tokens": ["Mut", ",", "du", "bist", "mir", "ed\u00b7ler", "Wein", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Sonnenschein beh\u00e4lt sein Licht,", "tokens": ["Son\u00b7nen\u00b7schein", "be\u00b7h\u00e4lt", "sein", "Licht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Saft der Reben altet nicht:", "tokens": ["Saft", "der", "Re\u00b7ben", "al\u00b7tet", "nicht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "So erlischt nicht k\u00fchner Mut,", "tokens": ["So", "er\u00b7lischt", "nicht", "k\u00fch\u00b7ner", "Mut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKNEG", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "So erbleicht nicht Heldenblut.", "tokens": ["So", "er\u00b7bleicht", "nicht", "Hel\u00b7den\u00b7blut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKNEG", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Will die Welt zu Scheitern gehn,", "tokens": ["Will", "die", "Welt", "zu", "Schei\u00b7tern", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mut bleibt fest und ruhig stehn;", "tokens": ["Mut", "bleibt", "fest", "und", "ru\u00b7hig", "stehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKVZ", "KON", "ADJD", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ja, f\u00e4llt selbst der Himmel ein,", "tokens": ["Ja", ",", "f\u00e4llt", "selbst", "der", "Him\u00b7mel", "ein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Mut wird Gott mit G\u00f6ttern sein.", "tokens": ["Mut", "wird", "Gott", "mit", "G\u00f6t\u00b7tern", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "APPR", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}