{"textgrid.poem.41854": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Rosse wiehern, Waffen blinken,", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Rosse wiehern, Waffen blinken,", "tokens": ["Ros\u00b7se", "wie\u00b7hern", ",", "Waf\u00b7fen", "blin\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deutschlands R\u00e4cher sind genaht,", "tokens": ["Deutschlands", "R\u00e4\u00b7cher", "sind", "ge\u00b7naht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und die bunten Fahnen winken", "tokens": ["Und", "die", "bun\u00b7ten", "Fah\u00b7nen", "win\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zu des Ruhmes goldnem Pfad.", "tokens": ["Zu", "des", "Ruh\u00b7mes", "gold\u00b7nem", "Pfad", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Soll ich stets dem Kummer dienen,", "tokens": ["Soll", "ich", "stets", "dem", "Kum\u00b7mer", "die\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sehnsuchtsvoll und hoffnungslos?", "tokens": ["Sehn\u00b7suchts\u00b7voll", "und", "hoff\u00b7nungs\u00b7los", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Sieh, das Ziel ist sch\u00f6n und gro\u00df;", "tokens": ["Sieh", ",", "das", "Ziel", "ist", "sch\u00f6n", "und", "gro\u00df", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Nimmer bl\u00fcht die That des K\u00fchnen", "tokens": ["Nim\u00b7mer", "bl\u00fcht", "die", "That", "des", "K\u00fch\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "In der Ruhe tr\u00e4gem Schoo\u00df.", "tokens": ["In", "der", "Ru\u00b7he", "tr\u00e4\u00b7gem", "Schoo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "La\u00df mich ziehn, wohin das Mahnen", "tokens": ["La\u00df", "mich", "ziehn", ",", "wo\u00b7hin", "das", "Mah\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "PPER", "VVINF", "$,", "PWAV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meines Busens mir gebeut;", "tokens": ["Mei\u00b7nes", "Bu\u00b7sens", "mir", "ge\u00b7beut", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Friedenspalmen sind die Fahnen,", "tokens": ["Frie\u00b7dens\u00b7pal\u00b7men", "sind", "die", "Fah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und zum Schlummer ruft der Streit.", "tokens": ["Und", "zum", "Schlum\u00b7mer", "ruft", "der", "Streit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Meine Freunde sind gefallen", "tokens": ["Mei\u00b7ne", "Freun\u00b7de", "sind", "ge\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Durch der Feinde blut'ges Schwert,", "tokens": ["Durch", "der", "Fein\u00b7de", "blut'\u00b7ges", "Schwert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und mein Herz blieb unerh\u00f6rt,", "tokens": ["Und", "mein", "Herz", "blieb", "un\u00b7er\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Und das Leben hat von allen", "tokens": ["Und", "das", "Le\u00b7ben", "hat", "von", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VAFIN", "APPR", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "W\u00fcnschen keinen mir gew\u00e4hrt.", "tokens": ["W\u00fcn\u00b7schen", "kei\u00b7nen", "mir", "ge\u00b7w\u00e4hrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Rauher Herbst, du wehst so schaurig", "tokens": ["Rau\u00b7her", "Herbst", ",", "du", "wehst", "so", "schau\u00b7rig"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PPER", "VVFIN", "ADV", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Um der Bl\u00fcthen \u00f6des Grab,", "tokens": ["Um", "der", "Bl\u00fc\u00b7then", "\u00f6\u00b7des", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Deine Wolken h\u00e4ngen traurig", "tokens": ["Dei\u00b7ne", "Wol\u00b7ken", "h\u00e4n\u00b7gen", "trau\u00b7rig"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf die dunkle Welt herab!", "tokens": ["Auf", "die", "dunk\u00b7le", "Welt", "her\u00b7ab", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Send', o Krieg, aus ehrnem Schlunde", "tokens": ["Send'", ",", "o", "Krieg", ",", "aus", "ehr\u00b7nem", "Schlun\u00b7de"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "FM", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Deine Flammen durchs Gefild,", "tokens": ["Dei\u00b7ne", "Flam\u00b7men", "durchs", "Ge\u00b7fild", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wirble, Trommel, hell und wild,", "tokens": ["Wirb\u00b7le", ",", "Trom\u00b7mel", ",", "hell", "und", "wild", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Da\u00df das kranke Herz gesunde", "tokens": ["Da\u00df", "das", "kran\u00b7ke", "Herz", "ge\u00b7sun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Durch des Lebens rasches Bild.", "tokens": ["Durch", "des", "Le\u00b7bens", "ra\u00b7sches", "Bild", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch nicht sey's ein dumpfes Z\u00fcrnen,", "tokens": ["Doch", "nicht", "sey's", "ein", "dum\u00b7pfes", "Z\u00fcr\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das zur fremden Bahn mich dr\u00e4ngt;", "tokens": ["Das", "zur", "frem\u00b7den", "Bahn", "mich", "dr\u00e4ngt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPRART", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Friede sey mit den Gestirnen,", "tokens": ["Frie\u00b7de", "sey", "mit", "den", "Ge\u00b7stir\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die mein feindlich Loos gelenkt.", "tokens": ["Die", "mein", "feind\u00b7lich", "Loos", "ge\u00b7lenkt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJD", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Freyer Wille ziemt dem Streiter,", "tokens": ["Frey\u00b7er", "Wil\u00b7le", "ziemt", "dem", "Strei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Den das Vaterland gewann,", "tokens": ["Den", "das", "Va\u00b7ter\u00b7land", "ge\u00b7wann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und es schlie\u00dft der deutsche Mann", "tokens": ["Und", "es", "schlie\u00dft", "der", "deut\u00b7sche", "Mann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Ruhig, unbet\u00e4ubt und heiter", "tokens": ["Ru\u00b7hig", ",", "un\u00b7be\u00b7t\u00e4ubt", "und", "hei\u00b7ter"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Sich dem sch\u00f6nen B\u00fcndni\u00df an.", "tokens": ["Sich", "dem", "sch\u00f6\u00b7nen", "B\u00fcn\u00b7dni\u00df", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "La\u00df uns scheiden! Sprich, was frommen", "tokens": ["La\u00df", "uns", "schei\u00b7den", "!", "Sprich", ",", "was", "from\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["VVIMP", "PPER", "VVINF", "$.", "VVIMP", "$,", "PWS", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Herz und Leben, stets entzweyt?", "tokens": ["Herz", "und", "Le\u00b7ben", ",", "stets", "ent\u00b7zweyt", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach, der Herbst ist l\u00e4ngst gekommen,", "tokens": ["Ach", ",", "der", "Herbst", "ist", "l\u00e4ngst", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ART", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und noch w\u00e4hrt des Lenzes Leid.", "tokens": ["Und", "noch", "w\u00e4hrt", "des", "Len\u00b7zes", "Leid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "La\u00df uns still und freundlich scheiden,", "tokens": ["La\u00df", "uns", "still", "und", "freund\u00b7lich", "schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "KON", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bis uns sch\u00f6nre Sonnen gl\u00fchn;", "tokens": ["Bis", "uns", "sch\u00f6n\u00b7re", "Son\u00b7nen", "gl\u00fchn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Alles hab' ich dir verziehn,", "tokens": ["Al\u00b7les", "hab'", "ich", "dir", "ver\u00b7ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Denn du hast f\u00fcr ird'sche Leiden", "tokens": ["Denn", "du", "hast", "f\u00fcr", "ird'\u00b7sche", "Lei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Ew'ge Sch\u00e4tze mir verliehn.", "tokens": ["Ew'\u00b7ge", "Sch\u00e4t\u00b7ze", "mir", "ver\u00b7liehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Friedlich will ich mich dir nahen,", "tokens": ["Fried\u00b7lich", "will", "ich", "mich", "dir", "na\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "PPER", "PPER", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deinen Segen zu erflehn,", "tokens": ["Dei\u00b7nen", "Se\u00b7gen", "zu", "er\u00b7flehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Will dein letztes Wort empfahen,", "tokens": ["Will", "dein", "letz\u00b7tes", "Wort", "em\u00b7pfa\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deinen Blick noch einmal sehn.", "tokens": ["Dei\u00b7nen", "Blick", "noch", "ein\u00b7mal", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADV", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "O nur ", "tokens": ["O", "nur"], "token_info": ["word", "word"], "pos": ["NE", "ADV"], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "F\u00fcr des Herzens wilden Streit,", "tokens": ["F\u00fcr", "des", "Her\u00b7zens", "wil\u00b7den", "Streit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "F\u00fcr der Zukunft langes Leid,", "tokens": ["F\u00fcr", "der", "Zu\u00b7kunft", "lan\u00b7ges", "Leid", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Und nicht l\u00e4nger will ich weinen", "tokens": ["Und", "nicht", "l\u00e4n\u00b7ger", "will", "ich", "wei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PTKNEG", "ADJD", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Um getr\u00e4umte Seligkeit!", "tokens": ["Um", "ge\u00b7tr\u00e4um\u00b7te", "Se\u00b7lig\u00b7keit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUI", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "La\u00df mich ziehn! Wie darfst du klagen,", "tokens": ["La\u00df", "mich", "ziehn", "!", "Wie", "darfst", "du", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVINF", "$.", "PWAV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn ich selbst mit starkem Sinn", "tokens": ["Wenn", "ich", "selbst", "mit", "star\u00b7kem", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Muthig bin, dir zu entsagen,", "tokens": ["Mut\u00b7hig", "bin", ",", "dir", "zu", "ent\u00b7sa\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Werth dich zu besitzen bin?", "tokens": ["Werth", "dich", "zu", "be\u00b7sit\u00b7zen", "bin", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Jedes Band will ich vernichten,", "tokens": ["Je\u00b7des", "Band", "will", "ich", "ver\u00b7nich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Das mich fesselnd noch umgiebt.", "tokens": ["Das", "mich", "fes\u00b7selnd", "noch", "um\u00b7giebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADJD", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Fr\u00fcher, als ich dich geliebt,", "tokens": ["Fr\u00fc\u00b7her", ",", "als", "ich", "dich", "ge\u00b7liebt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "PRF", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Hat das Vaterland die Pflichten", "tokens": ["Hat", "das", "Va\u00b7ter\u00b7land", "die", "Pflich\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Treuer Lieb' an mir ge\u00fcbt.", "tokens": ["Treu\u00b7er", "Lieb'", "an", "mir", "ge\u00b7\u00fcbt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Lebe wohl! Ich scheide nimmer;", "tokens": ["Le\u00b7be", "wohl", "!", "Ich", "schei\u00b7de", "nim\u00b7mer", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$.", "PPER", "VVFIN", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jedes mildgew\u00e4hrte Pfand,", "tokens": ["Je\u00b7des", "mild\u00b7ge\u00b7w\u00e4hr\u00b7te", "Pfand", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jeder heil'gen Stunde Schimmer", "tokens": ["Je\u00b7der", "heil'\u00b7gen", "Stun\u00b7de", "Schim\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Folgt mir nach in's ferne Land.", "tokens": ["Folgt", "mir", "nach", "in's", "fer\u00b7ne", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Lebe wohl, du Zarte, Reine!", "tokens": ["Le\u00b7be", "wohl", ",", "du", "Zar\u00b7te", ",", "Rei\u00b7ne", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "PPER", "NN", "$,", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ewig lebt dein holdes Bild", "tokens": ["E\u00b7wig", "lebt", "dein", "hol\u00b7des", "Bild"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Mir im Busen, still und mild;", "tokens": ["Mir", "im", "Bu\u00b7sen", ",", "still", "und", "mild", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Aber du, vergi\u00df das meine,", "tokens": ["A\u00b7ber", "du", ",", "ver\u00b7gi\u00df", "das", "mei\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "VVIMP", "PDS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Wenn mit Schmerz es dich erf\u00fcllt.", "tokens": ["Wenn", "mit", "Schmerz", "es", "dich", "er\u00b7f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "PPER", "PRF", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "O sey gl\u00fccklich und entsage,", "tokens": ["O", "sey", "gl\u00fcck\u00b7lich", "und", "ent\u00b7sa\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADJD", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gro\u00dfes Herz, dem stillen Gram!", "tokens": ["Gro\u00b7\u00dfes", "Herz", ",", "dem", "stil\u00b7len", "Gram", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was das Leben gab, ertrage", "tokens": ["Was", "das", "Le\u00b7ben", "gab", ",", "er\u00b7tra\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PWS", "ART", "NN", "VVFIN", "$,", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und verschmerze, was es nahm.", "tokens": ["Und", "ver\u00b7schmer\u00b7ze", ",", "was", "es", "nahm", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ohne Sorge la\u00df mich scheiden;", "tokens": ["Oh\u00b7ne", "Sor\u00b7ge", "la\u00df", "mich", "schei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVIMP", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Freudig sey das Herz und licht,", "tokens": ["Freu\u00b7dig", "sey", "das", "Herz", "und", "licht", ","], "token_info": ["word", "word", "word", "word", "word", 