{"textgrid.poem.41220": {"metadata": {"author": {"name": "Denis, Michael", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie lange reizen eure Saitengriffe", "genre": "verse", "period": "N.A.", "pub_year": 1764, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie lange reizen eure Saitengriffe", "tokens": ["Wie", "lan\u00b7ge", "rei\u00b7zen", "eu\u00b7re", "Sai\u00b7ten\u00b7grif\u00b7fe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Noch niedriges Gef\u00fchl?", "tokens": ["Noch", "nied\u00b7ri\u00b7ges", "Ge\u00b7f\u00fchl", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie lange weht noch in die Glut der L\u00fcste", "tokens": ["Wie", "lan\u00b7ge", "weht", "noch", "in", "die", "Glut", "der", "L\u00fcs\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VVFIN", "ADV", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Fl\u00fcgel eures Lieds?", "tokens": ["Der", "Fl\u00fc\u00b7gel", "eu\u00b7res", "Lieds", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ach Barden! \u2013 Dieser tugendholde Namen", "tokens": ["Ach", "Bar\u00b7den", "!", "\u2013", "Die\u00b7ser", "tu\u00b7gend\u00b7hol\u00b7de", "Na\u00b7men"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word"], "pos": ["ITJ", "NN", "$.", "$(", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Geb\u00fchret er noch euch?", "tokens": ["Ge\u00b7b\u00fch\u00b7ret", "er", "noch", "euch", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich geb' ihn euch! O leitete der Namen", "tokens": ["Ich", "geb'", "ihn", "euch", "!", "O", "lei\u00b7te\u00b7te", "der", "Na\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PPER", "$.", "NE", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zur Bardenpflicht zur\u00fcck'!", "tokens": ["Zur", "Bar\u00b7den\u00b7pflicht", "zu\u00b7r\u00fcck'", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ach Barden! waren fremder Wollusts\u00e4nger", "tokens": ["Ach", "Bar\u00b7den", "!", "wa\u00b7ren", "frem\u00b7der", "Wol\u00b7lus\u00b7ts\u00e4n\u00b7ger"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "NN", "$.", "VAFIN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nicht leider schon zu viel?", "tokens": ["Nicht", "lei\u00b7der", "schon", "zu", "viel", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADV", "PTKA", "PIS", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Entmanntet ihr nun auch zu eurer Schande", "tokens": ["Ent\u00b7mann\u00b7tet", "ihr", "nun", "auch", "zu", "eu\u00b7rer", "Schan\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das deutsche Saitenspiel?", "tokens": ["Das", "deut\u00b7sche", "Sai\u00b7ten\u00b7spiel", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "So klang es nicht in bied'ren Ahnenzeiten;", "tokens": ["So", "klang", "es", "nicht", "in", "bie\u00b7d'\u00b7ren", "Ah\u00b7nen\u00b7zei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Da war's der Tugend treu.", "tokens": ["Da", "wa\u00b7r's", "der", "Tu\u00b7gend", "treu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Warum verstimmtet ihr die Saiten? Saget,", "tokens": ["Wa\u00b7rum", "ver\u00b7stimm\u00b7tet", "ihr", "die", "Sai\u00b7ten", "?", "Sa\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "NN", "$.", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Was that die Tugend euch?", "tokens": ["Was", "that", "die", "Tu\u00b7gend", "euch", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Was thaten euch Thuisko's Heldens\u00f6hne?", "tokens": ["Was", "tha\u00b7ten", "euch", "Thuis\u00b7ko's", "Hel\u00b7den\u00b7s\u00f6h\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "NE", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ihr mach't sie geil und frech.", "tokens": ["Ihr", "mach't", "sie", "geil", "und", "frech", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was thaten euch Thusnelden's Enkelinnen?", "tokens": ["Was", "tha\u00b7ten", "euch", "Thus\u00b7nel\u00b7den's", "En\u00b7ke\u00b7lin\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "NE", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ihr decket sie mit Scham.", "tokens": ["Ihr", "de\u00b7cket", "sie", "mit", "Scham", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Zwar euer Lied ist s\u00fc\u00dfer Honigfladen,", "tokens": ["Zwar", "eu\u00b7er", "Lied", "ist", "s\u00fc\u00b7\u00dfer", "Ho\u00b7nig\u00b7fla\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ist bunter Blumenstrau\u00df;", "tokens": ["Ist", "bun\u00b7ter", "Blu\u00b7men\u00b7strau\u00df", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch hinter'm Fladen lauschet Bienenstachel,", "tokens": ["Doch", "hin\u00b7ter'm", "Fla\u00b7den", "lau\u00b7schet", "Bie\u00b7nen\u00b7sta\u00b7chel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im Strau\u00dfe Schlangenzahn.", "tokens": ["Im", "Strau\u00b7\u00dfe", "Schlan\u00b7gen\u00b7zahn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Des J\u00fcnglings ach! dem dieser Honigfladen", "tokens": ["Des", "J\u00fcng\u00b7lings", "ach", "!", "dem", "die\u00b7ser", "Ho\u00b7nig\u00b7fla\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ITJ", "$.", "ART", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den k\u00fchnen Gaumen reizt;", "tokens": ["Den", "k\u00fch\u00b7nen", "Gau\u00b7men", "reizt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Des M\u00e4dchens ach! die diesem Blumenstrau\u00dfe", "tokens": ["Des", "M\u00e4d\u00b7chens", "ach", "!", "die", "die\u00b7sem", "Blu\u00b7men\u00b7strau\u00b7\u00dfe"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ITJ", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den Busen anvertraut.", "tokens": ["Den", "Bu\u00b7sen", "an\u00b7ver\u00b7traut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Ihr saget: Unser Lied quillt reifem Alter,", "tokens": ["Ihr", "sa\u00b7get", ":", "Un\u00b7ser", "Lied", "quillt", "rei\u00b7fem", "Al\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPOSAT", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Jugend quillt es nicht.", "tokens": ["Der", "Ju\u00b7gend", "quillt", "es", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ha, nehmt ihr Ohr und Vorwitz, und dann saget:", "tokens": ["Ha", ",", "nehmt", "ihr", "Ohr", "und", "Vor\u00b7witz", ",", "und", "dann", "sa\u00b7get", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "PPOSAT", "NN", "KON", "NN", "$,", "KON", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Jugend quillt es nicht.", "tokens": ["Der", "Ju\u00b7gend", "quillt", "es", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Und k\u00f6nnt' es denn nicht auch der Jugend quellen?", "tokens": ["Und", "k\u00f6nnt'", "es", "denn", "nicht", "auch", "der", "Ju\u00b7gend", "quel\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "PTKNEG", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ha, Barden! blick't umher!", "tokens": ["Ha", ",", "Bar\u00b7den", "!", "blick't", "um\u00b7her", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "NN", "$.", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Scheint nichts, als thierverwandter Gliederk\u00fctzel", "tokens": ["Scheint", "nichts", ",", "als", "thier\u00b7ver\u00b7wand\u00b7ter", "Glie\u00b7der\u00b7k\u00fct\u00b7zel"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PIS", "$,", "KOUS", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Euch eurer Lieder werth?", "tokens": ["Euch", "eu\u00b7rer", "Lie\u00b7der", "werth", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Blick't auf! Hier oben herrsch't das erste Wesen,", "tokens": ["Blick't", "auf", "!", "Hier", "o\u00b7ben", "herr\u00b7sch't", "das", "ers\u00b7te", "We\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$.", "ADV", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sein Himmel unter ihm", "tokens": ["Sein", "Him\u00b7mel", "un\u00b7ter", "ihm"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit ewighellen Wundern rund behangen.", "tokens": ["Mit", "e\u00b7wig\u00b7hel\u00b7len", "Wun\u00b7dern", "rund", "be\u00b7han\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "San'g noch ein Bard' es aus?", "tokens": ["San'g", "noch", "ein", "Bard'", "es", "aus", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Begeistert Joseph, Friedrich keinen Busen?", "tokens": ["Be\u00b7geis\u00b7tert", "Jo\u00b7se\u00b7ph", ",", "Fried\u00b7rich", "kei\u00b7nen", "Bu\u00b7sen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "NE", "PIAT", "NN", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sind die genug gelobt?", "tokens": ["Sind", "die", "ge\u00b7nug", "ge\u00b7lobt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Blick't auf Theresien, auf Katharinen!", "tokens": ["Blick't", "auf", "The\u00b7re\u00b7si\u00b7en", ",", "auf", "Ka\u00b7tha\u00b7ri\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NE", "$,", "APPR", "NN", "$."], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Verdienen die kein Lied?", "tokens": ["Ver\u00b7die\u00b7nen", "die", "kein", "Lied", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Und nenn' ich alles, was Ges\u00e4nge wecket?", "tokens": ["Und", "nenn'", "ich", "al\u00b7les", ",", "was", "Ge\u00b7s\u00e4n\u00b7ge", "we\u00b7cket", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIS", "$,", "PWS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und nenn' ich's Barden! euch?", "tokens": ["Und", "nenn'", "ich's", "Bar\u00b7den", "!", "euch", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "NN", "$.", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wer weis es besser, w\u00fcrd' es besser singen", "tokens": ["Wer", "weis", "es", "bes\u00b7ser", ",", "w\u00fcrd'", "es", "bes\u00b7ser", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "PTKVZ", "PPER", "ADJD", "$,", "VAFIN", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ins Harfenspiel, als ihr?", "tokens": ["Ins", "Har\u00b7fen\u00b7spiel", ",", "als", "ihr", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "KOUS", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "O dann erschw\u00e4nge sich der Barden Jauchzen", "tokens": ["O", "dann", "er\u00b7schw\u00e4n\u00b7ge", "sich", "der", "Bar\u00b7den", "Jauch\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "ADV", "VVFIN", "PRF", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im weiten Erbe Teut's!", "tokens": ["Im", "wei\u00b7ten", "Er\u00b7be", "Teut's", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nun steh'n sie tr\u00fcblich eure Br\u00fcder, seufzen:", "tokens": ["Nun", "steh'n", "sie", "tr\u00fcb\u00b7lich", "eu\u00b7re", "Br\u00fc\u00b7der", ",", "seuf\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PPOSAT", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie sch\u00e4nden unsern Kranz!", "tokens": ["Sie", "sch\u00e4n\u00b7den", "un\u00b7sern", "Kranz", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Ihr lachet. Schulter steht an Schulter dr\u00e4ngend", "tokens": ["Ihr", "la\u00b7chet", ".", "Schul\u00b7ter", "steht", "an", "Schul\u00b7ter", "dr\u00e4n\u00b7gend"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "NN", "VVFIN", "APPR", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Um euch ein l\u00fcstern Volk.", "tokens": ["Um", "euch", "ein", "l\u00fcs\u00b7tern", "Volk", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hoch \u00fcber meine Klagen hebt sein R\u00fchmen,", "tokens": ["Hoch", "\u00fc\u