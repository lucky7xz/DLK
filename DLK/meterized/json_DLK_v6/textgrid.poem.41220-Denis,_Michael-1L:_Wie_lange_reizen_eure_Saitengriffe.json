": ["PPER", "VVFIN", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In mancher keuschen Brust.", "tokens": ["In", "man\u00b7cher", "keu\u00b7schen", "Brust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.21": {"line.1": {"text": "Wer heilet diese nun nach eurem Falle", "tokens": ["Wer", "hei\u00b7let", "die\u00b7se", "nun", "nach", "eu\u00b7rem", "Fal\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PDS", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von ihrer schn\u00f6den Sucht?", "tokens": ["Von", "ih\u00b7rer", "schn\u00f6\u00b7den", "Sucht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wer jene, die von diesen angestecket", "tokens": ["Wer", "je\u00b7ne", ",", "die", "von", "die\u00b7sen", "an\u00b7ge\u00b7ste\u00b7cket"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "PDS", "$,", "PRELS", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein gleiches Uebel fri\u00dft?", "tokens": ["Ein", "glei\u00b7ches", "Ue\u00b7bel", "fri\u00dft", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.22": {"line.1": {"text": "Wer d\u00e4mpfet eure Lieder? Ach sie schallen", "tokens": ["Wer", "d\u00e4mp\u00b7fet", "eu\u00b7re", "Lie\u00b7der", "?", "Ach", "sie", "schal\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPOSAT", "NN", "$.", "NN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im ganzen Erbe Teut's!", "tokens": ["Im", "gan\u00b7zen", "Er\u00b7be", "Teut's", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wer tilget sie? Sie dauern, wirken Unheil", "tokens": ["Wer", "til\u00b7get", "sie", "?", "Sie", "dau\u00b7ern", ",", "wir\u00b7ken", "Un\u00b7heil"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "$.", "PPER", "VVINF", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bis an der Zeiten End'!", "tokens": ["Bis", "an", "der", "Zei\u00b7ten", "End'", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.23": {"line.1": {"text": "So t\u00f6nt die Stimme. Werdet ihr dann lachen,", "tokens": ["So", "t\u00f6nt", "die", "Stim\u00b7me", ".", "Wer\u00b7det", "ihr", "dann", "la\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Euch Menschenlobes freu'n?", "tokens": ["Euch", "Men\u00b7schen\u00b7lo\u00b7bes", "freu'n", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie, oder wird der Aengsten f\u00fcrchterlichste", "tokens": ["Wie", ",", "o\u00b7der", "wird", "der", "A\u00b7engs\u00b7ten", "f\u00fcrch\u00b7ter\u00b7lichs\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "$,", "KON", "VAFIN", "ART", "NN", "VVFIN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Euch euren Geist durchgl\u00fch'n?", "tokens": ["Euch", "eu\u00b7ren", "Geist", "durch\u00b7gl\u00fch'n", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.24": {"line.1": {"text": "Der Hohn erwacht. Ihr seht ver\u00e4chtlich nieder", "tokens": ["Der", "Hohn", "er\u00b7wacht", ".", "Ihr", "seht", "ver\u00b7\u00e4cht\u00b7lich", "nie\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "$.", "PPER", "VVFIN", "ADJD", "PTKVZ"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf mich und auf mein Lied,", "tokens": ["Auf", "mich", "und", "auf", "mein", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "KON", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und geb't mich eurem br\u00fcnstigen Gefolge", "tokens": ["Und", "ge\u00b7b't", "mich", "eu\u00b7rem", "br\u00fcns\u00b7ti\u00b7gen", "Ge\u00b7fol\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Zum lauten Spotte preis.", "tokens": ["Zum", "lau\u00b7ten", "Spot\u00b7te", "preis", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.25": {"line.1": {"text": "Wohlan! ich will ihn meines Volkes wegen", "tokens": ["Wo\u00b7hlan", "!", "ich", "will", "ihn", "mei\u00b7nes", "Vol\u00b7kes", "we\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$.", "PPER", "VMFIN", "PPER", "PPOSAT", "NN", "APPR"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erdulden diesen Spott.", "tokens": ["E\u00b7rdul\u00b7den", "die\u00b7sen", "Spott", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mir ward kein Herz zum Hasse. Nur bedauren,", "tokens": ["Mir", "ward", "kein", "Herz", "zum", "Has\u00b7se", ".", "Nur", "be\u00b7dau\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "APPRART", "NN", "$.", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bedauren kann ich euch.", "tokens": ["Be\u00b7dau\u00b7ren", "kann", "ich", "euch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}