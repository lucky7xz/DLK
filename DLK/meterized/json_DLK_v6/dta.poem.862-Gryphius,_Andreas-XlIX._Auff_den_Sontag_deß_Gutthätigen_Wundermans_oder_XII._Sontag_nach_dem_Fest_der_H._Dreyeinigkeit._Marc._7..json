{"dta.poem.862": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "XlIX.  Auff den Sontag de\u00df Gutth\u00e4tigen  \n Wundermans/ oder  XII.  Sontag nach dem  \n Fest der H. Dreyeinigkeit. Marc. 7.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Wje kan ich HErr dein Lob vermehren/ ", "tokens": ["Wie", "kan", "ich", "Herr", "dein", "Lob", "ver\u00b7meh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "NN", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Weil mir die zunge sprachlo\u00df ligt", "tokens": ["Weil", "mir", "die", "zun\u00b7ge", "sprac\u00b7hlo\u00df", "ligt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df sich mein Hertz nicht nach dir f\u00fcgt:", "tokens": ["Da\u00df", "sich", "mein", "Hertz", "nicht", "nach", "dir", "f\u00fcgt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PPOSAT", "NN", "PTKNEG", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kommt\u2019 weil die Ohren gar nicht h\u00f6ren.", "tokens": ["Kommt'", "weil", "die", "Oh\u00b7ren", "gar", "nicht", "h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOUS", "ART", "NN", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie ", "tokens": ["Wie"], "token_info": ["word"], "pos": ["PWAV"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Weil mich die tolle Welt bekrigt ", "tokens": ["Weil", "mich", "die", "tol\u00b7le", "Welt", "be\u00b7krigt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wer hat den Lastern obgesigt ", "tokens": ["Wer", "hat", "den", "Las\u00b7tern", "ob\u00b7ge\u00b7sigt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "ART", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}}, "stanza.3": {"line.1": {"text": "Ach f\u00fchre mich weg von der Schar ", "tokens": ["Ach", "f\u00fch\u00b7re", "mich", "weg", "von", "der", "Schar"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "R\u00fchr an die Zunge/ die so gar ", "tokens": ["R\u00fchr", "an", "die", "Zun\u00b7ge", "/", "die", "so", "gar"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "$(", "ART", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dein ", "tokens": ["Dein"], "token_info": ["word"], "pos": ["PPOSAT"], "meter": "+", "measure": "single.up"}}, "stanza.4": {"line.1": {"text": "Thu auff mein Ohr da\u00df ich versp\u00fcr ", "tokens": ["Thu", "auff", "mein", "Ohr", "da\u00df", "ich", "ver\u00b7sp\u00fcr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "KOUS", "PPER", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie wol du di\u00df gemacht was wir/ ", "tokens": ["Wie", "wol", "du", "di\u00df", "ge\u00b7macht", "was", "wir", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "PDS", "VVPP", "PWS", "PPER", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Arm/ d\u00fcrfftig/ Taub vnd stum gefunden. ", "tokens": ["Arm", "/", "d\u00fcr\u00b7ff\u00b7tig", "/", "Taub", "vnd", "stum", "ge\u00b7fun\u00b7den", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "ADJD", "$(", "NN", "KON", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}