{"dta.poem.18166": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Ursachen warum uns GOtt  \n das Zuk\u00fcnftige verborgen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198805", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Menschen Neubegier, die sich mit Gril-", "tokens": ["Der", "Men\u00b7schen", "Neu\u00b7be\u00b7gier", ",", "die", "sich", "mit", "Gril"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "PRELS", "PRF", "APPR", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "len plagt,", "tokens": ["len", "plagt", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Ist niemahls th\u00f6rigter als wenn sie em-", "tokens": ["Ist", "nie\u00b7mahls", "th\u00f6\u00b7rig\u00b7ter", "als", "wenn", "sie", "em"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ADJD", "KOKOM", "KOUS", "PPER", "TRUNC"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "sig fragt", "tokens": ["sig", "fragt"], "token_info": ["word", "word"], "pos": ["ADJD", "VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Warum das ", "tokens": ["Wa\u00b7rum", "das"], "token_info": ["word", "word"], "pos": ["PWAV", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "verborgen,", "tokens": ["ver\u00b7bor\u00b7gen", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Darum wir Armen doch mit inren Gr\u00e4men sorgen.", "tokens": ["Da\u00b7rum", "wir", "Ar\u00b7men", "doch", "mit", "in\u00b7ren", "Gr\u00e4\u00b7men", "sor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "NN", "ADV", "APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der eine seufzet tief: Warum g\u00f6nt das Geschik", "tokens": ["Der", "ei\u00b7ne", "seuf\u00b7zet", "tief", ":", "Wa\u00b7rum", "g\u00f6nt", "das", "Ge\u00b7schik"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PIS", "VVFIN", "ADJD", "$.", "PWAV", "VVFIN", "ART", "NN"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Uns in die k\u00fcnftge Zeit nicht einen Vorsichts Blik,", "tokens": ["Uns", "in", "die", "k\u00fcnft\u00b7ge", "Zeit", "nicht", "ei\u00b7nen", "Vor\u00b7sichts", "Blik", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "PTKNEG", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Da\u00df wir schon zum voraus im gegenw\u00e4rtgen w\u00fcsten", "tokens": ["Da\u00df", "wir", "schon", "zum", "vo\u00b7raus", "im", "ge\u00b7gen\u00b7w\u00e4rt\u00b7gen", "w\u00fcs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPRART", "ADV", "APPRART", "ADJA", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Was wir in K\u00fcnftigen annoch erleben m\u00fcsten.", "tokens": ["Was", "wir", "in", "K\u00fcnf\u00b7ti\u00b7gen", "an\u00b7noch", "er\u00b7le\u00b7ben", "m\u00fcs\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "NN", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Der andre denket gar, ach! w\u00e4re mir mein Stand,", "tokens": ["Der", "and\u00b7re", "den\u00b7ket", "gar", ",", "ach", "!", "w\u00e4\u00b7re", "mir", "mein", "Stand", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "ADV", "$,", "ITJ", "$.", "VAFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Den ich bekleiden soll schon zum voraus bekand;", "tokens": ["Den", "ich", "be\u00b7klei\u00b7den", "soll", "schon", "zum", "vo\u00b7raus", "be\u00b7kand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "VVINF", "VMFIN", "ADV", "APPRART", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "So k\u00f6nt ich mich dazu, wie sichs geb\u00fchrt bereiten,", "tokens": ["So", "k\u00f6nt", "ich", "mich", "da\u00b7zu", ",", "wie", "sichs", "ge\u00b7b\u00fchrt", "be\u00b7rei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "PAV", "$,", "PWAV", "PIS", "VVPP", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und mit Geschikligkeit ein Ehren Amt bestreiten.", "tokens": ["Und", "mit", "Ge\u00b7schik\u00b7lig\u00b7keit", "ein", "Eh\u00b7ren", "Amt", "be\u00b7strei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Die Einfalt denket so; du blinder Sterblicher", "tokens": ["Die", "Ein\u00b7falt", "den\u00b7ket", "so", ";", "du", "blin\u00b7der", "Sterb\u00b7li\u00b7cher"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "$.", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.17": {"text": "Wenn dir schon zum voraus, dein Stand er\u00f6fnet", "tokens": ["Wenn", "dir", "schon", "zum", "vo\u00b7raus", ",", "dein", "Stand", "er\u00b7\u00f6f\u00b7net"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPRART", "PTKVZ", "$,", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "w\u00e4r;", "tokens": ["w\u00e4r", ";"], "token_info": ["word", "punct"], "pos": ["VAFIN", "$."], "meter": "+", "measure": "single.up"}, "line.19": {"text": "So w\u00fcrdest du vielmehr die beigelegten Gaben,", "tokens": ["So", "w\u00fcr\u00b7dest", "du", "viel\u00b7mehr", "die", "bei\u00b7ge\u00b7leg\u00b7ten", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-++--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.20": {"text": "Von Hofnung aufgebl\u00e4ht, in faulen Rost begraben.", "tokens": ["Von", "Hof\u00b7nung", "auf\u00b7ge\u00b7bl\u00e4ht", ",", "in", "fau\u00b7len", "Rost", "be\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Der Trieb der dich anspornt, auf das was unbestimmt,", "tokens": ["Der", "Trieb", "der", "dich", "ans\u00b7pornt", ",", "auf", "das", "was", "un\u00b7be\u00b7stimmt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "PPER", "VVFIN", "$,", "APPR", "ART", "PRELS", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Der Reitz der im Gem\u00fcth nach Ehr und Ansehn", "tokens": ["Der", "Reitz", "der", "im", "Ge\u00b7m\u00fcth", "nach", "Ehr", "und", "An\u00b7sehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "APPRART", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "glimmt", "tokens": ["glimmt"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+", "measure": "single.up"}, "line.24": {"text": "Der w\u00e4re auch erstikt, wenn auser dein Bestreben,", "tokens": ["Der", "w\u00e4\u00b7re", "auch", "ers\u00b7tikt", ",", "wenn", "au\u00b7ser", "dein", "Be\u00b7stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "VVPP", "$,", "KOUS", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Das Schiksahl unbedingt dir schon dein Loo\u00df gegeben.", "tokens": ["Das", "Schik\u00b7sahl", "un\u00b7be\u00b7dingt", "dir", "schon", "dein", "Loo\u00df", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Da aber dein Geschik dir vorher unbekant;", "tokens": ["Da", "a\u00b7ber", "dein", "Ge\u00b7schik", "dir", "vor\u00b7her", "un\u00b7be\u00b7kant", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "So h\u00e4lt das Ungewis die Lust in Glut und Brand;", "tokens": ["So", "h\u00e4lt", "das", "Un\u00b7ge\u00b7wis", "die", "Lust", "in", "Glut", "und", "Brand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "So mu\u00df dein reger Fleis, wenn sich dein Muth will", "tokens": ["So", "mu\u00df", "dein", "re\u00b7ger", "Fleis", ",", "wenn", "sich", "dein", "Muth", "will"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPOSAT", "ADJA", "NN", "$,", "KOUS", "PRF", "PPOSAT", "NN", "VMFIN"], "meter": "+-+--+--++-", "measure": "trochaic.penta.relaxed"}, "line.29": {"text": "schwingen,", "tokens": ["schwin\u00b7gen", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.30": {"text": "Dich zu dem Ehrenport auf Hofnungs Fl\u00fcgeln brin-", "tokens": ["Dich", "zu", "dem", "Eh\u00b7ren\u00b7port", "auf", "Hof\u00b7nungs", "Fl\u00fc\u00b7geln", "brin"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "NN", "APPR", "NN", "NN", "TRUNC"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.31": {"text": "gen.", "tokens": ["gen", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-", "measure": "single.down"}, "line.32": {"text": "Wie w\u00fcstest du das Gl\u00fck, das dir begegnen soll;", "tokens": ["Wie", "w\u00fcs\u00b7test", "du", "das", "Gl\u00fck", ",", "das", "dir", "be\u00b7geg\u00b7nen", "soll", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "NN", "$,", "PRELS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "So machte dich der Blik schon von der Freude voll;", "tokens": ["So", "mach\u00b7te", "dich", "der", "Blik", "schon", "von", "der", "Freu\u00b7de", "voll", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "ADV", "APPR", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Eh du die S\u00fc\u00dfigkeit des Schiksahls kanst geniessen,", "tokens": ["Eh", "du", "die", "S\u00fc\u00b7\u00dfig\u00b7keit", "des", "Schik\u00b7sahls", "kanst", "ge\u00b7nies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "So w\u00fcrde dein Gem\u00fcth darauf schon hungern m\u00fcssen.", "tokens": ["So", "w\u00fcr\u00b7de", "dein", "Ge\u00b7m\u00fcth", "da\u00b7rauf", "schon", "hun\u00b7gern", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "PAV", "ADV", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Und k\u00e4mst du zum Genus; so w\u00e4r der Ekel dar,", "tokens": ["Und", "k\u00e4mst", "du", "zum", "Ge\u00b7nus", ";", "so", "w\u00e4r", "der", "E\u00b7kel", "dar", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "$.", "ADV", "VAFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Weil, was man vor gesehn und schon gewohnet", "tokens": ["Weil", ",", "was", "man", "vor", "ge\u00b7sehn", "und", "schon", "ge\u00b7woh\u00b7net"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "$,", "PRELS", "PIS", "APPR", "VVPP", "KON", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.38": {"text": "war", "tokens": ["war"], "token_info": ["word"], "pos": ["VAFIN"], "meter": "-", "measure": "single.down"}, "line.39": {"text": "Uns nicht mehr so erfreut. Was uns recht soll erg\u00f6zzen", "tokens": ["Uns", "nicht", "mehr", "so", "er\u00b7freut", ".", "Was", "uns", "recht", "soll", "er\u00b7g\u00f6z\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "PTKNEG", "ADV", "ADV", "ADJD", "$.", "PWS", "PPER", "ADV", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Das mu\u00df uns zum voraus in bange Hofnung sezzen.", "tokens": ["Das", "mu\u00df", "uns", "zum", "vo\u00b7raus", "in", "ban\u00b7ge", "Hof\u00b7nung", "sez\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "APPRART", "ADV", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Wenn dir des Himmels Schlus vorhero schon ge-", "tokens": ["Wenn", "dir", "des", "Him\u00b7mels", "Schlus", "vor\u00b7he\u00b7ro", "schon", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "NE", "ADV", "ADV", "TRUNC"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.42": {"text": "lehrt,", "tokens": ["lehrt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.43": {"text": "Durch einen Ungl\u00fcksstern, was dir zur Last beschert;", "tokens": ["Durch", "ei\u00b7nen", "Un\u00b7gl\u00fckss\u00b7tern", ",", "was", "dir", "zur", "Last", "be\u00b7schert", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PWS", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.44": {"text": "So w\u00fcrde dein Gem\u00fct, noch vor den Kummertagen,", "tokens": ["So", "w\u00fcr\u00b7de", "dein", "Ge\u00b7m\u00fct", ",", "noch", "vor", "den", "Kum\u00b7mer\u00b7ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "$,", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Mit der bestimmten Angst, dich wie mit Foltern", "tokens": ["Mit", "der", "be\u00b7stimm\u00b7ten", "Angst", ",", "dich", "wie", "mit", "Fol\u00b7tern"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PPER", "KOKOM", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.46": {"text": "plagen;", "tokens": ["pla\u00b7gen", ";"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.47": {"text": "So machte dir die Furcht, bei jeden Augenblik", "tokens": ["So", "mach\u00b7te", "dir", "die", "Furcht", ",", "bei", "je\u00b7den", "Au\u00b7gen\u00b7blik"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Schon ein beklomnes Herz; So w\u00e4r dein Ungel\u00fck", "tokens": ["Schon", "ein", "be\u00b7klom\u00b7nes", "Herz", ";", "So", "w\u00e4r", "dein", "Un\u00b7ge\u00b7l\u00fck"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "$.", "ADV", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Viel unertr\u00e4glicher, als da es dir verborgen:", "tokens": ["Viel", "un\u00b7er\u00b7tr\u00e4g\u00b7li\u00b7cher