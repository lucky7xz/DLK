{"textgrid.poem.62443": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fchlingsgru\u00df an das Vaterland", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie mir deine Freuden winken", "tokens": ["Wie", "mir", "dei\u00b7ne", "Freu\u00b7den", "win\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nach der Knechtschaft, nach dem Streit!", "tokens": ["Nach", "der", "Knecht\u00b7schaft", ",", "nach", "dem", "Streit", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Vaterland, ich mu\u00df versinken", "tokens": ["Va\u00b7ter\u00b7land", ",", "ich", "mu\u00df", "ver\u00b7sin\u00b7ken"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PPER", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hier in deiner Herrlichkeit.", "tokens": ["Hier", "in", "dei\u00b7ner", "Herr\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wo die hohen Eichen sausen,", "tokens": ["Wo", "die", "ho\u00b7hen", "Ei\u00b7chen", "sau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Himmelan das Haupt gewandt,", "tokens": ["Him\u00b7me\u00b7lan", "das", "Haupt", "ge\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wo die starken Str\u00f6me brausen,", "tokens": ["Wo", "die", "star\u00b7ken", "Str\u00f6\u00b7me", "brau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Alles das ist deutsches Land.", "tokens": ["Al\u00b7les", "das", "ist", "deut\u00b7sches", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PDS", "VAFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Von dem Rheinfall hergegangen", "tokens": ["Von", "dem", "Rhein\u00b7fall", "her\u00b7ge\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Komm' ich, von der Donau Quell,", "tokens": ["Komm'", "ich", ",", "von", "der", "Do\u00b7nau", "Quell", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "APPR", "ART", "NE", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und in mir sind aufgegangen", "tokens": ["Und", "in", "mir", "sind", "auf\u00b7ge\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "VAFIN", "VVPP"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Liebessterne mild und hell;", "tokens": ["Lie\u00b7bess\u00b7ter\u00b7ne", "mild", "und", "hell", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KON", "ADJD", "$."], "meter": "+---+-+", "measure": "dactylic.init"}, "line.5": {"text": "Niedersteigen will ich, strahlen", "tokens": ["Nie\u00b7ders\u00b7tei\u00b7gen", "will", "ich", ",", "strah\u00b7len"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ADV", "VMFIN", "PPER", "$,", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Soll von mir der Freudenschein", "tokens": ["Soll", "von", "mir", "der", "Freu\u00b7den\u00b7schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "PPER", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "In des Neckars frohen Thalen", "tokens": ["In", "des", "Ne\u00b7ckars", "fro\u00b7hen", "Tha\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und am silberblauen Main.", "tokens": ["Und", "am", "sil\u00b7ber\u00b7blau\u00b7en", "Main", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "ADJA", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.3": {"line.1": {"text": "Weiter, weiter mu\u00dft du dringen,", "tokens": ["Wei\u00b7ter", ",", "wei\u00b7ter", "mu\u00dft", "du", "drin\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Du mein deutscher Freiheitsgru\u00df,", "tokens": ["Du", "mein", "deut\u00b7scher", "Frei\u00b7heits\u00b7gru\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sollst vor meiner H\u00fctte klingen", "tokens": ["Sollst", "vor", "mei\u00b7ner", "H\u00fct\u00b7te", "klin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "An dem fernen Memelflu\u00df.", "tokens": ["An", "dem", "fer\u00b7nen", "Me\u00b7mel\u00b7flu\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wo noch deutsche Worte gelten,", "tokens": ["Wo", "noch", "deut\u00b7sche", "Wor\u00b7te", "gel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wo die Herzen, stark und weich,", "tokens": ["Wo", "die", "Her\u00b7zen", ",", "stark", "und", "weich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Zu dem Freiheitskampf sich stellten,", "tokens": ["Zu", "dem", "Frei\u00b7heits\u00b7kampf", "sich", "stell\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ist auch heil'ges deutsches Reich.", "tokens": ["Ist", "auch", "heil'\u00b7ges", "deut\u00b7sches", "Reich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Alles ist in Gr\u00fcn gekleidet,", "tokens": ["Al\u00b7les", "ist", "in", "Gr\u00fcn", "ge\u00b7klei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Alles strahlt im jungen Licht,", "tokens": ["Al\u00b7les", "strahlt", "im", "jun\u00b7gen", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Anger, wo die Heerde weidet,", "tokens": ["An\u00b7ger", ",", "wo", "die", "Heer\u00b7de", "wei\u00b7det", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00fcgel, wo man Trauben bricht.", "tokens": ["H\u00fc\u00b7gel", ",", "wo", "man", "Trau\u00b7ben", "bricht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PIS", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Vaterland! in tausend Jahren", "tokens": ["Va\u00b7ter\u00b7land", "!", "in", "tau\u00b7send", "Jah\u00b7ren"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "APPR", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Kam dir solch ein Fr\u00fchling kaum,", "tokens": ["Kam", "dir", "solch", "ein", "Fr\u00fch\u00b7ling", "kaum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PIAT", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Was die hohen V\u00e4ter waren,", "tokens": ["Was", "die", "ho\u00b7hen", "V\u00e4\u00b7ter", "wa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Hei\u00dfet nimmermehr ein Traum.", "tokens": ["Hei\u00b7\u00dfet", "nim\u00b7mer\u00b7mehr", "ein", "Traum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Aber einmal m\u00fc\u00dft ihr ringen", "tokens": ["A\u00b7ber", "ein\u00b7mal", "m\u00fc\u00dft", "ihr", "rin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VMFIN", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Noch in ernster Geisterschlacht", "tokens": ["Noch", "in", "erns\u00b7ter", "Geis\u00b7ter\u00b7schlacht"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und den letzten Feind bezwingen,", "tokens": ["Und", "den", "letz\u00b7ten", "Feind", "be\u00b7zwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der im Innern drohend wacht.", "tokens": ["Der", "im", "In\u00b7nern", "dro\u00b7hend", "wacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "VVPP", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ha\u00df und Argwohn m\u00fc\u00dft ihr d\u00e4mpfen,", "tokens": ["Ha\u00df", "und", "Arg\u00b7wohn", "m\u00fc\u00dft", "ihr", "d\u00e4mp\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Geiz und Neid und b\u00f6se Lust,", "tokens": ["Geiz", "und", "Neid", "und", "b\u00f6\u00b7se", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Dann nach schweren langen K\u00e4mpfen", "tokens": ["Dann", "nach", "schwe\u00b7ren", "lan\u00b7gen", "K\u00e4mp\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Kannst du ruhen, deutsche Brust.", "tokens": ["Kannst", "du", "ru\u00b7hen", ",", "deut\u00b7sche", "Brust", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Jeder ist dann reich an Ehren,", "tokens": ["Je\u00b7der", "ist", "dann", "reich", "an", "Eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ADV", "ADJD", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Reich an Demuth und an Macht;", "tokens": ["Reich", "an", "De\u00b7muth", "und", "an", "Macht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "So nur kann sich recht verkl\u00e4ren", "tokens": ["So", "nur", "kann", "sich", "recht", "ver\u00b7kl\u00e4\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VMFIN", "PRF", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unsers Kaisers heil'ge Pracht.", "tokens": ["Un\u00b7sers", "Kai\u00b7sers", "heil'\u00b7ge", "Pracht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Alle S\u00fcnden m\u00fcssen sterben", "tokens": ["Al\u00b7le", "S\u00fcn\u00b7den", "m\u00fcs\u00b7sen", "ster\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "NN", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "In der gottgesandten Flut,", "tokens": ["In", "der", "gott\u00b7ge\u00b7sand\u00b7ten", "Flut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und an ", "tokens": ["Und", "an"], "token_info": ["word", "word"], "pos": ["KON", "APPR"], "meter": "-+", "measure": "iambic.single"}, "line.8": {"text": "Fallen das ents\u00fchnte Gut.", "tokens": ["Fal\u00b7len", "das", "ent\u00b7s\u00fchn\u00b7te", "Gut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Segen Gottes auf den Feldern,", "tokens": ["Se\u00b7gen", "Got\u00b7tes", "auf", "den", "Fel\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In des Weinstocks heil'ger Frucht,", "tokens": ["In", "des", "Wein\u00b7stocks", "heil'\u00b7ger", "Frucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Manneslust in gr\u00fcnen W\u00e4ldern,", "tokens": ["Man\u00b7nes\u00b7lust", "in", "gr\u00fc\u00b7nen", "W\u00e4l\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In den H\u00fctten frohe Zucht;", "tokens": ["In", "den", "H\u00fct\u00b7ten", "fro\u00b7he", "Zucht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "In der Brust ein frommes Sehnen,", "tokens": ["In", "der", "Brust", "ein", "from\u00b7mes", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ew'ger Freiheit Unterpfand,", "tokens": ["Ew'\u00b7ger", "Frei\u00b7heit", "Un\u00b7ter\u00b7pfand", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Liebe spricht in zarten T\u00f6nen", "tokens": ["Lie\u00b7be", "spricht", "in", "zar\u00b7ten", "T\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Nirgends wie im deutschen Land.", "tokens": ["Nir\u00b7gends", "wie", "im", "deut\u00b7schen", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Ihr in Schl\u00f6ssern, ihr in St\u00e4dten,", "tokens": ["Ihr", "in", "Schl\u00f6s\u00b7sern", ",", "ihr", "in", "St\u00e4d\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "$,", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Welche schm\u00fccken unser Land,", "tokens": ["Wel\u00b7che", "schm\u00fc\u00b7cken", "un\u00b7ser", "Land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ackersmann, der auf den Beeten", "tokens": ["A\u00b7ckers\u00b7mann", ",", "der", "auf", "den", "Bee\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "+--++-+-", "measure": "dactylic.init"}, "line.4": {"text": "Deutsche Frucht in Garben band,", "tokens": ["Deut\u00b7sche", "Frucht", "in"