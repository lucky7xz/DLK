{"dta.poem.14725": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Waldgespr\u00e4ch .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "So reich geschm\u00fcckt ist Ro\u00df und Weib,", "tokens": ["So", "reich", "ge\u00b7schm\u00fcckt", "ist", "Ro\u00df", "und", "Weib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So wundersch\u00f6n der junge Leib,", "tokens": ["So", "wun\u00b7der\u00b7sch\u00f6n", "der", "jun\u00b7ge", "Leib", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Jetzt kenn' ich Dich \u2014 Gott steh' mir bei!", "tokens": ["Jetzt", "kenn'", "ich", "Dich", "Gott", "steh'", "mir", "bei", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$(", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du bist die Hexe Loreley.", "tokens": ["Du", "bist", "die", "He\u00b7xe", "Lo\u00b7re\u00b7ley", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}