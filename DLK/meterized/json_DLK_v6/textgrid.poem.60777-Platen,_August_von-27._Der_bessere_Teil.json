{"textgrid.poem.60777": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "27. Der bessere Teil", "genre": "verse", "period": "N.A.", "pub_year": 1830, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jung und harmlos ist die Natur, der Mensch nur", "tokens": ["Jung", "und", "harm\u00b7los", "ist", "die", "Na\u00b7tur", ",", "der", "Mensch", "nur"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "KON", "ADJD", "VAFIN", "ART", "NN", "$,", "ART", "NN", "ADV"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Altert, Schuld aufh\u00e4ufend umher und Elend;", "tokens": ["Al\u00b7tert", ",", "Schuld", "auf\u00b7h\u00e4u\u00b7fend", "um\u00b7her", "und", "E\u00b7lend", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "VVPP", "PTKVZ", "KON", "NN", "$."], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Drum verhie\u00df ihm auch die gerechte Vorsicht", "tokens": ["Drum", "ver\u00b7hie\u00df", "ihm", "auch", "die", "ge\u00b7rech\u00b7te", "Vor\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Tod und Erl\u00f6sung.", "tokens": ["Tod", "und", "Er\u00b7l\u00f6\u00b7sung", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Stets von heut auf morgen vertagt die Hoffnung", "tokens": ["Stets", "von", "heut", "auf", "mor\u00b7gen", "ver\u00b7tagt", "die", "Hoff\u00b7nung"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADV", "APPR", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihr Phantom. Auswandert der Mensch in fremden", "tokens": ["Ihr", "Phan\u00b7tom", ".", "Aus\u00b7wan\u00b7dert", "der", "Mensch", "in", "frem\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "NN", "ART", "NN", "APPR", "ADJA"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Himmelsstrich; doch tauscht er indes die Not nur", "tokens": ["Him\u00b7mels\u00b7strich", ";", "doch", "tauscht", "er", "in\u00b7des", "die", "Not", "nur"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$.", "ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "ADV"], "meter": "+-+-+-++-+-", "measure": "unknown.measure.hexa"}, "line.4": {"text": "Gegen die Not aus!", "tokens": ["Ge\u00b7gen", "die", "Not", "aus", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Stets um Freiheit buhlt das Gem\u00fct, um Kenntnis;", "tokens": ["Stets", "um", "Frei\u00b7heit", "buhlt", "das", "Ge\u00b7m\u00fct", ",", "um", "Kennt\u00b7nis", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVFIN", "ART", "NN", "$,", "KOUI", "NN", "$."], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Doch um uns liegt rings, wie ein Reif, Beschr\u00e4nkung:", "tokens": ["Doch", "um", "uns", "liegt", "rings", ",", "wie", "ein", "Reif", ",", "Be\u00b7schr\u00e4n\u00b7kung", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "KOUI", "PPER", "VVFIN", "ADV", "$,", "PWAV", "ART", "NN", "$,", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Keine Kraft, selbst Tugend vermag der Zeit nicht", "tokens": ["Kei\u00b7ne", "Kraft", ",", "selbst", "Tu\u00b7gend", "ver\u00b7mag", "der", "Zeit", "nicht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "$,", "ADV", "NN", "VVFIN", "ART", "NN", "PTKNEG"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Immer zu trotzen.", "tokens": ["Im\u00b7mer", "zu", "trot\u00b7zen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Manchen Flug wagt menschliches Wissen, das doch", "tokens": ["Man\u00b7chen", "Flug", "wagt", "menschli\u00b7ches", "Wis\u00b7sen", ",", "das", "doch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "ADJA", "NN", "$,", "PRELS", "ADV"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Kaum ein Blatt aufschl\u00e4gt in dem Buch des Weltalls:", "tokens": ["Kaum", "ein", "Blatt", "auf\u00b7schl\u00e4gt", "in", "dem", "Buch", "des", "Welt\u00b7alls", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "--+-++-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Bist du je, Milchstra\u00dfen entlang, gewandelt", "tokens": ["Bist", "du", "je", ",", "Milch\u00b7stra\u00b7\u00dfen", "ent\u00b7lang", ",", "ge\u00b7wan\u00b7delt"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["VAFIN", "PPER", "ADV", "$,", "NN", "PTKVZ", "$,", "VVPP"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Nach dem Orion?", "tokens": ["Nach", "dem", "O\u00b7rion", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.5": {"line.1": {"text": "Nein \u2013 und deshalb lehrte der Mann der Weisheit,", "tokens": ["Nein", "\u2013", "und", "des\u00b7halb", "lehr\u00b7te", "der", "Mann", "der", "Weis\u00b7heit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$(", "KON", "PAV", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Den die Welt dankbar den Erl\u00f6ser nannte,", "tokens": ["Den", "die", "Welt", "dank\u00b7bar", "den", "Er\u00b7l\u00f6\u00b7ser", "nann\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Zuversicht auf h\u00f6heren Waltens Allmacht,", "tokens": ["Zu\u00b7ver\u00b7sicht", "auf", "h\u00f6\u00b7he\u00b7ren", "Wal\u00b7tens", "All\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Lehrte den Glauben.", "tokens": ["Lehr\u00b7te", "den", "Glau\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.6": {"line.1": {"text": "T\u00e4tigkeit l\u00f6st R\u00e4tsel und baut der Menschheit", "tokens": ["T\u00e4\u00b7tig\u00b7keit", "l\u00f6st", "R\u00e4t\u00b7sel", "und", "baut", "der", "Menschheit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "NN", "KON", "VVFIN", "ART", "NN"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Sch\u00f6nstes Werk; doch schm\u00e4he sie drum ein stilles,", "tokens": ["Sch\u00f6ns\u00b7tes", "Werk", ";", "doch", "schm\u00e4\u00b7he", "sie", "drum", "ein", "stil\u00b7les", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "ADV", "VVFIN", "PPER", "PAV", "ART", "ADJA", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Sanftes Herz nicht, weil es erw\u00e4hlt den bessern", "tokens": ["Sanf\u00b7tes", "Herz", "nicht", ",", "weil", "es", "er\u00b7w\u00e4hlt", "den", "bes\u00b7sern"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "PTKNEG", "$,", "KOUS", "PPER", "VVFIN", "ART", "ADJA"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Teil, wie Maria!", "tokens": ["Teil", ",", "wie", "Ma\u00b7ria", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "NE", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}}}}