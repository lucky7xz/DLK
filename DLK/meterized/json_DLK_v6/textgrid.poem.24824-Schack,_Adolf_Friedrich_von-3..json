{"textgrid.poem.24824": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von dunklem Schleier umsponnen", "tokens": ["Von", "dunk\u00b7lem", "Schlei\u00b7er", "um\u00b7spon\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ist mir das Tageslicht;", "tokens": ["Ist", "mir", "das", "Ta\u00b7ges\u00b7licht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wohl steigen neue Sonnen \u2013", "tokens": ["Wohl", "stei\u00b7gen", "neu\u00b7e", "Son\u00b7nen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich seh' sie nicht.", "tokens": ["Ich", "seh'", "sie", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Mir schweift der Blick hin\u00fcber", "tokens": ["Mir", "schweift", "der", "Blick", "hin\u00b7\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In Weiten, d\u00e4mmerfern;", "tokens": ["In", "Wei\u00b7ten", ",", "d\u00e4m\u00b7mer\u00b7fern", ";"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vom Himmel blinkt ein tr\u00fcber", "tokens": ["Vom", "Him\u00b7mel", "blinkt", "ein", "tr\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Einsamer Stern.", "tokens": ["Ein\u00b7sa\u00b7mer", "Stern", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Ein M\u00e4dchen, bleich von Wangen,", "tokens": ["Ein", "M\u00e4d\u00b7chen", ",", "bleich", "von", "Wan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Winkt mir von dr\u00fcben zu:", "tokens": ["Winkt", "mir", "von", "dr\u00fc\u00b7ben", "zu", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADV", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "\u00bbich bin vorangegangen;", "tokens": ["\u00bb", "ich", "bin", "vor\u00b7an\u00b7ge\u00b7gan\u00b7gen", ";"], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was z\u00f6gerst du?\u00ab", "tokens": ["Was", "z\u00f6\u00b7gerst", "du", "?", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}}}}}