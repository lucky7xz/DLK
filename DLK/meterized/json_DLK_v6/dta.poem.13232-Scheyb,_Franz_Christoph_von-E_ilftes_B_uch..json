{"dta.poem.13232": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "E ilftes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201ehingegen schickte GOtt uns Beystand, Muth und Rath,", "tokens": ["\u201e", "hin\u00b7ge\u00b7gen", "schick\u00b7te", "Gott", "uns", "Beys\u00b7tand", ",", "Muth", "und", "Rath", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "NN", "PPER", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "---+-+-+-+-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "70\u201dMit welchem unser Heer dem Feind entgegen trat.", "tokens": ["\"", "Mit", "wel\u00b7chem", "un\u00b7ser", "Heer", "dem", "Feind", "ent\u00b7ge\u00b7gen", "trat", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PWAT", "PPOSAT", "NN", "ART", "NN", "PTKVZ", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201edurch ihn befindet sich der Feind in seinen Schrancken;", "tokens": ["\u201e", "durch", "ihn", "be\u00b7fin\u00b7det", "sich", "der", "Feind", "in", "sei\u00b7nen", "Schran\u00b7cken", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PPER", "VVFIN", "PRF", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201eso wissen wir ja nichts als ihm darum zu dancken.", "tokens": ["\u201e", "so", "wis\u00b7sen", "wir", "ja", "nichts", "als", "ihm", "da\u00b7rum", "zu", "dan\u00b7cken", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "pun