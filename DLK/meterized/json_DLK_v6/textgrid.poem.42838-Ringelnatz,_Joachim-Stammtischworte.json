{"textgrid.poem.42838": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Stammtischworte", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn ein Schiffbruch dich ins nasse Element", "tokens": ["Wenn", "ein", "Schiff\u00b7bruch", "dich", "ins", "nas\u00b7se", "E\u00b7le\u00b7ment"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PPER", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Setzte. Wenn, wie es der Seemann nennt,", "tokens": ["Setz\u00b7te", ".", "Wenn", ",", "wie", "es", "der", "See\u00b7mann", "nennt", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "KOUS", "$,", "PWAV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-----+-+", "measure": "dactylic.init"}, "line.3": {"text": "K\u00e4lberk\u00f6pfe auf dem Meere zischen", "tokens": ["K\u00e4l\u00b7ber\u00b7k\u00f6p\u00b7fe", "auf", "dem", "Mee\u00b7re", "zi\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und ein Rettungsboot sich dir n\u00e4herw\u00fcrgt,", "tokens": ["Und", "ein", "Ret\u00b7tungs\u00b7boot", "sich", "dir", "n\u00e4\u00b7her\u00b7w\u00fcrgt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PRF", "PPER", "VVFIN", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Ja, dann ist noch lange nicht verb\u00fcrgt,", "tokens": ["Ja", ",", "dann", "ist", "noch", "lan\u00b7ge", "nicht", "ver\u00b7b\u00fcrgt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "VAFIN", "ADV", "ADV", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Ob sie dich erwischen.", "tokens": ["Ob", "sie", "dich", "er\u00b7wi\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Wenn du eine Blase wie ein Hai,", "tokens": ["Wenn", "du", "ei\u00b7ne", "Bla\u00b7se", "wie", "ein", "Hai", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Eine Nase wie ein Papagei", "tokens": ["Ei\u00b7ne", "Na\u00b7se", "wie", "ein", "Pa\u00b7pa\u00b7gei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KOKOM", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Oder Flossen an den Ohren h\u00e4ttest,", "tokens": ["O\u00b7der", "Flos\u00b7sen", "an", "den", "Oh\u00b7ren", "h\u00e4t\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ART", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Fragt es dennoch sich,", "tokens": ["Fragt", "es", "den\u00b7noch", "sich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PRF", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Ob du dich", "tokens": ["Ob", "du", "dich"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "PRF"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Rettest. \u2013 \u2013 \u2013", "tokens": ["Ret\u00b7test", ".", "\u2013", "\u2013", "\u2013"], "token_info": ["word", "punct", "punct", "punct", "punct"], "pos": ["NE", "$.", "$(", "$(", "$("], "meter": "+-", "measure": "trochaic.single"}}, "stanza.3": {"line.1": {"text": "Meinetwegen lasse dir dein Leben", "tokens": ["Mei\u00b7net\u00b7we\u00b7gen", "las\u00b7se", "dir", "dein", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Hoch versichern und dir Vorschu\u00df geben.", "tokens": ["Hoch", "ver\u00b7si\u00b7chern", "und", "dir", "Vor\u00b7schu\u00df", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "KON", "PPER", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Wollen sehen, wie der Hase l\u00e4uft.", "tokens": ["Wol\u00b7len", "se\u00b7hen", ",", "wie", "der", "Ha\u00b7se", "l\u00e4uft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "PWAV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Doch ich w\u00e4r' der erste, der sich freute,", "tokens": ["Doch", "ich", "w\u00e4r'", "der", "ers\u00b7te", ",", "der", "sich", "freu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ART", "ADJA", "$,", "PRELS", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Wenn dein Erbe, wenn du heute", "tokens": ["Wenn", "dein", "Er\u00b7be", ",", "wenn", "du", "heu\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "$,", "KOUS", "PPER", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Fern auf See ertr\u00e4nkest, morgen auch ers\u00e4uft. \u2013 \u2013 \u2013", "tokens": ["Fern", "auf", "See", "er\u00b7tr\u00e4n\u00b7kest", ",", "mor\u00b7gen", "auch", "er\u00b7s\u00e4uft", ".", "\u2013", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["NN", "APPR", "NN", "VVFIN", "$,", "ADV", "ADV", "VVPP", "$.", "$(", "$(", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.7": {"text": "Manchmal spart Ertrinken das Begraben. \u2013", "tokens": ["Manch\u00b7mal", "spart", "Er\u00b7trin\u00b7ken", "das", "Be\u00b7gra\u00b7ben", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJD", "NN", "ART", "NN", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "W\u00e4rst du nicht schon gar so alt und m\u00fcrbe,", "tokens": ["W\u00e4rst", "du", "nicht", "schon", "gar", "so", "alt", "und", "m\u00fcr\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADV", "ADV", "ADV", "ADJD", "KON", "ADJA", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "W\u00fcnschte ich mir, da\u00df ich vor dir st\u00fcrbe.", "tokens": ["W\u00fcnschte", "ich", "mir", ",", "da\u00df", "ich", "vor", "dir", "st\u00fcr\u00b7be", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$,", "KOUS", "PPER", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Jetzt will ich noch einen Whisky haben.", "tokens": ["Jetzt", "will", "ich", "noch", "ei\u00b7nen", "Whis\u00b7ky", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ART", "NN", "VAFIN", "$."], "meter": "++--+-+-+-", "measure": "trochaic.penta.relaxed"}}}}}