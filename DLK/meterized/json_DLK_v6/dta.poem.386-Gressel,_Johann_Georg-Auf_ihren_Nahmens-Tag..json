{"dta.poem.386": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Auf ihren Nahmens-Tag.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Osonne weiche nur mit deinem Glantz zur\u00fccke/", "tokens": ["O\u00b7son\u00b7ne", "wei\u00b7che", "nur", "mit", "dei\u00b7nem", "Glantz", "zu\u00b7r\u00fc\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "ADV", "APPR", "PPOSAT", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Solinde giebet uns die sch\u00f6nsten Gnaden-Blicke", "tokens": ["So\u00b7lin\u00b7de", "gie\u00b7bet", "uns", "die", "sch\u00f6ns\u00b7ten", "Gna\u00b7den\u00b7Bli\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Jhr holdes Nahmen-Licht prangt unvergleichlich sch\u00f6n", "tokens": ["Ihr", "hol\u00b7des", "Nah\u00b7men\u00b7Licht", "prangt", "un\u00b7ver\u00b7gleich\u00b7lich", "sch\u00f6n"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ADJD", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zwey Sonnen l\u00e4\u00dft es uns an ihrer Stirnen sehn.", "tokens": ["Zwey", "Son\u00b7nen", "l\u00e4\u00dft", "es", "uns", "an", "ih\u00b7rer", "Stir\u00b7nen", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+