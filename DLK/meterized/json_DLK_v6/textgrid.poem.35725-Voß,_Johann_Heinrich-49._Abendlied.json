{"textgrid.poem.35725": {"metadata": {"author": {"name": "Vo\u00df, Johann Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "49. Abendlied", "genre": "verse", "period": "N.A.", "pub_year": 1794, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Tagewerk ist abgethan.", "tokens": ["Das", "Ta\u00b7ge\u00b7werk", "ist", "ab\u00b7ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gieb, Vater, deinen Segen!", "tokens": ["Gieb", ",", "Va\u00b7ter", ",", "dei\u00b7nen", "Se\u00b7gen", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "$,", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nun d\u00fcrfen wir der Ruhe nahn;", "tokens": ["Nun", "d\u00fcr\u00b7fen", "wir", "der", "Ru\u00b7he", "nahn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wir thaten nach Verm\u00f6gen.", "tokens": ["Wir", "tha\u00b7ten", "nach", "Ver\u00b7m\u00f6\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die holde Nacht umh\u00fcllt die Welt,", "tokens": ["Die", "hol\u00b7de", "Nacht", "um\u00b7h\u00fcllt", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und Stille herrscht in Dorf und Feld.", "tokens": ["Und", "Stil\u00b7le", "herrscht", "in", "Dorf", "und", "Feld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ohn' Ende kreist der Rundelauf", "tokens": ["Ohn'", "En\u00b7de", "kreist", "der", "Run\u00b7de\u00b7lauf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der eitlen Lebenssorgen;", "tokens": ["Der", "eit\u00b7len", "Le\u00b7bens\u00b7sor\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Den M\u00fcden nimmt der Abend auf,", "tokens": ["Den", "M\u00fc\u00b7den", "nimmt", "der", "A\u00b7bend", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihn weckt der andre Morgen.", "tokens": ["Ihn", "weckt", "der", "and\u00b7re", "Mor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Man trachtet, hofft, genie\u00dft, wird satt;", "tokens": ["Man", "trach\u00b7tet", ",", "hofft", ",", "ge\u00b7nie\u00dft", ",", "wird", "satt", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gro\u00df sieht's, wer w\u00fcnscht, und klein, wer hat.", "tokens": ["Gro\u00df", "sieht's", ",", "wer", "w\u00fcnscht", ",", "und", "klein", ",", "wer", "hat", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PWS", "VVFIN", "$,", "KON", "ADJD", "$,", "PWS", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Aus Lieb' hat uns der Vater Schwei\u00df", "tokens": ["Aus", "Lieb'", "hat", "uns", "der", "Va\u00b7ter", "Schwei\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VAFIN", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und Arbeit auferleget.", "tokens": ["Und", "Ar\u00b7beit", "auf\u00b7er\u00b7le\u00b7get", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Des Leibes Wohl gedeiht durch Flei\u00df;", "tokens": ["Des", "Lei\u00b7bes", "Wohl", "ge\u00b7deiht", "durch", "Flei\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Geist auch wird erreget,", "tokens": ["Der", "Geist", "auch", "wird", "er\u00b7re\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und strebt aus eitler Sorgen Tand", "tokens": ["Und", "strebt", "aus", "eit\u00b7ler", "Sor\u00b7gen", "Tand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Empor zu Gott, der ihn gesandt.", "tokens": ["Em\u00b7por", "zu", "Gott", ",", "der", "ihn", "ge\u00b7sandt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wann du getreu vollendet hast,", "tokens": ["Wann", "du", "ge\u00b7treu", "voll\u00b7en\u00b7det", "hast", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wozu dich Gott bestellte;", "tokens": ["Wo\u00b7zu", "dich", "Gott", "be\u00b7stell\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Behaglich f\u00fchlst du dann die Rast", "tokens": ["Be\u00b7hag\u00b7lich", "f\u00fchlst", "du", "dann", "die", "Rast"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vom Thun in Hitz' und K\u00e4lte.", "tokens": ["Vom", "Thun", "in", "Hitz'", "und", "K\u00e4l\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Am Himmel blinkt der Abendstern,", "tokens": ["Am", "Him\u00b7mel", "blinkt", "der", "A\u00b7bends\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Und zeigt noch be\u00dfre Rast von fern.", "tokens": ["Und", "zeigt", "noch", "be\u00df\u00b7re", "Rast", "von", "fern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJA", "NN", "APPR", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Auf Halm und Blume l\u00e4\u00dft geheim", "tokens": ["Auf", "Halm", "und", "Blu\u00b7me", "l\u00e4\u00dft", "ge\u00b7heim"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Vater Labsal tauen;", "tokens": ["Der", "Va\u00b7ter", "Lab\u00b7sal", "tau\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mit lassem Kniee wandert heim", "tokens": ["Mit", "las\u00b7sem", "Kni\u00b7ee", "wan\u00b7dert", "heim"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Mensch aus k\u00fchlen Auen;", "tokens": ["Der", "Mensch", "aus", "k\u00fch\u00b7len", "Au\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ihn bettet Gott zu s\u00fc\u00dfer Ruh',", "tokens": ["Ihn", "bet\u00b7tet", "Gott", "zu", "s\u00fc\u00b7\u00dfer", "Ruh'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und zieht des Dunkels Vorhang zu.", "tokens": ["Und", "zieht", "des", "Dun\u00b7kels", "Vor\u00b7hang", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Er aber sorgt indes und wacht", "tokens": ["Er", "a\u00b7ber", "sorgt", "in\u00b7des", "und", "wacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "ADJD", "ADJA", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcr uns mit Vatergnade,", "tokens": ["F\u00fcr", "uns", "mit", "Va\u00b7terg\u00b7na\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df nicht ein Unfall wo bei Nacht", "tokens": ["Da\u00df", "nicht", "ein", "Un\u00b7fall", "wo", "bei", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "ART", "NN", "PWAV", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An Leib und Gut uns schade.", "tokens": ["An", "Leib", "und", "Gut", "uns", "scha\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wir ruhn uns selber unbewu\u00dft,", "tokens": ["Wir", "ruhn", "uns", "sel\u00b7ber", "un\u00b7be\u00b7wu\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und wachen auf voll Kraft und Lust.", "tokens": ["Und", "wa\u00b7chen", "auf", "voll", "Kraft", "und", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJD", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "So ruhn wir, naht das St\u00fcndlein einst,", "tokens": ["So", "ruhn", "wir", ",", "naht", "das", "St\u00fcnd\u00b7lein", "einst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Rasenbett der Erde.", "tokens": ["Im", "Ra\u00b7sen\u00b7bett", "der", "Er\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Was sinnest du am Grab und weinst!", "tokens": ["Was", "sin\u00b7nest", "du", "am", "Grab", "und", "weinst", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPRART", "NN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gott ruft auch hier sein Werde!", "tokens": ["Gott", "ruft", "auch", "hier", "sein", "Wer\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ADV", "VAINF", "VAFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Bald neugeschaffen stehn wir auf,", "tokens": ["Bald", "neu\u00b7ge\u00b7schaf\u00b7fen", "stehn", "wir", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und heben an den neuen Lauf.", "tokens": ["Und", "he\u00b7ben", "an", "den", "neu\u00b7en", "Lauf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}