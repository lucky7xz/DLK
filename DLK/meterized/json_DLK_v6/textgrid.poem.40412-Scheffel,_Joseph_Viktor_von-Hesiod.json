{"textgrid.poem.40412": {"metadata": {"author": {"name": "Scheffel, Joseph Viktor von", "birth": "N.A.", "death": "N.A."}, "title": "Hesiod", "genre": "verse", "period": "N.A.", "pub_year": 1856, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Licht gl\u00fchte des Helikon Klippe", "tokens": ["Licht", "gl\u00fch\u00b7te", "des", "He\u00b7li\u00b7kon", "Klip\u00b7pe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "NN"], "meter": "+---+--+-", "measure": "trochaic.tri.relaxed"}, "line.2": {"text": "In Mittagspurpur und Blau,", "tokens": ["In", "Mit\u00b7tags\u00b7pur\u00b7pur", "und", "Blau", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da schlief bei dem Quell Aganippe", "tokens": ["Da", "schlief", "bei", "dem", "Quell", "A\u00b7gan\u00b7ip\u00b7pe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ein Hirtenknabe im Tau.", "tokens": ["Ein", "Hir\u00b7ten\u00b7kna\u00b7be", "im", "Tau", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Die L\u00e4mmer von Askra zu h\u00fcten", "tokens": ["Die", "L\u00e4m\u00b7mer", "von", "As\u00b7kra", "zu", "h\u00fc\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NE", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "War er zum Gebirge entsandt,", "tokens": ["War", "er", "zum", "Ge\u00b7bir\u00b7ge", "ent\u00b7sandt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Nun hatte den allzufr\u00fch M\u00fcden", "tokens": ["Nun", "hat\u00b7te", "den", "all\u00b7zu\u00b7fr\u00fch", "M\u00fc\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "PIAT", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Des Helios Kraft \u00fcbermannt.", "tokens": ["Des", "He\u00b7li\u00b7os", "Kraft", "\u00fc\u00b7berm\u00b7annt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Da stieg aus den sonnigen Kl\u00fcften", "tokens": ["Da", "stieg", "aus", "den", "son\u00b7ni\u00b7gen", "Kl\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Eine g\u00f6ttliche Neunzahl herab,", "tokens": ["Ei\u00b7ne", "g\u00f6tt\u00b7li\u00b7che", "Neun\u00b7zahl", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Der schwebende Anmut die H\u00fcften", "tokens": ["Der", "schwe\u00b7ben\u00b7de", "An\u00b7mut", "die", "H\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und Goldreif die Locken umgab;", "tokens": ["Und", "Gold\u00b7reif", "die", "Lo\u00b7cken", "um\u00b7gab", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Sie schritten in rhythmischem Reigen", "tokens": ["Sie", "schrit\u00b7ten", "in", "rhyth\u00b7mi\u00b7schem", "Rei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Zum Hain, dem die Quelle entflo\u00df,", "tokens": ["Zum", "Hain", ",", "dem", "die", "Quel\u00b7le", "ent\u00b7flo\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Und stellten in heiligem Schweigen", "tokens": ["Und", "stell\u00b7ten", "in", "hei\u00b7li\u00b7gem", "Schwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Dem Tr\u00e4umer Geschenke ins Moos.", "tokens": ["Dem", "Tr\u00e4u\u00b7mer", "Ge\u00b7schen\u00b7ke", "ins", "Moos", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Die erste von Erz eine Feder,", "tokens": ["Die", "ers\u00b7te", "von", "Erz", "ei\u00b7ne", "Fe\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPR", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Die zweite f\u00fcr Tinte ein Fa\u00df,", "tokens": ["Die", "zwei\u00b7te", "f\u00fcr", "Tin\u00b7te", "ein", "Fa\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPR", "NN", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Die dritte ein Zwickbuch in Leder,", "tokens": ["Die", "drit\u00b7te", "ein", "Zwi\u00b7ck\u00b7buch", "in", "Le\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die viert' ein geschliffenes Glas.", "tokens": ["Die", "viert'", "ein", "ge\u00b7schlif\u00b7fe\u00b7nes", "Glas", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Die f\u00fcnft' einen Siegellackbarren,", "tokens": ["Die", "f\u00fcnft'", "ei\u00b7nen", "Sie\u00b7gel\u00b7lack\u00b7bar\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Die sechst' eine goldene Brill',", "tokens": ["Die", "sechst'", "ei\u00b7ne", "gol\u00b7de\u00b7ne", "Brill'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Die siebte ein Kistlein Zigarren,", "tokens": ["Die", "sieb\u00b7te", "ein", "Kist\u00b7lein", "Zi\u00b7gar\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Die acht' einen Strau\u00df Asphodill.", "tokens": ["Die", "acht'", "ei\u00b7nen", "Strau\u00df", "As\u00b7pho\u00b7dill", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NE", "NE", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.4": {"line.1": {"text": "Die neunte, die beugte sich nieder", "tokens": ["Die", "neun\u00b7te", ",", "die", "beug\u00b7te", "sich", "nie\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "$,", "PRELS", "VVFIN", "PRF", "PTKVZ"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Und k\u00fc\u00dfte die Lippen ihm zart,", "tokens": ["Und", "k\u00fc\u00df\u00b7te", "die", "Lip\u00b7pen", "ihm", "zart", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PPER", "ADJD", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Dann schwanden in Wolken sie wieder", "tokens": ["Dann", "schwan\u00b7den", "in", "Wol\u00b7ken", "sie", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN", "PPER", "ADV"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Als Wesen von h\u00f6herer Art.", "tokens": ["Als", "We\u00b7sen", "von", "h\u00f6\u00b7he\u00b7rer", "Art", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Der Schlummerer sprang von der Erde", "tokens": ["Der", "Schlum\u00b7me\u00b7rer", "sprang", "von", "der", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Und sang wie von Geistern gepackt", "tokens": ["Und", "sang", "wie", "von", "Geis\u00b7tern", "ge\u00b7packt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KOKOM", "APPR", "NN", "VVPP"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Und schwang mit verz\u00fcckter Geb\u00e4rde", "tokens": ["Und", "schwang", "mit", "ver\u00b7z\u00fcck\u00b7ter", "Ge\u00b7b\u00e4r\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Einen Lorbeerbengel im Takt.", "tokens": ["Ei\u00b7nen", "Lor\u00b7beer\u00b7ben\u00b7gel", "im", "Takt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.5": {"line.1": {"text": "Da liefen die Mithirtenknaben", "tokens": ["Da", "lie\u00b7fen", "die", "Mit\u00b7hir\u00b7ten\u00b7kna\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zusammen und priesen sein Gl\u00fcck", "tokens": ["Zu\u00b7sam\u00b7men", "und", "prie\u00b7sen", "sein", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Und f\u00fchrten ihn samt seinen Gaben", "tokens": ["Und", "f\u00fchr\u00b7ten", "ihn", "samt", "sei\u00b7nen", "Ga\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+++-+-", "measure": "unknown.measure.penta"}, "line.4": {"text": "Nach Askra im Festzug zur\u00fcck.", "tokens": ["Nach", "As\u00b7kra", "im", "Fest\u00b7zug", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Und alle askr\u00e4ischen M\u00e4nner", "tokens": ["Und", "al\u00b7le", "as\u00b7kr\u00e4i\u00b7schen", "M\u00e4n\u00b7ner"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Berieten die Sache im Rat,", "tokens": ["Be\u00b7rie\u00b7ten", "die", "Sa\u00b7che", "im", "Rat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Bis da\u00df der Nomarchos als Kenner", "tokens": ["Bis", "da\u00df", "der", "No\u00b7mar\u00b7chos", "als", "Ken\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "KOUS", "ART", "NE", "KOUS", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "B\u00f6otiens den Urteilsspruch tat:", "tokens": ["B\u00f6o\u00b7ti\u00b7ens", "den", "Ur\u00b7teils\u00b7spruch", "tat", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.6": {"line.1": {"text": "\u00bbbei ", "tokens": ["\u00bb", "bei"], "token_info": ["punct", "word"], "pos": ["$(", "APPR"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Und Schafzucht f\u00fcr immer vorbei,", "tokens": ["Und", "Schaf\u00b7zucht", "f\u00fcr", "im\u00b7mer", "vor\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ADV", "PTKVZ", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Er mu\u00df ein Unsterblicher werden", "tokens": ["Er", "mu\u00df", "ein", "U\u00b7nsterb\u00b7li\u00b7cher", "wer\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "NN", "VAINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Dichtkunst und Schriftstellerei!\u00ab", "tokens": ["Mit", "Dicht\u00b7kunst", "und", "Schrift\u00b7stel\u00b7le\u00b7rei", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$.", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "... Sie kauften ihm lange Gew\u00e4nder", "tokens": ["...", "Sie", "kauf\u00b7ten", "ihm", "lan\u00b7ge", "Ge\u00b7w\u00e4n\u00b7der"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "PPER", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Und weihten ihn ganz seinem Gott,", "tokens": ["Und", "weih\u00b7ten", "ihn", "ganz", "sei\u00b7nem", "Gott", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da verfa\u00dfte den Bauernkalender", "tokens": ["Da", "ver\u00b7fa\u00df\u00b7te", "den", "Bau\u00b7ern\u00b7ka\u00b7len\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.8": {"text": "Und die Theogonie \u2013 ", "tokens": ["Und", "die", "The\u00b7o\u00b7go\u00b7nie", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}