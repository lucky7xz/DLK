{"textgrid.poem.59799": {"metadata": {"author": {"name": "Bodenstedt, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn dermaleinst des Paradieses Pforten", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn dermaleinst des Paradieses Pforten", "tokens": ["Wenn", "der\u00b7ma\u00b7leinst", "des", "Pa\u00b7ra\u00b7die\u00b7ses", "Pfor\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Frommen zur Belohnung offen stehn", "tokens": ["Den", "From\u00b7men", "zur", "Be\u00b7loh\u00b7nung", "of\u00b7fen", "stehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN", "ADJD", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und buntgeschart die Menschen aller Orten", "tokens": ["Und", "bunt\u00b7ge\u00b7schart", "die", "Men\u00b7schen", "al\u00b7ler", "Or\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ART", "NN", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Davor in Zweifel, Angst und Hoffen stehn,", "tokens": ["Da\u00b7vor", "in", "Zwei\u00b7fel", ",", "Angst", "und", "Hof\u00b7fen", "stehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "NN", "$,", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Werd' ich allein von allen S\u00fcndern dorten", "tokens": ["Werd'", "ich", "al\u00b7lein", "von", "al\u00b7len", "S\u00fcn\u00b7dern", "dor\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "PIAT", "NN", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von Angst und Zweifel nicht betroffen stehn,", "tokens": ["Von", "Angst", "und", "Zwei\u00b7fel", "nicht", "be\u00b7trof\u00b7fen", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PTKNEG", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da lange schon auf Erden mir die Pforten", "tokens": ["Da", "lan\u00b7ge", "schon", "auf", "Er\u00b7den", "mir", "die", "Pfor\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADV", "APPR", "NN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des Paradieses durch dich offen stehn.", "tokens": ["Des", "Pa\u00b7ra\u00b7die\u00b7ses", "durch", "dich", "of\u00b7fen", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}