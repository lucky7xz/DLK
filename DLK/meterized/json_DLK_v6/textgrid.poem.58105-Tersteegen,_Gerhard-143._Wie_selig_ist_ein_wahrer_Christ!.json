{"textgrid.poem.58105": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "143. Wie selig ist ein wahrer Christ!", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie selig ist ein Mensch, der alle seine Zeit,", "tokens": ["Wie", "se\u00b7lig", "ist", "ein", "Mensch", ",", "der", "al\u00b7le", "sei\u00b7ne", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "NN", "$,", "PRELS", "PIS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auch Leibs- und Seelenkraft in Gottes Dienst verzehret,", "tokens": ["Auch", "Leibs", "und", "See\u00b7len\u00b7kraft", "in", "Got\u00b7tes", "Dienst", "ver\u00b7zeh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "TRUNC", "KON", "NN", "APPR", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der nichts f\u00fcr sich beh\u00e4lt und der in Ewigkeit", "tokens": ["Der", "nichts", "f\u00fcr", "sich", "be\u00b7h\u00e4lt", "und", "der", "in", "E\u00b7wig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "APPR", "PRF", "VVFIN", "KON", "ART", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Kein ander Teil und Gut als Gott allein begehret!", "tokens": ["Kein", "an\u00b7der", "Teil", "und", "Gut", "als", "Gott", "al\u00b7lein", "be\u00b7ge\u00b7hret", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "KON", "ADJD", "KOKOM", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}}}}}