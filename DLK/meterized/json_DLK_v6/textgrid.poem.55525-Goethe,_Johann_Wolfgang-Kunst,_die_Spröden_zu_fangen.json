{"textgrid.poem.55525": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Kunst, die Spr\u00f6den zu fangen", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Oft f\u00fchrt ich sie zum Haine", "tokens": ["Oft", "f\u00fchrt", "ich", "sie", "zum", "Hai\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und war mit ihr alleine;", "tokens": ["Und", "war", "mit", "ihr", "al\u00b7lei\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "PPER", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "O wie war ich erfreut!", "tokens": ["O", "wie", "war", "ich", "er\u00b7freut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "VAFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ist je ein Paar alleine,", "tokens": ["Ist", "je", "ein", "Paar", "al\u00b7lei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ist Amor niemals weit.", "tokens": ["Ist", "A\u00b7mor", "nie\u00b7mals", "weit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Du brauchst nicht scharf zu zielen,", "tokens": ["Du", "brauchst", "nicht", "scharf", "zu", "zie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Brust ist ohnbewehrt.", "tokens": ["Die", "Brust", "ist", "ohn\u00b7be\u00b7wehrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich hab ihr, wie im Spielen,", "tokens": ["Ich", "hab", "ihr", ",", "wie", "im", "Spie\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$,", "PWAV", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gar manches schon gelehrt,", "tokens": ["Gar", "man\u00b7ches", "schon", "ge\u00b7lehrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Was, ohne sich zu f\u00fchlen,", "tokens": ["Was", ",", "oh\u00b7ne", "sich", "zu", "f\u00fch\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "$,", "KOUI", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Kein junges M\u00e4dchen h\u00f6rt.", "tokens": ["Kein", "jun\u00b7ges", "M\u00e4d\u00b7chen", "h\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und kraftlos sank ihr Haupt zur\u00fccke.", "tokens": ["Und", "kraft\u00b7los", "sank", "ihr", "Haupt", "zu\u00b7r\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Erst irrten unbestimmt die Blicke", "tokens": ["Erst", "irr\u00b7ten", "un\u00b7be\u00b7stimmt", "die", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Umher, und fielen dann auf mich,", "tokens": ["Um\u00b7her", ",", "und", "fie\u00b7len", "dann", "auf", "mich", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KON", "VVFIN", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und eilten weg, und kamen wieder.", "tokens": ["Und", "eil\u00b7ten", "weg", ",", "und", "ka\u00b7men", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "$,", "KON", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sie l\u00e4chelte und schlug die Augen nieder,", "tokens": ["Sie", "l\u00e4\u00b7chel\u00b7te", "und", "schlug", "die", "Au\u00b7gen", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ihr f\u00fchlbar Herz emp\u00f6rte sich", "tokens": ["Ihr", "f\u00fchl\u00b7bar", "Herz", "em\u00b7p\u00f6r\u00b7te", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADJD", "NN", "VVFIN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und schickte brennendes Verlangen", "tokens": ["Und", "schick\u00b7te", "bren\u00b7nen\u00b7des", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "In ihren Busen, auf die Wangen,", "tokens": ["In", "ih\u00b7ren", "Bu\u00b7sen", ",", "auf", "die", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Die Wangen gl\u00fchten, und der Busen stieg.", "tokens": ["Die", "Wan\u00b7gen", "gl\u00fch\u00b7ten", ",", "und", "der", "Bu\u00b7sen", "stieg", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Da rief ich: \u00bbSieg! Sieg, Amor, Sieg!\u00ab", "tokens": ["Da", "rief", "ich", ":", "\u00bb", "Sieg", "!", "Sieg", ",", "A\u00b7mor", ",", "Sieg", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "$(", "NN", "$.", "NN", "$,", "NE", "$,", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Rief, als er in die L\u00fcfte stieg:", "tokens": ["Rief", ",", "als", "er", "in", "die", "L\u00fcf\u00b7te", "stieg", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbsieg! Sieg!\u00ab", "tokens": ["\u00bb", "sieg", "!", "Sieg", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "punct", "punct"], "pos": ["$(", "VVIMP", "$.", "NN", "$.", "$("], "meter": "-+", "measure": "iambic.single"}}}}}