{"textgrid.poem.41001": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Herbstjubel", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich kam in j\u00fcngster Mondennacht", "tokens": ["Ich", "kam", "in", "j\u00fcngs\u00b7ter", "Mon\u00b7den\u00b7nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In eines Kirchhofs Mauern,", "tokens": ["In", "ei\u00b7nes", "Kirch\u00b7hofs", "Mau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Kein Schl\u00e4fer unterm H\u00fcgel wacht,", "tokens": ["Kein", "Schl\u00e4\u00b7fer", "un\u00b7term", "H\u00fc\u00b7gel", "wacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ringsum herrscht Tod und Schauern.", "tokens": ["Ring\u00b7sum", "herrscht", "Tod", "und", "Schau\u00b7ern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "KON", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Doch pl\u00f6tzlich vom Gebirge schallt's", "tokens": ["Doch", "pl\u00f6tz\u00b7lich", "vom", "Ge\u00b7bir\u00b7ge", "schallt's"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gleichwie bacchant'scher Reigen,", "tokens": ["Gleich\u00b7wie", "bac\u00b7chant'\u00b7scher", "Rei\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "An hohlen Gr\u00e4bern widerhallt's", "tokens": ["An", "hoh\u00b7len", "Gr\u00e4\u00b7bern", "wi\u00b7der\u00b7hallt's"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und bricht ihr totes Schweigen.", "tokens": ["Und", "bricht", "ihr", "to\u00b7tes", "Schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ein lust'ger Chor von Zechern ruft", "tokens": ["Ein", "lust'\u00b7ger", "Chor", "von", "Ze\u00b7chern", "ruft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Lebehoch den Sch\u00f6nen,", "tokens": ["Ein", "Le\u00b7be\u00b7hoch", "den", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Raketen schwirren durch die Luft,", "tokens": ["Ra\u00b7ke\u00b7ten", "schwir\u00b7ren", "durch", "die", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Und die Gebirge dr\u00f6hnen.", "tokens": ["Und", "die", "Ge\u00b7bir\u00b7ge", "dr\u00f6h\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Der H\u00fcgel aber, wo ich steh',", "tokens": ["Der", "H\u00fc\u00b7gel", "a\u00b7ber", ",", "wo", "ich", "steh'", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Innersten erbebet,", "tokens": ["Im", "In\u00b7ners\u00b7ten", "er\u00b7be\u00b7bet", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Und ein Gerippe sich zur H\u00f6h'", "tokens": ["Und", "ein", "Ge\u00b7rip\u00b7pe", "sich", "zur", "H\u00f6h'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "PRF", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aus seinen Tiefen hebet.", "tokens": ["Aus", "sei\u00b7nen", "Tie\u00b7fen", "he\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Im Mondenscheine schreitet's vor,", "tokens": ["Im", "Mon\u00b7den\u00b7schei\u00b7ne", "schrei\u00b7tet's", "vor", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schwingt halb sich auf die Mauer", "tokens": ["Schwingt", "halb", "sich", "auf", "die", "Mau\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und ruft in den bacchant'schen Chor", "tokens": ["Und", "ruft", "in", "den", "bac\u00b7chant'\u00b7schen", "Chor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.4": {"text": "Also hinaus, ein Schauer:", "tokens": ["Al\u00b7so", "hin\u00b7aus", ",", "ein", "Schau\u00b7er", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.6": {"line.1": {"text": "\u00bbihr dort im Fleische, st\u00f6ret nicht", "tokens": ["\u00bb", "ihr", "dort", "im", "Flei\u00b7sche", ",", "st\u00f6\u00b7ret", "nicht"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "PPER", "ADV", "APPRART", "NN", "$,", "VVFIN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Toten Ruhest\u00e4tte!", "tokens": ["Der", "To\u00b7ten", "Ru\u00b7he\u00b7st\u00e4t\u00b7te", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bricht neu die Blum' ans Sonnenlicht,", "tokens": ["Bricht", "neu", "die", "Blum'", "ans", "Son\u00b7nen\u00b7licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schlaft ihr im gleichen Bette!\u00ab", "tokens": ["Schlaft", "ihr", "im", "glei\u00b7chen", "Bet\u00b7te", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PPOSAT", "APPRART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Der Mond erlischt am Himmelszelt,", "tokens": ["Der", "Mond", "er\u00b7lischt", "am", "Him\u00b7mels\u00b7zelt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "H\u00f6r' keinen Laut mehr schallen.", "tokens": ["H\u00f6r'", "kei\u00b7nen", "Laut", "mehr", "schal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "APPR", "PIS", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mir ist der Tod, der durch die Welt", "tokens": ["Mir", "ist", "der", "Tod", ",", "der", "durch", "die", "Welt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Jetzt schreitet, beigefallen.", "tokens": ["Jetzt", "schrei\u00b7tet", ",", "bei\u00b7ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "VVIZU", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}