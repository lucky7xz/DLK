{"textgrid.poem.36975": {"metadata": {"author": {"name": "Gla\u00dfbrenner, Adolf", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich sehe keinen Fr\u00fchling mehr,", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich sehe keinen Fr\u00fchling mehr,", "tokens": ["Ich", "se\u00b7he", "kei\u00b7nen", "Fr\u00fch\u00b7ling", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bis da\u00df die Freiheit bl\u00fcht;", "tokens": ["Bis", "da\u00df", "die", "Frei\u00b7heit", "bl\u00fcht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es duftet kein Rose mir,", "tokens": ["Es", "duf\u00b7tet", "kein", "Ro\u00b7se", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "PPER", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Bis jedes Herz ihr gl\u00fcht.", "tokens": ["Bis", "je\u00b7des", "Herz", "ihr", "gl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich h\u00f6re keinen Vogelsang", "tokens": ["Ich", "h\u00f6\u00b7re", "kei\u00b7nen", "Vo\u00b7gel\u00b7sang"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als meiner Dichter Wort;", "tokens": ["Als", "mei\u00b7ner", "Dich\u00b7ter", "Wort", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mich tr\u00e4gt kein Strom mehr als der Tag", "tokens": ["Mich", "tr\u00e4gt", "kein", "Strom", "mehr", "als", "der", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "PIAT", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zum Weltenmeere fort.", "tokens": ["Zum", "Wel\u00b7ten\u00b7mee\u00b7re", "fort", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich schaue keine Steinenpracht", "tokens": ["Ich", "schau\u00b7e", "kei\u00b7ne", "Stei\u00b7nen\u00b7pracht"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als Herrscherstolz und Zwang;", "tokens": ["Als", "Herr\u00b7scher\u00b7stolz", "und", "Zwang", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich habe keine Hoffnung mehr", "tokens": ["Ich", "ha\u00b7be", "kei\u00b7ne", "Hoff\u00b7nung", "mehr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als ihren Untergang.", "tokens": ["Als", "ih\u00b7ren", "Un\u00b7ter\u00b7gang", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die eine Sonne, die mir gl\u00e4nzt,", "tokens": ["Die", "ei\u00b7ne", "Son\u00b7ne", ",", "die", "mir", "gl\u00e4nzt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist meines Volkes Geist,", "tokens": ["Ist", "mei\u00b7nes", "Vol\u00b7kes", "Geist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und meine Kirche jede Brust,", "tokens": ["Und", "mei\u00b7ne", "Kir\u00b7che", "je\u00b7de", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die laut die Freiheit preist.", "tokens": ["Die", "laut", "die", "Frei\u00b7heit", "preist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ich hasse alle Wissenschaft,", "tokens": ["Ich", "has\u00b7se", "al\u00b7le", "Wis\u00b7sen\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die einen Bauch sich fri\u00dft;", "tokens": ["Die", "ei\u00b7nen", "Bauch", "sich", "fri\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich achte keinen Helden mehr,", "tokens": ["Ich", "ach\u00b7te", "kei\u00b7nen", "Hel\u00b7den", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der's seinem Herrscher ist.", "tokens": ["Der's", "sei\u00b7nem", "Herr\u00b7scher", "ist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ich habe keine Liebe mehr,", "tokens": ["Ich", "ha\u00b7be", "kei\u00b7ne", "Lie\u00b7be", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die um ein K\u00fc\u00dfchen minnt;", "tokens": ["Die", "um", "ein", "K\u00fc\u00df\u00b7chen", "minnt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mein Vaterland ist meine Braut,", "tokens": ["Mein", "Va\u00b7ter\u00b7land", "ist", "mei\u00b7ne", "Braut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die schon zur Hochzeit spinnt!", "tokens": ["Die", "schon", "zur", "Hoch\u00b7zeit", "spinnt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Die Wahrheit ist mein heil'ger Geist,", "tokens": ["Die", "Wahr\u00b7heit", "ist", "mein", "heil'\u00b7ger", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Gott und Seelenhirt!", "tokens": ["Mein", "Gott", "und", "See\u00b7len\u00b7hirt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich habe keinen Glauben mehr,", "tokens": ["Ich", "ha\u00b7be", "kei\u00b7nen", "Glau\u00b7ben", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als da\u00df es besser wird.", "tokens": ["Als", "da\u00df", "es", "bes\u00b7ser", "wird", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}