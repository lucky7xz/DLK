{"textgrid.poem.62196": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "Hundegebell im Fleischerladen", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit Hunden hetzen sie das arme Tier,", "tokens": ["Mit", "Hun\u00b7den", "het\u00b7zen", "sie", "das", "ar\u00b7me", "Tier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit Kolben sto\u00dfen sie's zu Tod!", "tokens": ["Mit", "Kol\u00b7ben", "sto\u00b7\u00dfen", "sie's", "zu", "Tod", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ist's nicht genug an Wein und Brot?", "tokens": ["Ist's", "nicht", "ge\u00b7nug", "an", "Wein", "und", "Brot", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nach Blut lechzt die Begier.", "tokens": ["Nach", "Blut", "lechzt", "die", "Be\u00b7gier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}