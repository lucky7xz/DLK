{"textgrid.poem.44221": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "[kein Schulpferd ist so gut zum Springen abgericht]", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kein Schulpferd ist so gut zum Springen abgericht,", "tokens": ["Kein", "Schul\u00b7pferd", "ist", "so", "gut", "zum", "Sprin\u00b7gen", "ab\u00b7ge\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADV", "ADJD", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als jezo das Geschrey von Messalinen spricht.", "tokens": ["Als", "je\u00b7zo", "das", "Ge\u00b7schrey", "von", "Mes\u00b7sa\u00b7li\u00b7nen", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dies aber ist und heist kein Wunder bey den Leuten:", "tokens": ["Dies", "a\u00b7ber", "ist", "und", "heist", "kein", "Wun\u00b7der", "bey", "den", "Leu\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VAFIN", "KON", "VVFIN", "PIAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Vater lehrt sie ja in jeder Predigt reiten.", "tokens": ["Der", "Va\u00b7ter", "lehrt", "sie", "ja", "in", "je\u00b7der", "Pre\u00b7digt", "rei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measur