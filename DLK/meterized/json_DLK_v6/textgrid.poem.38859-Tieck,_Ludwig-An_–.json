{"textgrid.poem.38859": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "An \u2013", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Unfreundlich, krank, betr\u00fcbt begann mein Leben,", "tokens": ["Un\u00b7freund\u00b7lich", ",", "krank", ",", "be\u00b7tr\u00fcbt", "be\u00b7gann", "mein", "Le\u00b7ben", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,", "VVPP", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Todesstrom vernahm ich unten schallen,", "tokens": ["Den", "To\u00b7des\u00b7strom", "ver\u00b7nahm", "ich", "un\u00b7ten", "schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da floh ich zu der Dichtkunst goldnen Hallen", "tokens": ["Da", "floh", "ich", "zu", "der", "Dicht\u00b7kunst", "gold\u00b7nen", "Hal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und bot dem Musengott mein liebend Streben.", "tokens": ["Und", "bot", "dem", "Mu\u00b7sen\u00b7gott", "mein", "lie\u00b7bend", "Stre\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Bald wollte sich der Busen frischer heben,", "tokens": ["Bald", "woll\u00b7te", "sich", "der", "Bu\u00b7sen", "fri\u00b7scher", "he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PRF", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dich w\u00e4hlt ich mir zum Freunde aus vor allen,", "tokens": ["Dich", "w\u00e4hlt", "ich", "mir", "zum", "Freun\u00b7de", "aus", "vor", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPER", "APPRART", "NN", "APPR", "APPR", "PIAT", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es sollte dir nur, was ich that, gefallen,", "tokens": ["Es", "soll\u00b7te", "dir", "nur", ",", "was", "ich", "that", ",", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "$,", "PWS", "PPER", "VVFIN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auf Freundschaftsfittig himmelan zu schweben.", "tokens": ["Auf", "Freund\u00b7schafts\u00b7fit\u00b7tig", "him\u00b7me\u00b7lan", "zu", "schwe\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ein k\u00fchnes Licht erhob sich in dem Dunkeln,", "tokens": ["Ein", "k\u00fch\u00b7nes", "Licht", "er\u00b7hob", "sich", "in", "dem", "Dun\u00b7keln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "APPR", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es bl\u00fchten aus dem Tode sch\u00f6ne Blumen,", "tokens": ["Es", "bl\u00fch\u00b7ten", "aus", "dem", "To\u00b7de", "sch\u00f6\u00b7ne", "Blu\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dein Auge sah ich leitend vor mir funkeln:", "tokens": ["Dein", "Au\u00b7ge", "sah", "ich", "lei\u00b7tend", "vor", "mir", "fun\u00b7keln", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADJD", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wie rief es mich zu jenen Heiligthumen! \u2013", "tokens": ["Wie", "rief", "es", "mich", "zu", "je\u00b7nen", "Hei\u00b7lig\u00b7thu\u00b7men", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "APPR", "PDAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Blume welkte, die ich mir erlesen,", "tokens": ["Die", "Blu\u00b7me", "welk\u00b7te", ",", "die", "ich", "mir", "er\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PRELS", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und den verlohr ich, der nie mein gewesen.", "tokens": ["Und", "den", "ver\u00b7lohr", "ich", ",", "der", "nie", "mein", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "VVFIN", "PPER", "$,", "PRELS", "ADV", "PPOSAT", "VAPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}