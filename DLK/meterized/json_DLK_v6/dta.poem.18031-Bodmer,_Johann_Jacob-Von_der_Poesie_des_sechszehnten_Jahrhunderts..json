{"dta.poem.18031": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Von der Poesie des sechszehnten  \n Jahrhunderts.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198462", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es ward kein Kayser nie so reich", "tokens": ["Es", "ward", "kein", "Kay\u00b7ser", "nie", "so", "reich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "ADV", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich mag ihm seyn an denken gleich.", "tokens": ["Ich", "mag", "ihm", "seyn", "an", "den\u00b7ken", "gleich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VAINF", "PTKVZ", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die F&#252;rsten zwingen mit Gewalt", "tokens": ["Die", "F", "&#252;", "rs\u00b7ten", "zwin\u00b7gen", "mit", "Ge\u00b7walt"], "token_info": ["word", "word", "XML_entity", "word", "word", "word", "word"], "pos": ["ART", "NE", "$(", "VVFIN", "APPR", "APPR", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Das Feld, Stein, Wasser, Berg und Wald.", "tokens": ["Das", "Feld", ",", "Stein", ",", "Was\u00b7ser", ",", "Berg", "und", "Wald", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dazu all Thier, beid wild und zam,", "tokens": ["Da\u00b7zu", "all", "Thier", ",", "beid", "wild", "und", "zam", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PIAT", "NN", "$,", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sie thetens auch der Luft allsam", "tokens": ["Sie", "the\u00b7tens", "auch", "der", "Luft", "all\u00b7sam"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "ADV", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wo sie verm&#339;chten mit Gewalt", "tokens": ["Wo", "sie", "verm", "&#339;", "ch\u00b7ten", "mit", "Ge\u00b7walt"], "token_info": ["word", "word", "word", "XML_entity", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADJD", "$(", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Aber Gott hat solches abgestalt.", "tokens": ["A\u00b7ber", "Gott", "hat", "sol\u00b7ches", "ab\u00b7ge\u00b7stalt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "Sie m&#252;ssen die gemein lan seyn.", "tokens": ["Sie", "m", "&#252;", "ssen", "die", "ge\u00b7mein", "lan", "seyn", "."], "token_info": ["word", "word", "XML_entity", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "$(", "VVFIN", "ART", "ADJD", "ADV", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "M&#339;chten sie uns der Sonnen Schein", "tokens": ["M", "&#339;", "ch\u00b7ten", "sie", "uns", "der", "Son\u00b7nen", "Schein"], "token_info": ["word", "XML_entity", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$(", "VVFIN", "PPER", "PRF", "ART", "NN", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Verbergen, den Tauw, Wind und Regen,", "tokens": ["Ver\u00b7ber\u00b7gen", ",", "den", "Tauw", ",", "Wind", "und", "Re\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.12": {"text": "Sie liessen es nicht unterwegen.", "tokens": ["Sie", "lies\u00b7sen", "es", "nicht", "un\u00b7ter\u00b7we\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}