{"dta.poem.5360": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "J rdisches  \n  V ergn\u00fcgen  \n in  \n  GOTT .  \n  F \u00fcnfter  T heil.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df unsre Seel\u2019 ie mehr und mehr sich durch Betrachtung", "tokens": ["Da\u00df", "uns\u00b7re", "Seel'", "ie", "mehr", "und", "mehr", "sich", "durch", "Be\u00b7trach\u00b7tung"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "ADV", "KON", "ADV", "PRF", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "angewehn',", "tokens": ["an\u00b7ge\u00b7wehn'", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Jm sch\u00f6nen Creaturen-Spiegel die wahre GOttheit an-", "tokens": ["Jm", "sch\u00f6\u00b7nen", "Crea\u00b7tu\u00b7ren\u00b7Spie\u00b7gel", "die", "wah\u00b7re", "Got\u00b7theit", "an"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ART", "ADJA", "NN", "TRUNC"], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "zusehn?", "tokens": ["zu\u00b7sehn", "?"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Weil sonsten, wenigstens f\u00fcr uns, wenn wir allein auf", "tokens": ["Weil", "sons\u00b7ten", ",", "we\u00b7nigs\u00b7tens", "f\u00fcr", "uns", ",", "wenn", "wir", "al\u00b7lein", "auf"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "$,", "ADV", "APPR", "PPER", "$,", "KOUS", "PPER", "ADV", "APPR"], "meter": "-+--+--+--+-+", "measure": "amphibrach.tetra.plus"}, "line.6": {"text": "Geld nur gaffen", "tokens": ["Geld", "nur", "gaf\u00b7fen"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADV", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Der Endzweck GOttes ja verlohren, die Welt f\u00fcr uns", "tokens": ["Der", "End\u00b7zweck", "Got\u00b7tes", "ja", "ver\u00b7loh\u00b7ren", ",", "die", "Welt", "f\u00fcr", "uns"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "ADV", "VVPP", "$,", "ART", "NN", "APPR", "PPER"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "umsonst erschaffen.", "tokens": ["um\u00b7sonst", "er\u00b7schaf\u00b7fen", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Hingegen unterscheidet sich der Mensch nicht blos nur von", "tokens": ["Hin\u00b7ge\u00b7gen", "un\u00b7ter\u00b7schei\u00b7det", "sich", "der", "Mensch", "nicht", "blos", "nur", "von"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "PTKNEG", "ADV", "ADV", "APPR"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.10": {"text": "den Thieren,", "tokens": ["den", "Thie\u00b7ren", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.11": {"text": "Als welche beym Genu\u00df der Dinge zwar Lust, doch keinen", "tokens": ["Als", "wel\u00b7che", "beym", "Ge\u00b7nu\u00df", "der", "Din\u00b7ge", "zwar", "Lust", ",", "doch", "kei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PRELS", "APPRART", "NN", "ART", "NN", "ADV", "NN", "$,", "ADV", "PIAT"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Gott, versp\u00fchren;", "tokens": ["Gott", ",", "ver\u00b7sp\u00fch\u00b7ren", ";"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.13": {"text": "Wir werden selbst, wenn, in der Sch\u00f6nheit der Creatur", "tokens": ["Wir", "wer\u00b7den", "selbst", ",", "wenn", ",", "in", "der", "Sch\u00f6n\u00b7heit", "der", "Crea\u00b7tur"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "$,", "KOUS", "$,", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "und ihren Sch\u00e4tzen,", "tokens": ["und", "ih\u00b7ren", "Sch\u00e4t\u00b7zen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.15": {"text": "Wir uns an ihrem ew\u2019gen Ursprung in Ehrfurcht-voller", "tokens": ["Wir", "uns", "an", "ih\u00b7rem", "ew'\u00b7gen", "Ur\u00b7sprung", "in", "Ehr\u00b7furcht\u00b7vol\u00b7ler"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "PRF", "APPR", "PPOSAT", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "Lust ergetzen,", "tokens": ["Lust", "er\u00b7get\u00b7zen", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.17": {"text": "Von der allgegenw\u00e4rtgen GOttheit, dem Sch\u00f6pfer Him-", "tokens": ["Von", "der", "all\u00b7ge\u00b7gen\u00b7w\u00e4rt\u00b7gen", "Got\u00b7theit", ",", "dem", "Sch\u00f6p\u00b7fer", "Him"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "ART", "NN", "TRUNC"], "meter": "--+--+--+-+-+", "measure": "anapaest.tri.plus"}, "line.18": {"text": "mels und der Erden,", "tokens": ["mels", "und", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.19": {"text": "Und seiner Weisheit, Herrlichkeit und Macht, vern\u00fcnft\u2019-", "tokens": ["Und", "sei\u00b7ner", "Weis\u00b7heit", ",", "Herr\u00b7lich\u00b7keit", "und", "Macht", ",", "ver\u00b7n\u00fcnft'"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KON", "PPOSAT", "NN", "$,", "NN", "KON", "NN", "$,", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "ge Spiegel werden;", "tokens": ["ge", "Spie\u00b7gel", "wer\u00b7den", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.21": {"text": "In welchen Geister, Seelen, Engel (wie wir, in hellem", "tokens": ["In", "wel\u00b7chen", "Geis\u00b7ter", ",", "See\u00b7len", ",", "En\u00b7gel", "(", "wie", "wir", ",", "in", "hel\u00b7lem"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PWAT", "NN", "$,", "NN", "$,", "NE", "$(", "PWAV", "PPER", "$,", "APPR", "ADJA"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.22": {"text": "Wieder-Schein", "tokens": ["Wie\u00b7der\u00b7Schein"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.23": {"text": "Des Wassers, unsrer Erden Schmuck) sein herrlich Werck", "tokens": ["Des", "Was\u00b7sers", ",", "uns\u00b7rer", "Er\u00b7den", "Schmuck", ")", "sein", "herr\u00b7lich", "Werck"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PPOSAT", "NN", "NN", "$(", "PPOSAT", "ADJD", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "vergeistert schauen", "tokens": ["ver\u00b7geis\u00b7tert", "schau\u00b7en"], "token_info": ["word", "word"], "pos": ["VVPP", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.25": {"text": "Und, an ger\u00fchrten Menschen-Seelen, sich selber, GOtt zum", "tokens": ["Und", ",", "an", "ge\u00b7r\u00fchr\u00b7ten", "Men\u00b7schen\u00b7See\u00b7len", ",", "sich", "sel\u00b7ber", ",", "Gott", "zum"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "$,", "APPR", "ADJA", "NN", "$,", "PRF", "ADV", "$,", "NN", "APPRART"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.26": {"text": "Ruhm, erbauen.", "tokens": ["Ruhm", ",", "er\u00b7bau\u00b7en", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.27": {"text": "Wann die Betrachtungen der Wercke des Sch\u00f6pfers nun", "tokens": ["Wann", "die", "Be\u00b7trach\u00b7tun\u00b7gen", "der", "Wer\u00b7cke", "des", "Sch\u00f6p\u00b7fers", "nun"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "ART", "NN", "ADV"], "meter": "+--+---+--+-+", "measure": "dactylic.di.plus"}, "line.28": {"text": "so n\u00f6htig seyn;", "tokens": ["so", "n\u00f6h\u00b7tig", "seyn", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAINF", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.29": {"text": "So bitt\u2019 und fleh ich: grosser Sch\u00f6pfer! um deine Lieb\u2019", "tokens": ["So", "bitt'", "und", "fleh", "ich", ":", "gros\u00b7ser", "Sch\u00f6p\u00b7fer", "!", "um", "dei\u00b7ne", "Lieb'"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "KON", "VVFIN", "PPER", "$.", "ADJA", "NN", "$.", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.30": {"text": "erbarm dich mein!", "tokens": ["er\u00b7barm", "dich", "mein", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "PPOSAT", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}