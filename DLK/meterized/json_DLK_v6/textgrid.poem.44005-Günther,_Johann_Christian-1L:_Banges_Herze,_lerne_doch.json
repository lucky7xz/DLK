{"textgrid.poem.44005": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Banges Herze, lerne doch", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Banges Herze, lerne doch", "tokens": ["Ban\u00b7ges", "Her\u00b7ze", ",", "ler\u00b7ne", "doch"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "$,", "VVFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dich in dein Verh\u00e4ngn\u00fc\u00df schicken", "tokens": ["Dich", "in", "dein", "Ver\u00b7h\u00e4ng\u00b7n\u00fc\u00df", "schi\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und das schwere Creuzesjoch", "tokens": ["Und", "das", "schwe\u00b7re", "Creu\u00b7zes\u00b7joch"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Durch Gedult vom Halse r\u00fccken,", "tokens": ["Durch", "Ge\u00b7dult", "vom", "Hal\u00b7se", "r\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Weil dem Auge, wenn es weint,", "tokens": ["Weil", "dem", "Au\u00b7ge", ",", "wenn", "es", "weint", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Alles doppelt gr\u00f6\u00dfer scheint.", "tokens": ["Al\u00b7les", "dop\u00b7pelt", "gr\u00f6\u00b7\u00dfer", "scheint", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ADJD", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Gerne tragen schw\u00e4cht die Last,", "tokens": ["Ger\u00b7ne", "tra\u00b7gen", "schw\u00e4cht", "die", "Last", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Willig leiden st\u00e4rckt die H\u00e4nde;", "tokens": ["Wil\u00b7lig", "lei\u00b7den", "st\u00e4rckt", "die", "H\u00e4n\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wer das Ruder muthig fast,", "tokens": ["Wer", "das", "Ru\u00b7der", "mut\u00b7hig", "fast", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "ADJD", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Macht der Schiffahrt bald ein Ende,", "tokens": ["Macht", "der", "Schif\u00b7fahrt", "bald", "ein", "En\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Welche man in dieser Welt", "tokens": ["Wel\u00b7che", "man", "in", "die\u00b7ser", "Welt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PIS", "APPR", "PDAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Durch das Meer der Tr\u00fcbsahl h\u00e4lt.", "tokens": ["Durch", "das", "Meer", "der", "Tr\u00fcb\u00b7sahl", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Zwar es ist kein schlechtes Werck,", "tokens": ["Zwar", "es", "ist", "kein", "schlech\u00b7tes", "Werck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich im Kummer fr\u00f6hlich zeigen.", "tokens": ["Sich", "im", "Kum\u00b7mer", "fr\u00f6h\u00b7lich", "zei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPRART", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wer vermag wohl einen Berg", "tokens": ["Wer", "ver\u00b7mag", "wohl", "ei\u00b7nen", "Berg"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ADV", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ohne Schwizen aufzusteigen?", "tokens": ["Oh\u00b7ne", "Schwi\u00b7zen", "auf\u00b7zu\u00b7stei\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch ein Weiser zwingt das Leid", "tokens": ["Doch", "ein", "Wei\u00b7ser", "zwingt", "das", "Leid"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Durch der Sinnen Tapferkeit.", "tokens": ["Durch", "der", "Sin\u00b7nen", "Tap\u00b7fer\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Nur getrost, betr\u00fcbter Geist!", "tokens": ["Nur", "ge\u00b7trost", ",", "be\u00b7tr\u00fcb\u00b7ter", "Geist", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Frisch gewagt ist halb gewonnen.", "tokens": ["Frisch", "ge\u00b7wagt", "ist", "halb", "ge\u00b7won\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "VAFIN", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Was dein Fernglas Wolcken heist,", "tokens": ["Was", "dein", "Fern\u00b7glas", "Wol\u00b7cken", "heist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist ein Himmel voller Sonnen,", "tokens": ["Ist", "ein", "Him\u00b7mel", "vol\u00b7ler", "Son\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Die des Kummers tr\u00fcbe Nacht", "tokens": ["Die", "des", "Kum\u00b7mers", "tr\u00fc\u00b7be", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Den Cometen \u00e4hnlich macht.", "tokens": ["Den", "Co\u00b7me\u00b7ten", "\u00e4hn\u00b7lich", "macht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Unser Glauben nimmt den Trost,", "tokens": ["Un\u00b7ser", "Glau\u00b7ben", "nimmt", "den", "Trost", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Weil die Qual nicht ewig w\u00e4hret.", "tokens": ["Weil", "die", "Qual", "nicht", "e\u00b7wig", "w\u00e4h\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PTKNEG", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dem, der mit der Hofnung lost,", "tokens": ["Dem", ",", "der", "mit", "der", "Hof\u00b7nung", "lost", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist das Kleinod oft bescheeret;", "tokens": ["Ist", "das", "Klei\u00b7nod", "oft", "be\u00b7schee\u00b7ret", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Aus dem Leiden ohne Ruh", "tokens": ["Aus", "dem", "Lei\u00b7den", "oh\u00b7ne", "Ruh"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "F\u00fchrt sie uns nach Gl\u00fcckstadt zu.", "tokens": ["F\u00fchrt", "sie", "uns", "nach", "Gl\u00fcck\u00b7stadt", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Ich verschweige, was mich dr\u00fcckt,", "tokens": ["Ich", "ver\u00b7schwei\u00b7ge", ",", "was", "mich", "dr\u00fcckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und bin in dem H\u00f6chsten stille:", "tokens": ["Und", "bin", "in", "dem", "H\u00f6chs\u00b7ten", "stil\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Gott hat es mir zugeschickt,", "tokens": ["Gott", "hat", "es", "mir", "zu\u00b7ge\u00b7schickt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und vielleicht ist es sein Wille,", "tokens": ["Und", "viel\u00b7leicht", "ist", "es", "sein", "Wil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Da\u00df nach Klageliedern bald", "tokens": ["Da\u00df", "nach", "Kla\u00b7ge\u00b7lie\u00b7dern", "bald"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Auch ein Halleluja schallt.", "tokens": ["Auch", "ein", "Hal\u00b7le\u00b7lu\u00b7ja", "schallt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}