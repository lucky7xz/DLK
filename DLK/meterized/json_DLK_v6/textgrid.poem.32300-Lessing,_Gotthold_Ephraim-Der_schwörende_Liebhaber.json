{"textgrid.poem.32300": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "Der schw\u00f6rende Liebhaber", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich schw\u00f6r' es dir, o Laura, dich zu hassen;", "tokens": ["Ich", "schw\u00f6r'", "es", "dir", ",", "o", "Lau\u00b7ra", ",", "dich", "zu", "has\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPER", "$,", "FM", "FM", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gerechten Ha\u00df schw\u00f6r' ich dir zu.", "tokens": ["Ge\u00b7rech\u00b7ten", "Ha\u00df", "schw\u00f6r'", "ich", "dir", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich schw\u00f6r' es allen Sch\u00f6nen, sie zu hassen;", "tokens": ["Ich", "schw\u00f6r'", "es", "al\u00b7len", "Sch\u00f6\u00b7nen", ",", "sie", "zu", "has\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "NN", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Weil alle treulos sind, wie du.", "tokens": ["Weil", "al\u00b7le", "treu\u00b7los", "sind", ",", "wie", "du", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "VAFIN", "$,", "PWAV", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich schw\u00f6r' es dir, vor Amors Ohren,", "tokens": ["Ich", "schw\u00f6r'", "es", "dir", ",", "vor", "A\u00b7mors", "Oh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPER", "$,", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df ich \u2013 \u2013 ach! da\u00df ich falsch geschworen.", "tokens": ["Da\u00df", "ich", "\u2013", "\u2013", "ach", "!", "da\u00df", "ich", "falsch", "ge\u00b7schwo\u00b7ren", "."], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$(", "$(", "ITJ", "$.", "KOUS", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}