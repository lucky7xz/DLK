{"dta.poem.8010": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Die Lieder der Vorzeit .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Als Knabe stieg ich in die Hallen", "tokens": ["Als", "Kna\u00b7be", "stieg", "ich", "in", "die", "Hal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Verla\u00dfner Burgen oft hinan;", "tokens": ["Ver\u00b7la\u00df\u00b7ner", "Bur\u00b7gen", "oft", "hi\u00b7nan", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Durch alte St\u00e4dte th\u00e4t ich wallen,", "tokens": ["Durch", "al\u00b7te", "St\u00e4d\u00b7te", "th\u00e4t", "ich", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und sah die hohen M\u00fcnster an.", "tokens": ["Und", "sah", "die", "ho\u00b7hen", "M\u00fcns\u00b7ter", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da war es, da\u00df mit stillem Mahnen", "tokens": ["Da", "war", "es", ",", "da\u00df", "mit", "stil\u00b7lem", "Mah\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "$,", "KOUS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Geist der Vorwelt bei mir stand,", "tokens": ["Der", "Geist", "der", "Vor\u00b7welt", "bei", "mir", "stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da lie\u00df er fr\u00fche schon mich ahnen,", "tokens": ["Da", "lie\u00df", "er", "fr\u00fc\u00b7he", "schon", "mich", "ah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "ADV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Was sp\u00e4ter ich in B\u00fcchern fand:", "tokens": ["Was", "sp\u00e4\u00b7ter", "ich", "in", "B\u00fc\u00b7chern", "fand", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Da\u00df Jungfraun dort von ew\u2019gem Preise,", "tokens": ["Da\u00df", "Jung\u00b7fraun", "dort", "von", "ew'\u00b7gem", "Prei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die heil\u2019gen Lieder, einst gewohnt,", "tokens": ["Die", "heil'\u00b7gen", "Lie\u00b7der", ",", "einst", "ge\u00b7wohnt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und in der Edelfrauen Kreise", "tokens": ["Und", "in", "der", "E\u00b7del\u00b7frau\u00b7en", "Krei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bei\u2019m Feste des Gesangs gethront.", "tokens": ["Bei'm", "Fes\u00b7te", "des", "Ge\u00b7sangs", "ge\u00b7thront", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da kam der Krieger wild Geschlechte", "tokens": ["Da", "kam", "der", "Krie\u00b7ger", "wild", "Ge\u00b7schlech\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und warf den Brand in\u2019s frohe Haus.", "tokens": ["Und", "warf", "den", "Brand", "in's", "fro\u00b7he", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die Schwestern flohn im Graun der N\u00e4chte", "tokens": ["Die", "Schwes\u00b7tern", "flohn", "im", "Graun", "der", "N\u00e4ch\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Nach allen Seiten zagend aus.", "tokens": ["Nach", "al\u00b7len", "Sei\u00b7ten", "za\u00b7gend", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie manche schmachtet, hart gefangen,", "tokens": ["Wie", "man\u00b7che", "schmach\u00b7tet", ",", "hart", "ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVFIN", "$,", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In eines Kerkers dunklem Grund!", "tokens": ["In", "ei\u00b7nes", "Ker\u00b7kers", "dunk\u00b7lem", "Grund", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zu keinem milden Ohr gelangen", "tokens": ["Zu", "kei\u00b7nem", "mil\u00b7den", "Ohr", "ge\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Kl\u00e4ng\u2019 aus ihrem zarten Mund.", "tokens": ["Die", "Kl\u00e4ng'", "aus", "ih\u00b7rem", "zar\u00b7ten", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ach! Jene, die auf \u00f6den Wegen", "tokens": ["Ach", "!", "Je\u00b7ne", ",", "die", "auf", "\u00f6\u00b7den", "We\u00b7gen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "PDS", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Umhergeirret, krank und m\u00fcd,", "tokens": ["Um\u00b7her\u00b7geir\u00b7ret", ",", "krank", "und", "m\u00fcd", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.7": {"text": "Sie ist dem schweren Gram erlegen,", "tokens": ["Sie", "ist", "dem", "schwe\u00b7ren", "Gram", "er\u00b7le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und sang noch einmal, eh sie schied.", "tokens": ["Und", "sang", "noch", "ein\u00b7mal", ",", "eh", "sie", "schied", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "In eines armen M\u00e4dchens Kammer", "tokens": ["In", "ei\u00b7nes", "ar\u00b7men", "M\u00e4d\u00b7chens", "Kam\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ist einer Andern Aufenthalt,", "tokens": ["Ist", "ei\u00b7ner", "An\u00b7dern", "Auf\u00b7ent\u00b7halt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie mischt sich in der Freundin Jammer,", "tokens": ["Sie", "mischt", "sich", "in", "der", "Freun\u00b7din", "Jam\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wann still der Mond am Himmel wallt.", "tokens": ["Wann", "still", "der", "Mond", "am", "Him\u00b7mel", "wallt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Auch manche wagt der M\u00e4rterinnen", "tokens": ["Auch", "man\u00b7che", "wagt", "der", "M\u00e4r\u00b7te\u00b7rin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PIS", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sich in des Marktes frech Gew\u00fchl,", "tokens": ["Sich", "in", "des", "Mark\u00b7tes", "frech", "Ge\u00b7w\u00fchl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sie will der Menschen Herz gewinnen", "tokens": ["Sie", "will", "der", "Men\u00b7schen", "Herz", "ge\u00b7win\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "NN", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und singet sanft zum Saitenspiel.", "tokens": ["Und", "sin\u00b7get", "sanft", "zum", "Sai\u00b7ten\u00b7spiel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Getrost! schon sinken eure Bande", "tokens": ["Ge\u00b7trost", "!", "schon", "sin\u00b7ken", "eu\u00b7re", "Ban\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und Boten ziehn nach Ost und West,", "tokens": ["Und", "Bo\u00b7ten", "ziehn", "nach", "Ost", "und", "West", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "NE", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In eine Stadt am Neckarstrande", "tokens": ["In", "ei\u00b7ne", "Stadt", "am", "Ne\u00b7ckarst\u00b7ran\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zu laden euch zum neuen Fest.", "tokens": ["Zu", "la\u00b7den", "euch", "zum", "neu\u00b7en", "Fest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ihr Heitern, kommt zu Tanzes Feier,", "tokens": ["Ihr", "Hei\u00b7tern", ",", "kommt", "zu", "Tan\u00b7zes", "Fei\u00b7er", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "La\u00dft wehn das rosige Gewand!", "tokens": ["La\u00dft", "wehn", "das", "ro\u00b7si\u00b7ge", "Ge\u00b7wand", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ihr Ernsten, wallt im Nonnenschleier,", "tokens": ["Ihr", "Erns\u00b7ten", ",", "wallt", "im", "Non\u00b7nen\u00b7schlei\u00b7er", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die weisse Lilie in der Hand!", "tokens": ["Die", "weis\u00b7se", "Li\u00b7lie", "in", "der", "Hand", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}