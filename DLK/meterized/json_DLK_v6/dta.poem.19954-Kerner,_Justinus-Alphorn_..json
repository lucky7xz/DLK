{"dta.poem.19954": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Alphorn .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Alphorn h\u00f6r' ich schallen,", "tokens": ["Ein", "Al\u00b7phorn", "h\u00f6r'", "ich", "schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das mich von hinnen ruft;", "tokens": ["Das", "mich", "von", "hin\u00b7nen", "ruft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "APPR", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "T\u00f6nt es aus wald'gen Hallen?", "tokens": ["T\u00f6nt", "es", "aus", "wald'\u00b7gen", "Hal\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "T\u00f6nt es aus blauer Luft?", "tokens": ["T\u00f6nt", "es", "aus", "blau\u00b7er", "Luft", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "T\u00f6nt es von Bergesh\u00f6he?", "tokens": ["T\u00f6nt", "es", "von", "Ber\u00b7ges\u00b7h\u00f6\u00b7he", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Aus blumenreichem Thal?", "tokens": ["Aus", "blu\u00b7men\u00b7rei\u00b7chem", "Thal", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wo ich nur steh' und gehe,", "tokens": ["Wo", "ich", "nur", "steh'", "und", "ge\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "H\u00f6r' ich's in s\u00fc\u00dfer Quaal.", "tokens": ["H\u00f6r'", "ich's", "in", "s\u00fc\u00b7\u00dfer", "Qua\u00b7al", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Bei Spiel und frohem Reigen,", "tokens": ["Bei", "Spiel", "und", "fro\u00b7hem", "Rei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Einsam mit mir allein,", "tokens": ["Ein\u00b7sam", "mit", "mir", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "ADV", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "T\u00f6nt's, ohne je zu schweigen,", "tokens": ["T\u00f6nt's", ",", "oh\u00b7ne", "je", "zu", "schwei\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUI", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "T\u00f6nt tief in's Herz hinein.", "tokens": ["T\u00f6nt", "tief", "in's", "Herz", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Noch nie hab' ich gefunden", "tokens": ["Noch", "nie", "hab'", "ich", "ge\u00b7fun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Den Ort, woher es schallt,", "tokens": ["Den", "Ort", ",", "wo\u00b7her", "es", "schallt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und nimmer wird gesunden", "tokens": ["Und", "nim\u00b7mer", "wird", "ge\u00b7sun\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die\u00df Herz, bis es verhallt.", "tokens": ["Die\u00df", "Herz", ",", "bis", "es", "ver\u00b7hallt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "$,", "KOUS", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}