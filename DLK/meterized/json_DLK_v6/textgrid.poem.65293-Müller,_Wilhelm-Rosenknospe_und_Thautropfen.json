{"textgrid.poem.65293": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Rosenknospe und Thautropfen", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So oft ich einen Tropfen Thau", "tokens": ["So", "oft", "ich", "ei\u00b7nen", "Trop\u00b7fen", "Thau"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Seh' an der Rosenknospe hangen,", "tokens": ["Seh'", "an", "der", "Ro\u00b7sen\u00b7knos\u00b7pe", "han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Erkenn' ich meiner Liebe Bild.", "tokens": ["Er\u00b7kenn'", "ich", "mei\u00b7ner", "Lie\u00b7be", "Bild", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Rosenknospe bist du selbst,", "tokens": ["Die", "Ro\u00b7sen\u00b7knos\u00b7pe", "bist", "du", "selbst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die, kalt und starr, vor jedem Strahle", "tokens": ["Die", ",", "kalt", "und", "starr", ",", "vor", "je\u00b7dem", "Strah\u00b7le"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "$,", "ADJD", "KON", "ADJD", "$,", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Sonne noch das Herz verschlie\u00dft.", "tokens": ["Der", "Son\u00b7ne", "noch", "das", "Herz", "ver\u00b7schlie\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich aber bin der Tropfen Thau,", "tokens": ["Ich", "a\u00b7ber", "bin", "der", "Trop\u00b7fen", "Thau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der, weil dein Herz ihm ist verschlossen,", "tokens": ["Der", ",", "weil", "dein", "Herz", "ihm", "ist", "ver\u00b7schlos\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "PPOSAT", "NN", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sich in der Sonne Brand verzehrt.", "tokens": ["Sich", "in", "der", "Son\u00b7ne", "Brand", "ver\u00b7zehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}