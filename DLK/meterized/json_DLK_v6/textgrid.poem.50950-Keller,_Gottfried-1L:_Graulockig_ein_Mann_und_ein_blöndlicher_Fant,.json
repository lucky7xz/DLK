{"textgrid.poem.50950": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Graulockig ein Mann und ein bl\u00f6ndlicher Fant,", "genre": "verse", "period": "N.A.", "pub_year": 1850, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Graulockig ein Mann und ein bl\u00f6ndlicher Fant,", "tokens": ["Grau\u00b7lo\u00b7ckig", "ein", "Mann", "und", "ein", "bl\u00f6nd\u00b7li\u00b7cher", "Fant", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "KON", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Die gehen spazieren am sonnigen Strand,", "tokens": ["Die", "ge\u00b7hen", "spa\u00b7zie\u00b7ren", "am", "son\u00b7ni\u00b7gen", "Strand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVINF", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Der \u00c4ltere spricht zu dem j\u00fcngeren Wicht:", "tokens": ["Der", "\u00c4l\u00b7te\u00b7re", "spricht", "zu", "dem", "j\u00fcn\u00b7ge\u00b7ren", "Wicht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "\u00bbwas schneidest du f\u00fcr ein betr\u00fcbtes Gesicht?\u00ab", "tokens": ["\u00bb", "was", "schnei\u00b7dest", "du", "f\u00fcr", "ein", "be\u00b7tr\u00fcb\u00b7tes", "Ge\u00b7sicht", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWS", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.2": {"line.1": {"text": "Der klagt ihm, wie er ein Weib hielt wert,", "tokens": ["Der", "klagt", "ihm", ",", "wie", "er", "ein", "Weib", "hielt", "wert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "$,", "PWAV", "PPER", "ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dem neulich er fruchtlos die Liebe erkl\u00e4rt,", "tokens": ["Dem", "neu\u00b7lich", "er", "frucht\u00b7los", "die", "Lie\u00b7be", "er\u00b7kl\u00e4rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "PPER", "ADJD", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Und wie nun verletzt seine stolze Brust,", "tokens": ["Und", "wie", "nun", "ver\u00b7letzt", "sei\u00b7ne", "stol\u00b7ze", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Da\u00df er den Mund nicht zu halten gewu\u00dft!", "tokens": ["Da\u00df", "er", "den", "Mund", "nicht", "zu", "hal\u00b7ten", "ge\u00b7wu\u00dft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKNEG", "PTKZU", "VVINF", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Und jener spricht: \u00bbDes F\u00e4hrmanns Magd,", "tokens": ["Und", "je\u00b7ner", "spricht", ":", "\u00bb", "Des", "F\u00e4hr\u00b7manns", "Magd", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "$.", "$(", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Siehst du, die \u00fcber dem Strome ragt,", "tokens": ["Siehst", "du", ",", "die", "\u00fc\u00b7ber", "dem", "Stro\u00b7me", "ragt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Gering und arm und der Zierde bar,", "tokens": ["Ge\u00b7ring", "und", "arm", "und", "der", "Zier\u00b7de", "bar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "KON", "ART", "NN", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und siehst auch mein ergrauendes Haar?", "tokens": ["Und", "siehst", "auch", "mein", "er\u00b7grau\u00b7en\u00b7des", "Haar", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.4": {"line.1": {"text": "Gl\u00f6mm mir ein F\u00fcnklein Lieb zu ihr,", "tokens": ["Gl\u00f6mm", "mir", "ein", "F\u00fcn\u00b7klein", "Lieb", "zu", "ihr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Laut rief' ich es von der Stelle hier,", "tokens": ["Laut", "rie\u00b7f'", "ich", "es", "von", "der", "Stel\u00b7le", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PPER", "PPER", "APPR", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Rief's laut in der Wellen t\u00f6nenden Gang,", "tokens": ["Rie\u00b7f's", "laut", "in", "der", "Wel\u00b7len", "t\u00f6\u00b7nen\u00b7den", "Gang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Mich d\u00fcnkt' es der allersch\u00f6nste Gesang!", "tokens": ["Mich", "d\u00fcnkt'", "es", "der", "al\u00b7ler\u00b7sch\u00f6ns\u00b7te", "Ge\u00b7sang", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Hoch schlug mir in meiner Jugend das Herz,", "tokens": ["Hoch", "schlug", "mir", "in", "mei\u00b7ner", "Ju\u00b7gend", "das", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "+---+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Und feurig schweifte das Aug allw\u00e4rts;", "tokens": ["Und", "feu\u00b7rig", "schweif\u00b7te", "das", "Aug", "all\u00b7w\u00e4rts", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Fromm hab ich so manches Gest\u00e4ndnis gemacht,", "tokens": ["Fromm", "hab", "ich", "so", "man\u00b7ches", "Ge\u00b7st\u00e4nd\u00b7nis", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ADV", "PIAT", "NN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Die ein' hat geweint und die andre gelacht.", "tokens": ["Die", "ein'", "hat", "ge\u00b7weint", "und", "die", "and\u00b7re", "ge\u00b7lacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VAFIN", "VVPP", "KON", "ART", "ADJA", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.6": {"line.1": {"text": "Bei ", "tokens": ["Bei"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Wie sehr es auch sterbend im Herzen gezuckt.", "tokens": ["Wie", "sehr", "es", "auch", "ster\u00b7bend", "im", "Her\u00b7zen", "ge\u00b7zuckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "ADV", "ADJD", "APPRART", "NN", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Ich glaube, sie ahnt' es und l\u00e4chelte fein;", "tokens": ["Ich", "glau\u00b7be", ",", "sie", "ahnt'", "es", "und", "l\u00e4\u00b7chel\u00b7te", "fein", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "KON", "VVFIN", "ADJD", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Doch wei\u00df ich nicht, sang's in ihr ja oder nein.", "tokens": ["Doch", "wei\u00df", "ich", "nicht", ",", "sang's", "in", "ihr", "ja", "o\u00b7der", "nein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "$,", "VVFIN", "APPR", "PPER", "ADV", "KON", "PTKANT", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.7": {"line.1": {"text": "Und gr\u00e4mlich schwieg ich und ging in die Welt,", "tokens": ["Und", "gr\u00e4m\u00b7lich", "schwieg", "ich", "und", "ging", "in", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Schlug auf, brach ab mein Wanderzelt;", "tokens": ["Schlug", "auf", ",", "brach", "ab", "mein", "Wan\u00b7der\u00b7zelt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auch oftmals kam ich wieder ins Land,", "tokens": ["Auch", "oft\u00b7mals", "kam", "ich", "wie\u00b7der", "ins", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Wo stets ich die l\u00e4chelnde Dame fand.", "tokens": ["Wo", "stets", "ich", "die", "l\u00e4\u00b7cheln\u00b7de", "Da\u00b7me", "fand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.8": {"line.1": {"text": "Der Sommer war warm und der Winter kalt,", "tokens": ["Der", "Som\u00b7mer", "war", "warm", "und", "der", "Win\u00b7ter", "kalt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ART", "NN", "ADJD", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Die Zeit verging, und wir wurden alt;", "tokens": ["Die", "Zeit", "ver\u00b7ging", ",", "und", "wir", "wur\u00b7den", "alt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Als ich zum letzten Mal sie sah,", "tokens": ["Als", "ich", "zum", "letz\u00b7ten", "Mal", "sie", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Lag sie im Leichenhemde da.", "tokens": ["Lag", "sie", "im", "Lei\u00b7chen\u00b7hem\u00b7de", "da", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Die Augen starrten mich offen an,", "tokens": ["Die", "Au\u00b7gen", "starr\u00b7ten", "mich", "of\u00b7fen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Weil niemand liebend sie zugetan,", "tokens": ["Weil", "nie\u00b7mand", "lie\u00b7bend", "sie", "zu\u00b7ge\u00b7tan", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "PPER", "VVPP", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Doch auf den Lippen, bleich und tot,", "tokens": ["Doch", "auf", "den", "Lip\u00b7pen", ",", "bleich", "und", "tot", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Lag lieblich l\u00e4chelnd noch der Spott.", "tokens": ["Lag", "lieb\u00b7lich", "l\u00e4\u00b7chelnd", "noch", "der", "Spott", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADJD", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Er schien zu sagen: 'O grober Mann,", "tokens": ["Er", "schien", "zu", "sa\u00b7gen", ":", "'o", "gro\u00b7ber", "Mann", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKZU", "VVINF", "$.", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der so mit Worten geizen kann!'", "tokens": ["Der", "so", "mit", "Wor\u00b7ten", "gei\u00b7zen", "kann", "!", "'"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "ADV", "APPR", "NN", "VVINF", "VMFIN", "$.", "$("], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Ich \u00e4rgerte und kr\u00e4nkte mich,", "tokens": ["Ich", "\u00e4r\u00b7ger\u00b7te", "und", "kr\u00e4nk\u00b7te", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df ich besch\u00e4mt von dannen schlich!\u00ab", "tokens": ["Da\u00df", "ich", "be\u00b7sch\u00e4mt", "von", "dan\u00b7nen", "schlich", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "ADJD", "APPR", "ADV", "ADJD", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}