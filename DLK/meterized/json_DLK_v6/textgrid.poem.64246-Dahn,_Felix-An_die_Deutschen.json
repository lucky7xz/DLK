{"textgrid.poem.64246": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "An die Deutschen", "genre": "verse", "period": "N.A.", "pub_year": 1873, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Senket von Sedan die Siegesfahnen,", "tokens": ["Sen\u00b7ket", "von", "Se\u00b7dan", "die", "Sie\u00b7ges\u00b7fah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NE", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Senket die H\u00e4upter in Scham, Germanen!", "tokens": ["Sen\u00b7ket", "die", "H\u00e4up\u00b7ter", "in", "Scham", ",", "Ger\u00b7ma\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "$,", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "\u00bbtreue der Deutschen\u00ab: \u2013 ein Wort der Schande!", "tokens": ["\u00bb", "treu\u00b7e", "der", "Deut\u00b7schen", "\u00ab", ":", "\u2013", "ein", "Wort", "der", "Schan\u00b7de", "!"], "token_info": ["punct", "word", "word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ART", "NN", "$(", "$.", "$(", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Unsere Schmach schreit \u00fcber die Lande!", "tokens": ["Un\u00b7se\u00b7re", "Schmach", "schreit", "\u00fc\u00b7ber", "die", "Lan\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Nimmer des Lorbeers, des \u00d6lbaums Reiser", "tokens": ["Nim\u00b7mer", "des", "Lor\u00b7beers", ",", "des", "\u00d6l\u00b7baums", "Rei\u00b7ser"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "$,", "ART", "NN", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.6": {"text": "Schirmen das teuere Haupt dem Kaiser!", "tokens": ["Schir\u00b7men", "das", "teu\u00b7e\u00b7re", "Haupt", "dem", "Kai\u00b7ser", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.7": {"text": "Heilig dem Fremden dies Angesicht: \u2013", "tokens": ["Hei\u00b7lig", "dem", "Frem\u00b7den", "dies", "An\u00b7ge\u00b7sicht", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADJA", "ART", "NN", "PDS", "NN", "$.", "$("], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.8": {"text": "Aber dem Wahn der Deutschen nicht! \u2013", "tokens": ["A\u00b7ber", "dem", "Wahn", "der", "Deut\u00b7schen", "nicht", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "PTKNEG", "$.", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Giftige F\u00e4ulnis ergriff dies Geschlecht:", "tokens": ["Gif\u00b7ti\u00b7ge", "F\u00e4ul\u00b7nis", "er\u00b7griff", "dies", "Ge\u00b7schlecht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PDS", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Aber gedenkt, da\u00df der ", "tokens": ["A\u00b7ber", "ge\u00b7denkt", ",", "da\u00df", "der"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "$,", "KOUS", "ART"], "meter": "+--+--", "measure": "dactylic.di.plus"}, "line.3": {"text": "Da\u00df es die ", "tokens": ["Da\u00df", "es", "die"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "ART"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Hoch erhebet des Rechtes Schild:", "tokens": ["Hoch", "er\u00b7he\u00b7bet", "des", "Rech\u00b7tes", "Schild", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "Schlagt mit dem Schwert des Kaisers daran", "tokens": ["Schlagt", "mit", "dem", "Schwert", "des", "Kai\u00b7sers", "da\u00b7ran"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN", "PAV"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "\u2013 In der Scheide nur trug es der mildeste Mann! \u2013:", "tokens": ["\u2013", "In", "der", "Schei\u00b7de", "nur", "trug", "es", "der", "mil\u00b7des\u00b7te", "Mann", "!", "\u2013", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "APPR", "ART", "NN", "ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$.", "$(", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.7": {"text": "Dr\u00f6hnend und drohend \u00fcber das Reich", "tokens": ["Dr\u00f6h\u00b7nend", "und", "dro\u00b7hend", "\u00fc\u00b7ber", "das", "Reich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "KON", "ADJD", "APPR", "ART", "NN"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Schalle der eherne, warnende Streich:", "tokens": ["Schal\u00b7le", "der", "e\u00b7her\u00b7ne", ",", "war\u00b7nen\u00b7de", "Streich", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.9": {"text": "Frevler zu schrecken, S\u00e4um'ge zu wecken,", "tokens": ["Frev\u00b7ler", "zu", "schre\u00b7cken", ",", "S\u00e4um'\u00b7ge", "zu", "we\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$,", "NN", "PTKZU", "VVINF", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.10": {"text": "Alle zu mahnen, den Kaiser zu decken!", "tokens": ["Al\u00b7le", "zu", "mah\u00b7nen", ",", "den", "Kai\u00b7ser", "zu", "de\u00b7cken", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PTKZU", "VVINF", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}}, "stanza.3": {"line.1": {"text": "Wahrlich, ihr deckt mit dem Kaiser zugleich", "tokens": ["Wahr\u00b7lich", ",", "ihr", "deckt", "mit", "dem", "Kai\u00b7ser", "zu\u00b7gleich"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PPER", "VVFIN", "APPR", "ART", "NN", "ADV"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Nicht nur die Ehre, den Ruhm und das Reich \u2013", "tokens": ["Nicht", "nur", "die", "Eh\u00b7re", ",", "den", "Ruhm", "und", "das", "Reich", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ART", "NN", "$,", "ART", "NN", "KON", "ART", "NN", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Alles, was heilig und edel und teuer:", "tokens": ["Al\u00b7les", ",", "was", "hei\u00b7lig", "und", "e\u00b7del", "und", "teu\u00b7er", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "ADJD", "KON", "ADJD", "KON", "ADJD", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Bildung und Zucht und des Herdes Feuer!", "tokens": ["Bil\u00b7dung", "und", "Zucht", "und", "des", "Her\u00b7des", "Feu\u00b7er", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "ART", "NN", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.5": {"text": "La\u00dft, ihr verblendeten Br\u00fcder, das Zanken!", "tokens": ["La\u00dft", ",", "ihr", "ver\u00b7blen\u00b7de\u00b7ten", "Br\u00fc\u00b7der", ",", "das", "Zan\u00b7ken", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "$,", "PPOSAT", "ADJA", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "F\u00fchlt ihr den Boden des Hauses nicht wanken?", "tokens": ["F\u00fchlt", "ihr", "den", "Bo\u00b7den", "des", "Hau\u00b7ses", "nicht", "wan\u00b7ken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "++-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Tretet sie aus, die aufz\u00fcngelnden Flammen: \u2013", "tokens": ["Tre\u00b7tet", "sie", "aus", ",", "die", "auf\u00b7z\u00fcn\u00b7geln\u00b7den", "Flam\u00b7men", ":", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "ART", "ADJA", "NN", "$.", "$("], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.8": {"text": "Krachend sonst brechen die Balken zusammen.", "tokens": ["Kra\u00b7chend", "sonst", "bre\u00b7chen", "die", "Bal\u00b7ken", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}}}}}