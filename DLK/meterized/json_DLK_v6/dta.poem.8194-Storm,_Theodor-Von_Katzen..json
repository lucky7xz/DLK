{"dta.poem.8194": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Von Katzen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1852", "urn": "urn:nbn:de:kobv:b4-200905196098", "language": ["de:0.99"], "booktitle": "Storm, Theodor: Gedichte. Kiel, 1852."}, "poem": {"stanza.1": {"line.1": {"text": "Zur Welt sechs allerliebste kleine K\u00e4tzchen,", "tokens": ["Zur", "Welt", "sechs", "al\u00b7ler\u00b7liebs\u00b7te", "klei\u00b7ne", "K\u00e4tz\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "CARD", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Maik\u00e4tzchen, alle wei\u00df mit schwarzen Schw\u00e4nzchen.", "tokens": ["Mai\u00b7k\u00e4tz\u00b7chen", ",", "al\u00b7le", "wei\u00df", "mit", "schwar\u00b7zen", "Schw\u00e4nz\u00b7chen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PIS", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.3": {"text": "F\u00fcrwahr, es war ein zierlich Wochenbettchen!", "tokens": ["F\u00fcr\u00b7wahr", ",", "es", "war", "ein", "zier\u00b7lich", "Wo\u00b7chen\u00b7bett\u00b7chen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die K\u00f6chin aber \u2014 K\u00f6chinnen sind grausam,", "tokens": ["Die", "K\u00f6\u00b7chin", "a\u00b7ber", "K\u00f6\u00b7chin\u00b7nen", "sind", "grau\u00b7sam", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$(", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und Menschlichkeit w\u00e4chst nicht in einer K\u00fcche \u2014", "tokens": ["Und", "Menschlich\u00b7keit", "w\u00e4chst", "nicht", "in", "ei\u00b7ner", "K\u00fc\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PTKNEG", "APPR", "ART", "NN", "$("], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Die wollte von den Sechsen f\u00fcnf ertr\u00e4nken,", "tokens": ["Die", "woll\u00b7te", "von", "den", "Sech\u00b7sen", "f\u00fcnf", "er\u00b7tr\u00e4n\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "APPR", "ART", "NN", "CARD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "F\u00fcnf wei\u00dfe, schwarzgeschw\u00e4nzte Maienk\u00e4tzchen", "tokens": ["F\u00fcnf", "wei\u00b7\u00dfe", ",", "schwarz\u00b7ge\u00b7schw\u00e4nz\u00b7te", "Mai\u00b7en\u00b7k\u00e4tz\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["CARD", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ermorden wollte dies verruchte Weib.", "tokens": ["Er\u00b7mor\u00b7den", "woll\u00b7te", "dies", "ver\u00b7ruch\u00b7te", "Weib", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PDS", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Ich half ihr heim! \u2014 der Himmel segne", "tokens": ["Ich", "half", "ihr", "heim", "!", "der", "Him\u00b7mel", "seg\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$.", "$(", "ART", "NN", "VVFIN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Mir meine Menschlichkeit! Die lieben K\u00e4tzchen,", "tokens": ["Mir", "mei\u00b7ne", "Menschlich\u00b7keit", "!", "Die", "lie\u00b7ben", "K\u00e4tz\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "$.", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Sie wuchsen auf und schritten binnen Kurzem", "tokens": ["Sie", "wuch\u00b7sen", "auf", "und", "schrit\u00b7ten", "bin\u00b7nen", "Kur\u00b7zem"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Erhobenen Schwanzes \u00fcber Hof und Heerd;", "tokens": ["Er\u00b7ho\u00b7be\u00b7nen", "Schwan\u00b7zes", "\u00fc\u00b7ber", "Hof", "und", "Heerd", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Ja, wie die K\u00f6chin auch ingrimmig drein sah,", "tokens": ["Ja", ",", "wie", "die", "K\u00f6\u00b7chin", "auch", "in\u00b7grim\u00b7mig", "drein", "sah", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PWAV", "ART", "NN", "ADV", "ADJD", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.14": {"text": "Sie wuchsen auf, und Nachts vor ihrem Fenster", "tokens": ["Sie", "wuch\u00b7sen", "auf", ",", "und", "Nachts", "vor", "ih\u00b7rem", "Fens\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "KON", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Probirten sie die allerliebsten Stimmchen.", "tokens": ["Pro\u00b7bir\u00b7ten", "sie", "die", "al\u00b7ler\u00b7liebs\u00b7ten", "Stimm\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Ich aber, wie ich sie so wachsen sahe,", "tokens": ["Ich", "a\u00b7ber", ",", "wie", "ich", "sie", "so", "wach\u00b7sen", "sa\u00b7he", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "PWAV", "PPER", "PPER", "ADV", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Ich pries mich selbst und meine Menschlichkeit. \u2014", "tokens": ["Ich", "pries", "mich", "selbst", "und", "mei\u00b7ne", "Menschlich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "KON", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Ein Jahr ist um, und Katzen sind die K\u00e4tzchen,", "tokens": ["Ein", "Jahr", "ist", "um", ",", "und", "Kat\u00b7zen", "sind", "die", "K\u00e4tz\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "$,", "KON", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Und Maitag ist's! \u2014 Wie soll ich es beschreiben,", "tokens": ["Und", "Mai\u00b7tag", "ist's", "!", "Wie", "soll", "ich", "es", "be\u00b7schrei\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "$.", "$(", "PWAV", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.20": {"text": "Das Schauspiel, das sich jetzt vor mir entfaltet!", "tokens": ["Das", "Schau\u00b7spiel", ",", "das", "sich", "jetzt", "vor", "mir", "ent\u00b7fal\u00b7tet", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "ADV", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-++-+--+-", "measure": "iambic.penta.relaxed"}, "line.21": {"text": "Mein ganzes Haus, vom Keller bis zum Giebel,", "tokens": ["Mein", "gan\u00b7zes", "Haus", ",", "vom", "Kel\u00b7ler", "bis", "zum", "Gie\u00b7bel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "APPRART", "NN", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Ein jeder Winkel ist ein Wochenbettchen!", "tokens": ["Ein", "je\u00b7der", "Win\u00b7kel", "ist", "ein", "Wo\u00b7chen\u00b7bett\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Hier liegt das eine, dort das andre K\u00e4tzchen,", "tokens": ["Hier", "liegt", "das", "ei\u00b7ne", ",", "dort", "das", "and\u00b7re", "K\u00e4tz\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ART", "$,", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "In Schr\u00e4nken, K\u00f6rben, unter Tisch und Treppen,", "tokens": ["In", "Schr\u00e4n\u00b7ken", ",", "K\u00f6r\u00b7ben", ",", "un\u00b7ter", "Tisch", "und", "Trep\u00b7pen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Die Alte gar \u2014 nein, es ist unaussprechlich,", "tokens": ["Die", "Al\u00b7te", "gar", "nein", ",", "es", "ist", "un\u00b7aus\u00b7sprech\u00b7lich", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$(", "PTKANT", "$,", "PPER", "VAFIN", "ADJD", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.26": {"text": "Liegt in der K\u00f6chin jungfr\u00e4ulichem Bette!", "tokens": ["Liegt", "in", "der", "K\u00f6\u00b7chin", "jung\u00b7fr\u00e4u\u00b7li\u00b7chem", "Bet\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.27": {"text": "Und jede, jede von den sieben Katzen", "tokens": ["Und", "je\u00b7de", ",", "je\u00b7de", "von", "den", "sie\u00b7ben", "Kat\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "$,", "PIAT", "APPR", "ART", "CARD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Hat sieben, denkt euch! sieben junge K\u00e4tzchen,", "tokens": ["Hat", "sie\u00b7ben", ",", "denkt", "euch", "!", "sie\u00b7ben", "jun\u00b7ge", "K\u00e4tz\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "CARD", "$,", "VVFIN", "PPER", "$.", "CARD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Maik\u00e4tzchen, alle wei\u00df mit schwarzen Schw\u00e4nzchen.", "tokens": ["Mai\u00b7k\u00e4tz\u00b7chen", ",", "al\u00b7le", "wei\u00df", "mit", "schwar\u00b7zen", "Schw\u00e4nz\u00b7chen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PIS", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.30": {"text": "Die K\u00f6chin ras't, ich kann der blinden Wuth", "tokens": ["Die", "K\u00f6\u00b7chin", "ras't", ",", "ich", "kann", "der", "blin\u00b7den", "Wuth"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VMFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "Nicht Schranken setzen dieses Frauenzimmers;", "tokens": ["Nicht", "Schran\u00b7ken", "set\u00b7zen", "die\u00b7ses", "Frau\u00b7en\u00b7zim\u00b7mers", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "VVFIN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "Ers\u00e4ufen will sie alle neun und vierzig!", "tokens": ["Er\u00b7s\u00e4u\u00b7fen", "will", "sie", "al\u00b7le", "neun", "und", "vier\u00b7zig", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PIAT", "CARD", "KON", "CARD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "Mir selber, ach, mir l\u00e4uft der Kopf davon \u2014", "tokens": ["Mir", "sel\u00b7ber", ",", "ach", ",", "mir", "l\u00e4uft", "der", "Kopf", "da\u00b7von"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "ITJ", "$,", "PPER", "VVFIN", "ART", "NN", "PAV", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.34": {"text": "O Menschlichkeit, wie soll ich dich bewahren!", "tokens": ["O", "Menschlich\u00b7keit", ",", "wie", "soll", "ich", "dich", "be\u00b7wah\u00b7ren", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWAV", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "+---+-+-+-", "measure": "dactylic.init"}, "line.35": {"text": "Was fang' ich an mit sechs und funfzig Katzen! \u2014", "tokens": ["Was", "fang'", "ich", "an", "mit", "sechs", "und", "funf\u00b7zig", "Kat\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "APPR", "CARD", "KON", "CARD", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}