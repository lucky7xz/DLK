{"dta.poem.20167": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Der Lieb-erfreute  \n  FJLAMON .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1666", "urn": "urn:nbn:de:kobv:b4-20424-1", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch-Historischer Lustgarten. Frankfurt (Main), 1666."}, "poem": {"stanza.1": {"line.1": {"text": "Die Tr\u00e4ume sind nichtig und machen zu Narren", "tokens": ["Die", "Tr\u00e4u\u00b7me", "sind", "nich\u00b7tig", "und", "ma\u00b7chen", "zu", "Nar\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "VVFIN", "APPR", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Fast alle die jenigen die darauf harren/", "tokens": ["Fast", "al\u00b7le", "die", "je\u00b7ni\u00b7gen", "die", "da\u00b7rauf", "har\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "ART", "PIAT", "ART", "PAV", "VVINF", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wenn ihnen was herliches tr\u00e4umet bey Nacht/", "tokens": ["Wenn", "ih\u00b7nen", "was", "her\u00b7li\u00b7ches", "tr\u00e4u\u00b7met", "bey", "Nacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "ADJA", "VVFIN", "APPR", "NN", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Nicht minder sind diese f\u00fcr Thoren zu sch\u00e4tzen/", "tokens": ["Nicht", "min\u00b7der", "sind", "die\u00b7se", "f\u00fcr", "Tho\u00b7ren", "zu", "sch\u00e4t\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VAFIN", "PDS", "APPR", "NN", "PTKZU", "VVINF", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.5": {"text": "Die sich f\u00fcr den schreklichen Tr\u00e4umen entsetzen/", "tokens": ["Die", "sich", "f\u00fcr", "den", "schrek\u00b7li\u00b7chen", "Tr\u00e4u\u00b7men", "ent\u00b7set\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "So ihnen der ", "tokens": ["So", "ih\u00b7nen", "der"], "token_info": ["word", "word", "word"], "pos": ["ADV", "PPER", "ART"], "meter": "-+--", "measure": "dactylic.init"}}}}}