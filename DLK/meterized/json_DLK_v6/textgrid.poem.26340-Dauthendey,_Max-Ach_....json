{"textgrid.poem.26340": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Ach ...", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach, k\u00f6nnte jetzt die Abendruhe dich bei mir f\u00fchlen,", "tokens": ["Ach", ",", "k\u00f6nn\u00b7te", "jetzt", "die", "A\u00b7ben\u00b7dru\u00b7he", "dich", "bei", "mir", "f\u00fch\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VMFIN", "ADV", "ART", "NN", "PRF", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ich w\u00fcrde gl\u00fchen in der Bergluft, der k\u00fchlen.", "tokens": ["Ich", "w\u00fcr\u00b7de", "gl\u00fc\u00b7hen", "in", "der", "Berg\u00b7luft", ",", "der", "k\u00fch\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJA", "APPR", "ART", "NN", "$,", "PRELS", "VVINF", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Drei Jahre sind jetzt gegangen seit der Trennungsstunde,", "tokens": ["Drei", "Jah\u00b7re", "sind", "jetzt", "ge\u00b7gan\u00b7gen", "seit", "der", "Tren\u00b7nungs\u00b7stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "ADV", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Drei Jahre r\u00fchrte keine Lippe an meinem Munde.", "tokens": ["Drei", "Jah\u00b7re", "r\u00fchr\u00b7te", "kei\u00b7ne", "Lip\u00b7pe", "an", "mei\u00b7nem", "Mun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PIAT", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Die Einsamkeit hielt mich friedlos und lieblos gefangen,", "tokens": ["Die", "Ein\u00b7sam\u00b7keit", "hielt", "mich", "fried\u00b7los", "und", "lieb\u00b7los", "ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "PTKVZ", "$,"], "meter": "-+--+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Seit deine Worte nicht mehr zu meinen Ohren drangen,", "tokens": ["Seit", "dei\u00b7ne", "Wor\u00b7te", "nicht", "mehr", "zu", "mei\u00b7nen", "Oh\u00b7ren", "dran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKNEG", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Wie die W\u00e4lder im Finstern mu\u00df ich im Schweigen harren,", "tokens": ["Wie", "die", "W\u00e4l\u00b7der", "im", "Fins\u00b7tern", "mu\u00df", "ich", "im", "Schwei\u00b7gen", "har\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN", "VMFIN", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "--+--+--+-+-+-", "measure": "anapaest.tri.plus"}, "line.8": {"text": "Mu\u00df wie k\u00f6rperlos nur auf meine Gedanken starren.", "tokens": ["Mu\u00df", "wie", "k\u00f6r\u00b7per\u00b7los", "nur", "auf", "mei\u00b7ne", "Ge\u00b7dan\u00b7ken", "star\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "KOKOM", "ADJD", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "--+--+-+--+-+-", "measure": "anapaest.di.plus"}, "line.9": {"text": "Wie manchmal ein Klang kommt aus fernem n\u00e4chtlichen Orte,", "tokens": ["Wie", "manch\u00b7mal", "ein", "Klang", "kommt", "aus", "fer\u00b7nem", "n\u00e4cht\u00b7li\u00b7chen", "Or\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "VVFIN", "APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "-+--+--+-+--+-", "measure": "amphibrach.tri.plus"}, "line.10": {"text": "Kommt Erinnerung und weckt l\u00e4ngst verschollene Worte.", "tokens": ["Kommt", "E\u00b7rin\u00b7ne\u00b7rung", "und", "weckt", "l\u00e4ngst", "ver\u00b7schol\u00b7le\u00b7ne", "Wor\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "KON", "VVFIN", "ADV", "ADJA", "NN", "$."], "meter": "--+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Dann brennen Tropfen in meinen Augen, die mich blenden, \u2013", "tokens": ["Dann", "bren\u00b7nen", "Trop\u00b7fen", "in", "mei\u00b7nen", "Au\u00b7gen", ",", "die", "mich", "blen\u00b7den", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "$("], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Der Himmel hat tausend Wege und will keinen senden.", "tokens": ["Der", "Him\u00b7mel", "hat", "tau\u00b7send", "We\u00b7ge", "und", "will", "kei\u00b7nen", "sen\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "CARD", "NN", "KON", "VMFIN", "PIAT", "ADJA", "$."], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}}}}}