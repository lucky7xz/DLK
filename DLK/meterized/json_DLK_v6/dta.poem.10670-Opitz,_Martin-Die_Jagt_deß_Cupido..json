{"dta.poem.10670": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Die Jagt de\u00df Cupido.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "In der sch\u00f6nesten der Zeiten/", "tokens": ["In", "der", "sch\u00f6\u00b7nes\u00b7ten", "der", "Zei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "VVFIN", "ART", "NN", "$("], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Wenn verj\u00fcngt wirdt alle Welt/", "tokens": ["Wenn", "ver\u00b7j\u00fcngt", "wirdt", "al\u00b7le", "Welt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVPP", "VAFIN", "PIAT", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wann die Flora Blumen spreiten", "tokens": ["Wann", "die", "Flo\u00b7ra", "Blu\u00b7men", "sprei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Thut/ durch Wisen/ Wald vnd Feldt/", "tokens": ["Thut", "/", "durch", "Wi\u00b7sen", "/", "Wald", "vnd", "Feldt", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$(", "APPR", "NN", "$(", "NN", "KON", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Kam der Venus Sohn gegangen/", "tokens": ["Kam", "der", "Ve\u00b7nus", "Sohn", "ge\u00b7gan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "NN", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Eh sich Lucifer er\u00e4u