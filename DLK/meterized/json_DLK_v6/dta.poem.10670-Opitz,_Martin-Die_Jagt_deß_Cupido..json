PDS", "NN", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Komm auch wieder her zu mir.", "tokens": ["Komm", "auch", "wie\u00b7der", "her", "zu", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ADV", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "Di\u00df derhalben zu vollf\u00fchren/", "tokens": ["Di\u00df", "der\u00b7hal\u00b7ben", "zu", "voll\u00b7f\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "PTKZU", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "War er baldt zur Jagt bereit/", "tokens": ["War", "er", "baldt", "zur", "Jagt", "be\u00b7reit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPRART", "VVFIN", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.23": {"text": "Nicht zur Jagt nach wilden Thieren/", "tokens": ["Nicht", "zur", "Jagt", "nach", "wil\u00b7den", "Thie\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPRART", "VVFIN", "APPR", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Wie Adonis vor der Zeit/", "tokens": ["Wie", "A\u00b7do\u00b7nis", "vor", "der", "Zeit", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "APPR", "ART", "NN", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.25": {"text": "Sondern da\u00df er m\u00f6chte zwingen/", "tokens": ["Son\u00b7dern", "da\u00df", "er", "m\u00f6ch\u00b7te", "zwin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VMFIN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Diese grosse weite Welt/", "toke