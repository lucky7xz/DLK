{"textgrid.poem.52787": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nein, \u00fcber's Herz kann ich's nicht bringen,", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nein, \u00fcber's Herz kann ich's nicht bringen,", "tokens": ["Nein", ",", "\u00fc\u00b7ber's", "Herz", "kann", "ich's", "nicht", "brin\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "APPRART", "NN", "VMFIN", "PIS", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich mu\u00df den sch\u00f6nen Tag besingen,", "tokens": ["Ich", "mu\u00df", "den", "sch\u00f6\u00b7nen", "Tag", "be\u00b7sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Womit nach so viel rauher Zeit", "tokens": ["Wo\u00b7mit", "nach", "so", "viel", "rau\u00b7her", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADV", "PIAT", "ADJA", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Der Himmel wieder uns erfreut.", "tokens": ["Der", "Him\u00b7mel", "wie\u00b7der", "uns", "er\u00b7freut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Aepfel-, Birn- und Zwetschgenb\u00e4ume", "tokens": ["Die", "A\u00b7e\u00b7pfel", ",", "Birn", "und", "Zwetschgen\u00b7b\u00e4u\u00b7me"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "TRUNC", "$,", "TRUNC", "KON", "NN"], "meter": "-+--+-++-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Traubenst\u00f6ck' und Dinkelkeime", "tokens": ["Die", "Trau\u00b7ben\u00b7st\u00f6ck", "und", "Din\u00b7kel\u00b7kei\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie alle streben schon empor,", "tokens": ["Sie", "al\u00b7le", "stre\u00b7ben", "schon", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auch ich erhebe Herz und Ohr.", "tokens": ["Auch", "ich", "er\u00b7he\u00b7be", "Herz", "und", "Ohr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Thal und H\u00fcgel werden gr\u00fcn", "tokens": ["Thal", "und", "H\u00fc\u00b7gel", "wer\u00b7den", "gr\u00fcn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VAFIN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Die B\u00e4ume schlagen aus,", "tokens": ["Die", "B\u00e4u\u00b7me", "schla\u00b7gen", "aus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ja mancher f\u00e4ngt schon an zu bl\u00fch'n,", "tokens": ["Ja", "man\u00b7cher", "f\u00e4ngt", "schon", "an", "zu", "bl\u00fch'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PIS", "VVFIN", "ADV", "APPR", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und bildet einen Strau\u00df.", "tokens": ["Und", "bil\u00b7det", "ei\u00b7nen", "Strau\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Herrlich stehen schon und hoch", "tokens": ["Herr\u00b7lich", "ste\u00b7hen", "schon", "und", "hoch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ADV", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Die gr\u00fcnen Wintersaaten,", "tokens": ["Die", "gr\u00fc\u00b7nen", "Win\u00b7ter\u00b7saa\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "O m\u00f6chten die Kartoffeln doch", "tokens": ["O", "m\u00f6ch\u00b7ten", "die", "Kar\u00b7tof\u00b7feln", "doch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VMFIN", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In diesem Jahr gerathen!", "tokens": ["In", "die\u00b7sem", "Jahr", "ge\u00b7ra\u00b7then", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ach Gott, der sch\u00f6ne Weinstock ist", "tokens": ["Ach", "Gott", ",", "der", "sch\u00f6\u00b7ne", "Wein\u00b7stock", "ist"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "NN", "$,", "ART", "ADJA", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In dieser Nacht erfroren,", "tokens": ["In", "die\u00b7ser", "Nacht", "er\u00b7fro\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Hoffnung, die den Schmerz vers\u00fc\u00dft,", "tokens": ["Die", "Hoff\u00b7nung", ",", "die", "den", "Schmerz", "ver\u00b7s\u00fc\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ach, Alles ist verloren.", "tokens": ["Ach", ",", "Al\u00b7les", "ist", "ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PIS", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Vier Jahre hat die K\u00e4lte itzt", "tokens": ["Vier", "Jah\u00b7re", "hat", "die", "K\u00e4l\u00b7te", "itzt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "VAFIN", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Uns Obst und Wein genommen,", "tokens": ["Uns", "Obst", "und", "Wein", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wohin wird, der in Armuth sitzt,", "tokens": ["Wo\u00b7hin", "wird", ",", "der", "in", "Ar\u00b7muth", "sitzt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wohin wird der noch kommen?", "tokens": ["Wo\u00b7hin", "wird", "der", "noch", "kom\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}