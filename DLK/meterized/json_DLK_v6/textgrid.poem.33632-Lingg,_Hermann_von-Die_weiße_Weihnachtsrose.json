{"textgrid.poem.33632": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Die wei\u00dfe Weihnachtsrose", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn \u00fcber Wege tief beschneit", "tokens": ["Wenn", "\u00fc\u00b7ber", "We\u00b7ge", "tief", "be\u00b7schneit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "NN", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Schlitten lustig rennt,", "tokens": ["Der", "Schlit\u00b7ten", "lus\u00b7tig", "rennt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Im Sp\u00e4tjahr in der D\u00e4mmerzeit,", "tokens": ["Im", "Sp\u00e4t\u00b7jahr", "in", "der", "D\u00e4m\u00b7mer\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Wochen im Advent,", "tokens": ["Die", "Wo\u00b7chen", "im", "Ad\u00b7vent", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Wenn aus dem Schnee das junge Reh", "tokens": ["Wenn", "aus", "dem", "Schnee", "das", "jun\u00b7ge", "Reh"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sich Kr\u00e4uter sucht und Moose,", "tokens": ["Sich", "Kr\u00e4u\u00b7ter", "sucht", "und", "Moo\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "NN", "VVFIN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Bl\u00fcht unverdorrt im Frost noch fort", "tokens": ["Bl\u00fcht", "un\u00b7ver\u00b7dorrt", "im", "Frost", "noch", "fort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "APPRART", "NN", "ADV", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die wei\u00dfe Weihnachtsrose.", "tokens": ["Die", "wei\u00b7\u00dfe", "Weih\u00b7nachts\u00b7ro\u00b7se", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Kein Bl\u00fcmchen sonst auf weiter Flur;", "tokens": ["Kein", "Bl\u00fcm\u00b7chen", "sonst", "auf", "wei\u00b7ter", "Flur", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "APPR", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In ihrem Dornenkleid", "tokens": ["In", "ih\u00b7rem", "Dor\u00b7nen\u00b7kleid"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nur sie, die niedre Distel nur", "tokens": ["Nur", "sie", ",", "die", "nied\u00b7re", "Dis\u00b7tel", "nur"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "$,", "ART", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Trotzt allem Winterleid;", "tokens": ["Trotzt", "al\u00b7lem", "Win\u00b7ter\u00b7leid", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Das macht, sie will erwarten still,", "tokens": ["Das", "macht", ",", "sie", "will", "er\u00b7war\u00b7ten", "still", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "PPER", "VMFIN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bis sich die Sonne wendet,", "tokens": ["Bis", "sich", "die", "Son\u00b7ne", "wen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Damit sie wei\u00df, da\u00df Schnee und Eis", "tokens": ["Da\u00b7mit", "sie", "wei\u00df", ",", "da\u00df", "Schnee", "und", "Eis"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "KOUS", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Auch diesmal wieder endet.", "tokens": ["Auch", "dies\u00b7mal", "wie\u00b7der", "en\u00b7det", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Doch ist's geschehn, nimmt f\u00fchlbar kaum", "tokens": ["Doch", "ist's", "ge\u00b7schehn", ",", "nimmt", "f\u00fchl\u00b7bar", "kaum"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VAFIN", "VVPP", "$,", "VVFIN", "ADJD", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der N\u00e4chte Dunkel ab,", "tokens": ["Der", "N\u00e4ch\u00b7te", "Dun\u00b7kel", "ab", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dann sinkt mit einem Hoffnungstraum", "tokens": ["Dann", "sinkt", "mit", "ei\u00b7nem", "Hoff\u00b7nungs\u00b7traum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auch sie zur\u00fcck ins Grab.", "tokens": ["Auch", "sie", "zu\u00b7r\u00fcck", "ins", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PTKVZ", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nun schl\u00e4ft sie gern, sie hat von fern", "tokens": ["Nun", "schl\u00e4ft", "sie", "gern", ",", "sie", "hat", "von", "fern"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "PPER", "VAFIN", "APPR", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Des Fr\u00fchlings Gru\u00df vernommen,", "tokens": ["Des", "Fr\u00fch\u00b7lings", "Gru\u00df", "ver\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und o wie bald wird glanzumwallt", "tokens": ["Und", "o", "wie", "bald", "wird", "glan\u00b7zum\u00b7wallt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "FM", "KOKOM", "ADV", "VAFIN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Er sie zu wecken kommen!", "tokens": ["Er", "sie", "zu", "we\u00b7cken", "kom\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "PTKZU", "VVINF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}