{"textgrid.poem.51086": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "6", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Apfelbaum in voller Bl\u00fcte steht,", "tokens": ["Ein", "Ap\u00b7fel\u00b7baum", "in", "vol\u00b7ler", "Bl\u00fc\u00b7te", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein leichter West in seinen Zweigen weht;", "tokens": ["Ein", "leich\u00b7ter", "West", "in", "sei\u00b7nen", "Zwei\u00b7gen", "weht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Er schaut, verkl\u00e4rt vom blendendroten Schein,", "tokens": ["Er", "schaut", ",", "ver\u00b7kl\u00e4rt", "vom", "blen\u00b7den\u00b7dro\u00b7ten", "Schein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Verwundert in den wilden Brand hinein.", "tokens": ["Ver\u00b7wun\u00b7dert", "in", "den", "wil\u00b7den", "Brand", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Es ist, als ob der helle Glanz ihn freut',", "tokens": ["Es", "ist", ",", "als", "ob", "der", "hel\u00b7le", "Glanz", "ihn", "freut'", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOKOM", "KOUS", "ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Weil Bl\u00fctenbl\u00e4tter in die Glut er streut;", "tokens": ["Weil", "Bl\u00fc\u00b7ten\u00b7bl\u00e4t\u00b7ter", "in", "die", "Glut", "er", "streut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Er atmet ein des Feuers hei\u00dfen Hauch,", "tokens": ["Er", "at\u00b7met", "ein", "des", "Feu\u00b7ers", "hei\u00b7\u00dfen", "Hauch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Durch seine Krone zieht der schwarze Rauch.", "tokens": ["Durch", "sei\u00b7ne", "Kro\u00b7ne", "zieht", "der", "schwar\u00b7ze", "Rauch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Da pl\u00f6tzlich langt her\u00fcber aus dem Brand", "tokens": ["Da", "pl\u00f6tz\u00b7lich", "langt", "her\u00b7\u00fc\u00b7ber", "aus", "dem", "Brand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In seine \u00c4ste tief die Flammenhand:", "tokens": ["In", "sei\u00b7ne", "\u00c4s\u00b7te", "tief", "die", "Flam\u00b7men\u00b7hand", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zu Kohlen brennt der sch\u00f6ne Bl\u00fctenbaum \u2013", "tokens": ["Zu", "Koh\u00b7len", "brennt", "der", "sch\u00f6\u00b7ne", "Bl\u00fc\u00b7ten\u00b7baum", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Hin ist ein dichterlicher Lebenstraum!", "tokens": ["Hin", "ist", "ein", "dich\u00b7ter\u00b7li\u00b7cher", "Le\u00b7ben\u00b7straum", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}