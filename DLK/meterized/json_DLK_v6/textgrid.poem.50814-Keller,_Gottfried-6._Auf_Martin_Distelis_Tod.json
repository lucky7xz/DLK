{"textgrid.poem.50814": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "6. Auf Martin Distelis Tod", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie haben Ruh, die Kutten braun und schwarz,", "tokens": ["Sie", "ha\u00b7ben", "Ruh", ",", "die", "Kut\u00b7ten", "braun", "und", "schwarz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "$,", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Flatterm\u00e4use, Eulen, blauen Kr\u00f6pfe,", "tokens": ["Die", "Flat\u00b7ter\u00b7m\u00e4u\u00b7se", ",", "Eu\u00b7len", ",", "blau\u00b7en", "Kr\u00f6p\u00b7fe", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NE", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Spie\u00dfe, die Philister und die Z\u00f6pfe,", "tokens": ["Die", "Spie\u00b7\u00dfe", ",", "die", "Phi\u00b7lis\u00b7ter", "und", "die", "Z\u00f6p\u00b7fe", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "All das verbrannte, z\u00e4he Pech und Harz!", "tokens": ["All", "das", "ver\u00b7brann\u00b7te", ",", "z\u00e4\u00b7he", "Pech", "und", "Harz", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PDS", "VVFIN", "$,", "ADJA", "NN", "KON", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Er hat sie scharf gepeitscht und arg gegei\u00dfelt", "tokens": ["Er", "hat", "sie", "scharf", "ge\u00b7peitscht", "und", "arg", "ge\u00b7gei\u00b7\u00dfelt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "VVPP", "KON", "ADJD", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die faulen B\u00e4uche und die krummen R\u00fccken,", "tokens": ["Die", "fau\u00b7len", "B\u00e4u\u00b7che", "und", "die", "krum\u00b7men", "R\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Er hat aus tausend giftgeschwollnen M\u00fccken", "tokens": ["Er", "hat", "aus", "tau\u00b7send", "gift\u00b7ge\u00b7schwoll\u00b7nen", "M\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "CARD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sich gar ein seltsam Monument gemei\u00dfelt!", "tokens": ["Sich", "gar", "ein", "selt\u00b7sam", "Mo\u00b7nu\u00b7ment", "ge\u00b7mei\u00b7\u00dfelt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "ART", "ADJD", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Schaut her, ihr drau\u00dfen, denen im Genick", "tokens": ["Schaut", "her", ",", "ihr", "drau\u00b7\u00dfen", ",", "de\u00b7nen", "im", "Ge\u00b7nick"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PTKVZ", "$,", "PPER", "ADV", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Geharnischte Tyrannen t\u00f6dlich lasten,", "tokens": ["Ge\u00b7har\u00b7nischte", "Ty\u00b7ran\u00b7nen", "t\u00f6d\u00b7lich", "las\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Schaut dies Gewimmel ohne Ruh und Rasten,", "tokens": ["Schaut", "dies", "Ge\u00b7wim\u00b7mel", "oh\u00b7ne", "Ruh", "und", "Ras\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den Bodensatz in einer Republik!", "tokens": ["Den", "Bo\u00b7den\u00b7satz", "in", "ei\u00b7ner", "Re\u00b7pub\u00b7lik", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Solch einen Abschaum wohlgemut zu zeichnen,", "tokens": ["Solch", "ei\u00b7nen", "Ab\u00b7schaum", "wohl\u00b7ge\u00b7mut", "zu", "zeich\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "NN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Braucht es f\u00fcrwahr ein gutes, starkes Herz!", "tokens": ["Braucht", "es", "f\u00fcr\u00b7wahr", "ein", "gu\u00b7tes", ",", "star\u00b7kes", "Herz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ihm lohnt es auch des Vaterlandes Schmerz,", "tokens": ["Ihm", "lohnt", "es", "auch", "des", "Va\u00b7ter\u00b7lan\u00b7des", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und seinen Namen wird es dankbar eignen!", "tokens": ["Und", "sei\u00b7nen", "Na\u00b7men", "wird", "es", "dank\u00b7bar", "eig\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PPER", "ADJD", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}