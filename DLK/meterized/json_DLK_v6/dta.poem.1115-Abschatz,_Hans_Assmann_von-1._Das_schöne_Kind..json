{"dta.poem.1115": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "1. Das sch\u00f6ne Kind.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Unreiffer Sch\u00f6nheit Bl\u00fct und fr\u00fches Morgen-Licht/ ", "tokens": ["Un\u00b7reif\u00b7fer", "Sch\u00f6n\u00b7heit", "Bl\u00fct", "und", "fr\u00fc\u00b7hes", "Mor\u00b7gen\u00b7Licht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "NN", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit Tocken mehr gewohnt zu spielen als mit Liebe/", "tokens": ["Mit", "To\u00b7cken", "mehr", "ge\u00b7wohnt", "zu", "spie\u00b7len", "als", "mit", "Lie\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVPP", "PTKZU", "VVINF", "KOKOM", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wenn ich mich wegen dein in stetem Seufftzen \u00fcbe/", "tokens": ["Wenn", "ich", "mich", "we\u00b7gen", "dein", "in", "ste\u00b7tem", "Seufft\u00b7zen", "\u00fc\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "PPOSAT", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So lacht die tumme Welt und gl\u00e4ubt mein Leiden nicht.", "tokens": ["So", "lacht", "die", "tum\u00b7me", "Welt", "und", "gl\u00e4ubt", "mein", "Lei\u00b7den", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "KON", "VVFIN", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Doch/ wie im gr\u00fcnen schon die junge Rose sticht/", "tokens": ["Doch", "/", "wie", "im", "gr\u00fc\u00b7nen", "schon", "die", "jun\u00b7ge", "Ro\u00b7se", "sticht", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "KOKOM", "APPRART", "ADJA", "ADV", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wie man die Blume lobt aus ihrer Knospen Triebe;", "tokens": ["Wie", "man", "die", "Blu\u00b7me", "lobt", "aus", "ih\u00b7rer", "Knos\u00b7pen", "Trie\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So werd ich auch gewahr/ wie gern ich ruhig bliebe/", "tokens": ["So", "werd", "ich", "auch", "ge\u00b7wahr", "/", "wie", "gern", "ich", "ru\u00b7hig", "blie\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJD", "$(", "PWAV", "ADV", "PPER", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da\u00df/ wo die Flamm entsteht/ auch bald die Hitz ausbricht.", "tokens": ["Da\u00df", "/", "wo", "die", "Flamm", "ent\u00b7steht", "/", "auch", "bald", "die", "Hitz", "aus\u00b7bricht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$(", "PWAV", "ART", "NN", "VVFIN", "$(", "ADV", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Mein Kind/ ein neuer Stein schl\u00e4gt offt am ehsten Glutt:", "tokens": ["Mein", "Kind", "/", "ein", "neu\u00b7er", "Stein", "schl\u00e4gt", "offt", "am", "ehs\u00b7ten", "Glutt", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "ART", "ADJA", "NN", "VVFIN", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ein kleiner Funcke darff in frischen Zunder sincken", "tokens": ["Ein", "klei\u00b7ner", "Fun\u00b7cke", "darff", "in", "fri\u00b7schen", "Zun\u00b7der", "sin\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VMFIN", "APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "So f\u00e4ngt er/ wo man nicht bald Gegenwehre thut:", "tokens": ["So", "f\u00e4ngt", "er", "/", "wo", "man", "nicht", "bald", "Ge\u00b7gen\u00b7weh\u00b7re", "thut", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$(", "PWAV", "PIS", "PTKNEG", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Zur Fr\u00fchzeit siehet man die Venus heller blincken:", "tokens": ["Zur", "Fr\u00fch\u00b7zeit", "sie\u00b7het", "man", "die", "Ve\u00b7nus", "hel\u00b7ler", "blin\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PIS", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.13": {"text": "Den Mittags-Glantz besiegt der Morgenr\u00f6the Zier:", "tokens": ["Den", "Mit\u00b7tags\u00b7Glantz", "be\u00b7siegt", "der", "Mor\u00b7gen\u00b7r\u00f6\u00b7the", "Zier", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Dem heissen Sommer geht der bunte Lentz weit f\u00fcr.", "tokens": ["Dem", "heis\u00b7sen", "Som\u00b7mer", "geht", "der", "bun\u00b7te", "Lentz", "weit", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "ADJD", "APPR", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}