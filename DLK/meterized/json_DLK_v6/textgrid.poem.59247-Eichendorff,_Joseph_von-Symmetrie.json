{"textgrid.poem.59247": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Symmetrie", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Gegenwart, wie bist du schnelle,", "tokens": ["O", "Ge\u00b7gen\u00b7wart", ",", "wie", "bist", "du", "schnel\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWAV", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zukunft, wie bist du morgenhelle,", "tokens": ["Zu\u00b7kunft", ",", "wie", "bist", "du", "mor\u00b7gen\u00b7hel\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "VAFIN", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Vergangenheit so abendrot!", "tokens": ["Ver\u00b7gan\u00b7gen\u00b7heit", "so", "a\u00b7ben\u00b7drot", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Abendrot soll ewig stehen,", "tokens": ["Das", "A\u00b7ben\u00b7drot", "soll", "e\u00b7wig", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die Morgenhelle frisch dreinwehen,", "tokens": ["Die", "Mor\u00b7gen\u00b7hel\u00b7le", "frisch", "drein\u00b7we\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So ist die Gegenwart nicht tot.", "tokens": ["So", "ist", "die", "Ge\u00b7gen\u00b7wart", "nicht", "tot", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Tor, der lahmt auf einem Bein,", "tokens": ["Der", "Tor", ",", "der", "lahmt", "auf", "ei\u00b7nem", "Bein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das ist gar nicht zu leiden,", "tokens": ["Das", "ist", "gar", "nicht", "zu", "lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Schlagt ihm das andre Bein entzwei,", "tokens": ["Schlagt", "ihm", "das", "and\u00b7re", "Bein", "ent\u00b7zwei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So hinkt er doch auf beiden!", "tokens": ["So", "hinkt", "er", "doch", "auf", "bei\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "PIAT", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}