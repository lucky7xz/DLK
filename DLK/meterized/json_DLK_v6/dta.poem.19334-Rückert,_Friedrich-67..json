{"dta.poem.19334": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "67.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn du f\u00fcr dein Verdienst erwartest reichen Lohn;", "tokens": ["Wenn", "du", "f\u00fcr", "dein", "Ver\u00b7dienst", "er\u00b7war\u00b7test", "rei\u00b7chen", "Lohn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Such dich um Stadt und Land verdient zu machen, Sohn!", "tokens": ["Such", "dich", "um", "Stadt", "und", "Land", "ver\u00b7dient", "zu", "ma\u00b7chen", ",", "Sohn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "PRF", "APPR", "NN", "KON", "NN", "VVPP", "PTKZU", "VVINF", "$,", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Denn Wohlthat einzelnen, wie sch\u00f6n sie sei, erwiesen,", "tokens": ["Denn", "Wohlt\u00b7hat", "ein\u00b7zel\u00b7nen", ",", "wie", "sch\u00f6n", "sie", "sei", ",", "er\u00b7wie\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "NN", "VVINF", "$,", "PWAV", "ADJD", "PPER", "VAFIN", "$,", "VVPP", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Bescheiden bleibt sie still von einzelnen gepriesen.", "tokens": ["Be\u00b7schei\u00b7den", "bleibt", "sie", "still", "von", "ein\u00b7zel\u00b7nen", "ge\u00b7prie\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADJD", "APPR", "ADJA", "VVPP", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Nur voll Beseligung ein g\u00f6ttlich Hochgef\u00fchl", "tokens": ["Nur", "voll", "Be\u00b7se\u00b7li\u00b7gung", "ein", "g\u00f6tt\u00b7lich", "Hoch\u00b7ge\u00b7f\u00fchl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "NN", "ART", "ADJD", "NN"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Ist, ringsum danken sehn ein lautes Volksgef\u00fchl,", "tokens": ["Ist", ",", "ring\u00b7sum", "dan\u00b7ken", "sehn", "ein", "lau\u00b7tes", "Volks\u00b7ge\u00b7f\u00fchl", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "ADV", "VVINF", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ins Antlitz keinem schaun der Kinder, M\u00fctter, V\u00e4ter,", "tokens": ["Ins", "Ant\u00b7litz", "kei\u00b7nem", "schaun", "der", "Kin\u00b7der", ",", "M\u00fct\u00b7ter", ",", "V\u00e4\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "PIS", "VVFIN", "ART", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ohn' in der Brust sich selbst zu f\u00fchlen ihr Wohlth\u00e4ter.", "tokens": ["Ohn'", "in", "der", "Brust", "sich", "selbst", "zu", "f\u00fch\u00b7len", "ihr", "Wohlt\u00b7h\u00e4\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PRF", "ADV", "PTKZU", "VVINF", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Dagegen kommt nicht auf, wie gro\u00df es sei und echt,", "tokens": ["Da\u00b7ge\u00b7gen", "kommt", "nicht", "auf", ",", "wie", "gro\u00df", "es", "sei", "und", "echt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PTKNEG", "PTKVZ", "$,", "PWAV", "ADJD", "PPER", "VAFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sonst ein Verdienst um Welt und menschliches Geschlecht,", "tokens": ["Sonst", "ein", "Ver\u00b7dienst", "um", "Welt", "und", "menschli\u00b7ches", "Ge\u00b7schlecht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.6": {"line.1": {"text": "Das geistig-fein und zart, von unsichtbarer Art,", "tokens": ["Das", "geis\u00b7tig\u00b7fein", "und", "zart", ",", "von", "un\u00b7sicht\u00b7ba\u00b7rer", "Art", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ADJD", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Entbehret auf dem Markt lebend'ger Gegenwart.", "tokens": ["Ent\u00b7beh\u00b7ret", "auf", "dem", "Markt", "le\u00b7ben\u00b7d'\u00b7ger", "Ge\u00b7gen\u00b7wart", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}}}}