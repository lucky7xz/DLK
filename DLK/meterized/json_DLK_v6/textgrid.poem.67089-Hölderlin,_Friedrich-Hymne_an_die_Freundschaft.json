{"textgrid.poem.67089": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Hymne an die Freundschaft", "genre": "verse", "period": "N.A.", "pub_year": 1792, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Rings in schwesterlicher Stille", "tokens": ["Rings", "in", "schwes\u00b7ter\u00b7li\u00b7cher", "Stil\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lauscht die bl\u00fchende Natur;", "tokens": ["Lauscht", "die", "bl\u00fc\u00b7hen\u00b7de", "Na\u00b7tur", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aus des k\u00fchnen Herzens F\u00fclle", "tokens": ["Aus", "des", "k\u00fch\u00b7nen", "Her\u00b7zens", "F\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "T\u00f6nt des Bundes Stimme nur;", "tokens": ["T\u00f6nt", "des", "Bun\u00b7des", "Stim\u00b7me", "nur", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Leise rauschts im Eichenhaine,", "tokens": ["Lei\u00b7se", "rauschts", "im", "Ei\u00b7chen\u00b7hai\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nie gef\u00fchlte L\u00fcfte wehn,", "tokens": ["Nie", "ge\u00b7f\u00fchl\u00b7te", "L\u00fcf\u00b7te", "wehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wo in h\u00f6hrem Sternenscheine", "tokens": ["Wo", "in", "h\u00f6h\u00b7rem", "Ster\u00b7nen\u00b7schei\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wir das ernste Fest begehn.", "tokens": ["Wir", "das", "erns\u00b7te", "Fest", "be\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ha! in s\u00fc\u00dfem Wohlgefallen", "tokens": ["Ha", "!", "in", "s\u00fc\u00b7\u00dfem", "Wohl\u00b7ge\u00b7fal\u00b7len"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ITJ", "$.", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "S\u00e4uselt hier der V\u00e4ter Schar,", "tokens": ["S\u00e4u\u00b7selt", "hier", "der", "V\u00e4\u00b7ter", "Schar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Abgeschiedne Freunde wallen", "tokens": ["Ab\u00b7ge\u00b7schied\u00b7ne", "Freun\u00b7de", "wal\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "L\u00e4chelnd um den Moosaltar;", "tokens": ["L\u00e4\u00b7chelnd", "um", "den", "Moo\u00b7sal\u00b7tar", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und der hellen Tyndariden", "tokens": ["Und", "der", "hel\u00b7len", "Tyn\u00b7da\u00b7ri\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Br\u00fcderliches Auge lacht", "tokens": ["Br\u00fc\u00b7der\u00b7li\u00b7ches", "Au\u00b7ge", "lacht"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Froh wie wir in deinem Frieden,", "tokens": ["Froh", "wie", "wir", "in", "dei\u00b7nem", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sch\u00f6ne feierliche Nacht!", "tokens": ["Sch\u00f6\u00b7ne", "fei\u00b7er\u00b7li\u00b7che", "Nacht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Heiliger und reiner t\u00f6nte", "tokens": ["Hei\u00b7li\u00b7ger", "und", "rei\u00b7ner", "t\u00f6n\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dieser Herzen Jubel nie,", "tokens": ["Die\u00b7ser", "Her\u00b7zen", "Ju\u00b7bel", "nie", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Unter Schwur und Ku\u00df versch\u00f6nte,", "tokens": ["Un\u00b7ter", "Schwur", "und", "Ku\u00df", "ver\u00b7sch\u00f6n\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Freundschaft! deine Milde sie;", "tokens": ["Freund\u00b7schaft", "!", "dei\u00b7ne", "Mil\u00b7de", "sie", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPOSAT", "NN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Z\u00fcrne nicht der Wonne Z\u00e4hren!", "tokens": ["Z\u00fcr\u00b7ne", "nicht", "der", "Won\u00b7ne", "Z\u00e4h\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "La\u00df, o la\u00df uns huldigen,", "tokens": ["La\u00df", ",", "o", "la\u00df", "uns", "hul\u00b7di\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "FM", "VVIMP", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Sch\u00f6nste von Olympos Heeren,", "tokens": ["Sch\u00f6ns\u00b7te", "von", "O\u00b7lym\u00b7pos", "Hee\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Krone der Unsterblichen!", "tokens": ["Kro\u00b7ne", "der", "U\u00b7nsterb\u00b7li\u00b7chen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+---", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Als der Geister Wunsch gelungen,", "tokens": ["Als", "der", "Geis\u00b7ter", "Wunsch", "ge\u00b7lun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und gereift die Stunde war,", "tokens": ["Und", "ge\u00b7reift", "die", "Stun\u00b7de", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da, von Ares Arm umschlungen,", "tokens": ["Da", ",", "von", "A\u00b7res", "Arm", "um\u00b7schlun\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Cytherea dich gebar,", "tokens": ["Cy\u00b7the\u00b7rea", "dich", "ge\u00b7bar", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADJD", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Als die Heldin ohne Tadel", "tokens": ["Als", "die", "Hel\u00b7din", "oh\u00b7ne", "Ta\u00b7del"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nun der Erde Sohn so nah", "tokens": ["Nun", "der", "Er\u00b7de", "Sohn", "so", "nah"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "NN", "ADV", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Staunend in des Vaters Adel,", "tokens": ["Stau\u00b7nend", "in", "des", "Va\u00b7ters", "A\u00b7del", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "In der Mutter G\u00fcrtel sah,", "tokens": ["In", "der", "Mut\u00b7ter", "G\u00fcr\u00b7tel", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Da begann zu Sonnenh\u00f6hen", "tokens": ["Da", "be\u00b7gann", "zu", "Son\u00b7nen\u00b7h\u00f6\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nie versuchten Adlerflug,", "tokens": ["Nie", "ver\u00b7such\u00b7ten", "Ad\u00b7ler\u00b7flug", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was von G\u00f6ttern ausersehen", "tokens": ["Was", "von", "G\u00f6t\u00b7tern", "au\u00b7ser\u00b7se\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "APPR", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kraft und Lieb im Busen trug;", "tokens": ["Kraft", "und", "Lieb", "im", "Bu\u00b7sen", "trug", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Stolzer hub des Sieges Fl\u00fcgel,", "tokens": ["Stol\u00b7zer", "hub", "des", "Sie\u00b7ges", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Rosiger der Friede sich;", "tokens": ["Ro\u00b7si\u00b7ger", "der", "Frie\u00b7de", "sich", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PRF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Jauchzend um die Blumenh\u00fcgel", "tokens": ["Jauch\u00b7zend", "um", "die", "Blu\u00b7men\u00b7h\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Gr\u00fc\u00dfte Gram und Sorge dich.", "tokens": ["Gr\u00fc\u00df\u00b7te", "Gram", "und", "Sor\u00b7ge", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Blutend trug die Siegesfahne,", "tokens": ["Blu\u00b7tend", "trug", "die", "Sie\u00b7ges\u00b7fah\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In der St\u00fcrme Donner schwamm", "tokens": ["In", "der", "St\u00fcr\u00b7me", "Don\u00b7ner", "schwamm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Durch die wilden Ozeane,", "tokens": ["Durch", "die", "wil\u00b7den", "O\u00b7zea\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Wer aus deinem Scho\u00dfe kam;", "tokens": ["Wer", "aus", "dei\u00b7nem", "Scho\u00b7\u00dfe", "kam", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Deiner Riesen Wehre klangen", "tokens": ["Dei\u00b7ner", "Rie\u00b7sen", "Weh\u00b7re", "klan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bis hinab zur alten Nacht \u2013", "tokens": ["Bis", "hin\u00b7ab", "zur", "al\u00b7ten", "Nacht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "APPRART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ha! des Orkus Tore sprangen,", "tokens": ["Ha", "!", "des", "Or\u00b7kus", "To\u00b7re", "spran\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ART", "NN", "NE", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Zitternd deiner Zaubermacht!", "tokens": ["Zit\u00b7ternd", "dei\u00b7ner", "Zau\u00b7ber\u00b7macht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Trunken, wie von Hebes Schale,", "tokens": ["Trun\u00b7ken", ",", "wie", "von", "He\u00b7bes", "Scha\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "APPR", "NE", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kos'ten sie in s\u00fc\u00dfer Rast", "tokens": ["Kos'\u00b7ten", "sie", "in", "s\u00fc\u00b7\u00dfer", "Rast"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Am ersehnten Opfermahle", "tokens": ["Am", "er\u00b7sehn\u00b7ten", "Op\u00b7fer\u00b7mah\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nach der schw\u00fclen Tage Last;", "tokens": ["Nach", "der", "schw\u00fc\u00b7len", "Ta\u00b7ge", "Last", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "G\u00f6ttern glich der Freunde R\u00e4cher,", "tokens": ["G\u00f6t\u00b7tern", "glich", "der", "Freun\u00b7de", "R\u00e4\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wenn die stolze Z\u00e4hre sank", "tokens": ["Wenn", "die", "stol\u00b7ze", "Z\u00e4h\u00b7re", "sank"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "In den vollen Labebecher,", "tokens": ["In", "den", "vol\u00b7len", "La\u00b7be\u00b7be\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Den er seinem Siege trank.", "tokens": ["Den", "er", "sei\u00b7nem", "Sie\u00b7ge", "trank", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Liebend stieg die Muse nieder,", "tokens": ["Lie\u00b7bend", "stieg", "die", "Mu\u00b7se", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Als sie in Arkadia", "tokens": ["Als", "sie", "in", "Ar\u00b7ka\u00b7dia"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dich im g\u00f6ttlichen Gefieder", "tokens": ["Dich", "im", "g\u00f6tt\u00b7li\u00b7chen", "Ge\u00b7fie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schwebend um die Sch\u00e4fer sah;", "tokens": ["Schwe\u00b7bend", "um", "die", "Sch\u00e4\u00b7fer", "sah", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mutter! Herz und Lippe brannten,", "tokens": ["Mut\u00b7ter", "!", "Herz", "und", "Lip\u00b7pe", "brann\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Feierten im Liede dich,", "tokens": ["Fei\u00b7er\u00b7ten", "im", "Lie\u00b7de", "dich", ","], "token_info": ["word", "word", 