{"textgrid.poem.24526": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was jener K\u00e4yser nechst aus Eyfersucht gethan/", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was jener K\u00e4yser nechst aus Eyfersucht gethan/", "tokens": ["Was", "je\u00b7ner", "K\u00e4y\u00b7ser", "nechst", "aus", "Ey\u00b7fer\u00b7sucht", "ge\u00b7than", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDAT", "NN", "ADV", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df er so gut nicht hat als wie ", "tokens": ["Da\u00df", "er", "so", "gut", "nicht", "hat", "als", "wie"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "PTKNEG", "VAFIN", "KOUS", "KOKOM"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Beweget mich zwar nicht ein gleiches auszu\u00fcben;", "tokens": ["Be\u00b7we\u00b7get", "mich", "zwar", "nicht", "ein", "glei\u00b7ches", "aus\u00b7zu\u00b7\u00fc\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKNEG", "ART", "ADJA", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Doch feuret etwas mich zu gleichen Eyfer an/", "tokens": ["Doch", "feu\u00b7ret", "et\u00b7was", "mich", "zu", "glei\u00b7chen", "Ey\u00b7fer", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "PRF", "APPR", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dich nicht allein mein Freund den Nahmen nach zu nennen/", "tokens": ["Dich", "nicht", "al\u00b7lein", "mein", "Freund", "den", "Nah\u00b7men", "nach", "zu", "nen\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "ADV", "PPOSAT", "NN", "ART", "NN", "APPR", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Nur dich wie deine Schrifft auch in der That zu kennen.", "tokens": ["Nur", "dich", "wie", "dei\u00b7ne", "Schrifft", "auch", "in", "der", "That", "zu", "ken\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "KOKOM", "PPOSAT", "NN", "ADV", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Da\u00df nunmehr Flamm' und Glut ", "tokens": ["Da\u00df", "nun\u00b7mehr", "Flamm'", "und", "Glut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Scheint meinen Sinnen nichts als Fabel-Werck zu seyn/", "tokens": ["Scheint", "mei\u00b7nen", "Sin\u00b7nen", "nichts", "als", "Fa\u00b7bel\u00b7\u00b7Werck", "zu", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PIS", "KOKOM", "NN", "PTKZU", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da dein ge\u00fcbter Kiel in mehr als Ertz und Stein", "tokens": ["Da", "dein", "ge\u00b7\u00fcb\u00b7ter", "Kiel", "in", "mehr", "als", "Ertz", "und", "Stein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "APPR", "PIAT", "KOKOM", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Verwunders-w\u00fcrdig schreitb/ und alle Tichter lehret/", "tokens": ["Ver\u00b7wun\u00b7der\u00b7sw\u00fcr\u00b7dig", "schreitb", "/", "und", "al\u00b7le", "Tich\u00b7ter", "leh\u00b7ret", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$(", "KON", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Da\u00df zwischen deiner Kunst und des ", "tokens": ["Da\u00df", "zwi\u00b7schen", "dei\u00b7ner", "Kunst", "und", "des"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPOSAT", "NN", "KON", "ART"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.6": {"text": "Man einen Unterscheid im Nahmen nur antrifft.", "tokens": ["Man", "ei\u00b7nen", "Un\u00b7ter\u00b7scheid", "im", "Nah\u00b7men", "nur", "an\u00b7trifft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "NN", "APPRART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}