{"dta.poem.20023": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Rath im Mai .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wo Saaten sich erheben,", "tokens": ["Wo", "Saa\u00b7ten", "sich", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wo froh die V\u00f6gel schweben", "tokens": ["Wo", "froh", "die", "V\u00f6\u00b7gel", "schwe\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mit Singen himmelw\u00e4rts,", "tokens": ["Mit", "Sin\u00b7gen", "him\u00b7mel\u00b7w\u00e4rts", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "$,"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.4": {"text": "In linden Maientagen,", "tokens": ["In", "lin\u00b7den", "Mai\u00b7en\u00b7ta\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Kannst du nicht ruhig schlagen,", "tokens": ["Kannst", "du", "nicht", "ru\u00b7hig", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Du krankes, krankes Herz?", "tokens": ["Du", "kran\u00b7kes", ",", "kran\u00b7kes", "Herz", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Geh' aus auf gr\u00fcner Haide,", "tokens": ["Geh'", "aus", "auf", "gr\u00fc\u00b7ner", "Hai\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wo's Bl\u00fcmlein bl\u00fcht voll Freude,", "tokens": ["Wo's", "Bl\u00fcm\u00b7lein", "bl\u00fcht", "voll", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "ADJD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In Duft, Gesang und Strahl;", "tokens": ["In", "Duft", ",", "Ge\u00b7sang", "und", "Strahl", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Leg' dich zu ihm darnieder,", "tokens": ["Leg'", "dich", "zu", "ihm", "dar\u00b7nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPER", "PAV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Duft, Himmelsglanz und Lieder,", "tokens": ["Duft", ",", "Him\u00b7mels\u00b7glanz", "und", "Lie\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die heilen deine Qual.", "tokens": ["Die", "hei\u00b7len", "dei\u00b7ne", "Qual", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "La\u00df ganz der Menschen Streben,", "tokens": ["La\u00df", "ganz", "der", "Men\u00b7schen", "Stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sey wieder frei gegeben", "tokens": ["Sey", "wie\u00b7der", "frei", "ge\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ADJD", "VVPP"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Der alten Einsamkeit!", "tokens": ["Der", "al\u00b7ten", "Ein\u00b7sam\u00b7keit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wie Vogel singt in L\u00fcften,", "tokens": ["Wie", "Vo\u00b7gel", "singt", "in", "L\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ausstr\u00f6mt die Blum' in D\u00fcften,", "tokens": ["Aus\u00b7str\u00f6mt", "die", "Blum'", "in", "D\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Str\u00f6mt aus, o Herz! dein Leid.", "tokens": ["Str\u00f6mt", "aus", ",", "o", "Herz", "!", "dein", "Leid", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "FM", "NN", "$.", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Dann kehre sonder Trauern", "tokens": ["Dann", "keh\u00b7re", "son\u00b7der", "Trau\u00b7ern"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In armer St\u00e4dte Mauern:", "tokens": ["In", "ar\u00b7mer", "St\u00e4d\u00b7te", "Mau\u00b7ern", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es kehret ohne Weh", "tokens": ["Es", "keh\u00b7ret", "oh\u00b7ne", "Weh"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die Blum' in's Erdreich wieder,", "tokens": ["Die", "Blum'", "in's", "Er\u00b7dreich", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Tr\u00e4umt Sonnenschein und Lieder", "tokens": ["Tr\u00e4umt", "Son\u00b7nen\u00b7schein", "und", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Tief unter Eis und Schnee.", "tokens": ["Tief", "un\u00b7ter", "Eis", "und", "Schnee", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}