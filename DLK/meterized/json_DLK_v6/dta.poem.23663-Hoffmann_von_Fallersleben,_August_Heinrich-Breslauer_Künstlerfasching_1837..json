{"dta.poem.23663": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Breslauer K\u00fcnstlerfasching 1837.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hoch lebe die Fastnacht!", "tokens": ["Hoch", "le\u00b7be", "die", "Fast\u00b7nacht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wo wir fasten und rasten", "tokens": ["Wo", "wir", "fas\u00b7ten", "und", "ras\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "VVFIN", "KON", "VVFIN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Um des Lebens Lasten,", "tokens": ["Um", "des", "Le\u00b7bens", "Las\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Und uns gew\u00f6hnen zu fr\u00f6hnen", "tokens": ["Und", "uns", "ge\u00b7w\u00f6h\u00b7nen", "zu", "fr\u00f6h\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVINF", "PTKZU", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Allem Sch\u00f6nen,", "tokens": ["Al\u00b7lem", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Wo wir anstecken", "tokens": ["Wo", "wir", "an\u00b7ste\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "PPER", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Die Kerzen unsrer Herzen,", "tokens": ["Die", "Ker\u00b7zen", "uns\u00b7rer", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und wie Gecken", "tokens": ["Und", "wie", "Ge\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["KON", "PWAV", "NN"], "meter": "--+-", "measure": "anapaest.init"}, "line.9": {"text": "Uns selbst zum Besten haben", "tokens": ["Uns", "selbst", "zum", "Bes\u00b7ten", "ha\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "APPRART", "NN", "VAFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Und mit heitern G\u00e4sten laben,", "tokens": ["Und", "mit", "hei\u00b7tern", "G\u00e4s\u00b7ten", "la\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Nach Fr\u00f6hlichkeit trachten und dichten", "tokens": ["Nach", "Fr\u00f6h\u00b7lich\u00b7keit", "trach\u00b7ten", "und", "dich\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVINF", "KON", "ADJA"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.12": {"text": "Und unsre Gedanken richten", "tokens": ["Und", "uns\u00b7re", "Ge\u00b7dan\u00b7ken", "rich\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.13": {"text": "Eher auf den besten Keller", "tokens": ["E\u00b7her", "auf", "den", "bes\u00b7ten", "Kel\u00b7ler"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Als auf den letzten Heller \u2014", "tokens": ["Als", "auf", "den", "letz\u00b7ten", "Hel\u00b7ler"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Es lebe ", "tokens": ["Es", "le\u00b7be"], "token_info": ["word", "word"], "pos": ["PPER", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.16": {"text": "Die keinem Last macht,", "tokens": ["Die", "kei\u00b7nem", "Last", "macht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.17": {"text": "Wo Wirth und Gast lacht", "tokens": ["Wo", "Wirth", "und", "Gast", "lacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.18": {"text": "Und ohne Rast wacht", "tokens": ["Und", "oh\u00b7ne", "Rast", "wacht"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "VVFIN"], "meter": "-+-++", "measure": "unknown.measure.tri"}, "line.19": {"text": "Bis an den Morgen", "tokens": ["Bis", "an", "den", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "APPR", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.20": {"text": "Abzuwerfen der Sorgen", "tokens": ["Ab\u00b7zu\u00b7wer\u00b7fen", "der", "Sor\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.21": {"text": "Ballast-Fracht", "tokens": ["Bal\u00b7last\u00b7Fracht"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.22": {"text": "Und was das Leben verhasst macht \u2014", "tokens": ["Und", "was", "das", "Le\u00b7ben", "ver\u00b7hasst", "macht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "VVFIN", "VVFIN", "$("], "meter": "-+-+-+++", "measure": "unknown.measure.penta"}, "line.23": {"text": "Hoch lebe die Fastnacht!", "tokens": ["Hoch", "le\u00b7be", "die", "Fast\u00b7nacht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}