{"textgrid.poem.67457": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Die unbekannte Blume", "genre": "verse", "period": "N.A.", "pub_year": 1793, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbunansehnliches Bl\u00fcmchen, wer bist Du?\u00ab \u00bbDu kennest mich also", "tokens": ["\u00bb", "un\u00b7an\u00b7sehn\u00b7li\u00b7ches", "Bl\u00fcm\u00b7chen", ",", "wer", "bist", "Du", "?", "\u00ab", "\u00bb", "Du", "ken\u00b7nest", "mich", "al\u00b7so"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "punct", "punct", "word", "word", "word", "word"], "pos": ["$(", "ADJA", "NN", "$,", "PWS", "VAFIN", "PPER", "$.", "$(", "$(", "PPER", "VVFIN", "PPER", "ADV"], "meter": "+-+--+-+-+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.2": {"text": "Nicht? und so hast Du mich denn auch nimmer vermi\u00dft?", "tokens": ["Nicht", "?", "und", "so", "hast", "Du", "mich", "denn", "auch", "nim\u00b7mer", "ver\u00b7mi\u00dft", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$.", "KON", "ADV", "VAFIN", "PPER", "PRF", "ADV", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-++-+--+", "measure": "iambic.hexa.chol"}, "line.3": {"text": "Myrten sich meiner, doch bl\u00fch' ich zur Erquickung ihm nah.", "tokens": ["Myr\u00b7ten", "sich", "mei\u00b7ner", ",", "doch", "bl\u00fch'", "ich", "zur", "Er\u00b7quic\u00b7kung", "ihm", "nah", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "PPOSAT", "$,", "ADV", "VVFIN", "PPER", "APPRART", "NN", "PPER", "ADJD", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.4": {"text": "Kr\u00e4nze Dich heut mit mir! Dem Herzen nahe, verwandeln", "tokens": ["Kr\u00e4n\u00b7ze", "Dich", "heut", "mit", "mir", "!", "Dem", "Her\u00b7zen", "na\u00b7he", ",", "ver\u00b7wan\u00b7deln"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["NN", "PPER", "ADV", "APPR", "PPER", "$.", "ART", "NN", "PTKVZ", "$,", "VVINF"], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}, "line.5": {"text": "Sich die Blumen und sind, was Du sie w\u00fcnschest zu sein.\u00ab", "tokens": ["Sich", "die", "Blu\u00b7men", "und", "sind", ",", "was", "Du", "sie", "w\u00fcn\u00b7schest", "zu", "sein", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PRF", "ART", "NN", "KON", "VAFIN", "$,", "PWS", "PPER", "PPER", "VVFIN", "PTKZU", "VAINF", "$.", "$("], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}}}}}