{"textgrid.poem.63058": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Geliebte,", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als ich mich heute Nacht in den Kissen richtete,", "tokens": ["Als", "ich", "mich", "heu\u00b7te", "Nacht", "in", "den", "Kis\u00b7sen", "rich\u00b7te\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Traf mich dein Atem wie das S\u00e4gen des Totenwurms,", "tokens": ["Traf", "mich", "dein", "A\u00b7tem", "wie", "das", "S\u00e4\u00b7gen", "des", "To\u00b7ten\u00b7wurms", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "KOKOM", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Der sich mit dem Surren des Sturms", "tokens": ["Der", "sich", "mit", "dem", "Sur\u00b7ren", "des", "Sturms"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PRF", "APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Drau\u00dfen \u2013 zu dumpfer Symphonie verdichtete.", "tokens": ["Drau\u00b7\u00dfen", "\u2013", "zu", "dum\u00b7pfer", "Sym\u00b7pho\u00b7nie", "ver\u00b7dich\u00b7te\u00b7te", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.2": {"line.1": {"text": "Auf der Stra\u00dfe klang es wie ein Getreck", "tokens": ["Auf", "der", "Stra\u00b7\u00dfe", "klang", "es", "wie", "ein", "Ge\u00b7treck"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "KOKOM", "ART", "NN"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Von Wagen zu einem Leichenzug...", "tokens": ["Von", "Wa\u00b7gen", "zu", "ei\u00b7nem", "Lei\u00b7chen\u00b7zug", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Das Haustor knirschte... im Zimmer war Totenruch...", "tokens": ["Das", "Haus\u00b7tor", "knirschte", "...", "im", "Zim\u00b7mer", "war", "To\u00b7ten\u00b7ruch", "..."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$(", "APPRART", "NN", "VAFIN", "NN", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Sie wollten den Sarg auf ihre Schultern heben...", "tokens": ["Sie", "woll\u00b7ten", "den", "Sarg", "auf", "ih\u00b7re", "Schul\u00b7tern", "he\u00b7ben", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Da ri\u00df ich dich an mein lebendiges Leben", "tokens": ["Da", "ri\u00df", "ich", "dich", "an", "mein", "le\u00b7ben\u00b7di\u00b7ges", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Und k\u00fc\u00dfte den Tod dir von den Lippen weg.", "tokens": ["Und", "k\u00fc\u00df\u00b7te", "den", "Tod", "dir", "von", "den", "Lip\u00b7pen", "weg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}}}}