{"dta.poem.11006": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Kl\u00e4gliche Unwissenheit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Vom Sch\u00f6pfer reder man so wenig im Christenthum, da\u00df\nes fast scheinet,", "tokens": ["Vom", "Sch\u00f6p\u00b7fer", "re\u00b7der", "man", "so", "we\u00b7nig", "im", "Chris\u00b7ten\u00b7thum", ",", "da\u00df", "es", "fast", "schei\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PIS", "ADV", "ADV", "APPRART", "NN", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+--+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Wenn man von Gottes Werken spricht, da\u00df man sich wundert,", "tokens": ["Wenn", "man", "von", "Got\u00b7tes", "Wer\u00b7ken", "spricht", ",", "da\u00df", "man", "sich", "wun\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "NN", "NN", "VVFIN", "$,", "KOUS", "PIS", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Als ob im Sch\u00f6pfer aller Ding, und Herrn der Schaaren, Ze-", "tokens": ["Als", "ob", "im", "Sch\u00f6p\u00b7fer", "al\u00b7ler", "Ding", ",", "und", "Herrn", "der", "Schaa\u00b7ren", ",", "Ze"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "KOUS", "APPRART", "NN", "PIAT", "NN", "$,", "KON", "NN", "ART", "NN", "$,", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Man ihnen etwas predigte, von einem unbekannten Gott.", "tokens": ["Man", "ih\u00b7nen", "et\u00b7was", "pre\u00b7dig\u00b7te", ",", "von", "ei\u00b7nem", "un\u00b7be\u00b7kann\u00b7ten", "Gott", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PPER", "PIS", "VVFIN", "$,", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}}}}}