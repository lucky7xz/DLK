{"textgrid.poem.25907": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der alte Mond, der geht", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der alte Mond, der geht", "tokens": ["Der", "al\u00b7te", "Mond", ",", "der", "geht"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Durch den Flu\u00df, den dunkeln.", "tokens": ["Durch", "den", "Flu\u00df", ",", "den", "dun\u00b7keln", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ART", "ADJA", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Siehst du's wie Ketten funkeln,", "tokens": ["Siehst", "du's", "wie", "Ket\u00b7ten", "fun\u00b7keln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "KOKOM", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wo er im Wasser steht?", "tokens": ["Wo", "er", "im", "Was\u00b7ser", "steht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "\u00dcber die Steinbr\u00fccke jagen", "tokens": ["\u00dc\u00b7ber", "die", "Stein\u00b7br\u00fc\u00b7cke", "ja\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVINF"], "meter": "+--++-+-", "measure": "dactylic.init"}, "line.2": {"text": "Rosse, eisenbeschlagen,", "tokens": ["Ros\u00b7se", ",", "ei\u00b7sen\u00b7be\u00b7schla\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Und voll Lichter ein Wagen, \u2013", "tokens": ["Und", "voll", "Lich\u00b7ter", "ein", "Wa\u00b7gen", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADJD", "NN", "ART", "NN", "$,", "$("], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Dort wird meine Freud fortgetragen.", "tokens": ["Dort", "wird", "mei\u00b7ne", "Freud", "fort\u00b7ge\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+--++-+-", "measure": "iambic.tetra.relaxed"}}}}}