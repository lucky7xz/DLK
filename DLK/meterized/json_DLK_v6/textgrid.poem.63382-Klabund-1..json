{"textgrid.poem.63382": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Regen rinnt schon tausend Jahr,", "tokens": ["Der", "Re\u00b7gen", "rinnt", "schon", "tau\u00b7send", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "CARD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die H\u00e4user sind voll Wasserspinnen,", "tokens": ["Die", "H\u00e4u\u00b7ser", "sind", "voll", "Was\u00b7ser\u00b7spin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Seekrebse nisten mir im Haar", "tokens": ["See\u00b7kreb\u00b7se", "nis\u00b7ten", "mir", "im", "Haar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "APPRART", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Und Austern auf des Domes Zinnen.", "tokens": ["Und", "Aus\u00b7tern", "auf", "des", "Do\u00b7mes", "Zin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Pfaff hier wurde eine Qualle,", "tokens": ["Der", "Pfaff", "hier", "wur\u00b7de", "ei\u00b7ne", "Qual\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "ADV", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Seepferdchen meine Nachbarin.", "tokens": ["See\u00b7pferd\u00b7chen", "mei\u00b7ne", "Nach\u00b7ba\u00b7rin", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Der blonde Seestern streckt mir alle", "tokens": ["Der", "blon\u00b7de", "Sees\u00b7tern", "streckt", "mir", "al\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "PIAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcnfhundert F\u00fchler z\u00e4rtlich hin.", "tokens": ["F\u00fcnf\u00b7hun\u00b7dert", "F\u00fch\u00b7ler", "z\u00e4rt\u00b7lich", "hin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Es ist so dunkel, kalt und feucht.", "tokens": ["Es", "ist", "so", "dun\u00b7kel", ",", "kalt", "und", "feucht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Wasser hat uns schon begraben.", "tokens": ["Das", "Was\u00b7ser", "hat", "uns", "schon", "be\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Gib deinen warmen Mund \u2013 mich deucht,", "tokens": ["Gib", "dei\u00b7nen", "war\u00b7men", "Mund", "\u2013", "mich", "deucht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "ADJA", "NN", "$(", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nichts bleibt uns als uns lieb zu haben.", "tokens": ["Nichts", "bleibt", "uns", "als", "uns", "lieb", "zu", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "KOUS", "PPER", "ADJD", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}