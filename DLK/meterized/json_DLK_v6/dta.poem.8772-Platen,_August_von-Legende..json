{"dta.poem.8772": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Legende.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Der benedeiten Himmelsbraut,", "tokens": ["Der", "be\u00b7ne\u00b7dei\u00b7ten", "Him\u00b7mels\u00b7braut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Die aller Welt zu Heil und Lohn", "tokens": ["Die", "al\u00b7ler", "Welt", "zu", "Heil", "und", "Lohn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Geboren den erlauchten Sohn.", "tokens": ["Ge\u00b7bo\u00b7ren", "den", "er\u00b7lauch\u00b7ten", "Sohn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie mauerten so manches Jahr,", "tokens": ["Sie", "mau\u00b7er\u00b7ten", "so", "man\u00b7ches", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Bis Dach und Decke fertig war;", "tokens": ["Bis", "Dach", "und", "De\u00b7cke", "fer\u00b7tig", "war", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Maler kam sodann herbey,", "tokens": ["Ein", "Ma\u00b7ler", "kam", "so\u00b7dann", "her\u00b7bey", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Zu bilden eine Schilderey:", "tokens": ["Zu", "bil\u00b7den", "ei\u00b7ne", "Schil\u00b7de\u00b7rey", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Auf m\u00e4chtigem Ger\u00fcst er stand,", "tokens": ["Auf", "m\u00e4ch\u00b7ti\u00b7gem", "Ge\u00b7r\u00fcst", "er", "stand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Den frommen Pinsel in der Hand,", "tokens": ["Den", "from\u00b7men", "Pin\u00b7sel", "in", "der", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Lebendig schaffend und genau", "tokens": ["Le\u00b7ben\u00b7dig", "schaf\u00b7fend", "und", "ge\u00b7nau"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "ADJD", "KON", "ADJD"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.11": {"text": "Das Angesicht der lieben Frau.", "tokens": ["Das", "An\u00b7ge\u00b7sicht", "der", "lie\u00b7ben", "Frau", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Doch als er fast am Ende war,", "tokens": ["Doch", "als", "er", "fast", "am", "En\u00b7de", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "APPRART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Bringt ihm ein falscher Tritt Gefahr,", "tokens": ["Bringt", "ihm", "ein", "fal\u00b7scher", "Tritt", "Ge\u00b7fahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Und vom Ger\u00fcste st\u00fcrzt er jach,", "tokens": ["Und", "vom", "Ge\u00b7r\u00fcs\u00b7te", "st\u00fcrzt", "er", "jach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Das unter ihm zusammenbrach.", "tokens": ["Das", "un\u00b7ter", "ihm", "zu\u00b7sam\u00b7men\u00b7brach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Da ruft er an, aus banger Brust,", "tokens": ["Da", "ruft", "er", "an", ",", "aus", "ban\u00b7ger", "Brust", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Das Bild, das er vollendet just:", "tokens": ["Das", "Bild", ",", "das", "er", "voll\u00b7en\u00b7det", "just", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Dir wandt' ich all mein Leben zu,", "tokens": ["Dir", "wandt'", "ich", "all", "mein", "Le\u00b7ben", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "O Himmlische, nun rette du!", "tokens": ["O", "Himm\u00b7li\u00b7sche", ",", "nun", "ret\u00b7te", "du", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADV", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Und sieh! Es fa\u00dft es kein Verstand,", "tokens": ["Und", "sieh", "!", "Es", "fa\u00dft", "es", "kein", "Ver\u00b7stand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "$.", "PPER", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Die Heil'ge streckt herab die Hand,", "tokens": ["Die", "Heil'\u00b7ge", "streckt", "her\u00b7ab", "die", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Und hielt so lang ihn wunderbar,", "tokens": ["Und", "hielt", "so", "lang", "ihn", "wun\u00b7der\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Bis Menschenh\u00fclf' erschienen war.", "tokens": ["Bis", "Men\u00b7schen\u00b7h\u00fcl\u00b7f'", "er\u00b7schie\u00b7nen", "war", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "VAFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}}}}