{"dta.poem.2177": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Meynung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was ist, das ist: nicht was es ist. F\u00fcr uns ist", "tokens": ["Was", "ist", ",", "das", "ist", ":", "nicht", "was", "es", "ist", ".", "F\u00fcr", "uns", "ist"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VAFIN", "$,", "PDS", "VAFIN", "$.", "PTKNEG", "PWS", "PPER", "VAFIN", "$.", "APPR", "PPER", "VAFIN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "alles, was es scheinet.", "tokens": ["al\u00b7les", ",", "was", "es", "schei\u00b7net", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Ich bin begl\u00fcckt. Was hilft es mir, wenn es mein Her-", "tokens": ["Ich", "bin", "be\u00b7gl\u00fcckt", ".", "Was", "hilft", "es", "mir", ",", "wenn", "es", "mein", "Her"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "$.", "PWS", "VVFIN", "PPER", "PPER", "$,", "KOUS", "PPER", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "ze nicht vermeynet?", "tokens": ["ze", "nicht", "ver\u00b7mey\u00b7net", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}