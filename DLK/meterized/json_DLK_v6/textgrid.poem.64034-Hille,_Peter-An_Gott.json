{"textgrid.poem.64034": {"metadata": {"author": {"name": "Hille, Peter", "birth": "N.A.", "death": "N.A."}, "title": "An Gott", "genre": "verse", "period": "N.A.", "pub_year": 1879, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Deine Himmel sind mir viel zu s\u00fc\u00df:", "tokens": ["Dei\u00b7ne", "Him\u00b7mel", "sind", "mir", "viel", "zu", "s\u00fc\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "ADV", "PTKA", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Gib mir, mit freier Brust zu ragen,", "tokens": ["Gib", "mir", ",", "mit", "frei\u00b7er", "Brust", "zu", "ra\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "APPR", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mit dir die Welten zu ertragen,", "tokens": ["Mit", "dir", "die", "Wel\u00b7ten", "zu", "er\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wo du bist!", "tokens": ["Wo", "du", "bist", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VAFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}