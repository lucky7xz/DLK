{"dta.poem.2107": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Aria .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "G\u00f6nn, Herr, deinen Kreaturen,", "tokens": ["G\u00f6nn", ",", "Herr", ",", "dei\u00b7nen", "Kre\u00b7a\u00b7tu\u00b7ren", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df sie deiner Weisheit Spuren", "tokens": ["Da\u00df", "sie", "dei\u00b7ner", "Weis\u00b7heit", "Spu\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die Proben deiner Macht,", "tokens": ["Und", "die", "Pro\u00b7ben", "dei\u00b7ner", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "In derselben Schmuck und Pracht,", "tokens": ["In", "der\u00b7sel\u00b7ben", "Schmuck", "und", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mit vern\u00fcnftgen Augen sehn;", "tokens": ["Mit", "ver\u00b7n\u00fcnft\u00b7gen", "Au\u00b7gen", "sehn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und wenn wir an ihren Sch\u00e4tzen", "tokens": ["Und", "wenn", "wir", "an", "ih\u00b7ren", "Sch\u00e4t\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.7": {"text": "Uns vergn\u00fcgen und ergetzen,", "tokens": ["Uns", "ver\u00b7gn\u00fc\u00b7gen", "und", "er\u00b7get\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "KON", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wir mit Lust und Dank gestehn:", "tokens": ["Wir", "mit", "Lust", "und", "Dank", "ge\u00b7stehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Da\u00df sie, blo\u00df durch dich, so sch\u00f6n.", "tokens": ["Da\u00df", "sie", ",", "blo\u00df", "durch", "dich", ",", "so", "sch\u00f6n", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ADV", "APPR", "PPER", "$,", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}