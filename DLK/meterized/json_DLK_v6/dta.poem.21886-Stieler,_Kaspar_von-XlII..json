{"dta.poem.21886": {"metadata": {"author": {"name": "Stieler, Kaspar von", "birth": "N.A.", "death": "N.A."}, "title": "XlII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1660", "urn": "urn:nbn:de:kobv:b4-20586-2", "language": ["de:0.99"], "booktitle": "Filidor der Dorfferer [i. e. Stieler, Kaspar von]: Die Geharnschte Venus. Hamburg, 1660."}, "poem": {"stanza.1": {"line.1": {"text": "Rosilis ich bin dein Gast", "tokens": ["Ro\u00b7si\u00b7lis", "ich", "bin", "dein", "Gast"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "PPER", "VAFIN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "denn so la\u00df uns beyd\u2019 allein.", "tokens": ["denn", "so", "la\u00df", "uns", "beyd'", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVIMP", "PPER", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Zeugen sind mir ganz verha\u00dft", "tokens": ["Zeu\u00b7gen", "sind", "mir", "ganz", "ver\u00b7ha\u00dft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "ADV", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "die auff einen jeden Bissen", "tokens": ["die", "auff", "ei\u00b7nen", "je\u00b7den", "Bis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "den wir ich und du geniessen/", "tokens": ["den", "wir", "ich", "und", "du", "ge\u00b7nies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "KON", "PPER", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Rosilis hats gut gemacht.", "tokens": ["Ro\u00b7si\u00b7lis", "hats", "gut", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "der holt Licht und der Tobak", "tokens": ["der", "holt", "Licht", "und", "der", "To\u00b7bak"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "VVFIN", "NN", "KON", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "den wil sie nach Milch au\u00dfschikken/", "tokens": ["den", "wil", "sie", "nach", "Milch", "au\u00df\u00b7schik\u00b7ken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VMFIN", "PPER", "APPR", "NN", "VVINF", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "der bringt Bier/ der f\u00fcnffte mag", "tokens": ["der", "bringt", "Bier", "/", "der", "f\u00fcnff\u00b7te", "mag"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "VVFIN", "NN", "$(", "ART", "ADJA", "VMFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "bleiben/ weil er geht auff Kr\u00fckken.", "tokens": ["blei\u00b7ben", "/", "weil", "er", "geht", "auff", "Kr\u00fck\u00b7ken", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVINF", "$(", "KOUS", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}