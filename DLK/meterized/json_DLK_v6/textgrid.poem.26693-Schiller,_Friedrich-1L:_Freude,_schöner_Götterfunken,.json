{"textgrid.poem.26693": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Freude, sch\u00f6ner G\u00f6tterfunken,", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freude, sch\u00f6ner G\u00f6tterfunken,", "tokens": ["Freu\u00b7de", ",", "sch\u00f6\u00b7ner", "G\u00f6t\u00b7ter\u00b7fun\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tochter aus Elysium,", "tokens": ["Toch\u00b7ter", "aus", "E\u00b7ly\u00b7si\u00b7um", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wir betreten feuertrunken", "tokens": ["Wir", "be\u00b7tre\u00b7ten", "feu\u00b7er\u00b7trun\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["PPER", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Himmlische, dein Heiligtum.", "tokens": ["Himm\u00b7li\u00b7sche", ",", "dein", "Hei\u00b7lig\u00b7tum", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Deine Zauber binden wieder,", "tokens": ["Dei\u00b7ne", "Zau\u00b7ber", "bin\u00b7den", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Was der Mode Schwert geteilt;", "tokens": ["Was", "der", "Mo\u00b7de", "Schwert", "ge\u00b7teilt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Bettler werden F\u00fcrstenbr\u00fcder,", "tokens": ["Bett\u00b7ler", "wer\u00b7den", "F\u00fcrs\u00b7ten\u00b7br\u00fc\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wo dein sanfter Fl\u00fcgel weilt.", "tokens": ["Wo", "dein", "sanf\u00b7ter", "Fl\u00fc\u00b7gel", "weilt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}