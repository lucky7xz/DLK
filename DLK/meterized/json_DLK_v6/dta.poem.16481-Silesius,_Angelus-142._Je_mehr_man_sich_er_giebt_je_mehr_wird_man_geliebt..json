{"dta.poem.16481": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "142.  Je mehr man sich er giebt/ je  \n  mehr wird man geliebt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Warumb wird Seraphin von GOtte mehr geliebt", "tokens": ["Wa\u00b7rumb", "wird", "Se\u00b7ra\u00b7phin", "von", "Got\u00b7te", "mehr", "ge\u00b7liebt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "NE", "APPR", "NN", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als eine M\u00fck? Es ist/ da\u00df er sich mehr ergiebt.", "tokens": ["Als", "ei\u00b7ne", "M\u00fck", "?", "Es", "ist", "/", "da\u00df", "er", "sich", "mehr", "er\u00b7giebt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$.", "PPER", "VAFIN", "$(", "KOUS", "PPER", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}