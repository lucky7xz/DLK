{"textgrid.poem.39160": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als ich noch jung gewesen,", "tokens": ["Als", "ich", "noch", "jung", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VAPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da machten die Leute ein Wesen,", "tokens": ["Da", "mach\u00b7ten", "die", "Leu\u00b7te", "ein", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Wenn ich in den Spiegel sah.", "tokens": ["Wenn", "ich", "in", "den", "Spie\u00b7gel", "sah", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sie schalten mich einen Gecken", "tokens": ["Sie", "schal\u00b7ten", "mich", "ei\u00b7nen", "Ge\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und stolz und eitel dabei;", "tokens": ["Und", "stolz", "und", "ei\u00b7tel", "da\u00b7bei", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "ADJD", "PAV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Wie w\u00fcrden sie jetzt erschrecken,", "tokens": ["Wie", "w\u00fcr\u00b7den", "sie", "jetzt", "er\u00b7schre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Jetzt hab' ich der Spiegel drei.", "tokens": ["Jetzt", "hab'", "ich", "der", "Spie\u00b7gel", "drei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "CARD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Zum Einen wend' ich mich heute,", "tokens": ["Zum", "Ei\u00b7nen", "wend'", "ich", "mich", "heu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PRF", "ADV", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Zum Andern morgen hin,", "tokens": ["Zum", "An\u00b7dern", "mor\u00b7gen", "hin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nun sagt mir, ihr guten Leute,", "tokens": ["Nun", "sagt", "mir", ",", "ihr", "gu\u00b7ten", "Leu\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ob ich nicht eitel bin?", "tokens": ["Ob", "ich", "nicht", "ei\u00b7tel", "bin", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADJD", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}