{"dta.poem.2093": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Jgel .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Man findet zahm und wilde Jgel, auch ist die Gattung", "tokens": ["Man", "fin\u00b7det", "zahm", "und", "wil\u00b7de", "Jgel", ",", "auch", "ist", "die", "Gat\u00b7tung"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ADJD", "KON", "ADJA", "NN", "$,", "ADV", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "zweyerley,", "tokens": ["zwey\u00b7er\u00b7ley", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Von welchen man die eine S\u00e4u- die andern Hundes-Jgel", "tokens": ["Von", "wel\u00b7chen", "man", "die", "ei\u00b7ne", "S\u00e4u", "die", "an\u00b7dern", "Hun\u00b7des\u00b7J\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PWAT", "PIS", "ART", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "nennet.", "tokens": ["nen\u00b7net", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "--", "measure": "unknown.measure.zero"}, "line.5": {"text": "Es n\u00fctzet uns die\u00df kleine Thier besonders in der Arzeney,", "tokens": ["Es", "n\u00fct\u00b7zet", "uns", "die\u00df", "klei\u00b7ne", "Thier", "be\u00b7son\u00b7ders", "in", "der", "Ar\u00b7ze\u00b7ney", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PDS", "ADJA", "NN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.6": {"text": "Indem die Galle, nebst der Leber, der Koth, die Milz,", "tokens": ["In\u00b7dem", "die", "Gal\u00b7le", ",", "nebst", "der", "Le\u00b7ber", ",", "der", "Koth", ",", "die", "Milz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "APPR", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "das Fett, das Blut,", "tokens": ["das", "Fett", ",", "das", "Blut", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Zusammt des Magens innerm H\u00e4utlein oft ganz besondre", "tokens": ["Zu\u00b7sammt", "des", "Ma\u00b7gens", "in\u00b7nerm", "H\u00e4ut\u00b7lein", "oft", "ganz", "be\u00b7sond\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "ADV", "ADV", "ADJA"], "meter": "-+-+--++--+-+-", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Wirkung thut.", "tokens": ["Wir\u00b7kung", "thut", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.10": {"text": "Zumalen soll von einem Jgel die Asche, wenn wir ihn", "tokens": ["Zu\u00b7ma\u00b7len", "soll", "von", "ei\u00b7nem", "Jgel", "die", "A\u00b7sche", ",", "wenn", "wir", "ihn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VMFIN", "APPR", "ART", "NN", "ART", "NN", "$,", "KOUS", "PPER", "PPER"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.11": {"text": "verbrennen,", "tokens": ["ver\u00b7bren\u00b7nen", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.12": {"text": "Ein kr\u00e4ftig Mittel seyn f\u00fcr die, so den Urin nicht halten", "tokens": ["Ein", "kr\u00e4f\u00b7tig", "Mit\u00b7tel", "seyn", "f\u00fcr", "die", ",", "so", "den", "U\u00b7rin", "nicht", "hal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "VAINF", "APPR", "ART", "$,", "ADV", "ART", "NN", "PTKNEG", "VVINF"], "meter": "-+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.13": {"text": "k\u00f6nnen.", "tokens": ["k\u00f6n\u00b7nen", "."], "token_info": ["word", "punct"], "pos": ["VMFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.14": {"text": "Es ist demnach auch dieses Thier, sowohl als alle andre,", "tokens": ["Es", "ist", "dem\u00b7nach", "auch", "die\u00b7ses", "Thier", ",", "so\u00b7wohl", "als", "al\u00b7le", "and\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PAV", "ADV", "PDAT", "NN", "$,", "KON", "KOUS", "PIS", "PIS", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.15": {"text": "werth,", "tokens": ["werth", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "+", "measure": "single.up"}, "line.16": {"text": "Da\u00df man in ihm auch einen Sch\u00f6pfer erkennt, und sel-", "tokens": ["Da\u00df", "man", "in", "ihm", "auch", "ei\u00b7nen", "Sch\u00f6p\u00b7fer", "er\u00b7kennt", ",", "und", "sel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PIS", "APPR", "PPER", "ADV", "ART", "NN", "VVFIN", "$,", "KON", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.17": {"text": "bigen verehrt.", "tokens": ["bi\u00b7gen", "ver\u00b7ehrt", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}}}}