{"textgrid.poem.56240": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fchling und Herbst", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "F\u00fcrwahr, der Fr\u00fchling ist erwacht;", "tokens": ["F\u00fcr\u00b7wahr", ",", "der", "Fr\u00fch\u00b7ling", "ist", "er\u00b7wacht", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den holden Liebling zu empfahn,", "tokens": ["Den", "hol\u00b7den", "Lieb\u00b7ling", "zu", "em\u00b7pfahn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hat sich mit frischer Blumenpracht", "tokens": ["Hat", "sich", "mit", "fri\u00b7scher", "Blu\u00b7men\u00b7pracht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die junge Erde angetan.", "tokens": ["Die", "jun\u00b7ge", "Er\u00b7de", "an\u00b7ge\u00b7tan", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die muntern V\u00f6gel, lieberw\u00e4rmt,", "tokens": ["Die", "mun\u00b7tern", "V\u00f6\u00b7gel", ",", "lie\u00b7ber\u00b7w\u00e4rmt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Begehn im gr\u00fcnen Hain ihr Fest.", "tokens": ["Be\u00b7gehn", "im", "gr\u00fc\u00b7nen", "Hain", "ihr", "Fest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein jeder singt, ein jeder schw\u00e4rmt,", "tokens": ["Ein", "je\u00b7der", "singt", ",", "ein", "je\u00b7der", "schw\u00e4rmt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "$,", "ART", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und bauet emsig sich sein Nest.", "tokens": ["Und", "bau\u00b7et", "em\u00b7sig", "sich", "sein", "Nest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PRF", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und alles lebt und liebt und singt,", "tokens": ["Und", "al\u00b7les", "lebt", "und", "liebt", "und", "singt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und preist den Fr\u00fchling wunderbar,", "tokens": ["Und", "preist", "den", "Fr\u00fch\u00b7ling", "wun\u00b7der\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Fr\u00fchling, der die Freude bringt;", "tokens": ["Den", "Fr\u00fch\u00b7ling", ",", "der", "die", "Freu\u00b7de", "bringt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich aber bleibe stumm und starr.", "tokens": ["Ich", "a\u00b7ber", "blei\u00b7be", "stumm", "und", "starr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Dir, Erde, g\u00f6nn ich deine Zier,", "tokens": ["Dir", ",", "Er\u00b7de", ",", "g\u00f6nn", "ich", "dei\u00b7ne", "Zier", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Euch, S\u00e4nger, g\u00f6nn ich eure Lust,", "tokens": ["Euch", ",", "S\u00e4n\u00b7ger", ",", "g\u00f6nn", "ich", "eu\u00b7re", "Lust", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So g\u00f6nnet meine Trauer mir,", "tokens": ["So", "g\u00f6n\u00b7net", "mei\u00b7ne", "Trau\u00b7er", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den tiefen Schmerz in meiner Brust.", "tokens": ["Den", "tie\u00b7fen", "Schmerz", "in", "mei\u00b7ner", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "F\u00fcr mich ist Herbst; der Nebelwind", "tokens": ["F\u00fcr", "mich", "ist", "Herbst", ";", "der", "Ne\u00b7bel\u00b7wind"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPER", "VAFIN", "NN", "$.", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durchw\u00fchlet kalt mein falbes Laub;", "tokens": ["Durch\u00b7w\u00fch\u00b7let", "kalt", "mein", "fal\u00b7bes", "Laub", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die \u00c4ste mir zerschlagen sind,", "tokens": ["Die", "\u00c4s\u00b7te", "mir", "zer\u00b7schla\u00b7gen", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und meine Krone liegt im Staub.", "tokens": ["Und", "mei\u00b7ne", "Kro\u00b7ne", "liegt", "im", "Staub", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}