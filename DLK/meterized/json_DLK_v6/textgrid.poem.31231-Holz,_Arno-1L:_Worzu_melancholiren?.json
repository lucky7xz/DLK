{"textgrid.poem.31231": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "1L: Worzu melancholiren?", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Worzu melancholiren?", "tokens": ["Wor\u00b7zu", "me\u00b7lan\u00b7cho\u00b7li\u00b7ren", "?"], "token_info": ["word", "word", "punct"], "pos": ["PAV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Schnell l\u00e4ufft die s\u00fcsse Zeit.", "tokens": ["Schnell", "l\u00e4ufft", "die", "s\u00fcs\u00b7se", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Amsteln drompettiren", "tokens": ["Die", "A\u00b7msteln", "drom\u00b7pet\u00b7ti\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.4": {"text": "des ", "tokens": ["des"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.5": {"text": "Die bundten Gr\u00e4sgens blincken/", "tokens": ["Die", "bund\u00b7ten", "Gr\u00e4s\u00b7gens", "blin\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "still lauscht die Fr\u00fchlings-Frau", "tokens": ["still", "lauscht", "die", "Fr\u00fch\u00b7lings\u00b7Frau"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "die Sonnen-Pferde drincken", "tokens": ["die", "Son\u00b7nen\u00b7P\u00b7fer\u00b7de", "drin\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "itzt nichts denn ", "tokens": ["itzt", "nichts", "denn"], "token_info": ["word", "word", "word"], "pos": ["ADV", "PIS", "ADV"], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Bald br\u00e4nnt des Hunds-Sterns Hizze/", "tokens": ["Bald", "br\u00e4nnt", "des", "Hunds\u00b7S\u00b7terns", "Hiz\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "dan ist mir mehr al\u00df wohl/", "tokens": ["dan", "ist", "mir", "mehr", "al\u00df", "wohl", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIAT", "KOKOM", "ADV", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "dan spannt der kleine Schizze", "tokens": ["dan", "spannt", "der", "klei\u00b7ne", "Schiz\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "nach mir sein Mord-Bistohl.", "tokens": ["nach", "mir", "sein", "Mord\u00b7Bi\u00b7sto\u00b7hl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PPOSAT", "NN", "$."], "meter": "+--++-+", "measure": "dactylic.init"}, "line.5": {"text": "Im Schlaff-gesunden Kimmel", "tokens": ["Im", "Schlaff\u00b7ge\u00b7sun\u00b7den", "Kim\u00b7mel"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "ligt man dan gern zu Zween/", "tokens": ["ligt", "man", "dan", "gern", "zu", "Zween", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "ADV", "APPR", "NN", "$("], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "inde\u00df am blauen Himmel", "tokens": ["in\u00b7de\u00df", "am", "blau\u00b7en", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "die weissen Sch\u00e4ffgens gehn.", "tokens": ["die", "weis\u00b7sen", "Sch\u00e4ff\u00b7gens", "gehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sordan dritt schwehr an Trauben", "tokens": ["Sor\u00b7dan", "dritt", "schwehr", "an", "Trau\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "dan kan ich kaum noch glauben", "tokens": ["dan", "kan", "ich", "kaum", "noch", "glau\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADV", "VVINF"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "an ", "tokens": ["an"], "token_info": ["word"], "pos": ["APPR"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "dan lihb ich es zu schweiffen/", "tokens": ["dan", "lihb", "ich", "es", "zu", "schwei\u00b7ffen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "PTKZU", "VVINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "dan macht mich frohen Sinns", "tokens": ["dan", "macht", "mich", "fro\u00b7hen", "Sinns"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "das angenehme Pfeiffen", "tokens": ["das", "an\u00b7ge\u00b7neh\u00b7me", "Pfeif\u00b7fen"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "der Grammets-V\u00f6gelckins.", "tokens": ["der", "Gram\u00b7mets\u00b7V\u00f6\u00b7gelckins", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Panduren und Krabaten!", "tokens": ["Pan\u00b7du\u00b7ren", "und", "Kra\u00b7ba\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "-++-+--", "measure": "unknown.measure.tri"}, "line.2": {"text": "Zurlezzt stapfft ", "tokens": ["Zur\u00b7lezzt", "stapfft"], "token_info": ["word", "word"], "pos": ["ADV", "VVFIN"], "meter": "+--", "measure": "dactylic.init"}, "line.3": {"text": "Der Teuffel soll den brahten/", "tokens": ["Der", "Teuf\u00b7fel", "soll", "den", "brah\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ART", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "der den nicht leiden kan!", "tokens": ["der", "den", "nicht", "lei\u00b7den", "kan", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Die Kindgens jubiliren/", "tokens": ["Die", "Kind\u00b7gens", "ju\u00b7bi\u00b7li\u00b7ren", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "wie's drau\u00dfen stihbt und schneyt.", "tokens": ["wie's", "drau\u00b7\u00dfen", "stihbt", "und", "schneyt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Lasst andre grillisiren/", "tokens": ["Lasst", "and\u00b7re", "gril\u00b7li\u00b7si\u00b7ren", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "ich b\u00fcn for Heiterkeit!", "tokens": ["ich", "b\u00fcn", "for", "Hei\u00b7ter\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}