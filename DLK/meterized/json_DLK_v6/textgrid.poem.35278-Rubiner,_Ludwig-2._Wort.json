{"textgrid.poem.35278": {"metadata": {"author": {"name": "Rubiner, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "2. Wort", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du sprichst. Dein Blut erduftet den Armen in levkojischen hellen Beeten", "tokens": ["Du", "sprichst", ".", "Dein", "Blut", "er\u00b7duf\u00b7tet", "den", "Ar\u00b7men", "in", "lev\u00b7ko\u00b7ji\u00b7schen", "hel\u00b7len", "Bee\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "PPOSAT", "NN", "VVFIN", "ART", "NN", "APPR", "ADJA", "ADJA", "NN"], "meter": "-+-+-+--+--+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Du erwinkst den Hungrigen die gro\u00dfen beladenen K\u00fchlmetzgerein,", "tokens": ["Du", "er\u00b7winkst", "den", "Hung\u00b7ri\u00b7gen", "die", "gro\u00b7\u00dfen", "be\u00b7la\u00b7de\u00b7nen", "K\u00fchl\u00b7metz\u00b7ge\u00b7rein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "--+-+-+-+--+--++-+", "measure": "iambic.octa.plus.relaxed"}, "line.3": {"text": "In kalten Nordlanden schie\u00dfen aus dir kristallene H\u00e4user von gl\u00e4sernem Stein.", "tokens": ["In", "kal\u00b7ten", "Nord\u00b7lan\u00b7den", "schie\u00b7\u00dfen", "aus", "dir", "kris\u00b7tal\u00b7le\u00b7ne", "H\u00e4u\u00b7ser", "von", "gl\u00e4\u00b7ser\u00b7nem", "Stein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "APPR", "PPER", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+-+-+--+--+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.2": {"line.1": {"text": "Du fliegst mit buckligen Steinsetzern aus splitterndem Staub in den Wald, ans Wasser, zu Tieren,", "tokens": ["Du", "fliegst", "mit", "buck\u00b7li\u00b7gen", "Stein\u00b7set\u00b7zern", "aus", "split\u00b7tern\u00b7dem", "Staub", "in", "den", "Wald", ",", "ans", "Was\u00b7ser", ",", "zu", "Tie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "APPR", "ADJA", "NN", "APPR", "ART", "NN", "$,", "APPRART", "NN", "$,", "APPR", "NN", "$,"], "meter": "-+-+--++--+--+--+-+--+-", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Du springst unter stehlende Kinder inmitten der Angst, und spielst Ball in Massenquartieren.", "tokens": ["Du", "springst", "un\u00b7ter", "steh\u00b7len\u00b7de", "Kin\u00b7der", "in\u00b7mit\u00b7ten", "der", "Angst", ",", "und", "spielst", "Ball", "in", "Mas\u00b7sen\u00b7quar\u00b7tie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "APPR", "ART", "NN", "$,", "KON", "VVFIN", "NN", "APPR", "NN", "$."], "meter": "----+--+--+--+-++-+--+-", "measure": "iambic.octa.plus.relaxed"}, "line.3": {"text": "Du wirfst um gepeitschte Landl\u00f6hner hohe St\u00e4dte her, eckige H\u00e4user mit Licht und gro\u00df,", "tokens": ["Du", "wirfst", "um", "ge\u00b7peitschte", "Land\u00b7l\u00f6h\u00b7ner", "ho\u00b7he", "St\u00e4d\u00b7te", "her", ",", "ec\u00b7ki\u00b7ge", "H\u00e4u\u00b7ser", "mit", "Licht", "und", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN", "ADJA", "NN", "PTKVZ", "$,", "ADJA", "NN", "APPR", "NN", "KON", "ADJD", "$,"], "meter": "-+-+-+--+-+--+--+--+-+", "measure": "iambic.octa.plus.relaxed"}, "line.4": {"text": "Du tanzt mit den alleinsitzenden M\u00e4dchen im Saal und ziehst sie auf deinen Scho\u00df.", "tokens": ["Du", "tanzt", "mit", "den", "al\u00b7lein\u00b7sit\u00b7zen\u00b7den", "M\u00e4d\u00b7chen", "im", "Saal", "und", "ziehst", "sie", "auf", "dei\u00b7nen", "Scho\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "APPRART", "NN", "KON", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+--+--+-+--+-+", "measure": "iambic.octa.plus.relaxed"}, "line.5": {"text": "Du liegst in der Nacht am gew\u00f6lbten Leib, du k\u00fc\u00dft nackte Arme z\u00e4rtlich und lang,", "tokens": ["Du", "liegst", "in", "der", "Nacht", "am", "ge\u00b7w\u00f6lb\u00b7ten", "Leib", ",", "du", "k\u00fc\u00dft", "nack\u00b7te", "Ar\u00b7me", "z\u00e4rt\u00b7lich", "und", "lang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "APPRART", "ADJA", "NN", "$,", "PPER", "VVFIN", "ADJA", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+--+-+-+--+-+--+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Du str\u00f6mst in alle Frauen der Welt und streichelst ihren Gang.", "tokens": ["Du", "str\u00f6mst", "in", "al\u00b7le", "Frau\u00b7en", "der", "Welt", "und", "strei\u00b7chelst", "ih\u00b7ren", "Gang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "ART", "NN", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.7": {"text": "Du sprichst: die Erde springt wie eine Fackel empor und zerstiebt im finstersten Traum,", "tokens": ["Du", "sprichst", ":", "die", "Er\u00b7de", "springt", "wie", "ei\u00b7ne", "Fa\u00b7ckel", "em\u00b7por", "und", "zer\u00b7stiebt", "im", "fins\u00b7ters\u00b7ten", "Traum", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "ART", "NN", "VVFIN", "KOKOM", "ART", "NN", "PTKVZ", "KON", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+--+--+-+--+", "measure": "iambic.octa.plus.relaxed"}, "line.8": {"text": "Von fernen Sternstrahlen haucht dein Wort und erbaut sie neu aus dem Raum.", "tokens": ["Von", "fer\u00b7nen", "Sterns\u00b7trah\u00b7len", "haucht", "dein", "Wort", "und", "er\u00b7baut", "sie", "neu", "aus", "dem", "Raum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "KON", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-+--+-+--+", "measure": "iambic.septa.relaxed"}}}}}