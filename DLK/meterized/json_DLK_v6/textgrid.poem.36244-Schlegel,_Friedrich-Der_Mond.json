{"textgrid.poem.36244": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Mond", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es streben alle Kr\u00e4fte,", "tokens": ["Es", "stre\u00b7ben", "al\u00b7le", "Kr\u00e4f\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So matt sie sind, zur Erde doch zu wirken.", "tokens": ["So", "matt", "sie", "sind", ",", "zur", "Er\u00b7de", "doch", "zu", "wir\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "VAFIN", "$,", "APPRART", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In den ew'gen Bezirken", "tokens": ["In", "den", "ew'\u00b7gen", "Be\u00b7zir\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Der sch\u00f6nen Welt ist das nur mein Gesch\u00e4fte;", "tokens": ["Der", "sch\u00f6\u00b7nen", "Welt", "ist", "das", "nur", "mein", "Ge\u00b7sch\u00e4f\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PDS", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Das mu\u00df ohnm\u00e4chtig immer ich versuchen,", "tokens": ["Das", "mu\u00df", "ohn\u00b7m\u00e4ch\u00b7tig", "im\u00b7mer", "ich", "ver\u00b7su\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADJD", "ADV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und traurig dem beschr\u00e4nkten Lose fluchen.", "tokens": ["Und", "trau\u00b7rig", "dem", "be\u00b7schr\u00e4nk\u00b7ten", "Lo\u00b7se", "flu\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Seht ihr mich milde gl\u00e4nzen,", "tokens": ["Seht", "ihr", "mich", "mil\u00b7de", "gl\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und warme Sommern\u00e4chte sch\u00f6n erhellen,", "tokens": ["Und", "war\u00b7me", "Som\u00b7mer\u00b7n\u00e4ch\u00b7te", "sch\u00f6n", "er\u00b7hel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wo leise Freudewellen", "tokens": ["Wo", "lei\u00b7se", "Freu\u00b7de\u00b7wel\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "ADJD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Erde Kinder k\u00fchlen nach den T\u00e4nzen;", "tokens": ["Der", "Er\u00b7de", "Kin\u00b7der", "k\u00fch\u00b7len", "nach", "den", "T\u00e4n\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Sind's Sonnengeister nur, die sanfter spielen.", "tokens": ["Sin\u00b7d's", "Son\u00b7nen\u00b7geis\u00b7ter", "nur", ",", "die", "sanf\u00b7ter", "spie\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADV", "$,", "PRELS", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.6": {"text": "Mein eignes Wesen k\u00f6nnt ihr so nicht f\u00fchlen.", "tokens": ["Mein", "eig\u00b7nes", "We\u00b7sen", "k\u00f6nnt", "ihr", "so", "nicht", "f\u00fch\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch wenn ich seltsam scheine,", "tokens": ["Doch", "wenn", "ich", "selt\u00b7sam", "schei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Aus dunkeln Wolken \u00e4ngstlich vorgeschlichen;", "tokens": ["Aus", "dun\u00b7keln", "Wol\u00b7ken", "\u00e4ngst\u00b7lich", "vor\u00b7ge\u00b7schli\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dann ist die H\u00fcll' entwichen,", "tokens": ["Dann", "ist", "die", "H\u00fcll'", "ent\u00b7wi\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Es merkt der Mensch mit Schaudern, was ich meine.", "tokens": ["Es", "merkt", "der", "Mensch", "mit", "Schau\u00b7dern", ",", "was", "ich", "mei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "So zeigen Geister sich, um euch zu wecken,", "tokens": ["So", "zei\u00b7gen", "Geis\u00b7ter", "sich", ",", "um", "euch", "zu", "we\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "PRF", "$,", "KOUI", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und lassen ahnden die verborgnen Schrecken.", "tokens": ["Und", "las\u00b7sen", "ahn\u00b7den", "die", "ver\u00b7borg\u00b7nen", "Schre\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}