{"textgrid.poem.43255": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wohl sind es Monde jetzt, seit ich Dich mied,", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohl sind es Monde jetzt, seit ich Dich mied,", "tokens": ["Wohl", "sind", "es", "Mon\u00b7de", "jetzt", ",", "seit", "ich", "Dich", "mied", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NE", "ADV", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Doch vorwurfslos hast Du mich aufgenommen,", "tokens": ["Doch", "vor\u00b7wurfs\u00b7los", "hast", "Du", "mich", "auf\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit freudiger Scheu sahst Du mich wiederkommen,", "tokens": ["Mit", "freu\u00b7di\u00b7ger", "Scheu", "sahst", "Du", "mich", "wie\u00b7der\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Von bleichen Lippen klingt Dein altes Lied.", "tokens": ["Von", "blei\u00b7chen", "Lip\u00b7pen", "klingt", "Dein", "al\u00b7tes", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Sei nicht so rathlos ... Ich bin kein Tyrann,", "tokens": ["Sei", "nicht", "so", "rath\u00b7los", "...", "Ich", "bin", "kein", "Ty\u00b7rann", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ADV", "ADJD", "$(", "PPER", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Bin kein Romanheld voll studirter Qualen", "tokens": ["Bin", "kein", "Ro\u00b7ma\u00b7nheld", "voll", "stu\u00b7dir\u00b7ter", "Qua\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PIAT", "NN", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein Zweifler bin ich, innerlich zerfallen,", "tokens": ["Ein", "Zweif\u00b7ler", "bin", "ich", ",", "in\u00b7ner\u00b7lich", "zer\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "$,", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein glaubensloser, lebenskranker Mann.", "tokens": ["Ein", "glau\u00b7bens\u00b7lo\u00b7ser", ",", "le\u00b7bens\u00b7kran\u00b7ker", "Mann", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}