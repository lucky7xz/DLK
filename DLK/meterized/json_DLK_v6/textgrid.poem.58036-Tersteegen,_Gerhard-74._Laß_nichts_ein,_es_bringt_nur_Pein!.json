{"textgrid.poem.58036": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "74. La\u00df nichts ein, es bringt nur Pein!", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Herz wird leicht und sanft an Kreaturen fest,", "tokens": ["Das", "Herz", "wird", "leicht", "und", "sanft", "an", "Kre\u00b7a\u00b7tu\u00b7ren", "fest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch kostet's Kampf und Schmerz, eh' man sie wieder l\u00e4\u00dft;", "tokens": ["Doch", "kos\u00b7tet's", "Kampf", "und", "Schmerz", ",", "eh'", "man", "sie", "wie\u00b7der", "l\u00e4\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "KON", "NN", "$,", "KOUS", "PIS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Drum fasse nichts mit Lust im Himmel und auf Erden,", "tokens": ["Drum", "fas\u00b7se", "nichts", "mit", "Lust", "im", "Him\u00b7mel", "und", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PIS", "APPR", "NN", "APPRART", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als was dir ewiglich nicht kann genommen werden!", "tokens": ["Als", "was", "dir", "e\u00b7wig\u00b7lich", "nicht", "kann", "ge\u00b7nom\u00b7men", "wer\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PWS", "PPER", "ADV", "PTKNEG", "VMFIN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}