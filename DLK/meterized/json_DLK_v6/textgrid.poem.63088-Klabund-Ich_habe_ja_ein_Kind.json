{"textgrid.poem.63088": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Ich habe ja ein Kind", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich habe ja ein Kind,", "tokens": ["Ich", "ha\u00b7be", "ja", "ein", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Nun kann ich nicht mehr sterben,", "tokens": ["Nun", "kann", "ich", "nicht", "mehr", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wenn meine Augen tot und blind,", "tokens": ["Wenn", "mei\u00b7ne", "Au\u00b7gen", "tot", "und", "blind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dann hab' ich einen Erben.", "tokens": ["Dann", "hab'", "ich", "ei\u00b7nen", "Er\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Alle meine Tr\u00e4ume flattern", "tokens": ["Al\u00b7le", "mei\u00b7ne", "Tr\u00e4u\u00b7me", "flat\u00b7tern"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In meines Kindes Augen wieder mit blauen Fl\u00fcgeln auf,", "tokens": ["In", "mei\u00b7nes", "Kin\u00b7des", "Au\u00b7gen", "wie\u00b7der", "mit", "blau\u00b7en", "Fl\u00fc\u00b7geln", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "ADV", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Schie\u00dfen zwitschernd um seines jungen Turmes sonnegoldnen Knauf,", "tokens": ["Schie\u00b7\u00dfen", "zwit\u00b7schernd", "um", "sei\u00b7nes", "jun\u00b7gen", "Tur\u00b7mes", "son\u00b7ne\u00b7gold\u00b7nen", "Knauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "PPOSAT", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.4": {"text": "Wenn dumpf schon ferne die Gewitter rattern.", "tokens": ["Wenn", "dumpf", "schon", "fer\u00b7ne", "die", "Ge\u00b7wit\u00b7ter", "rat\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ADV", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Du wirst mich ganz erf\u00fcllen,", "tokens": ["Du", "wirst", "mich", "ganz", "er\u00b7f\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und meine Unruh stillen,", "tokens": ["Und", "mei\u00b7ne", "Un\u00b7ruh", "stil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mein Kind... du \u00fcberwindest mein Martyrium.", "tokens": ["Mein", "Kind", "...", "du", "\u00fc\u00b7berw\u00b7in\u00b7dest", "mein", "Mar\u00b7ty\u00b7ri\u00b7um", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wenn ich begraben werde,", "tokens": ["Wenn", "ich", "be\u00b7gra\u00b7ben", "wer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wirf du die erste Handvoll Erde", "tokens": ["Wirf", "du", "die", "ers\u00b7te", "Hand\u00b7voll", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Auf meinen Sarg \u2013 und dreh dich lachend um.", "tokens": ["Auf", "mei\u00b7nen", "Sarg", "\u2013", "und", "dreh", "dich", "la\u00b7chend", "um", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$(", "KON", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Geh hin zum neuen Leben,", "tokens": ["Geh", "hin", "zum", "neu\u00b7en", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mehr kann ich dir nicht geben,", "tokens": ["Mehr", "kann", "ich", "dir", "nicht", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Als was ich war... und ich war ich.", "tokens": ["Als", "was", "ich", "war", "...", "und", "ich", "war", "ich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PWS", "PPER", "VAFIN", "$(", "KON", "PPER", "VAFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Blut soll in dir singen,", "tokens": ["Mein", "Blut", "soll", "in", "dir", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "In meine Tiefe dringen,", "tokens": ["In", "mei\u00b7ne", "Tie\u00b7fe", "drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wenn l\u00e4ngst sich Wurm auf Wurm in meinen Sch\u00e4del schlich.", "tokens": ["Wenn", "l\u00e4ngst", "sich", "Wurm", "auf", "Wurm", "in", "mei\u00b7nen", "Sch\u00e4\u00b7del", "schlich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PRF", "NN", "APPR", "NN", "APPR", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}