{"textgrid.poem.24814": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Heimatgef\u00fchl", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir schritten durch des Abends Schweigen;", "tokens": ["Wir", "schrit\u00b7ten", "durch", "des", "A\u00b7bends", "Schwei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Wetter war fernhin verrollt;", "tokens": ["Ein", "Wet\u00b7ter", "war", "fern\u00b7hin", "ver\u00b7rollt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Durchs feuchte Laub von Zweig zu Zweigen", "tokens": ["Durchs", "feuch\u00b7te", "Laub", "von", "Zweig", "zu", "Zwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "APPR", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Glitt sanft der Sonne letztes Gold.", "tokens": ["Glitt", "sanft", "der", "Son\u00b7ne", "letz\u00b7tes", "Gold", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Da von den L\u00e4ndern dir, den fernen,", "tokens": ["Da", "von", "den", "L\u00e4n\u00b7dern", "dir", ",", "den", "fer\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PPER", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Erz\u00e4hlt' ich, wo ich dein gedacht.", "tokens": ["Er\u00b7z\u00e4hlt'", "ich", ",", "wo", "ich", "dein", "ge\u00b7dacht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "PPER", "PPOSAT", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von Rast am Rande der Eisternen", "tokens": ["Von", "Rast", "am", "Ran\u00b7de", "der", "Eis\u00b7ter\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "APPRART", "NN", "ART", "NN"], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "In Syriens blauer Sommernacht,", "tokens": ["In", "Sy\u00b7ri\u00b7ens", "blau\u00b7er", "Som\u00b7mer\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Von Aegeus' Meer, wie l\u00e4ngs der K\u00fcsten", "tokens": ["Von", "A\u00b7e\u00b7geus'", "Meer", ",", "wie", "l\u00e4ngs", "der", "K\u00fcs\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NE", "NN", "$,", "PWAV", "APPR", "ART", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mich schaukelte der schwanke Kahn,", "tokens": ["Mich", "schau\u00b7kel\u00b7te", "der", "schwan\u00b7ke", "Kahn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und halb zerbrochne Marmorb\u00fcsten", "tokens": ["Und", "halb", "zer\u00b7broch\u00b7ne", "Mar\u00b7mor\u00b7b\u00fcs\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Aus Tempelnischen niedersahn.", "tokens": ["Aus", "Tem\u00b7pel\u00b7ni\u00b7schen", "nie\u00b7der\u00b7sahn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und hoch dein Auge sah ich leuchten;", "tokens": ["Und", "hoch", "dein", "Au\u00b7ge", "sah", "ich", "leuch\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPOSAT", "NN", "VVFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du heischtest mehr noch, doch ich schwieg,", "tokens": ["Du", "heischtest", "mehr", "noch", ",", "doch", "ich", "schwieg", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$,", "KON", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Indessen wallend in der feuchten", "tokens": ["In\u00b7des\u00b7sen", "wal\u00b7lend", "in", "der", "feuch\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sp\u00e4tluft der Wiesen Nebel stieg.", "tokens": ["Sp\u00e4t\u00b7luft", "der", "Wie\u00b7sen", "Ne\u00b7bel", "stieg", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Warum mich in die Ferne tr\u00e4umen?", "tokens": ["Wa\u00b7rum", "mich", "in", "die", "Fer\u00b7ne", "tr\u00e4u\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dacht' ich, mein Haupt an deins gelehnt:", "tokens": ["Dacht'", "ich", ",", "mein", "Haupt", "an", "deins", "ge\u00b7lehnt", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "NN", "APPR", "NE", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nach dir und unsern Lindenb\u00e4umen", "tokens": ["Nach", "dir", "und", "un\u00b7sern", "Lin\u00b7den\u00b7b\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie oft hab' ich mich dort gesehnt!", "tokens": ["Wie", "oft", "hab'", "ich", "mich", "dort", "ge\u00b7sehnt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "PPER", "PRF", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}