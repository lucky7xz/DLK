{"dta.poem.11883": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "60.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Baum merkt nicht die Last, h\u00e4lt drauf ein Vogel Rast;", "tokens": ["Der", "Baum", "merkt", "nicht", "die", "Last", ",", "h\u00e4lt", "drauf", "ein", "Vo\u00b7gel", "Rast", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "ART", "NN", "$,", "VVFIN", "PAV", "ART", "NE", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch fliegt der Vogel weg, so schwankt davon der Ast.", "tokens": ["Doch", "fliegt", "der", "Vo\u00b7gel", "weg", ",", "so", "schwankt", "da\u00b7von", "der", "Ast", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$,", "ADV", "VVFIN", "PAV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "So f\u00fchlst du nicht die Lust, die wohnt in deiner Brust;", "tokens": ["So", "f\u00fchlst", "du", "nicht", "die", "Lust", ",", "die", "wohnt", "in", "dei\u00b7ner", "Brust", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "$,", "PRELS", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch wenn sie dir entfliegt, so f\u00fchlst du den Verlust.", "tokens": ["Doch", "wenn", "sie", "dir", "ent\u00b7fliegt", ",", "so", "f\u00fchlst", "du", "den", "Ver\u00b7lust", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "So merkt, was einer strebt, die Welt nicht, weil er lebt;", "tokens": ["So", "merkt", ",", "was", "ei\u00b7ner", "strebt", ",", "die", "Welt", "nicht", ",", "weil", "er", "lebt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PRELS", "PIS", "VVFIN", "$,", "ART", "NN", "PTKNEG", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie merkt es dann villeicht, wenn man den Mann begr\u00e4bt.", "tokens": ["Sie", "merkt", "es", "dann", "vil\u00b7leicht", ",", "wenn", "man", "den", "Mann", "be\u00b7gr\u00e4bt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "$,", "KOUS", "PIS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Der Zweig ersch\u00fcttert bebt dem Vogel, der entschwebt;", "tokens": ["Der", "Zweig", "er\u00b7sch\u00fct\u00b7tert", "bebt", "dem", "Vo\u00b7gel", ",", "der", "ent\u00b7schwebt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "VVFIN", "ART", "NE", "$,", "PRELS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Fest steht der Stamm, indes ein Zweig sich senkt und hebt.", "tokens": ["Fest", "steht", "der", "Stamm", ",", "in\u00b7des", "ein", "Zweig", "sich", "senkt", "und", "hebt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,", "ADV", "ART", "NN", "PRF", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}