{"textgrid.poem.61024": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "18.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es stand ein Veilchenstrau\u00df an meinem Bette,", "tokens": ["Es", "stand", "ein", "Veil\u00b7chen\u00b7strau\u00df", "an", "mei\u00b7nem", "Bet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der duftete mir zu gar s\u00fc\u00dfen Traum:", "tokens": ["Der", "duf\u00b7te\u00b7te", "mir", "zu", "gar", "s\u00fc\u00b7\u00dfen", "Traum", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich lag am Abhang einer H\u00fcgelkette,", "tokens": ["Ich", "lag", "am", "Ab\u00b7hang", "ei\u00b7ner", "H\u00fc\u00b7gel\u00b7ket\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und \u00fcberbl\u00fcht von Veilchen war der Raum:", "tokens": ["Und", "\u00fc\u00b7berb\u00b7l\u00fcht", "von", "Veil\u00b7chen", "war", "der", "Raum", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NE", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "So viele wuchsen nie an einer St\u00e4tte,", "tokens": ["So", "vie\u00b7le", "wuch\u00b7sen", "nie", "an", "ei\u00b7ner", "St\u00e4t\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Man sah vor ihrem Blau den Rasen kaum;", "tokens": ["Man", "sah", "vor", "ih\u00b7rem", "Blau", "den", "Ra\u00b7sen", "kaum", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Da sprach das Herz: Hier ging mein Lieb, das traute,", "tokens": ["Da", "sprach", "das", "Herz", ":", "Hier", "ging", "mein", "Lieb", ",", "das", "trau\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "ADV", "VVFIN", "PPOSAT", "NN", "$,", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und Veilchen spro\u00dften auf, wohin sie schaute.", "tokens": ["Und", "Veil\u00b7chen", "spro\u00df\u00b7ten", "auf", ",", "wo\u00b7hin", "sie", "schau\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PTKVZ", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}