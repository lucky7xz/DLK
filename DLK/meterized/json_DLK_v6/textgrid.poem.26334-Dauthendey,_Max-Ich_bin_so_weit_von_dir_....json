{"textgrid.poem.26334": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Ich bin so weit von dir ...", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Regenwolken rauschen.", "tokens": ["Die", "Re\u00b7gen\u00b7wol\u00b7ken", "rau\u00b7schen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich bin so weit, so weit von dir ...", "tokens": ["Ich", "bin", "so", "weit", ",", "so", "weit", "von", "dir", "..."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "ADV", "ADJD", "APPR", "PPER", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mu\u00df zu den Wolken lauschen,", "tokens": ["Mu\u00df", "zu", "den", "Wol\u00b7ken", "lau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "++-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie sprechen laut mit mir.", "tokens": ["Sie", "spre\u00b7chen", "laut", "mit", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sie und das Reisekissen,", "tokens": ["Sie", "und", "das", "Rei\u00b7se\u00b7kis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das deine Hand f\u00fcr mich gen\u00e4ht,", "tokens": ["Das", "dei\u00b7ne", "Hand", "f\u00fcr", "mich", "ge\u00b7n\u00e4ht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie fragen, ich soll's wissen,", "tokens": ["Sie", "fra\u00b7gen", ",", "ich", "soll's", "wis\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wann's wieder heimw\u00e4rts geht.", "tokens": ["Wann's", "wie\u00b7der", "heim\u00b7w\u00e4rts", "geht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Im Kissen meine Tr\u00e4nen,", "tokens": ["Im", "Kis\u00b7sen", "mei\u00b7ne", "Tr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die trocknen, ach, so schwer, so schwer.", "tokens": ["Die", "trock\u00b7nen", ",", "ach", ",", "so", "schwer", ",", "so", "schwer", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ITJ", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Luft ist voller Sehnen,", "tokens": ["Die", "Luft", "ist", "vol\u00b7ler", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die H\u00e4nde bleiben leer.", "tokens": ["Die", "H\u00e4n\u00b7de", "blei\u00b7ben", "leer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}