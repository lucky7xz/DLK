{"dta.poem.13598": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Terzett .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn von den Auen", "tokens": ["Wenn", "von", "den", "Au\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Die Fl\u00f6te singt,", "tokens": ["Die", "Fl\u00f6\u00b7te", "singt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Aus Waldesrauschen", "tokens": ["Aus", "Wal\u00b7des\u00b7rau\u00b7schen"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Das Horn erklingt,", "tokens": ["Das", "Horn", "er\u00b7klingt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Da steh' ich sinnend", "tokens": ["Da", "steh'", "ich", "sin\u00b7nend"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "VVPP"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Im Morgenlicht \u2014", "tokens": ["Im", "Mor\u00b7gen\u00b7licht"], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Wem ich soll folgen,", "tokens": ["Wem", "ich", "soll", "fol\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Ich wei\u00df es nicht.", "tokens": ["Ich", "wei\u00df", "es", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}