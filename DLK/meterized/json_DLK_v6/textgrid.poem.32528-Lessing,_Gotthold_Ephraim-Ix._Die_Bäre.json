{"textgrid.poem.32528": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "Ix. Die B\u00e4re", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Den B\u00e4ren gl\u00fcckt' es, nun schon seit geraumer Zeit,", "tokens": ["Den", "B\u00e4\u00b7ren", "gl\u00fcckt'", "es", ",", "nun", "schon", "seit", "ge\u00b7rau\u00b7mer", "Zeit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "ADV", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit Brummen, plumpem Ernst und stolzer Fr\u00f6mmigkeit,", "tokens": ["Mit", "Brum\u00b7men", ",", "plum\u00b7pem", "Ernst", "und", "stol\u00b7zer", "Fr\u00f6m\u00b7mig\u00b7keit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das Sittenrichteramt, bei allen schw\u00e4chern Tieren,", "tokens": ["Das", "Sit\u00b7ten\u00b7rich\u00b7ter\u00b7amt", ",", "bei", "al\u00b7len", "schw\u00e4\u00b7chern", "Tie\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Aus angema\u00dfter Macht, gleich W\u00fctrichen, zu f\u00fchren.", "tokens": ["Aus", "an\u00b7ge\u00b7ma\u00df\u00b7ter", "Macht", ",", "gleich", "W\u00fct\u00b7ri\u00b7chen", ",", "zu", "f\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADV", "NN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Ein jedes furchte sich, und keines war so k\u00fchn,", "tokens": ["Ein", "je\u00b7des", "furch\u00b7te", "sich", ",", "und", "kei\u00b7nes", "war", "so", "k\u00fchn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "PRF", "$,", "KON", "PIS", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Sich um die saure Pflicht nebst ihnen zu bem\u00fchn;", "tokens": ["Sich", "um", "die", "sau\u00b7re", "Pflicht", "nebst", "ih\u00b7nen", "zu", "be\u00b7m\u00fchn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "ADJA", "NN", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Bis endlich noch im Fuchs der Patriot erwachte,", "tokens": ["Bis", "end\u00b7lich", "noch", "im", "Fuchs", "der", "Pat\u00b7ri\u00b7ot", "er\u00b7wach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADV", "APPRART", "NE", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und hier und da ein Fuchs auf Sittenspr\u00fcche dachte.", "tokens": ["Und", "hier", "und", "da", "ein", "Fuchs", "auf", "Sit\u00b7ten\u00b7spr\u00fc\u00b7che", "dach\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KON", "ADV", "ART", "NE", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Nun sah man beide stets auf gleiche Zwecke sehn;", "tokens": ["Nun", "sah", "man", "bei\u00b7de", "stets", "auf", "glei\u00b7che", "Zwe\u00b7cke", "sehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PIS", "ADV", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und beide sah man doch verschiedne Wege gehn.", "tokens": ["Und", "bei\u00b7de", "sah", "man", "doch", "ver\u00b7schied\u00b7ne", "We\u00b7ge", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PIS", "ADV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die B\u00e4re wollen nur durch Strenge heilig machen;", "tokens": ["Die", "B\u00e4\u00b7re", "wol\u00b7len", "nur", "durch", "Stren\u00b7ge", "hei\u00b7lig", "ma\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "APPR", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die F\u00fcchse strafen auch, doch strafen sie mit Lachen.", "tokens": ["Die", "F\u00fcch\u00b7se", "stra\u00b7fen", "auch", ",", "doch", "stra\u00b7fen", "sie", "mit", "La\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,", "ADV", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Dort brauchet man nur Fluch, hier brauchet man nur Scherz;", "tokens": ["Dort", "brau\u00b7chet", "man", "nur", "Fluch", ",", "hier", "brau\u00b7chet", "man", "nur", "Scherz", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "NN", "$,", "ADV", "VVFIN", "PIS", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Dort bessert man den Schein; hier bessert man das Herz.", "tokens": ["Dort", "bes\u00b7sert", "man", "den", "Schein", ";", "hier", "bes\u00b7sert", "man", "das", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ART", "NN", "$.", "ADV", "VVFIN", "PIS", "ART", "NN", "$."], "meter": "--+--+---+-+", "measure": "anapaest.di.plus"}, "line.15": {"text": "Dort sieht man D\u00fcsternheit; hier sieht man Licht und Leben;", "tokens": ["Dort", "sieht", "man", "D\u00fcs\u00b7tern\u00b7heit", ";", "hier", "sieht", "man", "Licht", "und", "Le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "NN", "$.", "ADV", "VVFIN", "PIS", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Dort nach der Heuchelei; hier nach der Tugend streben.", "tokens": ["Dort", "nach", "der", "Heu\u00b7che\u00b7lei", ";", "hier", "nach", "der", "Tu\u00b7gend", "stre\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$.", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Du, der du weiter denkst, fragst du mich nicht geschwind:", "tokens": ["Du", ",", "der", "du", "wei\u00b7ter", "denkst", ",", "fragst", "du", "mich", "nicht", "ge\u00b7schwind", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,", "VVFIN", "PPER", "PRF", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Ob beide Teile wohl auch gute Freunde sind?", "tokens": ["Ob", "bei\u00b7de", "Tei\u00b7le", "wohl", "auch", "gu\u00b7te", "Freun\u00b7de", "sind", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "ADV", "ADV", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "O w\u00e4ren sies! Welch Gl\u00fcck f\u00fcr Tugend, Witz und Sitten!", "tokens": ["O", "w\u00e4\u00b7ren", "sies", "!", "Welch", "Gl\u00fcck", "f\u00fcr", "Tu\u00b7gend", ",", "Witz", "und", "Sit\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PIS", "$.", "PIAT", "NN", "APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Doch nein, der arme Fuchs wird von dem B\u00e4r bestritten,", "tokens": ["Doch", "nein", ",", "der", "ar\u00b7me", "Fuchs", "wird", "von", "dem", "B\u00e4r", "be\u00b7strit\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKANT", "$,", "ART", "ADJA", "NE", "VAFIN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Und, trotz des guten Zwecks, von ihm in Bann getan.", "tokens": ["Und", ",", "trotz", "des", "gu\u00b7ten", "Zwecks", ",", "von", "ihm", "in", "Bann", "ge\u00b7tan", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "ART", "ADJA", "NN", "$,", "APPR", "PPER", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Warum? der Fuchs greift selbst die B\u00e4re tadelnd an.", "tokens": ["Wa\u00b7rum", "?", "der", "Fuchs", "greift", "selbst", "die", "B\u00e4\u00b7re", "ta\u00b7delnd", "an", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "ART", "NE", "VVFIN", "ADV", "ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ich kann mich diesmal nicht bei der Moral verweilen;", "tokens": ["Ich", "kann", "mich", "dies\u00b7mal", "nicht", "bei", "der", "Mo\u00b7ral", "ver\u00b7wei\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "PTKNEG", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die f\u00fcnfte Stunde schl\u00e4gt; ich mu\u00df zum Schauplatz eilen.", "tokens": ["Die", "f\u00fcnf\u00b7te", "Stun\u00b7de", "schl\u00e4gt", ";", "ich", "mu\u00df", "zum", "Schau\u00b7platz", "ei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$.", "PPER", "VMFIN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Freund, leg die Predigt weg! Willst du nicht mit mir gehn?", "tokens": ["Freund", ",", "leg", "die", "Pre\u00b7digt", "weg", "!", "Willst", "du", "nicht", "mit", "mir", "gehn", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NE", "ART", "NN", "PTKVZ", "$.", "VMFIN", "PPER", "PTKNEG", "APPR", "PPER", "VVINF", "$."], "meter": "---+-+-+-+-+", "measure": "unknown.measure.penta"}}}}}