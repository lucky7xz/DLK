{"textgrid.poem.60513": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "1L: Tust du nur einen Saitengriff,", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tust du nur einen Saitengriff,", "tokens": ["Tust", "du", "nur", "ei\u00b7nen", "Sai\u00b7ten\u00b7griff", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So f\u00e4ngt der Hund zu heulen an;", "tokens": ["So", "f\u00e4ngt", "der", "Hund", "zu", "heu\u00b7len", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PTKZU", "VVINF", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df sie sein Ohr nicht feiner schliff,", "tokens": ["Da\u00df", "sie", "sein", "Ohr", "nicht", "fei\u00b7ner", "schliff", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "PTKNEG", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hat ihm die Sch\u00f6pfung angetan;", "tokens": ["Hat", "ihm", "die", "Sch\u00f6p\u00b7fung", "an\u00b7ge\u00b7tan", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Drum, wenn dein Lied die Sch\u00f6pfung preist,", "tokens": ["Drum", ",", "wenn", "dein", "Lied", "die", "Sch\u00f6p\u00b7fung", "preist", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "KOUS", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gib acht, da\u00df dich der Hund nicht bei\u00dft.", "tokens": ["Gib", "acht", ",", "da\u00df", "dich", "der", "Hund", "nicht", "bei\u00dft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "CARD", "$,", "KOUS", "PPER", "ART", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}