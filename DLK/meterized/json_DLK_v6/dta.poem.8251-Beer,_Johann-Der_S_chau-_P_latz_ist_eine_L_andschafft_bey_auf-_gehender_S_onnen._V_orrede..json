{"dta.poem.8251": {"metadata": {"author": {"name": "Beer, Johann", "birth": "N.A.", "death": "N.A."}, "title": "Der  S chau- P latz ist eine  L andschafft bey auf-  \n gehender  S onnen.  \n  V orrede.", "genre": "Lyrik, Libretto", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-25222-5", "language": ["de:0.99"], "booktitle": "Beer, Johann: Antonius R\u00f6mischer Triumvir. Wei\u00dfenfels, 1697."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn mein Schwerdt die Leichen streuet/", "tokens": ["Wenn", "mein", "Schwerdt", "die", "Lei\u00b7chen", "streu\u00b7et", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das Gesch\u00fctz nur Feuer speyet/", "tokens": ["Das", "Ge\u00b7sch\u00fctz", "nur", "Feu\u00b7er", "spey\u00b7et", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "NN", "VVFIN", "$("], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Alles donnert/ kracht/ und blitzt;", "tokens": ["Al\u00b7les", "don\u00b7nert", "/", "kracht", "/", "und", "blitzt", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$(", "VVFIN", "$(", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mu\u00df das Gl\u00fcck vor mir erzittern/", "tokens": ["Mu\u00df", "das", "Gl\u00fcck", "vor", "mir", "er\u00b7zit\u00b7tern", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "APPR", "PPER", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und die gantze Welt ersch\u00fcttern/", "tokens": ["Und", "die", "gant\u00b7ze", "Welt", "er\u00b7sch\u00fct\u00b7tern", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die in meinen Flammen schwitzt.", "tokens": ["Die", "in", "mei\u00b7nen", "Flam\u00b7men", "schwitzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}