{"textgrid.poem.36368": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Siegeslieder h\u00f6rt' ich singen", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Siegeslieder h\u00f6rt' ich singen", "tokens": ["Sie\u00b7ges\u00b7lie\u00b7der", "h\u00f6rt'", "ich", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In den Gauen weit und breit;", "tokens": ["In", "den", "Gau\u00b7en", "weit", "und", "breit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Unsers Volkes Ruhm erklingen", "tokens": ["Un\u00b7sers", "Vol\u00b7kes", "Ruhm", "er\u00b7klin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In dem Spiel der Eitelkeit.", "tokens": ["In", "dem", "Spiel", "der", "Ei\u00b7tel\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Haltet ein, bet\u00f6rte Lieder!", "tokens": ["Hal\u00b7tet", "ein", ",", "be\u00b7t\u00f6r\u00b7te", "Lie\u00b7der", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Gottes Flammen leuchten wieder", "tokens": ["Got\u00b7tes", "Flam\u00b7men", "leuch\u00b7ten", "wie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "NN", "VVFIN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "In das dunkle Meer der Zeit.", "tokens": ["In", "das", "dunk\u00b7le", "Meer", "der", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sind die Dolche denn Befreier,", "tokens": ["Sind", "die", "Dol\u00b7che", "denn", "Be\u00b7frei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Selbst der eignen bangen Brust?", "tokens": ["Selbst", "der", "eig\u00b7nen", "ban\u00b7gen", "Brust", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Werdet frei erst, wahrhaft freier,", "tokens": ["Wer\u00b7det", "frei", "erst", ",", "wahr\u00b7haft", "frei\u00b7er", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADV", "$,", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Innen Gottes euch bewu\u00dft!", "tokens": ["In\u00b7nen", "Got\u00b7tes", "euch", "be\u00b7wu\u00dft", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Werft vor Seiner Kraft euch wieder,", "tokens": ["Werft", "vor", "Sei\u00b7ner", "Kraft", "euch", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "PPER", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Vor dem ew'gen Rechte nieder;", "tokens": ["Vor", "dem", "ew'\u00b7gen", "Rech\u00b7te", "nie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Dann genie\u00dft der Ehre Lust!", "tokens": ["Dann", "ge\u00b7nie\u00dft", "der", "Eh\u00b7re", "Lust", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und ihr andern wollt beschw\u00f6ren", "tokens": ["Und", "ihr", "an\u00b7dern", "wollt", "be\u00b7schw\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "PIS", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch ein k\u00fcnstlich Nichts den Sturm?", "tokens": ["Durch", "ein", "k\u00fcnst\u00b7lich", "Nichts", "den", "Sturm", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "PIS", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wen kann solch Geweb' abwehren,", "tokens": ["Wen", "kann", "solch", "Ge\u00b7web'", "ab\u00b7weh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PIAT", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Selbst zernagt vom L\u00fcgenwurm?", "tokens": ["Selbst", "zer\u00b7nagt", "vom", "L\u00fc\u00b7gen\u00b7wurm", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Was nicht Gott erbaut, mu\u00df fallen;", "tokens": ["Was", "nicht", "Gott", "er\u00b7baut", ",", "mu\u00df", "fal\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "NN", "VVPP", "$,", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Also ruft die Stimm' uns allen,", "tokens": ["Al\u00b7so", "ruft", "die", "Stimm'", "uns", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "PIS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Nieder st\u00fcrzt der Babelturm.", "tokens": ["Nie\u00b7der", "st\u00fcrzt", "der", "Ba\u00b7bel\u00b7turm", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Fruchtet nichts mehr unser Beten,", "tokens": ["Fruch\u00b7tet", "nichts", "mehr", "un\u00b7ser", "Be\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schlie\u00dfest Du der Gnade Born?", "tokens": ["Schlie\u00b7\u00dfest", "Du", "der", "Gna\u00b7de", "Born", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Willst die blut'ge Kelter treten,", "tokens": ["Willst", "die", "blut'\u00b7ge", "Kel\u00b7ter", "tre\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Herr, in des Gerichtes Zorn?", "tokens": ["Herr", ",", "in", "des", "Ge\u00b7rich\u00b7tes", "Zorn", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Kommt der Heil'ge auf den Rossen,", "tokens": ["Kommt", "der", "Heil'\u00b7ge", "auf", "den", "Ros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Siegreich in des Worts Geschossen;", "tokens": ["Sieg\u00b7reich", "in", "des", "Worts", "Ge\u00b7schos\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Schallt der Welt das Todeshorn?", "tokens": ["Schallt", "der", "Welt", "das", "To\u00b7des\u00b7horn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Werfen wir ans Herz dem Vater", "tokens": ["Wer\u00b7fen", "wir", "ans", "Herz", "dem", "Va\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "All die Schreckensorge nur;", "tokens": ["All", "die", "Schre\u00b7cken\u00b7sor\u00b7ge", "nur", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df sein Licht uns dien' als Rater", "tokens": ["Da\u00df", "sein", "Licht", "uns", "dien'", "als", "Ra\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "PDS", "KOUS", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und sein Wort zur Lebensspur!", "tokens": ["Und", "sein", "Wort", "zur", "Le\u00b7bens\u00b7spur", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Es vergehn noch Sternentage,", "tokens": ["Es", "ver\u00b7gehn", "noch", "Ster\u00b7nen\u00b7ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und Jahrhunderte voll Klage,", "tokens": ["Und", "Jahr\u00b7hun\u00b7der\u00b7te", "voll", "Kla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJD", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Eh' verkl\u00e4rt wird diese Flur.", "tokens": ["Eh'", "ver\u00b7kl\u00e4rt", "wird", "die\u00b7se", "Flur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "VAFIN", "PDAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Wenn d\u00e4monische Gewalten", "tokens": ["Wenn", "d\u00e4\u00b7mo\u00b7ni\u00b7sche", "Ge\u00b7wal\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Greifen an der V\u00f6lker Herz;", "tokens": ["Grei\u00b7fen", "an", "der", "V\u00f6l\u00b7ker", "Herz", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbwie l\u00e4\u00dft Gott sie also schalten?\u00ab", "tokens": ["\u00bb", "wie", "l\u00e4\u00dft", "Gott", "sie", "al\u00b7so", "schal\u00b7ten", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWAV", "VVFIN", "NN", "PPER", "ADV", "VVFIN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Klagen wir dann himmelw\u00e4rts.", "tokens": ["Kla\u00b7gen", "wir", "dann", "him\u00b7mel\u00b7w\u00e4rts", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ADV", "$."], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.5": {"text": "Soll sich neu die Welt gestalten,", "tokens": ["Soll", "sich", "neu", "die", "Welt", "ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "ADJD", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "L\u00e4\u00dft er frei das B\u00f6se walten,", "tokens": ["L\u00e4\u00dft", "er", "frei", "das", "B\u00f6\u00b7se", "wal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Bis das Licht entsteigt dem Schmerz.", "tokens": ["Bis", "das", "Licht", "ent\u00b7steigt", "dem", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Fluten seh' ich furchtbar rauschen", "tokens": ["Flu\u00b7ten", "seh'", "ich", "furcht\u00b7bar", "rau\u00b7schen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00dcber Fluten auf uns her;", "tokens": ["\u00dc\u00b7ber", "Flu\u00b7ten", "auf", "uns", "her", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "L\u00fcg' und Mord den Szepter tauschen,", "tokens": ["L\u00fcg'", "und", "Mord", "den", "Szep\u00b7ter", "tau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ein allblutig wildes Meer.", "tokens": ["Ein", "all\u00b7blu\u00b7tig", "wil\u00b7des", "Meer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Niemand mag sich widerstemmen,", "tokens": ["Nie\u00b7mand", "mag", "sich", "wi\u00b7ders\u00b7tem\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Keiner die Zerst\u00f6rung hemmen;", "tokens": ["Kei\u00b7ner", "die", "Zer\u00b7st\u00f6\u00b7rung", "hem\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Gott allein ist hier die Wehr.", "tokens": ["Gott", "al\u00b7lein", "ist", "hier", "die", "Wehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Auf dem Meer doch haucht und lebet", "tokens": ["Auf", "dem", "Meer", "doch", "haucht", "und", "le\u00b7bet"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADV", "VVFIN", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der das Licht dem Tod entrei\u00dft;", "tokens": ["Der", "das", "Licht", "dem", "Tod", "ent\u00b7rei\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ob der Verwesung schwebet", "tokens": ["Und", "ob", "der", "Ver\u00b7we\u00b7sung", "schwe\u00b7bet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Gottes ew'ger Lebensgeist.", "tokens": ["Got\u00b7tes", "ew'\u00b7ger", "Le\u00b7bens\u00b7geist", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Also wird ein lichter Morgen", "tokens": ["Al\u00b7so", "wird", "ein", "lich\u00b7ter", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Brechen durch der Menschen Sorgen,", "tokens": ["Bre\u00b7chen", "durch", "der", "Men\u00b7schen", "Sor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wie der Strahl der Schrift verhei\u00dft.", "tokens": ["Wie", "der", "Strahl", "der", "Schrift", "ver\u00b7hei\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Als den Mann des Todes weckte", "tokens": ["Als", "den", "Mann", "des", "To\u00b7des", "weck\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einst der Heiland aus dem Graus,", "tokens": ["Einst", "der", "Hei\u00b7land", "aus", "dem", "Graus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo den Leichnam Moder dec