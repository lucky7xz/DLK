{"dta.poem.8476": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Seufzer.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1783", "urn": "urn:nbn:de:kobv:b4-200905192568", "language": ["de:0.99"], "booktitle": "H\u00f6lty, Ludwig Christoph Heinrich: Gedichte. Hamburg, 1783."}, "poem": {"stanza.1": {"line.1": {"text": "Singt \u00fcberall", "tokens": ["Singt", "\u00fc\u00b7be\u00b7rall"], "token_info": ["word", "word"], "pos": ["VVFIN", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Auf gr\u00fcnen Reisen", "tokens": ["Auf", "gr\u00fc\u00b7nen", "Rei\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Die besten Weisen,", "tokens": ["Die", "bes\u00b7ten", "Wei\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Dass ringsum Wald", "tokens": ["Dass", "ring\u00b7sum", "Wald"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ADV", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Und Ufer schallt.", "tokens": ["Und", "U\u00b7fer", "schallt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Manch junges Paar", "tokens": ["Manch", "jun\u00b7ges", "Paar"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Geht dort, wo klar", "tokens": ["Geht", "dort", ",", "wo", "klar"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ADV", "$,", "PWAV", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Das B\u00e4chlein rauschet,", "tokens": ["Das", "B\u00e4ch\u00b7lein", "rau\u00b7schet", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Und steht, und lauschet", "tokens": ["Und", "steht", ",", "und", "lau\u00b7schet"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "$,", "KON", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Mit frohem Sinn", "tokens": ["Mit", "fro\u00b7hem", "Sinn"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Der S\u00e4ngerin.", "tokens": ["Der", "S\u00e4n\u00b7ge\u00b7rin", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Ich h\u00f6re bang'", "tokens": ["Ich", "h\u00f6\u00b7re", "bang'"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "NE"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Im d\u00fcstern Gang", "tokens": ["Im", "d\u00fcs\u00b7tern", "Gang"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Der Nachtigallen", "tokens": ["Der", "Nach\u00b7ti\u00b7gal\u00b7len"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Ges\u00e4nge schallen;", "tokens": ["Ge\u00b7s\u00e4n\u00b7ge", "schal\u00b7len", ";"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Denn ach! allein", "tokens": ["Denn", "ach", "!", "al\u00b7lein"], "token_info": ["word", "word", "punct", "word"], "pos": ["KON", "XY", "$.", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Irr' ich im Hain.", "tokens": ["Irr'", "ich", "im", "Hain", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}