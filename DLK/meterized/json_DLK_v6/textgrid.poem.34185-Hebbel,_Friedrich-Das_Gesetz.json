{"textgrid.poem.34185": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Das Gesetz", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was ich will vom Gesetz? Es soll das H\u00f6chste verlangen,", "tokens": ["Was", "ich", "will", "vom", "Ge\u00b7setz", "?", "Es", "soll", "das", "H\u00f6chs\u00b7te", "ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VMFIN", "APPRART", "NN", "$.", "PPER", "VMFIN", "ART", "ADJA", "VVINF", "$,"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Was der Beste vermag, wenn er die Kraft nur gebraucht.", "tokens": ["Was", "der", "Bes\u00b7te", "ver\u00b7mag", ",", "wenn", "er", "die", "Kraft", "nur", "ge\u00b7braucht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "ADV", "VVPP", "$."], "meter": "--+--+---+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "So besch\u00fctzt es die Welt vor'm B\u00f6sen und steht auch dem Guten", "tokens": ["So", "be\u00b7sch\u00fctzt", "es", "die", "Welt", "vor'm", "B\u00f6\u00b7sen", "und", "steht", "auch", "dem", "Gu\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "KON", "VVFIN", "ADV", "ART", "NN"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Gegen sich selber bei, wenn ihn die Stunde versucht.", "tokens": ["Ge\u00b7gen", "sich", "sel\u00b7ber", "bei", ",", "wenn", "ihn", "die", "Stun\u00b7de", "ver\u00b7sucht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ADV", "PTKVZ", "$,", "KOUS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}}}}}