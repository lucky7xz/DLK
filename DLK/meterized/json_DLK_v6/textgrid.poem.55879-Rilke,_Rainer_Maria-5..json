{"textgrid.poem.55879": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der blasse Zar wird nicht am Schwerte sterben,", "tokens": ["Der", "blas\u00b7se", "Zar", "wird", "nicht", "am", "Schwer\u00b7te", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PTKNEG", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "die fremde Sehnsucht macht ihn sakrosankt;", "tokens": ["die", "frem\u00b7de", "Sehn\u00b7sucht", "macht", "ihn", "sak\u00b7ro\u00b7sankt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "er wird die feierlichen Reiche erben,", "tokens": ["er", "wird", "die", "fei\u00b7er\u00b7li\u00b7chen", "Rei\u00b7che", "er\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NE", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "an denen seine sanfte Seele krankt.", "tokens": ["an", "de\u00b7nen", "sei\u00b7ne", "sanf\u00b7te", "See\u00b7le", "krankt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Schon jetzt, hintretend an ein Kremlfenster,", "tokens": ["Schon", "jetzt", ",", "hin\u00b7tre\u00b7tend", "an", "ein", "Kreml\u00b7fens\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.2": {"text": "sieht er ein Moskau, wei\u00dfer, unbegrenzter,", "tokens": ["sieht", "er", "ein", "Mos\u00b7kau", ",", "wei\u00b7\u00dfer", ",", "un\u00b7be\u00b7grenz\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NE", "$,", "ADJA", "$,", "ADJD", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "in seine endlich fertige Nacht gewebt;", "tokens": ["in", "sei\u00b7ne", "end\u00b7lich", "fer\u00b7ti\u00b7ge", "Nacht", "ge\u00b7webt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADV", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "so wie es ist im ersten Fr\u00fchlingswirken,", "tokens": ["so", "wie", "es", "ist", "im", "ers\u00b7ten", "Fr\u00fch\u00b7lings\u00b7wir\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PPER", "VAFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "wenn in den Gassen der Geruch aus Birken", "tokens": ["wenn", "in", "den", "Gas\u00b7sen", "der", "Ge\u00b7ruch", "aus", "Bir\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "von lauter Morgenglocken bebt.", "tokens": ["von", "lau\u00b7ter", "Mor\u00b7gen\u00b7glo\u00b7cken", "bebt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die gro\u00dfen Glocken, die so herrisch lauten,", "tokens": ["Die", "gro\u00b7\u00dfen", "Glo\u00b7cken", ",", "die", "so", "her\u00b7risch", "lau\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "sind seine V\u00e4ter, jene ersten Zaren,", "tokens": ["sind", "sei\u00b7ne", "V\u00e4\u00b7ter", ",", "je\u00b7ne", "ers\u00b7ten", "Za\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$,", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "die sich noch vor den Tagen der Tataren", "tokens": ["die", "sich", "noch", "vor", "den", "Ta\u00b7gen", "der", "Ta\u00b7ta\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PRELS", "PRF", "ADV", "APPR", "ART", "NN", "ART", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "aus Sagen, Abenteuern und Gefahren,", "tokens": ["aus", "Sa\u00b7gen", ",", "A\u00b7bent\u00b7eu\u00b7ern", "und", "Ge\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "aus Zorn und Demut z\u00f6gernd auferbauten.", "tokens": ["aus", "Zorn", "und", "De\u00b7mut", "z\u00f6\u00b7gernd", "au\u00b7fer\u00b7bau\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADJD", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und er begreift auf einmal, wer sie waren,", "tokens": ["Und", "er", "be\u00b7greift", "auf", "ein\u00b7mal", ",", "wer", "sie", "wa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ADV", "$,", "PWS", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "und da\u00df sie oft um ihres Dunkels Sinn", "tokens": ["und", "da\u00df", "sie", "oft", "um", "ih\u00b7res", "Dun\u00b7kels", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "in ", "tokens": ["in"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "und ihn, den Leisesten von den Erlauchten,", "tokens": ["und", "ihn", ",", "den", "Lei\u00b7ses\u00b7ten", "von", "den", "Er\u00b7lauch\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "in ihren Taten gro\u00df und fromm verbrauchten", "tokens": ["in", "ih\u00b7ren", "Ta\u00b7ten", "gro\u00df", "und", "fromm", "ver\u00b7brauch\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "KON", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "schon lang vor seinem Anbeginn.", "tokens": ["schon", "lang", "vor", "sei\u00b7nem", "An\u00b7be\u00b7ginn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und eine Dankbarkeit kommt \u00fcber ihn,", "tokens": ["Und", "ei\u00b7ne", "Dank\u00b7bar\u00b7keit", "kommt", "\u00fc\u00b7ber", "ihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "da\u00df sie ihn so verschwenderisch vergeben", "tokens": ["da\u00df", "sie", "ihn", "so", "ver\u00b7schwen\u00b7de\u00b7risch", "ver\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ADJD", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "an aller Dinge Durst und Drang.", "tokens": ["an", "al\u00b7ler", "Din\u00b7ge", "Durst", "und", "Drang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er war die Kraft zu ihrem \u00dcberschwang,", "tokens": ["Er", "war", "die", "Kraft", "zu", "ih\u00b7rem", "\u00dc\u00b7bersc\u00b7hwang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "der goldne Grund, vor dem ihr breites Leben", "tokens": ["der", "gold\u00b7ne", "Grund", ",", "vor", "dem", "ihr", "brei\u00b7tes", "Le\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "ART", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "geheimnisvoll zu dunkeln schien.", "tokens": ["ge\u00b7heim\u00b7nis\u00b7voll", "zu", "dun\u00b7keln", "schien", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "In allen ihren Werken schaut er ", "tokens": ["In", "al\u00b7len", "ih\u00b7ren", "Wer\u00b7ken", "schaut", "er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "PPOSAT", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "wie eingelegtes Silber in Zieraten,", "tokens": ["wie", "ein\u00b7ge\u00b7leg\u00b7tes", "Sil\u00b7ber", "in", "Zie\u00b7ra\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "und es giebt keine Tat in ihren Taten,", "tokens": ["und", "es", "giebt", "kei\u00b7ne", "Tat", "in", "ih\u00b7ren", "Ta\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PIAT", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "die nicht auch ", "tokens": ["die", "nicht", "auch"], "token_info": ["word", "word", "word"], "pos": ["ART", "PTKNEG", "ADV"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "in denen alles Handelns Rot verblich.", "tokens": ["in", "de\u00b7nen", "al\u00b7les", "Han\u00b7delns", "Rot", "ver\u00b7blich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PIAT", "NN", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}