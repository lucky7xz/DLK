{"textgrid.poem.34673": {"metadata": {"author": {"name": "Neukirch, Benjamin", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ist es m\u00f6glich, da\u00df du weinest?", "genre": "verse", "period": "N.A.", "pub_year": 1697, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ist es m\u00f6glich, da\u00df du weinest?", "tokens": ["Ist", "es", "m\u00f6g\u00b7lich", ",", "da\u00df", "du", "wei\u00b7nest", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist es m\u00f6glich, da\u00df du meinest,", "tokens": ["Ist", "es", "m\u00f6g\u00b7lich", ",", "da\u00df", "du", "mei\u00b7nest", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df ich dich verlassen kann?", "tokens": ["Da\u00df", "ich", "dich", "ver\u00b7las\u00b7sen", "kann", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ei, so schaue doch von fernen", "tokens": ["Ei", ",", "so", "schau\u00b7e", "doch", "von", "fer\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADV", "VVFIN", "ADV", "APPR", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Auch einmal bei andern Sternen", "tokens": ["Auch", "ein\u00b7mal", "bei", "an\u00b7dern", "Ster\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ADJA", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "Dich, o Sonne, selber an!", "tokens": ["Dich", ",", "o", "Son\u00b7ne", ",", "sel\u00b7ber", "an", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "FM", "NN", "$,", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Dein erhobnes Angesichte", "tokens": ["Dein", "er\u00b7hob\u00b7nes", "An\u00b7ge\u00b7sich\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Machet allen Glanz zu nichte,", "tokens": ["Ma\u00b7chet", "al\u00b7len", "Glanz", "zu", "nich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "APPR", "PIS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Der die blinde Welt beth\u00f6rt.", "tokens": ["Der", "die", "blin\u00b7de", "Welt", "be\u00b7th\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Phyllis selbst mu\u00df mit Betr\u00fcben", "tokens": ["Phyl\u00b7lis", "selbst", "mu\u00df", "mit", "Be\u00b7tr\u00fc\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "VMFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sich in deinen Mund verlieben,", "tokens": ["Sich", "in", "dei\u00b7nen", "Mund", "ver\u00b7lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wenn sie deine Lieder h\u00f6rt.", "tokens": ["Wenn", "sie", "dei\u00b7ne", "Lie\u00b7der", "h\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Alle junge Sch\u00e4ferinnen", "tokens": ["Al\u00b7le", "jun\u00b7ge", "Sch\u00e4\u00b7fe\u00b7rin\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fliehen ohne Ha\u00df", "tokens": ["Flie\u00b7hen", "oh\u00b7ne", "Ha\u00df"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPR", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Wenn sie dich, o Wunder, sehn.", "tokens": ["Wenn", "sie", "dich", ",", "o", "Wun\u00b7der", ",", "sehn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "$,", "FM", "NN", "$,", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Zephyr steigt aus seinen H\u00f6hlen,", "tokens": ["Ze\u00b7phyr", "steigt", "aus", "sei\u00b7nen", "H\u00f6h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Deinen Busen", "tokens": ["Dei\u00b7nen", "Bu\u00b7sen"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Und von neuem anzuwehn.", "tokens": ["Und", "von", "neu\u00b7em", "an\u00b7zu\u00b7wehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "VVIZU", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Venus wird von Zorn entz\u00fcndet,", "tokens": ["Ve\u00b7nus", "wird", "von", "Zorn", "ent\u00b7z\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weil sie Alles an dir findet,", "tokens": ["Weil", "sie", "Al\u00b7les", "an", "dir", "fin\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Was ihr sonst allein geb\u00fchrt;", "tokens": ["Was", "ihr", "sonst", "al\u00b7lein", "ge\u00b7b\u00fchrt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie sollt' ich denn, sch\u00f6nste Nymphe,", "tokens": ["Wie", "sollt'", "ich", "denn", ",", "sch\u00f6ns\u00b7te", "Nym\u00b7phe", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Dir zu Trotz und mir zum Schimpfe", "tokens": ["Dir", "zu", "Trotz", "und", "mir", "zum", "Schimp\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "NN", "KON", "PPER", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Hassen, was die G\u00f6tter r\u00fchrt?", "tokens": ["Has\u00b7sen", ",", "was", "die", "G\u00f6t\u00b7ter", "r\u00fchrt", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Denke selbst, ich bin getrieben;", "tokens": ["Den\u00b7ke", "selbst", ",", "ich", "bin", "ge\u00b7trie\u00b7ben", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "PPER", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ich will, soll und mu\u00df dich lieben,", "tokens": ["Ich", "will", ",", "soll", "und", "mu\u00df", "dich", "lie\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "VMFIN", "KON", "VMFIN", "PRF", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Nichts rei\u00dft meinen Vorsatz ein.", "tokens": ["Nichts", "rei\u00dft", "mei\u00b7nen", "Vor\u00b7satz", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn was du einmal geboren,", "tokens": ["Denn", "was", "du", "ein\u00b7mal", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "VVPP", "$,"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.5": {"text": "Mu\u00df, geht Alles gleich verloren,", "tokens": ["Mu\u00df", ",", "geht", "Al\u00b7les", "gleich", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "$,", "VVFIN", "PIS", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dennoch unverg\u00e4nglich sein.", "tokens": ["Den\u00b7noch", "un\u00b7ver\u00b7g\u00e4ng\u00b7lich", "sein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}