{"textgrid.poem.37258": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "[der alte F\u00f6rster P\u00fcsterich]", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der alte F\u00f6rster P\u00fcsterich", "tokens": ["Der", "al\u00b7te", "F\u00f6rs\u00b7ter", "P\u00fcs\u00b7te\u00b7rich"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der ging nach langer Pause", "tokens": ["Der", "ging", "nach", "lan\u00b7ger", "Pau\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mal wieder auf den Schnepfenstrich", "tokens": ["Mal", "wie\u00b7der", "auf", "den", "Schnep\u00b7fen\u00b7strich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und brachte auch eine nach Hause.", "tokens": ["Und", "brach\u00b7te", "auch", "ei\u00b7ne", "nach", "Hau\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "APPR", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Als er sie nun gebraten h\u00e4tt,", "tokens": ["Als", "er", "sie", "nun", "ge\u00b7bra\u00b7ten", "h\u00e4tt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da t\u00e4t ihn was verdreu\u00dfen;", "tokens": ["Da", "t\u00e4t", "ihn", "was", "ver\u00b7dreu\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das Tierlein roch wie sonst so nett,", "tokens": ["Das", "Tier\u00b7lein", "roch", "wie", "sonst", "so", "nett", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KOKOM", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nur konnt er's nicht recht mehr bei\u00dfen.", "tokens": ["Nur", "konnt", "er's", "nicht", "recht", "mehr", "bei\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "PTKNEG", "ADV", "ADV", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Ach ja! So seufzt er wehgemut", "tokens": ["Ach", "ja", "!", "So", "seufzt", "er", "weh\u00b7ge\u00b7mut"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "ITJ", "$.", "ADV", "VVFIN", "PPER", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und wischt sich ab die Tr\u00e4ne,", "tokens": ["Und", "wischt", "sich", "ab", "die", "Tr\u00e4\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Nase w\u00e4r so weit noch gut,", "tokens": ["Die", "Na\u00b7se", "w\u00e4r", "so", "weit", "noch", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nur blo\u00df, es fehlen die Z\u00e4hne.", "tokens": ["Nur", "blo\u00df", ",", "es", "feh\u00b7len", "die", "Z\u00e4h\u00b7ne", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}