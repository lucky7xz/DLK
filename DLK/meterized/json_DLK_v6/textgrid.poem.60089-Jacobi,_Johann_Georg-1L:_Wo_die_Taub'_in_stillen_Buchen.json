{"textgrid.poem.60089": {"metadata": {"author": {"name": "Jacobi, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wo die Taub' in stillen Buchen", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo die Taub' in stillen Buchen", "tokens": ["Wo", "die", "Taub'", "in", "stil\u00b7len", "Bu\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihren Tauber sich erw\u00e4hlt,", "tokens": ["Ih\u00b7ren", "Tau\u00b7ber", "sich", "er\u00b7w\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PRF", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo sich Nachtigallen suchen,", "tokens": ["Wo", "sich", "Nach\u00b7ti\u00b7gal\u00b7len", "su\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die Rebe sich verm\u00e4hlt;", "tokens": ["Und", "die", "Re\u00b7be", "sich", "ver\u00b7m\u00e4hlt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PRF", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wo die B\u00e4che sich vereinen,", "tokens": ["Wo", "die", "B\u00e4\u00b7che", "sich", "ver\u00b7ei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ging ich oft mit leichtem Scherz,", "tokens": ["Ging", "ich", "oft", "mit", "leich\u00b7tem", "Scherz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ging ich oft mit bangem Weinen,", "tokens": ["Ging", "ich", "oft", "mit", "ban\u00b7gem", "Wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Suchte mir ein liebend Herz.", "tokens": ["Such\u00b7te", "mir", "ein", "lie\u00b7bend", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "O, da gab die finstre Laube", "tokens": ["O", ",", "da", "gab", "die", "finst\u00b7re", "Lau\u00b7be"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Leisen Trost im Abendschein;", "tokens": ["Lei\u00b7sen", "Trost", "im", "A\u00b7bend\u00b7schein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "O, da kam ein s\u00fc\u00dfer Glaube", "tokens": ["O", ",", "da", "kam", "ein", "s\u00fc\u00b7\u00dfer", "Glau\u00b7be"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit dem Morgenglanz im Hain;", "tokens": ["Mit", "dem", "Mor\u00b7gen\u00b7glanz", "im", "Hain", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da vernahm ich's in den Winden,", "tokens": ["Da", "ver\u00b7nahm", "ich's", "in", "den", "Win\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ihr Gefl\u00fcster lehrte mich:", "tokens": ["Ihr", "Ge\u00b7fl\u00fcs\u00b7ter", "lehr\u00b7te", "mich", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Da\u00df ich suchen sollt', und finden,", "tokens": ["Da\u00df", "ich", "su\u00b7chen", "sollt'", ",", "und", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "VMFIN", "$,", "KON", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Finden, holde Liebe! dich.", "tokens": ["Fin\u00b7den", ",", "hol\u00b7de", "Lie\u00b7be", "!", "dich", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$.", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Aber ach! wo blieb auf Erden,", "tokens": ["A\u00b7ber", "ach", "!", "wo", "blieb", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$.", "PWAV", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Holde Liebe, deine Spur?", "tokens": ["Hol\u00b7de", "Lie\u00b7be", ",", "dei\u00b7ne", "Spur", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Lieben, um geliebt zu werden,", "tokens": ["Lie\u00b7ben", ",", "um", "ge\u00b7liebt", "zu", "wer\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "$,", "KOUI", "VVPP", "PTKZU", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist das Loos der Engel nur.", "tokens": ["Ist", "das", "Loos", "der", "En\u00b7gel", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Statt der Wonne fand' ich Schmerzen,", "tokens": ["Statt", "der", "Won\u00b7ne", "fand'", "ich", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Hing an dem, was mich verlie\u00df;", "tokens": ["Hing", "an", "dem", ",", "was", "mich", "ver\u00b7lie\u00df", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Frieden gibt den treuen Herzen", "tokens": ["Frie\u00b7den", "gibt", "den", "treu\u00b7en", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Nur ein k\u00fcnftig Paradies.", "tokens": ["Nur", "ein", "k\u00fcnf\u00b7tig", "Pa\u00b7ra\u00b7dies", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}