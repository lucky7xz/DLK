{"textgrid.poem.36396": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Sonett", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was will das bunte Tageslicht mir geben?", "tokens": ["Was", "will", "das", "bun\u00b7te", "Ta\u00b7ges\u00b7licht", "mir", "ge\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "ADJA", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den tr\u00fcben Blick verwunden seine Farben;", "tokens": ["Den", "tr\u00fc\u00b7ben", "Blick", "ver\u00b7wun\u00b7den", "sei\u00b7ne", "Far\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nur denen bl\u00fchend, so im Herzen starben,", "tokens": ["Nur", "de\u00b7nen", "bl\u00fc\u00b7hend", ",", "so", "im", "Her\u00b7zen", "star\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "VVPP", "$,", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den muntern Augen, die dem Leben leben.", "tokens": ["Den", "mun\u00b7tern", "Au\u00b7gen", ",", "die", "dem", "Le\u00b7ben", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Bis mich die Zeit zum Tode wird erheben,", "tokens": ["Bis", "mich", "die", "Zeit", "zum", "To\u00b7de", "wird", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "APPRART", "NN", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Lieb' ich im Leben nichts, als seine Narben,", "tokens": ["Lieb'", "ich", "im", "Le\u00b7ben", "nichts", ",", "als", "sei\u00b7ne", "Nar\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PIS", "$,", "KOUS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Weil sie die Lust zum Leben mir verdarben", "tokens": ["Weil", "sie", "die", "Lust", "zum", "Le\u00b7ben", "mir", "ver\u00b7dar\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "APPRART", "NN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und ewig wehren nach dem Nichts zu streben.", "tokens": ["Und", "e\u00b7wig", "weh\u00b7ren", "nach", "dem", "Nichts", "zu", "stre\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVINF", "APPR", "ART", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Im Dunkel nur, das alles zart verkleidet,", "tokens": ["Im", "Dun\u00b7kel", "nur", ",", "das", "al\u00b7les", "zart", "ver\u00b7klei\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "$,", "PRELS", "PIS", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ergetzt mein Auge sich, und mein Verlangen", "tokens": ["Er\u00b7getzt", "mein", "Au\u00b7ge", "sich", ",", "und", "mein", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "PRF", "$,", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Entz\u00fcnden sch\u00f6ner Liebe goldne Fernen.", "tokens": ["Ent\u00b7z\u00fcn\u00b7den", "sch\u00f6\u00b7ner", "Lie\u00b7be", "gold\u00b7ne", "Fer\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Des Todes Bild, das helle Laute meidet,", "tokens": ["Des", "To\u00b7des", "Bild", ",", "das", "hel\u00b7le", "Lau\u00b7te", "mei\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "K\u00f6mmt freundlich in der stillen Nacht gegangen,", "tokens": ["K\u00f6mmt", "freund\u00b7lich", "in", "der", "stil\u00b7len", "Nacht", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Umfunkelt von des Himmels ew'gen Sternen.", "tokens": ["Um\u00b7fun\u00b7kelt", "von", "des", "Him\u00b7mels", "ew'\u00b7gen", "Ster\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}