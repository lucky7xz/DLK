{"textgrid.poem.30403": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "17.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da Nummosus sterben solte, lieff er auff den Ober-S\u00f6ller;", "tokens": ["Da", "Num\u00b7mo\u00b7sus", "ster\u00b7ben", "sol\u00b7te", ",", "lieff", "er", "auff", "den", "O\u00b7ber\u00b7S\u00f6l\u00b7ler", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "VVINF", "VMFIN", "$,", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Da Bibosus sterben solte, lieff er nunter in den Keller;", "tokens": ["Da", "Bi\u00b7bo\u00b7sus", "ster\u00b7ben", "sol\u00b7te", ",", "lieff", "er", "nun\u00b7ter", "in", "den", "Kel\u00b7ler", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVINF", "VMFIN", "$,", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Doch den schwartzen Knochen-Mann hilt nicht auff noch hoch, noch tieff,", "tokens": ["Doch", "den", "schwart\u00b7zen", "Kno\u00b7chen\u00b7Mann", "hilt", "nicht", "auff", "noch", "hoch", ",", "noch", "tieff", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "PTKNEG", "APPR", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "--+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Da\u00df er beyden nicht hinnach, di\u00df er sie erhaschte, lieff.", "tokens": ["Da\u00df", "er", "bey\u00b7den", "nicht", "hin\u00b7nach", ",", "di\u00df", "er", "sie", "er\u00b7haschte", ",", "lieff", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "PTKNEG", "ADV", "$,", "PDS", "PPER", "PPER", "VVFIN", "$,", "VVFIN", "$."], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}}}}}