{"dta.poem.14782": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der Reitersmann .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Der hat so lieblich gelogen", "tokens": ["Der", "hat", "so", "lieb\u00b7lich", "ge\u00b7lo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ADV", "ADJD", "VVPP"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Mich aus der Treue heraus,", "tokens": ["Mich", "aus", "der", "Treu\u00b7e", "he\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Der Falsche hat mich betrogen,", "tokens": ["Der", "Fal\u00b7sche", "hat", "mich", "be\u00b7tro\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Zog weit in die Welt hinaus.\u201c \u2014", "tokens": ["Zog", "weit", "in", "die", "Welt", "hin\u00b7aus", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN", "PTKVZ", "$.", "$(", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}