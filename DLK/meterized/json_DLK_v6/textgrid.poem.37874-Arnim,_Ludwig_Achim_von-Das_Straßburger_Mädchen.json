{"textgrid.poem.37874": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Das Stra\u00dfburger M\u00e4dchen", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es trug das schwarzbraun M\u00e4delein", "tokens": ["Es", "trug", "das", "schwarz\u00b7braun", "M\u00e4\u00b7de\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Viel Becher rothen Wein,", "tokens": ["Viel", "Be\u00b7cher", "ro\u00b7then", "Wein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zu Stra\u00dfburg auf der Stra\u00dfen,", "tokens": ["Zu", "Stra\u00df\u00b7burg", "auf", "der", "Stra\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Begegnet ihr allda", "tokens": ["Be\u00b7geg\u00b7net", "ihr", "all\u00b7da"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "PAV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ein wundersch\u00f6ner Knab,", "tokens": ["Ein", "wun\u00b7der\u00b7sch\u00f6\u00b7ner", "Knab", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Er thut sie wohl anfassen.", "tokens": ["Er", "thut", "sie", "wohl", "an\u00b7fas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "\u00bbla\u00df ab, la\u00df ab, ey lasse ab,", "tokens": ["\u00bb", "la\u00df", "ab", ",", "la\u00df", "ab", ",", "ey", "las\u00b7se", "ab", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PTKVZ", "$,", "VVIMP", "PTKVZ", "$,", "NE", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein wundersch\u00f6ner Knab,", "tokens": ["Mein", "wun\u00b7der\u00b7sch\u00f6\u00b7ner", "Knab", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mein M\u00fctterlein thut schelten,", "tokens": ["Mein", "M\u00fct\u00b7ter\u00b7lein", "thut", "schel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Versch\u00fctte ich den Wein,", "tokens": ["Ver\u00b7sch\u00fct\u00b7te", "ich", "den", "Wein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Den rothen k\u00fchlen Wein,", "tokens": ["Den", "ro\u00b7then", "k\u00fch\u00b7len", "Wein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Der Wein thut sehr viel gelten.\u00ab", "tokens": ["Der", "Wein", "thut", "sehr", "viel", "gel\u00b7ten", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Bald hat das schwarzbraun M\u00e4delein,", "tokens": ["Bald", "hat", "das", "schwarz\u00b7braun", "M\u00e4\u00b7de\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verloren ihr Pant\u00f6ffelein,", "tokens": ["Ver\u00b7lo\u00b7ren", "ihr", "Pan\u00b7t\u00f6f\u00b7fe\u00b7lein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Sie kanns nicht wieder finden,", "tokens": ["Sie", "kanns", "nicht", "wie\u00b7der", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie suchet hin, sie suchet her,", "tokens": ["Sie", "su\u00b7chet", "hin", ",", "sie", "su\u00b7chet", "her", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Verliere nicht den andern mehr,", "tokens": ["Ver\u00b7lie\u00b7re", "nicht", "den", "an\u00b7dern", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "ADJA", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Noch unter dieser Linde.", "tokens": ["Noch", "un\u00b7ter", "die\u00b7ser", "Lin\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PDAT", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Denn zwischen zwey Berg und tiefe Thal,", "tokens": ["Denn", "zwi\u00b7schen", "zwey", "Berg", "und", "tie\u00b7fe", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "CARD", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ins gr\u00fcne ebne Thal,", "tokens": ["Ins", "gr\u00fc\u00b7ne", "eb\u00b7ne", "Thal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da flie\u00dft ein schiffreich Wasser,", "tokens": ["Da", "flie\u00dft", "ein", "schiff\u00b7reich", "Was\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wer sein Feinslieb nicht will,", "tokens": ["Wer", "sein", "Feins\u00b7lieb", "nicht", "will", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "PTKNEG", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wen sein Feinslieb nicht will,", "tokens": ["Wen", "sein", "Feins\u00b7lieb", "nicht", "will", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "PTKNEG", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Die m\u00fcssen sich fahren lassen.", "tokens": ["Die", "m\u00fcs\u00b7sen", "sich", "fah\u00b7ren", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PRF", "VVINF", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}