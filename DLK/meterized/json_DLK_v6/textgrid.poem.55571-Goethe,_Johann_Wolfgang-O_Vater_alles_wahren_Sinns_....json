{"textgrid.poem.55571": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "O Vater alles wahren Sinns ...", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Vater alles wahren Sinns", "tokens": ["O", "Va\u00b7ter", "al\u00b7les", "wah\u00b7ren", "Sinns"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NN", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und des gesunden Lebens,", "tokens": ["Und", "des", "ge\u00b7sun\u00b7den", "Le\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du Geber k\u00f6stlichen Gewinns,", "tokens": ["Du", "Ge\u00b7ber", "k\u00f6st\u00b7li\u00b7chen", "Ge\u00b7winns", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du F\u00f6rdrer treuen Strebens,", "tokens": ["Du", "F\u00f6rd\u00b7rer", "treu\u00b7en", "Stre\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sprich in mein Herz dein leises Wort,", "tokens": ["Sprich", "in", "mein", "Herz", "dein", "lei\u00b7ses", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "PPOSAT", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bewahre mich so fort und fort", "tokens": ["Be\u00b7wah\u00b7re", "mich", "so", "fort", "und", "fort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "PTKVZ", "KON", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "F\u00fcr Heuchlern und f\u00fcr Huren!", "tokens": ["F\u00fcr", "Heuch\u00b7lern", "und", "f\u00fcr", "Hu\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}