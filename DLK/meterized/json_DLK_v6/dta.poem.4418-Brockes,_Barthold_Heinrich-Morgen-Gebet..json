{"dta.poem.4418": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Morgen-Gebet.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1727", "urn": "urn:nbn:de:kobv:b4-200905198599", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "OgOTT, Der Du durch Deine Macht", "tokens": ["O\u00b7gOTT", ",", "Der", "Du", "durch", "Dei\u00b7ne", "Macht"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "ART", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die\u00df grosse Rund geschaffen,", "tokens": ["Die\u00df", "gros\u00b7se", "Rund", "ge\u00b7schaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich habe diese ganze Nacht", "tokens": ["Ich", "ha\u00b7be", "die\u00b7se", "gan\u00b7ze", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So ruhig k\u00f6nnen schlafen;", "tokens": ["So", "ru\u00b7hig", "k\u00f6n\u00b7nen", "schla\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ich bin vergn\u00fcget aufgewacht,", "tokens": ["Ich", "bin", "ver\u00b7gn\u00fc\u00b7get", "auf\u00b7ge\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gesund und ohne Schmerzen;", "tokens": ["Ge\u00b7sund", "und", "oh\u00b7ne", "Schmer\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ich seh der g\u00fcld\u2019nen Sonne Pracht,", "tokens": ["Ich", "seh", "der", "g\u00fcld'\u00b7nen", "Son\u00b7ne", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "De\u00df dank\u2019 ich Dir von Herzen.", "tokens": ["De\u00df", "dank'", "ich", "Dir", "von", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Gib, da\u00df ich diesen ganzen Tag", "tokens": ["Gib", ",", "da\u00df", "ich", "die\u00b7sen", "gan\u00b7zen", "Tag"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "$,", "KOUS", "PPER", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Nach Deinem Willen leben mag!", "tokens": ["Nach", "Dei\u00b7nem", "Wil\u00b7len", "le\u00b7ben", "mag", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Hilf, da\u00df mein Fu\u00df sonst nirgends ruh\u2019,", "tokens": ["Hilf", ",", "da\u00df", "mein", "Fu\u00df", "sonst", "nir\u00b7gends", "ruh'", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "PPOSAT", "NN", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Als nur auf Deinen Wegen,", "tokens": ["Als", "nur", "auf", "Dei\u00b7nen", "We\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Und gib zu allem, was ich thu\u2019,", "tokens": ["Und", "gib", "zu", "al\u00b7lem", ",", "was", "ich", "thu'", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "APPR", "PIS", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Aus Gnaden Deinen Segen!", "tokens": ["Aus", "Gna\u00b7den", "Dei\u00b7nen", "Se\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}