{"textgrid.poem.47919": {"metadata": {"author": {"name": "Klaj, Johann", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Liebe/ nicht der Knab/ der lange Zeit verk\u00fcrtzet/", "genre": "verse", "period": "N.A.", "pub_year": 1636, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Liebe/ nicht der Knab/ der lange Zeit verk\u00fcrtzet/", "tokens": ["Die", "Lie\u00b7be", "/", "nicht", "der", "Knab", "/", "der", "lan\u00b7ge", "Zeit", "ver\u00b7k\u00fcrt\u00b7zet", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PTKNEG", "ART", "NN", "$(", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "dem Blinden weist den Weg/ und seine Folger st\u00fcrtzet", "tokens": ["dem", "Blin\u00b7den", "weist", "den", "Weg", "/", "und", "sei\u00b7ne", "Fol\u00b7ger", "st\u00fcrt\u00b7zet"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$(", "KON", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "in S\u00fcnd und H\u00f6llenpful/ ich sage/ Gottes Lieb'", "tokens": ["in", "S\u00fcnd", "und", "H\u00f6l\u00b7len\u00b7pful", "/", "ich", "sa\u00b7ge", "/", "Got\u00b7tes", "Lieb'"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "$(", "PPER", "VVFIN", "$(", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "hatt' in dem harten Hertz de\u00df Menscher schwachen Trieb.", "tokens": ["hatt'", "in", "dem", "har\u00b7ten", "Hertz", "de\u00df", "Men\u00b7scher", "schwa\u00b7chen", "Trieb", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "ADJA", "NN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die h\u00f6nigs\u00fcssen Wort/ die wunderreichen Lehren", "tokens": ["Die", "h\u00f6\u00b7nig\u00b7s\u00fcs\u00b7sen", "Wort", "/", "die", "wun\u00b7der\u00b7rei\u00b7chen", "Leh\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "vermochten nicht das Hertz zu weichen und zu kehren.", "tokens": ["ver\u00b7moch\u00b7ten", "nicht", "das", "Hertz", "zu", "wei\u00b7chen", "und", "zu", "keh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "NN", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der Hammer de\u00df Gesetz f\u00fchrt einen starcken Schlag/", "tokens": ["Der", "Ham\u00b7mer", "de\u00df", "Ge\u00b7setz", "f\u00fchrt", "ei\u00b7nen", "star\u00b7cken", "Schlag", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "doch blieb' es unverruckt in steter S\u00fcndenplag'.", "tokens": ["doch", "blieb'", "es", "un\u00b7ver\u00b7ruckt", "in", "ste\u00b7ter", "S\u00fcn\u00b7den\u00b7plag'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Als diese Himmelslieb erstaunt/ nichts unterlassen/", "tokens": ["Als", "die\u00b7se", "Him\u00b7mels\u00b7lieb", "er\u00b7staunt", "/", "nichts", "un\u00b7ter\u00b7las\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "VVPP", "$(", "PIS", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "das diamantne Hertz auf manche Weis zu fassen/", "tokens": ["das", "di\u00b7a\u00b7mant\u00b7ne", "Hertz", "auf", "man\u00b7che", "Weis", "zu", "fas\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PIAT", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "hat sie das wilde Fleisch in Drechsel eingespannt/", "tokens": ["hat", "sie", "das", "wil\u00b7de", "Fleisch", "in", "Drech\u00b7sel", "ein\u00b7ge\u00b7spannt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "und von der b\u00f6sen Welt zu seinem Gott gewandt.", "tokens": ["und", "von", "der", "b\u00f6\u00b7sen", "Welt", "zu", "sei\u00b7nem", "Gott", "ge\u00b7wandt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Das Creutz/ Rohr/ N\u00e4gel/ Band/ die Geissel/ Seul und Ruten", "tokens": ["Das", "Creutz", "/", "Rohr", "/", "N\u00e4\u00b7gel", "/", "Band", "/", "die", "Geis\u00b7sel", "/", "Seul", "und", "Ru\u00b7ten"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$(", "NN", "$(", "NN", "$(", "NN", "$(", "ART", "NN", "$(", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "dient zu der Drechselbank/ und macht das Hertze bluten/", "tokens": ["dient", "zu", "der", "Drech\u00b7sel\u00b7bank", "/", "und", "macht", "das", "Hert\u00b7ze", "blu\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$(", "KON", "VVFIN", "PDS", "VVFIN", "VVFIN", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.15": {"text": "das Creutz treibt/ dz das Rohr sich an den Band\u1ebd schwingt/", "tokens": ["das", "Creutz", "treibt", "/", "dz", "das", "Rohr", "sich", "an", "den", "Ban\u00b7d\u1ebd", "schwingt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$(", "KOUS", "ART", "NN", "PRF", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "und da\u00df der scharffe Speer tieff in das Hertze dringt.", "tokens": ["und", "da\u00df", "der", "scharf\u00b7fe", "Speer", "tieff", "in", "das", "Hert\u00b7ze", "dringt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "ADJD", "APPR", "PDS", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Was irrig/ irdisch ist/ mu\u00df auf die Erde fallen/", "tokens": ["Was", "ir\u00b7rig", "/", "ir\u00b7disch", "ist", "/", "mu\u00df", "auf", "die", "Er\u00b7de", "fal\u00b7len", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "$(", "ADJD", "VAFIN", "$(", "VMFIN", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "was geist- und himmlisch ist/ gef\u00e4llet Gott vor allen.", "tokens": ["was", "geist", "und", "himm\u00b7lisch", "ist", "/", "ge\u00b7f\u00e4l\u00b7let", "Gott", "vor", "al\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "TRUNC", "KON", "ADJD", "VAFIN", "$(", "VVPP", "NN", "APPR", "PIAT", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Der wolgespitzte Speer geht durch das harte Hertz/", "tokens": ["Der", "wol\u00b7ge\u00b7spitz\u00b7te", "Speer", "geht", "durch", "das", "har\u00b7te", "Hertz", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "er bringt in Schmertzen Freud' unn in der Freude Schmertz.", "tokens": ["er", "bringt", "in", "Schmert\u00b7zen", "Freud'", "unn", "in", "der", "Freu\u00b7de", "Schmertz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "NN", "ADJD", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Er l\u00e4st die Vberschrifft de\u00df Creutzes so bemerket/", "tokens": ["Er", "l\u00e4st", "die", "Vber\u00b7schrifft", "de\u00df", "Creut\u00b7zes", "so", "be\u00b7mer\u00b7ket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "ADV", "VVFIN", "$("], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.22": {"text": "da\u00df es beh\u00e4glich wird ", "tokens": ["da\u00df", "es", "be\u00b7h\u00e4g\u00b7lich", "wird"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.23": {"text": "In Iesus Namen Ritz Ist unser Wunden heil/", "tokens": ["In", "Ie\u00b7sus", "Na\u00b7men", "Ritz", "Ist", "un\u00b7ser", "Wun\u00b7den", "heil", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "NE", "VAFIN", "PPOSAT", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Er ist de\u00df Glaubens Grund und unser St\u00e4rke Seul'.", "tokens": ["Er", "ist", "de\u00df", "Glau\u00b7bens", "Grund", "und", "un\u00b7ser", "St\u00e4r\u00b7ke", "Seul'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "KON", "PPOSAT", "NN", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Er ist/ der uns betr\u00fcbt/ und unsre Hertzen pr\u00fcft/", "tokens": ["Er", "ist", "/", "der", "uns", "be\u00b7tr\u00fcbt", "/", "und", "uns\u00b7re", "Hert\u00b7zen", "pr\u00fcft", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$(", "PRELS", "PPER", "VVPP", "$(", "KON", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "auf uns sein theures Blut au\u00df grosser Liebe trieft.", "tokens": ["auf", "uns", "sein", "theu\u00b7res", "Blut", "au\u00df", "gros\u00b7ser", "Lie\u00b7be", "trieft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PPOSAT", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Gib Jesu/ da\u00df mein Hertzstets denk an deine Pein/", "tokens": ["Gib", "Je\u00b7su", "/", "da\u00df", "mein", "Hertz\u00b7stets", "denk", "an", "dei\u00b7ne", "Pein", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NE", "$(", "KOUS", "PPOSAT", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "so wird mir alles Leid voll Freud' ertr\u00e4glich seyn!", "tokens": ["so", "wird", "mir", "al\u00b7les", "Leid", "voll", "Freud'", "er\u00b7tr\u00e4g\u00b7lich", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIAT", "NN", "ADJD", "NN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}