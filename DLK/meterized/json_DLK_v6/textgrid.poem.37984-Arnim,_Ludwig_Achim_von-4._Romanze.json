{"textgrid.poem.37984": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "4. Romanze", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Zierlichste meinte ein jeder zu seyn,", "tokens": ["Der", "Zier\u00b7lichs\u00b7te", "mein\u00b7te", "ein", "je\u00b7der", "zu", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "PIS", "PTKZU", "VAINF", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Der Stauffenberger zog auch ein,", "tokens": ["Der", "Stauf\u00b7fen\u00b7ber\u00b7ger", "zog", "auch", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Seins Gleichen war zugegen nicht,", "tokens": ["Seins", "Glei\u00b7chen", "war", "zu\u00b7ge\u00b7gen", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der so zierlich einher ritt,", "tokens": ["Der", "so", "zier\u00b7lich", "ein\u00b7her", "ritt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "ADJD", "VVFIN", "$,"], "meter": "+-+-++-", "measure": "unknown.measure.tetra"}, "line.5": {"text": "Der K\u00f6nig nahm sein eben wahr,", "tokens": ["Der", "K\u00f6\u00b7nig", "nahm", "sein", "e\u00b7ben", "wahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dazu die Frauen ernsthaft gar.", "tokens": ["Da\u00b7zu", "die", "Frau\u00b7en", "ernst\u00b7haft", "gar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "ADJD", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Trommeten fingen an zu blasen,", "tokens": ["Trom\u00b7me\u00b7ten", "fin\u00b7gen", "an", "zu", "bla\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Die Pferde fingen an zu tosen,", "tokens": ["Die", "Pfer\u00b7de", "fin\u00b7gen", "an", "zu", "to\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da lustig ward so Ro\u00df als Mann,", "tokens": ["Da", "lus\u00b7tig", "ward", "so", "Ro\u00df", "als", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ADV", "NN", "KOUS", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie das Turnier gefangen an,", "tokens": ["Wie", "das", "Tur\u00b7nier", "ge\u00b7fan\u00b7gen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJD", "PTKVZ", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Herr Peter alle darnieder rennt,", "tokens": ["Herr", "Pe\u00b7ter", "al\u00b7le", "dar\u00b7nie\u00b7der", "rennt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "PIS", "PAV", "VVFIN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.6": {"text": "Er macht dem Rennen bald ein End.", "tokens": ["Er", "macht", "dem", "Ren\u00b7nen", "bald", "ein", "End", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Als nun der Abend kam herbey,", "tokens": ["Als", "nun", "der", "A\u00b7bend", "kam", "her\u00b7bey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von neuem ging Trommetenschrey,", "tokens": ["Von", "neu\u00b7em", "ging", "Trom\u00b7me\u00b7ten\u00b7schrey", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als sie zu Hof gegessen hatten,", "tokens": ["Als", "sie", "zu", "Hof", "ge\u00b7ges\u00b7sen", "hat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den f\u00fcrstlichen Tanz sie allda thaten,", "tokens": ["Den", "f\u00fcrst\u00b7li\u00b7chen", "Tanz", "sie", "all\u00b7da", "tha\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Des K\u00f6nigs Base sch\u00f6n geziert,", "tokens": ["Des", "K\u00f6\u00b7nigs", "Ba\u00b7se", "sch\u00f6n", "ge\u00b7ziert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den ersten Dank in Handen f\u00fchrt.", "tokens": ["Den", "ers\u00b7ten", "Dank", "in", "Han\u00b7den", "f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Von Gold und Perlen diesen Kranz,", "tokens": ["Von", "Gold", "und", "Per\u00b7len", "die\u00b7sen", "Kranz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem Ritter sezt sie auf zum Tanz,", "tokens": ["Dem", "Rit\u00b7ter", "sezt", "sie", "auf", "zum", "Tanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Th\u00e4t auf das gelbe Haar ihm setzen,", "tokens": ["Th\u00e4t", "auf", "das", "gel\u00b7be", "Haar", "ihm", "set\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Th\u00e4t freundlich ihm den Finger pfetzen,", "tokens": ["Th\u00e4t", "freund\u00b7lich", "ihm", "den", "Fin\u00b7ger", "pfet\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Gab ihre Lieb ihm zu verstehn,", "tokens": ["Gab", "ih\u00b7re", "Lieb", "ihm", "zu", "ver\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Durch manchen Blick sch\u00f6n anzusehn.", "tokens": ["Durch", "man\u00b7chen", "Blick", "sch\u00f6n", "an\u00b7zu\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADJD", "VVIZU", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}