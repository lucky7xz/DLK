{"textgrid.poem.56402": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "23", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heim kamen die Farren,", "tokens": ["Heim", "ka\u00b7men", "die", "Far\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Die goldgeh\u00f6rnten,", "tokens": ["Die", "gold\u00b7ge\u00b7h\u00f6rn\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "ADJA", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Die schwarzen Rinder,", "tokens": ["Die", "schwar\u00b7zen", "Rin\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Dem Riesen zur Lust:", "tokens": ["Dem", "Rie\u00b7sen", "zur", "Lust", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "\u00bbhabe der Sch\u00e4tze viel,", "tokens": ["\u00bb", "ha\u00b7be", "der", "Sch\u00e4t\u00b7ze", "viel", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "ART", "NN", "ADV", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "Habe der Spangen viel,", "tokens": ["Ha\u00b7be", "der", "Span\u00b7gen", "viel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Fehlte mir Fraya", "tokens": ["Fehl\u00b7te", "mir", "Fray\u00b7a"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "NE"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Zu freien annoch.\u00ab", "tokens": ["Zu", "frei\u00b7en", "an\u00b7noch", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPR", "NE", "ADV", "$.", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}