{"dta.poem.4097": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Die liebe Farbe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1821", "urn": "urn:nbn:de:kobv:b4-200905194209", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In gr\u00fcne Thr\u00e4nenweiden,", "tokens": ["In", "gr\u00fc\u00b7ne", "Thr\u00e4\u00b7nen\u00b7wei\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mein Schatz hat's Gr\u00fcn so gern.", "tokens": ["Mein", "Schatz", "hat's", "Gr\u00fcn", "so", "gern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Will suchen einen Cypressenhain,", "tokens": ["Will", "su\u00b7chen", "ei\u00b7nen", "Cyp\u00b7res\u00b7sen\u00b7hain", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Eine Haide von gr\u00fcnem Rosmarein,", "tokens": ["Ei\u00b7ne", "Hai\u00b7de", "von", "gr\u00fc\u00b7nem", "Ros\u00b7ma\u00b7rein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Mein Schatz hat's Gr\u00fcn so gern.", "tokens": ["Mein", "Schatz", "hat's", "Gr\u00fcn", "so", "gern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wohlauf zum fr\u00f6hlichen Jagen!", "tokens": ["Wohl\u00b7auf", "zum", "fr\u00f6h\u00b7li\u00b7chen", "Ja\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wohlauf durch Haid' und Hagen!", "tokens": ["Wohl\u00b7auf", "durch", "Haid'", "und", "Ha\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mein Schatz hat's Jagen so gern.", "tokens": ["Mein", "Schatz", "hat's", "Ja\u00b7gen", "so", "gern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "ADV", "ADV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Das Wild, das ich jage, das ist der Tod,", "tokens": ["Das", "Wild", ",", "das", "ich", "ja\u00b7ge", ",", "das", "ist", "der", "Tod", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "PDS", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die Haide, die hei\u00df' ich die Liebesnoth,", "tokens": ["Die", "Hai\u00b7de", ",", "die", "hei\u00df'", "ich", "die", "Lie\u00b7bes\u00b7noth", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$,", "PRELS", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Mein Schatz hat's Jagen so gern.", "tokens": ["Mein", "Schatz", "hat's", "Ja\u00b7gen", "so", "gern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "ADV", "ADV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Grabt mir ein Grab im Wasen,", "tokens": ["Grabt", "mir", "ein", "Grab", "im", "Wa\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Deckt mich mit gr\u00fcnem Rasen,", "tokens": ["Deckt", "mich", "mit", "gr\u00fc\u00b7nem", "Ra\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Mein Schatz hat's Gr\u00fcn so gern.", "tokens": ["Mein", "Schatz", "hat's", "Gr\u00fcn", "so", "gern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Kein Kreuzlein schwarz, kein Bl\u00fcmlein bunt,", "tokens": ["Kein", "Kreuz\u00b7lein", "schwarz", ",", "kein", "Bl\u00fcm\u00b7lein", "bunt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJD", "$,", "PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Gr\u00fcn, Alles gr\u00fcn so rings und rund!", "tokens": ["Gr\u00fcn", ",", "Al\u00b7les", "gr\u00fcn", "so", "rings", "und", "rund", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PIS", "ADJD", "ADV", "ADV", "KON", "ADJD", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Mein Schatz hat's Gr\u00fcn so gern.", "tokens": ["Mein", "Schatz", "hat's", "Gr\u00fcn", "so", "gern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}