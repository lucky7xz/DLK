{"textgrid.poem.43787": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Auf dem Eis", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun wohlan, so la\u00dft uns wandern", "tokens": ["Nun", "wo\u00b7hlan", ",", "so", "la\u00dft", "uns", "wan\u00b7dern"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "ADV", "VVIMP", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nach dem Eis!", "tokens": ["Nach", "dem", "Eis", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "La\u00dft uns flink und fr\u00f6hlich glandern,", "tokens": ["La\u00dft", "uns", "flink", "und", "fr\u00f6h\u00b7lich", "glan\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "KON", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Immer einer nach dem andern", "tokens": ["Im\u00b7mer", "ei\u00b7ner", "nach", "dem", "an\u00b7dern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Auf dem Eis!", "tokens": ["Auf", "dem", "Eis", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Und was gleichet dem Vergn\u00fcgen", "tokens": ["Und", "was", "glei\u00b7chet", "dem", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf dem Eis,", "tokens": ["Auf", "dem", "Eis", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Wenn wir uns zusammen f\u00fcgen,", "tokens": ["Wenn", "wir", "uns", "zu\u00b7sam\u00b7men", "f\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Glandern dann in langen Z\u00fcgen", "tokens": ["Glan\u00b7dern", "dann", "in", "lan\u00b7gen", "Z\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Auf dem Eis?", "tokens": ["Auf", "dem", "Eis", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "F\u00e4llst du etwa mal danieder", "tokens": ["F\u00e4llst", "du", "et\u00b7wa", "mal", "da\u00b7nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "PAV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf dem Eis,", "tokens": ["Auf", "dem", "Eis", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Denke nicht an deine Glieder,", "tokens": ["Den\u00b7ke", "nicht", "an", "dei\u00b7ne", "Glie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKNEG", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Flink steh' auf und glandre wieder", "tokens": ["Flink", "steh'", "auf", "und", "gland\u00b7re", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PTKVZ", "KON", "VVFIN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Auf dem Eis!", "tokens": ["Auf", "dem", "Eis", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "Doch geschickt vor allen Dingen", "tokens": ["Doch", "ge\u00b7schickt", "vor", "al\u00b7len", "Din\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf dem Eis!", "tokens": ["Auf", "dem", "Eis", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "So nur kann es uns gelingen,", "tokens": ["So", "nur", "kann", "es", "uns", "ge\u00b7lin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Froh zu glandern und zu singen", "tokens": ["Froh", "zu", "glan\u00b7dern", "und", "zu", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Auf dem Eis!", "tokens": ["Auf", "dem", "Eis", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}