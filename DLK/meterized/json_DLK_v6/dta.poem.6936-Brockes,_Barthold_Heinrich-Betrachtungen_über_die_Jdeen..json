{"dta.poem.6936": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtungen \u00fcber die Jdeen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mit unterschiedner Gleichmaa\u00df hat", "tokens": ["Mit", "un\u00b7ter\u00b7schied\u00b7ner", "Gleich\u00b7maa\u00df", "hat"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der SCHOEPFER das, was c\u00f6rperlich, gef\u00fcget;", "tokens": ["Der", "ScHOEP\u00b7FER", "das", ",", "was", "c\u00f6r\u00b7per\u00b7lich", ",", "ge\u00b7f\u00fc\u00b7get", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "PDS", "$,", "PRELS", "ADJD", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Daher in ihnen, in der That,", "tokens": ["Da\u00b7her", "in", "ih\u00b7nen", ",", "in", "der", "That", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "PPER", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nichts als die Form und das Bewegen,", "tokens": ["Nichts", "als", "die", "Form", "und", "das", "Be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "KOKOM", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Vereinigung und Trennung lieget,", "tokens": ["Ver\u00b7ei\u00b7ni\u00b7gung", "und", "Tren\u00b7nung", "lie\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Durch welche sie den Eindruck blos erregen.", "tokens": ["Durch", "wel\u00b7che", "sie", "den", "Ein\u00b7druck", "blos", "er\u00b7re\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Der abgetheilte Geist hingegen", "tokens": ["Der", "ab\u00b7ge\u00b7theil\u00b7te", "Geist", "hin\u00b7ge\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Kan nichts, als blos allein ein rein", "tokens": ["Kan", "nichts", ",", "als", "blos", "al\u00b7lein", "ein", "rein"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PIS", "$,", "KOUS", "ADV", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Unc\u00f6rperliches Seyn,", "tokens": ["Un\u00b7c\u00f6r\u00b7per\u00b7li\u00b7ches", "Seyn", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.10": {"text": "Begriff, Verst\u00e4ndni\u00df und Jdeen,", "tokens": ["Be\u00b7griff", ",", "Ver\u00b7st\u00e4nd\u00b7ni\u00df", "und", "Jdeen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.11": {"text": "Sammt einem reiffen Uberlegen,", "tokens": ["Sammt", "ei\u00b7nem", "reif\u00b7fen", "U\u00b7ber\u00b7le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "In seinem Wesen sehen.", "tokens": ["In", "sei\u00b7nem", "We\u00b7sen", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}