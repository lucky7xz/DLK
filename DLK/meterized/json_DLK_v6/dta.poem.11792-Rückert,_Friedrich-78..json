{"dta.poem.11792": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "78.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Vernimm, der ewigen Natur vier Elemente,", "tokens": ["Ver\u00b7nimm", ",", "der", "e\u00b7wi\u00b7gen", "Na\u00b7tur", "vier", "E\u00b7le\u00b7men\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "ART", "ADJA", "NN", "CARD", "NN", "$,"], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wie in dir selbst sie sind als vier Temperamente.", "tokens": ["Wie", "in", "dir", "selbst", "sie", "sind", "als", "vier", "Tem\u00b7pe\u00b7ra\u00b7men\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "PPER", "ADV", "PPER", "VAFIN", "KOKOM", "CARD", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Das erste Element, die Luft, lind-ungelind,", "tokens": ["Das", "ers\u00b7te", "E\u00b7le\u00b7ment", ",", "die", "Luft", ",", "lin\u00b7dun\u00b7ge\u00b7lind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bald sanfter Hauch in dir, bald ungest\u00fcmer Wind.", "tokens": ["Bald", "sanf\u00b7ter", "Hauch", "in", "dir", ",", "bald", "un\u00b7ge\u00b7st\u00fc\u00b7mer", "Wind", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "NN", "APPR", "PPER", "$,", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Das zweite Element, das Wasser, ist geboren", "tokens": ["Das", "zwei\u00b7te", "E\u00b7le\u00b7ment", ",", "das", "Was\u00b7ser", ",", "ist", "ge\u00b7bo\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "$,", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bald fl\u00fcssiger Kristall in dir, bald Eis gefroren.", "tokens": ["Bald", "fl\u00fcs\u00b7si\u00b7ger", "Kris\u00b7tall", "in", "dir", ",", "bald", "Eis", "ge\u00b7fro\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "APPR", "PPER", "$,", "ADV", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Das dritte Element, das Licht und Feuer hei\u00dft,", "tokens": ["Das", "drit\u00b7te", "E\u00b7le\u00b7ment", ",", "das", "Licht", "und", "Feu\u00b7er", "hei\u00dft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist ebenso in dir Licht- oder Feuergeist.", "tokens": ["Ist", "e\u00b7ben\u00b7so", "in", "dir", "Licht", "o\u00b7der", "Feu\u00b7er\u00b7geist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PPER", "TRUNC", "KON", "NN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Das vierte Element, der andern Grund, die Erde,", "tokens": ["Das", "vier\u00b7te", "E\u00b7le\u00b7ment", ",", "der", "an\u00b7dern", "Grund", ",", "die", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Will da\u00df sie Schwerkraft bald in dir, bald Tr\u00e4gheit werde.", "tokens": ["Will", "da\u00df", "sie", "Schwer\u00b7kraft", "bald", "in", "dir", ",", "bald", "Tr\u00e4g\u00b7heit", "wer\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "KOUS", "PPER", "NN", "ADV", "APPR", "PPER", "$,", "ADV", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Wie die vier Element' in sich zwiefaltig sind,", "tokens": ["Wie", "die", "vier", "E\u00b7le\u00b7ment'", "in", "sich", "zwie\u00b7fal\u00b7tig", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "CARD", "NN", "APPR", "PRF", "ADJD", "VAFIN", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "So sind sie auch in dir zwiespaltig, Menschenkind.", "tokens": ["So", "sind", "sie", "auch", "in", "dir", "zwies\u00b7pal\u00b7tig", ",", "Men\u00b7schen\u00b7kind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "PPER", "ADJD", "$,", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Und wie der viere keins in der Natur vorhanden", "tokens": ["Und", "wie", "der", "vie\u00b7re", "keins", "in", "der", "Na\u00b7tur", "vor\u00b7han\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "ADJA", "NN", "APPR", "ART", "NN", "ADJD"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Allein ist, ohne da\u00df die drei sich ihm verbanden;", "tokens": ["Al\u00b7lein", "ist", ",", "oh\u00b7ne", "da\u00df", "die", "drei", "sich", "ihm", "ver\u00b7ban\u00b7den", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "KOUI", "KOUS", "ART", "CARD", "PRF", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "So deine innren L\u00fcft' und Fluten, Erd' und Flammen,", "tokens": ["So", "dei\u00b7ne", "inn\u00b7ren", "L\u00fcft'", "und", "Flu\u00b7ten", ",", "Erd'", "und", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "KON", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sind Lebensmischung nur, wo alle sind beisammen.", "tokens": ["Sind", "Le\u00b7bens\u00b7mi\u00b7schung", "nur", ",", "wo", "al\u00b7le", "sind", "bei\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADV", "$,", "PWAV", "PIS", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Die Wesen aber, die Ein Element in freister", "tokens": ["Die", "We\u00b7sen", "a\u00b7ber", ",", "die", "Ein", "E\u00b7le\u00b7ment", "in", "freis\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "$,", "PRELS", "ART", "NN", "APPR", "ADJA"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Bewegung haben, sind elementarische Geister.", "tokens": ["Be\u00b7we\u00b7gung", "ha\u00b7ben", ",", "sind", "e\u00b7lem\u00b7en\u00b7ta\u00b7ri\u00b7sche", "Geis\u00b7ter", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+--+--+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.10": {"line.1": {"text": "Luftgeister wie die Luft ein Wallen nur und Weben,", "tokens": ["Luft\u00b7geis\u00b7ter", "wie", "die", "Luft", "ein", "Wal\u00b7len", "nur", "und", "We\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "ART", "NN", "ADV", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Flutgeister wie die Flut ein Schwanken und ein Schweben.", "tokens": ["Flut\u00b7geis\u00b7ter", "wie", "die", "Flut", "ein", "Schwan\u00b7ken", "und", "ein", "Schwe\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "++-+-+-+-+-+-", "measure": "unknown.measure.septa"}}, "stanza.11": {"line.1": {"text": "Glutgeister wie die Glut ein Leuchten oder Spr\u00fchn,", "tokens": ["Glut\u00b7geis\u00b7ter", "wie", "die", "Glut", "ein", "Leuch\u00b7ten", "o\u00b7der", "Spr\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erdgeister wie die Erd' ein Starren oder Bl\u00fchn.", "tokens": ["Erd\u00b7geis\u00b7ter", "wie", "die", "Erd'", "ein", "Star\u00b7ren", "o\u00b7der", "Bl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.12": {"line.1": {"text": "Doch du, o Mensch, bist kein elementarisch Wesen,", "tokens": ["Doch", "du", ",", "o", "Mensch", ",", "bist", "kein", "e\u00b7lem\u00b7en\u00b7ta\u00b7risch", "We\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "FM", "NN", "$,", "VAFIN", "PIAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bist, oder kannst doch seyn, vom Sturm zur Ruh genesen.", "tokens": ["Bist", ",", "o\u00b7der", "kannst", "doch", "seyn", ",", "vom", "Sturm", "zur", "Ruh", "ge\u00b7ne\u00b7sen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "KON", "VMFIN", "ADV", "VAINF", "$,", "APPRART", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.13": {"line.1": {"text": "Du bists, sind erst in dir die vier in rechter Mischung,", "tokens": ["Du", "bists", ",", "sind", "erst", "in", "dir", "die", "vier", "in", "rech\u00b7ter", "Misc\u00b7hung", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "VAFIN", "ADV", "APPR", "PPER", "ART", "CARD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dann wechselwirkend stets einander zur Erfrischung.", "tokens": ["Dann", "wech\u00b7sel\u00b7wir\u00b7kend", "stets", "ein\u00b7an\u00b7der", "zur", "Er\u00b7fri\u00b7schung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PRF", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.14": {"line.1": {"text": "Da\u00df keines ohn' und durch das andre nehme Schaden,", "tokens": ["Da\u00df", "kei\u00b7nes", "ohn'", "und", "durch", "das", "and\u00b7re", "neh\u00b7me", "Scha\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "KON", "APPR", "ART", "PIS", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Liegt halb, o Mensch, an dir, und halb an Gottes Gnaden.", "tokens": ["Liegt", "halb", ",", "o", "Mensch", ",", "an", "dir", ",", "und", "halb", "an", "Got\u00b7tes", "Gna\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,", "FM", "NN", "$,", "APPR", "PPER", "$,", "KON", "ADJD", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.15": {"line.1": {"text": "Die gro\u00dfe H\u00e4lfte ist des Himmels, dein die kleine;", "tokens": ["Die", "gro\u00b7\u00dfe", "H\u00e4lf\u00b7te", "ist", "des", "Him\u00b7mels", ",", "dein", "die", "klei\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "NN", "$,", "PPOSAT", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er thut das Ganze, doch du thust dazu das deine.", "tokens": ["Er", "thut", "das", "Gan\u00b7ze", ",", "doch", "du", "thust", "da\u00b7zu", "das", "dei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "KON", "PPER", "VVFIN", "PAV", "ART", "PPOSAT", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.16": {"line.1": {"text": "Sei heiter wie die Luft, wie Feuer ohne Scheu,", "tokens": ["Sei", "hei\u00b7ter", "wie", "die", "Luft", ",", "wie", "Feu\u00b7er", "oh\u00b7ne", "Scheu", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "KOKOM", "ART", "NN", "$,", "PWAV", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie Wasser still und tief, wie Erde fest und treu.", "tokens": ["Wie", "Was\u00b7ser", "still", "und", "tief", ",", "wie", "Er\u00b7de", "fest", "und", "treu", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PTKVZ", "KON", "ADJD", "$,", "PWAV", "NN", "PTKVZ", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.17": {"line.1": {"text": "Wo Elemente so geeint sind und geviert,", "tokens": ["Wo", "E\u00b7le\u00b7men\u00b7te", "so", "ge\u00b7eint", "sind", "und", "ge\u00b7viert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADV", "VVPP", "VAFIN", "KON", "VVPP", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Solch ein Temperament ist wirklich temperiert.", "tokens": ["Solch", "ein", "Tem\u00b7pe\u00b7ra\u00b7ment", "ist", "wirk\u00b7lich", "tem\u00b7pe\u00b7riert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "NN", "VAFIN", "ADJD", "VVPP", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}}}}}