{"textgrid.poem.28414": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Da\u00df unsres Lebens Haupt ist au\u00df der Erden Staube", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df unsres Lebens Haupt ist au\u00df der Erden Staube", "tokens": ["Da\u00df", "uns\u00b7res", "Le\u00b7bens", "Haupt", "ist", "au\u00df", "der", "Er\u00b7den", "Stau\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "VAFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ins Himmels Glantz erh\u00f6ht, das st\u00e4rckt mich, da\u00df ich glaube,", "tokens": ["Ins", "Him\u00b7mels", "Glantz", "er\u00b7h\u00f6ht", ",", "das", "st\u00e4rckt", "mich", ",", "da\u00df", "ich", "glau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "VVPP", "$,", "PDS", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df dann zu seiner Zeit verg\u00fcnnet wird dem Leibe,", "tokens": ["Da\u00df", "dann", "zu", "sei\u00b7ner", "Zeit", "ver\u00b7g\u00fcn\u00b7net", "wird", "dem", "Lei\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df, wo sein Haupt verbleibt, auch er daselbst verbleibe.", "tokens": ["Da\u00df", ",", "wo", "sein", "Haupt", "ver\u00b7bleibt", ",", "auch", "er", "da\u00b7selbst", "ver\u00b7blei\u00b7be", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PWAV", "PPOSAT", "NN", "VVFIN", "$,", "ADV", "PPER", "PAV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}