{"dta.poem.9336": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "XiII .  \n Huictain/ Achtzeilich gesetze/  \n So sich von Weiblichen anf\u00e4ht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Oerdmuth g\u00fcldnes Licht! o Sonne dieser Erden!", "tokens": ["O\u00b7erd\u00b7muth", "g\u00fcld\u00b7nes", "Licht", "!", "o", "Son\u00b7ne", "die\u00b7ser", "Er\u00b7den", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$.", "FM", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich habe nicht gesehn bey andern diese zier/", "tokens": ["Ich", "ha\u00b7be", "nicht", "ge\u00b7sehn", "bey", "an\u00b7dern", "die\u00b7se", "zier", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "VVPP", "APPR", "PIS", "PDAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "die doppelt kann zugleich bey dir geschauer werden/", "tokens": ["die", "dop\u00b7pelt", "kann", "zu\u00b7gleich", "bey", "dir", "ge\u00b7schau\u00b7er", "wer\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VMFIN", "ADV", "APPR", "PPER", "ADJD", "VAINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "der R\u00f6the bild die schaam von innen bricht herf\u00fcr/", "tokens": ["der", "R\u00f6\u00b7the", "bild", "die", "schaam", "von", "in\u00b7nen", "bricht", "her\u00b7f\u00fcr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ART", "ADJD", "APPR", "ADV", "VVFIN", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "von au\u00dfen sch\u00f6nheit-pracht mit lieblichen Geberden/", "tokens": ["von", "au\u00b7\u00dfen", "sch\u00f6n\u00b7heit\u00b7pracht", "mit", "lieb\u00b7li\u00b7chen", "Ge\u00b7ber\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJD", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "man hat ein sch\u00f6nes hau\u00df un\u0303 sch\u00f6nen wirth an dier:", "tokens": ["man", "hat", "ein", "sch\u00f6\u00b7nes", "hau\u00df", "u\u00f1", "sch\u00f6\u00b7nen", "wirth", "an", "dier", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ART", "ADJA", "NN", "KON", "VVFIN", "ADJD", "APPR", "PDAT", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ob dier das Alter gleich den spiegel wolte brechen/", "tokens": ["Ob", "dier", "das", "Al\u00b7ter", "gleich", "den", "spie\u00b7gel", "wol\u00b7te", "bre\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "ART", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "so kan es doch die schaam und zierligkeit nit schwechen.", "tokens": ["so", "kan", "es", "doch", "die", "schaam", "und", "zier\u00b7lig\u00b7keit", "nit", "schwe\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ART", "ADJD", "KON", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}