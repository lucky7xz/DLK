{"textgrid.poem.59293": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Steckbrief", "genre": "verse", "period": "N.A.", "pub_year": 1811, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gr\u00fc\u00df euch aus Herzensgrund:", "tokens": ["Gr\u00fc\u00df", "euch", "aus", "Her\u00b7zens\u00b7grund", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Zwei Augen hell und rein,", "tokens": ["Zwei", "Au\u00b7gen", "hell", "und", "rein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zwei R\u00f6slein auf dem Mund,", "tokens": ["Zwei", "R\u00f6s\u00b7lein", "auf", "dem", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Kleid blank aus Sonnenschein!", "tokens": ["Kleid", "blank", "aus", "Son\u00b7nen\u00b7schein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "APPR", "NN", "$."], "meter": "++-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Nachtigall klagt und weint,", "tokens": ["Nach\u00b7ti\u00b7gall", "klagt", "und", "weint", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Woll\u00fcstig rauscht der Hain,", "tokens": ["Wol\u00b7l\u00fcs\u00b7tig", "rauscht", "der", "Hain", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Alles die Liebste meint:", "tokens": ["Al\u00b7les", "die", "Liebs\u00b7te", "meint", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Wo weilt sie so allein?", "tokens": ["Wo", "weilt", "sie", "so", "al\u00b7lein", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Weil's drau\u00dfen finster war,", "tokens": ["Weil's", "drau\u00b7\u00dfen", "fins\u00b7ter", "war", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Sah ich viel hellern Schein,", "tokens": ["Sah", "ich", "viel", "hel\u00b7lern", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Jetzt ist es licht und klar,", "tokens": ["Jetzt", "ist", "es", "licht", "und", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ich mu\u00df im Dunkeln sein.", "tokens": ["Ich", "mu\u00df", "im", "Dun\u00b7keln", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPRART", "ADJA", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Sonne nicht steigen mag,", "tokens": ["Son\u00b7ne", "nicht", "stei\u00b7gen", "mag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Sieht so verschlafen drein,", "tokens": ["Sieht", "so", "ver\u00b7schla\u00b7fen", "drein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "W\u00fcnschet den ganzen Tag,", "tokens": ["W\u00fcn\u00b7schet", "den", "gan\u00b7zen", "Tag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Da\u00df wieder Nacht m\u00f6cht sein.", "tokens": ["Da\u00df", "wie\u00b7der", "Nacht", "m\u00f6cht", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "NN", "VMFIN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Liebe geht durch die Luft,", "tokens": ["Lie\u00b7be", "geht", "durch", "die", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Holt fern die Liebste ein;", "tokens": ["Holt", "fern", "die", "Liebs\u00b7te", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "ART", "NN", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Fort \u00fcber Berg und Kluft!", "tokens": ["Fort", "\u00fc\u00b7ber", "Berg", "und", "Kluft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und Sie wird doch noch mein!", "tokens": ["Und", "Sie", "wird", "doch", "noch", "mein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "ADV", "PPOSAT", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}}}}