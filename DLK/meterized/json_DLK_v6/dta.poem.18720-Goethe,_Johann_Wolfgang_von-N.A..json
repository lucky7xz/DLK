{"dta.poem.18720": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Talismane werd\u2019 ich in dem Buch zerstreuen,", "tokens": ["Ta\u00b7lis\u00b7ma\u00b7ne", "werd'", "ich", "in", "dem", "Buch", "zer\u00b7streu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Das bewirkt ein Gleichgewicht.", "tokens": ["Das", "be\u00b7wirkt", "ein", "Gleich\u00b7ge\u00b7wicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wer mit gl\u00e4ubiger Nadel sticht", "tokens": ["Wer", "mit", "gl\u00e4u\u00b7bi\u00b7ger", "Na\u00b7del", "sticht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "ADJA", "NN", "VVFIN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Ueberall soll gutes Wort ihn freuen.", "tokens": ["Ue\u00b7be\u00b7rall", "soll", "gu\u00b7tes", "Wort", "ihn", "freu\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJA", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}