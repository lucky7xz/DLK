{"dta.poem.21029": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "XcIII.  \n Eines alten greulichen Weibes.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Affe von Gestalt/ ein Teufel von Gem\u00fcthe/", "tokens": ["Ein", "Af\u00b7fe", "von", "Ge\u00b7stalt", "/", "ein", "Teu\u00b7fel", "von", "Ge\u00b7m\u00fc\u00b7the", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$(", "ART", "NN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Eule von Geschrey/ ein Drache von Gebl\u00fcthe/", "tokens": ["Ein", "Eu\u00b7le", "von", "Ge\u00b7schrey", "/", "ein", "Dra\u00b7che", "von", "Ge\u00b7bl\u00fc\u00b7the", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$(", "ART", "NN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "War dieses alte Weib. Wer wolte doch nicht lachen/", "tokens": ["War", "die\u00b7ses", "al\u00b7te", "Weib", ".", "Wer", "wol\u00b7te", "doch", "nicht", "la\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "ADJA", "NN", "$.", "PWS", "VMFIN", "ADV", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Teufel liegt alhier bey Eulen/ Affen/ Drachen.", "tokens": ["Der", "Teu\u00b7fel", "liegt", "al\u00b7hier", "bey", "Eu\u00b7len", "/", "Af\u00b7fen", "/", "Dra\u00b7chen", "."], "token_info": ["wo