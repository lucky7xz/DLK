{"textgrid.poem.59200": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Ankl\u00e4nge", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "V\u00f6glein in den sonn'gen Tagen!", "tokens": ["V\u00f6\u00b7glein", "in", "den", "sonn'\u00b7gen", "Ta\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "L\u00fcfte blau, die mich verfahren!", "tokens": ["L\u00fcf\u00b7te", "blau", ",", "die", "mich", "ver\u00b7fah\u00b7ren", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "PRELS", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "K\u00f6nnt ich bunte Fl\u00fcgel r\u00fchren,", "tokens": ["K\u00f6nnt", "ich", "bun\u00b7te", "Fl\u00fc\u00b7gel", "r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00dcber Berg und Wald sie schlagen!", "tokens": ["\u00dc\u00b7ber", "Berg", "und", "Wald", "sie", "schla\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ach! es spricht des Fr\u00fchlings Sch\u00f6ne,", "tokens": ["Ach", "!", "es", "spricht", "des", "Fr\u00fch\u00b7lings", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die V\u00f6gel alle singen:", "tokens": ["Und", "die", "V\u00f6\u00b7gel", "al\u00b7le", "sin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sind die Farben denn nicht T\u00f6ne,", "tokens": ["Sind", "die", "Far\u00b7ben", "denn", "nicht", "T\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "PTKNEG", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die T\u00f6ne bunte Schwingen?", "tokens": ["Und", "die", "T\u00f6\u00b7ne", "bun\u00b7te", "Schwin\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "V\u00f6glein, ja, ich la\u00df das Zagen!", "tokens": ["V\u00f6\u00b7glein", ",", "ja", ",", "ich", "la\u00df", "das", "Za\u00b7gen", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PTKANT", "$,", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Winde sanft die Segel r\u00fchren,", "tokens": ["Win\u00b7de", "sanft", "die", "Se\u00b7gel", "r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ich lasse mich entf\u00fchren,", "tokens": ["Und", "ich", "las\u00b7se", "mich", "ent\u00b7f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach! wohin? mag ich nicht fragen.", "tokens": ["Ach", "!", "wo\u00b7hin", "?", "mag", "ich", "nicht", "fra\u00b7gen", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWAV", "$.", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}