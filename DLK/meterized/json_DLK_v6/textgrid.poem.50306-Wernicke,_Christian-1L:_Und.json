{"textgrid.poem.50306": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Und ", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Noch \u00fcber mehr als eine sein:", "tokens": ["Noch", "\u00fc\u00b7ber", "mehr", "als", "ei\u00b7ne", "sein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIS", "KOKOM", "ART", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Drum setzte ", "tokens": ["Drum", "setz\u00b7te"], "token_info": ["word", "word"], "pos": ["PAV", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Sich offt bey Herkuls truncknem Becher,", "tokens": ["Sich", "offt", "bey", "Her\u00b7kuls", "trunck\u00b7nem", "Be\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "APPR", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und soff den Wein wie Wasser ein:", "tokens": ["Und", "soff", "den", "Wein", "wie", "Was\u00b7ser", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "KOKOM", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dieweil, wenn Dunst das Aug' umziehet,", "tokens": ["Die\u00b7weil", ",", "wenn", "Dunst", "das", "Aug'", "um\u00b7zie\u00b7het", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}