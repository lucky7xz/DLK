{"dta.poem.2958": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In einem Irrthum seh' ich euch befangen alle,", "tokens": ["In", "ei\u00b7nem", "Irr\u00b7thum", "seh'", "ich", "euch", "be\u00b7fan\u00b7gen", "al\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "PPER", "VVFIN", "PIS", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Alsob nichts fest mehr steh' und alles ruhlos walle.", "tokens": ["Al\u00b7sob", "nichts", "fest", "mehr", "steh'", "und", "al\u00b7les", "ruh\u00b7los", "wal\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "ADJD", "ADV", "VVFIN", "KON", "PIS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wol unaufhaltsam geht voran das Weltgeschick,", "tokens": ["Wol", "un\u00b7auf\u00b7halt\u00b7sam", "geht", "vo\u00b7ran", "das", "Welt\u00b7ge\u00b7schick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Und etwas Neues bringt auch jeder Augenblick.", "tokens": ["Und", "et\u00b7was", "Neu\u00b7es", "bringt", "auch", "je\u00b7der", "Au\u00b7gen\u00b7blick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch was der eine bringt, das nimmt der andre wieder,", "tokens": ["Doch", "was", "der", "ei\u00b7ne", "bringt", ",", "das", "nimmt", "der", "and\u00b7re", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "PIS", "VVFIN", "$,", "PDS", "VVFIN", "ART", "PIS", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie eine Blas' im Strom aufsteigt und sinket nieder.", "tokens": ["Wie", "ei\u00b7ne", "Blas'", "im", "Strom", "auf\u00b7steigt", "und", "sin\u00b7ket", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN", "VVPP", "KON", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ihr Blasen auf dem Strom des Tages, bl\u00e4het euch!", "tokens": ["Ihr", "Bla\u00b7sen", "auf", "dem", "Strom", "des", "Ta\u00b7ges", ",", "bl\u00e4\u00b7het", "euch", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "ART", "NN", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bl\u00e4ht euch und blas't nur auf die Backen mit Gekeuch!", "tokens": ["Bl\u00e4ht", "euch", "und", "blas't", "nur", "auf", "die", "Ba\u00b7cken", "mit", "Ge\u00b7keuch", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "VVFIN", "ADV", "APPR", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Blas't, Blasen, bis ihr platzt, und macht einander Platz!", "tokens": ["Blas't", ",", "Bla\u00b7sen", ",", "bis", "ihr", "platzt", ",", "und", "macht", "ein\u00b7an\u00b7der", "Platz", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "KOUS", "PPER", "VVFIN", "$,", "KON", "VVFIN", "PRF", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn noch von Blasen liegt im Strom ein ganzer Schatz.", "tokens": ["Denn", "noch", "von", "Bla\u00b7sen", "liegt", "im", "Strom", "ein", "gan\u00b7zer", "Schatz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Doch eine Muschel ruht, gef\u00fcllt mit Weh und Lust,", "tokens": ["Doch", "ei\u00b7ne", "Mu\u00b7schel", "ruht", ",", "ge\u00b7f\u00fcllt", "mit", "Weh", "und", "Lust", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$,", "VVPP", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und bildet wie ein Herz die Perl' in ihrer Brust;", "tokens": ["Und", "bil\u00b7det", "wie", "ein", "Herz", "die", "Perl'", "in", "ih\u00b7rer", "Brust", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOKOM", "ART", "NN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "In welchem das Gef\u00fchl von Erd' und Himmel schl\u00e4gt,", "tokens": ["In", "wel\u00b7chem", "das", "Ge\u00b7f\u00fchl", "von", "Erd'", "und", "Him\u00b7mel", "schl\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "ART", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In welchem Ewiges ist endlich-sch\u00f6n gepr\u00e4gt,", "tokens": ["In", "wel\u00b7chem", "E\u00b7wi\u00b7ges", "ist", "end\u00b7lich\u00b7sch\u00f6n", "ge\u00b7pr\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "ADJA", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Dis Herz, wann es schon l\u00e4ngst hat aufgeh\u00f6rt zu schlagen,", "tokens": ["Dis", "Herz", ",", "wann", "es", "schon", "l\u00e4ngst", "hat", "auf\u00b7ge\u00b7h\u00f6rt", "zu", "schla\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "$,", "PWAV", "PPER", "ADV", "ADV", "VAFIN", "VVPP", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gibt einst, ihr gebt es nicht, ein Zeugni\u00df diesen Tagen.", "tokens": ["Gibt", "einst", ",", "ihr", "gebt", "es", "nicht", ",", "ein", "Zeug\u00b7ni\u00df", "die\u00b7sen", "Ta\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$,", "ART", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Ihr aber, lernt einmal, ihr Leute der Bewegung,", "tokens": ["Ihr", "a\u00b7ber", ",", "lernt", "ein\u00b7mal", ",", "ihr", "Leu\u00b7te", "der", "Be\u00b7we\u00b7gung", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "VVFIN", "ADV", "$,", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df ewig niemals ist des Augenblicks Aufregung.", "tokens": ["Da\u00df", "e\u00b7wig", "nie\u00b7mals", "ist", "des", "Au\u00b7gen\u00b7blicks", "Auf\u00b7re\u00b7gung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ADV", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}