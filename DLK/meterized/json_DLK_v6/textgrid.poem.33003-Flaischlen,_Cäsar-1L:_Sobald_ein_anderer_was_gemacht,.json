{"textgrid.poem.33003": {"metadata": {"author": {"name": "Flaischlen, C\u00e4sar", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sobald ein anderer was gemacht,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sobald ein anderer was gemacht,", "tokens": ["So\u00b7bald", "ein", "an\u00b7de\u00b7rer", "was", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "PWS", "VVPP", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "ist's gut und sch\u00f6n und klug bedacht,", "tokens": ["ist's", "gut", "und", "sch\u00f6n", "und", "klug", "be\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KON", "ADJD", "KON", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "man nimmt's, wie's ist, und freut sich dran:", "tokens": ["man", "nimmt's", ",", "wie's", "ist", ",", "und", "freut", "sich", "dran", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PWAV", "VAFIN", "$,", "KON", "VVFIN", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "wieder einer, der was kann!", "tokens": ["wie\u00b7der", "ei\u00b7ner", ",", "der", "was", "kann", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "$,", "PRELS", "PWS", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch wenn du selber damit k\u00e4mest,", "tokens": ["Doch", "wenn", "du", "sel\u00b7ber", "da\u00b7mit", "k\u00e4\u00b7mest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "PAV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "beg\u00e4nn ein Wackeln mit den Z\u00f6pfen,", "tokens": ["be\u00b7g\u00e4nn", "ein", "Wa\u00b7ckeln", "mit", "den", "Z\u00f6p\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "ein Sch\u00fctteln mit den weisen K\u00f6pfen:", "tokens": ["ein", "Sch\u00fct\u00b7teln", "mit", "den", "wei\u00b7sen", "K\u00f6p\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "die Sache sei viel zu verzwickt", "tokens": ["die", "Sa\u00b7che", "sei", "viel", "zu", "ver\u00b7zwickt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PIS", "PTKZU", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "und dies und das vorbeigegl\u00fcckt!", "tokens": ["und", "dies", "und", "das", "vor\u00b7bei\u00b7ge\u00b7gl\u00fcckt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "KON", "PDS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "man h\u00e4tte zu viel dabei zu denken!", "tokens": ["man", "h\u00e4t\u00b7te", "zu", "viel", "da\u00b7bei", "zu", "den\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PTKA", "PIS", "PAV", "PTKZU", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "man wolle Erholung, nicht Qu\u00e4lerein!", "tokens": ["man", "wol\u00b7le", "Er\u00b7ho\u00b7lung", ",", "nicht", "Qu\u00e4\u00b7le\u00b7rein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "NN", "$,", "PTKNEG", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "das Leben sei ohnehin ernst genug!", "tokens": ["das", "Le\u00b7ben", "sei", "oh\u00b7ne\u00b7hin", "ernst", "ge\u00b7nug", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "ADV", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.9": {"text": "Kurzum: man m\u00fcsse leichter sein!", "tokens": ["Kur\u00b7zum", ":", "man", "m\u00fcs\u00b7se", "leich\u00b7ter", "sein", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PIS", "VMFIN", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und gl\u00fcckte was mit leichterer Feder ...", "tokens": ["Und", "gl\u00fcck\u00b7te", "was", "mit", "leich\u00b7te\u00b7rer", "Fe\u00b7der", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "wei\u00df Gott, so k\u00e4men sie erst recht:", "tokens": ["wei\u00df", "Gott", ",", "so", "k\u00e4\u00b7men", "sie", "erst", "recht", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "ADV", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "so was k\u00f6nn heutzutage jeder!", "tokens": ["so", "was", "k\u00f6nn", "heut\u00b7zu\u00b7ta\u00b7ge", "je\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "VMFIN", "ADV", "PIS", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "mit solchen billigen Spielerein", "tokens": ["mit", "sol\u00b7chen", "bil\u00b7li\u00b7gen", "Spie\u00b7ler\u00b7ein"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "erw\u00fcrbe man sich kaum viel Gunst!", "tokens": ["er\u00b7w\u00fcr\u00b7be", "man", "sich", "kaum", "viel", "Gunst", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PRF", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "mehr-k\u00f6nnen m\u00fcsse, wer da wolle,", "tokens": ["mehr\u00b7k\u00f6n\u00b7nen", "m\u00fcs\u00b7se", ",", "wer", "da", "wol\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "$,", "PWS", "ADV", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "da\u00df man ihn h\u00f6her werten solle!", "tokens": ["da\u00df", "man", "ihn", "h\u00f6\u00b7her", "wer\u00b7ten", "sol\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "hart sei das Leben, hart sei auch die Kunst!", "tokens": ["hart", "sei", "das", "Le\u00b7ben", ",", "hart", "sei", "auch", "die", "Kunst", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$,", "ADJD", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}