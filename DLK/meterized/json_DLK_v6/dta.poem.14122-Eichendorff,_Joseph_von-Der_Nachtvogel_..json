{"dta.poem.14122": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der Nachtvogel .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Und kann ich nicht sein", "tokens": ["Und", "kann", "ich", "nicht", "sein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "PTKNEG", "VAINF"], "meter": "+---+", "measure": "dactylic.init"}, "line.2": {"text": "Mit Dir zu zwei'n,", "tokens": ["Mit", "Dir", "zu", "zwei'n", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "So will ich, allein,", "tokens": ["So", "will", "ich", ",", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "$,", "ADV", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Der Schwermuth mich weih'n!", "tokens": ["Der", "Schwer\u00b7muth", "mich", "weih'n", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}