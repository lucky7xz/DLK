{"textgrid.poem.57583": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Der Spatz", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00df schimpfen sie und l\u00e4stern,", "tokens": ["La\u00df", "schimp\u00b7fen", "sie", "und", "l\u00e4s\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVFIN", "PPER", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Uns knickt es nicht den Spa\u00df,", "tokens": ["Uns", "knickt", "es", "nicht", "den", "Spa\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Kieselwurf von gestern", "tokens": ["Der", "Kie\u00b7sel\u00b7wurf", "von", "ge\u00b7stern"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Traf nur das Fensterglas \u2013", "tokens": ["Traf", "nur", "das", "Fens\u00b7ter\u00b7glas", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Du schilpst dein freches Liedchen,", "tokens": ["Du", "schilpst", "dein", "fre\u00b7ches", "Lied\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ich h\u00f6r' dir lachend zu,", "tokens": ["Ich", "h\u00f6r'", "dir", "la\u00b7chend", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und streue dir die Kr\u00fcmel,", "tokens": ["Und", "streu\u00b7e", "dir", "die", "Kr\u00fc\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mein grauer Hausfreund du!", "tokens": ["Mein", "grau\u00b7er", "Haus\u00b7freund", "du", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sind vogelfrei wir beide,", "tokens": ["Sind", "vo\u00b7gel\u00b7frei", "wir", "bei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "PPER", "PIS", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ge\u00e4chtet und gebannt \u2013", "tokens": ["Ge\u00e4ch\u00b7tet", "und", "ge\u00b7bannt", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVPP", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Wer tat uns was zu Leide,", "tokens": ["Wer", "tat", "uns", "was", "zu", "Lei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PIS", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der keine Rache fand?", "tokens": ["Der", "kei\u00b7ne", "Ra\u00b7che", "fand", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Den feigen L\u00e4sterm\u00e4ulern", "tokens": ["Den", "fei\u00b7gen", "L\u00e4s\u00b7ter\u00b7m\u00e4u\u00b7lern"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Klinge ins Gesicht,", "tokens": ["Die", "Klin\u00b7ge", "ins", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Du tust noch ganz was andres,", "tokens": ["Du", "tust", "noch", "ganz", "was", "and\u00b7res", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "PWS", "PIS", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Doch das erz\u00e4hlt man nicht.", "tokens": ["Doch", "das", "er\u00b7z\u00e4hlt", "man", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "PIS", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Bek\u00fcmmernis und Sorgen", "tokens": ["Be\u00b7k\u00fcm\u00b7mer\u00b7nis", "und", "Sor\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sind nicht f\u00fcr uns gemacht,", "tokens": ["Sind", "nicht", "f\u00fcr", "uns", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Droht Blitz und Donner morgen,", "tokens": ["Droht", "Blitz", "und", "Don\u00b7ner", "mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wird heute doch gelacht,", "tokens": ["Wird", "heu\u00b7te", "doch", "ge\u00b7lacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Solang noch Kirschen schwellen", "tokens": ["So\u00b7lang", "noch", "Kir\u00b7schen", "schwel\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und Wein die Reifen dr\u00fcckt", "tokens": ["Und", "Wein", "die", "Rei\u00b7fen", "dr\u00fcckt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und M\u00e4dchenlippen bl\u00fchen,", "tokens": ["Und", "M\u00e4d\u00b7chen\u00b7lip\u00b7pen", "bl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Das Auge lustig blickt.", "tokens": ["Das", "Au\u00b7ge", "lus\u00b7tig", "blickt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und wenn die St\u00fcrme tosen", "tokens": ["Und", "wenn", "die", "St\u00fcr\u00b7me", "to\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und Schnee das Dach umtreibt \u2013", "tokens": ["Und", "Schnee", "das", "Dach", "um\u00b7treibt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wir brauchen keine Rosen,", "tokens": ["Wir", "brau\u00b7chen", "kei\u00b7ne", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Genug uns \u00fcbrig bleibt:", "tokens": ["Ge\u00b7nug", "uns", "\u00fcb\u00b7rig", "bleibt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ein molligwarmes Nestchen,", "tokens": ["Ein", "mol\u00b7lig\u00b7war\u00b7mes", "Nest\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Atzung und frischer Trank,", "tokens": ["At\u00b7zung", "und", "fri\u00b7scher", "Trank", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Ein rundes weiches Liebchen", "tokens": ["Ein", "run\u00b7des", "wei\u00b7ches", "Lieb\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und unverfrorner Sang!", "tokens": ["Und", "un\u00b7ver\u00b7fror\u00b7ner", "Sang", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}