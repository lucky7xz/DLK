{"dta.poem.14914": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der armen Sch\u00f6nheit Lebenslauf.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Und wer die arme Sch\u00f6nheit schauet,", "tokens": ["Und", "wer", "die", "ar\u00b7me", "Sch\u00f6n\u00b7heit", "schau\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sich wie auf gro\u00dfes Gl\u00fcck besinnt,", "tokens": ["Sich", "wie", "auf", "gro\u00b7\u00dfes", "Gl\u00fcck", "be\u00b7sinnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "KOKOM", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Seele f\u00fchlt sich recht erbauet,", "tokens": ["Die", "See\u00b7le", "f\u00fchlt", "sich", "recht", "er\u00b7bau\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie wenn der Fr\u00fchling neu beginnt.", "tokens": ["Wie", "wenn", "der", "Fr\u00fch\u00b7ling", "neu", "be\u00b7ginnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}