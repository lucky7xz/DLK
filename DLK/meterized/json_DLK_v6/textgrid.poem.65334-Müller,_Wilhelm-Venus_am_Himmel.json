{"textgrid.poem.65334": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Venus am Himmel", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tritt an's Fenster, meine Liebe, sieh den hellen Himmel an,", "tokens": ["Tritt", "an's", "Fens\u00b7ter", ",", "mei\u00b7ne", "Lie\u00b7be", ",", "sieh", "den", "hel\u00b7len", "Him\u00b7mel", "an", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$,", "PPOSAT", "NN", "$,", "VVIMP", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Wie der Mond, der keusche Freier, mit der Venus scherzen kann,", "tokens": ["Wie", "der", "Mond", ",", "der", "keu\u00b7sche", "Frei\u00b7er", ",", "mit", "der", "Ve\u00b7nus", "scher\u00b7zen", "kann", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,", "APPR", "ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Wie sie sich so nahe r\u00fccken, und die kleinen Sterne sehn", "tokens": ["Wie", "sie", "sich", "so", "na\u00b7he", "r\u00fc\u00b7cken", ",", "und", "die", "klei\u00b7nen", "Ster\u00b7ne", "sehn"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PRF", "ADV", "ADJD", "VVINF", "$,", "KON", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "L\u00fcstern nach dem sch\u00f6nen Paare und vergessen fort zu gehn.", "tokens": ["L\u00fcs\u00b7tern", "nach", "dem", "sch\u00f6\u00b7nen", "Paa\u00b7re", "und", "ver\u00b7ges\u00b7sen", "fort", "zu", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "KON", "VVFIN", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Tritt an's Fenster, meine Liebe, neige nieder dich, mein Stern!", "tokens": ["Tritt", "an's", "Fens\u00b7ter", ",", "mei\u00b7ne", "Lie\u00b7be", ",", "nei\u00b7ge", "nie\u00b7der", "dich", ",", "mein", "Stern", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$,", "PPOSAT", "NN", "$,", "VVFIN", "PTKVZ", "PPER", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Venus herrscht am Himmel heute, und die Erde folgt ihm gern.", "tokens": ["Ve\u00b7nus", "herrscht", "am", "Him\u00b7mel", "heu\u00b7te", ",", "und", "die", "Er\u00b7de", "folgt", "ihm", "gern", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPRART", "NN", "ADV", "$,", "KON", "ART", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}