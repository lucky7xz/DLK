{"dta.poem.7348": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Die Schmiede.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Wie kann der alte Aepfelbaum", "tokens": ["Wie", "kann", "der", "al\u00b7te", "A\u00b7e\u00b7pfel\u00b7baum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "ART", "ADJA", "NN"], "meter": "-+-+----+", "measure": "unknown.measure.tri"}, "line.2": {"text": "So lockre Fr\u00fcchte tragen,", "tokens": ["So", "lock\u00b7re", "Fr\u00fcch\u00b7te", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wo Mistelb\u00fcsch' und Mooses Flaum", "tokens": ["Wo", "Mis\u00b7tel\u00b7b\u00fcsch'", "und", "Moo\u00b7ses", "Flaum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "KON", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aus jeder Ritze ragen?", "tokens": ["Aus", "je\u00b7der", "Rit\u00b7ze", "ra\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Halb todt, halb lebend, wie ein Prinz", "tokens": ["Halb", "todt", ",", "halb", "le\u00b7bend", ",", "wie", "ein", "Prinz"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "ADJD", "$,", "ADJD", "ADJD", "$,", "PWAV", "ART", "NN"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.2": {"text": "In einem Ammenm\u00e4hrchen,", "tokens": ["In", "ei\u00b7nem", "Am\u00b7men\u00b7m\u00e4hr\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die eine Seite voll Gespinns,", "tokens": ["Die", "ei\u00b7ne", "Sei\u00b7te", "voll", "Ge\u00b7spinns", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wurmfra\u00df und Flockenh\u00e4\u00e4rchen,", "tokens": ["Wurm\u00b7fra\u00df", "und", "Flo\u00b7cken\u00b7h\u00e4\u00e4r\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "++-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Langt mit der andern, \u00fcppig roth,", "tokens": ["Langt", "mit", "der", "an\u00b7dern", ",", "\u00fcp\u00b7pig", "roth", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "$,", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er in die Funkenreigen,", "tokens": ["Er", "in", "die", "Fun\u00b7ken\u00b7rei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die knatternd aus der Schmiede Schlot", "tokens": ["Die", "knat\u00b7ternd", "aus", "der", "Schmie\u00b7de", "Schlot"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie Sternraketen steigen;", "tokens": ["Wie", "Stern\u00b7ra\u00b7ke\u00b7ten", "stei\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ein zweiter Sc\u00e4vola h\u00e4lt Jahr", "tokens": ["Ein", "zwei\u00b7ter", "Sc\u00e4\u00b7vo\u00b7la", "h\u00e4lt", "Jahr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf Jahr er seine Rechte", "tokens": ["Auf", "Jahr", "er", "sei\u00b7ne", "Rech\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Glut entgegen, die kein Haar", "tokens": ["Der", "Glut", "ent\u00b7ge\u00b7gen", ",", "die", "kein", "Haar"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "$,", "PRELS", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu sengen sich erfrechte.", "tokens": ["Zu", "sen\u00b7gen", "sich", "er\u00b7frech\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PRF", "VVFIN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.5": {"line.1": {"text": "Und drunten geht es Pink und Pank,", "tokens": ["Und", "drun\u00b7ten", "geht", "es", "Pink", "und", "Pank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Man h\u00f6rt die Flamme pfeifen,", "tokens": ["Man", "h\u00f6rt", "die", "Flam\u00b7me", "pfei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es keucht der Balg aus hohler Flank'", "tokens": ["Es", "keucht", "der", "Balg", "aus", "hoh\u00b7ler", "Flank'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und bildet Aschenstreifen;", "tokens": ["Und", "bil\u00b7det", "A\u00b7schen\u00b7strei\u00b7fen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Die Kohle knallt und dr\u00fcber dicht,", "tokens": ["Die", "Koh\u00b7le", "knallt", "und", "dr\u00fc\u00b7ber", "dicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "PAV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Augen wie Pyropen,", "tokens": ["Mit", "Au\u00b7gen", "wie", "Py\u00b7ro\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KOKOM", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Beugt sich das grimmige Gesicht", "tokens": ["Beugt", "sich", "das", "grim\u00b7mi\u00b7ge", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des ru\u00dfigen Cyklopen.", "tokens": ["Des", "ru\u00b7\u00dfi\u00b7gen", "Cyk\u00b7lo\u00b7pen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Er h\u00e4lt das Eisen in die Glut", "tokens": ["Er", "h\u00e4lt", "das", "Ei\u00b7sen", "in", "die", "Glut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie eine arme Seele,", "tokens": ["Wie", "ei\u00b7ne", "ar\u00b7me", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es knackt und spritzet Funkenblut", "tokens": ["Es", "knackt", "und", "sprit\u00b7zet", "Fun\u00b7ken\u00b7blut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und dunstet blaue Schwehle.", "tokens": ["Und", "duns\u00b7tet", "blau\u00b7e", "Schweh\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Dann auf dem Ambos, Schlag an Schlag,", "tokens": ["Dann", "auf", "dem", "Am\u00b7bos", ",", "Schlag", "an", "Schlag", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "L\u00e4\u00dft es sein Weh erklingen,", "tokens": ["L\u00e4\u00dft", "es", "sein", "Weh", "er\u00b7klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bis nun gekr\u00fcmmt in Zorn und Schmach", "tokens": ["Bis", "nun", "ge\u00b7kr\u00fcmmt", "in", "Zorn", "und", "Schmach"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "VVPP", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es kreucht zu Hufes Ringen.", "tokens": ["Es", "kreucht", "zu", "Hu\u00b7fes", "Rin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}