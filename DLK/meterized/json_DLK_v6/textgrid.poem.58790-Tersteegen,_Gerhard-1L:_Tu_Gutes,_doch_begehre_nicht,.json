{"textgrid.poem.58790": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "1L: Tu Gutes, doch begehre nicht,", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tu Gutes, doch begehre nicht,", "tokens": ["Tu", "Gu\u00b7tes", ",", "doch", "be\u00b7geh\u00b7re", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADV", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df man von deiner Tugend spricht;", "tokens": ["Da\u00df", "man", "von", "dei\u00b7ner", "Tu\u00b7gend", "spricht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vergessen und verachtet werden,", "tokens": ["Ver\u00b7ges\u00b7sen", "und", "ver\u00b7ach\u00b7tet", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sei dein Verlangen hier auf Erden!", "tokens": ["Sei", "dein", "Ver\u00b7lan\u00b7gen", "hier", "auf", "Er\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}