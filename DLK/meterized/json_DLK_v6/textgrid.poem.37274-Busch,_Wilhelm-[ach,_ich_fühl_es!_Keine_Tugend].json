{"textgrid.poem.37274": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "[ach, ich f\u00fchl es! Keine Tugend]", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach, ich f\u00fchl es! Keine Tugend", "tokens": ["Ach", ",", "ich", "f\u00fchl", "es", "!", "Kei\u00b7ne", "Tu\u00b7gend"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "PPER", "$.", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist so recht nach meinem Sinn;", "tokens": ["Ist", "so", "recht", "nach", "mei\u00b7nem", "Sinn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Stets befind ich mich am wohlsten,", "tokens": ["Stets", "be\u00b7find", "ich", "mich", "am", "wohls\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPRART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn ich damit fertig bin.", "tokens": ["Wenn", "ich", "da\u00b7mit", "fer\u00b7tig", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PAV", "ADJD", "VAFIN", "$."], "meter": "----+-+", "measure": "unknown.measure.di"}}, "stanza.2": {"line.1": {"text": "Dahingegen so ein Laster,", "tokens": ["Da\u00b7hin\u00b7ge\u00b7gen", "so", "ein", "Las\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ja, das macht mir viel Pl\u00e4sier;", "tokens": ["Ja", ",", "das", "macht", "mir", "viel", "Pl\u00e4\u00b7sier", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PDS", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ich hab die h\u00fcbschen Sachen", "tokens": ["Und", "ich", "hab", "die", "h\u00fcb\u00b7schen", "Sa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lieber vor als hinter mir.", "tokens": ["Lie\u00b7ber", "vor", "als", "hin\u00b7ter", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "KOKOM", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}