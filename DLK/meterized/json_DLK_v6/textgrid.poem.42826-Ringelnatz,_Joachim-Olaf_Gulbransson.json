{"textgrid.poem.42826": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Olaf Gulbransson", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In der freien Sonne, angesichts", "tokens": ["In", "der", "frei\u00b7en", "Son\u00b7ne", ",", "an\u00b7ge\u00b7sichts"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Sch\u00f6ner Tennisspielerinnen,", "tokens": ["Sch\u00f6\u00b7ner", "Ten\u00b7nis\u00b7spie\u00b7le\u00b7rin\u00b7nen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Steht ein Stier.", "tokens": ["Steht", "ein", "Stier", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Nur ein schmales Linnen", "tokens": ["Nur", "ein", "schma\u00b7les", "Lin\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Bammelt ihm vorm Bauch und verdeckt nichts.", "tokens": ["Bam\u00b7melt", "ihm", "vorm", "Bauch", "und", "ver\u00b7deckt", "nichts", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "KON", "VVFIN", "PIS", "$."], "meter": "+---+--+-", "measure": "trochaic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Goldig blinken kleine Einzelh\u00e4rchen", "tokens": ["Gol\u00b7dig", "blin\u00b7ken", "klei\u00b7ne", "Ein\u00b7zel\u00b7h\u00e4r\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Auf der nackten, braunen Haut.", "tokens": ["Auf", "der", "nack\u00b7ten", ",", "brau\u00b7nen", "Haut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Etwas brummt behaglich. Und ein M\u00e4rchen", "tokens": ["Et\u00b7was", "brummt", "be\u00b7hag\u00b7lich", ".", "Und", "ein", "M\u00e4r\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD", "$.", "KON", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "W\u00e4chst ringsum aus Gras und Kraut.", "tokens": ["W\u00e4chst", "ring\u00b7sum", "aus", "Gras", "und", "Kraut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Etwas rund und blank wie Billardglatze", "tokens": ["Et\u00b7was", "rund", "und", "blank", "wie", "Bil\u00b7lard\u00b7glat\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KON", "ADJD", "KOKOM", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wendet sich. Man sieht:", "tokens": ["Wen\u00b7det", "sich", ".", "Man", "sieht", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "$.", "PIS", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Eine undressierbar wilde Katze.", "tokens": ["Ei\u00b7ne", "un\u00b7dres\u00b7sier\u00b7bar", "wil\u00b7de", "Kat\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Die beugt sich zur\u00fcck und zieht \u2013", "tokens": ["Die", "beugt", "sich", "zu\u00b7r\u00fcck", "und", "zieht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "PTKVZ", "KON", "VVFIN", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Gott wei\u00df wie \u2013 wunderliche,", "tokens": ["Gott", "wei\u00df", "wie", "\u2013", "wun\u00b7der\u00b7li\u00b7che", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "VVFIN", "KOKOM", "$(", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Unvergleichbar sichre Zauberstriche.", "tokens": ["Un\u00b7ver\u00b7gleich\u00b7bar", "sich\u00b7re", "Zau\u00b7ber\u00b7stri\u00b7che", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Breitbeturbant geht ein Riesenkind", "tokens": ["Breit\u00b7be\u00b7tur\u00b7bant", "geht", "ein", "Rie\u00b7sen\u00b7kind"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "In dem schon geschilderten Gewande", "tokens": ["In", "dem", "schon", "ge\u00b7schil\u00b7der\u00b7ten", "Ge\u00b7wan\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADV", "ADJA", "NN"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Grinsend durch die Wiese und den Wind", "tokens": ["Grin\u00b7send", "durch", "die", "Wie\u00b7se", "und", "den", "Wind"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "KON", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Nach dem Strande.", "tokens": ["Nach", "dem", "Stran\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.6": {"line.1": {"text": "Einen dreisten Seehund sieht man in dem k\u00fchlen", "tokens": ["Ei\u00b7nen", "dreis\u00b7ten", "See\u00b7hund", "sieht", "man", "in", "dem", "k\u00fch\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PIS", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Wasser drau\u00dfen sich zu Hause f\u00fchlen.", "tokens": ["Was\u00b7ser", "drau\u00b7\u00dfen", "sich", "zu", "Hau\u00b7se", "f\u00fch\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PRF", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Echter Whisky str\u00f6mt durch echte Kehle.", "tokens": ["Ech\u00b7ter", "Whis\u00b7ky", "str\u00f6mt", "durch", "ech\u00b7te", "Keh\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Irgendein beschissner Tropf", "tokens": ["Ir\u00b7gend\u00b7ein", "be\u00b7schiss\u00b7ner", "Tropf"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Will sich \u00fcber Gro\u00dfes lustig machen.", "tokens": ["Will", "sich", "\u00fc\u00b7ber", "Gro\u00b7\u00dfes", "lus\u00b7tig", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "APPR", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Eine Flasche fliegt ihm an den Kopf.", "tokens": ["Ei\u00b7ne", "Fla\u00b7sche", "fliegt", "ihm", "an", "den", "Kopf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Es ert\u00f6nt ein echtes Lachen.", "tokens": ["Es", "er\u00b7t\u00f6nt", "ein", "ech\u00b7tes", "La\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.8": {"text": "Leise seitw\u00e4rts schreitet eine zarte Weltallseele.", "tokens": ["Lei\u00b7se", "seit\u00b7w\u00e4rts", "schrei\u00b7tet", "ei\u00b7ne", "zar\u00b7te", "Welt\u00b7all\u00b7see\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}}}}}