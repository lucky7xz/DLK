{"textgrid.poem.63726": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Sappho", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbich seh' die Stern' am Himmel schon erblassen,", "tokens": ["\u00bb", "ich", "seh'", "die", "Stern'", "am", "Him\u00b7mel", "schon", "er\u00b7blas\u00b7sen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Morgenwind weht k\u00fchl herauf vom Meere.", "tokens": ["Der", "Mor\u00b7gen\u00b7wind", "weht", "k\u00fchl", "her\u00b7auf", "vom", "Mee\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mein schweres Herz hat mich nicht schlafen lassen,", "tokens": ["Mein", "schwe\u00b7res", "Herz", "hat", "mich", "nicht", "schla\u00b7fen", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "PPER", "PTKNEG", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und auch am Tag bedr\u00fcckt mich seine Schwere.", "tokens": ["Und", "auch", "am", "Tag", "be\u00b7dr\u00fcckt", "mich", "sei\u00b7ne", "Schwe\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wo bist du, Liebster, da\u00df ich dich nicht sehe?", "tokens": ["Wo", "bist", "du", ",", "Liebs\u00b7ter", ",", "da\u00df", "ich", "dich", "nicht", "se\u00b7he", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "$,", "NN", "$,", "KOUS", "PPER", "PRF", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich finde Schlummer nur in deiner N\u00e4he.", "tokens": ["Ich", "fin\u00b7de", "Schlum\u00b7mer", "nur", "in", "dei\u00b7ner", "N\u00e4\u00b7he", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Warum bist du so weit von mir geschieden?", "tokens": ["Wa\u00b7rum", "bist", "du", "so", "weit", "von", "mir", "ge\u00b7schie\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "ADJD", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In deinem Arm nur find' ich Ruh und Frieden!\u00ab", "tokens": ["In", "dei\u00b7nem", "Arm", "nur", "find'", "ich", "Ruh", "und", "Frie\u00b7den", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "VVFIN", "PPER", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "So, als ich fr\u00fch am Tag vor\u00fcberging,", "tokens": ["So", ",", "als", "ich", "fr\u00fch", "am", "Tag", "vor\u00b7\u00fc\u00b7ber\u00b7ging", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "ADJD", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sang vom Altan ein vierzehnj\u00e4hrig Ding.", "tokens": ["Sang", "vom", "Al\u00b7tan", "ein", "vier\u00b7zehn\u00b7j\u00e4h\u00b7rig", "Ding", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "ADJD", "NN", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Tief in die Stirn fiel ihr das schwarze Haar,", "tokens": ["Tief", "in", "die", "Stirn", "fiel", "ihr", "das", "schwar\u00b7ze", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}, "line.2": {"text": "Darunter gl\u00fcht' ein schwarzes Augenpaar.", "tokens": ["Da\u00b7run\u00b7ter", "gl\u00fcht'", "ein", "schwar\u00b7zes", "Au\u00b7gen\u00b7paar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Den Rocken schwang sie in der braunen Hand,", "tokens": ["Den", "Ro\u00b7cken", "schwang", "sie", "in", "der", "brau\u00b7nen", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Den Feuerblick ins Meer hinausgespannt.", "tokens": ["Den", "Feu\u00b7er\u00b7blick", "ins", "Meer", "hin\u00b7aus\u00b7ge\u00b7spannt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Sang sie von schon erlebtem Liebesschmerz,", "tokens": ["Sang", "sie", "von", "schon", "er\u00b7leb\u00b7tem", "Lie\u00b7bes\u00b7schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie? oder tr\u00e4umt' ihn nur voraus ihr Herz?", "tokens": ["Wie", "?", "o\u00b7der", "tr\u00e4umt'", "ihn", "nur", "vo\u00b7raus", "ihr", "Herz", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "KON", "VVFIN", "PPER", "ADV", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Sie sang's vielleicht der \u00e4ltern Schwester nach,", "tokens": ["Sie", "sang's", "viel\u00b7leicht", "der", "\u00e4l\u00b7tern", "Schwes\u00b7ter", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Verstand nicht halb die Worte, die sie sprach.", "tokens": ["Ver\u00b7stand", "nicht", "halb", "die", "Wor\u00b7te", ",", "die", "sie", "sprach", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ADJD", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Gleichviel. Ihr alle seid von Sapphos Art,", "tokens": ["Gleich\u00b7viel", ".", "Ihr", "al\u00b7le", "seid", "von", "Sap\u00b7phos", "Art", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PPER", "PIS", "VAFIN", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Zu t\u00f6dlich wildem Herzweh aufgespart.", "tokens": ["Zu", "t\u00f6d\u00b7lich", "wil\u00b7dem", "Herz\u00b7weh", "auf\u00b7ge\u00b7spart", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Doch Leid um Liebe d\u00fcnkt auch euch Gewinn", "tokens": ["Doch", "Leid", "um", "Lie\u00b7be", "d\u00fcnkt", "auch", "euch", "Ge\u00b7winn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "APPR", "NN", "VVFIN", "ADV", "PPER", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und macht das \u00e4rmste Kind zur Dichterin.", "tokens": ["Und", "macht", "das", "\u00e4rms\u00b7te", "Kind", "zur", "Dich\u00b7te\u00b7rin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Ich aber will zu allen G\u00f6ttern flehn,", "tokens": ["Ich", "a\u00b7ber", "will", "zu", "al\u00b7len", "G\u00f6t\u00b7tern", "flehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dir m\u00f6ge nicht das Bitterste geschehn,", "tokens": ["Dir", "m\u00f6\u00b7ge", "nicht", "das", "Bit\u00b7ters\u00b7te", "ge\u00b7schehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Da\u00df dich dein Herz, von Kummer \u00fcberschwer,", "tokens": ["Da\u00df", "dich", "dein", "Herz", ",", "von", "Kum\u00b7mer", "\u00fc\u00b7bersc\u00b7hwer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "$,", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Herab vom hohen Felsen st\u00fcrz' ins Meer,", "tokens": ["Her\u00b7ab", "vom", "ho\u00b7hen", "Fel\u00b7sen", "st\u00fcrz'", "ins", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.13": {"line.1": {"text": "Und k\u00f6nnt' auch, \u00c4rmste, dein Rispettisingen", "tokens": ["Und", "k\u00f6nnt'", "auch", ",", "\u00c4rms\u00b7te", ",", "dein", "Ris\u00b7pet\u00b7ti\u00b7sin\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KON", "VMFIN", "ADV", "$,", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dir einer Sappho ew'gen Ruhm erringen.", "tokens": ["Dir", "ei\u00b7ner", "Sap\u00b7pho", "ew'\u00b7gen", "Ruhm", "er\u00b7rin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NE", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}