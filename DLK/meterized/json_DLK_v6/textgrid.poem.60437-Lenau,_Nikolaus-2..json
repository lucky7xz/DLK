{"textgrid.poem.60437": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mischka spielt zu einem Hochzeitreigen,", "tokens": ["Mischka", "spielt", "zu", "ei\u00b7nem", "Hoch\u00b7zei\u00b7trei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Lustgel\u00e4chter, Sporen, Gl\u00e4ser, Geigen", "tokens": ["Lust\u00b7ge\u00b7l\u00e4ch\u00b7ter", ",", "Spo\u00b7ren", ",", "Gl\u00e4\u00b7ser", ",", "Gei\u00b7gen"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Brausen wild im Edelhaus zusammen;", "tokens": ["Brau\u00b7sen", "wild", "im", "E\u00b7del\u00b7haus", "zu\u00b7sam\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und die T\u00e4nzer schie\u00dfen durcheinander,", "tokens": ["Und", "die", "T\u00e4n\u00b7zer", "schie\u00b7\u00dfen", "durch\u00b7ein\u00b7an\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Um das Brautpaar, sturmgejagte Brander", "tokens": ["Um", "das", "Braut\u00b7paar", ",", "sturm\u00b7ge\u00b7jag\u00b7te", "Bran\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KOUI", "ART", "NN", "$,", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Auf dem Meer der Lust in hellen Flammen.", "tokens": ["Auf", "dem", "Meer", "der", "Lust", "in", "hel\u00b7len", "Flam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Trauben, die des Sommers Strahl und Glut", "tokens": ["Trau\u00b7ben", ",", "die", "des", "Som\u00b7mers", "Strahl", "und", "Glut"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "NN", "KON", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Eingesogen in ihr Blut,", "tokens": ["Ein\u00b7ge\u00b7so\u00b7gen", "in", "ihr", "Blut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Str\u00f6men den empfangnen Himmel wieder", "tokens": ["Str\u00f6\u00b7men", "den", "emp\u00b7fang\u00b7nen", "Him\u00b7mel", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN", "ADV"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Den Magyaren in die Glieder.", "tokens": ["Den", "Ma\u00b7gy\u00b7a\u00b7ren", "in", "die", "Glie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Frauen, prangend in der Jugend Glanz,", "tokens": ["Frau\u00b7en", ",", "pran\u00b7gend", "in", "der", "Ju\u00b7gend", "Glanz", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Schwebend durch den Saal im raschen Tanz,", "tokens": ["Schwe\u00b7bend", "durch", "den", "Saal", "im", "ra\u00b7schen", "Tanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Und im Fluge heller Liebesblicke", "tokens": ["Und", "im", "Flu\u00b7ge", "hel\u00b7ler", "Lie\u00b7bes\u00b7bli\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Z\u00fcnden sich die seligsten Geschicke.", "tokens": ["Z\u00fcn\u00b7den", "sich", "die", "se\u00b7ligs\u00b7ten", "Ge\u00b7schi\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "ART", "ADJA", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "Ha! Musik! wie waltet Mischkas Bogen!", "tokens": ["Ha", "!", "Mu\u00b7sik", "!", "wie", "wal\u00b7tet", "Mischkas", "Bo\u00b7gen", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "NN", "$.", "PWAV", "VVFIN", "NE", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "In den Rausch wird jedes Herz gezogen,", "tokens": ["In", "den", "Rausch", "wird", "je\u00b7des", "Herz", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "PIAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Jeder Tropfen Weines scheint zu klingen,", "tokens": ["Je\u00b7der", "Trop\u00b7fen", "Wei\u00b7nes", "scheint", "zu", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "NN", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Jedes sch\u00f6ne Auge laut zu singen.", "tokens": ["Je\u00b7des", "sch\u00f6\u00b7ne", "Au\u00b7ge", "laut", "zu", "sin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Ist die Braut auch schon entschleiert,", "tokens": ["Ist", "die", "Braut", "auch", "schon", "ent\u00b7schlei\u00b7ert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Noch drei Tage, noch drei N\u00e4chte", "tokens": ["Noch", "drei", "Ta\u00b7ge", ",", "noch", "drei", "N\u00e4ch\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "CARD", "NN", "$,", "ADV", "CARD", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Wird die Hochzeit fortgefeiert", "tokens": ["Wird", "die", "Hoch\u00b7zeit", "fort\u00b7ge\u00b7fei\u00b7ert"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Von dem freuder\u00fcstigen Geschlechte.", "tokens": ["Von", "dem", "freu\u00b7de\u00b7r\u00fcs\u00b7ti\u00b7gen", "Ge\u00b7schlech\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}