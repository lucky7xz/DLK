{"textgrid.poem.39158": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Spiegel von b\u00f6sem Schimmer,", "tokens": ["Ein", "Spie\u00b7gel", "von", "b\u00f6\u00b7sem", "Schim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Das ist Dein Auge blau,", "tokens": ["Das", "ist", "Dein", "Au\u00b7ge", "blau", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Darin ich nimmer und nimmer", "tokens": ["Da\u00b7rin", "ich", "nim\u00b7mer", "und", "nim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "PPER", "ADV", "KON", "ADV"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und nimmer mich m\u00fcde schau'.", "tokens": ["Und", "nim\u00b7mer", "mich", "m\u00fc\u00b7de", "schau'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Doch ob ich schaue und schaue,", "tokens": ["Doch", "ob", "ich", "schau\u00b7e", "und", "schau\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Viel Gutes erseh' ich mir nicht,", "tokens": ["Viel", "Gu\u00b7tes", "er\u00b7seh'", "ich", "mir", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "PPER", "PTKNEG", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Nie spiegelt sich unter der Braue", "tokens": ["Nie", "spie\u00b7gelt", "sich", "un\u00b7ter", "der", "Brau\u00b7e"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Mein eigenes Angesicht.", "tokens": ["Mein", "ei\u00b7ge\u00b7nes", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Zwei fremde Augen sind es,", "tokens": ["Zwei", "frem\u00b7de", "Au\u00b7gen", "sind", "es", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "VAFIN", "PPER", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die sehen mich spottend an,", "tokens": ["Die", "se\u00b7hen", "mich", "spot\u00b7tend", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Im Auge des sch\u00f6nen Kindes,", "tokens": ["Im", "Au\u00b7ge", "des", "sch\u00f6\u00b7nen", "Kin\u00b7des", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Da malt sich ein fremder Mann.", "tokens": ["Da", "malt", "sich", "ein", "frem\u00b7der", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}