{"textgrid.poem.39138": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: K\u00f6nig Helge war ein alter Held,", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "K\u00f6nig Helge war ein alter Held,", "tokens": ["K\u00f6\u00b7nig", "Hel\u00b7ge", "war", "ein", "al\u00b7ter", "Held", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Der hatte sein Schwert zur Ruh' gestellt.", "tokens": ["Der", "hat\u00b7te", "sein", "Schwert", "zur", "Ruh'", "ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Den Panzer er in die Halle hing,", "tokens": ["Den", "Pan\u00b7zer", "er", "in", "die", "Hal\u00b7le", "hing", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Spinne Geweb' den Helm umfing.", "tokens": ["Der", "Spin\u00b7ne", "Ge\u00b7web'", "den", "Helm", "um\u00b7fing", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Sein schwarzes Schiff die Bucht umschlo\u00df,", "tokens": ["Sein", "schwar\u00b7zes", "Schiff", "die", "Bucht", "um\u00b7schlo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf der Weide trabte sein wei\u00dfes Ro\u00df.", "tokens": ["Auf", "der", "Wei\u00b7de", "trab\u00b7te", "sein", "wei\u00b7\u00dfes", "Ro\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Er waltete gut und herrschte gerecht,", "tokens": ["Er", "wal\u00b7te\u00b7te", "gut", "und", "herrschte", "ge\u00b7recht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "VVFIN", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wog strenges Ma\u00df f\u00fcr F\u00fcrst und Knecht.", "tokens": ["Wog", "stren\u00b7ges", "Ma\u00df", "f\u00fcr", "F\u00fcrst", "und", "Knecht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Das frommte Landen und Leuten ba\u00df,", "tokens": ["Das", "fromm\u00b7te", "Lan\u00b7den", "und", "Leu\u00b7ten", "ba\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "ADV", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Auf Norwegs Felsen wuchs Korn und Gras.", "tokens": ["Auf", "Nor\u00b7wegs", "Fel\u00b7sen", "wuchs", "Korn", "und", "Gras", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Den Pflug hinschleppte des Stieres Mut;", "tokens": ["Den", "Pflug", "hin\u00b7schlepp\u00b7te", "des", "Stie\u00b7res", "Mut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Kaufmann pfl\u00fcgte die blaue Flut.", "tokens": ["Der", "Kauf\u00b7mann", "pfl\u00fcg\u00b7te", "die", "blau\u00b7e", "Flut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "Aufstiegen St\u00e4dte aus w\u00fcstem Moor,", "tokens": ["Auf\u00b7stie\u00b7gen", "St\u00e4d\u00b7te", "aus", "w\u00fcs\u00b7tem", "Moor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Und Freya herrschte f\u00fcr Aukathor.", "tokens": ["Und", "Frey\u00b7a", "herrschte", "f\u00fcr", "Au\u00b7ka\u00b7thor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Der Bauer, der lebte frei und froh,", "tokens": ["Der", "Bau\u00b7er", ",", "der", "leb\u00b7te", "frei", "und", "froh", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das wollten die trotzigen Jarls nicht so.", "tokens": ["Das", "woll\u00b7ten", "die", "trot\u00b7zi\u00b7gen", "Jarls", "nicht", "so", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ART", "ADJA", "NN", "PTKNEG", "ADV", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.9": {"line.1": {"text": "Sie ritten zu Hauf', wohl drei\u00dfig und mehr,", "tokens": ["Sie", "rit\u00b7ten", "zu", "Hauf'", ",", "wohl", "drei\u00b7\u00dfig", "und", "mehr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,", "ADV", "CARD", "KON", "ADV", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "In des K\u00f6nigs Halle: da traten sie her;", "tokens": ["In", "des", "K\u00f6\u00b7nigs", "Hal\u00b7le", ":", "da", "tra\u00b7ten", "sie", "her", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "$.", "ADV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "--+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.10": {"line.1": {"text": "Da traten sie her, in Erz und Stahl,", "tokens": ["Da", "tra\u00b7ten", "sie", "her", ",", "in", "Erz", "und", "Stahl", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Vom Sporenklange dr\u00f6hnte der Saal.", "tokens": ["Vom", "Spo\u00b7ren\u00b7klan\u00b7ge", "dr\u00f6hn\u00b7te", "der", "Saal", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.11": {"line.1": {"text": "Jarl Irold vor den K\u00f6nig schritt,", "tokens": ["Jarl", "I\u00b7rold", "vor", "den", "K\u00f6\u00b7nig", "schritt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hoch war sein Helmbusch und keck sein Tritt.", "tokens": ["Hoch", "war", "sein", "Helm\u00b7busch", "und", "keck", "sein", "Tritt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPOSAT", "NN", "KON", "ADJD", "PPOSAT", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.12": {"line.1": {"text": "Sein Schwert an den Boden er rasselnd stie\u00df,", "tokens": ["Sein", "Schwert", "an", "den", "Bo\u00b7den", "er", "ras\u00b7selnd", "stie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Sein Wort er zornig erschallen lie\u00df:", "tokens": ["Sein", "Wort", "er", "zor\u00b7nig", "er\u00b7schal\u00b7len", "lie\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "ADJD", "VVINF", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.13": {"line.1": {"text": "\u00bbwir wollen nicht sitzen und Spindeln drehn,", "tokens": ["\u00bb", "wir", "wol\u00b7len", "nicht", "sit\u00b7zen", "und", "Spin\u00b7deln", "drehn", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "PTKNEG", "VVINF", "KON", "NN", "VVINF", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Mit dem Normannsschwerte nicht Hafer m\u00e4hn.", "tokens": ["Mit", "dem", "Nor\u00b7manns\u00b7schwer\u00b7te", "nicht", "Ha\u00b7fer", "m\u00e4hn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKNEG", "NN", "VVINF", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.14": {"line.1": {"text": "Wir wollen furchen, wie Harald tat,", "tokens": ["Wir", "wol\u00b7len", "fur\u00b7chen", ",", "wie", "Ha\u00b7rald", "tat", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$,", "PWAV", "NE", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mit dem schwarzen Segler den feuchten Pfad.", "tokens": ["Mit", "dem", "schwar\u00b7zen", "Seg\u00b7ler", "den", "feuch\u00b7ten", "Pfad", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.15": {"line.1": {"text": "Wir wollen tragen, wie Rollo trug,", "tokens": ["Wir", "wol\u00b7len", "tra\u00b7gen", ",", "wie", "Rol\u00b7lo", "trug", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$,", "PWAV", "NE", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Auf S\u00fcdlands Acker den Nordlandspflug.", "tokens": ["Auf", "S\u00fcd\u00b7lands", "A\u00b7cker", "den", "Nord\u00b7lands\u00b7pflug", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.16": {"line.1": {"text": "Wir sind des K\u00f6nigs m\u00fcd' und satt,", "tokens": ["Wir", "sind", "des", "K\u00f6\u00b7nigs", "m\u00fcd'", "und", "satt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der immer das Schwert in der Scheide hat.", "tokens": ["Der", "im\u00b7mer", "das", "Schwert", "in", "der", "Schei\u00b7de", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "APPR", "ART", "NN", "VAFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.17": {"line.1": {"text": "Wir sind des K\u00f6nigs satt und m\u00fcd',", "tokens": ["Wir", "sind", "des", "K\u00f6\u00b7nigs", "satt", "und", "m\u00fcd'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Unkraut j\u00e4tet und R\u00fcben zieht.", "tokens": ["Der", "Un\u00b7kraut", "j\u00e4\u00b7tet", "und", "R\u00fc\u00b7ben", "zieht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "KON", "NN", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.18": {"line.1": {"text": "Und wer will z\u00e4hmen des Normanns Blut,", "tokens": ["Und", "wer", "will", "z\u00e4h\u00b7men", "des", "Nor\u00b7manns", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VMFIN", "VVINF", "ART", "NN", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der halte das Schwert und halt' es gut!\u00ab", "tokens": ["Der", "hal\u00b7te", "das", "Schwert", "und", "halt'", "es", "gut", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "KON", "VVFIN", "PPER", "ADJD", "$.", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.19": {"line.1": {"text": "Jarl Irold sprach's; der K\u00f6nig schwieg,", "tokens": ["Jarl", "I\u00b7rold", "sprach's", ";", "der", "K\u00f6\u00b7nig", "schwieg", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "$.", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf der Stirn ihm grimmig die Ader stieg;", "tokens": ["Auf", "der", "Stirn", "ihm", "grim\u00b7mig", "die", "A\u00b7der", "stieg", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.20": {"line.1": {"text": "Aus den Augen fuhr's ihm, wie Blitz und Flamm',", "tokens": ["Aus", "den", "Au\u00b7gen", "fuhr's", "ihm", ",", "wie", "Blitz", "und", "Flamm'", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "$,", "PWAV", "NN", "KON", "NN", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Brust ward voll, die Faust ward stramm.", "tokens": ["Die", "Brust", "ward", "voll", ",", "die", "Faust", "ward", "stramm", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "VAFIN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.21": {"line.1": {"text": "Aus dem Sessel sprang er, der krachend brach;", "tokens": ["Aus", "dem", "Ses\u00b7sel", "sprang", "er", ",", "der", "kra\u00b7chend", "brach", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "$,", "PRELS", "ADJD", "VVFIN", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wie dumpfer Donner er also sprach:", "tokens": ["Wie", "dum\u00b7pfer", "Don\u00b7ner", "er", "al\u00b7so", "sprach", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.22": {"line.1": {"text": "\u00bbmein Aug' ist tr\u00fcb', mein Haupt ist kahl,", "tokens": ["\u00bb", "mein", "Aug'", "ist", "tr\u00fcb'", ",", "mein", "Haupt", "ist", "kahl", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "VAFIN", "ADJD", "$,", "PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Am Nagel rostet mein guter Stahl.", "tokens": ["Am", "Na\u00b7gel", "ros\u00b7tet", "mein", "gu\u00b7ter", "Stahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NE", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.23": {"line.1": {"text": "Und tragt nach dem Schwert Ihr so hei\u00dfen Trieb,", "tokens": ["Und", "tragt", "nach", "dem", "Schwert", "Ihr", "so", "hei\u00b7\u00dfen", "Trieb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "So nehmt f\u00fcr heut mit der Faust vorlieb.\u00ab", "tokens": ["So", "nehmt", "f\u00fcr", "heut", "mit", "der", "Faust", "vor\u00b7lieb", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADV", "APPR", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.24": {"line.1": {"text": "Der K\u00f6nig sprach es und macht' es kurz:", "tokens": ["Der", "K\u00f6\u00b7nig", "sprach", "es", "und", "macht'", "es", "kurz", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "KON", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Er hieb den Jarl auf den Helmessturz.", "tokens": ["Er", "hieb", "den", "Ja