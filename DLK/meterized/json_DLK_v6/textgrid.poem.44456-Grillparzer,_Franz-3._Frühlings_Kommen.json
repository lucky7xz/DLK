{"textgrid.poem.44456": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "3. Fr\u00fchlings Kommen", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der W\u00e4chter auf den Zinnen", "tokens": ["Der", "W\u00e4ch\u00b7ter", "auf", "den", "Zin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Treibt gar gewaltgen Spuk.", "tokens": ["Treibt", "gar", "ge\u00b7walt\u00b7gen", "Spuk", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJA", "NN", "$."], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.3": {"text": "Sieht er wohl G\u00e4ste kommen?", "tokens": ["Sieht", "er", "wohl", "G\u00e4s\u00b7te", "kom\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Er schreit: \u00bbGuck, guck! Guckguck!\u00ab", "tokens": ["Er", "schreit", ":", "\u00bb", "Guck", ",", "guck", "!", "Guck\u00b7guck", "!", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "NE", "$,", "VVIMP", "$.", "NE", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ein Diener auf sein Rufen", "tokens": ["Ein", "Die\u00b7ner", "auf", "sein", "Ru\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Herum im Hause geht,", "tokens": ["He\u00b7rum", "im", "Hau\u00b7se", "geht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der nimmt die wei\u00dfen H\u00fcllen", "tokens": ["Der", "nimmt", "die", "wei\u00b7\u00dfen", "H\u00fcl\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vom schimmernden Ger\u00e4t.", "tokens": ["Vom", "schim\u00b7mern\u00b7den", "Ge\u00b7r\u00e4t", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ein andrer breitet Teppich,", "tokens": ["Ein", "an\u00b7drer", "brei\u00b7tet", "Tep\u00b7pich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Milchfarb und rosenrot;", "tokens": ["Milch\u00b7farb", "und", "ro\u00b7sen\u00b7rot", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Baumwollen das Gewebe:", "tokens": ["Baum\u00b7wol\u00b7len", "das", "Ge\u00b7we\u00b7be", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "++-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Baum die Wolle bot.", "tokens": ["Der", "Baum", "die", "Wol\u00b7le", "bot", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Drauf kommen Musikanten,", "tokens": ["Drauf", "kom\u00b7men", "Mu\u00b7si\u00b7kan\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sie stimmen, proben nie,", "tokens": ["Sie", "stim\u00b7men", ",", "pro\u00b7ben", "nie", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und doch, kommts nun zum Spielen,", "tokens": ["Und", "doch", ",", "kommts", "nun", "zum", "Spie\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie herrlich stimmen sie.", "tokens": ["Wie", "herr\u00b7lich", "stim\u00b7men", "sie", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ein Vorhang, rot von Seide,", "tokens": ["Ein", "Vor\u00b7hang", ",", "rot", "von", "Sei\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "APPR", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Fliegt weichend von der T\u00fcr,", "tokens": ["Fliegt", "wei\u00b7chend", "von", "der", "T\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Pf\u00f6rtner, golden schimmernd,", "tokens": ["Der", "Pf\u00f6rt\u00b7ner", ",", "gol\u00b7den", "schim\u00b7mernd", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Kommt \u00f6ffnend draus herf\u00fcr.", "tokens": ["Kommt", "\u00f6ff\u00b7nend", "draus", "her\u00b7f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PAV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Halb zieht er nur den Vorhang,", "tokens": ["Halb", "zieht", "er", "nur", "den", "Vor\u00b7hang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df Tag und Dunkel gleich,", "tokens": ["Da\u00df", "Tag", "und", "Dun\u00b7kel", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da tritt herein der Fremdling,", "tokens": ["Da", "tritt", "her\u00b7ein", "der", "Fremd\u00b7ling", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKVZ", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein K\u00f6nig in sein Reich.", "tokens": ["Ein", "K\u00f6\u00b7nig", "in", "sein", "Reich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Was Augen hat, schlie\u00dft auf sie,", "tokens": ["Was", "Au\u00b7gen", "hat", ",", "schlie\u00dft", "auf", "sie", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VAFIN", "$,", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.2": {"text": "Im Garten Haupt an Haupt,", "tokens": ["Im", "Gar\u00b7ten", "Haupt", "an", "Haupt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Am Raine schiebt und dr\u00e4ngt sichs,", "tokens": ["Am", "Rai\u00b7ne", "schiebt", "und", "dr\u00e4ngt", "sichs", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "KON", "VVFIN", "PIS", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die G\u00e4nge stehn umlaubt.", "tokens": ["Die", "G\u00e4n\u00b7ge", "stehn", "um\u00b7laubt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Am Tor auch pochts des Herzens.", "tokens": ["Am", "Tor", "auch", "pochts", "des", "Her\u00b7zens", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Willst hier auch freien Lauf?", "tokens": ["Willst", "hier", "auch", "frei\u00b7en", "Lauf", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nun, bringst du sch\u00f6ne Lieder,", "tokens": ["Nun", ",", "bringst", "du", "sch\u00f6\u00b7ne", "Lie\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So mach ich dir wohl auf.", "tokens": ["So", "mach", "ich", "dir", "wohl", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}