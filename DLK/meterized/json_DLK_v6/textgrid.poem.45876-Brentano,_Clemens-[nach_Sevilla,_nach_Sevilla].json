{"textgrid.poem.45876": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[nach Sevilla, nach Sevilla]", "genre": "verse", "period": "N.A.", "pub_year": 1802, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nach Sevilla, nach Sevilla,", "tokens": ["Nach", "Se\u00b7vil\u00b7la", ",", "nach", "Se\u00b7vil\u00b7la", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo die hohen Prachtgeb\u00e4ude", "tokens": ["Wo", "die", "ho\u00b7hen", "Pracht\u00b7ge\u00b7b\u00e4u\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In den breiten Stra\u00dfen stehen,", "tokens": ["In", "den", "brei\u00b7ten", "Stra\u00b7\u00dfen", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aus den Fenstern reiche Leute,", "tokens": ["Aus", "den", "Fens\u00b7tern", "rei\u00b7che", "Leu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sch\u00f6n geputzte Frauen sehn,", "tokens": ["Sch\u00f6n", "ge\u00b7putz\u00b7te", "Frau\u00b7en", "sehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Dahin sehnt mein Herz sich nicht!", "tokens": ["Da\u00b7hin", "sehnt", "mein", "Herz", "sich", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPOSAT", "NN", "PRF", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Nach Sevilla, nach Sevilla,", "tokens": ["Nach", "Se\u00b7vil\u00b7la", ",", "nach", "Se\u00b7vil\u00b7la", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo die letzten H\u00e4user stehen,", "tokens": ["Wo", "die", "letz\u00b7ten", "H\u00e4u\u00b7ser", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sich die Nachbarn freundlich gr\u00fc\u00dfen,", "tokens": ["Sich", "die", "Nach\u00b7barn", "freund\u00b7lich", "gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "M\u00e4dchen aus dem Fenster sehn,", "tokens": ["M\u00e4d\u00b7chen", "aus", "dem", "Fens\u00b7ter", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ihre Blumen zu begie\u00dfen,", "tokens": ["Ih\u00b7re", "Blu\u00b7men", "zu", "be\u00b7gie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ach, da sehnt mein Herz sich hin!", "tokens": ["Ach", ",", "da", "sehnt", "mein", "Herz", "sich", "hin", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "VVFIN", "PPOSAT", "NN", "PRF", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "In Sevilla, in Sevilla", "tokens": ["In", "Se\u00b7vil\u00b7la", ",", "in", "Se\u00b7vil\u00b7la"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "NE", "$,", "APPR", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wei\u00df ich wohl ein reines St\u00fcbchen,", "tokens": ["Wei\u00df", "ich", "wohl", "ein", "rei\u00b7nes", "St\u00fcb\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Helle K\u00fcche, stille Kammer,", "tokens": ["Hel\u00b7le", "K\u00fc\u00b7che", ",", "stil\u00b7le", "Kam\u00b7mer", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In dem Hause wohnt mein Liebchen,", "tokens": ["In", "dem", "Hau\u00b7se", "wohnt", "mein", "Lieb\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und am Pf\u00f6rtchen gl\u00e4nzt ein Hammer.", "tokens": ["Und", "am", "Pf\u00f6rt\u00b7chen", "gl\u00e4nzt", "ein", "Ham\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Poch' ich, macht die Jungfrau auf!", "tokens": ["Poch'", "ich", ",", "macht", "die", "Jung\u00b7frau", "auf", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Guten Abend, guten Abend \u2013", "tokens": ["Gu\u00b7ten", "A\u00b7bend", ",", "gu\u00b7ten", "A\u00b7bend", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lieber Vater, setzt euch nieder,", "tokens": ["Lie\u00b7ber", "Va\u00b7ter", ",", "setzt", "euch", "nie\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ei, wo seid ihr dann gewesen?", "tokens": ["Ei", ",", "wo", "seid", "ihr", "dann", "ge\u00b7we\u00b7sen", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "VAFIN", "PPER", "ADV", "VAPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und dann singt sie sch\u00f6ne Lieder,", "tokens": ["Und", "dann", "singt", "sie", "sch\u00f6\u00b7ne", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Kann so h\u00fcbsch in B\u00fcchern lesen,", "tokens": ["Kann", "so", "h\u00fcbsch", "in", "B\u00fc\u00b7chern", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADJD", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ach! und ist mein einzig Kind.", "tokens": ["Ach", "!", "und", "ist", "mein", "ein\u00b7zig", "Kind", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "KON", "VAFIN", "PPOSAT", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}