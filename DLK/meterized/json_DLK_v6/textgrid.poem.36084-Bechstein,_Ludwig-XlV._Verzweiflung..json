{"textgrid.poem.36084": {"metadata": {"author": {"name": "Bechstein, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "XlV. Verzweiflung.", "genre": "verse", "period": "N.A.", "pub_year": 1830, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wach auf, wach auf! Was liegst Du so tr\u00e4g, fast leblos da?", "tokens": ["Wach", "auf", ",", "wach", "auf", "!", "Was", "liegst", "Du", "so", "tr\u00e4g", ",", "fast", "leb\u00b7los", "da", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "ADJD", "PTKVZ", "$.", "PWS", "VVFIN", "PPER", "ADV", "ADJD", "$,", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Wach auf Du Gottverworfner, Dein letzter Tag ist nah!", "tokens": ["Wach", "auf", "Du", "Gott\u00b7ver\u00b7worf\u00b7ner", ",", "Dein", "letz\u00b7ter", "Tag", "ist", "nah", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "NN", "$,", "PPOSAT", "ADJA", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ob grausendes Entsetzen Dir durch die Nerven bebt,", "tokens": ["Ob", "grau\u00b7sen\u00b7des", "Ent\u00b7set\u00b7zen", "Dir", "durch", "die", "Ner\u00b7ven", "bebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Dein Sand ist ausgelaufen, Faustus hat ausgelebt!", "tokens": ["Dein", "Sand", "ist", "aus\u00b7ge\u00b7lau\u00b7fen", ",", "Faus\u00b7tus", "hat", "aus\u00b7ge\u00b7lebt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,", "NE", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "\u00bbweh mir! Weh mir! Was wollt ihr, ihr Stimmen schwarzer Nacht?", "tokens": ["\u00bb", "weh", "mir", "!", "Weh", "mir", "!", "Was", "wollt", "ihr", ",", "ihr", "Stim\u00b7men", "schwar\u00b7zer", "Nacht", "?"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PPER", "$.", "NN", "PPER", "$.", "PWS", "VMFIN", "PPER", "$,", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": "Ihr, die mit furchtbar'm Weckruf mein Herz erzittern macht?", "tokens": ["Ihr", ",", "die", "mit", "furcht\u00b7ba\u00b7r'm", "Weck\u00b7ruf", "mein", "Herz", "er\u00b7zit\u00b7tern", "macht", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "APPR", "ADJA", "NN", "PPOSAT", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Und die Gedanken zittern, zerst\u00e4uben, kreiselnd drehn?", "tokens": ["Und", "die", "Ge\u00b7dan\u00b7ken", "zit\u00b7tern", ",", "zer\u00b7st\u00e4u\u00b7ben", ",", "krei\u00b7selnd", "drehn", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "$,", "VVPP", "$,", "ADJD", "VVINF", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Wie wird mir! Wehe, wehe! \u2013 Ist's schon um mich gescheh'n?\u00ab", "tokens": ["Wie", "wird", "mir", "!", "We\u00b7he", ",", "we\u00b7he", "!", "\u2013", "Ist's", "schon", "um", "mich", "ge\u00b7scheh'n", "?", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "$.", "NN", "$,", "ADJD", "$.", "$(", "NE", "ADV", "APPR", "PPER", "VVPP", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "\u00bbhalt! Wartet noch ein wenig! Dort \u2013 noch ein K\u00f6rnlein Sand!", "tokens": ["\u00bb", "halt", "!", "War\u00b7tet", "noch", "ein", "we\u00b7nig", "!", "Dort", "\u2013", "noch", "ein", "K\u00f6rn\u00b7lein", "Sand", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$.", "VAFIN", "ADV", "ART", "PIS", "$.", "ADV", "$(", "ADV", "ART", "NN", "NN", "$."], "meter": "-+---+----+-+", "measure": "dactylic.init"}, "line.2": {"text": "Halt! Sch\u00fcttelt an der Uhr nicht! Lasst stehn sie, wo sie stand.", "tokens": ["Halt", "!", "Sch\u00fct\u00b7telt", "an", "der", "Uhr", "nicht", "!", "Lasst", "stehn", "sie", ",", "wo", "sie", "stand", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "$.", "NN", "APPR", "ART", "NN", "PTKNEG", "$.", "NN", "VVFIN", "PPER", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Habt Ihr so eilig, Teufel! K\u00f6nnt Zeit erwarten nicht,", "tokens": ["Habt", "Ihr", "so", "ei\u00b7lig", ",", "Teu\u00b7fel", "!", "K\u00f6nnt", "Zeit", "er\u00b7war\u00b7ten", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "$,", "NN", "$.", "NN", "NN", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Bis sie zu Scherben schmettert, bis sie in Splitter bricht?\u00ab \u2013", "tokens": ["Bis", "sie", "zu", "Scher\u00b7ben", "schmet\u00b7tert", ",", "bis", "sie", "in", "Split\u00b7ter", "bricht", "?", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVFIN", "$,", "KOUS", "PPER", "APPR", "NN", "VVFIN", "$.", "$(", "$("], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "\u00bbes tr\u00e4gt der Baum des Lebens verschiedne Bl\u00fcth' und Frucht;", "tokens": ["\u00bb", "es", "tr\u00e4gt", "der", "Baum", "des", "Le\u00b7bens", "ver\u00b7schied\u00b7ne", "Bl\u00fcth'", "und", "Frucht", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ART", "NN", "ART", "NN", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Die einen sind gesegnet, die andern sind verflucht!", "tokens": ["Die", "ei\u00b7nen", "sind", "ge\u00b7seg\u00b7net", ",", "die", "an\u00b7dern", "sind", "ver\u00b7flucht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VAFIN", "VVPP", "$,", "PRELS", "PIS", "VAFIN", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Auch die verfluchten schimmern, geschm\u00fcckt von Gottes Hand,", "tokens": ["Auch", "die", "ver\u00b7fluch\u00b7ten", "schim\u00b7mern", ",", "ge\u00b7schm\u00fcckt", "von", "Got\u00b7tes", "Hand", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "VVFIN", "$,", "VVPP", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Doch innen sind sie Moder, Gew\u00fcrm und Asch' und Brand!\u00ab", "tokens": ["Doch", "in\u00b7nen", "sind", "sie", "Mo\u00b7der", ",", "Ge\u00b7w\u00fcrm", "und", "Asch'", "und", "Brand", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "NN", "$,", "NN", "KON", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "\u00bbich bin die Frucht Gomorras, die in sich selbst zerf\u00fcllt,", "tokens": ["\u00bb", "ich", "bin", "die", "Frucht", "Go\u00b7mor\u00b7ras", ",", "die", "in", "sich", "selbst", "zer\u00b7f\u00fcllt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ART", "NN", "NE", "$,", "PRELS", "APPR", "PRF", "ADV", "VVPP", "$,"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Der W\u00fcste Koloquinte, die S\u00fcssigkeit verg\u00e4llt.", "tokens": ["Der", "W\u00fcs\u00b7te", "Ko\u00b7loq\u00b7uin\u00b7te", ",", "die", "S\u00fcs\u00b7sig\u00b7keit", "ver\u00b7g\u00e4llt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "O bitter, bitter alles, ein giftiger Genuss!", "tokens": ["O", "bit\u00b7ter", ",", "bit\u00b7ter", "al\u00b7les", ",", "ein", "gif\u00b7ti\u00b7ger", "Ge\u00b7nuss", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "$,", "ADJD", "PIS", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Mein Honig ward zur Galle, l\u00e4ngst Lust zum Ueberdruss!\u00ab", "tokens": ["Mein", "Ho\u00b7nig", "ward", "zur", "Gal\u00b7le", ",", "l\u00e4ngst", "Lust", "zum", "Ue\u00b7berd\u00b7russ", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPRART", "NN", "$,", "ADV", "NN", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.6": {"line.1": {"text": "\u00bbvon Ewigkeit bestimmt sein zu solch endloser Qual,", "tokens": ["\u00bb", "von", "E\u00b7wig\u00b7keit", "be\u00b7stimmt", "sein", "zu", "solch", "end\u00b7lo\u00b7ser", "Qual", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "NN", "VVPP", "VAINF", "APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Das nenn' ich Vaterg\u00fcte, das nenn' ich Gnadenwahl!", "tokens": ["Das", "nenn'", "ich", "Va\u00b7ter\u00b7g\u00fc\u00b7te", ",", "das", "nenn'", "ich", "Gna\u00b7den\u00b7wahl", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "NN", "$,", "PDS", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Fluch ihr, die mich verflucht hat! Die mich verworfen! \u2013 Wie?", "tokens": ["Fluch", "ihr", ",", "die", "mich", "ver\u00b7flucht", "hat", "!", "Die", "mich", "ver\u00b7wor\u00b7fen", "!", "\u2013", "Wie", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["NN", "PPER", "$,", "PRELS", "PPER", "VVPP", "VAFIN", "$.", "ART", "PPER", "VVPP", "$.", "$(", "PWAV", "$."], "meter": "+-+--++-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Wir haben uns \u00fcberworfen, verworfen sie mich \u2013 ich sie!\u00ab \u2013", "tokens": ["Wir", "ha\u00b7ben", "uns", "\u00fc\u00b7ber\u00b7wor\u00b7fen", ",", "ver\u00b7wor\u00b7fen", "sie", "mich", "\u2013", "ich", "sie", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVPP", "$,", "VVFIN", "PPER", "PPER", "$(", "PPER", "PPER", "$.", "$(", "$("], "meter": "-+--+-+--+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.7": {"line.1": {"text": "\u00bbich bin verflucht! Das ist es; vom Anbeginn verflucht!", "tokens": ["\u00bb", "ich", "bin", "ver\u00b7flucht", "!", "Das", "ist", "es", ";", "vom", "An\u00b7be\u00b7ginn", "ver\u00b7flucht", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "VVPP", "$.", "PDS", "VAFIN", "PPER", "$.", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Verflucht, eh' ich erzeugt ward! Hab' ich denn Gott versucht?", "tokens": ["Ver\u00b7flucht", ",", "eh'", "ich", "er\u00b7zeugt", "ward", "!", "Hab'", "ich", "denn", "Gott", "ver\u00b7sucht", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KOUS", "PPER", "VVPP", "VAFIN", "$.", "NN", "PPER", "ADV", "NN", "VVPP", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Hab' ich denn Gott gel\u00e4stert? \u2013 Es ist kein Kind so rein;", "tokens": ["Hab'", "ich", "denn", "Gott", "ge\u00b7l\u00e4s\u00b7tert", "?", "\u2013", "Es", "ist", "kein", "Kind", "so", "rein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "NN", "VVPP", "$.", "$(", "PPER", "VAFIN", "PIAT", "NN", "ADV", "ADJD", "$."], "meter": "---+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Nur der Bestimmung folgt' ich, und ", "tokens": ["Nur", "der", "Be\u00b7stim\u00b7mung", "folgt'", "ich", ",", "und"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPER", "$,", "KON"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "\u00bb", "tokens": ["\u00bb"], "token_info": ["punct"], "pos": ["$("]}, "line.2": {"text": "Du bist die ehrne Pforte, durch die das Leben geht!", "tokens": ["Du", "bist", "die", "ehr\u00b7ne", "Pfor\u00b7te", ",", "durch", "die", "das", "Le\u00b7ben", "geht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,", "APPR", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Durch Himmel fliegen Sterne, s\u00fcss t\u00f6nt ihr Fl\u00fcgelschlag,", "tokens": ["Durch", "Him\u00b7mel", "flie\u00b7gen", "Ster\u00b7ne", ",", "s\u00fcss", "t\u00f6nt", "ihr", "Fl\u00fc\u00b7gel\u00b7schlag", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "$,", "ADJD", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Sie kommen still gehorsam, all' der Bestimmung nach.\u00ab", "tokens": ["Sie", "kom\u00b7men", "still", "ge\u00b7hor\u00b7sam", ",", "all'", "der", "Be\u00b7stim\u00b7mung", "nach", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ADJD", "$,", "PIS", "ART", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.9": {"line.1": {"text": "\u00bbauch ich! Auch ich, ich komme! Ruft ihr, und ist's nun Zeit?", "tokens": ["\u00bb", "auch", "ich", "!", "Auch", "ich", ",", "ich", "kom\u00b7me", "!", "Ruft", "ihr", ",", "und", "ist's", "nun", "Zeit", "?"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "PPER", "$.", "ADV", "PPER", "$,", "PPER", "VVFIN", "$.", "VVFIN", "PPER", "$,", "KON", "VAFIN", "ADV", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ich bin zum letzten Gang schon, bin zum Triumph bereit!", "tokens": ["Ich", "bin", "zum", "letz\u00b7ten", "Gang", "schon", ",", "bin", "zum", "Tri\u00b7umph", "be\u00b7reit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "ADJA", "NN", "ADV", "$,", "VAFIN", "APPRART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.3": {"text": "Schm\u00fcck' H\u00f6lle Deine Hallen mit hellem Flammenschein,", "tokens": ["Schm\u00fcck", "H\u00f6l\u00b7le", "Dei\u00b7ne", "Hal\u00b7len", "mit", "hel\u00b7lem", "Flam\u00b7men\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und lass Dein Heulen schallen, gleich zieht Dein K\u00f6nig ein!\u00ab", "tokens": ["Und", "lass", "Dein", "Heu\u00b7len", "schal\u00b7len", ",", "gleich", "zieht", "Dein", "K\u00f6\u00b7nig", "ein", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "VVINF", "$,", "ADV", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.10": {"line.1": {"text": "\u00bbich, ich \u2013 ich bin Dein K\u00f6nig! Mir Deine Krone, mir!", "tokens": ["\u00bb", "ich", ",", "ich", "\u2013", "ich", "bin", "Dein", "K\u00f6\u00b7nig", "!", "Mir", "Dei\u00b7ne", "Kro\u00b7ne", ",", "mir", "!"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "PPER", "$,", "PPER", "$(", "PPER", "VAFIN", "PPOSAT", "NN", "$.", "NE", "PPOSAT", "NN", "$,", "PPER", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Wie gl\u00fchend gl\u00e4nzt der Goldreif, des Hauptes stolze Zier!", "tokens": ["Wie", "gl\u00fc\u00b7hend", "gl\u00e4nzt", "der", "Gold\u00b7reif", ",", "des", "Haup\u00b7tes", "stol\u00b7ze", "Zier", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "ART", "NN", "$,", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Den Herrscher st\u00fcrz' ich nieder, dem dieses Reich geh\u00f6rt,", "tokens": ["Den", "Herr\u00b7scher", "st\u00fcrz'", "ich", "nie\u00b7der", ",", "dem", "die\u00b7ses", "Reich", "ge\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,", "PRELS", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Nicht vor dem Teufel zittert, wer sich gen Gott emp\u00f6rt!\u00ab", "tokens": ["Nicht", "vor", "dem", "Teu\u00b7fel", "zit\u00b7tert", ",", "wer", "sich", "gen", "Gott", "em\u00b7p\u00f6rt", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "VVFIN", "$,", "PWS", "PRF", "APPR", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.11": {"line.1": {"text": "\u00bbich Faustus, ich Dein K\u00f6nig! Schreibt es ans Firmament,", "tokens": ["\u00bb", "ich", "Faus\u00b7tus", ",", "ich", "Dein", "K\u00f6\u00b7nig", "!", "Schreibt", "es", "ans", "Fir\u00b7ma\u00b7ment", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "NE", "$,", "PPER", "PPOSAT", "NN", "$.", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Dass hell die Schrift den Herrscher der Welt ins Auge brennt!", "tokens": ["Dass", "hell", "die", "Schrift", "den", "Herr\u00b7scher", "der", "Welt", "ins", "Au\u00b7ge", "brennt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ART", "NN", "ART", "NN", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ruft es in allen Landen laut aus, und kommt heran!", "tokens": ["Ruft", "es", "in", "al\u00b7len", "Lan\u00b7den", "laut", "aus", ",", "und", "kommt", "he\u00b7ran", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PIAT", "NN", "ADJD", "PTKVZ", "$,", "KON", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "K\u00f6nnt ihr auch beten, Teufel? Vor mir dann betet an!\u00ab", "tokens": ["K\u00f6nnt", "ihr", "auch", "be\u00b7ten", ",", "Teu\u00b7fel", "?", "Vor", "mir", "dann", "be\u00b7tet", "an", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "PPER", "ADV", "VVINF", "$,", "NN", "$.", "APPR", "PPER", "ADV", "VVFIN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+----+", "measure": "unknown.measure.penta"}}, "stanza.12": {"line.1": {"text": "\u00bbvon mir ist schon geweissagt! Ich bin der Abbadon!", "tokens": ["\u00bb", "von", "mir", "ist", "schon", "ge\u00b7weis\u00b7sagt", "!", "Ich", "bin", "der", "Ab\u00b7ba\u00b7don", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PPER", "VAFIN", "ADV", "VVPP", "$.", "PPER", "VAFIN", "ART", "NN", "$."], "meter": "+-+---+-+-+-+", "measure": "unknown.measure.hexa"}, "line.2": {"text": "Ich bin des Abgrunds Engel, bin der Apollyon!", "tokens": ["Ich", "bin", "des", "Ab\u00b7grunds", "En\u00b7gel", ",", "bin", "der", "A\u00b7pol\u00b7ly\u00b7on", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NE", "$,", "VAFIN", "ART", "NE", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein Sch\u00e4nder, ein Verderber \u2013 vom Richter schon gerichtet,", "tokens": ["Ein", "Sch\u00e4n\u00b7der", ",", "ein", "Ver\u00b7der\u00b7ber", "\u2013", "vom", "Rich\u00b7ter", "schon", "ge\u00b7rich\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$(", "APPRART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Ein nichtig Nichts, ein Giftstaub, vom Hauch des Herrn vernichtet!\u00ab \u2013", "tokens": ["Ein", "nich\u00b7tig", "Nichts", ",", "ein", "Gift\u00b7staub", ",", "vom", "Hauch", "des", "Herrn", "ver\u00b7nich\u00b7tet", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "ADJD", "PIS", "$,", "ART", "NN", "$,", "APPRART", "NN", "ART", "NN", "VVPP", "$.", "$(", "$("], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.13": {"line.1": {"text": "\u00bbich habe Dich gel\u00e4stert, zerschmettre mein Gebein!", "tokens": ["\u00bb", "ich", "ha\u00b7be", "Dich", "ge\u00b7l\u00e4s\u00b7tert", ",", "zer\u00b7schmett\u00b7re", "mein", "Ge\u00b7bein", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "PPER", "VVPP", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und Donnerkeile schleudre, lass sie Vernichtung sein!", "tokens": ["Und", "Don\u00b7ner\u00b7kei\u00b7le", "schleud\u00b7re", ",", "lass", "sie", "Ver\u00b7nich\u00b7tung", "sein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$,", "VVFIN", "PPER", "NN", "VAINF", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Raff' mich in Windeswirbeln hinweg! Du warst mir Spott!", "tokens": ["Raff'", "mich", "in", "Win\u00b7des\u00b7wir\u00b7beln", "hin\u00b7weg", "!", "Du", "warst", "mir", "Spott", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "NN", "PTKVZ", "$.", "PPER", "VAFIN", "PPER", "NN", "$."], "meter": "+--+--+-+-+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Nimm Rache durch Vernichtung, mein \u2013 nein, nein, nicht ", "tokens": ["Nimm", "Ra\u00b7che", "durch", "Ver\u00b7nich\u00b7tung", ",", "mein", "\u2013", "nein", ",", "nein", ",", "nicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["NE", "NN", "APPR", "NN", "$,", "PPOSAT", "$(", "PTKANT", "$,", "PTKANT", "$,", "PTKNEG"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.14": {"line.1": {"text": "\u00bbwie's auf der Stirn mir kalt liegt, wie feuchtes kaltes Eis!", "tokens": ["\u00bb", "wie's", "auf", "der", "Stirn", "mir", "kalt", "liegt", ",", "wie", "feuch\u00b7tes", "kal\u00b7tes", "Eis", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "APPR", "ART", "NN", "PPER", "ADJD", "VVFIN", "$,", "PWAV", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Wie die Gedanken brennen, so gl\u00fchend, gl\u00fchendheiss!", "tokens": ["Wie", "die", "Ge\u00b7dan\u00b7ken", "bren\u00b7nen", ",", "so", "gl\u00fc\u00b7hend", ",", "gl\u00fc\u00b7hend\u00b7heiss", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVINF", "$,", "ADV", "ADJD", "$,", "VVFIN", "$."], "meter": "---+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wer will Gedanken kaufen? Wer giebt mir Gift darum?", "tokens": ["Wer", "will", "Ge\u00b7dan\u00b7ken", "kau\u00b7fen", "?", "Wer", "giebt", "mir", "Gift", "da\u00b7rum", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "NN", "VVINF", "$.", "PWS", "VVFIN", "PPER", "NN", "PAV", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Ha denken, denken! Wehe, das Denken bringt mich um!\u00ab", "tokens": ["Ha", "den\u00b7ken", ",", "den\u00b7ken", "!", "We\u00b7he", ",", "das", "Den\u00b7ken", "bringt", "mich", "um", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "VVINF", "$,", "VVINF", "$.", "NN", "$,", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.15": {"line.1": {"text": "\u00bbgebt mir ein Schwert, zu spiessen im Hirn die M\u00fcckenbrut,", "tokens": ["\u00bb", "gebt", "mir", "ein", "Schwert", ",", "zu", "spies\u00b7sen", "im", "Hirn", "die", "M\u00fc\u00b7cken\u00b7brut", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "ART", "NN", "$,", "PTKZU", "VVINF", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Die mir mit ihrem Summen so weh, so wehe thut!", "tokens": ["Die", "mir", "mit", "ih\u00b7rem", "Sum\u00b7men", "so", "weh", ",", "so", "we\u00b7he", "thut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPOSAT", "NN", "ADV", "PTKVZ", "$,", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Oh! \u2013 Ha, dort seh' ich blinken und winken scharfen Stahl!", "tokens": ["Oh", "!", "\u2013", "Ha", ",", "dort", "seh'", "ich", "blin\u00b7ken", "und", "win\u00b7ken", "schar\u00b7fen", "Stahl", "!"], "token_info": ["word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "$(", "ITJ", "$,", "ADV", "VVFIN", "PPER", "VVINF", "KON", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Komm, h\u00fclfreich Messer, ende die Qual mit einemmal!\u00ab", "tokens": ["Komm", ",", "h\u00fclf\u00b7reich", "Mes\u00b7ser", ",", "en\u00b7de", "die", "Qual", "mit", "ei\u00b7nem\u00b7mal", "!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "$,", "ADJD", "NN", "$,", "NN", "ART", "NN", "APPR", "ADV", "$.", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.16": {"line.1": {"text": "\u00bbleb' wohl, mein s\u00fcsses Leben! Leb' wohl, Ade! Ade!", "tokens": ["\u00bb", "leb'", "wohl", ",", "mein", "s\u00fcs\u00b7ses", "Le\u00b7ben", "!", "Leb'", "wohl", ",", "A\u00b7de", "!", "A\u00b7de", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["$(", "VVIMP", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$.", "VVIMP", "ADV", "$,", "NN", "$.", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Rasch! \u2013 Oh, da sinkt die Waffe, da lahmt die Hand! O weh!", "tokens": ["Rasch", "!", "\u2013", "Oh", ",", "da", "sinkt", "die", "Waf\u00b7fe", ",", "da", "lahmt", "die", "Hand", "!", "O", "weh", "!"], "token_info": ["word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$.", "$(", "ITJ", "$,", "ADV", "VVFIN", "ART", "NN", "$,", "ADV", "VVFIN", "ART", "NN", "$.", "NE", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ich kann nicht sterben, soll nicht! Das Werkzeug widerstrebt;", "tokens": ["Ich", "kann", "nicht", "ster\u00b7ben", ",", "soll", "nicht", "!", "Das", "Werk\u00b7zeug", "wi\u00b7der\u00b7strebt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "$,", "VMFIN", "PTKNEG", "$.", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Was h\u00f6hnt ihr mich? Ihr rieft doch: Faustus hat ausgebebt!\u00ab \u2013", "tokens": ["Was", "h\u00f6hnt", "ihr", "mich", "?", "Ihr", "rieft", "doch", ":", "Faus\u00b7tus", "hat", "aus\u00b7ge\u00b7bebt", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PRF", "$.", "PPER", "VVFIN", "ADV", "$.", "NE", "VAFIN", "VVPP", "$.", "$(", "$("], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}}}}