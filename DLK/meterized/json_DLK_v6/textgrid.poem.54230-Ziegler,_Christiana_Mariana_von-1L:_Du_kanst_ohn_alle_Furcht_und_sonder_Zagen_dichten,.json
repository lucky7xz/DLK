{"textgrid.poem.54230": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du kanst ohn alle Furcht und sonder Zagen dichten,", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du kanst ohn alle Furcht und sonder Zagen dichten,", "tokens": ["Du", "kanst", "ohn", "al\u00b7le", "Furcht", "und", "son\u00b7der", "Za\u00b7gen", "dich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PIAT", "NN", "KON", "ADJA", "NN", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie k\u00f6nt ich b\u00f6se seyn, auf das, was mich erg\u00f6tzt,", "tokens": ["Wie", "k\u00f6nt", "ich", "b\u00f6\u00b7se", "seyn", ",", "auf", "das", ",", "was", "mich", "er\u00b7g\u00f6tzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADJD", "VAINF", "$,", "APPR", "ART", "$,", "PWS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wer wird sich, werther Freund, nach Tage-Zeiten richten,", "tokens": ["Wer", "wird", "sich", ",", "wert\u00b7her", "Freund", ",", "nach", "Ta\u00b7ge\u00b7Zei\u00b7ten", "rich\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PRF", "$,", "ADJA", "NN", "$,", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dein Blat, so offt es kommt, wird immer hochgesch\u00e4tzt.", "tokens": ["Dein", "Blat", ",", "so", "offt", "es", "kommt", ",", "wird", "im\u00b7mer", "hoch\u00b7ge\u00b7sch\u00e4tzt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADV", "ADV", "PPER", "VVFIN", "$,", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Erbrech ich einen Brief von so geschickten H\u00e4nden,", "tokens": ["Er\u00b7brech", "ich", "ei\u00b7nen", "Brief", "von", "so", "ge\u00b7schick\u00b7ten", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So w\u00fcnsch ich, k\u00f6nt ich doch den Schreiber selber sehn;", "tokens": ["So", "w\u00fcnsch", "ich", ",", "k\u00f6nt", "ich", "doch", "den", "Schrei\u00b7ber", "sel\u00b7ber", "sehn", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "VMFIN", "PPER", "ADV", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Jedoch das Blat scheint sich nach meinen Wunsch zu wenden,", "tokens": ["Je\u00b7doch", "das", "Blat", "scheint", "sich", "nach", "mei\u00b7nen", "Wunsch", "zu", "wen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Weil solches, wie du schreibst, in kurtzen wird geschehn.", "tokens": ["Weil", "sol\u00b7ches", ",", "wie", "du", "schreibst", ",", "in", "kurt\u00b7zen", "wird", "ge\u00b7schehn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "$,", "PWAV", "PPER", "VVFIN", "$,", "APPR", "ADJA", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Was k\u00f6nte sch\u00f6ners seyn? ich hoff auf mein Vergn\u00fcgen,", "tokens": ["Was", "k\u00f6n\u00b7te", "sch\u00f6\u00b7ners", "seyn", "?", "ich", "hoff", "auf", "mein", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "VAINF", "$.", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die Sayten sind bereits schon zum Concert gestimmt.", "tokens": ["Die", "Say\u00b7ten", "sind", "be\u00b7reits", "schon", "zum", "Con\u00b7cert", "ge\u00b7stimmt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "O da\u00df du heute schon verm\u00f6chtest herzufliegen!", "tokens": ["O", "da\u00df", "du", "heu\u00b7te", "schon", "ver\u00b7m\u00f6ch\u00b7test", "her\u00b7zu\u00b7flie\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "PPER", "ADV", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Weil unser Musen-Chor nichts eher unternimmt.", "tokens": ["Weil", "un\u00b7ser", "Mu\u00b7sen\u00b7Chor", "nichts", "e\u00b7her", "un\u00b7ter\u00b7nimmt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PIS", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Beschleunge deinen Lauf, verk\u00fcrtze mein Verlangen,", "tokens": ["Be\u00b7schleun\u00b7ge", "dei\u00b7nen", "Lauf", ",", "ver\u00b7k\u00fcrt\u00b7ze", "mein", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Das liebe Noten-Volck vergi\u00df bey Leibe nicht.", "tokens": ["Das", "lie\u00b7be", "No\u00b7ten\u00b7Volck", "ver\u00b7gi\u00df", "bey", "Lei\u00b7be", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Kan ich mit deiner Schaar, mein Orpheus, dich empfangen,", "tokens": ["Kan", "ich", "mit", "dei\u00b7ner", "Schaar", ",", "mein", "Or\u00b7pheus", ",", "dich", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PPOSAT", "NN", "$,", "PPOSAT", "NE", "$,", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "So wei\u00df ich, da\u00df mir wohl bey solchen Gru\u00df geschicht.", "tokens": ["So", "wei\u00df", "ich", ",", "da\u00df", "mir", "wohl", "bey", "sol\u00b7chen", "Gru\u00df", "ge\u00b7schicht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADV", "APPR", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Es wird dich freylich zwar die Zunfft nicht gerne missen,", "tokens": ["Es", "wird", "dich", "frey\u00b7lich", "zwar", "die", "Zunfft", "nicht", "ger\u00b7ne", "mis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADV", "ART", "NN", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Die, wie du selber schreibst, dir stets zur Seiten ist,", "tokens": ["Die", ",", "wie", "du", "sel\u00b7ber", "schreibst", ",", "dir", "stets", "zur", "Sei\u00b7ten", "ist", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PWAV", "PPER", "ADV", "VVFIN", "$,", "PPER", "ADV", "APPRART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Denn alle Welt will dich in ihren Umgang wissen,", "tokens": ["Denn", "al\u00b7le", "Welt", "will", "dich", "in", "ih\u00b7ren", "Um\u00b7gang", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VMFIN", "PRF", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Dieweil du so beliebt und angenehme bist.", "tokens": ["Die\u00b7weil", "du", "so", "be\u00b7liebt", "und", "an\u00b7ge\u00b7neh\u00b7me", "bist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "KON", "ADJA", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Doch unser Leipzig w\u00fcnscht, wann ich die Wahrheit sage,", "tokens": ["Doch", "un\u00b7ser", "Leip\u00b7zig", "w\u00fcnscht", ",", "wann", "ich", "die", "Wahr\u00b7heit", "sa\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NE", "VVFIN", "$,", "PWAV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Sich, einmahl wiederum, Orontes, dich zu sehn.", "tokens": ["Sich", ",", "ein\u00b7mahl", "wie\u00b7de\u00b7rum", ",", "O\u00b7ron\u00b7tes", ",", "dich", "zu", "sehn", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "$,", "ADV", "ADV", "$,", "NE", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Die andern haben dich um sich fast alle Tage;", "tokens": ["Die", "an\u00b7dern", "ha\u00b7ben", "dich", "um", "sich", "fast", "al\u00b7le", "Ta\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "PPER", "APPR", "PRF", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Hier aber pfleget es gar sparsam zu geschehn.", "tokens": ["Hier", "a\u00b7ber", "pfle\u00b7get", "es", "gar", "spar\u00b7sam", "zu", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ADV", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Nicht frage, wie ich mir die Zeit vorjetzt pa\u00dfire?", "tokens": ["Nicht", "fra\u00b7ge", ",", "wie", "ich", "mir", "die", "Zeit", "vor\u00b7jetzt", "pa\u00b7\u00dfi\u00b7re", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "$,", "PWAV", "PPER", "PPER", "ART", "NN", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Music und Poesie verbleibet meine Lust;", "tokens": ["Mu\u00b7sic", "und", "Poe\u00b7sie", "ver\u00b7blei\u00b7bet", "mei\u00b7ne", "Lust", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.27": {"text": "Wann ich, so schlecht es klingt, mein Saytenspiel nur r\u00fchre,", "tokens": ["Wann", "ich", ",", "so", "schlecht", "es", "klingt", ",", "mein", "Say\u00b7ten\u00b7spiel", "nur", "r\u00fch\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "ADV", "VVFIN", "PPER", "VVFIN", "$,", "PPOSAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "So jag ich allen Schmertz und Gram aus Sinn und Brust.", "tokens": ["So", "jag", "ich", "al\u00b7len", "Schmertz", "und", "Gram", "aus", "Sinn", "und", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "KON", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Doch meyne nicht, da\u00df ich dir suche nachzuklettern,", "tokens": ["Doch", "mey\u00b7ne", "nicht", ",", "da\u00df", "ich", "dir", "su\u00b7che", "nach\u00b7zu\u00b7klet\u00b7tern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "PPER", "VVFIN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Du sitzest schon so hoch, als man kaum sehen kan.", "tokens": ["Du", "sit\u00b7zest", "schon", "so", "hoch", ",", "als", "man", "kaum", "se\u00b7hen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADJD", "$,", "KOUS", "PIS", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Ich stehe, leider! noch bey leer und kahlen Bl\u00e4ttern,", "tokens": ["Ich", "ste\u00b7he", ",", "lei\u00b7der", "!", "noch", "bey", "leer", "und", "kah\u00b7len", "Bl\u00e4t\u00b7tern", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADV", "$.", "ADV", "APPR", "ADJD", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Da deine Blumen sich bereits hervor gethan.", "tokens": ["Da", "dei\u00b7ne", "Blu\u00b7men", "sich", "be\u00b7reits", "her\u00b7vor", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PRF", "ADV", "PTKVZ", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Die fl\u00fcchtge Zeit will mir die Feder niederdr\u00fccken /", "tokens": ["Die", "fl\u00fccht\u00b7ge", "Zeit", "will", "mir", "die", "Fe\u00b7der", "nie\u00b7der\u00b7dr\u00fc\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PPER", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "In Hoffnung, da\u00df ich dich, wie du versprochen hast,", "tokens": ["In", "Hoff\u00b7nung", ",", "da\u00df", "ich", "dich", ",", "wie", "du", "ver\u00b7spro\u00b7chen", "hast", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KOUS", "PPER", "PRF", "$,", "PWAV", "PPER", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Einmahl auch wiederum pers\u00f6nlich werd erblicken,", "tokens": ["Ein\u00b7mahl", "auch", "wie\u00b7de\u00b7rum", "per\u00b7s\u00f6n\u00b7lich", "werd", "er\u00b7bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ADJD", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Befl\u00fcgle deinen Lauf, komm l\u00e4ngst gew\u00fcnschter Gast!", "tokens": ["Be\u00b7fl\u00fcg\u00b7le", "dei\u00b7nen", "Lauf", ",", "komm", "l\u00e4ngst", "ge\u00b7w\u00fcnschter", "Gast", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "VVFIN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}}}}