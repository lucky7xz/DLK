{"textgrid.poem.53579": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Saisonbeginn", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun schn\u00fcren sich die Musen in ihr Mieder.", "tokens": ["Nun", "schn\u00fc\u00b7ren", "sich", "die", "Mu\u00b7sen", "in", "ihr", "Mie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf neu geb\u00fcgelt wird der Kintop-Beau.", "tokens": ["Auf", "neu", "ge\u00b7b\u00fc\u00b7gelt", "wird", "der", "Kin\u00b7top\u00b7Beau", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "VVPP", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sogar den alten Holzbock kitzelts wieder \u2013", "tokens": ["So\u00b7gar", "den", "al\u00b7ten", "Holz\u00b7bock", "kit\u00b7zelts", "wie\u00b7der", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "NE", "ADV", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Rideau!", "tokens": ["Ri\u00b7de\u00b7au", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Rideau!", "tokens": ["Ri\u00b7de\u00b7au", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Es tauchen auf die \u00e4ltsten braven Possen.", "tokens": ["Es", "tau\u00b7chen", "auf", "die", "\u00e4lts\u00b7ten", "bra\u00b7ven", "Pos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vor jeder Bude gibts ein Mordsgeschrei.", "tokens": ["Vor", "je\u00b7der", "Bu\u00b7de", "gibts", "ein", "Mords\u00b7ge\u00b7schrei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Kritiker bei Ullstein, Scherl und Mossen", "tokens": ["Der", "Kri\u00b7ti\u00b7ker", "bei", "U\u00b7llstein", ",", "Scherl", "und", "Mos\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NE", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "spitzt Ohr und Blei.", "tokens": ["spitzt", "Ohr", "und", "Blei", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "L. Fulda und der Knabe Hasenschiller,", "tokens": ["L.", "Ful\u00b7da", "und", "der", "Kna\u00b7be", "Ha\u00b7sen\u00b7schil\u00b7ler", ","], "token_info": ["abbreviation", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "KON", "ART", "NN", "NE", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "sie schreiben monatlich ein neues St\u00fcck;", "tokens": ["sie", "schrei\u00b7ben", "mo\u00b7nat\u00b7lich", "ein", "neu\u00b7es", "St\u00fcck", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "schon sitzen beide je in einer Villa \u2013", "tokens": ["schon", "sit\u00b7zen", "bei\u00b7de", "je", "in", "ei\u00b7ner", "Vil\u00b7la", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+---", "measure": "unknown.measure.tetra"}, "line.4": {"text": "ein Gl\u00fcck! ein Gl\u00fcck!", "tokens": ["ein", "Gl\u00fcck", "!", "ein", "Gl\u00fcck", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Beim Kino rast die Hausse in den Kassen.", "tokens": ["Beim", "Ki\u00b7no", "rast", "die", "Haus\u00b7se", "in", "den", "Kas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich hoff, wenns wieder mal im Lande kracht,", "tokens": ["Ich", "hoff", ",", "wenns", "wie\u00b7der", "mal", "im", "Lan\u00b7de", "kracht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ADV", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "da\u00df die Regie bei den Verschw\u00f6rermassen", "tokens": ["da\u00df", "die", "Re\u00b7gie", "bei", "den", "Ver\u00b7schw\u00f6\u00b7rer\u00b7mas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Herr Lubitsch macht.", "tokens": ["Herr", "Lu\u00b7bitsch", "macht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Schon steht der Inspizient an den Kulissen.", "tokens": ["Schon", "steht", "der", "Ins\u00b7pi\u00b7zi\u00b7ent", "an", "den", "Ku\u00b7lis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die Orska bibbert: \u00bbGredchen lahst mich sain!\u00ab", "tokens": ["Die", "O\u00b7rska", "bib\u00b7bert", ":", "\u00bb", "Gred\u00b7chen", "lahst", "mich", "sain", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NE", "VVFIN", "$.", "$(", "NN", "VVFIN", "PPER", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Rolle nimmt sich doch als fetten Bissen", "tokens": ["Die", "Rol\u00b7le", "nimmt", "sich", "doch", "als", "fet\u00b7ten", "Bis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADV", "KOUS", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "das Pfr\u00e4ulein Pfein.", "tokens": ["das", "Pfr\u00e4u\u00b7lein", "Pfein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Thalia tingelt froh in jeder Scheune.", "tokens": ["Tha\u00b7lia", "tin\u00b7gelt", "froh", "in", "je\u00b7der", "Scheu\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADJD", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ihr lieben Leute des Theaterbaus!", "tokens": ["Ihr", "lie\u00b7ben", "Leu\u00b7te", "des", "The\u00b7a\u00b7ter\u00b7baus", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Gemach, gemach! Und denkt stets dran: Nach neune", "tokens": ["Ge\u00b7mach", ",", "ge\u00b7mach", "!", "Und", "denkt", "stets", "dran", ":", "Nach", "neu\u00b7ne"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "$,", "ADV", "$.", "KON", "VVFIN", "ADV", "PTKVZ", "$.", "APPR", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "ist alles aus . . . !", "tokens": ["ist", "al\u00b7les", "aus", ".", ".", ".", "!"], "token_info": ["word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["VAFIN", "PIS", "PTKVZ", "$.", "$.", "$.", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}