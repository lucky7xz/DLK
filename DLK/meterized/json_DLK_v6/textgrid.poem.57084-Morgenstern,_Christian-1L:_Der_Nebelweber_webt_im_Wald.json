{"textgrid.poem.57084": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Nebelweber webt im Wald", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Nebelweber webt im Wald", "tokens": ["Der", "Ne\u00b7bel\u00b7we\u00b7ber", "webt", "im", "Wald"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "ein wei\u00dfes Hemd f\u00fcr sein Gemahl.", "tokens": ["ein", "wei\u00b7\u00dfes", "Hemd", "f\u00fcr", "sein", "Ge\u00b7mahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die steht wie eine Birke schmal", "tokens": ["Die", "steht", "wie", "ei\u00b7ne", "Bir\u00b7ke", "schmal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "KOKOM", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "in einem grauen Felsenspalt.", "tokens": ["in", "ei\u00b7nem", "grau\u00b7en", "Fel\u00b7sen\u00b7spalt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Im Winde schauert leis und bebt", "tokens": ["Im", "Win\u00b7de", "schau\u00b7ert", "leis", "und", "bebt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ADJD", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "ihr d\u00e4mmergr\u00fcnes Lockenlaub.", "tokens": ["ihr", "d\u00e4m\u00b7mer\u00b7gr\u00fc\u00b7nes", "Lo\u00b7cken\u00b7laub", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie l\u00e4\u00dft ihr Zittern ihm als Raub.", "tokens": ["Sie", "l\u00e4\u00dft", "ihr", "Zit\u00b7tern", "ihm", "als", "Raub", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PPER", "KOUS", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Nebelweber webt und webt ...", "tokens": ["Der", "Ne\u00b7bel\u00b7we\u00b7ber", "webt", "und", "webt", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}