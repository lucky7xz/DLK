{"dta.poem.7001": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Vereinigung und dem Unter-  \n scheid der Seelen und des C\u00f6rpers.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Bemercken wir denn nicht ein innerlichs Erkenntni\u00df,", "tokens": ["Be\u00b7mer\u00b7cken", "wir", "denn", "nicht", "ein", "in\u00b7ner\u00b7lichs", "Er\u00b7kennt\u00b7ni\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Spricht nicht mit uns, und lehrt uns kein geheim Verst\u00e4ndni\u00df?", "tokens": ["Spricht", "nicht", "mit", "uns", ",", "und", "lehrt", "uns", "kein", "ge\u00b7heim", "Ver\u00b7st\u00e4nd\u00b7ni\u00df", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "APPR", "PPER", "$,", "KON", "VVFIN", "PPER", "PIAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nichts \u00fcberzeuglichers hat unser Geist.", "tokens": ["Nichts", "\u00fc\u00b7berz\u00b7eug\u00b7li\u00b7chers", "hat", "un\u00b7ser", "Geist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Hat es auch den geringsten Schein,", "tokens": ["Hat", "es", "auch", "den", "ge\u00b7rings\u00b7ten", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Ob k\u00f6nn\u2019 ein C\u00f6rper f\u00e4hig seyn,", "tokens": ["Ob", "k\u00f6nn'", "ein", "C\u00f6r\u00b7per", "f\u00e4\u00b7hig", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "ART", "NN", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So reger Munterkeit und solcher edlen Triebe,", "tokens": ["So", "re\u00b7ger", "Mun\u00b7ter\u00b7keit", "und", "sol\u00b7cher", "ed\u00b7len", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "KON", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wodurch die Seele sich dem Leiblichen entreisst", "tokens": ["Wo\u00b7durch", "die", "See\u00b7le", "sich", "dem", "Leib\u00b7li\u00b7chen", "en\u00b7treisst"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "PRF", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und zur Unsterblichkeit verkl\u00e4rten Sitz sich schwinget?", "tokens": ["Und", "zur", "U\u00b7nsterb\u00b7lich\u00b7keit", "ver\u00b7kl\u00e4r\u00b7ten", "Sitz", "sich", "schwin\u00b7get", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ADJA", "NN", "PRF", "VVFIN", "$."], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.9": {"text": "Die Seele, welche aus den Ketten", "tokens": ["Die", "See\u00b7le", ",", "wel\u00b7che", "aus", "den", "Ket\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Von der Materie verm\u00f6gend sich zu retten,", "tokens": ["Von", "der", "Ma\u00b7te\u00b7rie", "ver\u00b7m\u00f6\u00b7gend", "sich", "zu", "ret\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "PRF", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.11": {"text": "Und welcher alles, was die Welt", "tokens": ["Und", "wel\u00b7cher", "al\u00b7les", ",", "was", "die", "Welt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PWS", "PIS", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "F\u00fcr sch\u00f6n und herrlichs in sich h\u00e4lt,", "tokens": ["F\u00fcr", "sch\u00f6n", "und", "herr\u00b7lichs", "in", "sich", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "KON", "VVFIN", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Nur eine laue Freude bringet.", "tokens": ["Nur", "ei\u00b7ne", "lau\u00b7e", "Freu\u00b7de", "brin\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Die Seele, die bi\u00df in den Himmel dringet,", "tokens": ["Die", "See\u00b7le", ",", "die", "bi\u00df", "in", "den", "Him\u00b7mel", "drin\u00b7get", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.15": {"text": "Ein unbeschreiblich Licht, ein gr\u00fcndliches Verstehn", "tokens": ["Ein", "un\u00b7be\u00b7schreib\u00b7lich", "Licht", ",", "ein", "gr\u00fcnd\u00b7li\u00b7ches", "Ver\u00b7stehn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Sollt\u2019 in des Grabes Nacht verl\u00f6schen und vergehn?", "tokens": ["Sollt'", "in", "des", "Gra\u00b7bes", "Nacht", "ver\u00b7l\u00f6\u00b7schen", "und", "ver\u00b7gehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "NN", "NN", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}