{"textgrid.poem.35532": {"metadata": {"author": {"name": "Conradi, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Ein Ende vor dem Anfang", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ganz leise erst, noch in den zartsten F\u00e4den,", "tokens": ["Ganz", "lei\u00b7se", "erst", ",", "noch", "in", "den", "zarts\u00b7ten", "F\u00e4\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "$,", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Spann sich ein Band von dir zu mir her\u00fcber ...", "tokens": ["Spann", "sich", "ein", "Band", "von", "dir", "zu", "mir", "her\u00b7\u00fc\u00b7ber", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PRF", "ART", "NN", "APPR", "PPER", "APPR", "PPER", "ADV", "$("], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.3": {"text": "Oh! Voll war ich des k\u00f6stlichsten Erwartens,", "tokens": ["Oh", "!", "Voll", "war", "ich", "des", "k\u00f6st\u00b7lichs\u00b7ten", "Er\u00b7war\u00b7tens", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADJD", "VAFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Und s\u00fc\u00dfe Hoffnung hat mich oft berauscht.", "tokens": ["Und", "s\u00fc\u00b7\u00dfe", "Hoff\u00b7nung", "hat", "mich", "oft", "be\u00b7rauscht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ich liebte dich vielleicht noch nicht ... Und doch \u2013", "tokens": ["Ich", "lieb\u00b7te", "dich", "viel\u00b7leicht", "noch", "nicht", "...", "Und", "doch", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "PTKNEG", "$(", "KON", "ADV", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ich wu\u00dfte es: die Stunde, ja, sie k\u00e4me,", "tokens": ["Ich", "wu\u00df\u00b7te", "es", ":", "die", "Stun\u00b7de", ",", "ja", ",", "sie", "k\u00e4\u00b7me", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$.", "ART", "NN", "$,", "PTKANT", "$,", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Wo ich dich sanft in meine Arme n\u00e4hme,", "tokens": ["Wo", "ich", "dich", "sanft", "in", "mei\u00b7ne", "Ar\u00b7me", "n\u00e4h\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Dich an mich z\u00f6ge, k\u00fc\u00dfte ... und tief atmend", "tokens": ["Dich", "an", "mich", "z\u00f6\u00b7ge", ",", "k\u00fc\u00df\u00b7te", "...", "und", "tief", "at\u00b7mend"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "APPR", "PPER", "VVFIN", "$,", "VVFIN", "$(", "KON", "ADJD", "VVPP"], "meter": "-+-+-+-+++-", "measure": "unknown.measure.hexa"}, "line.9": {"text": "Du dich auch mir zu eigen geben w\u00fcrdest ...", "tokens": ["Du", "dich", "auch", "mir", "zu", "ei\u00b7gen", "ge\u00b7ben", "w\u00fcr\u00b7dest", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "ADV", "PPER", "PTKA", "ADJD", "VVINF", "VAFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich lebte dieser Stunde still entgegen", "tokens": ["Ich", "leb\u00b7te", "die\u00b7ser", "Stun\u00b7de", "still", "ent\u00b7ge\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PDAT", "NN", "ADJD", "PTKVZ"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und zehrte scheu von ihrer Freude Segen ...", "tokens": ["Und", "zehr\u00b7te", "scheu", "von", "ih\u00b7rer", "Freu\u00b7de", "Se\u00b7gen", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und nun kam's doch noch anders. Zaghaft fast", "tokens": ["Und", "nun", "kam's", "doch", "noch", "an\u00b7ders", ".", "Zag\u00b7haft", "fast"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ADV", "ADV", "ADV", "$.", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Stand auf ein m\u00fcder, milder Wind ... und langsam,", "tokens": ["Stand", "auf", "ein", "m\u00fc\u00b7der", ",", "mil\u00b7der", "Wind", "...", "und", "lang\u00b7sam", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$(", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wie spielend, wie in harmlos neck'schem Zufall,", "tokens": ["Wie", "spie\u00b7lend", ",", "wie", "in", "harm\u00b7los", "neck'\u00b7schem", "Zu\u00b7fall", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "PWAV", "APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Hat er die weichen Flocken des Gewebes,", "tokens": ["Hat", "er", "die", "wei\u00b7chen", "Flo\u00b7cken", "des", "Ge\u00b7we\u00b7bes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Das zart von dir zu mir sich angesponnen,", "tokens": ["Das", "zart", "von", "dir", "zu", "mir", "sich", "an\u00b7ge\u00b7spon\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "APPR", "PPER", "APPR", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Zert\u00e4ndelt ... Sieh, mein liebes Kind, nun flattern", "tokens": ["Zer\u00b7t\u00e4n\u00b7delt", "...", "Sieh", ",", "mein", "lie\u00b7bes", "Kind", ",", "nun", "flat\u00b7tern"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "$(", "NE", "$,", "PPOSAT", "ADJA", "NN", "$,", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Die kleinen, losen Maschen wie verwaiste,", "tokens": ["Die", "klei\u00b7nen", ",", "lo\u00b7sen", "Ma\u00b7schen", "wie", "ver\u00b7wais\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "KOKOM", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Verlorne Seelchen durch die stummen L\u00fcfte ...", "tokens": ["Ver\u00b7lor\u00b7ne", "Seel\u00b7chen", "durch", "die", "stum\u00b7men", "L\u00fcf\u00b7te", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Und dr\u00fcben nun stehst du, ich stehe h\u00fcben \u2013", "tokens": ["Und", "dr\u00fc\u00b7ben", "nun", "stehst", "du", ",", "ich", "ste\u00b7he", "h\u00fc\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "ADV", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Und traurig sehn wir unser Gl\u00fcck zerstieben ...", "tokens": ["Und", "trau\u00b7rig", "sehn", "wir", "un\u00b7ser", "Gl\u00fcck", "zer\u00b7stie\u00b7ben", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich liebte dich vielleicht noch nicht ... Vielleicht", "tokens": ["Ich", "lieb\u00b7te", "dich", "viel\u00b7leicht", "noch", "nicht", "...", "Viel\u00b7leicht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "PTKNEG", "$(", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Lieb' ich dich jetzt noch nicht ... vielleicht nicht mehr ...", "tokens": ["Lieb'", "ich", "dich", "jetzt", "noch", "nicht", "...", "viel\u00b7leicht", "nicht", "mehr", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "ADV", "PTKNEG", "$(", "ADV", "PTKNEG", "ADV", "$("], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Nein! Aus dem Wege gehen wir uns nicht.", "tokens": ["Nein", "!", "Aus", "dem", "We\u00b7ge", "ge\u00b7hen", "wir", "uns", "nicht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "APPR", "ART", "NN", "VVFIN", "PPER", "PRF", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ja! Wir begegnen uns noch ziemlich oft ...", "tokens": ["Ja", "!", "Wir", "be\u00b7geg\u00b7nen", "uns", "noch", "ziem\u00b7lich", "oft", "..."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PPER", "VVFIN", "PPER", "ADV", "ADV", "ADV", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und unsre Augen suchen sich und bleiben", "tokens": ["Und", "uns\u00b7re", "Au\u00b7gen", "su\u00b7chen", "sich", "und", "blei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PRF", "KON", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Auf einen ... Augenblick in tiefem Anschau'n,", "tokens": ["Auf", "ei\u00b7nen", "...", "Au\u00b7gen\u00b7blick", "in", "tie\u00b7fem", "An\u00b7schau'n", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$(", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Nicht scheu, nicht sch\u00fcchtern und wohl vorwurfslos ...", "tokens": ["Nicht", "scheu", ",", "nicht", "sch\u00fcch\u00b7tern", "und", "wohl", "vor\u00b7wurfs\u00b7los", "..."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "$,", "PTKNEG", "VVINF", "KON", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und nur wie Neugier, wie ganz zarte Neugier", "tokens": ["Und", "nur", "wie", "Neu\u00b7gier", ",", "wie", "ganz", "zar\u00b7te", "Neu\u00b7gier"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "KOKOM", "NN", "$,", "PWAV", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Liegt es in unserm Blick ... dann gehen wir", "tokens": ["Liegt", "es", "in", "un\u00b7serm", "Blick", "...", "dann", "ge\u00b7hen", "wir"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$(", "ADV", "VVFIN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Vor\u00fcber aneinander stumm und still ...", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "an\u00b7ein\u00b7an\u00b7der", "stumm", "und", "still", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "KON", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ich wei\u00df: wir werden uns nicht wiederfinden \u2013", "tokens": ["Ich", "wei\u00df", ":", "wir", "wer\u00b7den", "uns", "nicht", "wie\u00b7der\u00b7fin\u00b7den", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VAFIN", "PPER", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und auseinander weiter, immer weiter", "tokens": ["Und", "aus\u00b7ein\u00b7an\u00b7der", "wei\u00b7ter", ",", "im\u00b7mer", "wei\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "ADV", "$,", "ADV", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wird uns das Leben unsre Wege f\u00fchren ...", "tokens": ["Wird", "uns", "das", "Le\u00b7ben", "uns\u00b7re", "We\u00b7ge", "f\u00fch\u00b7ren", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nur manchmal zittert leis die Frage auf,", "tokens": ["Nur", "manch\u00b7mal", "zit\u00b7tert", "leis", "die", "Fra\u00b7ge", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ADJD", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Das scheue Kind verschwiegner Stunden: wenn", "tokens": ["Das", "scheu\u00b7e", "Kind", "ver\u00b7schwieg\u00b7ner", "Stun\u00b7den", ":", "wenn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$.", "KOUS"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Nun dieses Wissen dennoch tr\u00fcgrisch w\u00e4re?", "tokens": ["Nun", "die\u00b7ses", "Wis\u00b7sen", "den\u00b7noch", "tr\u00fcg\u00b7risch", "w\u00e4\u00b7re", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "NN", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "So tr\u00fcgrisch wie das erste? W\u00fcrd' ich s\u00e4umen,", "tokens": ["So", "tr\u00fcg\u00b7risch", "wie", "das", "ers\u00b7te", "?", "W\u00fcrd'", "ich", "s\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ART", "ADJA", "$.", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Da sich zum andern Mal das Gl\u00fcck mir b\u00f6te,", "tokens": ["Da", "sich", "zum", "an\u00b7dern", "Mal", "das", "Gl\u00fcck", "mir", "b\u00f6\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "APPRART", "ADJA", "NN", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Es zu ergreifen und es festzuhalten? ...", "tokens": ["Es", "zu", "er\u00b7grei\u00b7fen", "und", "es", "fest\u00b7zu\u00b7hal\u00b7ten", "?", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "KON", "PPER", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Es folgt der Nacht die junge Morgenr\u00f6te \u2013", "tokens": ["Es", "folgt", "der", "Nacht", "die", "jun\u00b7ge", "Mor\u00b7gen\u00b7r\u00f6\u00b7te", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch meinem Leben bl\u00fchet noch das Licht,", "tokens": ["Doch", "mei\u00b7nem", "Le\u00b7ben", "bl\u00fc\u00b7het", "noch", "das", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch meinem Leben bl\u00fchet noch der Tag", "tokens": ["Doch", "mei\u00b7nem", "Le\u00b7ben", "bl\u00fc\u00b7het", "noch", "der", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und seines Schaffens ungemess'ne Freude ...", "tokens": ["Und", "sei\u00b7nes", "Schaf\u00b7fens", "un\u00b7ge\u00b7mess'\u00b7ne", "Freu\u00b7de", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Noch darf ich meine Kraft im Kampf vergeuden,", "tokens": ["Noch", "darf", "ich", "mei\u00b7ne", "Kraft", "im", "Kampf", "ver\u00b7geu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPOSAT", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Noch habe ich ein Recht auf rote Wunden,", "tokens": ["Noch",