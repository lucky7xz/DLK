{"textgrid.poem.25401": {"metadata": {"author": {"name": "Stadler, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Schnellzug tastet sich", "genre": "verse", "period": "N.A.", "pub_year": 1898, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Schnellzug tastet sich", "tokens": ["Der", "Schnell\u00b7zug", "tas\u00b7tet", "sich"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "und st\u00f6\u00dft die Dunkelheit entlang.", "tokens": ["und", "st\u00f6\u00dft", "die", "Dun\u00b7kel\u00b7heit", "ent\u00b7lang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kein Stern will vor. Die ganze Welt ist nur ein enger,", "tokens": ["Kein", "Stern", "will", "vor", ".", "Die", "gan\u00b7ze", "Welt", "ist", "nur", "ein", "en\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "PTKVZ", "$.", "ART", "ADJA", "NN", "VAFIN", "ADV", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "nachtumschienter Minengang,", "tokens": ["nach\u00b7tum\u00b7schien\u00b7ter", "Mi\u00b7nen\u00b7gang", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Darein zuweilen F\u00f6rderstellen", "tokens": ["Da\u00b7rein", "zu\u00b7wei\u00b7len", "F\u00f6r\u00b7der\u00b7stel\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["PAV", "ADV", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "blauen Lichtes j\u00e4he Horizonte rei\u00dfen: Feuerkreis", "tokens": ["blau\u00b7en", "Lich\u00b7tes", "j\u00e4\u00b7he", "Ho\u00b7ri\u00b7zon\u00b7te", "rei\u00b7\u00dfen", ":", "Feu\u00b7er\u00b7kreis"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADJA", "NN", "ADJA", "NN", "VVFIN", "$.", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.7": {"text": "Von Kugellampen, D\u00e4chern, Schloten,", "tokens": ["Von", "Ku\u00b7gel\u00b7lam\u00b7pen", ",", "D\u00e4\u00b7chern", ",", "Schlo\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "dampfend, str\u00f6mend ... nur sekundenweis ...", "tokens": ["damp\u00b7fend", ",", "str\u00f6\u00b7mend", "...", "nur", "se\u00b7kun\u00b7den\u00b7weis", "..."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADJD", "$(", "ADV", "PTKVZ", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "Und wieder alles schwarz.", "tokens": ["Und", "wie\u00b7der", "al\u00b7les", "schwarz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIS", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Als f\u00fchren wir ins Eingeweid der Nacht zur Schicht.", "tokens": ["Als", "f\u00fch\u00b7ren", "wir", "ins", "Ein\u00b7ge\u00b7weid", "der", "Nacht", "zur", "Schicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Nun taumeln Lichter her ... verirrt, trostlos vereinsamt ...", "tokens": ["Nun", "tau\u00b7meln", "Lich\u00b7ter", "her", "...", "ver\u00b7irrt", ",", "trost\u00b7los", "ver\u00b7ein\u00b7samt", "..."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "PTKVZ", "$(", "VVPP", "$,", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.12": {"text": "mehr ... und sammeln sich ... und werden dicht.", "tokens": ["mehr", "...", "und", "sam\u00b7meln", "sich", "...", "und", "wer\u00b7den", "dicht", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "KON", "VVFIN", "PRF", "$(", "KON", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.13": {"text": "Gerippe grauer H\u00e4userfronten liegen blo\u00df,", "tokens": ["Ge\u00b7rip\u00b7pe", "grau\u00b7er", "H\u00e4u\u00b7ser\u00b7fron\u00b7ten", "lie\u00b7gen", "blo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "im Zwielicht bleichend, tot \u2013", "tokens": ["im", "Zwie\u00b7licht", "blei\u00b7chend", ",", "tot", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "$,", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "etwas mu\u00df kommen ... o, ich f\u00fchl es schwer", "tokens": ["et\u00b7was", "mu\u00df", "kom\u00b7men", "...", "o", ",", "ich", "f\u00fchl", "es", "schwer"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "VVINF", "$(", "FM", "$,", "PPER", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Im Hirn. Eine Beklemmung singt im Blut.", "tokens": ["Im", "Hirn", ".", "Ei\u00b7ne", "Be\u00b7klem\u00b7mung", "singt", "im", "Blut", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$.", "ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+---+-+-+", "measure": "dactylic.init"}, "line.17": {"text": "Dann dr\u00f6hnt der Boden pl\u00f6tzlich wie ein Meer:", "tokens": ["Dann", "dr\u00f6hnt", "der", "Bo\u00b7den", "pl\u00f6tz\u00b7lich", "wie", "ein", "Meer", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Wir fliegen, aufgehoben,", "tokens": ["Wir", "flie\u00b7gen", ",", "auf\u00b7ge\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "k\u00f6niglich durch nachtentrissne Luft, hoch \u00fcbern Strom.", "tokens": ["k\u00f6\u00b7nig\u00b7lich", "durch", "nach\u00b7tent\u00b7riss\u00b7ne", "Luft", ",", "hoch", "\u00fc\u00b7bern", "Strom", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "$,", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.20": {"text": "O Biegung der Millionen Lichter, stumme Wacht,", "tokens": ["O", "Bie\u00b7gung", "der", "Mil\u00b7lion\u00b7en", "Lich\u00b7ter", ",", "stum\u00b7me", "Wacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "NN", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Vor deren blitzender Parade", "tokens": ["Vor", "de\u00b7ren", "blit\u00b7zen\u00b7der", "Pa\u00b7ra\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PRELAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "schwer die Wasser abw\u00e4rts rollen.", "tokens": ["schwer", "die", "Was\u00b7ser", "ab\u00b7w\u00e4rts", "rol\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Endloses Spalier, zum Gru\u00df gestellt bei Nacht!", "tokens": ["End\u00b7lo\u00b7ses", "Spa\u00b7lier", ",", "zum", "Gru\u00df", "ge\u00b7stellt", "bei", "Nacht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "APPRART", "NN", "VVPP", "APPR", "NN", "$."], "meter": "+---+-+-+-+", "measure": "dactylic.init"}, "line.24": {"text": "Wie Fackeln st\u00fcrmend! Freudiges!", "tokens": ["Wie", "Fa\u00b7ckeln", "st\u00fcr\u00b7mend", "!", "Freu\u00b7di\u00b7ges", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "NN", "VVPP", "$.", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Salut von Schiffen \u00fcber blauer See! Bestirntes Fest!", "tokens": ["Sa\u00b7lut", "von", "Schif\u00b7fen", "\u00fc\u00b7ber", "blau\u00b7er", "See", "!", "Bes\u00b7tirn\u00b7tes", "Fest", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "APPR", "ADJA", "NN", "$.", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.26": {"text": "Wimmelnd, mit hellen Augen hingedr\u00e4ngt!", "tokens": ["Wim\u00b7melnd", ",", "mit", "hel\u00b7len", "Au\u00b7gen", "hin\u00b7ge\u00b7dr\u00e4ngt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.27": {"text": "Bis wo die Stadt", "tokens": ["Bis", "wo", "die", "Stadt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PWAV", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.28": {"text": "mit letzten H\u00e4usern ihren Gast entl\u00e4\u00dft.", "tokens": ["mit", "letz\u00b7ten", "H\u00e4u\u00b7sern", "ih\u00b7ren", "Gast", "ent\u00b7l\u00e4\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Und dann die langen Einsamkeiten. Nackte Ufer.", "tokens": ["Und", "dann", "die", "lan\u00b7gen", "Ein\u00b7sam\u00b7kei\u00b7ten", ".", "Nack\u00b7te", "U\u00b7fer", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "$.", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Stille. Nacht. Besinnung. Einkehr. Kommunion.", "tokens": ["Stil\u00b7le", ".", "Nacht", ".", "Be\u00b7sin\u00b7nung", ".", "Ein\u00b7kehr", ".", "Kom\u00b7mu\u00b7ni\u00b7on", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "NN", "$.", "NN", "$.", "NN", "$."], "meter": "+-+-+----+-+", "measure": "unknown.measure.penta"}, "line.31": {"text": "Und Glut und Drang", "tokens": ["Und", "Glut", "und", "Drang"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.32": {"text": "Zum Letzten, Segnenden. Zum Zeugungsfest.", "tokens": ["Zum", "Letz\u00b7ten", ",", "Seg\u00b7nen\u00b7den", ".", "Zum", "Zeu\u00b7gungs\u00b7fest", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "$,", "NN", "$.", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.33": {"text": "Zur Wollust. Zum Gebet. Zum Meer.", "tokens": ["Zur", "Wol\u00b7lust", ".", "Zum", "Ge\u00b7bet", ".", "Zum", "Meer", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$.", "APPRART", "NN", "$.", "APPRART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.34": {"text": "Zum Untergang.", "tokens": ["Zum", "Un\u00b7ter\u00b7gang", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}