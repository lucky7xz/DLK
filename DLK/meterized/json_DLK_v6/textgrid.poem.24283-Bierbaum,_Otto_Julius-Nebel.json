{"textgrid.poem.24283": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Nebel", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Durch dicken Nebel, der vom Berge fiel,", "tokens": ["Durch", "di\u00b7cken", "Ne\u00b7bel", ",", "der", "vom", "Ber\u00b7ge", "fiel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Dr\u00f6hnt sonderbar die Glocke des fernen Dorfs.", "tokens": ["Dr\u00f6hnt", "son\u00b7der\u00b7bar", "die", "Glo\u00b7cke", "des", "fer\u00b7nen", "Dorfs", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ruft sie um Hilfe in Erstickungsangst?", "tokens": ["Ruft", "sie", "um", "Hil\u00b7fe", "in", "Er\u00b7sti\u00b7ckungs\u00b7angst", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Steigt dort vielleicht die Sintflut \u00fcbers Dach", "tokens": ["Steigt", "dort", "viel\u00b7leicht", "die", "Sint\u00b7flut", "\u00fc\u00b7bers", "Dach"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der alten Kirche bis zum Glockenstuhl?", "tokens": ["Der", "al\u00b7ten", "Kir\u00b7che", "bis", "zum", "Glo\u00b7cken\u00b7stuhl", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Es wird mit einem Male lastend Nacht.", "tokens": ["Es", "wird", "mit", "ei\u00b7nem", "Ma\u00b7le", "las\u00b7tend", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Vor meinem Fenster die Esche, sie ist nicht mehr.", "tokens": ["Vor", "mei\u00b7nem", "Fens\u00b7ter", "die", "E\u00b7sche", ",", "sie", "ist", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "$,", "PPER", "VAFIN", "PTKNEG", "ADV", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Ihr scharf gezacktes Bl\u00e4tterfiligran", "tokens": ["Ihr", "scharf", "ge\u00b7zack\u00b7tes", "Bl\u00e4t\u00b7ter\u00b7fi\u00b7li\u00b7gran"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Zerflatterte, zerflo\u00df ins feuchte Grau,", "tokens": ["Zer\u00b7flat\u00b7ter\u00b7te", ",", "zer\u00b7flo\u00df", "ins", "feuch\u00b7te", "Grau", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Das immer dunkler, immer dichter wird:", "tokens": ["Das", "im\u00b7mer", "dunk\u00b7ler", ",", "im\u00b7mer", "dich\u00b7ter", "wird", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADJA", "$,", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Leviathans Rachen hat sie mir verschluckt.", "tokens": ["Le\u00b7vi\u00b7a\u00b7thans", "Ra\u00b7chen", "hat", "sie", "mir", "ver\u00b7schluckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VAFIN", "PPER", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.2": {"line.1": {"text": "So wird es sein, wenn einst die Stunde kommt,", "tokens": ["So", "wird", "es", "sein", ",", "wenn", "einst", "die", "Stun\u00b7de", "kommt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VAINF", "$,", "KOUS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die aus der Helle mich ins Dunkle schlingt.", "tokens": ["Die", "aus", "der", "Hel\u00b7le", "mich", "ins", "Dunk\u00b7le", "schlingt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}