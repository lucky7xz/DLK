{"textgrid.poem.64837": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Von edlem Kampfesmuth umlichtet", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von edlem Kampfesmuth umlichtet", "tokens": ["Von", "ed\u00b7lem", "Kamp\u00b7fes\u00b7muth", "um\u00b7lich\u00b7tet"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Standst du einst drohend, k\u00fchn und hehr,", "tokens": ["Standst", "du", "einst", "dro\u00b7hend", ",", "k\u00fchn", "und", "hehr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "VVPP", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Seele sch\u00f6nen Zorn gerichtet", "tokens": ["Der", "See\u00b7le", "sch\u00f6\u00b7nen", "Zorn", "ge\u00b7rich\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gen der Philister schn\u00f6des Heer.", "tokens": ["Gen", "der", "Phi\u00b7lis\u00b7ter", "schn\u00f6\u00b7des", "Heer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Auf Thorheit, Wahn und Vorurtheile,", "tokens": ["Auf", "Thor\u00b7heit", ",", "Wahn", "und", "Vor\u00b7urt\u00b7hei\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und auf der L\u00fcgen Molchgeschlecht,", "tokens": ["Und", "auf", "der", "L\u00fc\u00b7gen", "Molch\u00b7ge\u00b7schlecht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Entsandtest du die scharfen Pfeile,", "tokens": ["Ent\u00b7sand\u00b7test", "du", "die", "schar\u00b7fen", "Pfei\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als K\u00e4mpfer f\u00fcr ein h\u00f6h'res Recht.", "tokens": ["Als", "K\u00e4mp\u00b7fer", "f\u00fcr", "ein", "h\u00f6h'\u00b7res", "Recht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie viel' auch deiner Feinde waren \u2013", "tokens": ["Wie", "viel'", "auch", "dei\u00b7ner", "Fein\u00b7de", "wa\u00b7ren", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "PPOSAT", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du hast sie \u00e4ngstlich nie gez\u00e4hlt; \u2013", "tokens": ["Du", "hast", "sie", "\u00e4ngst\u00b7lich", "nie", "ge\u00b7z\u00e4hlt", ";", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "ADV", "VVPP", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es wichen zagend ihre Schaaren,", "tokens": ["Es", "wi\u00b7chen", "za\u00b7gend", "ih\u00b7re", "Schaa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Scheu vor dem Gott, der dich beseelt.", "tokens": ["Scheu", "vor", "dem", "Gott", ",", "der", "dich", "be\u00b7seelt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.4": {"line.1": {"text": "Doch, als nach wechselnden Geschicken", "tokens": ["Doch", ",", "als", "nach", "wech\u00b7seln\u00b7den", "Ge\u00b7schi\u00b7cken"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$,", "KOUS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dein gro\u00dfer, heil'ger Sieg dir nah,", "tokens": ["Dein", "gro\u00b7\u00dfer", ",", "heil'\u00b7ger", "Sieg", "dir", "nah", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,", "ADJA", "NN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da sandten sie, dich zu ber\u00fccken,", "tokens": ["Da", "sand\u00b7ten", "sie", ",", "dich", "zu", "be\u00b7r\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Sinnenfreude Dalila.", "tokens": ["Der", "Sin\u00b7nen\u00b7freu\u00b7de", "Da\u00b7li\u00b7la", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Was keines Schwertes Flammenzungen,", "tokens": ["Was", "kei\u00b7nes", "Schwer\u00b7tes", "Flam\u00b7men\u00b7zun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was keines Kummers Pfeil vermocht,", "tokens": ["Was", "kei\u00b7nes", "Kum\u00b7mers", "Pfeil", "ver\u00b7mocht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIAT", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ist ihrem falschen Ku\u00df gelungen \u2013", "tokens": ["Ist", "ih\u00b7rem", "fal\u00b7schen", "Ku\u00df", "ge\u00b7lun\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Dirne hat dich unterjocht!", "tokens": ["Die", "Dir\u00b7ne", "hat", "dich", "un\u00b7ter\u00b7jocht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Mit s\u00fc\u00dfen T\u00e4ndelworten raubte", "tokens": ["Mit", "s\u00fc\u00b7\u00dfen", "T\u00e4n\u00b7del\u00b7wor\u00b7ten", "raub\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie deiner Seele Kraft und Schwung,", "tokens": ["Sie", "dei\u00b7ner", "See\u00b7le", "Kraft", "und", "Schwung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und ri\u00df von deinem trunknen Haupte", "tokens": ["Und", "ri\u00df", "von", "dei\u00b7nem", "trunk\u00b7nen", "Haup\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Glorie der Begeisterung.", "tokens": ["Die", "Glo\u00b7rie", "der", "Be\u00b7geis\u00b7te\u00b7rung", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+---+-+", "measure": "dactylic.init"}}, "stanza.7": {"line.1": {"text": "Und als ersch\u00f6pft du eingeschlafen", "tokens": ["Und", "als", "er\u00b7sch\u00f6pft", "du", "ein\u00b7ge\u00b7schla\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In einer Nacht voll Saus und Braus,", "tokens": ["In", "ei\u00b7ner", "Nacht", "voll", "Saus", "und", "Braus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da lieferte sie dich als Sklaven", "tokens": ["Da", "lie\u00b7fer\u00b7te", "sie", "dich", "als", "Skla\u00b7ven"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "KOUS", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den harrenden Philistern aus.", "tokens": ["Den", "har\u00b7ren\u00b7den", "Phi\u00b7lis\u00b7tern", "aus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Was jetzo auch dein Herz durchw\u00fchle,", "tokens": ["Was", "jet\u00b7zo", "auch", "dein", "Herz", "durch\u00b7w\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du dienest ihnen doch als Knecht,", "tokens": ["Du", "die\u00b7nest", "ih\u00b7nen", "doch", "als", "Knecht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "KOUS", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und mahlst ihr Korn dort auf der M\u00fchle", "tokens": ["Und", "mahlst", "ihr", "Korn", "dort", "auf", "der", "M\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und treibst, was ihnen eben recht. \u2013", "tokens": ["Und", "treibst", ",", "was", "ih\u00b7nen", "e\u00b7ben", "recht", ".", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "$,", "PWS", "PPER", "ADV", "ADJD", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "O! blieb von deinem fr\u00fchern Streben", "tokens": ["O", "!", "blieb", "von", "dei\u00b7nem", "fr\u00fc\u00b7hern", "Stre\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In dir ein leiser Wiederhall:", "tokens": ["In", "dir", "ein", "lei\u00b7ser", "Wie\u00b7der\u00b7hall", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So trachte, dich noch zu erheben", "tokens": ["So", "trach\u00b7te", ",", "dich", "noch", "zu", "er\u00b7he\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "PPER", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von deinem ungeheuern Fall!", "tokens": ["Von", "dei\u00b7nem", "un\u00b7ge\u00b7heu\u00b7ern", "Fall", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Versuch es, dich zu Gott zu wenden,", "tokens": ["Ver\u00b7such", "es", ",", "dich", "zu", "Gott", "zu", "wen\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PRF", "APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gedenke an dein fr\u00fch'res Sein,", "tokens": ["Ge\u00b7den\u00b7ke", "an", "dein", "fr\u00fch'\u00b7res", "Sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und st\u00fcrze dann mit deinen H\u00e4nden", "tokens": ["Und", "st\u00fcr\u00b7ze", "dann", "mit", "dei\u00b7nen", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den schn\u00f6den G\u00f6tzentempel ein!", "tokens": ["Den", "schn\u00f6\u00b7den", "G\u00f6t\u00b7zen\u00b7tem\u00b7pel", "ein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Erfasse k\u00fchn die S\u00e4ulenhalme,", "tokens": ["Er\u00b7fas\u00b7se", "k\u00fchn", "die", "S\u00e4u\u00b7len\u00b7hal\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vom Untergange nicht erschreckt!", "tokens": ["Vom", "Un\u00b7ter\u00b7gan\u00b7ge", "nicht", "er\u00b7schreckt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ob dich und mich ihr Sturz zermalme", "tokens": ["Ob", "dich", "und", "mich", "ihr", "Sturz", "zer\u00b7mal\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "KON", "PPER", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn er nur deine Schmach bedeckt!", "tokens": ["Wenn", "er", "nur", "dei\u00b7ne", "Schmach", "be\u00b7deckt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}