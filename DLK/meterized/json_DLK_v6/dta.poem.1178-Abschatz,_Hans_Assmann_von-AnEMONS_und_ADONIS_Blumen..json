{"dta.poem.1178": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "AnEMONS  \n und  \n  ADONIS  \n Blumen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Betr\u00fcger/ die ich ehr/ ", "tokens": ["Be\u00b7tr\u00fc\u00b7ger", "/", "die", "ich", "ehr", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PRELS", "PPER", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Untreue/ die ich liebe/", "tokens": ["Un\u00b7treu\u00b7e", "/", "die", "ich", "lie\u00b7be", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PRELS", "PPER", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Was stralet ihr so sehr", "tokens": ["Was", "stra\u00b7let", "ihr", "so", "sehr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ihr schlauen Hertzens-Diebe!", "tokens": ["Ihr", "schlau\u00b7en", "Hert\u00b7zens\u00b7Die\u00b7be", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wer siehet wie ihr spielt/ und bildet ihm nicht ein/", "tokens": ["Wer", "sie\u00b7het", "wie", "ihr", "spielt", "/", "und", "bil\u00b7det", "ihm", "nicht", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "KOKOM", "PPER", "VVFIN", "$(", "KON", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ihr werdet voll Erbarmen seyn?", "tokens": ["Ihr", "wer\u00b7det", "voll", "Er\u00b7bar\u00b7men", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die falsche Freundligkeit", "tokens": ["Die", "fal\u00b7sche", "Freund\u00b7lig\u00b7keit"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und eur verliebtes Blicken/", "tokens": ["Und", "eur", "ver\u00b7lieb\u00b7tes", "Bli\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Zeigt Sonn und sch\u00f6ne Zeit/", "tokens": ["Zeigt", "Sonn", "und", "sch\u00f6\u00b7ne", "Zeit", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Pflegt Blitz und Nacht zu schicken.", "tokens": ["Pflegt", "Blitz", "und", "Nacht", "zu", "schi\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wer siehet wie ihr spielt/ und kan ihm bilden ein/", "tokens": ["Wer", "sie\u00b7het", "wie", "ihr", "spielt", "/", "und", "kan", "ihm", "bil\u00b7den", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "KOKOM", "PPER", "VVFIN", "$(", "KON", "VMFIN", "PPER", "VVFIN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df ihr so grausam sollet seyn?", "tokens": ["Da\u00df", "ihr", "so", "grau\u00b7sam", "sol\u00b7let", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Macht Augen/ da\u00df euch nicht", "tokens": ["Macht", "Au\u00b7gen", "/", "da\u00df", "euch", "nicht"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "NN", "$(", "KOUS", "PPER", "PTKNEG"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Die Welt Cometen nennet!", "tokens": ["Die", "Welt", "Co\u00b7me\u00b7ten", "nen\u00b7net", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Seyd das gepaarte Licht", "tokens": ["Seyd", "das", "ge\u00b7paar\u00b7te", "Licht"], "token_info": ["word", "word", "word", "word"], "pos": ["VAIMP", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Dem Tifis Opffer brennet/", "tokens": ["Dem", "Ti\u00b7fis", "Opf\u00b7fer", "bren\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "NN", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "F\u00fchrt uns durch euren Glantz in sichern Hafen ein:", "tokens": ["F\u00fchrt", "uns", "durch", "eu\u00b7ren", "Glantz", "in", "si\u00b7chern", "Ha\u00b7fen", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Man wird euch ewig danckbar seyn.", "tokens": ["Man", "wird", "euch", "e\u00b7wig", "dan\u00b7ck\u00b7bar", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "ADJD", "ADJD", "VAINF", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}}}}