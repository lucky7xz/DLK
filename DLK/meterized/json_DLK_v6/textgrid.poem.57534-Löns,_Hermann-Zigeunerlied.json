{"textgrid.poem.57534": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Zigeunerlied", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Lisa eine Hexe war,", "tokens": ["Die", "Li\u00b7sa", "ei\u00b7ne", "He\u00b7xe", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das wu\u00dften alle Leute,", "tokens": ["Das", "wu\u00df\u00b7ten", "al\u00b7le", "Leu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Als K\u00e4tzchen ging sie gestern um,", "tokens": ["Als", "K\u00e4tz\u00b7chen", "ging", "sie", "ge\u00b7stern", "um", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Als K\u00e4uzchen flog sie heute.", "tokens": ["Als", "K\u00e4uz\u00b7chen", "flog", "sie", "heu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Doch endlich hat man sie gefa\u00dft", "tokens": ["Doch", "end\u00b7lich", "hat", "man", "sie", "ge\u00b7fa\u00dft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PIS", "PPER", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Wald beim Wurzelsuchen", "tokens": ["Im", "Wald", "beim", "Wur\u00b7zel\u00b7su\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und schleppte sie zum Galgenberg", "tokens": ["Und", "schlepp\u00b7te", "sie", "zum", "Gal\u00b7gen\u00b7berg"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Trotz Wehgeheul und Fluchen.", "tokens": ["Trotz", "Weh\u00b7ge\u00b7heul", "und", "Flu\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Doch als sie auf dem Holzsto\u00df war,", "tokens": ["Doch", "als", "sie", "auf", "dem", "Holz\u00b7sto\u00df", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da sprach sie zu mir leise:", "tokens": ["Da", "sprach", "sie", "zu", "mir", "lei\u00b7se", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPER", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbhol' mir die alte Fiedel her,", "tokens": ["\u00bb", "hol'", "mir", "die", "al\u00b7te", "Fie\u00b7del", "her", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PPER", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu spielen letzte Weise.\u00ab", "tokens": ["Zu", "spie\u00b7len", "letz\u00b7te", "Wei\u00b7se", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PTKZU", "VVINF", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Als ich ihr dann die Geige gab,", "tokens": ["Als", "ich", "ihr", "dann", "die", "Gei\u00b7ge", "gab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Begann ein schrilles T\u00f6nen,", "tokens": ["Be\u00b7gann", "ein", "schril\u00b7les", "T\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und Kl\u00e4nge wild, gespensterhaft", "tokens": ["Und", "Kl\u00e4n\u00b7ge", "wild", ",", "ge\u00b7spens\u00b7ter\u00b7haft"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["KON", "NN", "ADJD", "$,", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Entlockte sie den Sehnen.", "tokens": ["Ent\u00b7lock\u00b7te", "sie", "den", "Seh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Da\u00df alles Volk im Kreise rings", "tokens": ["Da\u00df", "al\u00b7les", "Volk", "im", "Krei\u00b7se", "rings"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIAT", "NN", "APPRART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verfiel dem Zauberreigen,", "tokens": ["Ver\u00b7fiel", "dem", "Zau\u00b7berr\u00b7ei\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und immer toller noch begann", "tokens": ["Und", "im\u00b7mer", "tol\u00b7ler", "noch", "be\u00b7gann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Alte da zu geigen.", "tokens": ["Die", "Al\u00b7te", "da", "zu", "gei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Bis lang und kurz und jung und alt", "tokens": ["Bis", "lang", "und", "kurz", "und", "jung", "und", "alt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJD", "KON", "ADJD", "KON", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vor wildem Taumel trunken \u2013", "tokens": ["Vor", "wil\u00b7dem", "Tau\u00b7mel", "trun\u00b7ken", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da warf sie mir die Fiedel hin,", "tokens": ["Da", "warf", "sie", "mir", "die", "Fie\u00b7del", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verschwand als wie versunken.", "tokens": ["Ver\u00b7schwand", "als", "wie", "ver\u00b7sun\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KOUS", "KOKOM", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Als ich das alte Geigenholz", "tokens": ["Als", "ich", "das", "al\u00b7te", "Gei\u00b7gen\u00b7holz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nun an mich hatt' genommen,", "tokens": ["Nun", "an", "mich", "hatt'", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Hat eine wilde Wanderlust", "tokens": ["Hat", "ei\u00b7ne", "wil\u00b7de", "Wan\u00b7der\u00b7lust"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mich st\u00fcrmisch \u00fcberkommen.", "tokens": ["Mich", "st\u00fcr\u00b7misch", "\u00fc\u00b7ber\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Wohl durch das ganze Ungarland", "tokens": ["Wohl", "durch", "das", "gan\u00b7ze", "Un\u00b7gar\u00b7land"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Begann ich froh zu wandern,", "tokens": ["Be\u00b7gann", "ich", "froh", "zu", "wan\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "PTKZU", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Von Agram bis nach Debrezcin", "tokens": ["Von", "Ag\u00b7ram", "bis", "nach", "De\u00b7brez\u00b7cin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "ADV", "APPR", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von einem Nest zum andern.", "tokens": ["Von", "ei\u00b7nem", "Nest", "zum", "an\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Wo immer meine Fiedel klingt,", "tokens": ["Wo", "im\u00b7mer", "mei\u00b7ne", "Fie\u00b7del", "klingt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mu\u00df Schmerz und Trauer schwinden,", "tokens": ["Mu\u00df", "Schmerz", "und", "Trau\u00b7er", "schwin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie fliehn vor meinem Zauberspiel,", "tokens": ["Sie", "fliehn", "vor", "mei\u00b7nem", "Zau\u00b7ber\u00b7spiel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie Flugsand vor den Winden.", "tokens": ["Wie", "Flug\u00b7sand", "vor", "den", "Win\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Drei Saiten hat die Fiedel nur,", "tokens": ["Drei", "Sai\u00b7ten", "hat", "die", "Fie\u00b7del", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die halten wohl noch lange,", "tokens": ["Die", "hal\u00b7ten", "wohl", "noch", "lan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und jeden fasset wilde Lust", "tokens": ["Und", "je\u00b7den", "fas\u00b7set", "wil\u00b7de", "Lust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bei ihrem tollen Klange.", "tokens": ["Bei", "ih\u00b7rem", "tol\u00b7len", "Klan\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Doch wenn die letzte Sehne rei\u00dft,", "tokens": ["Doch", "wenn", "die", "letz\u00b7te", "Seh\u00b7ne", "rei\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mu\u00df sich mein Wandern enden,", "tokens": ["Mu\u00df", "sich", "mein", "Wan\u00b7dern", "en\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dann ruh ich unterm Rasen aus,", "tokens": ["Dann", "ruh", "ich", "un\u00b7term", "Ra\u00b7sen", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Fiedel in den H\u00e4nden.", "tokens": ["Die", "Fie\u00b7del", "in", "den", "H\u00e4n\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}