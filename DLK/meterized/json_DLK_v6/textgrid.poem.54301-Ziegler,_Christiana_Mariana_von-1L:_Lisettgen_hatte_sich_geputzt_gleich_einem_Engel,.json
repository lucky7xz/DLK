{"textgrid.poem.54301": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Lisettgen hatte sich geputzt gleich einem Engel,", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lisettgen hatte sich geputzt gleich einem Engel,", "tokens": ["Li\u00b7sett\u00b7gen", "hat\u00b7te", "sich", "ge\u00b7putzt", "gleich", "ei\u00b7nem", "En\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PRF", "VVPP", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ob gleich ihr Angesicht voll ungezehlter M\u00e4ngel.", "tokens": ["Ob", "gleich", "ihr", "An\u00b7ge\u00b7sicht", "voll", "un\u00b7ge\u00b7zehl\u00b7ter", "M\u00e4n\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sie gab sich trefflich Air, trug ihre Brust empor,", "tokens": ["Sie", "gab", "sich", "treff\u00b7lich", "Air", ",", "trug", "ih\u00b7re", "Brust", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "NN", "$,", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bi\u00df stets das M\u00e4ulgen ein und schwatzte ieden vor", "tokens": ["Bi\u00df", "stets", "das", "M\u00e4ul\u00b7gen", "ein", "und", "schwatz\u00b7te", "ie\u00b7den", "vor"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "ART", "NN", "PTKVZ", "KON", "VVFIN", "PIS", "APPR"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wie angenehm es w\u00e4r, wenn ein Paar recht Verliebten", "tokens": ["Wie", "an\u00b7ge\u00b7nehm", "es", "w\u00e4r", ",", "wenn", "ein", "Paar", "recht", "Ver\u00b7lieb\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "PPER", "VAFIN", "$,", "KOUS", "ART", "NN", "ADJD", "NN"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Sich in geheimer Lust und L\u00f6ffeln flei\u00dfig \u00fcbten.", "tokens": ["Sich", "in", "ge\u00b7hei\u00b7mer", "Lust", "und", "L\u00f6f\u00b7feln", "flei\u00b7\u00dfig", "\u00fcb\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ADJA", "NN", "KON", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Als sie bey allen nun ihr Kr\u00e4mgen angebracht,", "tokens": ["Als", "sie", "bey", "al\u00b7len", "nun", "ihr", "Kr\u00b7\u00e4m\u00b7gen", "an\u00b7ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PIS", "ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-++-+-+", "measure": "unknown.measure.septa"}, "line.8": {"text": "So war doch keiner nicht, der h\u00e4tte nachgedacht.", "tokens": ["So", "war", "doch", "kei\u00b7ner", "nicht", ",", "der", "h\u00e4t\u00b7te", "nach\u00b7ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PIS", "PTKNEG", "$,", "PRELS", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Was ihre Meynung hie\u00df; der Streich war mi\u00dfgelungen,", "tokens": ["Was", "ih\u00b7re", "Mey\u00b7nung", "hie\u00df", ";", "der", "Streich", "war", "mi\u00df\u00b7ge\u00b7lun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVFIN", "$.", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Weil sie nicht aus den Thon verliebter Thoren sungen.", "tokens": ["Weil", "sie", "nicht", "aus", "den", "Thon", "ver\u00b7lieb\u00b7ter", "Tho\u00b7ren", "sun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "APPR", "ART", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Das arme Way\u00dfgen stand voll Schmertz und K\u00fcmmern\u00fc\u00df,", "tokens": ["Das", "ar\u00b7me", "Way\u00df\u00b7gen", "stand", "voll", "Schmertz", "und", "K\u00fcm\u00b7mer\u00b7n\u00fc\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Weil sich kein einiger in Sprenckel fangen lie\u00df:", "tokens": ["Weil", "sich", "kein", "ei\u00b7ni\u00b7ger", "in", "Spren\u00b7ckel", "fan\u00b7gen", "lie\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PIAT", "PIAT", "APPR", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Sie sang in ihren Sinn nur eitel Klage-Lieder,", "tokens": ["Sie", "sang", "in", "ih\u00b7ren", "Sinn", "nur", "ei\u00b7tel", "Kla\u00b7ge\u00b7Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "ADV", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und gienge h\u00f6chst best\u00fcrtzt das Zimmer auf und nieder", "tokens": ["Und", "gien\u00b7ge", "h\u00f6chst", "be\u00b7st\u00fcrtzt", "das", "Zim\u00b7mer", "auf", "und", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "VVFIN", "ART", "NN", "PTKVZ", "KON", "PTKVZ"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Man sah mehr als zu wohl, da\u00df sie die Liebe sch--", "tokens": ["Man", "sah", "mehr", "als", "zu", "wohl", ",", "da\u00df", "sie", "die", "Lie\u00b7be", "sch"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PIS", "KOKOM", "APPR", "ADV", "$,", "KOUS", "PPER", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Denn niemand heilt ihr Hertz und brachte Pflaster vor.", "tokens": ["Denn", "nie\u00b7mand", "heilt", "ihr", "Hertz", "und", "brach\u00b7te", "Pflas\u00b7ter", "vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPOSAT", "NN", "KON", "VVFIN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Zu Lindrung ihren Schmertz und K\u00fchlung solcher Flammen,", "tokens": ["Zu", "Lind\u00b7rung", "ih\u00b7ren", "Schmertz", "und", "K\u00fch\u00b7lung", "sol\u00b7cher", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "KON", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Fieng sie das Manns-Volck an, aus Rache, zu verdammen.", "tokens": ["Fi\u00b7eng", "sie", "das", "Manns\u00b7Volck", "an", ",", "aus", "Ra\u00b7che", ",", "zu", "ver\u00b7dam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,", "APPR", "NN", "$,", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.19": {"text": "Der Eyfer jagt ihr gleich hierauf den Vorsatz ein,", "tokens": ["Der", "Ey\u00b7fer", "jagt", "ihr", "gleich", "hier\u00b7auf", "den", "Vor\u00b7satz", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "PAV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Sie wolt ins erste Stift, wo nichts als Nonnen seyn.", "tokens": ["Sie", "wolt", "ins", "ers\u00b7te", "Stift", ",", "wo", "nichts", "als", "Non\u00b7nen", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPRART", "ADJA", "NN", "$,", "PWAV", "PIS", "KOKOM", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Durch dieses meynte sie der M\u00e4nner Hertz zu beugen;", "tokens": ["Durch", "die\u00b7ses", "meyn\u00b7te", "sie", "der", "M\u00e4n\u00b7ner", "Hertz", "zu", "beu\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "VVFIN", "PPER", "ART", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Allein man sahe sie, wie vor, noch alle schweigen.", "tokens": ["Al\u00b7lein", "man", "sa\u00b7he", "sie", ",", "wie", "vor", ",", "noch", "al\u00b7le", "schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "PPER", "$,", "PWAV", "PTKVZ", "$,", "ADV", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Es wolte keiner dran, so starck die Zunfft auch hie\u00df,", "tokens": ["Es", "wol\u00b7te", "kei\u00b7ner", "dran", ",", "so", "starck", "die", "Zunfft", "auch", "hie\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIS", "PAV", "$,", "ADV", "ADJD", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Der, wie die Flieg am Hartz, die Freyheit kleben lie\u00df.", "tokens": ["Der", ",", "wie", "die", "Flieg", "am", "Hartz", ",", "die", "Frey\u00b7heit", "kle\u00b7ben", "lie\u00df", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PWAV", "ART", "NN", "APPRART", "NN", "$,", "ART", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Drum gab sie gute Nacht, und eilte nach der Kammer,", "tokens": ["Drum", "gab", "sie", "gu\u00b7te", "Nacht", ",", "und", "eil\u00b7te", "nach", "der", "Kam\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADJA", "NN", "$,", "KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Die offters Zeuge heist von ihren Liebes-Jammer,", "tokens": ["Die", "off\u00b7ters", "Zeu\u00b7ge", "heist", "von", "ih\u00b7ren", "Lie\u00b7bes\u00b7Jam\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Darinne schl\u00e4fft sie noch in ihrer Bangigkeit,", "tokens": ["Da\u00b7rin\u00b7ne", "schl\u00e4fft", "sie", "noch", "in", "ih\u00b7rer", "Ban\u00b7gig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Will iemand Tr\u00f6ster seyn, so ist es hohe Zeit,", "tokens": ["Will", "ie\u00b7mand", "Tr\u00f6s\u00b7ter", "seyn", ",", "so", "ist", "es", "ho\u00b7he", "Zeit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "NN", "VAINF", "$,", "ADV", "VAFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Die Thr\u00e4nen rollen ihr auf den verwelckten Wangen,", "tokens": ["Die", "Thr\u00e4\u00b7nen", "rol\u00b7len", "ihr", "auf", "den", "ver\u00b7welck\u00b7ten", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Sie wei\u00df bey dieser Noth nichts weiter anzufangen,", "tokens": ["Sie", "wei\u00df", "bey", "die\u00b7ser", "Noth", "nichts", "wei\u00b7ter", "an\u00b7zu\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PDAT", "NN", "PIS", "ADV", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Sie denckt auf ihren Todt, auf Morden, Stahl und Gifft,", "tokens": ["Sie", "denckt", "auf", "ih\u00b7ren", "Todt", ",", "auf", "Mor\u00b7den", ",", "Stahl", "und", "Gifft", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,", "APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Weil sie kein g\u00fcltig Loo\u00df in Amors Gl\u00fccks-Topff trifft.", "tokens": ["Weil", "sie", "kein", "g\u00fcl\u00b7tig", "Loo\u00df", "in", "A\u00b7mors", "Gl\u00fccks\u00b7Topff", "trifft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "ADJD", "NN", "APPR", "NE", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Was wird man, thut sie di\u00df, wohl auf ihr Grabmahl \u00e4tzen?", "tokens": ["Was", "wird", "man", ",", "thut", "sie", "di\u00df", ",", "wohl", "auf", "ihr", "Grab\u00b7mahl", "\u00e4t\u00b7zen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PIS", "$,", "VVFIN", "PPER", "PDS", "$,", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Mir ist, als s\u00e4h ich schon den Momus dieses setzen:", "tokens": ["Mir", "ist", ",", "als", "s\u00e4h", "ich", "schon", "den", "Mo\u00b7mus", "die\u00b7ses", "set\u00b7zen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "ADJD", "PPER", "ADV", "ART", "NN", "PDAT", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Bewundert nicht, was dort Lucretia verricht,", "tokens": ["Be\u00b7wun\u00b7dert", "nicht", ",", "was", "dort", "Lu\u00b7cre\u00b7tia", "ver\u00b7richt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,", "PRELS", "ADV", "NE", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.36": {"text": "Denn die geno\u00df noch was, Lisettgen aber nicht.", "tokens": ["Denn", "die", "ge\u00b7no\u00df", "noch", "was", ",", "Li\u00b7sett\u00b7gen", "a\u00b7ber", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "VVFIN", "ADV", "PIS", "$,", "NE", "ADV", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}