{"textgrid.poem.39339": {"metadata": {"author": {"name": "Lavater, Johann Kaspar", "birth": "N.A.", "death": "N.A."}, "title": "1L: O edles Menschenherz! Auch \u00fcberworfen", "genre": "verse", "period": "N.A.", "pub_year": 1770, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O edles Menschenherz! Auch \u00fcberworfen", "tokens": ["O", "ed\u00b7les", "Men\u00b7schen\u00b7herz", "!", "Auch", "\u00fc\u00b7ber\u00b7wor\u00b7fen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NE", "ADJA", "NN", "$.", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von Sturm und Wogen, schmachtend, seufzend auch,", "tokens": ["Von", "Sturm", "und", "Wo\u00b7gen", ",", "schmach\u00b7tend", ",", "seuf\u00b7zend", "auch", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "ADJD", "$,", "VVPP", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wenn schwarze Wuth zehnfacher List und Bosheit", "tokens": ["Wenn", "schwar\u00b7ze", "Wuth", "zehn\u00b7fa\u00b7cher", "List", "und", "Bos\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "NN", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dich zehnfach \u00e4ngstigt, Scham Dir anhaucht,", "tokens": ["Dich", "zehn\u00b7fach", "\u00e4ngs\u00b7tigt", ",", "Scham", "Dir", "an\u00b7haucht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "$,", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Da\u00df Br\u00fcder so entweyh'n der Menschheit Namen,", "tokens": ["Da\u00df", "Br\u00fc\u00b7der", "so", "ent\u00b7wey\u00b7h'n", "der", "Menschheit", "Na\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Des Menschenherzens Ehre so verl\u00e4stern,", "tokens": ["Des", "Men\u00b7schen\u00b7her\u00b7zens", "Eh\u00b7re", "so", "ver\u00b7l\u00e4s\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Im hei\u00dfesten Moment der edeln Wehmuth", "tokens": ["Im", "hei\u00b7\u00dfes\u00b7ten", "Mo\u00b7ment", "der", "e\u00b7deln", "Weh\u00b7muth"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Verl\u00e4\u00dft Dich nicht der Weisheit Heldenkraft,", "tokens": ["Ver\u00b7l\u00e4\u00dft", "Dich", "nicht", "der", "Weis\u00b7heit", "Hel\u00b7den\u00b7kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Zu s\u00f6ndern scharf die Bosheit von dem B\u00f6sen,", "tokens": ["Zu", "s\u00f6n\u00b7dern", "scharf", "die", "Bos\u00b7heit", "von", "dem", "B\u00f6\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Nicht ihm, der Bosheit nur in ihm zu fluchen!", "tokens": ["Nicht", "ihm", ",", "der", "Bos\u00b7heit", "nur", "in", "ihm", "zu", "flu\u00b7chen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPER", "$,", "ART", "NN", "ADV", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Du hassest nur, was Licht und Liebe hasset!", "tokens": ["Du", "has\u00b7sest", "nur", ",", "was", "Licht", "und", "Lie\u00b7be", "has\u00b7set", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$,", "PWS", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Du segnest den, de\u00df bitterscharfen Schiefsinn", "tokens": ["Du", "seg\u00b7nest", "den", ",", "de\u00df", "bit\u00b7ter\u00b7schar\u00b7fen", "Schief\u00b7sinn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du z\u00fcrnend, wehmuthvoll und bang verdammest", "tokens": ["Du", "z\u00fcr\u00b7nend", ",", "weh\u00b7muth\u00b7voll", "und", "bang", "ver\u00b7dam\u00b7mest"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVPP", "$,", "ADJD", "KON", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und in den Schlund des bodenlosen Abgrunds", "tokens": ["Und", "in", "den", "Schlund", "des", "bo\u00b7den\u00b7lo\u00b7sen", "Ab\u00b7grunds"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Verw\u00fcnschest, welchem er, dem Hauch der Pest gleich,", "tokens": ["Ver\u00b7w\u00fcn\u00b7schest", ",", "wel\u00b7chem", "er", ",", "dem", "Hauch", "der", "Pest", "gleich", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PRELS", "PPER", "$,", "ART", "NN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die rechts und links erw\u00fcrgt und schlachtet, aufstieg!", "tokens": ["Die", "rechts", "und", "links", "er\u00b7w\u00fcrgt", "und", "schlach\u00b7tet", ",", "auf\u00b7stieg", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADV", "KON", "ADV", "VVPP", "KON", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.3": {"line.1": {"text": "O edles Herz! Du z\u00fcrnst, wie Engel z\u00fcrnen!", "tokens": ["O", "ed\u00b7les", "Herz", "!", "Du", "z\u00fcrnst", ",", "wie", "En\u00b7gel", "z\u00fcr\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$.", "PPER", "VVFIN", "$,", "PWAV", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "O edles Herz, dein Zorn ist sch\u00f6ner noch,", "tokens": ["O", "ed\u00b7les", "Herz", ",", "dein", "Zorn", "ist", "sch\u00f6\u00b7ner", "noch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "PPOSAT", "NN", "VAFIN", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Noch lieblicher als Sanftmuth, Huld und Gnade!", "tokens": ["Noch", "lieb\u00b7li\u00b7cher", "als", "Sanft\u00b7muth", ",", "Huld", "und", "Gna\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So z\u00fcrnte der mit M\u00e4nnerzorn und Liebe", "tokens": ["So", "z\u00fcrn\u00b7te", "der", "mit", "M\u00e4n\u00b7ner\u00b7zorn", "und", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Des Vaters, dessen Brust voll Kinderhuld", "tokens": ["Des", "Va\u00b7ters", ",", "des\u00b7sen", "Brust", "voll", "Kin\u00b7der\u00b7huld"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELAT", "NN", "ADJD", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und milden Segens war f\u00fcr holde Kinder!", "tokens": ["Und", "mil\u00b7den", "Se\u00b7gens", "war", "f\u00fcr", "hol\u00b7de", "Kin\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VAFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "O edles Herz! Du z\u00fcrnest, blutig weynend,", "tokens": ["O", "ed\u00b7les", "Herz", "!", "Du", "z\u00fcr\u00b7nest", ",", "blu\u00b7tig", "wey\u00b7nend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$.", "PPER", "VVFIN", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn Du nicht retten kannst und nach der Rettung", "tokens": ["Wenn", "Du", "nicht", "ret\u00b7ten", "kannst", "und", "nach", "der", "Ret\u00b7tung"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "VVINF", "VMFIN", "KON", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Des B\u00f6sewichts, wie nach dem Morgenroth", "tokens": ["Des", "B\u00f6\u00b7se\u00b7wichts", ",", "wie", "nach", "dem", "Mor\u00b7gen\u00b7roth"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PWAV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der W\u00e4chter, lechzest, lechzest, wie nach Einem,", "tokens": ["Der", "W\u00e4ch\u00b7ter", ",", "lech\u00b7zest", ",", "lech\u00b7zest", ",", "wie", "nach", "Ei\u00b7nem", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$,", "VVFIN", "$,", "VVFIN", "$,", "PWAV", "APPR", "PIS", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Nur Einem Tropfen K\u00fchlung jener lechzte,", "tokens": ["Nur", "Ei\u00b7nem", "Trop\u00b7fen", "K\u00fch\u00b7lung", "je\u00b7ner", "lechz\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "PDAT", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der den verdr\u00e4ngten Brosamd\u00fcrftigen,", "tokens": ["Der", "den", "ver\u00b7dr\u00e4ng\u00b7ten", "Bro\u00b7sam\u00b7d\u00fcrf\u00b7ti\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Beleckten von der Hunde Mitleid, fern sah", "tokens": ["Be\u00b7leck\u00b7ten", "von", "der", "Hun\u00b7de", "Mit\u00b7leid", ",", "fern", "sah"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$,", "ADJD", "VVFIN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.8": {"text": "Im Schose Abrahams, der Schmerzenstage,", "tokens": ["Im", "Scho\u00b7se", "Ab\u00b7ra\u00b7hams", ",", "der", "Schmer\u00b7zens\u00b7ta\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "NE", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Der Jammern\u00e4chte frey, umstralt mit Lichte!", "tokens": ["Der", "Jam\u00b7mer\u00b7n\u00e4ch\u00b7te", "frey", ",", "um\u00b7stralt", "mit", "Lich\u00b7te", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}