{"dta.poem.719": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "V.  Vber de\u00df Herrn Leiche.  \n  Bidermanni. Eheu! flebile funus.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Ach weh! was seh' ich hier? ein au\u00df gestr\u00e4ckte leichen. ", "tokens": ["Ach", "weh", "!", "was", "seh'", "ich", "hier", "?", "ein", "au\u00df", "ge\u00b7str\u00e4ck\u00b7te", "lei\u00b7chen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "$.", "PWS", "VVFIN", "PPER", "ADV", "$.", "ART", "APPR", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "An der man von fu\u00df auff nichts vnzerschlagen findt:", "tokens": ["An", "der", "man", "von", "fu\u00df", "auff", "nichts", "vn\u00b7zer\u00b7schla\u00b7gen", "findt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIS", "APPR", "PTKVZ", "APPR", "PIS", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Seit au\u00df der das ", "tokens": ["Die", "Seit", "au\u00df", "der", "das"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "ART"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Die wangen so von schmertz in todes angft erbleichen. ", "tokens": ["Die", "wan\u00b7gen", "so", "von", "schmertz", "in", "to\u00b7des", "angft", "er\u00b7blei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "ADJD", "APPR", "NN", "VVPP", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wer hat dich so verletzt? Wer hat mit Geissel streichen", "tokens": ["Wer", "hat", "dich", "so", "ver\u00b7letzt", "?", "Wer", "hat", "mit", "Geis\u00b7sel", "strei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "VVPP", "$.", "PWS", "VAFIN", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gew\u00fcttet auff di\u00df fleisch? welch grimmes Tygerkind", "tokens": ["Ge\u00b7w\u00fct\u00b7tet", "auff", "di\u00df", "fleisch", "?", "welch", "grim\u00b7mes", "Ty\u00b7ger\u00b7kind"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVPP", "APPR", "PDS", "ADJD", "$.", "PWAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Hat hand hier angelegt/ als diese glieder find ", "tokens": ["Hat", "hand", "hier", "an\u00b7ge\u00b7legt", "/", "als", "die\u00b7se", "glie\u00b7der", "find"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "NN", "ADV", "VVPP", "$(", "KOUS", "PDAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mit N\u00e4geln gantz durchbort? ", "tokens": ["Mit", "N\u00e4\u00b7geln", "gantz", "durch\u00b7bort", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der deine zarte ", "tokens": ["Der", "dei\u00b7ne", "zar\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["ART", "PPOSAT", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Wer hat mein Br\u00e4utigamb mit Galle dich getr\u00e4nckt?", "tokens": ["Wer", "hat", "mein", "Br\u00e4u\u00b7ti\u00b7gamb", "mit", "Gal\u00b7le", "dich", "ge\u00b7tr\u00e4nckt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPOSAT", "NN", "APPR", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ach! di\u00df hat deine Lieb vnd meine schuld ver\u00fcbet.", "tokens": ["Ach", "!", "di\u00df", "hat", "dei\u00b7ne", "Lieb", "vnd", "mei\u00b7ne", "schuld", "ver\u00b7\u00fc\u00b7bet", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PDS", "VAFIN", "PPOSAT", "NN", "KON", "PPOSAT", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wen diese liebe nicht zu wiederliebe zwingt?", "tokens": ["Wen", "die\u00b7se", "lie\u00b7be", "nicht", "zu", "wie\u00b7der\u00b7lie\u00b7be", "zwingt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDS", "VVFIN", "PTKNEG", "PTKZU", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wem dieses Jammerbild nicht ", "tokens": ["Wem", "die\u00b7ses", "Jam\u00b7mer\u00b7bild", "nicht"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "PDAT", "NN", "PTKNEG"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "dringt?", "tokens": ["dringt", "?"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Verdint das er empfind was f\u00fcr vnd f\u00fcr betr\u00fcbet.", "tokens": ["Ver\u00b7dint", "das", "er", "emp\u00b7find", "was", "f\u00fcr", "vnd", "f\u00fcr", "be\u00b7tr\u00fc\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "PPER", "VVFIN", "PIS", "APPR", "KON", "APPR", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}