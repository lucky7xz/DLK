{"textgrid.poem.63174": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mensch, es str\u00f6men die Jahrtausende", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mensch, es str\u00f6men die Jahrtausende", "tokens": ["Mensch", ",", "es", "str\u00f6\u00b7men", "die", "Jahr\u00b7tau\u00b7sen\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PPER", "VVFIN", "ART", "NN"], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.2": {"text": "In dein offnes Herz. Der sausende", "tokens": ["In", "dein", "off\u00b7nes", "Herz", ".", "Der", "sau\u00b7sen\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$.", "ART", "ADJA"], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Fl\u00fcgelschlag der Zeit best\u00fcrme dich!", "tokens": ["Fl\u00fc\u00b7gel\u00b7schlag", "der", "Zeit", "be\u00b7st\u00fcr\u00b7me", "dich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Halte fest der Promethiden Feuer,", "tokens": ["Hal\u00b7te", "fest", "der", "Pro\u00b7me\u00b7thi\u00b7den", "Feu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Und in ihrem heiligen Glanz erneuer", "tokens": ["Und", "in", "ih\u00b7rem", "hei\u00b7li\u00b7gen", "Glanz", "er\u00b7neu\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN", "ADJA"], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Zart zu Faltern das Gew\u00fcrme sich.", "tokens": ["Zart", "zu", "Fal\u00b7tern", "das", "Ge\u00b7w\u00fcr\u00b7me", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "ART", "NN", "PRF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Gingest du nicht deinen Gott verkaufen", "tokens": ["Gin\u00b7gest", "du", "nicht", "dei\u00b7nen", "Gott", "ver\u00b7kau\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Unter L\u00e4cheln, Liebeln, Huren, Saufen?", "tokens": ["Un\u00b7ter", "L\u00e4\u00b7cheln", ",", "Lie\u00b7beln", ",", "Hu\u00b7ren", ",", "Sau\u00b7fen", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "$,", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "War mit Gold gef\u00fcllt nicht Raum und Zeit?", "tokens": ["War", "mit", "Gold", "ge\u00b7f\u00fcllt", "nicht", "Raum", "und", "Zeit", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "VVPP", "PTKNEG", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Lern an reiner Quelle wieder trinken,", "tokens": ["Lern", "an", "rei\u00b7ner", "Quel\u00b7le", "wie\u00b7der", "trin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Lerne wieder liebend niedersinken", "tokens": ["Ler\u00b7ne", "wie\u00b7der", "lie\u00b7bend", "nie\u00b7der\u00b7sin\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADJD", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "In die Kniee vor der Ewigkeit.", "tokens": ["In", "die", "Kni\u00b7ee", "vor", "der", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Aus den Kratern schweben die D\u00e4monen,", "tokens": ["Aus", "den", "Kra\u00b7tern", "schwe\u00b7ben", "die", "D\u00e4\u00b7mo\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Welche bei den schwarzen Engeln wohnen,", "tokens": ["Wel\u00b7che", "bei", "den", "schwar\u00b7zen", "En\u00b7geln", "woh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "APPR", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und es steigt die s\u00fcd- und nordsche Flut,", "tokens": ["Und", "es", "steigt", "die", "s\u00fcd", "und", "nord\u00b7sche", "Flut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "TRUNC", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Schwing die Fackel deiner reinen Seele.", "tokens": ["Schwing", "die", "Fa\u00b7ckel", "dei\u00b7ner", "rei\u00b7nen", "See\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Horch: schon zwitschert wieder Philomele,", "tokens": ["Horch", ":", "schon", "zwit\u00b7schert", "wie\u00b7der", "Phi\u00b7lo\u00b7me\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADV", "CARD", "ADV", "NE", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Und es schwirrt der Zukunft Adlerbrut.", "tokens": ["Und", "es", "schwirrt", "der", "Zu\u00b7kunft", "Ad\u00b7ler\u00b7brut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Sollen Irre durch die Gassen taumeln?", "tokens": ["Sol\u00b7len", "Ir\u00b7re", "durch", "die", "Gas\u00b7sen", "tau\u00b7meln", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Sollen Schwangere am Galgen baumeln?", "tokens": ["Sol\u00b7len", "Schwan\u00b7ge\u00b7re", "am", "Gal\u00b7gen", "bau\u00b7meln", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Freiheit, welche mordet, ist nur Wahn.", "tokens": ["Frei\u00b7heit", ",", "wel\u00b7che", "mor\u00b7det", ",", "ist", "nur", "Wahn", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "VVFIN", "$,", "VAFIN", "ADV", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Stosst hinab in tiefste H\u00f6llentiefen,", "tokens": ["Stosst", "hin\u00b7ab", "in", "tiefs\u00b7te", "H\u00f6l\u00b7len\u00b7tie\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Wo noch immer nicht sie endlich schliefen,", "tokens": ["Wo", "noch", "im\u00b7mer", "nicht", "sie", "end\u00b7lich", "schlie\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "PTKNEG", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Nero, Robespierre und Dschingiskhan.", "tokens": ["Ne\u00b7ro", ",", "Ro\u00b7be\u00b7spier\u00b7re", "und", "Dschin\u00b7gis\u00b7khan", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "KON", "NE", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Die ihr lebend starbet in den Gr\u00fcften", "tokens": ["Die", "ihr", "le\u00b7bend", "star\u00b7bet", "in", "den", "Gr\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADJD", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Unsrer St\u00e4dte: schwingt euch mit den L\u00fcften", "tokens": ["Uns\u00b7rer", "St\u00e4d\u00b7te", ":", "schwingt", "euch", "mit", "den", "L\u00fcf\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Eines neuen Fr\u00fchlings in die Welt.", "tokens": ["Ei\u00b7nes", "neu\u00b7en", "Fr\u00fch\u00b7lings", "in", "die", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Liebe will sich liebend euch ergeben,", "tokens": ["Lie\u00b7be", "will", "sich", "lie\u00b7bend", "euch", "er\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PRF", "ADJD", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Lachend werdet ihr das Leben leben,", "tokens": ["La\u00b7chend", "wer\u00b7det", "ihr", "das", "Le\u00b7ben", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Wenn der morsche Tempel f\u00e4llt!", "tokens": ["Wenn", "der", "mor\u00b7sche", "Tem\u00b7pel", "f\u00e4llt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}