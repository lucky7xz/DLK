{"textgrid.poem.54659": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "73. Uber die unterirdischen Gr\u00fcffte der heiligen M\u00e4rterer zu Rom", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Gr\u00fcffte die du schawst/ hat dise Schar besessen/", "tokens": ["Die", "Gr\u00fcff\u00b7te", "die", "du", "schaw\u00b7st", "/", "hat", "di\u00b7se", "Schar", "be\u00b7ses\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "PPER", "VVFIN", "$(", "VAFIN", "PDAT", "NN", "VVPP", "$("], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Die lebend Tod der Welt und ihrer Lust vergessen/", "tokens": ["Die", "le\u00b7bend", "Tod", "der", "Welt", "und", "ih\u00b7rer", "Lust", "ver\u00b7ges\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "ART", "NN", "KON", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und L\u00e4nder doch bekehrt/ und H\u00f6ll und Feind erschreckt/", "tokens": ["Und", "L\u00e4n\u00b7der", "doch", "be\u00b7kehrt", "/", "und", "H\u00f6ll", "und", "Feind", "er\u00b7schreckt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "VVPP", "$(", "KON", "NN", "KON", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und Wunderwerck erweist und Todten aufferweckt.", "tokens": ["Und", "Wun\u00b7der\u00b7werck", "er\u00b7weist", "und", "Tod\u00b7ten", "auf\u00b7fer\u00b7weckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVPP", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der heilig-hohe Glantz dem sie hir wollen dinen;", "tokens": ["Der", "hei\u00b7lig\u00b7ho\u00b7he", "Glantz", "dem", "sie", "hir", "wol\u00b7len", "di\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "PPER", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Hat disen Ort bestrahlt und ihr Gem\u00fctt beschinen/", "tokens": ["Hat", "di\u00b7sen", "Ort", "be\u00b7strahlt", "und", "ihr", "Ge\u00b7m\u00fctt", "be\u00b7schi\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "NN", "VVPP", "KON", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Itzt jauchtzen sie bey Gott/ wir leben in der Welt/", "tokens": ["Itzt", "jaucht\u00b7zen", "sie", "bey", "Gott", "/", "wir", "le\u00b7ben", "in", "der", "Welt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "$(", "PPER", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wir/ dehnen ihre Wei\u00df' und Wohnung nicht gef\u00e4lt.", "tokens": ["Wir", "/", "deh\u00b7nen", "ih\u00b7re", "Wei\u00df'", "und", "Woh\u00b7nung", "nicht", "ge\u00b7f\u00e4lt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "PRELS", "PPOSAT", "NN", "KON", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Drumb mu\u00df bey unserm Tag ihr sch\u00f6nes Licht erbleichen.", "tokens": ["Drumb", "mu\u00df", "bey", "un\u00b7serm", "Tag", "ihr", "sch\u00f6\u00b7nes", "Licht", "er\u00b7blei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "APPR", "PPOSAT", "NN", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wir gehn mit Menschen umb: Drumb h\u00f6rt uns keine Leichen.", "tokens": ["Wir", "gehn", "mit", "Men\u00b7schen", "umb", ":", "Drumb", "h\u00f6rt", "uns", "kei\u00b7ne", "Lei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "PTKVZ", "$.", "PAV", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Sie wohnten/ schaw/ wie tieff! Doch stig ihr Geist hinauff.", "tokens": ["Sie", "wohn\u00b7ten", "/", "schaw", "/", "wie", "tieff", "!", "Doch", "stig", "ihr", "Geist", "hin\u00b7auff", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "VVFIN", "$(", "PWAV", "ADJD", "$.", "KON", "ADJD", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wir bawen hoch! Ach! Ach! Wohin sinckt unser Lauff.", "tokens": ["Wir", "ba\u00b7wen", "hoch", "!", "Ach", "!", "Ach", "!", "Wo\u00b7hin", "sinckt", "un\u00b7ser", "Lauff", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$.", "ITJ", "$.", "ITJ", "$.", "PWAV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}