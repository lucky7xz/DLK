{"textgrid.poem.26642": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1. Von der Akademie", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein gro\u00dfes Fest! \u2013 La\u00dft, Freunde, la\u00dft erschallen! \u2013", "tokens": ["Ein", "gro\u00b7\u00dfes", "Fest", "!", "\u2013", "La\u00dft", ",", "Freun\u00b7de", ",", "la\u00dft", "er\u00b7schal\u00b7len", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "$(", "VVIMP", "$,", "NN", "$,", "VVIMP", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein sch\u00f6nes Fest weckt uns zu edler Lust!", "tokens": ["Ein", "sch\u00f6\u00b7nes", "Fest", "weckt", "uns", "zu", "ed\u00b7ler", "Lust", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "La\u00dft himmelan den stolzen Jubel hallen,", "tokens": ["La\u00dft", "him\u00b7me\u00b7lan", "den", "stol\u00b7zen", "Ju\u00b7bel", "hal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Dankgef\u00fchl durchwalle jede Brust.", "tokens": ["Und", "Dank\u00b7ge\u00b7f\u00fchl", "durch\u00b7wal\u00b7le", "je\u00b7de", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VMFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Einst wollte die Natur ein Fest erschaffen,", "tokens": ["Einst", "woll\u00b7te", "die", "Na\u00b7tur", "ein", "Fest", "er\u00b7schaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Fest, wo Tugenden mit Grazien", "tokens": ["Ein", "Fest", ",", "wo", "Tu\u00b7gen\u00b7den", "mit", "Gra\u00b7zi\u00b7en"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PWAV", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Harmonisch ineinandertrafen", "tokens": ["Har\u00b7mo\u00b7nisch", "in\u00b7ein\u00b7an\u00b7der\u00b7tra\u00b7fen"], "token_info": ["word", "word"], "pos": ["ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und in dem sch\u00f6nsten Bunde sollten stehn,", "tokens": ["Und", "in", "dem", "sch\u00f6ns\u00b7ten", "Bun\u00b7de", "soll\u00b7ten", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und dieses Fest aufs reizendste zu zieren,", "tokens": ["Und", "die\u00b7ses", "Fest", "aufs", "rei\u00b7zends\u00b7te", "zu", "zie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "APPRART", "ADJA", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sah die Natur nach einem Namen um \u2013", "tokens": ["Sah", "die", "Na\u00b7tur", "nach", "ei\u00b7nem", "Na\u00b7men", "um", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "APPR", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Franziskens Namen sollt es f\u00fchren,", "tokens": ["Fran\u00b7zis\u00b7kens", "Na\u00b7men", "sollt", "es", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So war das Fest ein Heiligtum!", "tokens": ["So", "war", "das", "Fest", "ein", "Hei\u00b7lig\u00b7tum", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und dieses Fest, ihr Freunde, ist erschienen,", "tokens": ["Und", "die\u00b7ses", "Fest", ",", "ihr", "Freun\u00b7de", ",", "ist", "er\u00b7schie\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "$,", "PPOSAT", "NN", "$,", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Euch jauchz ichs mit Entz\u00fccken zu!", "tokens": ["Euch", "jauchz", "ichs", "mit", "Ent\u00b7z\u00fc\u00b7cken", "zu", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Jauchzt, Freunde, jauchzt mir nach: Es ist erschienen,", "tokens": ["Jauchzt", ",", "Freun\u00b7de", ",", "jauchzt", "mir", "nach", ":", "Es", "ist", "er\u00b7schie\u00b7nen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "VVFIN", "PPER", "PTKVZ", "$.", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und h\u00fcpft empor aus tatenloser Ruh!", "tokens": ["Und", "h\u00fcpft", "em\u00b7por", "aus", "ta\u00b7ten\u00b7lo\u00b7ser", "Ruh", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Heut wird kein Ach geh\u00f6rt \u2013 heut flie\u00dfet keine Tr\u00e4ne;", "tokens": ["Heut", "wird", "kein", "Ach", "ge\u00b7h\u00f6rt", "\u2013", "heut", "flie\u00b7\u00dfet", "kei\u00b7ne", "Tr\u00e4\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "VVFIN", "$(", "ADV", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nur froher Dank steigt himmelw\u00e4rts!", "tokens": ["Nur", "fro\u00b7her", "Dank", "steigt", "him\u00b7mel\u00b7w\u00e4rts", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "Die Luft erschallt von jubelndem Get\u00f6ne,", "tokens": ["Die", "Luft", "er\u00b7schallt", "von", "ju\u00b7beln\u00b7dem", "Ge\u00b7t\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Franziskens Name lebt durch jedes Herz.", "tokens": ["Fran\u00b7zis\u00b7kens", "Na\u00b7me", "lebt", "durch", "je\u00b7des", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Sie ist der D\u00fcrftgen Trost \u2013 sie gibt der Bl\u00f6\u00dfe Kleider,", "tokens": ["Sie", "ist", "der", "D\u00fcrft\u00b7gen", "Trost", "\u2013", "sie", "gibt", "der", "Bl\u00f6\u00b7\u00dfe", "Klei\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$(", "PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem Durste gibt sie Trank, dem Hunger Brot!", "tokens": ["Dem", "Durs\u00b7te", "gibt", "sie", "Trank", ",", "dem", "Hun\u00b7ger", "Brot", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "NN", "$,", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Traurigen macht schon ihr Anblick heiter", "tokens": ["Die", "Trau\u00b7ri\u00b7gen", "macht", "schon", "ihr", "An\u00b7blick", "hei\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "PPOSAT", "NN", "ADJD"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und scheucht vom Krankenlager weg den Tod.", "tokens": ["Und", "scheucht", "vom", "Kran\u00b7ken\u00b7la\u00b7ger", "weg", "den", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Ihr Anblick segenvoll \u2013 wie Sonnenblick den Fluren,", "tokens": ["Ihr", "An\u00b7blick", "se\u00b7gen\u00b7voll", "\u2013", "wie", "Son\u00b7nen\u00b7blick", "den", "Flu\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "$(", "KOKOM", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie wenn vom Himmel Fr\u00fchling niederstr\u00f6mt,", "tokens": ["Wie", "wenn", "vom", "Him\u00b7mel", "Fr\u00fch\u00b7ling", "nie\u00b7der\u00b7str\u00f6mt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "APPRART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Belebend Feuer f\u00fcllt die jauchzende Naturen,", "tokens": ["Be\u00b7le\u00b7bend", "Feu\u00b7er", "f\u00fcllt", "die", "jauch\u00b7zen\u00b7de", "Na\u00b7tu\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und alles wird mit Strahlen \u00fcberschwemmt,", "tokens": ["Und", "al\u00b7les", "wird", "mit", "Strah\u00b7len", "\u00fc\u00b7bersc\u00b7hwemmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "So l\u00e4chelt alle Welt \u2013 So schimmern die Gefilde,", "tokens": ["So", "l\u00e4\u00b7chelt", "al\u00b7le", "Welt", "\u2013", "So", "schim\u00b7mern", "die", "Ge\u00b7fil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "$(", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn sie wie G\u00f6ttin unter Menschen geht,", "tokens": ["Wenn", "sie", "wie", "G\u00f6t\u00b7tin", "un\u00b7ter", "Men\u00b7schen", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "KOKOM", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Von ihr flie\u00dft Segen aus und himmelvolle Milde", "tokens": ["Von", "ihr", "flie\u00dft", "Se\u00b7gen", "aus", "und", "him\u00b7mel\u00b7vol\u00b7le", "Mil\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "NN", "PTKVZ", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Auf jeden, den ihr sanfter Blick ersp\u00e4ht,", "tokens": ["Auf", "je\u00b7den", ",", "den", "ihr", "sanf\u00b7ter", "Blick", "er\u00b7sp\u00e4ht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "$,", "PRELS", "PPER", "ADJD", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Ihr holder Name fliegt hoch auf des Ruhmes Fl\u00fcgeln,", "tokens": ["Ihr", "hol\u00b7der", "Na\u00b7me", "fliegt", "hoch", "auf", "des", "Ruh\u00b7mes", "Fl\u00fc\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Unsterblichkeit verhei\u00dft ihr jeder Blick,", "tokens": ["U\u00b7nsterb\u00b7lich\u00b7keit", "ver\u00b7hei\u00dft", "ihr", "je\u00b7der", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}, "line.3": {"text": "Im Herzen thronet sie \u2013 und Freudentr\u00e4nen spiegeln", "tokens": ["Im", "Her\u00b7zen", "thro\u00b7net", "sie", "\u2013", "und", "Freu\u00b7den\u00b7tr\u00e4\u00b7nen", "spie\u00b7geln"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "$(", "KON", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Franziskens holdes Himmelbild zur\u00fcck.", "tokens": ["Fran\u00b7zis\u00b7kens", "hol\u00b7des", "Him\u00b7mel\u00b7bild", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "So wandelt sie dahin auf Rosenpfaden,", "tokens": ["So", "wan\u00b7delt", "sie", "da\u00b7hin", "auf", "Ro\u00b7sen\u00b7pfa\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PAV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihr Leben ist die sch\u00f6nste Harmonie,", "tokens": ["Ihr", "Le\u00b7ben", "ist", "die", "sch\u00f6ns\u00b7te", "Har\u00b7mo\u00b7nie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Umgl\u00e4nzt von tausend tugendsamen Taten,", "tokens": ["Um\u00b7gl\u00e4nzt", "von", "tau\u00b7send", "tu\u00b7gend\u00b7sa\u00b7men", "Ta\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "CARD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Seht die belohnte Tugend! \u2013 Sie!", "tokens": ["Seht", "die", "be\u00b7lohn\u00b7te", "Tu\u00b7gend", "!", "\u2013", "Sie", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$.", "$(", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "O Freunde, la\u00dft uns nie von unsrer Ehrfurcht wanken,", "tokens": ["O", "Freun\u00b7de", ",", "la\u00dft", "uns", "nie", "von", "uns\u00b7rer", "Ehr\u00b7furcht", "wan\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVIMP", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "La\u00dft unser Herz Franziskens Denkmal sein!", "tokens": ["La\u00dft", "un\u00b7ser", "Herz", "Fran\u00b7zis\u00b7kens", "Denk\u00b7mal", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "NE", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So werden wir mit niedrigen Gedanken", "tokens": ["So", "wer\u00b7den", "wir", "mit", "nied\u00b7ri\u00b7gen", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Niemalen unser Herz entweihn!", "tokens": ["Nie\u00b7ma\u00b7len", "un\u00b7ser", "Herz", "ent\u00b7weihn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}