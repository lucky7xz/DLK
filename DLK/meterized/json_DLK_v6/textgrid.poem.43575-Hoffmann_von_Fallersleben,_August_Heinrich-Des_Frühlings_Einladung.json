{"textgrid.poem.43575": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Des Fr\u00fchlings Einladung", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer steht auf unserm Anger und winket uns zu?", "tokens": ["Wer", "steht", "auf", "un\u00b7serm", "An\u00b7ger", "und", "win\u00b7ket", "uns", "zu", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "PPOSAT", "NN", "KON", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der Fr\u00fchling im sonnigen Glanze:", "tokens": ["Der", "Fr\u00fch\u00b7ling", "im", "son\u00b7ni\u00b7gen", "Glan\u00b7ze", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "\u00bbkommt Alle herbei von Berg und Thal", "tokens": ["\u00bb", "kommt", "Al\u00b7le", "her\u00b7bei", "von", "Berg", "und", "Thal"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "VVFIN", "PIS", "ADV", "APPR", "NN", "KON", "NN"], "meter": "+---+-+-+", "measure": "dactylic.init"}, "line.4": {"text": "Zum Ringelrosentanze!", "tokens": ["Zum", "Rin\u00b7gel\u00b7ro\u00b7sen\u00b7tan\u00b7ze", "!"], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Kommt und springet nun froh froh froh!", "tokens": ["Kommt", "und", "sprin\u00b7get", "nun", "froh", "froh", "froh", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "ADV", "ADJD", "ADJD", "ADJD", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "Kommt und singet hallo ho ho!", "tokens": ["Kommt", "und", "sin\u00b7get", "hal\u00b7lo", "ho", "ho", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "FM", "FM", "FM", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Heisa! Jeglichem reich' ich einen Kranz", "tokens": ["Hei\u00b7sa", "!", "Jeg\u00b7li\u00b7chem", "reich'", "ich", "ei\u00b7nen", "Kranz"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "PIS", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "Zum Ringelrosentanz.\u00ab", "tokens": ["Zum", "Rin\u00b7gel\u00b7ro\u00b7sen\u00b7tanz", ".", "\u00ab"], "token_info": ["word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wer singt und flattert hoch in den Himmel empor?", "tokens": ["Wer", "singt", "und", "flat\u00b7tert", "hoch", "in", "den", "Him\u00b7mel", "em\u00b7por", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "KON", "VVFIN", "ADJD", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und jubelt im sonnigen Glanze?", "tokens": ["Und", "ju\u00b7belt", "im", "son\u00b7ni\u00b7gen", "Glan\u00b7ze", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "\u00bbmeine Lerchen, die soll'n euch musicir'n", "tokens": ["\u00bb", "mei\u00b7ne", "Ler\u00b7chen", ",", "die", "soll'n", "euch", "mu\u00b7si\u00b7cir'n"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "PPOSAT", "NN", "$,", "PRELS", "PIS", "PPER", "VMFIN"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Zum Ringelrosentanze!", "tokens": ["Zum", "Rin\u00b7gel\u00b7ro\u00b7sen\u00b7tan\u00b7ze", "!"], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Kommt und springet nun froh froh froh!", "tokens": ["Kommt", "und", "sprin\u00b7get", "nun", "froh", "froh", "froh", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "ADV", "ADJD", "ADJD", "ADJD", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "Kommt und singet hallo ho ho!", "tokens": ["Kommt", "und", "sin\u00b7get", "hal\u00b7lo", "ho", "ho", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "FM", "FM", "FM", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Hei\u00dfa! Jeglichem reich' ich einen Kranz", "tokens": ["Hei\u00b7\u00dfa", "!", "Jeg\u00b7li\u00b7chem", "reich'", "ich", "ei\u00b7nen", "Kranz"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "PIS", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "Zum Ringelrosentanz.\u00ab", "tokens": ["Zum", "Rin\u00b7gel\u00b7ro\u00b7sen\u00b7tanz", ".", "\u00ab"], "token_info": ["word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}