{"dta.poem.1237": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Als er in Gesellschafft/ aber die Un-  \n rechte/ k\u00fc\u00dfte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Ich bin von K\u00fc\u00dfen satt: was hab ich nun davon?", "tokens": ["Ich", "bin", "von", "K\u00fc\u00b7\u00dfen", "satt", ":", "was", "hab", "ich", "nun", "da\u00b7von", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "ADJD", "$.", "PWS", "VAFIN", "PPER", "ADV", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein m\u00fcder Uberdru\u00df ist meiner Arbeit Lohn.", "tokens": ["Ein", "m\u00fc\u00b7der", "U\u00b7berd\u00b7ru\u00df", "ist", "mei\u00b7ner", "Ar\u00b7beit", "Lohn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der d\u00fcrren Lippen Staub klebt noch an meinen Z\u00e4hnen.", "tokens": ["Der", "d\u00fcr\u00b7ren", "Lip\u00b7pen", "Staub", "klebt", "noch", "an", "mei\u00b7nen", "Z\u00e4h\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich hab aus H\u00f6ffligkeit Margillens trocknen Mund ber\u00fchrt/", "tokens": ["Ich", "hab", "aus", "H\u00f6ff\u00b7lig\u00b7keit", "Mar\u00b7gil\u00b7lens", "trock\u00b7nen", "Mund", "be\u00b7r\u00fchrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "NE", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.5": {"text": "Corallen/ aber falsch/ und Rosen ohne Krafft gesp\u00fcrt.", "tokens": ["Co\u00b7ral\u00b7len", "/", "a\u00b7ber", "falsch", "/", "und", "Ro\u00b7sen", "oh\u00b7ne", "Krafft", "ge\u00b7sp\u00fcrt", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "ADV", "ADJD", "$(", "KON", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.6": {"text": "Die K\u00fcsse sind ein Thau/ der ohne Gunst und Gegen-Gunst", "tokens": ["Die", "K\u00fcs\u00b7se", "sind", "ein", "Thau", "/", "der", "oh\u00b7ne", "Gunst", "und", "Ge\u00b7gen\u00b7Gunst"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$(", "ART", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.7": {"text": "Wird Meel-Thau oder Reiff. Wer k\u00fcsset/ wenn er k\u00fc\u00dft um-", "tokens": ["Wird", "Meel\u00b7\u00b7T\u00b7hau", "o\u00b7der", "Reiff", ".", "Wer", "k\u00fcs\u00b7set", "/", "wenn", "er", "k\u00fc\u00dft", "um"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "NN", "KON", "NN", "$.", "PWS", "VVFIN", "$(", "KOUS", "PPER", "VVFIN", "TRUNC"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "sunst/", "tokens": ["sunst", "/"], "token_info": ["word", "punct"], "pos": ["ADV", "$("], "meter": "-", "measure": "single.down"}, "line.9": {"text": "Dem wird der Safft darvon zu Wasser oder Thr\u00e4nen/", "tokens": ["Dem", "wird", "der", "Safft", "dar\u00b7von", "zu", "Was\u00b7ser", "o\u00b7der", "Thr\u00e4\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "PAV", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ein einig Ku\u00df von mir an rechten Ort gesezt/", "tokens": ["Ein", "ei\u00b7nig", "Ku\u00df", "von", "mir", "an", "rech\u00b7ten", "Ort", "ge\u00b7sezt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "APPR", "PPER", "APPR", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Der h\u00e4tte mich weit mehr als alle die erg\u00f6zt.", "tokens": ["Der", "h\u00e4t\u00b7te", "mich", "weit", "mehr", "als", "al\u00b7le", "die", "er\u00b7g\u00f6zt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADJD", "PIAT", "KOKOM", "PIAT", "PDS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}