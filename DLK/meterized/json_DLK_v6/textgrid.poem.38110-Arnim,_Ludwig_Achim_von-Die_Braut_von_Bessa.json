": ["Wa\u00b7ren", "al\u00b7le", "roth", "L\u00fcndsch", "ge\u00b7kleidt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADJD", "NN", "VVPP", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "In gelben Wammes und Hosen,", "tokens": ["In", "gel\u00b7ben", "Wam\u00b7mes", "und", "Ho\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Sie sprungen als w\u00e4ren sie rasend,", "tokens": ["Sie", "sprun\u00b7gen", "als", "w\u00e4\u00b7ren", "sie", "ra\u00b7send", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Sie machten sich so breit,", "tokens": ["Sie", "mach\u00b7ten", "sich", "so", "breit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Zum Streit waren sie bereit.", "tokens": ["Zum", "Streit", "wa\u00b7ren", "sie", "be\u00b7reit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "ADJD", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.11": {"line.1": {"text": "Gro\u00dfen Hochmuth th\u00e4ten sie treiben,", "tokens": ["Gro\u00b7\u00dfen", "Hoch\u00b7muth", "th\u00e4\u00b7ten", "sie", "trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Mit Trotzen, Keiben und Schlan,", "tokens": ["Mit", "Trot\u00b7zen", ",", "Kei\u00b7ben", "und", "Schlan", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Das wolt ich bald aufschreiben,", "tokens": ["Das", "wolt", "ich", "bald", "auf\u00b7schrei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und nicht vergessen lan,", "tokens": ["Und", "nicht", "ver\u00b7ges\u00b7sen", "lan", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "VVPP", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie th\u00e4ten sich bald bedenken,", "tokens": ["Sie", "th\u00e4\u00b7ten", "sich", "bald", "be\u00b7den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Ein Trinkgeld wolten sie mir schenken,", "tokens": ["Ein", "Trink\u00b7geld", "wol\u00b7ten", "sie", "mir", "schen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Sie brachtens zu mir her,", "tokens": ["Sie", "brach\u00b7tens", "zu", "mir", "her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "War gar nicht mein Beger.", "tokens": ["War", "gar", "nicht", "mein", "Be\u00b7ger", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Sie th\u00e4ten mich bald fragen:", "tokens": ["Sie", "th\u00e4\