{"textgrid.poem.38110": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Die Braut von Bessa", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu Felsberg bat mich Kledte,", "tokens": ["Zu", "Fels\u00b7berg", "bat", "mich", "Kled\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich solt ihm schreiben recht,", "tokens": ["Ich", "solt", "ihm", "schrei\u00b7ben", "recht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was ich gesehen h\u00e4tte,", "tokens": ["Was", "ich", "ge\u00b7se\u00b7hen", "h\u00e4t\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von manchem stolzen Knecht,", "tokens": ["Von", "man\u00b7chem", "stol\u00b7zen", "Knecht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "In einem Dorf hie\u00df Bessa,", "tokens": ["In", "ei\u00b7nem", "Dorf", "hie\u00df", "Bes\u00b7sa", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Da war ein gro\u00df K\u00fcrmes,", "tokens": ["Da", "war", "ein", "gro\u00df", "K\u00fcr\u00b7mes", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJD", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Darzu ein grosser Tanz", "tokens": ["Dar\u00b7zu", "ein", "gros\u00b7ser", "Tanz"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Um einen Ketten-Kranz.", "tokens": ["Um", "ei\u00b7nen", "Ket\u00b7ten\u00b7Kranz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich kam einmal gen Be\u00df,", "tokens": ["Ich", "kam", "ein\u00b7mal", "gen", "Be\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Auf einen Sonntag fr\u00fch,", "tokens": ["Auf", "ei\u00b7nen", "Sonn\u00b7tag", "fr\u00fch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da war ein gro\u00df K\u00fcrmes,", "tokens": ["Da", "war", "ein", "gro\u00df", "K\u00fcr\u00b7mes", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJD", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Davon ich singen will,", "tokens": ["Da\u00b7von", "ich", "sin\u00b7gen", "will", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich ward gar sch\u00f6n empfangen,", "tokens": ["Ich", "ward", "gar", "sch\u00f6n", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Von ein'm der hie\u00df Hans Lange,", "tokens": ["Von", "ein'm", "der", "hie\u00df", "Hans", "Lan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "VVFIN", "NE", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Mit dem ich ziehen pflegt", "tokens": ["Mit", "dem", "ich", "zie\u00b7hen", "pflegt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "VVINF", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Gar manche liebe T\u00e4g.", "tokens": ["Gar", "man\u00b7che", "lie\u00b7be", "T\u00e4g", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Er bracht mich unter ein Linde,", "tokens": ["Er", "bracht", "mich", "un\u00b7ter", "ein", "Lin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NE", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die war unterschieden recht,", "tokens": ["Die", "war", "un\u00b7ter\u00b7schie\u00b7den", "recht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da fand sich ein w\u00fcst Gesinde,", "tokens": ["Da", "fand", "sich", "ein", "w\u00fcst", "Ge\u00b7sin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "VVFIN", "NN", "$,"], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Das waren die Eisern Knecht,", "tokens": ["Das", "wa\u00b7ren", "die", "Ei\u00b7sern", "Knecht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "NE", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Die h\u00e4tten sich bezeichnet mit Weiden,", "tokens": ["Die", "h\u00e4t\u00b7ten", "sich", "be\u00b7zeich\u00b7net", "mit", "Wei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PRF", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Kein Hochmuth wollen sie leiden,", "tokens": ["Kein", "Hoch\u00b7muth", "wol\u00b7len", "sie", "lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Sie sprungen auf den Plan,", "tokens": ["Sie", "sprun\u00b7gen", "auf", "den", "Plan", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Ein jeder wolt den Vorreihen han.", "tokens": ["Ein", "je\u00b7der", "wolt", "den", "Vor\u00b7rei\u00b7hen", "han", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VMFIN", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-++-+", "measure": "unknown.measure.penta"}}, "stanza.4": {"line.1": {"text": "Die andern trugen Berken,", "tokens": ["Die", "an\u00b7dern", "tru\u00b7gen", "Ber\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das war also gethan,", "tokens": ["Das", "war", "al\u00b7so", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das einer den andern soll merken,", "tokens": ["Das", "ei\u00b7ner", "den", "an\u00b7dern", "soll", "mer\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "ART", "ADJA", "VMFIN", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Wann sich erh\u00fcb ein Schlan,", "tokens": ["Wann", "sich", "er\u00b7h\u00fcb", "ein", "Schlan", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die dritten trugen Hopfen", "tokens": ["Die", "drit\u00b7ten", "tru\u00b7gen", "Hop\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Am Hals und auch am Kopf,", "tokens": ["Am", "Hals", "und", "auch", "am", "Kopf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Truz wer sie zornig m\u00e4cht,", "tokens": ["Truz", "wer", "sie", "zor\u00b7nig", "m\u00e4cht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PWS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.8": {"text": "Und sie zum Zanke br\u00e4cht.", "tokens": ["Und", "sie", "zum", "Zan\u00b7ke", "br\u00e4cht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wohl an demselbigen Tanze,", "tokens": ["Wohl", "an", "dem\u00b7sel\u00b7bi\u00b7gen", "Tan\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sahe man ein wunder sch\u00f6ne Magd,", "tokens": ["Sa\u00b7he", "man", "ein", "wun\u00b7der", "sch\u00f6\u00b7ne", "Magd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Sie kunt gar wohl umschwanzen,", "tokens": ["Sie", "kunt", "gar", "wohl", "um\u00b7schwan\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vor allen wohlbehagt,", "tokens": ["Vor", "al\u00b7len", "wohl\u00b7be\u00b7hagt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie kunt gar wol begaffen", "tokens": ["Sie", "kunt", "gar", "wol", "be\u00b7ga\u00b7ffen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADJD", "ADV", "ADV", "VVINF"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Mit M\u00f6nchen und mit Pfaffen,", "tokens": ["Mit", "M\u00f6n\u00b7chen", "und", "mit", "Pfaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Sie wolt kein andern han,", "tokens": ["Sie", "wolt", "kein", "an\u00b7dern", "han", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIAT", "PIS", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Als Eisserer Henzen Sohn.", "tokens": ["Als", "Eis\u00b7se\u00b7rer", "Hen\u00b7zen", "Sohn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "NE", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Sie hie\u00df die Riebel feiste,", "tokens": ["Sie", "hie\u00df", "die", "Rie\u00b7bel", "feis\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das war ganz offenbar.", "tokens": ["Das", "war", "ganz", "of\u00b7fen\u00b7bar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Viel Spott th\u00e4t sie beweisen,", "tokens": ["Viel", "Spott", "th\u00e4t", "sie", "be\u00b7wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "An manchem Knecht f\u00fcrwahr,", "tokens": ["An", "man\u00b7chem", "Knecht", "f\u00fcr\u00b7wahr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es hofft ein jeder Knabe", "tokens": ["Es", "hofft", "ein", "je\u00b7der", "Kna\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Kundschaft mit ihr zu haben,", "tokens": ["Kund\u00b7schaft", "mit", "ihr", "zu", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "PTKZU", "VAINF", "$,"], "meter": "---+-+-", "measure": "unknown.measure.di"}, "line.7": {"text": "Dadurch wuchs ihr der Muth,", "tokens": ["Da\u00b7durch", "wuchs", "ihr", "der", "Muth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.8": {"text": "Keinem Geringen th\u00e4t sie gut.", "tokens": ["Kei\u00b7nem", "Ge\u00b7rin\u00b7gen", "th\u00e4t", "sie", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.7": {"line.1": {"text": "Sie war so sch\u00f6n gezieret,", "tokens": ["Sie", "war", "so", "sch\u00f6n", "ge\u00b7zie\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den Sternen ward sie gleich,", "tokens": ["Den", "Ster\u00b7nen", "ward", "sie", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Darzu konnt sie vexieren", "tokens": ["Dar\u00b7zu", "konnt", "sie", "ve\u00b7xie\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "PPER", "VVINF"], "meter": "-+---+-", "measure": "dactylic.init"}, "line.4": {"text": "Die Knaben meisterlich,", "tokens": ["Die", "Kna\u00b7ben", "meis\u00b7ter\u00b7lich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie war von solcher Schanze,", "tokens": ["Sie", "war", "von", "sol\u00b7cher", "Schan\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Da\u00df jeder wolt mit ihr tanzen,", "tokens": ["Da\u00df", "je\u00b7der", "wolt", "mit", "ihr", "tan\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VMFIN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Dadurch zulezt geschah,", "tokens": ["Da\u00b7durch", "zu\u00b7lezt", "ge\u00b7schah", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Gro\u00df Leid und Ungemach.", "tokens": ["Gro\u00df", "Leid", "und", "Un\u00b7ge\u00b7mach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Da kam ein stolzer Knabe,", "tokens": ["Da", "kam", "ein", "stol\u00b7zer", "Kna\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der hie\u00df Bellerstein,", "tokens": ["Der", "hie\u00df", "Bel\u00b7ler\u00b7stein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Den Vortanz wolt er haben,", "tokens": ["Den", "Vor\u00b7tanz", "wolt", "er", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit der sch\u00f6nen Magd allein,", "tokens": ["Mit", "der", "sch\u00f6\u00b7nen", "Magd", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Er sprach: Mich thun verdrie\u00dfen,", "tokens": ["Er", "sprach", ":", "Mich", "thun", "ver\u00b7drie\u00b7\u00dfen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Helleparten und die Spiesse,", "tokens": ["Die", "Hel\u00b7le\u00b7par\u00b7ten", "und", "die", "Spies\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Der sehe ich also viel,", "tokens": ["Der", "se\u00b7he", "ich", "al\u00b7so", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "ADV", "$,"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.8": {"text": "Da\u00df ich nicht tanzen will.", "tokens": ["Da\u00df", "ich", "nicht", "tan\u00b7zen", "will", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Ein Zank erhub sich balde,", "tokens": ["Ein", "Zank", "er\u00b7hub", "sich", "bal\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Durch die Eissern und Bessar Knecht,", "tokens": ["Durch", "die", "Eis\u00b7sern", "und", "Bes\u00b7sar", "Knecht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "NE", "NE", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Ein jeder wolt den Plaz behalten,", "tokens": ["Ein", "je\u00b7der", "wolt", "den", "Plaz", "be\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie waren all k\u00fchn und frech,", "tokens": ["Sie", "wa\u00b7ren", "all", "k\u00fchn", "und", "frech", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Sie begundten sich zu schlagen,", "tokens": ["Sie", "be\u00b7gund\u00b7ten", "sich", "zu", "sc