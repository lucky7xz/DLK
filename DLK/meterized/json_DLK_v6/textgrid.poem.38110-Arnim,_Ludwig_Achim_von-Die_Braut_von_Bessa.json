"meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Ich macht mich bald darvon,", "tokens": ["Ich", "macht", "mich", "bald", "dar\u00b7von", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PAV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Begert von ihn kein Lohn.", "tokens": ["Be\u00b7gert", "von", "ihn", "kein", "Lohn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Der uns dies Lied thut singen,", "tokens": ["Der", "uns", "dies", "Lied", "thut", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PDS", "NN", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Will ich berichten bald,", "tokens": ["Will", "ich", "be\u00b7rich\u00b7ten", "bald", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Von dannen mu\u00dft er entspringen,", "tokens": ["Von", "dan\u00b7nen", "mu\u00dft", "er", "ent\u00b7sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Sonst er nicht worden alt,", "tokens": ["Sonst", "er", "nicht", "wor\u00b7den", "alt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PTKNEG", "VAPP", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er ging mit kurzen Schritten,", "tokens": ["Er", "ging", "mit", "kur\u00b7zen", "Schrit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFI