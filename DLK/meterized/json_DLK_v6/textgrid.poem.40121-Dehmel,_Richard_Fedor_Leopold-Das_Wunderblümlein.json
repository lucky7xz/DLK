{"textgrid.poem.40121": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Das Wunderbl\u00fcmlein", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Uns ist ein' Ros' entsprungen", "tokens": ["Uns", "ist", "ein'", "Ros'", "ent\u00b7sprun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "aus einer Wurzel zart;", "tokens": ["aus", "ei\u00b7ner", "Wur\u00b7zel", "zart", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "wie uns die Alten sungen,", "tokens": ["wie", "uns", "die", "Al\u00b7ten", "sun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "von Jesse kam die Art;", "tokens": ["von", "Jes\u00b7se", "kam", "die", "Art", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "und hat ein Bl\u00fcmlein bracht", "tokens": ["und", "hat", "ein", "Bl\u00fcm\u00b7lein", "bracht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ART", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "mitten im kalten Winter,", "tokens": ["mit\u00b7ten", "im", "kal\u00b7ten", "Win\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.7": {"text": "wol zu der halben Nacht.", "tokens": ["wol", "zu", "der", "hal\u00b7ben", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Das Bl\u00fcmlein war so reine", "tokens": ["Das", "Bl\u00fcm\u00b7lein", "war", "so", "rei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "und duftete so s\u00fc\u00df;", "tokens": ["und", "duf\u00b7te\u00b7te", "so", "s\u00fc\u00df", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "mit seinem milden Scheine", "tokens": ["mit", "sei\u00b7nem", "mil\u00b7den", "Schei\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "verkl\u00e4rt's die Finsternis;", "tokens": ["ver\u00b7kl\u00e4rt's", "die", "Fins\u00b7ter\u00b7nis", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "und leuchtet immerdar,", "tokens": ["und", "leuch\u00b7tet", "im\u00b7mer\u00b7dar", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "tr\u00f6stet die Menschenkinder", "tokens": ["tr\u00f6s\u00b7tet", "die", "Men\u00b7schen\u00b7kin\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.7": {"text": "holdselig, wunderbar.", "tokens": ["hold\u00b7se\u00b7lig", ",", "wun\u00b7der\u00b7bar", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Ein Stern mit hellem Scheine", "tokens": ["Ein", "Stern", "mit", "hel\u00b7lem", "Schei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "hat es der Welt verk\u00fcndt,", "tokens": ["hat", "es", "der", "Welt", "ver\u00b7k\u00fcndt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "den Hirten und den Heiden,", "tokens": ["den", "Hir\u00b7ten", "und", "den", "Hei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "wo man dies Bl\u00fcmlein findt.", "tokens": ["wo", "man", "dies", "Bl\u00fcm\u00b7lein", "findt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PDS", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nun ist uns nicht mehr bang,", "tokens": ["Nun", "ist", "uns", "nicht", "mehr", "bang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKNEG", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "seit aus der dunklen Erde", "tokens": ["seit", "aus", "der", "dunk\u00b7len", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "solch k\u00f6stlich Kn\u00f6splein sprang.", "tokens": ["solch", "k\u00f6st\u00b7lich", "Kn\u00f6s\u00b7plein", "sprang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}