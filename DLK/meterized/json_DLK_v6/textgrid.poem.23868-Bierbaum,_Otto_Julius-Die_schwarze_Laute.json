{"textgrid.poem.23868": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Die schwarze Laute", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus dem Rosenstocke", "tokens": ["Aus", "dem", "Ro\u00b7sen\u00b7sto\u00b7cke"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Vom Grabe des Christ", "tokens": ["Vom", "Gra\u00b7be", "des", "Christ"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Eine schwarze Laute", "tokens": ["Ei\u00b7ne", "schwar\u00b7ze", "Lau\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Gebauet ist;", "tokens": ["Ge\u00b7bau\u00b7et", "ist", ";"], "token_info": ["word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Der wurden gr\u00fcne Reben", "tokens": ["Der", "wur\u00b7den", "gr\u00fc\u00b7ne", "Re\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Zu Saiten", "tokens": ["Zu", "Sai\u00b7ten"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Gegeben.", "tokens": ["Ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.8": {"text": "Oh wehe du, wie selig sang,", "tokens": ["Oh", "we\u00b7he", "du", ",", "wie", "se\u00b7lig", "sang", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PPER", "$,", "PWAV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "So eross\u00fc\u00df, so jesusbang.", "tokens": ["So", "e\u00b7ros\u00b7s\u00fc\u00df", ",", "so", "je\u00b7sus\u00b7bang", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Die schwarze Rosenlaute.", "tokens": ["Die", "schwar\u00b7ze", "Ro\u00b7sen\u00b7lau\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich h\u00f6rte sie singen", "tokens": ["Ich", "h\u00f6r\u00b7te", "sie", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "In mailichter Nacht,", "tokens": ["In", "mai\u00b7lich\u00b7ter", "Nacht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Da bin ich zur Liebe", "tokens": ["Da", "bin", "ich", "zur", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPRART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "In Schmerzen erwacht,", "tokens": ["In", "Schmer\u00b7zen", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Da wurde meinem Leben", "tokens": ["Da", "wur\u00b7de", "mei\u00b7nem", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Sehnsucht", "tokens": ["Die", "Sehn\u00b7sucht"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Gegeben.", "tokens": ["Ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.8": {"text": "Oh wehe du, wie selig sang,", "tokens": ["Oh", "we\u00b7he", "du", ",", "wie", "se\u00b7lig", "sang", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PPER", "$,", "PWAV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "So jesuss\u00fc\u00df, so erosbang,", "tokens": ["So", "je\u00b7sus\u00b7s\u00fc\u00df", ",", "so", "e\u00b7ros\u00b7bang", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Die schwarze Rosenlaute.", "tokens": ["Die", "schwar\u00b7ze", "Ro\u00b7sen\u00b7lau\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}