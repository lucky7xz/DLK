{"dta.poem.14062": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "J\u00e4ger-Katechismus .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Ihr werd't doch nicht die Lust erjagen,", "tokens": ["Ihr", "werd't", "doch", "nicht", "die", "Lust", "er\u00b7ja\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PTKNEG", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr m\u00f6g't durch alle W\u00e4lder geh'n;", "tokens": ["Ihr", "m\u00f6g't", "durch", "al\u00b7le", "W\u00e4l\u00b7der", "geh'n", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nur m\u00fcde F\u00fc\u00df' und leere Magen \u2014", "tokens": ["Nur", "m\u00fc\u00b7de", "F\u00fc\u00df'", "und", "lee\u00b7re", "Ma\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mir m\u00f6cht' die J\u00e4gerei vergeh'n!", "tokens": ["Mir", "m\u00f6cht'", "die", "J\u00e4\u00b7ge\u00b7rei", "ver\u00b7geh'n", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}