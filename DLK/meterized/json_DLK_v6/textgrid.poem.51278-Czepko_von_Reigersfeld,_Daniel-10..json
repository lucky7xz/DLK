{"textgrid.poem.51278": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "10.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Gold, da\u00df du so hoch bist in der Welt geacht,", "tokens": ["O", "Gold", ",", "da\u00df", "du", "so", "hoch", "bist", "in", "der", "Welt", "ge\u00b7acht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "KOUS", "PPER", "ADV", "ADJD", "VAFIN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht, weil dir G\u00fctt und Recht mu\u00df knien zu den F\u00fc\u00dfen,", "tokens": ["Nicht", ",", "weil", "dir", "G\u00fctt", "und", "Recht", "mu\u00df", "kni\u00b7en", "zu", "den", "F\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "KOUS", "PPER", "NN", "KON", "NN", "VMFIN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nicht, weil die Welt du kanst in strenge Dienste schl\u00fc\u00dfen,", "tokens": ["Nicht", ",", "weil", "die", "Welt", "du", "kanst", "in", "stren\u00b7ge", "Diens\u00b7te", "schl\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "KOUS", "ART", "NN", "PPER", "VMFIN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nur, weil du aus der H\u00f6ll' hab ich dein Thun verlacht.", "tokens": ["Nur", ",", "weil", "du", "aus", "der", "H\u00f6ll'", "hab", "ich", "dein", "Thun", "ver\u00b7lacht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "APPR", "ART", "NN", "VAFIN", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Welt steigt in die Erd, erhebt, was sie versenckt", "tokens": ["Die", "Welt", "steigt", "in", "die", "Erd", ",", "er\u00b7hebt", ",", "was", "sie", "ver\u00b7senckt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,", "VVFIN", "$,", "PRELS", "PPER", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und uns hinunter st\u00f6\u00dft: Der Felsen Eingeweide", "tokens": ["Und", "uns", "hin\u00b7un\u00b7ter", "st\u00f6\u00dft", ":", "Der", "Fel\u00b7sen", "Ein\u00b7ge\u00b7wei\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "ADV", "VVFIN", "$.", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und ihr vergifftes Marck f\u00fchrt uns zum steten Leide:", "tokens": ["Und", "ihr", "ver\u00b7giff\u00b7tes", "Marck", "f\u00fchrt", "uns", "zum", "ste\u00b7ten", "Lei\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zur Freyheit, die uns druckt, zur Wolfarth, die uns kr\u00e4nckt.", "tokens": ["Zur", "Frey\u00b7heit", ",", "die", "uns", "druckt", ",", "zur", "Wolf\u00b7arth", ",", "die", "uns", "kr\u00e4nckt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "APPRART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Drumb Gold, wer dich nur hat, dem fehlt denn nichts als du,", "tokens": ["Drumb", "Gold", ",", "wer", "dich", "nur", "hat", ",", "dem", "fehlt", "denn", "nichts", "als", "du", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "NN", "$,", "PWS", "PPER", "ADV", "VAFIN", "$,", "PRELS", "VVFIN", "ADV", "PIS", "KOKOM", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er kan ohn alle Scheu, was er begehrt, erreichen:", "tokens": ["Er", "kan", "ohn", "al\u00b7le", "Scheu", ",", "was", "er", "be\u00b7gehrt", ",", "er\u00b7rei\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PIAT", "NN", "$,", "PWS", "PPER", "VVPP", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch sage mir, wie da\u00df du also must erbleichen?", "tokens": ["Doch", "sa\u00b7ge", "mir", ",", "wie", "da\u00df", "du", "al\u00b7so", "must", "er\u00b7blei\u00b7chen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "KOKOM", "KOUS", "PPER", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich f\u00fcrchte mich so sehr, denn niemand l\u00e4st mir Ruh.", "tokens": ["Ich", "f\u00fcrch\u00b7te", "mich", "so", "sehr", ",", "denn", "nie\u00b7mand", "l\u00e4st", "mir", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "$,", "KON", "PIS", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}