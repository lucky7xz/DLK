{"textgrid.poem.62534": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Pilger zieht in weites Land,", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Pilger zieht in weites Land,", "tokens": ["Ein", "Pil\u00b7ger", "zieht", "in", "wei\u00b7tes", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er klopft an manches Thor;", "tokens": ["Er", "klopft", "an", "man\u00b7ches", "Thor", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was Sch\u00f6nes seinem Blick entschwand,", "tokens": ["Was", "Sch\u00f6\u00b7nes", "sei\u00b7nem", "Blick", "ent\u00b7schwand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schwebt seinem Geiste vor.", "tokens": ["Schwebt", "sei\u00b7nem", "Geis\u00b7te", "vor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Er tr\u00e4gt und hegt es im Gem\u00fcth,", "tokens": ["Er", "tr\u00e4gt", "und", "hegt", "es", "im", "Ge\u00b7m\u00fcth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was Liebes ihn erfreut,", "tokens": ["Was", "Lie\u00b7bes", "ihn", "er\u00b7freut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es labt ihn, wie ein altes Lied", "tokens": ["Es", "labt", "ihn", ",", "wie", "ein", "al\u00b7tes", "Lied"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aus ferner Knabenzeit.", "tokens": ["Aus", "fer\u00b7ner", "Kna\u00b7ben\u00b7zeit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ach! immer vorw\u00e4rts geht der Weg,", "tokens": ["Ach", "!", "im\u00b7mer", "vor\u00b7w\u00e4rts", "geht", "der", "Weg", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Inde\u00df man r\u00fcckw\u00e4rts denkt,", "tokens": ["In\u00b7de\u00df", "man", "r\u00fcck\u00b7w\u00e4rts", "denkt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wohin sich Br\u00fccke nicht noch Steg", "tokens": ["Wo\u00b7hin", "sich", "Br\u00fc\u00b7cke", "nicht", "noch", "Steg"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PRF", "NN", "PTKNEG", "ADV", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und keine Stra\u00dfe lenkt.", "tokens": ["Und", "kei\u00b7ne", "Stra\u00b7\u00dfe", "lenkt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Doch vorw\u00e4rts, r\u00fcckw\u00e4rts \u2013 Alles schlie\u00dft", "tokens": ["Doch", "vor\u00b7w\u00e4rts", ",", "r\u00fcck\u00b7w\u00e4rts", "\u2013", "Al\u00b7les", "schlie\u00dft"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "$,", "ADV", "$(", "PIS", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die sel'ge Ewigkeit,", "tokens": ["Die", "sel'\u00b7ge", "E\u00b7wig\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Vorzeit und die Zukunft gr\u00fc\u00dft,", "tokens": ["Die", "Vor\u00b7zeit", "und", "die", "Zu\u00b7kunft", "gr\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn man sich der geweiht.", "tokens": ["Wenn", "man", "sich", "der", "ge\u00b7weiht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PRF", "ART", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Zur Rechten nicht noch Linken schaut,", "tokens": ["Zur", "Rech\u00b7ten", "nicht", "noch", "Lin\u00b7ken", "schaut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKNEG", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wer treu das Eine meint.", "tokens": ["Wer", "treu", "das", "Ei\u00b7ne", "meint", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "ART", "PIS", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nur Eins ist Noth, o Gottesbraut,", "tokens": ["Nur", "Eins", "ist", "Noth", ",", "o", "Got\u00b7tes\u00b7braut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PIS", "VAFIN", "NN", "$,", "FM", "FM", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df hell die Lampe scheint!", "tokens": ["Da\u00df", "hell", "die", "Lam\u00b7pe", "scheint", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "La\u00df deine Augen fromm und klar", "tokens": ["La\u00df", "dei\u00b7ne", "Au\u00b7gen", "fromm", "und", "klar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPOSAT", "NN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und still die Seele sein,", "tokens": ["Und", "still", "die", "See\u00b7le", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Zeuch fr\u00f6hlich in das neue Jahr,", "tokens": ["Zeuch", "fr\u00f6h\u00b7lich", "in", "das", "neu\u00b7e", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ins neue Leben ein!", "tokens": ["Ins", "neu\u00b7e", "Le\u00b7ben", "ein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Du kennst Ihn ja, du hast Ihn doch,", "tokens": ["Du", "kennst", "Ihn", "ja", ",", "du", "hast", "Ihn", "doch", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "PPER", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der nimmer von dir weicht;", "tokens": ["Der", "nim\u00b7mer", "von", "dir", "weicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So schwer ist unsers Meisters Joch,", "tokens": ["So", "schwer", "ist", "un\u00b7sers", "Meis\u00b7ters", "Joch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPOSAT", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und ach! so wunderleicht!", "tokens": ["Und", "ach", "!", "so", "wun\u00b7der\u00b7leicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "XY", "$.", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Dein Garten bl\u00fcht in Seiner Hut,", "tokens": ["Dein", "Gar\u00b7ten", "bl\u00fcht", "in", "Sei\u00b7ner", "Hut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er ist so lieb und mild,", "tokens": ["Er", "ist", "so", "lieb", "und", "mild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Br\u00fcnnlein tr\u00e4nkt er aus der Flut,", "tokens": ["Die", "Br\u00fcnn\u00b7lein", "tr\u00e4nkt", "er", "aus", "der", "Flut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die ewig labend quillt.", "tokens": ["Die", "e\u00b7wig", "la\u00b7bend", "quillt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Wir treiben unsre Pilgrimschaft", "tokens": ["Wir", "trei\u00b7ben", "uns\u00b7re", "Pil\u00b7grim\u00b7schaft"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und wandern gl\u00e4ubig fort,", "tokens": ["Und", "wan\u00b7dern", "gl\u00e4u\u00b7big", "fort", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Uns bindet ", "tokens": ["Uns", "bin\u00b7det"], "token_info": ["word", "word"], "pos": ["PPER", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Auch an dem fernsten Ort.", "tokens": ["Auch", "an", "dem", "ferns\u00b7ten", "Ort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Wir finden uns gewi\u00df am Ziel,", "tokens": ["Wir", "fin\u00b7den", "uns", "ge\u00b7wi\u00df", "am", "Ziel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In unsers Vaters Haus,", "tokens": ["In", "un\u00b7sers", "Va\u00b7ters", "Haus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und ruhn an B\u00e4chen, traut und k\u00fchl,", "tokens": ["Und", "ruhn", "an", "B\u00e4\u00b7chen", ",", "traut", "und", "k\u00fchl", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "$,", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An Jesu Wunden aus.", "tokens": ["An", "Je\u00b7su", "Wun\u00b7den", "aus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}