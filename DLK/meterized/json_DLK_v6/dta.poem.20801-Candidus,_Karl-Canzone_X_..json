{"dta.poem.20801": {"metadata": {"author": {"name": "Candidus, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Canzone   X .", "genre": "Epos", "period": "N.A.", "pub_year": "1854", "urn": "urn:nbn:de:kobv:b4-20090519783", "language": ["de:0.99"], "booktitle": "Candidus, Karl: Der deutsche Christus. F\u00fcnfzehn Canzonen. Leipzig, 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df Jegliches an seinem ", "tokens": ["Da\u00df", "Jeg\u00b7li\u00b7ches", "an", "sei\u00b7nem"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PIS", "APPR", "PPOSAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Darin besteht der ", "tokens": ["Da\u00b7rin", "be\u00b7steht", "der"], "token_info": ["word", "word", "word"], "pos": ["PAV", "VVFIN", "ART"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Wenn in der Sch\u00f6pfung Schoo\u00df die Rose pranget", "tokens": ["Wenn", "in", "der", "Sch\u00f6p\u00b7fung", "Schoo\u00df", "die", "Ro\u00b7se", "pran\u00b7get"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sind Primel und Viole l\u00e4ngst gewesen,", "tokens": ["Sind", "Pri\u00b7mel", "und", "Vi\u00b7o\u00b7le", "l\u00e4ngst", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "ADV", "VAPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und bl\u00fchen herbstlich Astern im Vereine,", "tokens": ["Und", "bl\u00fc\u00b7hen", "herbst\u00b7lich", "As\u00b7tern", "im", "Ver\u00b7ei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ist, traun! ein Thor wer Tulpen noch verlanget.", "tokens": ["Ist", ",", "traun", "!", "ein", "Thor", "wer", "Tul\u00b7pen", "noch", "ver\u00b7lan\u00b7get", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "VVINF", "$.", "ART", "NN", "PWS", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Wenn so in Angeln hanget,", "tokens": ["Wenn", "so", "in", "An\u00b7geln", "han\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die unverr\u00fcckbar sind, der Zeiten Kreislauf,", "tokens": ["Die", "un\u00b7ver\u00b7r\u00fcck\u00b7bar", "sind", ",", "der", "Zei\u00b7ten", "Kreis\u00b7lauf", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAFIN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Wenn bei der glanzumhauchten Sommerhitze", "tokens": ["Wenn", "bei", "der", "glan\u00b7zum\u00b7hauch\u00b7ten", "Som\u00b7mer\u00b7hit\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Mit allem eurem Witze", "tokens": ["Mit", "al\u00b7lem", "eu\u00b7rem", "Wit\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIS", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Vergeblich ihr beriethet einen Eislauf,", "tokens": ["Ver\u00b7geb\u00b7lich", "ihr", "be\u00b7rie\u00b7thet", "ei\u00b7nen", "Eis\u00b7lauf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Warum doch soll des Geistes Fl\u00fcgelspreiten", "tokens": ["Wa\u00b7rum", "doch", "soll", "des", "Geis\u00b7tes", "Fl\u00fc\u00b7gel\u00b7sprei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VMFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Nicht auch erfolgen zu bestimmten Zeiten?", "tokens": ["Nicht", "auch", "er\u00b7fol\u00b7gen", "zu", "be\u00b7stimm\u00b7ten", "Zei\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVINF", "PTKZU", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wer schafft von seinem Ort den Himalaya", "tokens": ["Wer", "schafft", "von", "sei\u00b7nem", "Ort", "den", "Hi\u00b7ma\u00b7la\u00b7ya"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Und macht ihn gleich der kleinsten H\u00fcgelkette?", "tokens": ["Und", "macht", "ihn", "gleich", "der", "kleins\u00b7ten", "H\u00fc\u00b7gel\u00b7ket\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wer zieht in Gr\u00f6nland s\u00fc\u00dfe Rebenbl\u00fcten?", "tokens": ["Wer", "zieht", "in", "Gr\u00f6n\u00b7land", "s\u00fc\u00b7\u00dfe", "Re\u00b7ben\u00b7bl\u00fc\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wer kann dem Palmbaum \u00e4ndern seine St\u00e4tte", "tokens": ["Wer", "kann", "dem", "Palm\u00b7baum", "\u00e4n\u00b7dern", "sei\u00b7ne", "St\u00e4t\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "ART", "NN", "VVINF", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Da\u00df er nicht schatte mehr dem stolzen Raja?", "tokens": ["Da\u00df", "er", "nicht", "schat\u00b7te", "mehr", "dem", "stol\u00b7zen", "Ra\u00b7ja", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Kann auch das Nordland Strau\u00dfeneier br\u00fcten?", "tokens": ["Kann", "auch", "das", "Nord\u00b7land", "Strau\u00b7\u00dfen\u00b7ei\u00b7er", "br\u00fc\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "So m\u00f6gt ihr nicht verh\u00fcten", "tokens": ["So", "m\u00f6gt", "ihr", "nicht", "ver\u00b7h\u00fc\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PTKNEG", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Da\u00df jedem Geist gemessen sei der Schauplatz.", "tokens": ["Da\u00df", "je\u00b7dem", "Geist", "ge\u00b7mes\u00b7sen", "sei", "der", "Schau\u00b7platz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "VVPP", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Doch ob von jedem Stein wir h\u00e4tten Kunde", "tokens": ["Doch", "ob", "von", "je\u00b7dem", "Stein", "wir", "h\u00e4t\u00b7ten", "Kun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "APPR", "PIAT", "NN", "PPER", "VAFIN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Wo und zu welcher Stunde", "tokens": ["Wo", "und", "zu", "wel\u00b7cher", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "KON", "APPR", "PWAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Er einst erschienen auf der Menschheit Bauplatz,", "tokens": ["Er", "einst", "er\u00b7schie\u00b7nen", "auf", "der", "Menschheit", "Bau\u00b7platz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Ihr g\u00f6nnt in Zeit und Raum dem keine Stelle", "tokens": ["Ihr", "g\u00f6nnt", "in", "Zeit", "und", "Raum", "dem", "kei\u00b7ne", "Stel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "ART", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Den laut zum Eckstein schlug des Bauherrn Kelle.", "tokens": ["Den", "laut", "zum", "E\u00b7ckstein", "schlug", "des", "Bau\u00b7herrn", "Kel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPRART", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nach zeitma\u00dfvollem Reigenschritt der Horen", "tokens": ["Nach", "zeit\u00b7ma\u00df\u00b7vol\u00b7lem", "Rei\u00b7gen\u00b7schritt", "der", "Ho\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ergl\u00fchte jedes Aufschwungs Morgenr\u00f6te", "tokens": ["Er\u00b7gl\u00fch\u00b7te", "je\u00b7des", "Auf\u00b7schwungs", "Mor\u00b7gen\u00b7r\u00f6\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PIAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Stets wo und wann des Cynthiers Gespann kam.", "tokens": ["Stets", "wo", "und", "wann", "des", "Cyn\u00b7thiers", "Ge\u00b7spann", "kam", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "KON", "PWAV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ihr billigt wol da\u00df Hegel und da\u00df G\u00f6the", "tokens": ["Ihr", "bil\u00b7ligt", "wol", "da\u00df", "He\u00b7gel", "und", "da\u00df", "G\u00f6\u00b7the"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "KOUS", "NE", "KON", "KOUS", "NE"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Gerade da und dann uns ward geboren?", "tokens": ["Ge\u00b7ra\u00b7de", "da", "und", "dann", "uns", "ward", "ge\u00b7bo\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KON", "ADV", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Doch da\u00df das Wort, was uns als Gottesmann kam,", "tokens": ["Doch", "da\u00df", "das", "Wort", ",", "was", "uns", "als", "Got\u00b7tes\u00b7mann", "kam", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "$,", "PRELS", "PPER", "KOUS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.7": {"text": "Gerade da und dann kam,", "tokens": ["Ge\u00b7ra\u00b7de", "da", "und", "dann", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KON", "ADV", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Das R\u00e4tselwort darob die V\u00f6lker schwitzen,", "tokens": ["Das", "R\u00e4t\u00b7sel\u00b7wort", "da\u00b7rob", "die", "V\u00f6l\u00b7ker", "schwit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PAV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Der Menschheit und Geschichte Wort, da\u00df Stunden", "tokens": ["Der", "Menschheit", "und", "Ge\u00b7schich\u00b7te", "Wort", ",", "da\u00df", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "NN", "$,", "KOUS", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Und R\u00e4ume das gebunden,", "tokens": ["Und", "R\u00e4u\u00b7me", "das", "ge\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Ein rechtes Ziel d\u00e4ucht Solches euern Witzen.", "tokens": ["Ein", "rech\u00b7tes", "Ziel", "d\u00e4ucht", "Sol\u00b7ches", "eu\u00b7ern", "Wit\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PIS", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Inde\u00df wo Ewiges auf Erden auftrat", "tokens": ["In\u00b7de\u00df", "wo", "E\u00b7wi\u00b7ges", "auf", "Er\u00b7den", "auf\u00b7trat"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PWAV", "ADJA", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "War es ein Mensch der in der Zeiten Lauf trat.", "tokens": ["War", "es", "ein", "Mensch", "der", "in", "der", "Zei\u00b7ten", "Lauf", "trat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ART", "APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Weil ihr in abgezogner Allgemeinheit", "tokens": ["Weil", "ihr", "in", "ab\u00b7ge\u00b7zog\u00b7ner", "All\u00b7ge\u00b7mein\u00b7heit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Ewige nur w\u00e4hnet, ist ein Gr\u00e4uel", "tokens": ["Das", "E\u00b7wi\u00b7ge", "nur", "w\u00e4h\u00b7net", ",", "ist", "ein", "Gr\u00e4u\u00b7el"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "ADV", "VVFIN", "$,", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die herrlichste Gestalt euch der Geschichte.", "tokens": ["Die", "herr\u00b7lichs\u00b7te", "Ge\u00b7stalt", "euch", "der", "Ge\u00b7schich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Wahrheit seht ihr nur wie einen Kn\u00e4uel.", "tokens": ["Die", "Wahr\u00b7heit", "seht", "ihr", "nur", "wie", "ei\u00b7nen", "Kn\u00e4u\u00b7el", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Versucht's und schaut des Nazareners Reinheit", "tokens": ["Ver\u00b7sucht's", "und", "schaut", "des", "Na\u00b7za\u00b7re\u00b7ners", "Rein\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "KON", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Mit uns in seines Mittlerthumes Lichte", "tokens": ["Mit", "uns", "in", "sei\u00b7nes", "Mitt\u00b7ler\u00b7thu\u00b7mes", "Lich\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Worin er treu und schlichte", "tokens": ["Wo\u00b7rin", "er", "treu", "und", "schlich\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADJD", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Fortlebt, fortstirbt und s\u00fchnt durch seine S\u00f6hne", "tokens": ["Fort\u00b7lebt", ",", "forts\u00b7tirbt", "und", "s\u00fchnt", "durch", "sei\u00b7ne", "S\u00f6h\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "VVFIN", "KON", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Bis da\u00df er Alles einverleibt dem Einen,", "tokens": ["Bis", "da\u00df", "er", "Al\u00b7les", "ein\u00b7ver\u00b7leibt", "dem", "Ei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "PIS", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "De\u00df Minnen und de\u00df Meinen", "tokens": ["De\u00df", "Min\u00b7nen", "und", "de\u00df", "Mei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Das seine war und ist als h\u00f6chstes Sch\u00f6ne.", "tokens": ["Das", "sei\u00b7ne", "war", "und", "ist", "als", "h\u00f6chs\u00b7tes", "Sch\u00f6\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "VAFIN", "KON", "VAFIN", "KOKOM", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Vielleicht erfa\u00df