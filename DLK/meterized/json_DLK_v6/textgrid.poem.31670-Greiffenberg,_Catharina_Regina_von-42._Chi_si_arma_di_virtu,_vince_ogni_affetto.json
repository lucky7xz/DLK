{"textgrid.poem.31670": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "42. Chi si arma di virtu, vince ogni affetto", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer sich mit der Tugend r\u00fcstet / wie der Perseus mit den Haupt", "tokens": ["Wer", "sich", "mit", "der", "Tu\u00b7gend", "r\u00fcs\u00b7tet", "/", "wie", "der", "Per\u00b7seus", "mit", "den", "Haupt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PRF", "APPR", "ART", "NN", "VVFIN", "$(", "KOKOM", "ART", "NE", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "der Medusa / so dem Drachen seinem Feind das f\u00fchlen raubt:", "tokens": ["der", "Me\u00b7du\u00b7sa", "/", "so", "dem", "Dra\u00b7chen", "sei\u00b7nem", "Feind", "das", "f\u00fch\u00b7len", "raubt", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$(", "ADV", "ART", "NN", "PPOSAT", "NN", "ART", "ADJA", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "machet der Begierde Schlangen / durch der Keuschheit Schild zu Stein /", "tokens": ["ma\u00b7chet", "der", "Be\u00b7gier\u00b7de", "Schlan\u00b7gen", "/", "durch", "der", "Keuschheit", "Schild", "zu", "Stein", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$(", "APPR", "ART", "NN", "NN", "APPR", "NN", "$("], "meter": "+-+-+-+-+--+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "welchen ihm die Freyheit schenket / da\u00df das Herze frey kan seyn.", "tokens": ["wel\u00b7chen", "ihm", "die", "Frey\u00b7heit", "schen\u00b7ket", "/", "da\u00df", "das", "Her\u00b7ze", "frey", "kan", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "PPER", "ART", "NN", "VVFIN", "$(", "KOUS", "PDS", "VVFIN", "ADJD", "VMFIN", "VAINF", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}