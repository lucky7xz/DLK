{"dta.poem.10191": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "Sicilianen .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.99"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "Du hast wohl einen Wunsch, noch so bescheiden,           ", "tokens": ["Du", "hast", "wohl", "ei\u00b7nen", "Wunsch", ",", "noch", "so", "be\u00b7schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "$,", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Leben will ihn nimmer dir gew\u00e4hren.", "tokens": ["Das", "Le\u00b7ben", "will", "ihn", "nim\u00b7mer", "dir", "ge\u00b7w\u00e4h\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "ADV", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein anderer hat\u2019s, doch wird er dich beneiden", "tokens": ["Ein", "an\u00b7de\u00b7rer", "hat's", ",", "doch", "wird", "er", "dich", "be\u00b7nei\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "VAFIN", "$,", "ADV", "VAFIN", "PPER", "PRF", "VVINF"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Um das, was dein, im Fieber sich verzehren.", "tokens": ["Um", "das", ",", "was", "dein", ",", "im", "Fie\u00b7ber", "sich", "ver\u00b7zeh\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PDS", "$,", "PRELS", "PPOSAT", "$,", "APPRART", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Was willst du dir dein schmales Gl\u00fcck beschneiden", "tokens": ["Was", "willst", "du", "dir", "dein", "schma\u00b7les", "Gl\u00fcck", "be\u00b7schnei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "PPER", "PPER", "PPOSAT", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und Birnen brechen aus Getreide\u00e4hren.", "tokens": ["Und", "Bir\u00b7nen", "bre\u00b7chen", "aus", "Ge\u00b7trei\u00b7de\u00e4h\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Ich wette, tr\u00fcgest du das Wams von Seiden,", "tokens": ["Ich", "wet\u00b7te", ",", "tr\u00fc\u00b7gest", "du", "das", "Wams", "von", "Sei\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "PPER", "ART", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Du w\u00fcnschtest. Dir den Zottelpelz des B\u00e4ren.", "tokens": ["Du", "w\u00fcnschtest", ".", "Dir", "den", "Zot\u00b7tel\u00b7pelz", "des", "B\u00e4\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}