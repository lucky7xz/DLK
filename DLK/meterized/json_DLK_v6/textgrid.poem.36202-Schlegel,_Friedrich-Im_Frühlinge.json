{"textgrid.poem.36202": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Im Fr\u00fchlinge", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie freut sich die Seele der Freude erschlossen,", "tokens": ["Wie", "freut", "sich", "die", "See\u00b7le", "der", "Freu\u00b7de", "er\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PRF", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Im Fr\u00fchlingestagen,", "tokens": ["Im", "Fr\u00fch\u00b7lin\u00b7ge\u00b7sta\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Die mutigen Lieder zu wagen,", "tokens": ["Die", "mu\u00b7ti\u00b7gen", "Lie\u00b7der", "zu", "wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Entrissen dem Z\u00fcgel in Freiheit zu jagen,", "tokens": ["Ent\u00b7ris\u00b7sen", "dem", "Z\u00fc\u00b7gel", "in", "Frei\u00b7heit", "zu", "ja\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.5": {"text": "Das Ziel zu erreichen mit k\u00fchnen Geschossen.", "tokens": ["Das", "Ziel", "zu", "er\u00b7rei\u00b7chen", "mit", "k\u00fch\u00b7nen", "Ge\u00b7schos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Das Feuer der Fluren will Freude nur sagen;", "tokens": ["Das", "Feu\u00b7er", "der", "Flu\u00b7ren", "will", "Freu\u00b7de", "nur", "sa\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VMFIN", "NN", "ADV", "VVINF", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Im Dunkel der B\u00e4ume", "tokens": ["Im", "Dun\u00b7kel", "der", "B\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Da bilden sich rosige Tr\u00e4ume,", "tokens": ["Da", "bil\u00b7den", "sich", "ro\u00b7si\u00b7ge", "Tr\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Da schwellen die Kr\u00e4fte, da schwindet das Zagen.", "tokens": ["Da", "schwel\u00b7len", "die", "Kr\u00e4f\u00b7te", ",", "da", "schwin\u00b7det", "das", "Za\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.5": {"text": "Nun w\u00e4chst Fantasie wie Felsen zu ragen,", "tokens": ["Nun", "w\u00e4chst", "Fan\u00b7ta\u00b7sie", "wie", "Fel\u00b7sen", "zu", "ra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "KOKOM", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Es kommen geschossen", "tokens": ["Es", "kom\u00b7men", "ge\u00b7schos\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVINF", "VVPP"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Gestalten auf feurigen Rossen,", "tokens": ["Ge\u00b7stal\u00b7ten", "auf", "feu\u00b7ri\u00b7gen", "Ros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Im Silber der Fl\u00fcsse dann Friede geflossen", "tokens": ["Im", "Sil\u00b7ber", "der", "Fl\u00fcs\u00b7se", "dann", "Frie\u00b7de", "ge\u00b7flos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "ADV", "NN", "VVPP"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.9": {"text": "Und dunkel erklingen die heiligen Klagen.", "tokens": ["Und", "dun\u00b7kel", "er\u00b7klin\u00b7gen", "die", "hei\u00b7li\u00b7gen", "Kla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "Wenn k\u00fchne Gedichte den Lippen entflossen", "tokens": ["Wenn", "k\u00fch\u00b7ne", "Ge\u00b7dich\u00b7te", "den", "Lip\u00b7pen", "ent\u00b7flos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "NN", "ART", "NN", "VVINF"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "In fliegenden Worten,", "tokens": ["In", "flie\u00b7gen\u00b7den", "Wor\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "So \u00f6ffnen sich feurige Pforten,", "tokens": ["So", "\u00f6ff\u00b7nen", "sich", "feu\u00b7ri\u00b7ge", "Pfor\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und klar ist der Fr\u00fchling der Gottheit Genossen.", "tokens": ["Und", "klar", "ist", "der", "Fr\u00fch\u00b7ling", "der", "Got\u00b7theit", "Ge\u00b7nos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ART", "NN", "ART", "NN", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.5": {"text": "Von Wogen des Lebens harmonisch umflossen,", "tokens": ["Von", "Wo\u00b7gen", "des", "Le\u00b7bens", "har\u00b7mo\u00b7nisch", "um\u00b7flos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Kann Kummer sie nagen?", "tokens": ["Kann", "Kum\u00b7mer", "sie", "na\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "PPER", "VVFIN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Sie sehen den Morgen ja tagen,", "tokens": ["Sie", "se\u00b7hen", "den", "Mor\u00b7gen", "ja", "ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Im Herzen die Erde vor Liebe noch schlagen,", "tokens": ["Im", "Her\u00b7zen", "die", "Er\u00b7de", "vor", "Lie\u00b7be", "noch", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "APPR", "NN", "ADV", "VVINF", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.9": {"text": "Die ewigen Str\u00f6me von neuem ergossen.", "tokens": ["Die", "e\u00b7wi\u00b7gen", "Str\u00f6\u00b7me", "von", "neu\u00b7em", "er\u00b7gos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "VVINF", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}}}}