{"textgrid.poem.59485": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Graf Arnold und der Schiffer", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wem begegnet' je solch Wunder,", "tokens": ["Wem", "be\u00b7geg\u00b7net'", "je", "solch", "Wun\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Als Graf Arnold ist geschehn,", "tokens": ["Als", "Graf", "Ar\u00b7nold", "ist", "ge\u00b7schehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "NE", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da er am Johannesmorgen", "tokens": ["Da", "er", "am", "Jo\u00b7han\u00b7nes\u00b7mor\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wollt am Meere jagen gehn?", "tokens": ["Wollt", "am", "Mee\u00b7re", "ja\u00b7gen", "gehn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPRART", "NN", "VVINF", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Auf dem Meer ein Schifflein fahren", "tokens": ["Auf", "dem", "Meer", "ein", "Schif\u00b7flein", "fah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sah er, als ob's landen wollt,", "tokens": ["Sah", "er", ",", "als", "ob's", "lan\u00b7den", "wollt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "NE", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seiden seine Segel waren", "tokens": ["Sei\u00b7den", "sei\u00b7ne", "Se\u00b7gel", "wa\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "VAFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und das Tauwerk war von Gold.", "tokens": ["Und", "das", "Tau\u00b7werk", "war", "von", "Gold", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Fing der Schiffer da zu singen,", "tokens": ["Fing", "der", "Schif\u00b7fer", "da", "zu", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wunderbar zu singen an,", "tokens": ["Wun\u00b7der\u00b7bar", "zu", "sin\u00b7gen", "an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKZU", "VVINF", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df die Wogen leiser gingen,", "tokens": ["Da\u00df", "die", "Wo\u00b7gen", "lei\u00b7ser", "gin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wind hielt seinen Atem an;", "tokens": ["Wind", "hielt", "sei\u00b7nen", "A\u00b7tem", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Da\u00df die Fische lauschend stiegen", "tokens": ["Da\u00df", "die", "Fi\u00b7sche", "lau\u00b7schend", "stie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tief aus ihrem k\u00fchlen Haus,", "tokens": ["Tief", "aus", "ih\u00b7rem", "k\u00fch\u00b7len", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die V\u00f6gel, die da fliegen,", "tokens": ["Und", "die", "V\u00f6\u00b7gel", ",", "die", "da", "flie\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "ADV", "VVINF", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Auf dem Maste ruhten aus:", "tokens": ["Auf", "dem", "Mas\u00b7te", "ruh\u00b7ten", "aus", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "\u00bbdurch die Einsamkeit der Wogen,", "tokens": ["\u00bb", "durch", "die", "Ein\u00b7sam\u00b7keit", "der", "Wo\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schifflein, lenk dich Gottes Hand", "tokens": ["Schif\u00b7flein", ",", "lenk", "dich", "Got\u00b7tes", "Hand"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "PPER", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "An Gibraltars Felsenbogen,", "tokens": ["An", "Gib\u00b7ral\u00b7tars", "Fel\u00b7sen\u00b7bo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "An dem t\u00fcck'schen Mohrenstrand.", "tokens": ["An", "dem", "t\u00fc\u00b7ck'\u00b7schen", "Moh\u00b7ren\u00b7strand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Flandern g\u00fcrten sand'ge Banken,", "tokens": ["Flan\u00b7dern", "g\u00fcr\u00b7ten", "san\u00b7d'\u00b7ge", "Ban\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Bei Leon da steht ein Riff,", "tokens": ["Bei", "Le\u00b7on", "da", "steht", "ein", "Riff", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo schon viele Schiffe sanken,", "tokens": ["Wo", "schon", "vie\u00b7le", "Schif\u00b7fe", "san\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00fct dich Gott, mein sch\u00f6nes Schiff!\u00ab", "tokens": ["H\u00fct", "dich", "Gott", ",", "mein", "sch\u00f6\u00b7nes", "Schiff", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PPER", "NN", "$,", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "\u00bbschiffer!\u00ab rief der Graf am Strande,", "tokens": ["\u00bb", "schif\u00b7fer", "!", "\u00ab", "rief", "der", "Graf", "am", "Stran\u00b7de", ","], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$.", "$(", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00bbschiffer, lehre mich dein Lied!\u00ab \u2013", "tokens": ["\u00bb", "schif\u00b7fer", ",", "leh\u00b7re", "mich", "dein", "Lied", "!", "\u00ab", "\u2013"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "ADJD", "$,", "VVFIN", "PPER", "PPOSAT", "NN", "$.", "$(", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch der Schiffer lenkt' vom Lande:", "tokens": ["Doch", "der", "Schif\u00b7fer", "lenkt'", "vom", "Lan\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00bblehr's nur den, der mit mir zieht.\u00ab", "tokens": ["\u00bb", "lehr's", "nur", "den", ",", "der", "mit", "mir", "zieht", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PIS", "ADV", "ART", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$.", "$("], "meter": "----+-+", "measure": "unknown.measure.di"}}}}}