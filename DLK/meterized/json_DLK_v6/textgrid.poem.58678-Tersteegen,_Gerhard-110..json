{"textgrid.poem.58678": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "110.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und alsdann soll die Stadt genannt werden: \u00bbHier ist der Herr.\u00ab Hes. 48, 35", "tokens": ["Und", "als\u00b7dann", "soll", "die", "Stadt", "ge\u00b7nannt", "wer\u00b7den", ":", "\u00bb", "Hier", "ist", "der", "Herr", ".", "\u00ab", "Hes", ".", "48", ",", "35"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct", "word", "punct", "number", "punct", "number"], "pos": ["KON", "ADV", "VMFIN", "ART", "NN", "VVPP", "VAINF", "$.", "$(", "ADV", "VAFIN", "ART", "NN", "$.", "$(", "NE", "$.", "CARD", "$,", "CARD"], "meter": "-+-+-+-+---+--+", "measure": "iambic.hexa.chol"}}, "stanza.2": {"line.1": {"text": "\u00bbder Herr, der ist allda\u00ab, so nenn' ich solche Seelen,", "tokens": ["\u00bb", "der", "Herr", ",", "der", "ist", "all\u00b7da", "\u00ab", ",", "so", "nenn'", "ich", "sol\u00b7che", "See\u00b7len", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$,", "PRELS", "VAFIN", "ADV", "$(", "$,", "ADV", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die aller Kreatur als tot geworden sein,", "tokens": ["Die", "al\u00b7ler", "Kre\u00b7a\u00b7tur", "als", "tot", "ge\u00b7wor\u00b7den", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "KOKOM", "ADJD", "VAPP", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die nichts zu ihrer Lust als Gott allein erw\u00e4hlen,", "tokens": ["Die", "nichts", "zu", "ih\u00b7rer", "Lust", "als", "Gott", "al\u00b7lein", "er\u00b7w\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "APPR", "PPOSAT", "NN", "KOUS", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die manche Kreuzesprob' gemachet klein und rein.", "tokens": ["Die", "man\u00b7che", "Kreu\u00b7zes\u00b7prob'", "ge\u00b7ma\u00b7chet", "klein", "und", "rein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVPP", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So kommt Jehova dann und wohnt im Seelengrunde,", "tokens": ["So", "kommt", "Je\u00b7ho\u00b7va", "dann", "und", "wohnt", "im", "See\u00b7len\u00b7grun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "ADV", "KON", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die H\u00fctte Gottes wird alsdann den Menschen nah,", "tokens": ["Die", "H\u00fct\u00b7te", "Got\u00b7tes", "wird", "als\u00b7dann", "den", "Men\u00b7schen", "nah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ADV", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Man schauet Gott im Geist geb\u00fcckt zu aller Stunde,", "tokens": ["Man", "schau\u00b7et", "Gott", "im", "Geist", "ge\u00b7b\u00fcckt", "zu", "al\u00b7ler", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "NN", "APPRART", "NN", "VVPP", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und von dem Tage an hei\u00dft sie: \u00bbDer Herr ist da.\u00ab", "tokens": ["Und", "von", "dem", "Ta\u00b7ge", "an", "hei\u00dft", "sie", ":", "\u00bb", "Der", "Herr", "ist", "da", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "APPR", "ART", "NN", "APPR", "VVFIN", "PPER", "$.", "$(", "ART", "NN", "VAFIN", "ADV", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}