{"textgrid.poem.43864": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich leugne nicht die starcken Triebe", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich leugne nicht die starcken Triebe", "tokens": ["Ich", "leug\u00b7ne", "nicht", "die", "star\u00b7cken", "Trie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und seufze nach der Gegenliebe", "tokens": ["Und", "seuf\u00b7ze", "nach", "der", "Ge\u00b7gen\u00b7lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Sch\u00f6nheit, die mich angesteckt.", "tokens": ["Der", "Sch\u00f6n\u00b7heit", ",", "die", "mich", "an\u00b7ge\u00b7steckt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Traum entz\u00fcckt mir das Gem\u00fcthe,", "tokens": ["Der", "Traum", "ent\u00b7z\u00fcckt", "mir", "das", "Ge\u00b7m\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "So oft mir mein erregt Gebl\u00fcte", "tokens": ["So", "oft", "mir", "mein", "er\u00b7regt", "Ge\u00b7bl\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPER", "PPOSAT", "VVFIN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dein artig Bild auch blind entdeckt.", "tokens": ["Dein", "ar\u00b7tig", "Bild", "auch", "blind", "ent\u00b7deckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Allein die Ehrfurcht heist mich schweigen.", "tokens": ["Al\u00b7lein", "die", "Ehr\u00b7furcht", "heist", "mich", "schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Sclave darf die Ketten zeigen", "tokens": ["Ein", "Scla\u00b7ve", "darf", "die", "Ket\u00b7ten", "zei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und in der Noth um Rettung schreyn,", "tokens": ["Und", "in", "der", "Noth", "um", "Ret\u00b7tung", "schreyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "APPR", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nur ich mu\u00df diesen Trost entbehren", "tokens": ["Nur", "ich", "mu\u00df", "die\u00b7sen", "Trost", "ent\u00b7beh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VMFIN", "PDAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und darf den Jammer nicht erkl\u00e4ren:", "tokens": ["Und", "darf", "den", "Jam\u00b7mer", "nicht", "er\u00b7kl\u00e4\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das heist ja zweyfach elend seyn.", "tokens": ["Das", "heist", "ja", "zwey\u00b7fach", "e\u00b7lend", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Inde\u00dfen, darf der Mund nicht klagen,", "tokens": ["In\u00b7de\u00b7\u00dfen", ",", "darf", "der", "Mund", "nicht", "kla\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VMFIN", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So wird dir doch mein Auge sagen,", "tokens": ["So", "wird", "dir", "doch", "mein", "Au\u00b7ge", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wie tief mein Herz verwundet sey.", "tokens": ["Wie", "tief", "mein", "Herz", "ver\u00b7wun\u00b7det", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPOSAT", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erwege nur Gestalt und Mienen,", "tokens": ["Er\u00b7we\u00b7ge", "nur", "Ge\u00b7stalt", "und", "Mie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sie werden dir zum Zeugn\u00fc\u00df dienen:", "tokens": ["Sie", "wer\u00b7den", "dir", "zum", "Zeug\u00b7n\u00fc\u00df", "die\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ich kan und mag nicht wieder frey.", "tokens": ["Ich", "kan", "und", "mag", "nicht", "wie\u00b7der", "frey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "KON", "VMFIN", "PTKNEG", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Mich deucht, du nimmst es wohl zu Herzen.", "tokens": ["Mich", "deucht", ",", "du", "nimmst", "es", "wohl", "zu", "Her\u00b7zen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Erhalt ich das in meinen Schmerzen,", "tokens": ["Er\u00b7halt", "ich", "das", "in", "mei\u00b7nen", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df dir mein Feuer wohlgef\u00e4llt,", "tokens": ["Da\u00df", "dir", "mein", "Feu\u00b7er", "wohl\u00b7ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So will ich heimlich gerne brennen", "tokens": ["So", "will", "ich", "heim\u00b7lich", "ger\u00b7ne", "bren\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und dir sonst nichts als dies bekennen:", "tokens": ["Und", "dir", "sonst", "nichts", "als", "dies", "be\u00b7ken\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "PIS", "KOKOM", "PDS", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Du seyst die Sch\u00f6nheit dieser Welt.", "tokens": ["Du", "seyst", "die", "Sch\u00f6n\u00b7heit", "die\u00b7ser", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}