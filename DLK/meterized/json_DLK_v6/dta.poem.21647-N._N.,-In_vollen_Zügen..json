{"dta.poem.21647": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "In vollen Z\u00fcgen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "In vollen Z&#252;gen saug' ich Deinen Duft,               ", "tokens": ["In", "vol\u00b7len", "Z", "&#252;", "gen", "saug'", "ich", "Dei\u00b7nen", "Duft", ","], "token_info": ["word", "word", "word", "XML_entity", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$(", "APPR", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Erquickung spendende Gewitterluft.", "tokens": ["Er\u00b7quic\u00b7kung", "spen\u00b7den\u00b7de", "Ge\u00b7wit\u00b7ter\u00b7luft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Jagd von Blitz und Donner fuhr vorbei,", "tokens": ["Die", "Jagd", "von", "Blitz", "und", "Don\u00b7ner", "fuhr", "vor\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Gepre\u00dfte Brust, wie athmest du so frei!", "tokens": ["Ge\u00b7pre\u00df\u00b7te", "Brust", ",", "wie", "ath\u00b7mest", "du", "so", "frei", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PWAV", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Doch schwellst du auf mit wonnevollem Beben,", "tokens": ["Doch", "schwellst", "du", "auf", "mit", "won\u00b7ne\u00b7vol\u00b7lem", "Be\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Getr\u00e4nkt mit wunderbar erneutem Leben.", "tokens": ["Ge\u00b7tr\u00e4nkt", "mit", "wun\u00b7der\u00b7bar", "er\u00b7neu\u00b7tem", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Noch f\u00e4llt das Na\u00df, die Wolken ziehn dahin,", "tokens": ["Noch", "f\u00e4llt", "das", "Na\u00df", ",", "die", "Wol\u00b7ken", "ziehn", "da\u00b7hin", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ART", "NN", "VVFIN", "PAV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und mit den Wolken zieht mein fl\u00fccht\u2019ger Sinn!", "tokens": ["Und", "mit", "den", "Wol\u00b7ken", "zieht", "mein", "fl\u00fccht'\u00b7ger", "Sinn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Wer baut flughemmend jenen Wolken Schranken?", "tokens": ["Wer", "baut", "flug\u00b7hem\u00b7mend", "je\u00b7nen", "Wol\u00b7ken", "Schran\u00b7ken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADJD", "PDAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Wer setzt ein Ziel den schweifenden Gedanken?", "tokens": ["Wer", "setzt", "ein", "Ziel", "den", "schwei\u00b7fen\u00b7den", "Ge\u00b7dan\u00b7ken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Wer bannt mich fest, wer hei\u00dft mich rasten tr\u00e4g,", "tokens": ["Wer", "bannt", "mich", "fest", ",", "wer", "hei\u00dft", "mich", "ras\u00b7ten", "tr\u00e4g", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PTKVZ", "$,", "PWS", "VVFIN", "PPER", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Wer \u00e4ndert mir den selbsterw\u00e4hlten Weg?", "tokens": ["Wer", "\u00e4n\u00b7dert", "mir", "den", "selbs\u00b7ter\u00b7w\u00e4hl\u00b7ten", "Weg", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Frei ist die Bahn, und Niemand darf mich z&#252;geln,               ", "tokens": ["Frei", "ist", "die", "Bahn", ",", "und", "Nie\u00b7mand", "darf", "mich", "z", "&#252;", "geln", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "XML_entity", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$,", "KON", "PIS", "VMFIN", "PRF", "NE", "$(", "VVINF", "$,"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.14": {"text": "Ich st\u00fcrme fort auf adlerschnellen Fl\u00fcgeln.", "tokens": ["Ich", "st\u00fcr\u00b7me", "fort", "auf", "ad\u00b7ler\u00b7schnel\u00b7len", "Fl\u00fc\u00b7geln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Der Welten R\u00e4ume messe ich zur Stund\u2019,", "tokens": ["Der", "Wel\u00b7ten", "R\u00e4u\u00b7me", "mes\u00b7se", "ich", "zur", "Stund'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Von Himmelsfernen bis zum H\u00f6llenschlund,", "tokens": ["Von", "Him\u00b7mels\u00b7fer\u00b7nen", "bis", "zum", "H\u00f6l\u00b7len\u00b7schlund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Von Pol zu Pol, durch H\u00f6hen und durch Gr\u00fcnde,", "tokens": ["Von", "Pol", "zu", "Pol", ",", "durch", "H\u00f6\u00b7hen", "und", "durch", "Gr\u00fcn\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "$,", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Von Gott zu Bel, von Menschlichkeit zur S\u00fcnde.", "tokens": ["Von", "Gott", "zu", "Bel", ",", "von", "Menschlich\u00b7keit", "zur", "S\u00fcn\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NE", "$,", "APPR", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.19": {"text": "Kein ruhig Ueberwesen weckt mir Neid:", "tokens": ["Kein", "ru\u00b7hig", "Ue\u00b7ber\u00b7we\u00b7sen", "weckt", "mir", "Neid", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJD", "VVINF", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Wir sind das All', wir sind die Herrlichkeit,               ", "tokens": ["Wir", "sind", "das", "All'", ",", "wir", "sind", "die", "Herr\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Wir sind uns selbst das Ma\u00df in allen Dingen,", "tokens": ["Wir", "sind", "uns", "selbst", "das", "Ma\u00df", "in", "al\u00b7len", "Din\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ART", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Wir sind die K\u00e4mpfer, die den Sieg erringen,", "tokens": ["Wir", "sind", "die", "K\u00e4mp\u00b7fer", ",", "die", "den", "Sieg", "er\u00b7rin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "In uns\u2019rem Hirne brennt der Wahrheit Licht.", "tokens": ["In", "un\u00b7s'\u00b7rem", "Hir\u00b7ne", "brennt", "der", "Wahr\u00b7heit", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.24": {"text": "In uns\u2019rem Herzen pocht des Mitleids Pflicht.", "tokens": ["In", "un\u00b7s'\u00b7rem", "Her\u00b7zen", "pocht", "des", "Mit\u00b7leids", "Pflicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.25": {"text": "Die Liebe gl\u00fcht in ", "tokens": ["Die", "Lie\u00b7be", "gl\u00fcht", "in"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR"], "meter": "-+-+-", "measure": "iambic.di"}, "line.26": {"text": "Und nur der Mensch geht auf der Sch\u00f6nheit Spur.", "tokens": ["Und", "nur", "der", "Mensch", "geht", "auf", "der", "Sch\u00f6n\u00b7heit", "Spur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Ha, wie die Strahlen durch die Wolken schie\u00dfen!", "tokens": ["Ha", ",", "wie", "die", "Strah\u00b7len", "durch", "die", "Wol\u00b7ken", "schie\u00b7\u00dfen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.28": {"text": "Du Wurm am Wege, kannst Du\u2019s auch genie\u00dfen?", "tokens": ["Du", "Wurm", "am", "We\u00b7ge", ",", "kannst", "Du's", "auch", "ge\u00b7nie\u00b7\u00dfen", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPRART", "NN", "$,", "VMFIN", "PIS", "ADV", "VVINF", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.29": {"text": "O nein, dich freut der Kitzel Deiner Haut,", "tokens": ["O", "nein", ",", "dich", "freut", "der", "Kit\u00b7zel", "Dei\u00b7ner", "Haut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKANT", "$,", "PPER", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "Doch nicht der Geist, der schimmernd niederthaut,", "tokens": ["Doch", "nicht", "der", "Geist", ",", "der", "schim\u00b7mernd", "nie\u00b7dert\u00b7haut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ART", "NN", "$,", "PRELS", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "Wenn mit den Tropfen sich die Strahlen einen", "tokens": ["Wenn", "mit", "den", "Trop\u00b7fen", "sich", "die", "Strah\u00b7len", "ei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "PRF", "ART", "NN", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "Und lieblich l\u00e4chelnd Erd\u2019 und Himmel weinen.", "tokens": ["Und", "lieb\u00b7lich", "l\u00e4\u00b7chelnd", "Erd'", "und", "Him\u00b7mel", "wei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJD", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}