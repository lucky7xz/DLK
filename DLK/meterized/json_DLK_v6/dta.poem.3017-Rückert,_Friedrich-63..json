{"dta.poem.3017": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "63.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was jegliches Gem\u00fct als klaren Kern enth\u00e4lt,", "tokens": ["Was", "jeg\u00b7li\u00b7ches", "Ge\u00b7m\u00fct", "als", "kla\u00b7ren", "Kern", "ent\u00b7h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "NN", "KOUS", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df Gott die Wurzel und der Schl\u00fcssel ist der Welt,", "tokens": ["Da\u00df", "Gott", "die", "Wur\u00b7zel", "und", "der", "Schl\u00fcs\u00b7sel", "ist", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "KON", "ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Versucht Philosophie vielnamig zu benennen,", "tokens": ["Ver\u00b7sucht", "Phi\u00b7lo\u00b7so\u00b7phie", "viel\u00b7na\u00b7mig", "zu", "be\u00b7nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Damit die Schulen nur sich an Merkzeichen kennen.", "tokens": ["Da\u00b7mit", "die", "Schu\u00b7len", "nur", "sich", "an", "Merk\u00b7zei\u00b7chen", "ken\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "ADV", "PRF", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Unendliche Substanz, bestimmte Harmonie,", "tokens": ["Un\u00b7end\u00b7li\u00b7che", "Subs\u00b7tanz", ",", "be\u00b7stimm\u00b7te", "Har\u00b7mo\u00b7nie", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Realit\u00e4ten-Inbegriff ersinnen sie;", "tokens": ["Re\u00b7a\u00b7li\u00b7t\u00e4\u00b7ten\u00b7In\u00b7be\u00b7griff", "er\u00b7sin\u00b7nen", "sie", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Gewisheit des Gef\u00fchls, Bewu\u00dftseyns feste Grenzen,", "tokens": ["Ge\u00b7wis\u00b7heit", "des", "Ge\u00b7f\u00fchls", ",", "Be\u00b7wu\u00df\u00b7tseyns", "fes\u00b7te", "Gren\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Ich im Ich, Indifferenz der Differenzen;", "tokens": ["Das", "Ich", "im", "Ich", ",", "In\u00b7dif\u00b7fe\u00b7renz", "der", "Dif\u00b7fe\u00b7ren\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "APPRART", "PPER", "$,", "KOUS", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Selbstwerdender Begriff, und wie von Frost zu Frost", "tokens": ["Selbst\u00b7wer\u00b7den\u00b7der", "Be\u00b7griff", ",", "und", "wie", "von", "Frost", "zu", "Frost"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "KON", "PWAV", "APPR", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Namengebung steigt, ist alles ohne Trost.", "tokens": ["Die", "Na\u00b7men\u00b7ge\u00b7bung", "steigt", ",", "ist", "al\u00b7les", "oh\u00b7ne", "Trost", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "VAFIN", "PIS", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Es thut nicht noth da\u00df du Sternwarten erst erbaust,", "tokens": ["Es", "thut", "nicht", "noth", "da\u00df", "du", "Stern\u00b7war\u00b7ten", "erst", "er\u00b7baust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "NN", "KOUS", "PPER", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn du im Seelengrund den klaren Himmel schaust.", "tokens": ["Wenn", "du", "im", "See\u00b7len\u00b7grund", "den", "kla\u00b7ren", "Him\u00b7mel", "schaust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}