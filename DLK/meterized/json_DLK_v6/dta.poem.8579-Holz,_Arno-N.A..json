{"dta.poem.8579": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1899", "urn": "urn:nbn:de:kobv:b4-200905192690", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Drei kleine Strassen", "tokens": ["Drei", "klei\u00b7ne", "Stras\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["CARD", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "mit H\u00e4userchen wie aus einer Spielzeugschachtel", "tokens": ["mit", "H\u00e4u\u00b7ser\u00b7chen", "wie", "aus", "ei\u00b7ner", "Spiel\u00b7zeug\u00b7schach\u00b7tel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KOKOM", "APPR", "ART", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "m\u00fcnden auf den stillen Marktplatz.", "tokens": ["m\u00fcn\u00b7den", "auf", "den", "stil\u00b7len", "Markt\u00b7platz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Der alte Brunnen vor dem Kirchlein rauscht,", "tokens": ["Der", "al\u00b7te", "Brun\u00b7nen", "vor", "dem", "Kirch\u00b7lein", "rauscht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "die Linden duften.", "tokens": ["die", "Lin\u00b7den", "duf\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Das ist das ganze St\u00e4dtchen.", "tokens": ["Das", "ist", "das", "gan\u00b7ze", "St\u00e4dt\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Aber draussen,", "tokens": ["A\u00b7ber", "draus\u00b7sen", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "ADV", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "wo aus einem blauen, tiefen Himmel Lerchen singen,", "tokens": ["wo", "aus", "ei\u00b7nem", "blau\u00b7en", ",", "tie\u00b7fen", "Him\u00b7mel", "Ler\u00b7chen", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "ADJA", "$,", "ADJA", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.3": {"text": "blinkt der See und wogen Kornfelder.", "tokens": ["blinkt", "der", "See", "und", "wo\u00b7gen", "Korn\u00b7fel\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Mir ist Alles wie ein Traum.", "tokens": ["Mir", "ist", "Al\u00b7les", "wie", "ein", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "KOKOM", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Soll ich bleiben? Soll ich weiterziehn?", "tokens": ["Soll", "ich", "blei\u00b7ben", "?", "Soll", "ich", "wei\u00b7ter\u00b7ziehn", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$.", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Der Brunnen rauscht . . . die Linden duften.", "tokens": ["Der", "Brun\u00b7nen", "rauscht", ".", ".", ".", "die", "Lin\u00b7den", "duf\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$.", "$.", "ART", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}