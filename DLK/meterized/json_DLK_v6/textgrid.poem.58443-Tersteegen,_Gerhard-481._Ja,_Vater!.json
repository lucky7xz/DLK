{"textgrid.poem.58443": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "481. Ja, Vater!", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ohn' eigne Wahl und Widerstreben", "tokens": ["Ohn'", "eig\u00b7ne", "Wahl", "und", "Wi\u00b7der\u00b7stre\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In alles seinen Willen geben,", "tokens": ["In", "al\u00b7les", "sei\u00b7nen", "Wil\u00b7len", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Stets sagen ja und nimmer nein,", "tokens": ["Stets", "sa\u00b7gen", "ja", "und", "nim\u00b7mer", "nein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "KON", "ADV", "PTKANT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So wird man frei von aller Pein.", "tokens": ["So", "wird", "man", "frei", "von", "al\u00b7ler", "Pein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "ADJD", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vergn\u00fcgt und still nach Gottes Winken", "tokens": ["Ver\u00b7gn\u00fcgt", "und", "still", "nach", "Got\u00b7tes", "Win\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "KON", "ADJD", "APPR", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mu\u00df unser Wille stracks ersinken", "tokens": ["Mu\u00df", "un\u00b7ser", "Wil\u00b7le", "stracks", "er\u00b7sin\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPOSAT", "NN", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und ohne Forschen hei\u00dfen gut,", "tokens": ["Und", "oh\u00b7ne", "For\u00b7schen", "hei\u00b7\u00dfen", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Was Gottes Wille will und tut.", "tokens": ["Was", "Got\u00b7tes", "Wil\u00b7le", "will", "und", "tut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "NN", "VMFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}