{"textgrid.poem.65891": {"metadata": {"author": {"name": "D\u00e4ubler, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "An H\u00f6lderlin", "genre": "verse", "period": "N.A.", "pub_year": 1905, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du warst in Hellas, ehrf\u00fcrchtiger Dichter,", "tokens": ["Du", "warst", "in", "Hel\u00b7las", ",", "ehr\u00b7f\u00fcrch\u00b7ti\u00b7ger", "Dich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NE", "$,", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Nur zagte vor Athenas Land der Fu\u00df;", "tokens": ["Nur", "zag\u00b7te", "vor", "A\u00b7the\u00b7nas", "Land", "der", "Fu\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NE", "NN", "ART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Du fl\u00fcgeltest wie Hermes, sachten Schuhs,", "tokens": ["Du", "fl\u00fc\u00b7gel\u00b7test", "wie", "Her\u00b7mes", ",", "sach\u00b7ten", "Schuhs", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "NE", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Als heimlich hochgesichteter Beschwichter", "tokens": ["Als", "heim\u00b7lich", "hoch\u00b7ge\u00b7sich\u00b7te\u00b7ter", "Be\u00b7schwich\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Von Sonnumfangenheit durch Abendlichter:", "tokens": ["Von", "Son\u00b7num\u00b7fan\u00b7gen\u00b7heit", "durch", "A\u00b7bend\u00b7lich\u00b7ter", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dein Atem fand sich Steile eines Nu's \u2013", "tokens": ["Dein", "A\u00b7tem", "fand", "sich", "Stei\u00b7le", "ei\u00b7nes", "Nu's", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "NN", "ART", "NE", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Umzagtem Wunder nahtest du: ich tu's \u2013", "tokens": ["Um\u00b7zag\u00b7tem", "Wun\u00b7der", "nah\u00b7test", "du", ":", "ich", "tu's", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "$.", "PPER", "NE", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und urgebannt bestimmten uns Gesichter.", "tokens": ["Und", "ur\u00b7ge\u00b7bannt", "be\u00b7stimm\u00b7ten", "uns", "Ge\u00b7sich\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wo staunten die erbittrungsfernen Helden,", "tokens": ["Wo", "staun\u00b7ten", "die", "er\u00b7bit\u00b7trungs\u00b7fer\u00b7nen", "Hel\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df sie ein blutendes Geschlecht erblickt?", "tokens": ["Da\u00df", "sie", "ein", "blu\u00b7ten\u00b7des", "Ge\u00b7schlecht", "er\u00b7blickt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Gedichtete, ihr habt uns hergenickt!", "tokens": ["Ge\u00b7dich\u00b7te\u00b7te", ",", "ihr", "habt", "uns", "her\u00b7ge\u00b7nickt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Umwolkte G\u00f6tter, deren Namen melden,", "tokens": ["Um\u00b7wolk\u00b7te", "G\u00f6t\u00b7ter", ",", "de\u00b7ren", "Na\u00b7men", "mel\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie weilten, nie erreicht, auf fahlem Firn,", "tokens": ["Sie", "weil\u00b7ten", ",", "nie", "er\u00b7reicht", ",", "auf", "fah\u00b7lem", "Firn", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADV", "VVPP", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Gewitterten um kl\u00e4rungssichre Stirn.", "tokens": ["Ge\u00b7wit\u00b7ter\u00b7ten", "um", "kl\u00e4\u00b7rungs\u00b7sich\u00b7re", "Stirn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}