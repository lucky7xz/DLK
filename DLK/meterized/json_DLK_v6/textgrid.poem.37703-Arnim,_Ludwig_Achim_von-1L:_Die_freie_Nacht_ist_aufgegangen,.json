{"textgrid.poem.37703": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die freie Nacht ist aufgegangen,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die freie Nacht ist aufgegangen,", "tokens": ["Die", "frei\u00b7e", "Nacht", "ist", "auf\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Unsichtbar wird ein Mensch dem andern,", "tokens": ["Un\u00b7sicht\u00b7bar", "wird", "ein", "Mensch", "dem", "an\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So kann ich mit den Thr\u00e4nen prangen", "tokens": ["So", "kann", "ich", "mit", "den", "Thr\u00e4\u00b7nen", "pran\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und hin zu Liebchens Fenster wandern.", "tokens": ["Und", "hin", "zu", "Lieb\u00b7chens", "Fens\u00b7ter", "wan\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der W\u00e4chter rufet seine Stunden,", "tokens": ["Der", "W\u00e4ch\u00b7ter", "ru\u00b7fet", "sei\u00b7ne", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Kranke jammert seine Schmerzen,", "tokens": ["Der", "Kran\u00b7ke", "jam\u00b7mert", "sei\u00b7ne", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die Liebe klaget ihre Wunden,", "tokens": ["Die", "Lie\u00b7be", "kla\u00b7get", "ih\u00b7re", "Wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und bei der Leiche schimmern Kerzen.", "tokens": ["Und", "bei", "der", "Lei\u00b7che", "schim\u00b7mern", "Ker\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Liebste ist mir heut gestorben,", "tokens": ["Die", "Liebs\u00b7te", "ist", "mir", "heut", "ge\u00b7stor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wo sie dem Feinde sich verm\u00e4hlet,", "tokens": ["Wo", "sie", "dem", "Fein\u00b7de", "sich", "ver\u00b7m\u00e4h\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich habe Lieb' in Leid geborgen,", "tokens": ["Ich", "ha\u00b7be", "Lieb'", "in", "Leid", "ge\u00b7bor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr Thr\u00e4nen mir die Sterne z\u00e4hlet.", "tokens": ["Ihr", "Thr\u00e4\u00b7nen", "mir", "die", "Ster\u00b7ne", "z\u00e4h\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wie herzhaft ist das Licht der Sterne,", "tokens": ["Wie", "herz\u00b7haft", "ist", "das", "Licht", "der", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wie schmerzhaft ist das Licht der Fenster,", "tokens": ["Wie", "schmerz\u00b7haft", "ist", "das", "Licht", "der", "Fens\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ein dichter Nebel deckt die Ferne,", "tokens": ["Ein", "dich\u00b7ter", "Ne\u00b7bel", "deckt", "die", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und mich umspinnen die Gespenster.", "tokens": ["Und", "mich", "um\u00b7spin\u00b7nen", "die", "Ge\u00b7spens\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Im Hause ist ein wildes Klingen.", "tokens": ["Im", "Hau\u00b7se", "ist", "ein", "wil\u00b7des", "Klin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Menschen mir so still ausweichen,", "tokens": ["Die", "Men\u00b7schen", "mir", "so", "still", "aus\u00b7wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "In Mitleid mich dann fern umringen:", "tokens": ["In", "Mit\u00b7leid", "mich", "dann", "fern", "um\u00b7rin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So bin ich auch von eures Gleichen?", "tokens": ["So", "bin", "ich", "auch", "von", "eu\u00b7res", "Glei\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Mich hielt der Wald bei Tag verborgen", "tokens": ["Mich", "hielt", "der", "Wald", "bei", "Tag", "ver\u00b7bor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die schwarze Nacht hat mich befreiet.", "tokens": ["Die", "schwar\u00b7ze", "Nacht", "hat", "mich", "be\u00b7frei\u00b7et", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Mein Liebchen weckt ein sch\u00f6ner Morgen,", "tokens": ["Mein", "Lieb\u00b7chen", "weckt", "ein", "sch\u00f6\u00b7ner", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der mich dem ew'gen Jammer weihet.", "tokens": ["Der", "mich", "dem", "ew'\u00b7gen", "Jam\u00b7mer", "wei\u00b7het", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wie oft hab' ich hier froh gesessen,", "tokens": ["Wie", "oft", "hab'", "ich", "hier", "froh", "ge\u00b7ses\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "PPER", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn alle Sterne im Erblassen,", "tokens": ["Wenn", "al\u00b7le", "Ster\u00b7ne", "im", "Er\u00b7blas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ach alle Welt hat mich vergessen,", "tokens": ["Ach", "al\u00b7le", "Welt", "hat", "mich", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PIAT", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Seit mich die Liebste hat verlassen:", "tokens": ["Seit", "mich", "die", "Liebs\u00b7te", "hat", "ver\u00b7las\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Nichts wei\u00df von mir die gr\u00fcne Erde,", "tokens": ["Nichts", "wei\u00df", "von", "mir", "die", "gr\u00fc\u00b7ne", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Nichts wei\u00df von mir die lichte Sonne,", "tokens": ["Nichts", "wei\u00df", "von", "mir", "die", "lich\u00b7te", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Der Mondenglanz ist mir Beschwerde,", "tokens": ["Der", "Mon\u00b7den\u00b7glanz", "ist", "mir", "Be\u00b7schwer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die Nacht ist meiner Thr\u00e4nen Bronne.", "tokens": ["Die", "Nacht", "ist", "mei\u00b7ner", "Thr\u00e4\u00b7nen", "Bron\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}