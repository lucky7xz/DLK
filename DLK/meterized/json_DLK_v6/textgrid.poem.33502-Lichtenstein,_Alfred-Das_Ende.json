{"textgrid.poem.33502": {"metadata": {"author": {"name": "Lichtenstein, Alfred", "birth": "N.A.", "death": "N.A."}, "title": "Das Ende", "genre": "verse", "period": "N.A.", "pub_year": 1914, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Windklo\u00df \u00fcberzieht wie wei\u00dfer Schwamm", "tokens": ["Ein", "Wind\u00b7klo\u00df", "\u00fc\u00b7berz\u00b7ieht", "wie", "wei\u00b7\u00dfer", "Schwamm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "KOKOM", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die gr\u00fcne Leiche der verlornen Welt.", "tokens": ["Die", "gr\u00fc\u00b7ne", "Lei\u00b7che", "der", "ver\u00b7lor\u00b7nen", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Erfrorne Fl\u00fcsse sind ein Eisendamm,", "tokens": ["Er\u00b7fror\u00b7ne", "Fl\u00fcs\u00b7se", "sind", "ein", "Ei\u00b7sen\u00b7damm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der morsche Reste noch zusammenh\u00e4lt.", "tokens": ["Der", "mor\u00b7sche", "Res\u00b7te", "noch", "zu\u00b7sam\u00b7men\u00b7h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "In einer kleinen Regenecke steht", "tokens": ["In", "ei\u00b7ner", "klei\u00b7nen", "Re\u00b7gen\u00b7e\u00b7cke", "steht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die letzte Stadt in steinerner Geduld.", "tokens": ["Die", "letz\u00b7te", "Stadt", "in", "stei\u00b7ner\u00b7ner", "Ge\u00b7duld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein toter Sch\u00e4del liegt \u2013 wie ein Gebet \u2013", "tokens": ["Ein", "to\u00b7ter", "Sch\u00e4\u00b7del", "liegt", "\u2013", "wie", "ein", "Ge\u00b7bet", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$(", "KOKOM", "ART", "NN", "$("], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Schief auf dem Leib, dem schwarzen B\u00fc\u00dferpult.", "tokens": ["Schief", "auf", "dem", "Leib", ",", "dem", "schwar\u00b7zen", "B\u00fc\u00b7\u00dfer\u00b7pult", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}}}}