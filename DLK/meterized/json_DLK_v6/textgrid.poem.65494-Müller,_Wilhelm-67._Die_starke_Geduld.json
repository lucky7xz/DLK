{"textgrid.poem.65494": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "67. Die starke Geduld", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bedenke, Freund, da\u00df die Geduld nie bei der Schwachheit steht.", "tokens": ["Be\u00b7den\u00b7ke", ",", "Freund", ",", "da\u00df", "die", "Ge\u00b7duld", "nie", "bei", "der", "Schwach\u00b7heit", "steht", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "KOUS", "ART", "NN", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Das M\u00fchlenrad, des Baches Spiel, mit Saus und Braus sich dreht,", "tokens": ["Das", "M\u00fch\u00b7len\u00b7rad", ",", "des", "Ba\u00b7ches", "Spiel", ",", "mit", "Saus", "und", "Braus", "sich", "dreht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "$,", "APPR", "NN", "KON", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Inde\u00df die Sonne ruhig stets die ew'ge Bahn durchkreist,", "tokens": ["In\u00b7de\u00df", "die", "Son\u00b7ne", "ru\u00b7hig", "stets", "die", "ew'\u00b7ge", "Bahn", "durch\u00b7kreist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJD", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Wie toll und arg die Erdenwelt sich unter ihr erweist.", "tokens": ["Wie", "toll", "und", "arg", "die", "Er\u00b7den\u00b7welt", "sich", "un\u00b7ter", "ihr", "er\u00b7weist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "KON", "ADJD", "ART", "NN", "PRF", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}}}}}