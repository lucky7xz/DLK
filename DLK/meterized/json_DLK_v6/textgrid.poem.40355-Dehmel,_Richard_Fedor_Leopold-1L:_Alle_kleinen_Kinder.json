{"textgrid.poem.40355": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "1L: Alle kleinen Kinder", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Alle kleinen Kinder", "tokens": ["Al\u00b7le", "klei\u00b7nen", "Kin\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "schrein Hurrah, Hurrah.", "tokens": ["schrein", "Hur\u00b7rah", ",", "Hur\u00b7rah", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Mutterchen liegt still zu Bett,", "tokens": ["Mut\u00b7ter\u00b7chen", "liegt", "still", "zu", "Bett", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "APPR", "NN", "$,"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.4": {"text": "Kindchen schreit Hurrah.", "tokens": ["Kind\u00b7chen", "schreit", "Hur\u00b7rah", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Vater steht daneben,", "tokens": ["Va\u00b7ter", "steht", "da\u00b7ne\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PAV", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "steht und brummt: ja ja,", "tokens": ["steht", "und", "brummt", ":", "ja", "ja", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "$.", "ADV", "ADV", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "ist ein schweres Leben.", "tokens": ["ist", "ein", "schwe\u00b7res", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Kindchen schreit Hurrah.", "tokens": ["Kind\u00b7chen", "schreit", "Hur\u00b7rah", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Mutterchen brummt garnicht,", "tokens": ["Mut\u00b7ter\u00b7chen", "brummt", "gar\u00b7nicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "VVFIN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "selig liegt sie da.", "tokens": ["se\u00b7lig", "liegt", "sie", "da", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Denn das kleine Menschenkind", "tokens": ["Denn", "das", "klei\u00b7ne", "Men\u00b7schen\u00b7kind"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "schreit Hurrah, Hurrah.", "tokens": ["schreit", "Hur\u00b7rah", ",", "Hur\u00b7rah", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJD", "NN", "$,", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}