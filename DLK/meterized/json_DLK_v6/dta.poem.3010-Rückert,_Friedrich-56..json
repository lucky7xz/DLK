{"dta.poem.3010": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "56.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es nutzt nicht da\u00df du rein und klar wie Wasser seist,", "tokens": ["Es", "nutzt", "nicht", "da\u00df", "du", "rein", "und", "klar", "wie", "Was\u00b7ser", "seist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "KOUS", "PPER", "ADJD", "KON", "ADJD", "KOKOM", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn dich dem Wasser gleich treibt ein unruhiger Geist.", "tokens": ["Wenn", "dich", "dem", "Was\u00b7ser", "gleich", "treibt", "ein", "un\u00b7ru\u00b7hi\u00b7ger", "Geist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Du must von keinem Sturm auch lassen dich aufwiegeln", "tokens": ["Du", "must", "von", "kei\u00b7nem", "Sturm", "auch", "las\u00b7sen", "dich", "auf\u00b7wie\u00b7geln"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "PIAT", "NN", "ADV", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn du den Himmel willst in glatter Fl\u00e4che spiegeln.", "tokens": ["Wenn", "du", "den", "Him\u00b7mel", "willst", "in", "glat\u00b7ter", "Fl\u00e4\u00b7che", "spie\u00b7geln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VMFIN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Das Wasser hat nicht Kraft dem Sturm zu widerstreben,", "tokens": ["Das", "Was\u00b7ser", "hat", "nicht", "Kraft", "dem", "Sturm", "zu", "wi\u00b7der\u00b7stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du aber, wenn du willst, kanst ruhig seyn und eben.", "tokens": ["Du", "a\u00b7ber", ",", "wenn", "du", "willst", ",", "kanst", "ru\u00b7hig", "seyn", "und", "e\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "KOUS", "PPER", "VMFIN", "$,", "VMFIN", "ADJD", "VAINF", "KON", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}