{"textgrid.poem.56823": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich m\u00fc\u00dft' es malen, solltet ihr sie sehen,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich m\u00fc\u00dft' es malen, solltet ihr sie sehen,", "tokens": ["Ich", "m\u00fc\u00dft'", "es", "ma\u00b7len", ",", "soll\u00b7tet", "ihr", "sie", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVINF", "$,", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "wie ich sie sah, die beiden schwarzen Schwestern \u2013:", "tokens": ["wie", "ich", "sie", "sah", ",", "die", "bei\u00b7den", "schwar\u00b7zen", "Schwes\u00b7tern", "\u2013", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "PPER", "PPER", "VVFIN", "$,", "PRELS", "PIAT", "ADJA", "NN", "$(", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Allein sich glaubend im beschneiten Walde,", "tokens": ["Al\u00b7lein", "sich", "glau\u00b7bend", "im", "be\u00b7schnei\u00b7ten", "Wal\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "der Jugend s\u00fc\u00dfe Ungeduld nicht z\u00fcgelnd,", "tokens": ["der", "Ju\u00b7gend", "s\u00fc\u00b7\u00dfe", "Un\u00b7ge\u00b7duld", "nicht", "z\u00fc\u00b7gelnd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "mit einem Male Menschen, M\u00e4dchen, Kinder.", "tokens": ["mit", "ei\u00b7nem", "Ma\u00b7le", "Men\u00b7schen", ",", "M\u00e4d\u00b7chen", ",", "Kin\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die Kleider flogen um die leichten F\u00fc\u00dfe,", "tokens": ["Die", "Klei\u00b7der", "flo\u00b7gen", "um", "die", "leich\u00b7ten", "F\u00fc\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "die H\u00fcften wiegten sich, und jubelnd jagten", "tokens": ["die", "H\u00fcf\u00b7ten", "wieg\u00b7ten", "sich", ",", "und", "ju\u00b7belnd", "jag\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "$,", "KON", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "sie sich mit wei\u00dfen B\u00e4llen durch die B\u00e4ume ...", "tokens": ["sie", "sich", "mit", "wei\u00b7\u00dfen", "B\u00e4l\u00b7len", "durch", "die", "B\u00e4u\u00b7me", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "APPR", "ADJA", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Ein schwerer Ast begrub sie fast in Flocken ...", "tokens": ["Ein", "schwe\u00b7rer", "Ast", "be\u00b7grub", "sie", "fast", "in", "Flo\u00b7cken", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADV", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Ein Reh erschreckte sie, \u2013 und wie des Schreckens", "tokens": ["Ein", "Reh", "er\u00b7schreck\u00b7te", "sie", ",", "\u2013", "und", "wie", "des", "Schre\u00b7ckens"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "$(", "KON", "PWAV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "sich sch\u00e4mend, klatschten toll sie in die H\u00e4nde ...", "tokens": ["sich", "sch\u00e4\u00b7mend", ",", "klatschten", "toll", "sie", "in", "die", "H\u00e4n\u00b7de", "..."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "$,", "VVFIN", "ADJD", "PPER", "APPR", "ART", "NN", "$("], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Dann stellten sie sich pl\u00f6tzlich gegen\u00fcber", "tokens": ["Dann", "stell\u00b7ten", "sie", "sich", "pl\u00f6tz\u00b7lich", "ge\u00b7gen\u00b7\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADJD", "APPR"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "und ma\u00dfen ihre Kraft, die offnen Finger", "tokens": ["und", "ma\u00b7\u00dfen", "ih\u00b7re", "Kraft", ",", "die", "off\u00b7nen", "Fin\u00b7ger"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "verstrickend, bis die eine lachend kniete ...", "tokens": ["ver\u00b7stri\u00b7ckend", ",", "bis", "die", "ei\u00b7ne", "la\u00b7chend", "knie\u00b7te", "..."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KOUS", "ART", "ART", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Und fort und fort so heitre Kurzweil treibend,", "tokens": ["Und", "fort", "und", "fort", "so", "heit\u00b7re", "Kurz\u00b7weil", "trei\u00b7bend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKVZ", "KON", "PTKVZ", "ADV", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "entschwanden sie dem nicht geahnten Sp\u00e4her,", "tokens": ["ent\u00b7schwan\u00b7den", "sie", "dem", "nicht", "ge\u00b7ahn\u00b7ten", "Sp\u00e4\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "PTKNEG", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "bis selbst die Stimmen, heller Lieder selig,", "tokens": ["bis", "selbst", "die", "Stim\u00b7men", ",", "hel\u00b7ler", "Lie\u00b7der", "se\u00b7lig", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$,", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "im Winterwald sich endlich fern verloren.", "tokens": ["im", "Win\u00b7ter\u00b7wald", "sich", "end\u00b7lich", "fern", "ver\u00b7lo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PRF", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}