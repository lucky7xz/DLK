{"textgrid.poem.58382": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "420. Das Kreuz ist Last, solang man's ha\u00dft", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Kreuz ist dir, wie du dem Kreuze bist:", "tokens": ["Das", "Kreuz", "ist", "dir", ",", "wie", "du", "dem", "Kreu\u00b7ze", "bist", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "$,", "PWAV", "PPER", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Bist du ihm feind, es dir zuwider ist,", "tokens": ["Bist", "du", "ihm", "feind", ",", "es", "dir", "zu\u00b7wi\u00b7der", "ist", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "NN", "$,", "PPER", "PPER", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch wenn man 's Kreuz umfa\u00dft und liebet,", "tokens": ["Doch", "wenn", "man", "'s", "Kreuz", "um\u00b7fa\u00dft", "und", "lie\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "PPER", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Das Kreuz uns Gott und alle Gnade giebet.", "tokens": ["Das", "Kreuz", "uns", "Gott", "und", "al\u00b7le", "Gna\u00b7de", "gie\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "NN", "KON", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}