{"textgrid.poem.43371": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: L\u00fcg' und Widerspruch, wo w\u00e4re", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "L\u00fcg' und Widerspruch, wo w\u00e4re", "tokens": ["L\u00fcg'", "und", "Wi\u00b7der\u00b7spruch", ",", "wo", "w\u00e4\u00b7re"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "KON", "NN", "$,", "PWAV", "VAFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eins von diesen beiden nicht,", "tokens": ["Eins", "von", "die\u00b7sen", "bei\u00b7den", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PDAT", "PIAT", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Im Civil und Milit\u00e4re,", "tokens": ["Im", "Ci\u00b7vil", "und", "Mi\u00b7li\u00b7t\u00e4\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "In Regierung und Gericht?", "tokens": ["In", "Re\u00b7gie\u00b7rung", "und", "Ge\u00b7richt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "L\u00fcg' und Widerspruch, die beiden", "tokens": ["L\u00fcg'", "und", "Wi\u00b7der\u00b7spruch", ",", "die", "bei\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "KON", "NN", "$,", "PRELS", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sind wie Br\u00e4utigam und Braut,", "tokens": ["Sind", "wie", "Br\u00e4u\u00b7ti\u00b7gam", "und", "Braut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "NE", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und zu manches Landes Leiden", "tokens": ["Und", "zu", "man\u00b7ches", "Lan\u00b7des", "Lei\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hat der Teufel sie getraut.", "tokens": ["Hat", "der", "Teu\u00b7fel", "sie", "ge\u00b7traut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}