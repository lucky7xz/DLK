{"textgrid.poem.29454": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "69.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "W\u00fcrmer im Gewissen,", "tokens": ["W\u00fcr\u00b7mer", "im", "Ge\u00b7wis\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Kleider wol zerrissen,", "tokens": ["Klei\u00b7der", "wol", "zer\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wolbenarbte Leiber,", "tokens": ["Wol\u00b7ben\u00b7arb\u00b7te", "Lei\u00b7ber", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Wolgebrauchte Weiber,", "tokens": ["Wol\u00b7ge\u00b7brauch\u00b7te", "Wei\u00b7ber", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Ungewisse Kinder,", "tokens": ["Un\u00b7ge\u00b7wis\u00b7se", "Kin\u00b7der", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Weder Pferd noch Rinder,", "tokens": ["We\u00b7der", "Pferd", "noch", "Rin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Nimmer Brot im Sacke,", "tokens": ["Nim\u00b7mer", "Brot", "im", "Sa\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "Nimmer Geld im Packe", "tokens": ["Nim\u00b7mer", "Geld", "im", "Pa\u00b7cke"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "NN", "APPRART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "Haben mit genummen,", "tokens": ["Ha\u00b7ben", "mit", "ge\u00b7num\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.10": {"text": "Die vom Kriege kummen.", "tokens": ["Die", "vom", "Krie\u00b7ge", "kum\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.11": {"text": "Wer dann hat die Beute?", "tokens": ["Wer", "dann", "hat", "die", "Beu\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VAFIN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.12": {"text": "Eitel fremde Leute.", "tokens": ["Ei\u00b7tel", "frem\u00b7de", "Leu\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}