{"textgrid.poem.44713": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Drum dum, dum dum,", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Drum dum, dum dum,", "tokens": ["Drum", "dum", ",", "dum", "dum", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "PTKVZ", "$,", "FM.la", "FM.la", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Wir gehen herum,", "tokens": ["Wir", "ge\u00b7hen", "he\u00b7rum", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Mit Horn und Cimbel mannigfalt,", "tokens": ["Mit", "Horn", "und", "Cim\u00b7bel", "man\u00b7nig\u00b7falt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hochauf musizieren wir der Gewalt.", "tokens": ["Hoch\u00b7auf", "mu\u00b7si\u00b7zie\u00b7ren", "wir", "der", "Ge\u00b7walt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ART", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und schl\u00e4gts mal um,", "tokens": ["Und", "schl\u00e4g\u00b7ts", "mal", "um", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Drum dum, dum dum,", "tokens": ["Drum", "dum", ",", "dum", "dum", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "PTKVZ", "$,", "FM.la", "FM.la", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Was drauf folgt, findet uns auch nicht stumm.", "tokens": ["Was", "drauf", "folgt", ",", "fin\u00b7det", "uns", "auch", "nicht", "stumm", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PAV", "VVFIN", "$,", "VVFIN", "PPER", "ADV", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}}}}