{"textgrid.poem.41180": {"metadata": {"author": {"name": "Denis, Michael", "birth": "N.A.", "death": "N.A."}, "title": "Erster Traum", "genre": "verse", "period": "N.A.", "pub_year": 1764, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tochter des sch\u00f6nsten Rosenstockes! Einsam", "tokens": ["Toch\u00b7ter", "des", "sch\u00f6ns\u00b7ten", "Ro\u00b7sen\u00b7sto\u00b7ckes", "!", "Ein\u00b7sam"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["NN", "ART", "ADJA", "NN", "$.", "ART"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Stehst du noch immer. Deine Schwestern alle", "tokens": ["Stehst", "du", "noch", "im\u00b7mer", ".", "Dei\u00b7ne", "Schwes\u00b7tern", "al\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "$.", "PPOSAT", "NN", "PIAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Gl\u00e4nzen und duften lange schon um edler", "tokens": ["Gl\u00e4n\u00b7zen", "und", "duf\u00b7ten", "lan\u00b7ge", "schon", "um", "ed\u00b7ler"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "VVFIN", "ADV", "ADV", "APPR", "ADJA"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "J\u00fcnglinge Scheitel.", "tokens": ["J\u00fcng\u00b7lin\u00b7ge", "Schei\u00b7tel", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Wandelt der Sohn des Liedes dich vor\u00fcber", "tokens": ["Wan\u00b7delt", "der", "Sohn", "des", "Lie\u00b7des", "dich", "vor\u00b7\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "PPER", "PTKVZ"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Unter dem Abendwinde, dann umwallt ihn", "tokens": ["Un\u00b7ter", "dem", "Ab\u00b7end\u00b7win\u00b7de", ",", "dann", "um\u00b7wallt", "ihn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "ADV", "VVFIN", "PPER"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Deines Geruches s\u00fc\u00dfe Fluth. Er seufzet:", "tokens": ["Dei\u00b7nes", "Ge\u00b7ru\u00b7ches", "s\u00fc\u00b7\u00dfe", "Fluth", ".", "Er", "seuf\u00b7zet", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "ADJA", "NN", "$.", "PPER", "VVFIN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Immer noch einsam!", "tokens": ["Im\u00b7mer", "noch", "ein\u00b7sam", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Aber um dich her schrecken unvers\u00f6hnte", "tokens": ["A\u00b7ber", "um", "dich", "her", "schre\u00b7cken", "un\u00b7ver\u00b7s\u00f6hn\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUI", "PRF", "APZR", "VVFIN", "ADJA"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "D\u00f6rner, um dich her windet sich ein hoher", "tokens": ["D\u00f6r\u00b7ner", ",", "um", "dich", "her", "win\u00b7det", "sich", "ein", "ho\u00b7her"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "KOUI", "PRF", "APZR", "VVFIN", "PRF", "ART", "ADJA"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Dreimal geflochtner Zaun, und g\u00f6nnt dem Auge", "tokens": ["Drei\u00b7mal", "ge\u00b7flocht\u00b7ner", "Zaun", ",", "und", "g\u00f6nnt", "dem", "Au\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "$,", "KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Kaum dich zu sehen.", "tokens": ["Kaum", "dich", "zu", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Blick' ich durch seine Kl\u00fcfte, dann entdeck' ich,", "tokens": ["Blick'", "ich", "durch", "sei\u00b7ne", "Kl\u00fcf\u00b7te", ",", "dann", "ent\u00b7deck'", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,", "ADV", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Holde! dein Streben. Hielte dich dein Stengel", "tokens": ["Hol\u00b7de", "!", "dein", "Stre\u00b7ben", ".", "Hiel\u00b7te", "dich", "dein", "Sten\u00b7gel"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "PPOSAT", "NN", "$.", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Minder, du trotztest deiner H\u00fcrde, fl\u00f6gest", "tokens": ["Min\u00b7der", ",", "du", "trotz\u00b7test", "dei\u00b7ner", "H\u00fcr\u00b7de", ",", "fl\u00f6\u00b7gest"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["NN", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Feurig her\u00fcber.", "tokens": ["Feu\u00b7rig", "her\u00b7\u00fc\u00b7ber", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "ADV", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "Freudig erh\u00fcbe deinen Flug der Barde.", "tokens": ["Freu\u00b7dig", "er\u00b7h\u00fc\u00b7be", "dei\u00b7nen", "Flug", "der", "Bar\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Freudig erkl\u00e4ngen seine Feiersaiten:", "tokens": ["Freu\u00b7dig", "er\u00b7kl\u00e4n\u00b7gen", "sei\u00b7ne", "Fei\u00b7er\u00b7sai\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Heil dem begl\u00fcckten Erdesohn', in dessen", "tokens": ["Heil", "dem", "be\u00b7gl\u00fcck\u00b7ten", "Er\u00b7de\u00b7sohn'", ",", "in", "des\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN", "$,", "APPR", "PRELAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "H\u00e4nde du s\u00e4nkest.", "tokens": ["H\u00e4n\u00b7de", "du", "s\u00e4n\u00b7kest", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVFIN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.6": {"line.1": {"text": "Wonnevoll h\u00fcpften Fluren dir entgegen,", "tokens": ["Won\u00b7ne\u00b7voll", "h\u00fcpf\u00b7ten", "Flu\u00b7ren", "dir", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "PPER", "PTKVZ", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Trunken von Hoffnung, sich mit Erben deiner", "tokens": ["Trun\u00b7ken", "von", "Hoff\u00b7nung", ",", "sich", "mit", "Er\u00b7ben", "dei\u00b7ner"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "$,", "PRF", "APPR", "NN", "PPOSAT"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Farbe zu kleiden, sich mit Erben deiner", "tokens": ["Far\u00b7be", "zu", "klei\u00b7den", ",", "sich", "mit", "Er\u00b7ben", "dei\u00b7ner"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PTKZU", "VVINF", "$,", "PRF", "APPR", "NN", "PPOSAT"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "D\u00fcfte zu kleiden.", "tokens": ["D\u00fcf\u00b7te", "zu", "klei\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.7": {"line.1": {"text": "Tochter des sch\u00f6nsten Rosenstockes! Aengstig", "tokens": ["Toch\u00b7ter", "des", "sch\u00f6ns\u00b7ten", "Ro\u00b7sen\u00b7sto\u00b7ckes", "!", "A\u00b7engs\u00b7tig"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["NN", "ART", "ADJA", "NN", "$.", "NE"], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Ist mir um dich die Seele. Deine milden", "tokens": ["Ist", "mir", "um", "dich", "die", "See\u00b7le", ".", "Dei\u00b7ne", "mil\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "PRF", "ART", "NN", "$.", "PPOSAT", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sonnen verblinken, und die Morgen hauchen", "tokens": ["Son\u00b7nen", "ver\u00b7blin\u00b7ken", ",", "und", "die", "Mor\u00b7gen", "hau\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVINF", "$,", "KON", "ART", "NN", "VVFIN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "K\u00e4lter, und Reif dr\u00e4ut.", "tokens": ["K\u00e4l\u00b7ter", ",", "und", "Reif", "dr\u00e4ut", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "NN", "VVFIN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.8": {"line.1": {"text": "F\u00e4llt er, und welken deine Bl\u00e4tter, o dann", "tokens": ["F\u00e4llt", "er", ",", "und", "wel\u00b7ken", "dei\u00b7ne", "Bl\u00e4t\u00b7ter", ",", "o", "dann"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "KON", "PWAT", "PPOSAT", "NN", "$,", "FM", "ADV"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Bleibet dir noch ein Trost beschieden. Einstens", "tokens": ["Blei\u00b7bet", "dir", "noch", "ein", "Trost", "be\u00b7schie\u00b7den", ".", "Eins\u00b7tens"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "VVPP", "$.", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sieht dich mein Aug' in seligern Gefilden", "tokens": ["Sieht", "dich", "mein", "Aug'", "in", "se\u00b7li\u00b7gern", "Ge\u00b7fil\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Herrlicher aufbl\u00fch'n.", "tokens": ["Herr\u00b7li\u00b7cher", "auf\u00b7bl\u00fch'", "n."], "token_info": ["word", "word", "abbreviation"], "pos": ["FM.la", "FM.la", "FM.la"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}