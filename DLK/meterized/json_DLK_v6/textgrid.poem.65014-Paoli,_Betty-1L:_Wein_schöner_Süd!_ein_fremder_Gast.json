{"textgrid.poem.65014": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wein sch\u00f6ner S\u00fcd! ein fremder Gast", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wein sch\u00f6ner S\u00fcd! ein fremder Gast", "tokens": ["Wein", "sch\u00f6\u00b7ner", "S\u00fcd", "!", "ein", "frem\u00b7der", "Gast"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ADJA", "NN", "$.", "ART", "ADJA", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Bin ich zu dir gekommen,", "tokens": ["Bin", "ich", "zu", "dir", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Allein mit Freundesarmen hast", "tokens": ["Al\u00b7lein", "mit", "Freun\u00b7des\u00b7ar\u00b7men", "hast"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du mild mich aufgenommen.", "tokens": ["Du", "mild", "mich", "auf\u00b7ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Je l\u00e4nger ich bei dir verweilt", "tokens": ["Je", "l\u00e4n\u00b7ger", "ich", "bei", "dir", "ver\u00b7weilt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "PPER", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So heller ward mein Sinnen,", "tokens": ["So", "hel\u00b7ler", "ward", "mein", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du hast die wunde Brust geheilt", "tokens": ["Du", "hast", "die", "wun\u00b7de", "Brust", "ge\u00b7heilt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und alle Schmerzen drinnen.", "tokens": ["Und", "al\u00b7le", "Schmer\u00b7zen", "drin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Aus ihrer dumpfen Grabesruh", "tokens": ["Aus", "ih\u00b7rer", "dum\u00b7pfen", "Gra\u00b7be\u00b7sruh"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erwecktest du die Lieder", "tokens": ["Er\u00b7weck\u00b7test", "du", "die", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und als ich schied, riefst du mir zu:", "tokens": ["Und", "als", "ich", "schied", ",", "riefst", "du", "mir", "zu", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u00bbo kehre baldigst wieder!\u00ab", "tokens": ["\u00bb", "o", "keh\u00b7re", "bal\u00b7digst", "wie\u00b7der", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "FM", "VVFIN", "ADV", "ADV", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Mein sch\u00f6ner S\u00fcd! du bist der Stern,", "tokens": ["Mein", "sch\u00f6\u00b7ner", "S\u00fcd", "!", "du", "bist", "der", "Stern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem zustrebt mein Verlangen,", "tokens": ["Dem", "zu\u00b7strebt", "mein", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch h\u00e4lt mich hier im Norden fern", "tokens": ["Doch", "h\u00e4lt", "mich", "hier", "im", "Nor\u00b7den", "fern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPRART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des M\u00fcssens Bann gefangen.", "tokens": ["Des", "M\u00fcs\u00b7sens", "Bann", "ge\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Den theuern, k\u00f6stlichen Ertrag", "tokens": ["Den", "theu\u00b7ern", ",", "k\u00f6st\u00b7li\u00b7chen", "Er\u00b7trag"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von deinen Liebesspenden,", "tokens": ["Von", "dei\u00b7nen", "Lie\u00b7bes\u00b7spen\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Den mu\u00df ich jetzo Tag f\u00fcr Tag", "tokens": ["Den", "mu\u00df", "ich", "jet\u00b7zo", "Tag", "f\u00fcr", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VMFIN", "PPER", "ADV", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vergeuden und verschwenden!", "tokens": ["Ver\u00b7geu\u00b7den", "und", "ver\u00b7schwen\u00b7den", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Doch wenn ich einst den Sieg gewann,", "tokens": ["Doch", "wenn", "ich", "einst", "den", "Sieg", "ge\u00b7wann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Wirren all geschlichtet,", "tokens": ["Die", "Wir\u00b7ren", "all", "ge\u00b7schlich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dann fl\u00fcchte ich zu dir, wie man", "tokens": ["Dann", "fl\u00fcch\u00b7te", "ich", "zu", "dir", ",", "wie", "man"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPER", "$,", "PWAV", "PIS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu einem Freunde fl\u00fcchtet. \u2013", "tokens": ["Zu", "ei\u00b7nem", "Freun\u00b7de", "fl\u00fcch\u00b7tet", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Um mir an deinem Bl\u00fcthenscherz,", "tokens": ["Um", "mir", "an", "dei\u00b7nem", "Bl\u00fc\u00b7then\u00b7scherz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An deinen Sonnenblicken", "tokens": ["An", "dei\u00b7nen", "Son\u00b7nen\u00b7bli\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Den m\u00fcden Geist, das m\u00fcde Herz", "tokens": ["Den", "m\u00fc\u00b7den", "Geist", ",", "das", "m\u00fc\u00b7de", "Herz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf's neue zu erquicken.", "tokens": ["Auf's", "neu\u00b7e", "zu", "er\u00b7qui\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}