{"dta.poem.13518": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der irre Spielmann .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Aus stiller Kindheit unschuldiger Hut", "tokens": ["Aus", "stil\u00b7ler", "Kind\u00b7heit", "un\u00b7schul\u00b7di\u00b7ger", "Hut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Trieb mich der tolle, frevelnde Muth.", "tokens": ["Trieb", "mich", "der", "tol\u00b7le", ",", "fre\u00b7veln\u00b7de", "Muth", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Seit ich da drau\u00dfen so frei nun bin", "tokens": ["Seit", "ich", "da", "drau\u00b7\u00dfen", "so", "frei", "nun", "bin"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADV", "ADJD", "ADV", "VAFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Find' ich nicht wieder nach Hause hin.", "tokens": ["Find'", "ich", "nicht", "wie\u00b7der", "nach", "Hau\u00b7se", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADV", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Durch's Leben jag' ich manch tr\u00fcg'risch Bild,", "tokens": ["Durch's", "Le\u00b7ben", "jag'", "ich", "manch", "tr\u00fcg'\u00b7risch", "Bild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "PPER", "PIAT", "ADJD", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Wer ist der J\u00e4ger da? wer ist das Wild?", "tokens": ["Wer", "ist", "der", "J\u00e4\u00b7ger", "da", "?", "wer", "ist", "das", "Wild", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "PTKVZ", "$.", "PWS", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es pfeift der Wind mir schneidend durchs Haar,", "tokens": ["Es", "pfeift", "der", "Wind", "mir", "schnei\u00b7dend", "durchs", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPER", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Ach Welt, wie bist Du so kalt und klar!", "tokens": ["Ach", "Welt", ",", "wie", "bist", "Du", "so", "kalt", "und", "klar", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$,", "PWAV", "VAFIN", "PPER", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Du frommes Kindlein im stillen Haus,", "tokens": ["Du", "from\u00b7mes", "Kin\u00b7dlein", "im", "stil\u00b7len", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Schau' nicht so l\u00fcstern zum Fenster hinaus!", "tokens": ["Schau'", "nicht", "so", "l\u00fcs\u00b7tern", "zum", "Fens\u00b7ter", "hin\u00b7aus", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ADV", "ADJD", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Frag mich nicht, Kindlein, woher und wohin?", "tokens": ["Frag", "mich", "nicht", ",", "Kin\u00b7dlein", ",", "wo\u00b7her", "und", "wo\u00b7hin", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "$,", "NN", "$,", "PWAV", "KON", "PWAV", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Wei\u00df ich doch selber nicht wo ich bin!", "tokens": ["Wei\u00df", "ich", "doch", "sel\u00b7ber", "nicht", "wo", "ich", "bin", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "PTKNEG", "PWAV", "PPER", "VAFIN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Von S\u00fcnde und Reue zerrissen die Brust,", "tokens": ["Von", "S\u00fcn\u00b7de", "und", "Reu\u00b7e", "zer\u00b7ris\u00b7sen", "die", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Wie rasend in verzweifelter Lust,", "tokens": ["Wie", "ra\u00b7send", "in", "ver\u00b7zwei\u00b7fel\u00b7ter", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Brech ich im Fluge mir Blumen zum Strau\u00df,", "tokens": ["Brech", "ich", "im", "Flu\u00b7ge", "mir", "Blu\u00b7men", "zum", "Strau\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPRART", "NN", "PPER", "NN", "APPRART", "NE", "$,"], "meter": "++-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Wird doch kein fr\u00f6hlicher Kranz nicht daraus! \u2014", "tokens": ["Wird", "doch", "kein", "fr\u00f6h\u00b7li\u00b7cher", "Kranz", "nicht", "da\u00b7raus", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "ADV", "PIAT", "ADJA", "NN", "PTKNEG", "PAV", "$.", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Ich m\u00f6cht' in den tiefsten Wald wohl hinein,", "tokens": ["Ich", "m\u00f6cht'", "in", "den", "tiefs\u00b7ten", "Wald", "wohl", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ART", "ADJA", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Recht aus der Brust den Jammer zu schrei'n,", "tokens": ["Recht", "aus", "der", "Brust", "den", "Jam\u00b7mer", "zu", "schrei'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Ich m\u00f6chte reiten an's Ende der Welt,", "tokens": ["Ich", "m\u00f6ch\u00b7te", "rei\u00b7ten", "an's", "En\u00b7de", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wo der Mond und die Sonne hinunter f\u00e4llt.", "tokens": ["Wo", "der", "Mond", "und", "die", "Son\u00b7ne", "hin\u00b7un\u00b7ter", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "KON", "ART", "NN", "APPO", "VVFIN", "$."], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}}, "stanza.6": {"line.1": {"text": "Wo schwindelnd beginnt die Ewigkeit,", "tokens": ["Wo", "schwin\u00b7delnd", "be\u00b7ginnt", "die", "E\u00b7wig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wie ein Meer, so erschrecklich still und weit,", "tokens": ["Wie", "ein", "Meer", ",", "so", "er\u00b7schreck\u00b7lich", "still", "und", "weit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "ADV", "ADJD", "PTKVZ", "KON", "ADJD", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Da sinken all' Str\u00f6m' und Segel hinein,", "tokens": ["Da", "sin\u00b7ken", "all'", "Str\u00f6m'", "und", "Se\u00b7gel", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Da wird es wohl endlich auch ruhig sein.", "tokens": ["Da", "wird", "es", "wohl", "end\u00b7lich", "auch", "ru\u00b7hig", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "ADV", "ADJD", "VAINF", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}}}}