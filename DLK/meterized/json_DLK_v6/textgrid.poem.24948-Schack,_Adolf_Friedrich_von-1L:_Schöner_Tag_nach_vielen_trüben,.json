{"textgrid.poem.24948": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sch\u00f6ner Tag nach vielen tr\u00fcben,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6ner Tag nach vielen tr\u00fcben,", "tokens": ["Sch\u00f6\u00b7ner", "Tag", "nach", "vie\u00b7len", "tr\u00fc\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PIAT", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hat in unser Nebelland", "tokens": ["Hat", "in", "un\u00b7ser", "Ne\u00b7bel\u00b7land"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dich die Sonnenheimat dr\u00fcben,", "tokens": ["Dich", "die", "Son\u00b7nen\u00b7hei\u00b7mat", "dr\u00fc\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dich der Orient gesandt?", "tokens": ["Dich", "der", "O\u00b7rient", "ge\u00b7sandt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVPP", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Leisen Hauches scheucht ein reiner", "tokens": ["Lei\u00b7sen", "Hau\u00b7ches", "scheucht", "ein", "rei\u00b7ner"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Oft das schwere Wolkengrau,", "tokens": ["Oft", "das", "schwe\u00b7re", "Wol\u00b7ken\u00b7grau", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und mein Odem steigt wie deiner", "tokens": ["Und", "mein", "O\u00b7dem", "steigt", "wie", "dei\u00b7ner"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "KOKOM", "PPOSAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Leicht ins klare Himmelsblau.", "tokens": ["Leicht", "ins", "kla\u00b7re", "Him\u00b7mels\u00b7blau", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und in deine Pracht versunken", "tokens": ["Und", "in", "dei\u00b7ne", "Pracht", "ver\u00b7sun\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit Gebirg und Meer und Thal", "tokens": ["Mit", "Ge\u00b7birg", "und", "Meer", "und", "Thal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schwelgt die Seele mir, wie trunken,", "tokens": ["Schwelgt", "die", "See\u00b7le", "mir", ",", "wie", "trun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "$,", "PWAV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In des Lichtes goldnem Strahl.", "tokens": ["In", "des", "Lich\u00b7tes", "gold\u00b7nem", "Strahl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "So an Syriens Felsgestaden,", "tokens": ["So", "an", "Sy\u00b7ri\u00b7ens", "Fels\u00b7ge\u00b7sta\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "NN", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "So am Archipelagus,", "tokens": ["So", "am", "Ar\u00b7chi\u00b7pe\u00b7la\u00b7gus", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf den sonnigen Cykladen", "tokens": ["Auf", "den", "son\u00b7ni\u00b7gen", "Cyk\u00b7la\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gr\u00fc\u00dfte mich dein Feuerku\u00df,", "tokens": ["Gr\u00fc\u00df\u00b7te", "mich", "dein", "Feu\u00b7er\u00b7ku\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Als das Licht, das gottentstammte,", "tokens": ["Als", "das", "Licht", ",", "das", "got\u00b7tent\u00b7stamm\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das von Asiens Bergen kam,", "tokens": ["Das", "von", "A\u00b7siens", "Ber\u00b7gen", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "NE", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fr\u00fch in meine Tr\u00e4ume flammte", "tokens": ["Fr\u00fch", "in", "mei\u00b7ne", "Tr\u00e4u\u00b7me", "flamm\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die Sorge von mir nahm.", "tokens": ["Und", "die", "Sor\u00b7ge", "von", "mir", "nahm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Goldner Tag! Aus deinem Scho\u00dfe", "tokens": ["Gold\u00b7ner", "Tag", "!", "Aus", "dei\u00b7nem", "Scho\u00b7\u00dfe"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hab' ich taumelnd, sinnverwirrt,", "tokens": ["Hab'", "ich", "tau\u00b7melnd", ",", "sinn\u00b7ver\u00b7wirrt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "PPER", "VVPP", "$,", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Also glaub' ich, in die gro\u00dfe", "tokens": ["Al\u00b7so", "glaub'", "ich", ",", "in", "die", "gro\u00b7\u00dfe"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weite Weltnacht mich verirrt.", "tokens": ["Wei\u00b7te", "Welt\u00b7nacht", "mich", "ver\u00b7irrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Der des Lebens Erstgeborne", "tokens": ["Der", "des", "Le\u00b7bens", "Erst\u00b7ge\u00b7bor\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Du geweiht zu h\u00f6hrer Lust,", "tokens": ["Du", "ge\u00b7weiht", "zu", "h\u00f6h\u00b7rer", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "PTKZU", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nimm dein Kind denn, das verlorne,", "tokens": ["Nimm", "dein", "Kind", "denn", ",", "das", "ver\u00b7lor\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "ADV", "$,", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nimm's zur\u00fcck an deine Brust!", "tokens": ["Nim\u00b7m's", "zu\u00b7r\u00fcck", "an", "dei\u00b7ne", "Brust", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}