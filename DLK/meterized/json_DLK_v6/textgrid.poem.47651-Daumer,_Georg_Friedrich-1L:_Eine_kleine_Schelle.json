{"textgrid.poem.47651": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Eine kleine Schelle", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eine kleine Schelle", "tokens": ["Ei\u00b7ne", "klei\u00b7ne", "Schel\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Klingelt durch die Zweige silberhelle; \u2013", "tokens": ["Klin\u00b7gelt", "durch", "die", "Zwei\u00b7ge", "sil\u00b7ber\u00b7hel\u00b7le", ";", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Ja, sie ist's, es nahet diesem Ort", "tokens": ["Ja", ",", "sie", "ist's", ",", "es", "na\u00b7het", "die\u00b7sem", "Ort"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "$,", "PPER", "VVFIN", "PDAT", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Mamia's befreundete Gazelle.", "tokens": ["Ma\u00b7mi\u00b7a's", "be\u00b7freun\u00b7de\u00b7te", "Ga\u00b7zel\u00b7le", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Wonnevolle Kunden", "tokens": ["Won\u00b7ne\u00b7vol\u00b7le", "Kun\u00b7den"], "token_info": ["word", "word"], "pos": ["NN", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Sind an ihren schlanken Hals gebunden,", "tokens": ["Sind", "an", "ih\u00b7ren", "schlan\u00b7ken", "Hals", "ge\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Eine Schrift von Bl\u00fcthen, reich an Sinn,", "tokens": ["Ei\u00b7ne", "Schrift", "von", "Bl\u00fc\u00b7then", ",", "reich", "an", "Sinn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,", "ADJD", "APPR", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Linde stillend meiner Sehnsucht Wunden.", "tokens": ["Lin\u00b7de", "stil\u00b7lend", "mei\u00b7ner", "Sehn\u00b7sucht", "Wun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Bote du, getreuer", "tokens": ["Bo\u00b7te", "du", ",", "ge\u00b7treu\u00b7er"], "token_info": ["word", "word", "punct", "word"], "pos": ["VVFIN", "PPER", "$,", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Ihrer Huld, ich gr\u00fc\u00dfe dich mit Feuer,", "tokens": ["Ih\u00b7rer", "Huld", ",", "ich", "gr\u00fc\u00b7\u00dfe", "dich", "mit", "Feu\u00b7er", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "K\u00fcsse dich, als w\u00e4rest du sie selbst,", "tokens": ["K\u00fcs\u00b7se", "dich", ",", "als", "w\u00e4\u00b7rest", "du", "sie", "selbst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "KOKOM", "VAFIN", "PPER", "PPER", "ADV", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Die mir \u00fcber Alles lieb und theuer.", "tokens": ["Die", "mir", "\u00fc\u00b7ber", "Al\u00b7les", "lieb", "und", "theu\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PIS", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Dies Gestr\u00e4u\u00df behende,", "tokens": ["Dies", "Ge\u00b7str\u00e4u\u00df", "be\u00b7hen\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "NN", "ADJA", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Das ich ihr zu gleicher Antwort sende,", "tokens": ["Das", "ich", "ihr", "zu", "glei\u00b7cher", "Ant\u00b7wort", "sen\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Nimm es hin und liefer' es im Sprung,", "tokens": ["Nimm", "es", "hin", "und", "lie\u00b7fer'", "es", "im", "Sprung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PTKVZ", "KON", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Wie du pflegst, in ihre zarten H\u00e4nde!", "tokens": ["Wie", "du", "pflegst", ",", "in", "ih\u00b7re", "zar\u00b7ten", "H\u00e4n\u00b7de", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}