{"dta.poem.16079": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "133.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Von Freunden sagt man dir, die mit dem Gl\u00fccke k\u00e4men,", "tokens": ["Von", "Freun\u00b7den", "sagt", "man", "dir", ",", "die", "mit", "dem", "Gl\u00fc\u00b7cke", "k\u00e4\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PIS", "PPER", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit ihm verweileten, und mit ihm Abschied n\u00e4hmen.", "tokens": ["Mit", "ihm", "ver\u00b7wei\u00b7le\u00b7ten", ",", "und", "mit", "ihm", "Ab\u00b7schied", "n\u00e4h\u00b7men", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "$,", "KON", "APPR", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die falschen hei\u00dfen sie, dagegen in der Noth", "tokens": ["Die", "fal\u00b7schen", "hei\u00b7\u00dfen", "sie", ",", "da\u00b7ge\u00b7gen", "in", "der", "Noth"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "VVFIN", "PPER", "$,", "PAV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der wahre kommt, der dir H\u00fclf' oder Trost doch bot.", "tokens": ["Der", "wah\u00b7re", "kommt", ",", "der", "dir", "H\u00fcl\u00b7f'", "o\u00b7der", "Trost", "doch", "bot", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "$,", "PRELS", "PPER", "NN", "KON", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Glaub nur den Weisen, was sie tadeln oder loben,", "tokens": ["Glaub", "nur", "den", "Wei\u00b7sen", ",", "was", "sie", "ta\u00b7deln", "o\u00b7der", "lo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "$,", "PRELS", "PPER", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch m\u00f6gest du an dir die Weisheit nie erproben.", "tokens": ["Doch", "m\u00f6\u00b7gest", "du", "an", "dir", "die", "Weis\u00b7heit", "nie", "er\u00b7pro\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "APPR", "PPER", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Nie brauchen m\u00f6gest du den leidigen getreuen,", "tokens": ["Nie", "brau\u00b7chen", "m\u00f6\u00b7gest", "du", "den", "lei\u00b7di\u00b7gen", "ge\u00b7treu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VMFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Stets mit den falschen dich, den fr\u00f6hlichen, dich freuen.", "tokens": ["Stets", "mit", "den", "fal\u00b7schen", "dich", ",", "den", "fr\u00f6h\u00b7li\u00b7chen", ",", "dich", "freu\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "PPER", "$,", "ART", "ADJA", "$,", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}