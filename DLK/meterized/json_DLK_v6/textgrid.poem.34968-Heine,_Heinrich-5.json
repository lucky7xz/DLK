{"textgrid.poem.34968": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "5", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich sah sie lachen, sah sie l\u00e4cheln,", "tokens": ["Ich", "sah", "sie", "la\u00b7chen", ",", "sah", "sie", "l\u00e4\u00b7cheln", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVINF", "$,", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich sah sie ganz zugrunde gehn;", "tokens": ["Ich", "sah", "sie", "ganz", "zu\u00b7grun\u00b7de", "gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich h\u00f6rt ihr Weinen und ihr R\u00f6cheln,", "tokens": ["Ich", "h\u00f6rt", "ihr", "Wei\u00b7nen", "und", "ihr", "R\u00f6\u00b7cheln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und habe ruhig zugesehn.", "tokens": ["Und", "ha\u00b7be", "ru\u00b7hig", "zu\u00b7ge\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Leidtragend folgt ich ihren S\u00e4rgen,", "tokens": ["Leid\u00b7tra\u00b7gend", "folgt", "ich", "ih\u00b7ren", "S\u00e4r\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und bis zum Kirchhof ging ich mit;", "tokens": ["Und", "bis", "zum", "Kirch\u00b7hof", "ging", "ich", "mit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hernach, ich will es nicht verbergen,", "tokens": ["Her\u00b7nach", ",", "ich", "will", "es", "nicht", "ver\u00b7ber\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Speist ich zu Mittag mit App'tit.", "tokens": ["Speist", "ich", "zu", "Mit\u00b7tag", "mit", "Ap\u00b7p'\u00b7tit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "APPR", "NN", "$."], "meter": "+-----+-+", "measure": "dactylic.init"}}, "stanza.3": {"line.1": {"text": "Doch jetzt auf einmal mit Betr\u00fcbnis", "tokens": ["Doch", "jetzt", "auf", "ein\u00b7mal", "mit", "Be\u00b7tr\u00fcb\u00b7nis"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Denk ich der l\u00e4ngstverstorbnen Schar;", "tokens": ["Denk", "ich", "der", "l\u00e4ngst\u00b7ver\u00b7storb\u00b7nen", "Schar", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie lodernd pl\u00f6tzliche Verliebnis", "tokens": ["Wie", "lo\u00b7dernd", "pl\u00f6tz\u00b7li\u00b7che", "Ver\u00b7lieb\u00b7nis"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "St\u00fcrmt's auf im Herzen wunderbar!", "tokens": ["St\u00fcrmt's", "auf", "im", "Her\u00b7zen", "wun\u00b7der\u00b7bar", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "APPRART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Besonders sind es Julchens Tr\u00e4nen,", "tokens": ["Be\u00b7son\u00b7ders", "sind", "es", "Jul\u00b7chens", "Tr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NE", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die im Ged\u00e4chtnis rinnen mir;", "tokens": ["Die", "im", "Ge\u00b7d\u00e4cht\u00b7nis", "rin\u00b7nen", "mir", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Wehmut wird zu wildem Sehnen,", "tokens": ["Die", "Weh\u00b7mut", "wird", "zu", "wil\u00b7dem", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und Tag und Nacht ruf ich nach ihr! \u2013 \u2013", "tokens": ["Und", "Tag", "und", "Nacht", "ruf", "ich", "nach", "ihr", "!", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "NN", "KON", "NN", "NN", "PPER", "APPR", "PPER", "$.", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Oft kommt zu mir die tote Blume", "tokens": ["Oft", "kommt", "zu", "mir", "die", "to\u00b7te", "Blu\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Im Fiebertraum; alsdann zumut'", "tokens": ["Im", "Fie\u00b7ber\u00b7traum", ";", "als\u00b7dann", "zu\u00b7mut'"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "$.", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ist mir, als b\u00f6te sie postume", "tokens": ["Ist", "mir", ",", "als", "b\u00f6\u00b7te", "sie", "pos\u00b7tu\u00b7me"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "$,", "KOUS", "ADJA", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gew\u00e4hrung meiner Liebesglut.", "tokens": ["Ge\u00b7w\u00e4h\u00b7rung", "mei\u00b7ner", "Lie\u00b7bes\u00b7glut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "O z\u00e4rtliches Phantom, umschlie\u00dfe", "tokens": ["O", "z\u00e4rt\u00b7li\u00b7ches", "Phan\u00b7tom", ",", "um\u00b7schlie\u00b7\u00dfe"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["NE", "ADJA", "NN", "$,", "VVFIN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Mich fest und fester, deinen Mund,", "tokens": ["Mich", "fest", "und", "fes\u00b7ter", ",", "dei\u00b7nen", "Mund", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "KON", "ADJD", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dr\u00fcck ihn auf meinen Mund \u2013 vers\u00fc\u00dfe", "tokens": ["Dr\u00fcck", "ihn", "auf", "mei\u00b7nen", "Mund", "\u2013", "ver\u00b7s\u00fc\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["NN", "PPER", "APPR", "PPOSAT", "NN", "$(", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Bitternis der letzten Stund'!", "tokens": ["Die", "Bit\u00b7ter\u00b7nis", "der", "letz\u00b7ten", "Stund'", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}