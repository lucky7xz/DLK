{"textgrid.poem.62135": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "[ach, meine Mutter, f\u00e4nd' ich Dich wieder]", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach, meine Mutter, f\u00e4nd' ich Dich wieder \u2013", "tokens": ["Ach", ",", "mei\u00b7ne", "Mut\u00b7ter", ",", "f\u00e4nd'", "ich", "Dich", "wie\u00b7der", "\u2013"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPOSAT", "NN", "$,", "VVFIN", "PPER", "PRF", "ADV", "$("], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ach, in der Welten unendlichem Raum,", "tokens": ["Ach", ",", "in", "der", "Wel\u00b7ten", "un\u00b7end\u00b7li\u00b7chem", "Raum", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "So w\u00fcrd' ich Dich suchen mit allen Kr\u00e4ften,", "tokens": ["So", "w\u00fcrd'", "ich", "Dich", "su\u00b7chen", "mit", "al\u00b7len", "Kr\u00e4f\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-++--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Wie jetzt ich Dich suche im Wachen und Traum.", "tokens": ["Wie", "jetzt", "ich", "Dich", "su\u00b7che", "im", "Wa\u00b7chen", "und", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "PPER", "VVFIN", "APPRART", "NN", "KON", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}}}}