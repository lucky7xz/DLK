{"dta.poem.1944": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Uberschrifften und Sinn-  \n Gedichte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Du wirst zwar Jungfrau noch/ doch zweiffelhafft genennt/", "tokens": ["Du", "wirst", "zwar", "Jung\u00b7frau", "noch", "/", "doch", "zweif\u00b7fel\u00b7hafft", "ge\u00b7nennt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "ADV", "$(", "ADV", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Hau\u00df das bleibt ein Hau\u00df/ wiewohl es abgebrennt.", "tokens": ["Ein", "Hau\u00df", "das", "bleibt", "ein", "Hau\u00df", "/", "wie\u00b7wohl", "es", "ab\u00b7ge\u00b7brennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDS", "VVFIN", "ART", "NN", "$(", "KOUS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "in Dieb mu\u00df offtermahls den Titel/ Ehrlich/ f\u00fchren/", "tokens": ["in", "Dieb", "mu\u00df", "off\u00b7ter\u00b7mahls", "den", "Ti\u00b7tel", "/", "Ehr\u00b7lich", "/", "f\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "ADV", "ART", "NN", "$(", "NE", "$(", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "o will der Titel dir der Jungfrau auch geb\u00fchren.", "tokens": ["o", "will", "der", "Ti\u00b7tel", "dir", "der", "Jung\u00b7frau", "auch", "ge\u00b7b\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "VMFIN", "ART", "NN", "PPER", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}