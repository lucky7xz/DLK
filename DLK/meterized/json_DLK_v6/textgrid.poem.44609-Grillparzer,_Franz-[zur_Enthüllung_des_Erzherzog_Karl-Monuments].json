{"textgrid.poem.44609": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[zur Enth\u00fcllung des Erzherzog Karl-Monuments]", "genre": "verse", "period": "N.A.", "pub_year": 1860, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Welt war schwarz mit Nacht umzogen,", "tokens": ["Die", "Welt", "war", "schwarz", "mit", "Nacht", "um\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "APPR", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Als du am Steuer standst der Macht,", "tokens": ["Als", "du", "am", "Steu\u00b7er", "standst", "der", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bald unter und bald ob den Wogen", "tokens": ["Bald", "un\u00b7ter", "und", "bald", "ob", "den", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "KON", "ADV", "KOUS", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Ward in den Port das Schiff gebracht.", "tokens": ["Ward", "in", "den", "Port", "das", "Schiff", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NE", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Zeiten haben wieder sich verschlimmert,", "tokens": ["Die", "Zei\u00b7ten", "ha\u00b7ben", "wie\u00b7der", "sich", "ver\u00b7schlim\u00b7mert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sei uns ein Stern in d\u00fcstrer Nacht!", "tokens": ["Sei", "uns", "ein", "Stern", "in", "d\u00fcst\u00b7rer", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Solange uns dein Beispiel schimmert,", "tokens": ["So\u00b7lan\u00b7ge", "uns", "dein", "Bei\u00b7spiel", "schim\u00b7mert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Eint sich die Treue mit der Macht.", "tokens": ["Eint", "sich", "die", "Treu\u00b7e", "mit", "der", "Macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.9": {"text": "...", "tokens": ["..."], "token_info": ["punct"], "pos": ["$("]}}}}}