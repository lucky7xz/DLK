{"dta.poem.23748": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "Die Sonne.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1767", "urn": "urn:nbn:de:kobv:b4-20643-5", "language": ["de:0.99"], "booktitle": "Wei\u00dfe, Christian Felix: Lieder f\u00fcr Kinder. Leipzig, 1767."}, "poem": {"stanza.1": {"line.1": {"text": "Gegr\u00fc\u00dfet seyst du, edles Licht,", "tokens": ["Ge\u00b7gr\u00fc\u00b7\u00dfet", "seyst", "du", ",", "ed\u00b7les", "Licht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O Sonne, die mein Angesicht", "tokens": ["O", "Son\u00b7ne", ",", "die", "mein", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$,", "PRELS", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Aufs neu jetzund erhellet!", "tokens": ["Aufs", "neu", "je\u00b7tzund", "er\u00b7hel\u00b7let", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJD", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie gro\u00df ist der, der dich gemacht,", "tokens": ["Wie", "gro\u00df", "ist", "der", ",", "der", "dich", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und deine Majest\u00e4t und Pracht", "tokens": ["Und", "dei\u00b7ne", "Ma\u00b7jes\u00b7t\u00e4t", "und", "Pracht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ans Firmament gestellet!", "tokens": ["Ans", "Fir\u00b7ma\u00b7ment", "ge\u00b7stel\u00b7let", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Aus deinem Feuermeere flie\u00dft", "tokens": ["Aus", "dei\u00b7nem", "Feu\u00b7er\u00b7mee\u00b7re", "flie\u00dft"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die W\u00e4rm in alles, was da ist,", "tokens": ["Die", "W\u00e4rm", "in", "al\u00b7les", ",", "was", "da", "ist", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PIS", "$,", "PRELS", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Jhm Kraft und Glanz zu geben.", "tokens": ["Jhm", "Kraft", "und", "Glanz", "zu", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Eichbaum und das kleinste Gras", "tokens": ["Der", "Eich\u00b7baum", "und", "das", "kleins\u00b7te", "Gras"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Empf\u00e4ngt von dir in gleichem Maas", "tokens": ["Emp\u00b7f\u00e4ngt", "von", "dir", "in", "glei\u00b7chem", "Maas"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Flor, Wachsthum, Reife, Leben.", "tokens": ["Flor", ",", "Wach\u00b7sthum", ",", "Rei\u00b7fe", ",", "Le\u00b7ben", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Du bist des frommen Weisen Bild,", "tokens": ["Du", "bist", "des", "from\u00b7men", "Wei\u00b7sen", "Bild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der stets, mit Menschenlieb erf\u00fcllt,", "tokens": ["Der", "stets", ",", "mit", "Men\u00b7schen\u00b7lieb", "er\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vertheilt, was er besitzet.", "tokens": ["Ver\u00b7theilt", ",", "was", "er", "be\u00b7sit\u00b7zet", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den Bl\u00f6den leuchtet sein Verstand,", "tokens": ["Den", "Bl\u00f6\u00b7den", "leuch\u00b7tet", "sein", "Ver\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Indem die immer offne Hand", "tokens": ["In\u00b7dem", "die", "im\u00b7mer", "off\u00b7ne", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wohlth\u00e4tig andern n\u00fctzet.", "tokens": ["Wohlt\u00b7h\u00e4\u00b7tig", "an\u00b7dern", "n\u00fct\u00b7zet", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}