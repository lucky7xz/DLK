 "line.56": {"text": "Die wir benedeyen wollen, wo sie unsern Mentem trift.", "tokens": ["Die", "wir", "be\u00b7ne\u00b7de\u00b7yen", "wol\u00b7len", ",", "wo", "sie", "un\u00b7sern", "Men\u00b7tem", "trift", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVINF", "VMFIN", "$,", "PWAV", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.57": {"text": "Phoebus ruft uns: Spannt doch aus! Denn der Pegasus ist m\u00fcde,", "tokens": ["Phoe\u00b7bus", "ruft", "uns", ":", "Spannt", "doch", "aus", "!", "Denn", "der", "Pe\u00b7ga\u00b7sus", "ist", "m\u00fc\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$.", "VVFIN", "ADV", "PTKVZ", "$.", "KON", "ART", "NE", "VAFIN", "ADJD", "$,"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.58": {"text": "Treibt den abgerittnen Gaul in die allernechste Schmiede,", "tokens": ["Treibt", "den", "ab\u00b7ge\u00b7ritt\u00b7nen", "Gaul", "in", "die", "al\u00b7ler\u00b7nechs\u00b7te", "Schmie\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.59": {"text": "Seht ihr nicht, der stumpfe Klepper mu\u00df noch mehr als Strangvieh gehn", "tokens": ["Seht", "ihr", "nicht", ",", "der", "stump\u00b7fe", "Klep\u00b7per", "mu\u00df", "noch", "mehr", "als", "Strang\u00b7vieh", "gehn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,", "ART", "ADJA", "NN", "VMFIN", "ADV", "ADV", "KOUS", "NN", "VVINF"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.oct