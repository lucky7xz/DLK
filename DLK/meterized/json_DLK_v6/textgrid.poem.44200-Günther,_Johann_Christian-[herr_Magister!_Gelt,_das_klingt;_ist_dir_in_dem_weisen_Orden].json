{"textgrid.poem.44200": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "[herr Magister! Gelt, das klingt; ist dir in dem weisen Orden]", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr Magister! Gelt, das klingt; ist dir in dem weisen Orden", "tokens": ["Herr", "Ma\u00b7gis\u00b7ter", "!", "Gelt", ",", "das", "klingt", ";", "ist", "dir", "in", "dem", "wei\u00b7sen", "Or\u00b7den"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "$.", "NN", "$,", "PDS", "VVFIN", "$.", "VAFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Nicht die Nase wie der Rang einen Fu\u00dfbreit h\u00f6her worden,", "tokens": ["Nicht", "die", "Na\u00b7se", "wie", "der", "Rang", "ei\u00b7nen", "Fu\u00df\u00b7breit", "h\u00f6\u00b7her", "wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "KOKOM", "ART", "NN", "ART", "NN", "ADJD", "VAPP", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "O so blicke diese Bl\u00e4tter so verliebt und freundlich an,", "tokens": ["O", "so", "bli\u00b7cke", "die\u00b7se", "Bl\u00e4t\u00b7ter", "so", "ver\u00b7liebt", "und", "freund\u00b7lich", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "PDAT", "NN", "ADV", "VVPP", "KON", "ADJD", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Als du damahls in den Eichen, weist du gegen wem? gethan.", "tokens": ["Als", "du", "da\u00b7mahls", "in", "den", "Ei\u00b7chen", ",", "weist", "du", "ge\u00b7gen", "wem", "?", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "ART", "NN", "$,", "VVFIN", "PPER", "APPR", "PWS", "$.", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Darfstu dich doch eben nicht auf ein nettes Lied verspizen;", "tokens": ["Darfs\u00b7tu", "dich", "doch", "e\u00b7ben", "nicht", "auf", "ein", "net\u00b7tes", "Lied", "ver\u00b7spi\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "ADV", "ADV", "PTKNEG", "APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Du verstehst schon, wie es h\u00e4lt, wo ihr viel zu Tische sizen,", "tokens": ["Du", "ver\u00b7stehst", "schon", ",", "wie", "es", "h\u00e4lt", ",", "wo", "ihr", "viel", "zu", "Ti\u00b7sche", "si\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PWAV", "PPER", "VVFIN", "$,", "PWAV", "PPER", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.7": {"text": "Einer \u00fcberschreyt den andern, der flickt dies, der jenes ein,", "tokens": ["Ei\u00b7ner", "\u00fc\u00b7bersc\u00b7hreyt", "den", "an\u00b7dern", ",", "der", "flickt", "dies", ",", "der", "je\u00b7nes", "ein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "ADJA", "$,", "PRELS", "VVFIN", "PDS", "$,", "PRELS", "PDS", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Und ein Bild, von vielen H\u00e4nden wird sich nirgends \u00e4hnlich seyn.", "tokens": ["Und", "ein", "Bild", ",", "von", "vie\u00b7len", "H\u00e4n\u00b7den", "wird", "sich", "nir\u00b7gends", "\u00e4hn\u00b7lich", "seyn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "APPR", "PIAT", "NN", "VAFIN", "PRF", "ADV", "ADJD", "VAINF", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.9": {"text": "Aber doch vermehlen sich die getreuen Freundschaftsflammen,", "tokens": ["A\u00b7ber", "doch", "ver\u00b7meh\u00b7len", "sich", "die", "ge\u00b7treu\u00b7en", "Freund\u00b7schafts\u00b7flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.10": {"text": "Und wir schie\u00dfen allerseits unsern Dichterkram zusammen,", "tokens": ["Und", "wir", "schie\u00b7\u00dfen", "al\u00b7ler\u00b7seits", "un\u00b7sern", "Dich\u00b7ter\u00b7kram", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "--+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Blos die Freude zu bezeugen, da dich Fieckchens Ehrenfest", "tokens": ["Blos", "die", "Freu\u00b7de", "zu", "be\u00b7zeu\u00b7gen", ",", "da", "dich", "Fieck\u00b7chens", "Eh\u00b7ren\u00b7fest"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "PTKZU", "VVFIN", "$,", "KOUS", "PPER", "NN", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.12": {"text": "(aber nicht das braune Fieckchen) \u00fcber ihren Brautkranz l\u00e4st.", "tokens": ["(", "a\u00b7ber", "nicht", "das", "brau\u00b7ne", "Fieck\u00b7chen", ")", "\u00fc\u00b7ber", "ih\u00b7ren", "Braut\u00b7kranz", "l\u00e4st", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "PTKNEG", "ART", "ADJA", "NN", "$(", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.13": {"text": "H\u00e4tten wir doch nur die Lust, dich im Schmucke zu erblicken;", "tokens": ["H\u00e4t\u00b7ten", "wir", "doch", "nur", "die", "Lust", ",", "dich", "im", "Schmu\u00b7cke", "zu", "er\u00b7bli\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ART", "NN", "$,", "PRF", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "+--+--+--+-+-+-", "measure": "elegiambus"}, "line.14": {"text": "Denn du wirst dich, glauben wir, be\u00dfer in die Hoffart schicken", "tokens": ["Denn", "du", "wirst", "dich", ",", "glau\u00b7ben", "wir", ",", "be\u00b7\u00dfer", "in", "die", "Hof\u00b7fart", "schi\u00b7cken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "PPER", "$,", "VVFIN", "PPER", "$,", "ADJD", "APPR", "ART", "NN", "VVINF"], "meter": "--+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.15": {"text": "Als Sabinchen in die Hauben und Sylvander in das Band,", "tokens": ["Als", "Sa\u00b7bin\u00b7chen", "in", "die", "Hau\u00b7ben", "und", "Syl\u00b7van\u00b7der", "in", "das", "Band", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "APPR", "ART", "NN", "KON", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.16": {"text": "Das ihm Lieschens kleine Schwester zur Erk\u00e4ntligkeit gesand.", "tokens": ["Das", "ihm", "Lie\u00b7schens", "klei\u00b7ne", "Schwes\u00b7ter", "zur", "Er\u00b7k\u00e4nt\u00b7lig\u00b7keit", "ge\u00b7sand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "NN", "ADJA", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.17": {"text": "Bruder, weistu was noch mehr? Las dich doch im K\u00e4pchen mahlen,", "tokens": ["Bru\u00b7der", ",", "weis\u00b7tu", "was", "noch", "mehr", "?", "Las", "dich", "doch", "im", "K\u00e4p\u00b7chen", "mah\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PWS", "ADV", "ADV", "$.", "NE", "PPER", "ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "+--+-+-+-+-+-+-", "measure": "iambic.septa.invert"}, "line.18": {"text": "Amaranthe, sey gewis, wird den Abri\u00df gern bezahlen.", "tokens": ["A\u00b7ma\u00b7ran\u00b7the", ",", "sey", "ge\u00b7wis", ",", "wird", "den", "Ab\u00b7ri\u00df", "gern", "be\u00b7zah\u00b7len", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VAFIN", "ADV", "$,", "VAFIN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+--++--+-", "measure": "trochaic.septa.relaxed"}, "line.19": {"text": "Sie ergezt sich in der Seele, wenn dein wohl gestalltes Bild", "tokens": ["Sie", "er\u00b7gezt", "sich", "in", "der", "See\u00b7le", ",", "wenn", "dein", "wohl", "ge\u00b7stall\u00b7tes", "Bild"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "$,", "KOUS", "PPOSAT", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.20": {"text": "In dem angef\u00fchrten Staate ihr Corsettchen vorw\u00e4rts f\u00fcllt.", "tokens": ["In", "dem", "an\u00b7ge\u00b7f\u00fchr\u00b7ten", "Staa\u00b7te", "ihr", "Cor\u00b7sett\u00b7chen", "vor\u00b7w\u00e4rts", "f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PPOSAT", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.21": {"text": "Z\u00fcrne nicht, es ist ein Scherz; wir verstehn so gut einander", "tokens": ["Z\u00fcr\u00b7ne", "nicht", ",", "es", "ist", "ein", "Scherz", ";", "wir", "ver\u00b7stehn", "so", "gut", "ein\u00b7an\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "PTKNEG", "$,", "PPER", "VAFIN", "ART", "NN", "$.", "PPER", "VVFIN", "ADV", "ADJD", "PRF"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.22": {"text": "Als das Pferd Bucephalus und ein gro\u00dfer Alexander.", "tokens": ["Als", "das", "Pferd", "Bu\u00b7ce\u00b7pha\u00b7lus", "und", "ein", "gro\u00b7\u00dfer", "A\u00b7lex\u00b7an\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NE", "KON", "ART", "ADJA", "NE", "$."], "meter": "--++-+-+-+-+-+-", "measure": "anapaest.init"}, "line.23": {"text": "Hinckt das Gleichn\u00fc\u00df, so gedencke: Verse brechen oft ein Bein,", "tokens": ["Hinckt", "das", "Gleich\u00b7n\u00fc\u00df", ",", "so", "ge\u00b7den\u00b7cke", ":", "Ver\u00b7se", "bre\u00b7chen", "oft", "ein", "Bein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ADV", "VVFIN", "$.", "NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.24": {"text": "Wenn der Jungfern Zaubereyen dreymahl \u00fcber Achsel spein.", "tokens": ["Wenn", "der", "Jung\u00b7fern", "Zau\u00b7be\u00b7re\u00b7yen", "drey\u00b7mahl", "\u00fc\u00b7ber", "Ach\u00b7sel", "spein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "ADV", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.25": {"text": "Du bist auch nicht selten da, wo man Stacheln\u00fc\u00dfe sch\u00fcttelt,", "tokens": ["Du", "bist", "auch", "nicht", "sel\u00b7ten", "da", ",", "wo", "man", "Sta\u00b7chel\u00b7n\u00fc\u00b7\u00dfe", "sch\u00fct\u00b7telt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PTKNEG", "ADJD", "PTKVZ", "$,", "PWAV", "PIS", "NN", "VVFIN", "$,"], "meter": "-+-++--+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.26": {"text": "Und man weis schon, da\u00df dein Kiel an dem kleinen Teller n\u00fcttelt,", "tokens": ["Und", "man", "weis", "schon", ",", "da\u00df", "dein", "Kiel", "an", "dem", "klei\u00b7nen", "Tel\u00b7ler", "n\u00fct\u00b7telt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PTKVZ", "ADV", "$,", "KOUS", "PPOSAT", "NN", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "--+--++--+-+-+-", "measure": "anapaest.di.plus"}, "line.27": {"text": "\u00dcber welchen Albimene artig und galant coupirt", "tokens": ["\u00dc\u00b7ber", "wel\u00b7chen", "Al\u00b7bi\u00b7me\u00b7ne", "ar\u00b7tig", "und", "ga\u00b7lant", "cou\u00b7pirt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PWAT", "NN", "ADJD", "KON", "ADJD", "VVPP"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.28": {"text": "Und gleichwohl im engen Raume keinen Nebentritt verliert.", "tokens": ["Und", "gleich\u00b7wohl", "im", "en\u00b7gen", "Rau\u00b7me", "kei\u00b7nen", "Ne\u00b7bent\u00b7ritt", "ver\u00b7liert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "ADJA", "NN", "PIAT", "NN", "VVFIN", "$."], "meter": "----+-+-+-+-+-+", "measure": "unknown.measure.hexa"}, "line.29": {"text": "Umgekehrt! Der Weg ist falsch, und wir gehn zu weit vom Ziele.", "tokens": ["Um\u00b7ge\u00b7kehrt", "!", "Der", "Weg", "ist", "falsch", ",", "und", "wir", "gehn", "zu", "weit", "vom", "Zie\u00b7le", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$.", "ART", "NN", "VAFIN", "ADJD", "$,", "KON", "PPER", "VVFIN", "PTKA", "ADJD", "APPRART", "NN", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.30": {"text": "Deine W\u00fcrde lehrt uns jezt, da\u00df man andre Sachen spiele,", "tokens": ["Dei\u00b7ne", "W\u00fcr\u00b7de", "lehrt", "uns", "jezt", ",", "da\u00df", "man", "and\u00b7re", "Sa\u00b7chen", "spie\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADV", "$,", "KOUS", "PIS", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.31": {"text": "Die zum Preis der Wi\u00dfenschaften (o was herrscht nicht hier vor Wind!)", "tokens": ["Die", "zum", "Preis", "der", "Wi\u00b7\u00dfen\u00b7schaf\u00b7ten", "(", "o", "was", "herrscht", "nicht", "hier", "vor", "Wind", "!", ")"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "APPRART", "NN", "ART", "NN", "$(", "FM", "PWS", "VVFIN", "PTKNEG", "ADV", "APPR", "NN", "$.", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.32": {"text": "N\u00fczlich, dienlich, artig, lustig, n\u00f6thig und erbaulich sind.", "tokens": ["N\u00fcz\u00b7lich", ",", "dien\u00b7lich", ",", "ar\u00b7tig", ",", "lus\u00b7tig", ",", "n\u00f6\u00b7thig", "und", "er\u00b7bau\u00b7lich", "sind", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "$,", "ADJD", "$,", "ADJD", "$,", "ADJD", "KON", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.33": {"text": "Dein Examen ist zwar aus, und du hast dich wohl gehalten", "tokens": ["Dein", "E\u00b7xa\u00b7men", "ist", "zwar", "aus", ",", "und", "du", "hast", "dich", "wohl", "ge\u00b7hal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "PTKVZ", "$,", "KON", "PPER", "VAFIN", "PPER", "ADV", "VVPP"], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.34": {"text": "Und vermagst nunmehr dein Amt als ein Meister zu verwalten;", "tokens": ["Und", "ver\u00b7magst", "nun\u00b7mehr", "dein", "Amt", "als", "ein", "Meis\u00b7ter", "zu", "ver\u00b7wal\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PPOSAT", "NN", "KOKOM", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "--+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.35": {"text": "Aber h\u00f6re, r\u00fccke n\u00e4her, Herr Magister, im Vertraun,", "tokens": ["A\u00b7ber", "h\u00f6\u00b7re", ",", "r\u00fc\u00b7cke", "n\u00e4\u00b7her", ",", "Herr", "Ma\u00b7gis\u00b7ter", ",", "im", "Ver\u00b7traun", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "VVFIN", "ADJD", "$,", "NN", "NN", "$,", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.36": {"text": "Las uns auch ein gutes Muster deines klugen Flei\u00dfes schaun.", "tokens":