{"textgrid.poem.65827": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "101", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Saumsel'ge Muse! wie wirst du dein Schweigen b\u00fc\u00dfen", "tokens": ["Saum\u00b7sel'\u00b7ge", "Mu\u00b7se", "!", "wie", "wirst", "du", "dein", "Schwei\u00b7gen", "b\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "PWAV", "VAFIN", "PPER", "PPOSAT", "NN", "VVINF"], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}, "line.2": {"text": "Vom Wahren, das im Sch\u00f6nen sich verkl\u00e4rt?", "tokens": ["Vom", "Wah\u00b7ren", ",", "das", "im", "Sch\u00f6\u00b7nen", "sich", "ver\u00b7kl\u00e4rt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "APPRART", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wahrheit und Sch\u00f6nheit sind auf meinen Freund gewiesen;", "tokens": ["Wahr\u00b7heit", "und", "Sch\u00f6n\u00b7heit", "sind", "auf", "mei\u00b7nen", "Freund", "ge\u00b7wie\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.4": {"text": "So bist auch du, und darin ruht dein Wert.", "tokens": ["So", "bist", "auch", "du", ",", "und", "da\u00b7rin", "ruht", "dein", "Wert", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PPER", "$,", "KON", "PAV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Gib Antwort, Muse! sagst du nicht vielleicht:", "tokens": ["Gib", "Ant\u00b7wort", ",", "Mu\u00b7se", "!", "sagst", "du", "nicht", "viel\u00b7leicht", ":"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NN", "$,", "NN", "$.", "VVFIN", "PPER", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "\u00bbwahrheit braucht Schmuck nicht, ihre Farb' ist feste;", "tokens": ["\u00bb", "wahr\u00b7heit", "braucht", "Schmuck", "nicht", ",", "ih\u00b7re", "Fa\u00b7rb'", "ist", "fes\u00b7te", ";"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "NN", "VVFIN", "NN", "PTKNEG", "$,", "PPOSAT", "NN", "VAFIN", "ADJA", "$."], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}, "line.7": {"text": "Sch\u00f6nheit nicht Pinsel, der ihr Wahres zeigt;", "tokens": ["Sch\u00f6n\u00b7heit", "nicht", "Pin\u00b7sel", ",", "der", "ihr", "Wah\u00b7res", "zeigt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.8": {"text": "Am besten ist ganz unvermischt das beste\u00ab?", "tokens": ["Am", "bes\u00b7ten", "ist", "ganz", "un\u00b7ver\u00b7mischt", "das", "bes\u00b7te", "\u00ab", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PTKA", "ADJD", "VAFIN", "ADV", "ADJD", "ART", "ADJA", "$(", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "So willst du schweigen, weil ihm Lob nicht not?", "tokens": ["So", "willst", "du", "schwei\u00b7gen", ",", "weil", "ihm", "Lob", "nicht", "not", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "$,", "KOUS", "PPER", "NN", "PTKNEG", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Entschuld'ge ", "tokens": ["Ent\u00b7schuld'\u00b7ge"], "token_info": ["word"], "pos": ["NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.11": {"text": "Erheben \u00fcber goldnes Grab und Tod.", "tokens": ["Er\u00b7he\u00b7ben", "\u00fc\u00b7ber", "gold\u00b7nes", "Grab", "und", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Da\u00df ihm noch r\u00fchmt die ungeborne Zeit.", "tokens": ["Da\u00df", "ihm", "noch", "r\u00fchmt", "die", "un\u00b7ge\u00b7bor\u00b7ne", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "So, Muse, tu dein Amt! Wie ", "tokens": ["So", ",", "Mu\u00b7se", ",", "tu", "dein", "Amt", "!", "Wie"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ADV", "$,", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$.", "PWAV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Lehr' ich dich sp\u00e4ten Enkeln ihn verk\u00fcnden.", "tokens": ["Lehr'", "ich", "dich", "sp\u00e4\u00b7ten", "En\u00b7keln", "ihn", "ver\u00b7k\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PRF", "ADJA", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}