{"textgrid.poem.28722": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "37.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Schaten pflegt zu stehn, nach dem die Sonne steht;", "tokens": ["Der", "Scha\u00b7ten", "pflegt", "zu", "stehn", ",", "nach", "dem", "die", "Son\u00b7ne", "steht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKZU", "VVINF", "$,", "APPR", "ART", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Niemand ist, wann sie scheint, der ohne Schaten geht;", "tokens": ["Nie\u00b7mand", "ist", ",", "wann", "sie", "scheint", ",", "der", "oh\u00b7ne", "Scha\u00b7ten", "geht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "$,", "PWAV", "PPER", "VVFIN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$."], "meter": "+----+-+-+-+", "measure": "dactylic.init"}, "line.3": {"text": "Niemand ist, dem nicht was von Thorheit folgte bey;", "tokens": ["Nie\u00b7mand", "ist", ",", "dem", "nicht", "was", "von", "Thor\u00b7heit", "folg\u00b7te", "bey", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "$,", "PRELS", "PTKNEG", "PWS", "APPR", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Der, dem der Sinn ist klar, der merckt, wie gro\u00df sie sey.", "tokens": ["Der", ",", "dem", "der", "Sinn", "ist", "klar", ",", "der", "merckt", ",", "wie", "gro\u00df", "sie", "sey", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PRELS", "ART", "NN", "VAFIN", "ADJD", "$,", "PRELS", "VVFIN", "$,", "PWAV", "ADJD", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}