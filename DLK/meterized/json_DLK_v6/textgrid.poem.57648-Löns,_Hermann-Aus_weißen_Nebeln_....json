{"textgrid.poem.57648": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Aus wei\u00dfen Nebeln ...", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus wei\u00dfen Nebeln tauchen schwarze B\u00e4ume,", "tokens": ["Aus", "wei\u00b7\u00dfen", "Ne\u00b7beln", "tau\u00b7chen", "schwar\u00b7ze", "B\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "An kahlen B\u00fcschen perlt der Silbertau,", "tokens": ["An", "kah\u00b7len", "B\u00fc\u00b7schen", "perlt", "der", "Sil\u00b7ber\u00b7tau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Aus blauen W\u00e4ldern flie\u00dfen rote Gluten,", "tokens": ["Aus", "blau\u00b7en", "W\u00e4l\u00b7dern", "flie\u00b7\u00dfen", "ro\u00b7te", "Glu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Sonne kommt, die Luft weht scharf und rauh.", "tokens": ["Die", "Son\u00b7ne", "kommt", ",", "die", "Luft", "weht", "scharf", "und", "rauh", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Das wird ein sch\u00f6ner Tag, sch\u00f6n wie die Liebe,", "tokens": ["Das", "wird", "ein", "sch\u00f6\u00b7ner", "Tag", ",", "sch\u00f6n", "wie", "die", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Liebe einer strengen, stolzen Frau.", "tokens": ["Die", "Lie\u00b7be", "ei\u00b7ner", "stren\u00b7gen", ",", "stol\u00b7zen", "Frau", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So reich an Helligkeit und Glut und Wonne,", "tokens": ["So", "reich", "an", "Hel\u00b7lig\u00b7keit", "und", "Glut", "und", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und gab sie sich auch erst so kalt und rauh.", "tokens": ["Und", "gab", "sie", "sich", "auch", "erst", "so", "kalt", "und", "rauh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "ADV", "ADV", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Der Nebel f\u00e4llt als feiner Regen nieder,", "tokens": ["Der", "Ne\u00b7bel", "f\u00e4llt", "als", "fei\u00b7ner", "Re\u00b7gen", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KOKOM", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Luft wird wieder dick und grau und schwer,", "tokens": ["Die", "Luft", "wird", "wie\u00b7der", "dick", "und", "grau", "und", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Sonne kommt nun heute nicht mehr wieder,", "tokens": ["Die", "Son\u00b7ne", "kommt", "nun", "heu\u00b7te", "nicht", "mehr", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "PTKNEG", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mein Herz bleibt wieder taub und kahl und leer.", "tokens": ["Mein", "Herz", "bleibt", "wie\u00b7der", "taub", "und", "kahl", "und", "leer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "ADJD", "KON", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}