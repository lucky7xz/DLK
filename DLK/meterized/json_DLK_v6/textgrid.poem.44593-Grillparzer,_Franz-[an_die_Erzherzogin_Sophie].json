{"textgrid.poem.44593": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "[an die Erzherzogin Sophie]", "genre": "verse", "period": "N.A.", "pub_year": 1852, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Schmerzerinnrung r\u00fcckzuf\u00fchren,", "tokens": ["Die", "Schmer\u00b7ze\u00b7rinn\u00b7rung", "r\u00fcck\u00b7zu\u00b7f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Scheint Ungeschick und H\u00e4rte fast,", "tokens": ["Scheint", "Un\u00b7ge\u00b7schick", "und", "H\u00e4r\u00b7te", "fast", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch in das Graun, ihn zu verlieren,", "tokens": ["Doch", "in", "das", "Graun", ",", "ihn", "zu", "ver\u00b7lie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mischt sich der Trost, da\u00df du ihn hast.", "tokens": ["Mischt", "sich", "der", "Trost", ",", "da\u00df", "du", "ihn", "hast", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "$,", "KOUS", "PPER", "PPER", "VAFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Und nicht nur du, die V\u00f6lkerrunde,", "tokens": ["Und", "nicht", "nur", "du", ",", "die", "V\u00f6l\u00b7ker\u00b7run\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADV", "PPER", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Gott durch dich ihn zugeteilt,", "tokens": ["Der", "Gott", "durch", "dich", "ihn", "zu\u00b7ge\u00b7teilt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie blutete mit ihm aus einer Wunde", "tokens": ["Sie", "blu\u00b7te\u00b7te", "mit", "ihm", "aus", "ei\u00b7ner", "Wun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und sie sind heil, weil er geheilt.", "tokens": ["Und", "sie", "sind", "heil", ",", "weil", "er", "ge\u00b7heilt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "$,", "KOUS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Es z\u00e4hlt der Herr das Haar auf unserm Haupte,", "tokens": ["Es", "z\u00e4hlt", "der", "Herr", "das", "Haar", "auf", "un\u00b7serm", "Haup\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch z\u00e4hlt er nicht nur, er bewacht.", "tokens": ["Doch", "z\u00e4hlt", "er", "nicht", "nur", ",", "er", "be\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "ADV", "$,", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Erst wenn man sich am Abgrund glaubte,", "tokens": ["Erst", "wenn", "man", "sich", "am", "Ab\u00b7grund", "glaub\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PIS", "PRF", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fchlt man als Liebe seine Macht.", "tokens": ["F\u00fchlt", "man", "als", "Lie\u00b7be", "sei\u00b7ne", "Macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "KOKOM", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}