{"textgrid.poem.32398": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "76. Entschuldigung", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So wahr ich lebe, Freund, ich wollte ganze Tage", "tokens": ["So", "wahr", "ich", "le\u00b7be", ",", "Freund", ",", "ich", "woll\u00b7te", "gan\u00b7ze", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "PPER", "VVFIN", "$,", "NN", "$,", "PPER", "VMFIN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und ganze N\u00e4chte bei dir sein:", "tokens": ["Und", "gan\u00b7ze", "N\u00e4ch\u00b7te", "bei", "dir", "sein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "PPER", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Um mich mit dir die ganzen Tage,", "tokens": ["Um", "mich", "mit", "dir", "die", "gan\u00b7zen", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PRF", "APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die ganzen N\u00e4chte zu erfreun.", "tokens": ["Die", "gan\u00b7zen", "N\u00e4ch\u00b7te", "zu", "er\u00b7freun", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch tausend Schritte sinds, die unsre Wohnung trennen;", "tokens": ["Doch", "tau\u00b7send", "Schrit\u00b7te", "sinds", ",", "die", "uns\u00b7re", "Woh\u00b7nung", "tren\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "VAFIN", "$,", "PRELS", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und hundert wohl noch oben drein.", "tokens": ["Und", "hun\u00b7dert", "wohl", "noch", "o\u00b7ben", "drein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "ADV", "ADV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und wollt' ich sie auch gern, die tausend Schritte, rennen,", "tokens": ["Und", "wollt'", "ich", "sie", "auch", "gern", ",", "die", "tau\u00b7send", "Schrit\u00b7te", ",", "ren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PPER", "ADV", "ADV", "$,", "PRELS", "CARD", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und jene hundert oben drein:", "tokens": ["Und", "je\u00b7ne", "hun\u00b7dert", "o\u00b7ben", "drein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "So wei\u00df ich doch, da\u00df ich am Ende", "tokens": ["So", "wei\u00df", "ich", "doch", ",", "da\u00df", "ich", "am", "En\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Des langen Wegs, dich zwanzigmal nicht f\u00e4nde.", "tokens": ["Des", "lan\u00b7gen", "Wegs", ",", "dich", "zwan\u00b7zig\u00b7mal", "nicht", "f\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PPER", "ADV", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Denn \u00f6fters bist du nicht zu Hause,", "tokens": ["Denn", "\u00f6f\u00b7ters", "bist", "du", "nicht", "zu", "Hau\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "PTKNEG", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Und manchmal bist du's nicht f\u00fcr mich:", "tokens": ["Und", "manch\u00b7mal", "bist", "du's", "nicht", "f\u00fcr", "mich", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PIS", "PTKNEG", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Wenn nach dem langen Zirkelschmause", "tokens": ["Wenn", "nach", "dem", "lan\u00b7gen", "Zir\u00b7kelsc\u00b7hmau\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Der kleinste Gast dir hinderlich.", "tokens": ["Der", "kleins\u00b7te", "Gast", "dir", "hin\u00b7der\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Ich wollte, wie gesagt, gern tausend Schritte rennen,", "tokens": ["Ich", "woll\u00b7te", ",", "wie", "ge\u00b7sagt", ",", "gern", "tau\u00b7send", "Schrit\u00b7te", "ren\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PWAV", "VVPP", "$,", "ADV", "CARD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Dich, liebster Freund, dich sehn zu k\u00f6nnen:", "tokens": ["Dich", ",", "liebs\u00b7ter", "Freund", ",", "dich", "sehn", "zu", "k\u00f6n\u00b7nen", ":"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJA", "NN", "$,", "PRF", "VVINF", "PTKZU", "VMINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Doch, allzu weiter Freund, dich nicht zu sehn,", "tokens": ["Doch", ",", "all\u00b7zu", "wei\u00b7ter", "Freund", ",", "dich", "nicht", "zu", "sehn", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PTKA", "ADV", "NN", "$,", "PPER", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Verdreu\u00dft michs, Einen nur zu gehn.", "tokens": ["Ver\u00b7dreu\u00dft", "michs", ",", "Ei\u00b7nen", "nur", "zu", "gehn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "ART", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}