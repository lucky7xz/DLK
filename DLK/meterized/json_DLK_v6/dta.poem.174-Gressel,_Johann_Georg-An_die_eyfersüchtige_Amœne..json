{"dta.poem.174": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "An die eyfers\u00fcchtige  Am\u0153ne.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Ich meyns so b\u00f6se nicht/ annehmliche Am&#339;ne,", "tokens": ["Ich", "meyns", "so", "b\u00f6\u00b7se", "nicht", "/", "an\u00b7nehm\u00b7li\u00b7che", "Am", "&#339;", "ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "XML_entity", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "PTKNEG", "$(", "FM", "FM", "FM", "FM", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wenn bey ", "tokens": ["Wenn", "bey"], "token_info": ["word", "word"], "pos": ["KOUS", "APPR"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Jhr geht doch/ wie ihr wi\u00dft/ ", "tokens": ["Ihr", "geht", "doch", "/", "wie", "ihr", "wi\u00dft", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$(", "PWAV", "PPER", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "So weit die g\u00fcldne Sonn dem Mond nimmt seine Zier.", "tokens": ["So", "weit", "die", "g\u00fcld\u00b7ne", "Sonn", "dem", "Mond", "nimmt", "sei\u00b7ne", "Zier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich fliehe nun fortan die schmeichelnde ", "tokens": ["Ich", "flie\u00b7he", "nun", "for\u00b7tan", "die", "schmei\u00b7cheln\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ART", "ADJA"], "meter": "-+--+--+--", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Ich schlag ver\u00e4chtlich aus ihr sch\u00e4dlichs Lust-Geth\u00f6ne", "tokens": ["Ich", "schlag", "ver\u00b7\u00e4cht\u00b7lich", "aus", "ihr", "sch\u00e4d\u00b7lichs", "Lust\u00b7Get\u00b7h\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ob ich dem Ansehn gleich mich hefftig nach ihr sehne", "tokens": ["Ob", "ich", "dem", "An\u00b7sehn", "gleich", "mich", "heff\u00b7tig", "nach", "ihr", "seh\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "PPER", "ADJD", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und tausend mahl veracht\u2019 eur Sch\u00f6n-seyn gleich bey ihr", "tokens": ["Und", "tau\u00b7send", "mahl", "ver\u00b7acht'", "eur", "Sch\u00f6n\u00b7seyn", "gleich", "bey", "ihr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "CARD", "ADV", "VVFIN", "PPOSAT", "NN", "ADV", "APPR", "PPOSAT"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.9": {"text": "Ich meyns so b\u00f6se nicht.", "tokens": ["Ich", "meyns", "so", "b\u00f6\u00b7se", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Nenn ich ", "tokens": ["Nenn", "ich"], "token_info": ["word", "word"], "pos": ["KOUS", "PPER"], "meter": "+-", "measure": "trochaic.single"}, "line.11": {"text": "Glaubt mir da\u00df ich den Mohr mit leichten Firni\u00df kr\u00f6hne", "tokens": ["Glaubt", "mir", "da\u00df", "ich", "den", "Mohr", "mit", "leich\u00b7ten", "Fir\u00b7ni\u00df", "kr\u00f6h\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "KOUS", "PPER", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Mein Lob verbessert nicht das schwartze Murmel-Thier", "tokens": ["Mein", "Lob", "ver\u00b7bes\u00b7sert", "nicht", "das", "schwart\u00b7ze", "Mur\u00b7mel\u00b7Thier"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKNEG", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Drum z\u00fcrnt ", "tokens": ["Drum", "z\u00fcrnt"], "token_info": ["word", "word"], "pos": ["PAV", "VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.14": {"text": "Wenn bey ", "tokens": ["Wenn", "bey"], "token_info": ["word", "word"], "pos": ["KOUS", "APPR"], "meter": "+-", "measure": "trochaic.single"}, "line.15": {"text": "Ich meyns so b\u00f6se nicht.", "tokens": ["Ich", "meyns", "so", "b\u00f6\u00b7se", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}