{"textgrid.poem.29108": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "23.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Di\u00df und jenes schneidt man auff von der Hochzeit ersten Nacht.", "tokens": ["Di\u00df", "und", "je\u00b7nes", "schneidt", "man", "auff", "von", "der", "Hoch\u00b7zeit", "ers\u00b7ten", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "KON", "PDS", "VVFIN", "PIS", "APPR", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Mich, sagt Elsa, schreckt es nicht, werde br\u00fcnstig nur gemacht,", "tokens": ["Mich", ",", "sagt", "El\u00b7sa", ",", "schreckt", "es", "nicht", ",", "wer\u00b7de", "br\u00fcns\u00b7tig", "nur", "ge\u00b7macht", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "VVFIN", "NE", "$,", "VVFIN", "PPER", "PTKNEG", "$,", "VAFIN", "ADJD", "ADV", "VVPP", "$,"], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Unter Augen dem zu gehn, was mir letzlich kummen soll;", "tokens": ["Un\u00b7ter", "Au\u00b7gen", "dem", "zu", "gehn", ",", "was", "mir", "letz\u00b7lich", "kum\u00b7men", "soll", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "PTKZU", "VVINF", "$,", "PWS", "PPER", "ADV", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Der, was ihm verordnet ist, fliehen wil, der thut nicht wol.", "tokens": ["Der", ",", "was", "ihm", "ver\u00b7ord\u00b7net", "ist", ",", "flie\u00b7hen", "wil", ",", "der", "thut", "nicht", "wol", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PWS", "PPER", "VVPP", "VAFIN", "$,", "VVINF", "VMFIN", "$,", "PRELS", "VVFIN", "PTKNEG", "ADV", "$."], "meter": "-+--+--+-+-+-+", "measure": "amphibrach.tri.plus"}}}}}