{"textgrid.poem.50093": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Denckt nicht, dass Salomon ein ", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Denckt nicht, dass Salomon ein ", "tokens": ["Denckt", "nicht", ",", "dass", "Sa\u00b7lo\u00b7mon", "ein"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "$,", "KOUS", "NE", "ART"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und nehmt es ", "tokens": ["Und", "nehmt", "es"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Dass ein ", "tokens": ["Dass", "ein"], "token_info": ["word", "word"], "pos": ["KOUS", "ART"], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Das Urtheil ist des Richters werth,", "tokens": ["Das", "Ur\u00b7theil", "ist", "des", "Rich\u00b7ters", "werth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und man sieht leichtlich, dass sein Schwerdt", "tokens": ["Und", "man", "sieht", "leicht\u00b7lich", ",", "dass", "sein", "Schwerdt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "ADJD", "$,", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}