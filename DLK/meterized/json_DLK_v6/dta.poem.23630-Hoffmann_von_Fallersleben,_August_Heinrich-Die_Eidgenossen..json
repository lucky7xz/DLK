{"dta.poem.23630": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Eidgenossen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es war einmal ein arm Schulmeisterlein,", "tokens": ["Es", "war", "ein\u00b7mal", "ein", "arm", "Schul\u00b7meis\u00b7ter\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "ADJD", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der wollt' in seinem Lohn verbessert sein.", "tokens": ["Der", "wollt'", "in", "sei\u00b7nem", "Lohn", "ver\u00b7bes\u00b7sert", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "APPR", "PPOSAT", "NN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch war sein Dorf nur klein und, Gott erbarm!", "tokens": ["Doch", "war", "sein", "Dorf", "nur", "klein", "und", ",", "Gott", "er\u00b7barm", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "ADV", "ADJD", "KON", "$,", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Bauern waren alle gar zu arm.", "tokens": ["Die", "Bau\u00b7ern", "wa\u00b7ren", "al\u00b7le", "gar", "zu", "arm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIS", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Drum ging zum reichen Dorf der arme Mann!", "tokens": ["Drum", "ging", "zum", "rei\u00b7chen", "Dorf", "der", "ar\u00b7me", "Mann", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "APPRART", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Trug dort den Bauern seine Dienste an.", "tokens": ["Trug", "dort", "den", "Bau\u00b7ern", "sei\u00b7ne", "Diens\u00b7te", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.2": {"line.1": {"text": "Er pries den Leuten seine T\u00fcchtigkeit,", "tokens": ["Er", "pries", "den", "Leu\u00b7ten", "sei\u00b7ne", "T\u00fcch\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Auch k\u00f6nn' er Wetter machen jederzeit.", "tokens": ["Auch", "k\u00f6nn'", "er", "Wet\u00b7ter", "ma\u00b7chen", "je\u00b7der\u00b7zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da sprachen sie: das ist f\u00fcr uns der Mann!", "tokens": ["Da", "spra\u00b7chen", "sie", ":", "das", "ist", "f\u00fcr", "uns", "der", "Mann", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "PDS", "VAFIN", "APPR", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und nahmen ihn sogleich zum Me\u00dfner an.", "tokens": ["Und", "nah\u00b7men", "ihn", "sog\u00b7leich", "zum", "Me\u00df\u00b7ner", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Doch blieb das Wetter immer wie es war,", "tokens": ["Doch", "blieb", "das", "Wet\u00b7ter", "im\u00b7mer", "wie", "es", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "KOKOM", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Heut neblicht, regnicht, morgen hell und klar.", "tokens": ["Heut", "neb\u00b7licht", ",", "reg\u00b7nicht", ",", "mor\u00b7gen", "hell", "und", "klar", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "VVFIN", "$,", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Da sagten sie: ist das nun unser Lohn?", "tokens": ["Da", "sag\u00b7ten", "sie", ":", "ist", "das", "nun", "un\u00b7ser", "Lohn", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "VAFIN", "PDS", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Solch Wetter hatten wir ja immer schon.", "tokens": ["Solch", "Wet\u00b7ter", "hat\u00b7ten", "wir", "ja", "im\u00b7mer", "schon", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ja, sprach er, ja, sobald ihr einig seid,", "tokens": ["Ja", ",", "sprach", "er", ",", "ja", ",", "so\u00b7bald", "ihr", "ei\u00b7nig", "seid", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "PPER", "$,", "PTKANT", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Bin ich zum Wettermachen gleich bereit.", "tokens": ["Bin", "ich", "zum", "Wet\u00b7ter\u00b7ma\u00b7chen", "gleich", "be\u00b7reit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Doch war von Einigkeit nicht eine Spur,", "tokens": ["Doch", "war", "von", "Ei\u00b7nig\u00b7keit", "nicht", "ei\u00b7ne", "Spur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "NN", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Denn jeder wollte stets ", "tokens": ["Denn", "je\u00b7der", "woll\u00b7te", "stets"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIS", "VMFIN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}