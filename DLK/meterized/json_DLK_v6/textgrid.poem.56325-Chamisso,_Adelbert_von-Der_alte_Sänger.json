{"textgrid.poem.56325": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "Der alte S\u00e4nger", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sang der sonderbare Greise", "tokens": ["Sang", "der", "son\u00b7der\u00b7ba\u00b7re", "Grei\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf den M\u00e4rkten, Stra\u00dfen, Gassen", "tokens": ["Auf", "den", "M\u00e4rk\u00b7ten", ",", "Stra\u00b7\u00dfen", ",", "Gas\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["APPR", "ART", "NN", "$,", "NN", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gellend, z\u00fcrnend seine Weise:", "tokens": ["Gel\u00b7lend", ",", "z\u00fcr\u00b7nend", "sei\u00b7ne", "Wei\u00b7se", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADJD", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00bbbin, der in die W\u00fcste schreit.", "tokens": ["\u00bb", "bin", ",", "der", "in", "die", "W\u00fcs\u00b7te", "schreit", "."], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Langsam, langsam und gelassen!", "tokens": ["Lang\u00b7sam", ",", "lang\u00b7sam", "und", "ge\u00b7las\u00b7sen", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "KON", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nichts unzeitig! nichts gewaltsam!", "tokens": ["Nichts", "un\u00b7zei\u00b7tig", "!", "nichts", "ge\u00b7walt\u00b7sam", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "ADJD", "$.", "PIS", "ADJD", "$."], "meter": "++--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Unabl\u00e4ssig, unaufhaltsam,", "tokens": ["Un\u00b7ab\u00b7l\u00e4s\u00b7sig", ",", "un\u00b7auf\u00b7halt\u00b7sam", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Allgewaltig naht die Zeit.", "tokens": ["All\u00b7ge\u00b7wal\u00b7tig", "naht", "die", "Zeit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Torenwerk, ihr wilden Knaben,", "tokens": ["To\u00b7ren\u00b7werk", ",", "ihr", "wil\u00b7den", "Kna\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "An dem Baum der Zeit zu r\u00fctteln,", "tokens": ["An", "dem", "Baum", "der", "Zeit", "zu", "r\u00fct\u00b7teln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Seine Last ihm abzustreifen,", "tokens": ["Sei\u00b7ne", "Last", "ihm", "ab\u00b7zu\u00b7strei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VVIZU", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wann er erst mit Bl\u00fcten prangt!", "tokens": ["Wann", "er", "erst", "mit", "Bl\u00fc\u00b7ten", "prangt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "La\u00dft ihn seine Fr\u00fcchte reifen", "tokens": ["La\u00dft", "ihn", "sei\u00b7ne", "Fr\u00fcch\u00b7te", "rei\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und den Wind die \u00c4ste sch\u00fctteln,", "tokens": ["Und", "den", "Wind", "die", "\u00c4s\u00b7te", "sch\u00fct\u00b7teln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Selber bringt er euch die Gaben,", "tokens": ["Sel\u00b7ber", "bringt", "er", "euch", "die", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Die ihr ungest\u00fcm verlangt.\u00ab", "tokens": ["Die", "ihr", "un\u00b7ge\u00b7st\u00fcm", "ver\u00b7langt", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "PPER", "ADJD", "VVPP", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und die aufgeregte Menge", "tokens": ["Und", "die", "auf\u00b7ge\u00b7reg\u00b7te", "Men\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zischt und schm\u00e4ht den alten S\u00e4nger:", "tokens": ["Zischt", "und", "schm\u00e4ht", "den", "al\u00b7ten", "S\u00e4n\u00b7ger", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bblohnt ihm seine Schmachges\u00e4nge!", "tokens": ["\u00bb", "lohnt", "ihm", "sei\u00b7ne", "Schmach\u00b7ge\u00b7s\u00e4n\u00b7ge", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tragt ihm seine Lieder nach!", "tokens": ["Tragt", "ihm", "sei\u00b7ne", "Lie\u00b7der", "nach", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dulden wir den Knecht noch l\u00e4nger?", "tokens": ["Dul\u00b7den", "wir", "den", "Knecht", "noch", "l\u00e4n\u00b7ger", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "ADV", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Werfet, werfet ihn mit Steinen!", "tokens": ["Wer\u00b7fet", ",", "wer\u00b7fet", "ihn", "mit", "Stei\u00b7nen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Ausgesto\u00dfen von den Reinen", "tokens": ["Aus\u00b7ge\u00b7sto\u00b7\u00dfen", "von", "den", "Rei\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Treff ihn aller Orten Schmach!\u00ab", "tokens": ["Treff", "ihn", "al\u00b7ler", "Or\u00b7ten", "Schmach", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Sang der sonderbare Greise", "tokens": ["Sang", "der", "son\u00b7der\u00b7ba\u00b7re", "Grei\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In den k\u00f6niglichen Hallen", "tokens": ["In", "den", "k\u00f6\u00b7nig\u00b7li\u00b7chen", "Hal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.3": {"text": "Gellend, z\u00fcrnend seine Weise:", "tokens": ["Gel\u00b7lend", ",", "z\u00fcr\u00b7nend", "sei\u00b7ne", "Wei\u00b7se", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADJD", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00bbbin, der in die W\u00fcste schreit.", "tokens": ["\u00bb", "bin", ",", "der", "in", "die", "W\u00fcs\u00b7te", "schreit", "."], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Vorw\u00e4rts! vorw\u00e4rts! nimmer l\u00e4ssig!", "tokens": ["Vor\u00b7w\u00e4rts", "!", "vor\u00b7w\u00e4rts", "!", "nim\u00b7mer", "l\u00e4s\u00b7sig", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$.", "PTKVZ", "$.", "ADV", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nimmer zaghaft! k\u00fchn vor allen!", "tokens": ["Nim\u00b7mer", "zag\u00b7haft", "!", "k\u00fchn", "vor", "al\u00b7len", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$.", "ADJD", "APPR", "PIAT", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Unaufhaltsam, unabl\u00e4ssig,", "tokens": ["Un\u00b7auf\u00b7halt\u00b7sam", ",", "un\u00b7ab\u00b7l\u00e4s\u00b7sig", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Allgewaltig dr\u00e4ngt die Zeit.", "tokens": ["All\u00b7ge\u00b7wal\u00b7tig", "dr\u00e4ngt", "die", "Zeit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Mit dem Strom und vor dem Winde!", "tokens": ["Mit", "dem", "Strom", "und", "vor", "dem", "Win\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mache dir, dich stark zu zeigen,", "tokens": ["Ma\u00b7che", "dir", ",", "dich", "stark", "zu", "zei\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PRF", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Strom- und Windeskraft zu eigen!", "tokens": ["Strom", "und", "Win\u00b7des\u00b7kraft", "zu", "ei\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["TRUNC", "KON", "NN", "PTKA", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wider beide, g\u00e4hnt dein Grab.", "tokens": ["Wi\u00b7der", "bei\u00b7de", ",", "g\u00e4hnt", "dein", "Grab", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Steure k\u00fchn in grader Richtung!", "tokens": ["Steu\u00b7re", "k\u00fchn", "in", "gra\u00b7der", "Rich\u00b7tung", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Klippen dort? die Furt nur finde!", "tokens": ["Klip\u00b7pen", "dort", "?", "die", "Furt", "nur", "fin\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$.", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Umzulenken heischt Vernichtung;", "tokens": ["Um\u00b7zu\u00b7len\u00b7ken", "heischt", "Ver\u00b7nich\u00b7tung", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Treibst als Wrak du doch hinab.\u00ab", "tokens": ["Treibst", "als", "Wrak", "du", "doch", "hin\u00b7ab", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "KOKOM", "VVFIN", "PPER", "ADV", "PTKVZ", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Einen sah man da erschrocken", "tokens": ["Ei\u00b7nen", "sah", "man", "da", "er\u00b7schro\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "VVFIN", "PIS", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bald err\u00f6ten, bald erblassen;", "tokens": ["Bald", "er\u00b7r\u00f6\u00b7ten", ",", "bald", "er\u00b7blas\u00b7sen", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbwer hat ihn herein gelassen,", "tokens": ["\u00bb", "wer", "hat", "ihn", "her\u00b7ein", "ge\u00b7las\u00b7sen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VAFIN", "PPER", "PTKVZ", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dessen Stimme zu uns drang?", "tokens": ["Des\u00b7sen", "Stim\u00b7me", "zu", "uns", "drang", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wahnsinn spricht aus diesem Alten;", "tokens": ["Wahn\u00b7sinn", "spricht", "aus", "die\u00b7sem", "Al\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PDAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Soll er uns das Volk verlocken?", "tokens": ["Soll", "er", "uns", "das", "Volk", "ver\u00b7lo\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Sorgt den Toren festzuhalten,", "tokens": ["Sorgt", "den", "To\u00b7ren", "fest\u00b7zu\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "La\u00dft verstummen den Gesang.\u00ab", "tokens": ["La\u00dft", "ver\u00b7stum\u00b7men", "den", "Ge\u00b7sang", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VVIMP", "VVFIN", "ART", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Sang der sonderbare Greise", "tokens": ["Sang", "der", "son\u00b7der\u00b7ba\u00b7re", "Grei\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Immer noch im finstern Turme", "tokens": ["Im\u00b7mer", "noch", "im", "fins\u00b7tern", "Tur\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ruhig, heiter seine Weise:", "tokens": ["Ru\u00b7hig", ",", "hei\u00b7ter", "sei\u00b7ne", "Wei\u00b7se", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00bbbin, der in die W\u00fcste schreit.", "tokens": ["\u00bb", "bin", ",", "der", "in", "die", "W\u00fcs\u00b7te", "schreit", "."], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Schreien mu\u00dft ich es dem Sturme;", "tokens": ["Schrei\u00b7en", "mu\u00dft", "ich", "es", "dem", "Stur\u00b7me", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Der Propheten Lohn erhalt ich!", "tokens": ["Der", "Pro\u00b7phe\u00b7ten", "Lohn", "er\u00b7halt", "ich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Unabl\u00e4ssig, allgewaltig,", "tokens": ["Un\u00b7ab\u00b7l\u00e4s\u00b7sig", ",", "all\u00b7ge\u00b7wal\u00b7tig", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Unaufhaltsam naht die Zeit.\u00ab", "tokens": ["Un\u00b7auf\u00b7halt\u00b7sam", "naht", "die", "Zeit", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}