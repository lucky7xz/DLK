{"textgrid.poem.53706": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Verfassungstag", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Guck hin\u00fcber, guck her\u00fcber,", "tokens": ["Guck", "hin\u00b7\u00fc\u00b7ber", ",", "guck", "her\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "VVIMP", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": " wohl \u00fcber die Stra\u00df hinum.", "tokens": ["wohl", "\u00fc\u00b7ber", "die", "Stra\u00df", "hi\u00b7num", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": " kann Deutschland nicht finden;", "tokens": ["kann", "Deutschland", "nicht", "fin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NE", "PTKNEG", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": " rutsch alleweil drauf rum.", "tokens": ["rutsch", "al\u00b7le\u00b7weil", "drauf", "rum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "PAV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}