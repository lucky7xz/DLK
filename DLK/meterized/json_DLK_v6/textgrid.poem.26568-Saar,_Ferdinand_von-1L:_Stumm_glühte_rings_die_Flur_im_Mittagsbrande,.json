{"textgrid.poem.26568": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Stumm gl\u00fchte rings die Flur im Mittagsbrande,", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Stumm gl\u00fchte rings die Flur im Mittagsbrande,", "tokens": ["Stumm", "gl\u00fch\u00b7te", "rings", "die", "Flur", "im", "Mit\u00b7tags\u00b7bran\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ADV", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nur flei\u00df'ge Bienen summten durch die Schw\u00fcle,", "tokens": ["Nur", "flei\u00df'\u00b7ge", "Bie\u00b7nen", "summ\u00b7ten", "durch", "die", "Schw\u00fc\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Als sinnend wir, an stiller Gr\u00e4ber Rande,", "tokens": ["Als", "sin\u00b7nend", "wir", ",", "an", "stil\u00b7ler", "Gr\u00e4\u00b7ber", "Ran\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPER", "$,", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Durchschritten eines Friedhofs Schattenk\u00fchle.", "tokens": ["Durch\u00b7schrit\u00b7ten", "ei\u00b7nes", "Fried\u00b7hofs", "Schat\u00b7ten\u00b7k\u00fch\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die holden Gleichnisse der Erdendinge,", "tokens": ["Die", "hol\u00b7den", "Gleich\u00b7nis\u00b7se", "der", "Er\u00b7den\u00b7din\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Blumen, sah'n wir auf den H\u00fcgeln beben,", "tokens": ["Die", "Blu\u00b7men", ",", "sah'n", "wir", "auf", "den", "H\u00fc\u00b7geln", "be\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und d'r\u00fcber hin, mit kaum bewegter Schwinge,", "tokens": ["Und", "d'\u00b7r\u00fcber", "hin", ",", "mit", "kaum", "be\u00b7weg\u00b7ter", "Schwin\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "PTKVZ", "$,", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie traumverloren m\u00fcde Falter schweben.", "tokens": ["Wie", "traum\u00b7ver\u00b7lo\u00b7ren", "m\u00fc\u00b7de", "Fal\u00b7ter", "schwe\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADJD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und leichte Schauer rieselten und wehten,", "tokens": ["Und", "leich\u00b7te", "Schau\u00b7er", "rie\u00b7sel\u00b7ten", "und", "weh\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Des gro\u00dfen R\u00e4thsels unsichtbare Boten;", "tokens": ["Des", "gro\u00b7\u00dfen", "R\u00e4th\u00b7sels", "un\u00b7sicht\u00b7ba\u00b7re", "Bo\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Wir falteten die H\u00e4nde wie zum Beten,", "tokens": ["Wir", "fal\u00b7te\u00b7ten", "die", "H\u00e4n\u00b7de", "wie", "zum", "Be\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KOKOM", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit sanfter Wehmuth denkend an die Todten.", "tokens": ["Mit", "sanf\u00b7ter", "Weh\u00b7muth", "den\u00b7kend", "an", "die", "Tod\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Es war, als glitten leise wir hin\u00fcber", "tokens": ["Es", "war", ",", "als", "glit\u00b7ten", "lei\u00b7se", "wir", "hin\u00b7\u00fc\u00b7ber"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "VVFIN", "ADJD", "PPER", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zu Jenen, die schon l\u00e4ngst dahin gegangen;", "tokens": ["Zu", "Je\u00b7nen", ",", "die", "schon", "l\u00e4ngst", "da\u00b7hin", "ge\u00b7gan\u00b7gen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "PRELS", "ADV", "ADV", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der bleiche Engel zog an uns vor\u00fcber \u2013", "tokens": ["Der", "blei\u00b7che", "En\u00b7gel", "zog", "an", "uns", "vor\u00b7\u00fc\u00b7ber", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PPER", "PTKVZ", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wir h\u00f6rten fast, wie seine Fl\u00fcgel klangen.", "tokens": ["Wir", "h\u00f6r\u00b7ten", "fast", ",", "wie", "sei\u00b7ne", "Fl\u00fc\u00b7gel", "klan\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PWAV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Doch als wir jetzt, nach ihm empor zu schauen,", "tokens": ["Doch", "als", "wir", "jetzt", ",", "nach", "ihm", "em\u00b7por", "zu", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "$,", "APPR", "PPER", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit sanftem Druck uns aneinander schlossen,", "tokens": ["Mit", "sanf\u00b7tem", "Druck", "uns", "an\u00b7ein\u00b7an\u00b7der", "schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da f\u00fchlten wir, versenkt in Todesgrauen,", "tokens": ["Da", "f\u00fchl\u00b7ten", "wir", ",", "ver\u00b7senkt", "in", "To\u00b7des\u00b7grau\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Uns pl\u00f6tzlich von dem w\u00e4rmsten Hauch durchflossen.", "tokens": ["Uns", "pl\u00f6tz\u00b7lich", "von", "dem", "w\u00e4rms\u00b7ten", "Hauch", "durch\u00b7flos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Durch uns're Liebe, die wir scheu vergessen,", "tokens": ["Durch", "un\u00b7s'\u00b7re", "Lie\u00b7be", ",", "die", "wir", "scheu", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PRELS", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Dem Dasein wieder ganz zur\u00fcckgegeben,", "tokens": ["Dem", "Da\u00b7sein", "wie\u00b7der", "ganz", "zu\u00b7r\u00fcck\u00b7ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Erwachten wir im Dunkel der Cypressen", "tokens": ["Er\u00b7wach\u00b7ten", "wir", "im", "Dun\u00b7kel", "der", "Cyp\u00b7res\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und hielten uns und k\u00fc\u00dften uns mit Beben.", "tokens": ["Und", "hiel\u00b7ten", "uns", "und", "k\u00fc\u00df\u00b7ten", "uns", "mit", "Be\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "KON", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Nun schien ein Opfer uns die stille Stunde,", "tokens": ["Nun", "schien", "ein", "Op\u00b7fer", "uns", "die", "stil\u00b7le", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dem Tod gebracht, und uns're Wangen gl\u00fchten,", "tokens": ["Dem", "Tod", "ge\u00b7bracht", ",", "und", "un\u00b7s'\u00b7re", "Wan\u00b7gen", "gl\u00fch\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "KON", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "So wie die Rosen, die, mit uns im Bunde,", "tokens": ["So", "wie", "die", "Ro\u00b7sen", ",", "die", ",", "mit", "uns", "im", "Bun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "$,", "PRELS", "$,", "APPR", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auf Gr\u00e4bern ihren s\u00fc\u00dfen Duft verspr\u00fchten.", "tokens": ["Auf", "Gr\u00e4\u00b7bern", "ih\u00b7ren", "s\u00fc\u00b7\u00dfen", "Duft", "ver\u00b7spr\u00fch\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}