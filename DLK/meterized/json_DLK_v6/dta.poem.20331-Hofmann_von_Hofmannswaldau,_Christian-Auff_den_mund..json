{"dta.poem.20331": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Auff den mund.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mund! der die seelen kan durch lust zusammen hetzen/", "tokens": ["Mund", "!", "der", "die", "see\u00b7len", "kan", "durch", "lust", "zu\u00b7sam\u00b7men", "het\u00b7zen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "ART", "VVINF", "VMFIN", "APPR", "VVFIN", "ADV", "VVINF", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Mund! der viel s\u00fcsser ist als starcker himmels-wein/", "tokens": ["Mund", "!", "der", "viel", "s\u00fcs\u00b7ser", "ist", "als", "star\u00b7cker", "him\u00b7mels\u00b7wein", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "ADV", "ADJD", "VAFIN", "KOKOM", "ADJA", "NN", "$("], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Mund! der du alakant des lebens schenckest ein/", "tokens": ["Mund", "!", "der", "du", "a\u00b7la\u00b7kant", "des", "le\u00b7bens", "schen\u00b7ckest", "ein", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "PPER", "ADJD", "ART", "NN", "VVFIN", "ART", "$("], "meter": "+--+-+--+--+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Mund! den ich vorziehn mu\u00df der Inden reichen sch\u00e4tzen/", "tokens": ["Mund", "!", "den", "ich", "vor\u00b7ziehn", "mu\u00df", "der", "In\u00b7den", "rei\u00b7chen", "sch\u00e4t\u00b7zen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "PPER", "VVINF", "VMFIN", "ART", "NN", "VVINF", "VVINF", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Mund! dessen balsam uns kan st\u00e4rcken und verletzen/", "tokens": ["Mund", "!", "des\u00b7sen", "bal\u00b7sam", "uns", "kan", "st\u00e4r\u00b7cken", "und", "ver\u00b7let\u00b7zen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PDS", "VVFIN", "PPER", "VMFIN", "VVINF", "KON", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Mund! der vergn\u00fcgter bl\u00fcht/ als aller rosen schein.", "tokens": ["Mund", "!", "der", "ver\u00b7gn\u00fcg\u00b7ter", "bl\u00fcht", "/", "als", "al\u00b7ler", "ro\u00b7sen", "schein", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "ADJA", "VVFIN", "$(", "KOKOM", "PIAT", "NN", "PTKVZ", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.7": {"text": "Mund! welchen kein rubin kan gleich und \u00e4hnlich seyn.", "tokens": ["Mund", "!", "wel\u00b7chen", "kein", "ru\u00b7bin", "kan", "gleich", "und", "\u00e4hn\u00b7lich", "seyn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PWAT", "PIAT", "NN", "VMFIN", "ADV", "KON", "ADJD", "VAINF", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.8": {"text": "Mund! denn die Gratien mit ihren qvellen netzen;", "tokens": ["Mund", "!", "denn", "die", "Gra\u00b7ti\u00b7en", "mit", "ih\u00b7ren", "qvel\u00b7len", "net\u00b7zen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "KON", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Mund! Ach corallen-mund/ mein eintziges ergetzen!", "tokens": ["Mund", "!", "Ach", "coral\u00b7len\u00b7mund", "/", "mein", "eint\u00b7zi\u00b7ges", "er\u00b7get\u00b7zen", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "NE", "$(", "PPOSAT", "ADJA", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.10": {"text": "Mund! la\u00df mich einen ku\u00df auff deinen purpur setzen.", "tokens": ["Mund", "!", "la\u00df", "mich", "ei\u00b7nen", "ku\u00df", "auff", "dei\u00b7nen", "pur\u00b7pur", "set\u00b7zen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVIMP", "PPER", "ART", "NN", "APPR", "PPOSAT", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}