{"textgrid.poem.54458": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dis ist der trawte sitz den Themis ihr erkohren.", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dis ist der trawte sitz den Themis ihr erkohren.", "tokens": ["Dis", "ist", "der", "traw\u00b7te", "sitz", "den", "The\u00b7mis", "ihr", "er\u00b7koh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "ART", "NE", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dran Svada sich verlibt/ der hohen weisheitt zelt", "tokens": ["Dran", "Sva\u00b7da", "sich", "ver\u00b7libt", "/", "der", "ho\u00b7hen", "weis\u00b7heitt", "zelt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PAV", "NE", "PRF", "VVFIN", "$(", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das aller k\u00fcnste schar in seinen schrancken h\u00e4lt.", "tokens": ["Das", "al\u00b7ler", "k\u00fcns\u00b7te", "schar", "in", "sei\u00b7nen", "schran\u00b7cken", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "ADJD", "APPR", "PPOSAT", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vnd was ber\u00fchmte leut aus ihrem sin gebohren.", "tokens": ["Vnd", "was", "be\u00b7r\u00fchm\u00b7te", "leut", "aus", "ih\u00b7rem", "sin", "ge\u00b7boh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Hier les ich was vorl\u00e4ngst Gott seinem volck geschworen", "tokens": ["Hier", "les", "ich", "was", "vor\u00b7l\u00e4ngst", "Gott", "sei\u00b7nem", "volck", "ge\u00b7schwo\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PIS", "PPER", "PIS", "VVFIN", "NN", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Hier sindt gesetz vndt recht' hier wird die grosse welt", "tokens": ["Hier", "sindt", "ge\u00b7setz", "vndt", "recht'", "hier", "wird", "die", "gros\u00b7se", "welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD", "KON", "VVFIN", "ADV", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "In b\u00fcchern/ vnd was mehr in bildern vorgestelt.", "tokens": ["In", "b\u00fc\u00b7chern", "/", "vnd", "was", "mehr", "in", "bil\u00b7dern", "vor\u00b7ge\u00b7stelt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$(", "KON", "PWS", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Hier ist die zeitt die sich von anbegin verlohren.", "tokens": ["Hier", "ist", "die", "zeitt", "die", "sich", "von", "an\u00b7be\u00b7gin", "ver\u00b7loh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "VVFIN", "ART", "PRF", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Hier find ich was ich will/ hier lern' ich was ein Geist.", "tokens": ["Hier", "find", "ich", "was", "ich", "will", "/", "hier", "lern'", "ich", "was", "ein", "Geist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PWS", "PPER", "VMFIN", "$(", "ADV", "VVFIN", "PPER", "PIS", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Hier seh ich was ein leib/ vnd was man tugend heist.", "tokens": ["Hier", "seh", "ich", "was", "ein", "leib", "/", "vnd", "was", "man", "tu\u00b7gend", "heist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "ART", "NN", "$(", "KON", "PWS", "PIS", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Schaw aller st\u00e4dte wei\u00df' vnd wie man die regiret.", "tokens": ["Schaw", "al\u00b7ler", "st\u00e4d\u00b7te", "wei\u00df'", "vnd", "wie", "man", "die", "re\u00b7gi\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "VVFIN", "PTKVZ", "KON", "PWAV", "PIS", "ART", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Hier bl\u00fcht natur vnd kunst/ vndt was man seltzam n\u00e4nt.", "tokens": ["Hier", "bl\u00fcht", "na\u00b7tur", "vnd", "kunst", "/", "vndt", "was", "man", "selt\u00b7zam", "n\u00e4nt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "KON", "NN", "$(", "KON", "PWS", "PIS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Doch als ich diesen mann/ der alhier lebt erk\u00e4nt;", "tokens": ["Doch", "als", "ich", "die\u00b7sen", "mann", "/", "der", "al\u00b7hier", "lebt", "er\u00b7k\u00e4nt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PDAT", "NN", "$(", "ART", "ADV", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Befandt ich/ das ihn di\u00df vndt mehr den dises zihret.", "tokens": ["Be\u00b7fandt", "ich", "/", "das", "ihn", "di\u00df", "vndt", "mehr", "den", "di\u00b7ses", "zih\u00b7ret", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "PRELS", "PPER", "PDS", "KON", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}