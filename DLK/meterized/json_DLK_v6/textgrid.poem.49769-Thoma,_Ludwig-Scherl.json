{"textgrid.poem.49769": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Scherl", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der du die Wildsau hast im Blatt geschildert,", "tokens": ["Der", "du", "die", "Wild\u00b7sau", "hast", "im", "Blatt", "ge\u00b7schil\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VAFIN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie sie den allerh\u00f6chsten Sto\u00df bekommt,", "tokens": ["Wie", "sie", "den", "al\u00b7ler\u00b7h\u00f6chs\u00b7ten", "Sto\u00df", "be\u00b7kommt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der alles, was es gibt, gelichtgebildert,", "tokens": ["Der", "al\u00b7les", ",", "was", "es", "gibt", ",", "ge\u00b7licht\u00b7ge\u00b7bil\u00b7dert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PIS", "$,", "PWS", "PPER", "VVFIN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nun hat dir endlich deine M\u00fch' gefrommt.", "tokens": ["Nun", "hat", "dir", "end\u00b7lich", "dei\u00b7ne", "M\u00fch'", "ge\u00b7frommt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Heil dem Weisen, der's verstand!", "tokens": ["Heil", "dem", "Wei\u00b7sen", ",", "der's", "ver\u00b7stand", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PIS", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Alles G'schwerl", "tokens": ["Al\u00b7les", "G'\u00b7schwerl"], "token_info": ["word", "word"], "pos": ["PIAT", "NN"], "meter": "+---", "measure": "dactylic.init"}, "line.7": {"text": "Bringt der Scherl", "tokens": ["Bringt", "der", "Scherl"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "In der \u00bbWoche\u00ab.", "tokens": ["In", "der", "\u00bb", "Wo\u00b7che", "\u00ab", "."], "token_info": ["word", "word", "punct", "word", "punct", "punct"], "pos": ["APPR", "ART", "$(", "NN", "$(", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Aus dem Loche", "tokens": ["Aus", "dem", "Lo\u00b7che"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "--+-", "measure": "anapaest.init"}, "line.10": {"text": "H\u00e4ngt ihm jetzt ein Ordensband.", "tokens": ["H\u00e4ngt", "ihm", "jetzt", "ein", "Or\u00b7dens\u00b7band", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Du zeigst dem niedern Volk die hohen Kreise,", "tokens": ["Du", "zeigst", "dem", "nie\u00b7dern", "Volk", "die", "ho\u00b7hen", "Krei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie sich der Graf, wie der Baron sich mopst,", "tokens": ["Wie", "sich", "der", "Graf", ",", "wie", "der", "Ba\u00b7ron", "sich", "mopst", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "NN", "$,", "PWAV", "ART", "NN", "PRF", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und wie sie zu sich nehmen Trank und Speise,", "tokens": ["Und", "wie", "sie", "zu", "sich", "neh\u00b7men", "Trank", "und", "Spei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "APPR", "PRF", "VVINF", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und wie die Tochter im Basare hopst.", "tokens": ["Und", "wie", "die", "Toch\u00b7ter", "im", "Ba\u00b7sa\u00b7re", "hopst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Heil dem Manne unsrer Zeit!", "tokens": ["Heil", "dem", "Man\u00b7ne", "uns\u00b7rer", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Eine Perl", "tokens": ["Ei\u00b7ne", "Perl"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Ist der Scherl;", "tokens": ["Ist", "der", "Scherl", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Jede Chose,", "tokens": ["Je\u00b7de", "Cho\u00b7se", ","], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Jede Pose", "tokens": ["Je\u00b7de", "Po\u00b7se"], "token_info": ["word", "word"], "pos": ["PIAT", "NN"], "meter": "+---", "measure": "dactylic.init"}, "line.10": {"text": "Weiht er der Unsterblichkeit.", "tokens": ["Weiht", "er", "der", "U\u00b7nsterb\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "Du bringst die Herren, wie sie br\u00fcnstig beten,", "tokens": ["Du", "bringst", "die", "Her\u00b7ren", ",", "wie", "sie", "br\u00fcns\u00b7tig", "be\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PWAV", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sogar in Kirchen knipst dein Apparat.", "tokens": ["So\u00b7gar", "in", "Kir\u00b7chen", "knipst", "dein", "Ap\u00b7pa\u00b7rat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du zeigst die Dichter, wie sie Verse kneten,", "tokens": ["Du", "zeigst", "die", "Dich\u00b7ter", ",", "wie", "sie", "Ver\u00b7se", "kne\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PWAV", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und die Minister bei dem Abendskat.", "tokens": ["Und", "die", "Mi\u00b7nis\u00b7ter", "bei", "dem", "A\u00b7bends\u00b7kat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Heil dem Guten, der gesiegt!", "tokens": ["Heil", "dem", "Gu\u00b7ten", ",", "der", "ge\u00b7siegt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PRELS", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Ja, ein Kerl", "tokens": ["Ja", ",", "ein", "Kerl"], "token_info": ["word", "punct", "word", "word"], "pos": ["PTKANT", "$,", "ART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Ist der Scherl!", "tokens": ["Ist", "der", "Scherl", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Durch das Blitzlicht \u2013", "tokens": ["Durch", "das", "Blitz\u00b7licht", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "'s ist kein Witz nicht \u2013", "tokens": ["'s", "ist", "kein", "Witz", "nicht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "PTKNEG", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.10": {"text": "Hat 'nen Orden er gekriegt.", "tokens": ["Hat", "'nen", "Or\u00b7den", "er", "ge\u00b7kriegt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Der gr\u00f6\u00dfte Staatsmann l\u00e4\u00dft von dir sich typen,", "tokens": ["Der", "gr\u00f6\u00df\u00b7te", "Staats\u00b7mann", "l\u00e4\u00dft", "von", "dir", "sich", "ty\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und die Soubrette wird uns aufgetischt.", "tokens": ["Und", "die", "Soub\u00b7ret\u00b7te", "wird", "uns", "auf\u00b7ge\u00b7tischt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Lumpen prangen neben den Polypen,", "tokens": ["Die", "Lum\u00b7pen", "pran\u00b7gen", "ne\u00b7ben", "den", "Po\u00b7ly\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Die teils sie fingen, teils sie nicht erwischt.", "tokens": ["Die", "teils", "sie", "fin\u00b7gen", ",", "teils", "sie", "nicht", "er\u00b7wischt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPER", "VVFIN", "$,", "KOUS", "PPER", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Heil dem Manne, der es kennt,", "tokens": ["Heil", "dem", "Man\u00b7ne", ",", "der", "es", "kennt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wie der Scherl,", "tokens": ["Wie", "der", "Scherl", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Diese Perl,", "tokens": ["Die\u00b7se", "Perl", ","], "token_info": ["word", "word", "punct"], "pos": ["PDAT", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Was Epoche", "tokens": ["Was", "E\u00b7po\u00b7che"], "token_info": ["word", "word"], "pos": ["PWS", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Macht der \u00bbWoche\u00ab,", "tokens": ["Macht", "der", "\u00bb", "Wo\u00b7che", "\u00ab", ","], "token_info": ["word", "word", "punct", "word", "punct", "punct"], "pos": ["NN", "ART", "$(", "NN", "$(", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.10": {"text": "Was ein packender Moment.", "tokens": ["Was", "ein", "pa\u00b7cken\u00b7der", "Mo\u00b7ment", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "O fahre weiter fort uns einzuimpfen", "tokens": ["O", "fah\u00b7re", "wei\u00b7ter", "fort", "uns", "ein\u00b7zu\u00b7imp\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ADV", "PTKVZ", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Respekt vor dem, was von sich reden macht.", "tokens": ["Res\u00b7pekt", "vor", "dem", ",", "was", "von", "sich", "re\u00b7den", "macht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "$,", "PRELS", "APPR", "PRF", "VVINF", "VVFIN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die Leute, welche ernstlich auf dich schimpfen,", "tokens": ["Die", "Leu\u00b7te", ",", "wel\u00b7che", "ernst\u00b7lich", "auf", "dich", "schimp\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sind nur die paar, die du noch nicht gebracht.", "tokens": ["Sind", "nur", "die", "paar", ",", "die", "du", "noch", "nicht", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "PIAT", "$,", "PRELS", "PPER", "ADV", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Heil dem Weisen, der's verstand!", "tokens": ["Heil", "dem", "Wei\u00b7sen", ",", "der's", "ver\u00b7stand", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PIS", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Alles G'schwerl", "tokens": ["Al\u00b7les", "G'\u00b7schwerl"], "token_info": ["word", "word"], "pos": ["PIAT", "NN"], "meter": "+---", "measure": "dactylic.init"}, "line.7": {"text": "Bringt der Scherl", "tokens": ["Bringt", "der", "Scherl"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "In der \u00bbWoche\u00ab.", "tokens": ["In", "der", "\u00bb", "Wo\u00b7che", "\u00ab", "."], "token_info": ["word", "word", "punct", "word", "punct", "punct"], "pos": ["APPR", "ART", "$(", "NN", "$(", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Aus dem Loche", "tokens": ["Aus", "dem", "Lo\u00b7che"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "--+-", "measure": "anapaest.init"}, "line.10": {"text": "H\u00e4ngt ihm jetzt ein Ordensband.", "tokens": ["H\u00e4ngt", "ihm", "jetzt", "ein", "Or\u00b7dens\u00b7band", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}