{"dta.poem.1309": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Himmel-Schl\u00fcssel  \n oder  \n Geistliche Gedichte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Jahr ist wieder um! Ich soll mit Danck erscheinen/", "tokens": ["Ein", "Jahr", "ist", "wie\u00b7der", "um", "!", "Ich", "soll", "mit", "Danck", "er\u00b7schei\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PTKVZ", "$.", "PPER", "VMFIN", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "F\u00fcr Gnade mancher Art/ die mir und auch den Meinen", "tokens": ["F\u00fcr", "Gna\u00b7de", "man\u00b7cher", "Art", "/", "die", "mir", "und", "auch", "den", "Mei\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "PIAT", "NN", "$(", "PRELS", "PPER", "KON", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Des H\u00f6chsten Hand erzeigt. HErr/ nimm das Opffer hin/", "tokens": ["Des", "H\u00f6chs\u00b7ten", "Hand", "er\u00b7zeigt", ".", "Herr", "/", "nimm", "das", "Opf\u00b7fer", "hin", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$.", "NN", "$(", "VVIMP", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Was ich aus schwacher Krafft zu bringen f\u00e4hig bin.", "tokens": ["Was", "ich", "aus", "schwa\u00b7cher", "Krafft", "zu", "brin\u00b7gen", "f\u00e4\u00b7hig", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "ADJA", "NN", "PTKZU", "VVINF", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wer bin ich/ Adams Sohn/ da\u00df du an mich gedenckest/", "tokens": ["Wer", "bin", "ich", "/", "A\u00b7dams", "Sohn", "/", "da\u00df", "du", "an", "mich", "ge\u00b7den\u00b7ckest", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$(", "NE", "NN", "$(", "KOUS", "PPER", "APPR", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df du mir deine Huld und Vaters-Liebe schenckest?", "tokens": ["Da\u00df", "du", "mir", "dei\u00b7ne", "Huld", "und", "Va\u00b7ter\u00b7sLie\u00b7be", "schen\u00b7ckest", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PPOSAT", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein leichter Erden-Klo\u00df/ ein Schatten/ der verschwindt/", "tokens": ["Ein", "leich\u00b7ter", "Er\u00b7den\u00b7Klo\u00df", "/", "ein", "Schat\u00b7ten", "/", "der", "ver\u00b7schwindt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "NN", "$(", "ART", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und dennoch h\u00e4ltst du mich als dein geliebtes Kind.", "tokens": ["Und", "den\u00b7noch", "h\u00e4ltst", "du", "mich", "als", "dein", "ge\u00b7lieb\u00b7tes", "Kind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PRF", "KOUS", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Was find ich auff der Welt/ da\u00df deinen Ruhm vermehre/", "tokens": ["Was", "find", "ich", "auff", "der", "Welt", "/", "da\u00df", "dei\u00b7nen", "Ruhm", "ver\u00b7meh\u00b7re", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "ART", "NN", "$(", "KOUS", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dein ist der Prei\u00df allein/ dein ist allein die Ehre.", "tokens": ["Dein", "ist", "der", "Prei\u00df", "al\u00b7lein", "/", "dein", "ist", "al\u00b7lein", "die", "Eh\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VAFIN", "ART", "NN", "ADV", "$(", "PPOSAT", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich sag es frey heraus/ nichts guttes wohnt in mir/", "tokens": ["Ich", "sag", "es", "frey", "he\u00b7raus", "/", "nichts", "gut\u00b7tes", "wohnt", "in", "mir", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "PTKVZ", "$(", "PIS", "ADJA", "VVFIN", "APPR", "PPER", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Was ich izt dencke/ schreib und danck\u2019 ist selbst von dir.", "tokens": ["Was", "ich", "izt", "den\u00b7cke", "/", "schreib", "und", "danck", "ist", "selbst", "von", "dir", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVFIN", "$(", "VVFIN", "KON", "PAV", "VAFIN", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Nun HErr entz\u00fcnd in mir der Gegen-Liebe Flammen/", "tokens": ["Nun", "Herr", "ent\u00b7z\u00fcnd", "in", "mir", "der", "Ge\u00b7gen\u00b7Lie\u00b7be", "Flam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "APPR", "PPER", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "La\u00df reiner Andacht Brunst aus meinem Hertzen stammen/", "tokens": ["La\u00df", "rei\u00b7ner", "An\u00b7dacht", "Brunst", "aus", "mei\u00b7nem", "Hert\u00b7zen", "stam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADJA", "NN", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Gieb/ da\u00df ich dir di\u00df Jahr getreu und danckbar sey/", "tokens": ["Gieb", "/", "da\u00df", "ich", "dir", "di\u00df", "Jahr", "ge\u00b7treu", "und", "dan\u00b7ck\u00b7bar", "sey", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$(", "KOUS", "PPER", "PPER", "PDS", "NN", "ADJD", "KON", "ADJD", "VAFIN", "$("], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.4": {"text": "Und setz uns was du wilt f\u00fcr Segen ferner bey.", "tokens": ["Und", "setz", "uns", "was", "du", "wilt", "f\u00fcr", "Se\u00b7gen", "fer\u00b7ner", "bey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "PWS", "PPER", "VMFIN", "APPR", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "K\u00f6mmt denn die Zeit heran die Erde zu gesegnen/", "tokens": ["K\u00f6mmt", "denn", "die", "Zeit", "he\u00b7ran", "die", "Er\u00b7de", "zu", "ge\u00b7seg\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PTKVZ", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So la\u00df uns deine Krafft in lezter Noth begegnen/", "tokens": ["So", "la\u00df", "uns", "dei\u00b7ne", "Krafft", "in", "lez\u00b7ter", "Noth", "be\u00b7geg\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Gieb da\u00df man willig folgt/ wenn nun die Stunde schl\u00e4gt/", "tokens": ["Gieb", "da\u00df", "man", "wil\u00b7lig", "folgt", "/", "wenn", "nun", "die", "Stun\u00b7de", "schl\u00e4gt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "KOUS", "PIS", "ADJD", "VVFIN", "$(", "KOUS", "ADV", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und sich auff dein Verdienst getrost zur Ruhe legt.", "tokens": ["Und", "sich", "auff", "dein", "Ver\u00b7dienst", "ge\u00b7trost", "zur", "Ru\u00b7he", "legt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "APPR", "PPOSAT", "NN", "VVPP", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}