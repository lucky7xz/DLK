{"dta.poem.9506": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "Vom  \n  Deutschen Hexameter,  \n aus einer  \n Abhandlung vom Sylbenmaasse.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1769", "urn": "urn:nbn:de:kobv:b4-200905199341", "language": ["de:0.85", "nl:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Drohend erscholl der gefl\u00fcgelte Donnergesang in der Heerschaar.", "tokens": ["Dro\u00b7hend", "er\u00b7scholl", "der", "ge\u00b7fl\u00fc\u00b7gel\u00b7te", "Don\u00b7ner\u00b7ge\u00b7sang", "in", "der", "Heer\u00b7schaar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Aber da nun in der Nacht Wehklage vom Grab' aufrufte.", "tokens": ["A\u00b7ber", "da", "nun", "in", "der", "Nacht", "Weh\u00b7kla\u00b7ge", "vom", "Grab'", "auf\u00b7ruf\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "APPR", "ART", "NN", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+--+--+++-+-+--", "measure": "dactylic.di.plus"}, "line.3": {"text": "R\u00fchmt und preist, gl\u00fcckselige Mitgenossen der Wonne.", "tokens": ["R\u00fchmt", "und", "preist", ",", "gl\u00fcck\u00b7se\u00b7li\u00b7ge", "Mit\u00b7ge\u00b7nos\u00b7sen", "der", "Won\u00b7ne", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "VVFIN", "$,", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Ach wie liebt ich ihn sonst, ich einst Schutzgeist des Verworsnen.", "tokens": ["Ach", "wie", "liebt", "ich", "ihn", "sonst", ",", "ich", "einst", "Schutz\u00b7geist", "des", "Ver\u00b7wors\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "VVFIN", "PPER", "PPER", "ADV", "$,", "PPER", "ADV", "NN", "ART", "NN", "$."], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.5": {"text": "Als der Erdkreis Gott vernahm, Gott nieder vom Paran", "tokens": ["Als", "der", "Erd\u00b7kreis", "Gott", "ver\u00b7nahm", ",", "Gott", "nie\u00b7der", "vom", "Pa\u00b7ran"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "NN", "VVFIN", "$,", "NN", "PTKVZ", "APPRART", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.6": {"text": "Strom, steh still! der Posaunhall ruft, und das Volk des Herrn kommt.", "tokens": ["Strom", ",", "steh", "still", "!", "der", "Po\u00b7saun\u00b7hall", "ruft", ",", "und", "das", "Volk", "des", "Herrn", "kommt", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PTKVZ", "$.", "ART", "NN", "VVFIN", "$,", "KON", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+--+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "Jeder, dem jetzt am Tage des Herrn das Gericht Weh zurief.", "tokens": ["Je\u00b7der", ",", "dem", "jetzt", "am", "Ta\u00b7ge", "des", "Herrn", "das", "Ge\u00b7richt", "Weh", "zu\u00b7rief", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "ADV", "APPRART", "NN", "ART", "NN", "ART", "NN", "NN", "VVFIN", "$."], "meter": "---+-+--+---+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Ach es vernahm von dem Thron den Gerichtsausspruch die Versammlung.", "tokens": ["Ach", "es", "ver\u00b7nahm", "von", "dem", "Thron", "den", "Ge\u00b7richts\u00b7aus\u00b7spruch", "die", "Ver\u00b7samm\u00b7lung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PPER", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "ART", "NN", "$."], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.9": {"text": "Aber da nun des Gerichts Ausspruch vom gef\u00fcrchteten Thron scholl.", "tokens": ["A\u00b7ber", "da", "nun", "des", "Ge\u00b7richts", "Aus\u00b7spruch", "vom", "ge\u00b7f\u00fcrch\u00b7te\u00b7ten", "Thron", "scholl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ART", "NN", "NN", "APPRART", "ADJA", "NN", "ADJD", "$."], "meter": "+--+---+-+-+--++", "measure": "dactylic.di.plus"}}}}}