{"textgrid.poem.52679": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fchling", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fr\u00fchling, bist du wiedergekommen?", "tokens": ["Fr\u00fch\u00b7ling", ",", "bist", "du", "wie\u00b7der\u00b7ge\u00b7kom\u00b7men", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VAFIN", "PPER", "VVINF", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Lieblicher Lenz, du lachendes Kind!", "tokens": ["Lieb\u00b7li\u00b7cher", "Lenz", ",", "du", "la\u00b7chen\u00b7des", "Kind", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "ADJA", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Kommst du auf dem Flu\u00df geschwommen?", "tokens": ["Kommst", "du", "auf", "dem", "Flu\u00df", "ge\u00b7schwom\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Oder kommst du mit dem Wind?", "tokens": ["O\u00b7der", "kommst", "du", "mit", "dem", "Wind", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Unter den weichen singenden Wellen,", "tokens": ["Un\u00b7ter", "den", "wei\u00b7chen", "sin\u00b7gen\u00b7den", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Aus den Wassern melodisch klar,", "tokens": ["Aus", "den", "Was\u00b7sern", "me\u00b7lo\u00b7disch", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ueber die H\u00fcgel, die waldig schwellen,", "tokens": ["Ue\u00b7ber", "die", "H\u00fc\u00b7gel", ",", "die", "wal\u00b7dig", "schwel\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ADJD", "VVINF", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Luget dein kluges Augenpaar.", "tokens": ["Lu\u00b7get", "dein", "klu\u00b7ges", "Au\u00b7gen\u00b7paar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "Schaue ich nur in dein sonniges Auge,", "tokens": ["Schau\u00b7e", "ich", "nur", "in", "dein", "son\u00b7ni\u00b7ges", "Au\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "K\u00fcsse ich nur deinen wonnigen Mund,", "tokens": ["K\u00fcs\u00b7se", "ich", "nur", "dei\u00b7nen", "won\u00b7ni\u00b7gen", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Trink ich von deinem bl\u00fchenden Hauche,", "tokens": ["Trink", "ich", "von", "dei\u00b7nem", "bl\u00fc\u00b7hen\u00b7den", "Hau\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wird auch mein winterlich Herze gesund!", "tokens": ["Wird", "auch", "mein", "win\u00b7ter\u00b7lich", "Her\u00b7ze", "ge\u00b7sund", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "ADJD", "VVFIN", "ADJD", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}}}}