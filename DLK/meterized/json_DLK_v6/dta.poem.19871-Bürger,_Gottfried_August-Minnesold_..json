{"dta.poem.19871": {"metadata": {"author": {"name": "B\u00fcrger, Gottfried August", "birth": "N.A.", "death": "N.A."}, "title": "Minnesold .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1778", "urn": "urn:nbn:de:kobv:b4-20090519672", "language": ["de:0.99"], "booktitle": "B\u00fcrger, Gottfried August: Gedichte. G\u00f6ttingen, 1778."}, "poem": {"stanza.1": {"line.1": {"text": "Wem der Minnedienst gelinget,               ", "tokens": ["Wem", "der", "Min\u00b7ne\u00b7dienst", "ge\u00b7lin\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "O, wie hoch wird der belohnt!", "tokens": ["O", ",", "wie", "hoch", "wird", "der", "be\u00b7lohnt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ADJD", "VAFIN", "ART", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Keinen bessern Lohn erringet,", "tokens": ["Kei\u00b7nen", "bes\u00b7sern", "Lohn", "er\u00b7rin\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wer dem gr\u00f6sten Kaiser frohnt.", "tokens": ["Wer", "dem", "gr\u00f6s\u00b7ten", "Kai\u00b7ser", "frohnt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn mit Szepter, Kron\u2019 und Gold,", "tokens": ["Denn", "mit", "Szep\u00b7ter", ",", "Kron'", "und", "Gold", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Frohnt er selbst um Minnesold.", "tokens": ["Frohnt", "er", "selbst", "um", "Min\u00b7ne\u00b7sold", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Was sind Gold und Edelsteine?", "tokens": ["Was", "sind", "Gold", "und", "E\u00b7del\u00b7stei\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was des Mogols Perlenpracht?", "tokens": ["Was", "des", "Mo\u00b7gols", "Per\u00b7len\u00b7pracht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Minnesold ist doch alleine,", "tokens": ["Min\u00b7ne\u00b7sold", "ist", "doch", "al\u00b7lei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was auch reich die Herzen macht.", "tokens": ["Was", "auch", "reich", "die", "Her\u00b7zen", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.5": {"text": "Perlen, Edelstein und Gold", "tokens": ["Per\u00b7len", ",", "E\u00b7del\u00b7stein", "und", "Gold"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "N\u00e4m\u2019 ich nicht f\u00fcr Minnesold.", "tokens": ["N\u00e4m'", "ich", "nicht", "f\u00fcr", "Min\u00b7ne\u00b7sold", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "APPR", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Minnesold l\u00e4st Amt und Ehren,", "tokens": ["Min\u00b7ne\u00b7sold", "l\u00e4st", "Amt", "und", "Eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Goldnen Sporn und Ritterschlag,", "tokens": ["Gold\u00b7nen", "Sporn", "und", "Rit\u00b7ter\u00b7schlag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "L\u00e4sset ohne Neid entberen,", "tokens": ["L\u00e4s\u00b7set", "oh\u00b7ne", "Neid", "ent\u00b7be\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was der Kaiser geben mag.", "tokens": ["Was", "der", "Kai\u00b7ser", "ge\u00b7ben", "mag."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["PWS", "ART", "NN", "VVINF", "VMFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Ehre lacht nicht halb so hold,", "tokens": ["Eh\u00b7re", "lacht", "nicht", "halb", "so", "hold", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKNEG", "ADJD", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Als der Minne Freudensold.", "tokens": ["Als", "der", "Min\u00b7ne", "Freu\u00b7den\u00b7sold", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Nimmer, nimmermehr hienieden", "tokens": ["Nim\u00b7mer", ",", "nim\u00b7mer\u00b7mehr", "hien\u00b7ie\u00b7den"], "token_info": ["word", "punct", "word", "word"], "pos": ["ADV", "$,", "ADV", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "F\u00e4nd\u2019 ich s\u00fcsseren Genies.", "tokens": ["F\u00e4nd'", "ich", "s\u00fcs\u00b7se\u00b7ren", "Ge\u00b7nies", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "S\u00fcsseres ist nur beschieden", "tokens": ["S\u00fcs\u00b7se\u00b7res", "ist", "nur", "be\u00b7schie\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VAFIN", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seligen im Paradies.", "tokens": ["Se\u00b7li\u00b7gen", "im", "Pa\u00b7ra\u00b7dies", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "S\u00fcs ist, was die Biene zolt;", "tokens": ["S\u00fcs", "ist", ",", "was", "die", "Bie\u00b7ne", "zolt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "S\u00fcsser dennoch Minnesold.", "tokens": ["S\u00fcs\u00b7ser", "den\u00b7noch", "Min\u00b7ne\u00b7sold", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Minnesold ist aller Freuden,", "tokens": ["Min\u00b7ne\u00b7sold", "ist", "al\u00b7ler", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aller Freuden F\u00fcnftelsaft;", "tokens": ["Al\u00b7ler", "Freu\u00b7den", "F\u00fcnf\u00b7tel\u00b7saft", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Minnesold hat aller Leiden,", "tokens": ["Min\u00b7ne\u00b7sold", "hat", "al\u00b7ler", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aller Leiden Heilungskraft.", "tokens": ["Al\u00b7ler", "Lei\u00b7den", "Hei\u00b7lungs\u00b7kraft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Was der Balsamstand\u2019 entrollt,", "tokens": ["Was", "der", "Bal\u00b7sam\u00b7stand'", "ent\u00b7rollt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Heilet nicht, wie Minnesold.", "tokens": ["Hei\u00b7let", "nicht", ",", "wie", "Min\u00b7ne\u00b7sold", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,", "PWAV", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Minnesold lehrt frei verachten", "tokens": ["Min\u00b7ne\u00b7sold", "lehrt", "frei", "ver\u00b7ach\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ADJD", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aller F\u00e4rlichkeiten Not,", "tokens": ["Al\u00b7ler", "F\u00e4r\u00b7lich\u00b7kei\u00b7ten", "Not", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Flammen, Wasserfluten, Schlachten,", "tokens": ["Flam\u00b7men", ",", "Was\u00b7ser\u00b7flu\u00b7ten", ",", "Schlach\u00b7ten", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lehrt verschm\u00e4hen jeden Tod.", "tokens": ["Lehrt", "ver\u00b7schm\u00e4\u00b7hen", "je\u00b7den", "Tod", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "St\u00fcrb\u2019 ich nicht f\u00fcr Ehr\u2019 und Gold,", "tokens": ["St\u00fcrb'", "ich", "nicht", "f\u00fcr", "Ehr'", "und", "Gold", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKNEG", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "St\u00fcrb\u2019 ich doch f\u00fcr Minnesold.", "tokens": ["St\u00fcrb'", "ich", "doch", "f\u00fcr", "Min\u00b7ne\u00b7sold", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "APPR", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Auszuspenden alle Habe,", "tokens": ["Aus\u00b7zu\u00b7spen\u00b7den", "al\u00b7le", "Ha\u00b7be", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zu verbluten mit Gedult,", "tokens": ["Zu", "ver\u00b7blu\u00b7ten", "mit", "Ge\u00b7dult", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "W\u00e4r\u2019 ein Sch\u00e4rflein Armengabe,", "tokens": ["W\u00e4r'", "ein", "Sch\u00e4r\u00b7flein", "Ar\u00b7men\u00b7ga\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fcr der Minne Dank und Huld.", "tokens": ["F\u00fcr", "der", "Min\u00b7ne", "Dank", "und", "Huld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Den Verlust von Gut und Blut", "tokens": ["Den", "Ver\u00b7lust", "von", "Gut", "und", "Blut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Macht der Sold der Minne gut.", "tokens": ["Macht", "der", "Sold", "der", "Min\u00b7ne", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "O, so wil ich immer harren,", "tokens": ["O", ",", "so", "wil", "ich", "im\u00b7mer", "har\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Immerdar, mit stetem Mut;", "tokens": ["Im\u00b7mer\u00b7dar", ",", "mit", "ste\u00b7tem", "Mut", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Im Dezemberfrost erstarren,", "tokens": ["Im", "De\u00b7zem\u00b7berf\u00b7rost", "er\u00b7star\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Schmachten in des Heumonds Glut.", "tokens": ["Schmach\u00b7ten", "in", "des", "Heu\u00b7monds", "Glut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn das alles lohnt der Sold,", "tokens": ["Denn", "das", "al\u00b7les", "lohnt", "der", "Sold", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "PIS", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Den getreue Minne zolt.", "tokens": ["Den", "ge\u00b7treu\u00b7e", "Min\u00b7ne", "zolt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}