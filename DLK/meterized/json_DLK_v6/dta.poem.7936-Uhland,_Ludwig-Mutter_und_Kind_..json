{"dta.poem.7936": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Mutter und Kind .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Da\u00df kein Engel mich je von der liebenden Brust dir entf\u00fchre,", "tokens": ["Da\u00df", "kein", "En\u00b7gel", "mich", "je", "von", "der", "lie\u00b7ben\u00b7den", "Brust", "dir", "ent\u00b7f\u00fch\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "PPER", "ADV", "APPR", "ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Mutter, so sage du mir, wie ich betr\u00fcben dich kann!", "tokens": ["Mut\u00b7ter", ",", "so", "sa\u00b7ge", "du", "mir", ",", "wie", "ich", "be\u00b7tr\u00fc\u00b7ben", "dich", "kann", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVFIN", "PPER", "PPER", "$,", "PWAV", "PPER", "VVFIN", "PPER", "VMFIN", "$."], "meter": "+-+---+-+-+--+", "measure": "iambic.hexa.chol"}}}}}