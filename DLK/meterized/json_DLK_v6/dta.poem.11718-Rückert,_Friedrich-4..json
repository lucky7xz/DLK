{"dta.poem.11718": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der neugeborne Gott schlief an der Erde Grund;", "tokens": ["Der", "neu\u00b7ge\u00b7bor\u00b7ne", "Gott", "schlief", "an", "der", "Er\u00b7de", "Grund", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Neugierig \u00f6ffnete die Mutter seinen Mund.", "tokens": ["Neu\u00b7gie\u00b7rig", "\u00f6ff\u00b7ne\u00b7te", "die", "Mut\u00b7ter", "sei\u00b7nen", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Mutter wu\u00dfte nicht vor Lust wie ihr geschah,", "tokens": ["Die", "Mut\u00b7ter", "wu\u00df\u00b7te", "nicht", "vor", "Lust", "wie", "ihr", "ge\u00b7schah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "APPR", "NN", "KOKOM", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als sie im Kindesmund den Glanz der Welten sah.", "tokens": ["Als", "sie", "im", "Kin\u00b7des\u00b7mund", "den", "Glanz", "der", "Wel\u00b7ten", "sah", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die sieben Himmel und acht Paradiese sah", "tokens": ["Die", "sie\u00b7ben", "Him\u00b7mel", "und", "acht", "Pa\u00b7ra\u00b7die\u00b7se", "sah"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "CARD", "NN", "KON", "CARD", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie im gew\u00f6lbten Mund, fern waren sie und nah.", "tokens": ["Sie", "im", "ge\u00b7w\u00f6lb\u00b7ten", "Mund", ",", "fern", "wa\u00b7ren", "sie", "und", "nah", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "ADJA", "NN", "$,", "ADJD", "VAFIN", "PPER", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wie kommt die Herrlichkeit in einen Kindesmund?", "tokens": ["Wie", "kommt", "die", "Herr\u00b7lich\u00b7keit", "in", "ei\u00b7nen", "Kin\u00b7des\u00b7mund", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da that es ihr der Geist, der \u00fcberm Kind war, kund:", "tokens": ["Da", "that", "es", "ihr", "der", "Geist", ",", "der", "\u00fc\u00b7berm", "Kind", "war", ",", "kund", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ART", "NN", "$,", "PRELS", "APPRART", "NN", "VAFIN", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Im Mund beschlossen sind Himmel und Paradiese;", "tokens": ["Im", "Mund", "be\u00b7schlos\u00b7sen", "sind", "Him\u00b7mel", "und", "Pa\u00b7ra\u00b7die\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "VAFIN", "NN", "KON", "NN", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Entfalten wird das Kind in seiner Lehre diese.", "tokens": ["Ent\u00b7fal\u00b7ten", "wird", "das", "Kind", "in", "sei\u00b7ner", "Leh\u00b7re", "die\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "PDAT", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}