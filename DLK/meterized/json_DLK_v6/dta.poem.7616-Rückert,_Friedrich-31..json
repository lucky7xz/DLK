{"dta.poem.7616": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "31.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du bist begl\u00fcckt, wenn dir, was da ist, ganz gef\u00e4llt,", "tokens": ["Du", "bist", "be\u00b7gl\u00fcckt", ",", "wenn", "dir", ",", "was", "da", "ist", ",", "ganz", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "KOUS", "PPER", "$,", "PRELS", "ADV", "VAFIN", "$,", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und deine Lust daran solang h\u00e4lt als es h\u00e4lt,", "tokens": ["Und", "dei\u00b7ne", "Lust", "da\u00b7ran", "so\u00b7lang", "h\u00e4lt", "als", "es", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PAV", "APZR", "VVFIN", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und dann vergeht, wenn es zum Gehn auch Anstalt macht;", "tokens": ["Und", "dann", "ver\u00b7geht", ",", "wenn", "es", "zum", "Gehn", "auch", "An\u00b7stalt", "macht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "$,", "KOUS", "PPER", "APPRART", "NN", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dann ist dir andre Lust an anderm zugedacht.", "tokens": ["Dann", "ist", "dir", "and\u00b7re", "Lust", "an", "an\u00b7derm", "zu\u00b7ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word",