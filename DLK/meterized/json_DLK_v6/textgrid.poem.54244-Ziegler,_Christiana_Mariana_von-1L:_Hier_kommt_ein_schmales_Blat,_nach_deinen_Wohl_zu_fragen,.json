{"textgrid.poem.54244": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hier kommt ein schmales Blat, nach deinen Wohl zu fragen,", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier kommt ein schmales Blat, nach deinen Wohl zu fragen,", "tokens": ["Hier", "kommt", "ein", "schma\u00b7les", "Blat", ",", "nach", "dei\u00b7nen", "Wohl", "zu", "fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wornach die Sehnsucht mich gar \u00f6ffters fragen heist,", "tokens": ["Wor\u00b7nach", "die", "Sehn\u00b7sucht", "mich", "gar", "\u00f6ff\u00b7ters", "fra\u00b7gen", "heist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPER", "ADV", "ADV", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch will dir selbges auch, entfernte Freundin, sagen,", "tokens": ["Doch", "will", "dir", "selb\u00b7ges", "auch", ",", "ent\u00b7fern\u00b7te", "Freun\u00b7din", ",", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "ADV", "$,", "VVFIN", "NN", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df in der That mit dir mein Hertz zugleich verreist.", "tokens": ["Da\u00df", "in", "der", "That", "mit", "dir", "mein", "Hertz", "zu\u00b7gleich", "ver\u00b7reist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "APPR", "PPER", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich schertze nicht mit dir, denn ohne dich zu leben,", "tokens": ["Ich", "schert\u00b7ze", "nicht", "mit", "dir", ",", "denn", "oh\u00b7ne", "dich", "zu", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "PPER", "$,", "KON", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Geht mir, ich schw\u00f6r es dir, recht schwer und bitter ein.", "tokens": ["Geht", "mir", ",", "ich", "schw\u00f6r", "es", "dir", ",", "recht", "schwer", "und", "bit\u00b7ter", "ein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPER", "VVFIN", "PPER", "PPER", "$,", "ADV", "ADJD", "KON", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "An deinen Umgang scheint mein Hertze mit zu kleben,", "tokens": ["An", "dei\u00b7nen", "Um\u00b7gang", "scheint", "mein", "Hert\u00b7ze", "mit", "zu", "kle\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPOSAT", "VVFIN", "APPR", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und dein Verlust setzt mich in nicht geringe Pein.", "tokens": ["Und", "dein", "Ver\u00b7lust", "setzt", "mich", "in", "nicht", "ge\u00b7rin\u00b7ge", "Pein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPER", "APPR", "PTKNEG", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wiewohl mich nicht allein, auch andre lassen sp\u00fchren,", "tokens": ["Wie\u00b7wohl", "mich", "nicht", "al\u00b7lein", ",", "auch", "and\u00b7re", "las\u00b7sen", "sp\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "$,", "ADV", "PIS", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die sich als Freunde mit in deinen Rollen sehn,", "tokens": ["Die", "sich", "als", "Freun\u00b7de", "mit", "in", "dei\u00b7nen", "Rol\u00b7len", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "KOUS", "NN", "APPR", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wie z\u00e4rtlich allerseits sie deine Flucht mu\u00df r\u00fchren,", "tokens": ["Wie", "z\u00e4rt\u00b7lich", "al\u00b7ler\u00b7seits", "sie", "dei\u00b7ne", "Flucht", "mu\u00df", "r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADV", "PPER", "PPOSAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wie weh auch selbigen mit mir zugleich geschehn.", "tokens": ["Wie", "weh", "auch", "sel\u00b7bi\u00b7gen", "mit", "mir", "zu\u00b7gleich", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "ADV", "APPR", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.13": {"text": "Auff meinen Pindus h\u00f6rt man nichts als Klage-Lieder,", "tokens": ["Auff", "mei\u00b7nen", "Pin\u00b7dus", "h\u00f6rt", "man", "nichts", "als", "Kla\u00b7ge\u00b7Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PIS", "PIS", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Mir d\u00fcnckt, das unsre Stadt gantz ausgestorben ist,", "tokens": ["Mir", "d\u00fcnckt", ",", "das", "uns\u00b7re", "Stadt", "gantz", "aus\u00b7ge\u00b7stor\u00b7ben", "ist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "PPOSAT", "NN", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Gesellschafft, Spiel, und Schertz wird mir nunmehr zuwider,", "tokens": ["Ge\u00b7sell\u00b7schafft", ",", "Spiel", ",", "und", "Schertz", "wird", "mir", "nun\u00b7mehr", "zu\u00b7wi\u00b7der", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "KON", "NN", "VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Weil dich bey selbigen mein Augen-Paar vermist.", "tokens": ["Weil", "dich", "bey", "sel\u00b7bi\u00b7gen", "mein", "Au\u00b7gen\u00b7Paar", "ver\u00b7mist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "PPOSAT", "NN", "VVPP", "$."], "meter": "--+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.17": {"text": "Du weist, wie sehr ich dich vor allen andern liebe,", "tokens": ["Du", "weist", ",", "wie", "sehr", "ich", "dich", "vor", "al\u00b7len", "an\u00b7dern", "lie\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ADV", "PPER", "PRF", "APPR", "PIAT", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Dein Scheiden w\u00fcrckt bey mir recht w\u00fcrcklichen Verdru\u00df.", "tokens": ["Dein", "Schei\u00b7den", "w\u00fcrckt", "bey", "mir", "recht", "w\u00fcrck\u00b7li\u00b7chen", "Ver\u00b7dru\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "PPER", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+++-+-+", "measure": "unknown.measure.septa"}, "line.19": {"text": "Jedoch, was mach ich dir dein Bade-Wasser tr\u00fcbe,", "tokens": ["Je\u00b7doch", ",", "was", "mach", "ich", "dir", "dein", "Ba\u00b7de\u00b7Was\u00b7ser", "tr\u00fc\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWS", "VVFIN", "PPER", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Das, wenn es helffen soll, gantz helle fliessen mu\u00df.", "tokens": ["Das", ",", "wenn", "es", "helf\u00b7fen", "soll", ",", "gantz", "hel\u00b7le", "flies\u00b7sen", "mu\u00df", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "KOUS", "PPER", "VVINF", "VMFIN", "$,", "ADV", "ADJA", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Die Grillen m\u00fcssen sich nicht mit in B\u00e4der mischen,", "tokens": ["Die", "Gril\u00b7len", "m\u00fcs\u00b7sen", "sich", "nicht", "mit", "in", "B\u00e4\u00b7der", "mi\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PRF", "PTKNEG", "APPR", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Der Geist soll aufger\u00e4umt und ohne Sorgen seyn;", "tokens": ["Der", "Geist", "soll", "auf\u00b7ge\u00b7r\u00e4umt", "und", "oh\u00b7ne", "Sor\u00b7gen", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVPP", "KON", "APPR", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Drum will ich, deinen Sinn zugleich mit aufzufrischen,", "tokens": ["Drum", "will", "ich", ",", "dei\u00b7nen", "Sinn", "zu\u00b7gleich", "mit", "auf\u00b7zu\u00b7fri\u00b7schen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "$,", "PPOSAT", "NN", "ADV", "APPR", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Von Klagen weiter nichts, dir liebste Freundin, streun,", "tokens": ["Von", "Kla\u00b7gen", "wei\u00b7ter", "nichts", ",", "dir", "liebs\u00b7te", "Freun\u00b7din", ",", "streun", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "ADV", "PIS", "$,", "PPER", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Die Sorgen sind bereits am Nagel auffgehangen,", "tokens": ["Die", "Sor\u00b7gen", "sind", "be\u00b7reits", "am", "Na\u00b7gel", "auff\u00b7ge\u00b7han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "APPRART", "NE", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Gnug da\u00df bey meinen Gram mir die Versichrung bleibt,", "tokens": ["Gnug", "da\u00df", "bey", "mei\u00b7nen", "Gram", "mir", "die", "Ver\u00b7sich\u00b7rung", "bleibt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "APPR", "PPOSAT", "NN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Diejenige werde dich bald wiederum umfangen,", "tokens": ["Die\u00b7je\u00b7ni\u00b7ge", "wer\u00b7de", "dich", "bald", "wie\u00b7de\u00b7rum", "um\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.28": {"text": "Die di\u00df recht mit Begierd und gr\u00f6ster Sehnsucht schreibt.", "tokens": ["Die", "di\u00df", "recht", "mit", "Be\u00b7gierd", "und", "gr\u00f6s\u00b7ter", "Sehn\u00b7sucht", "schreibt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PDS", "ADV", "APPR", "NN", "KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Ich will inde\u00df voraus die s\u00fcsse Stunde k\u00fcssen,", "tokens": ["Ich", "will", "in\u00b7de\u00df", "vo\u00b7raus", "die", "s\u00fcs\u00b7se", "Stun\u00b7de", "k\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Da deine Gegenwart mich wiederum erquickt.", "tokens": ["Da", "dei\u00b7ne", "Ge\u00b7gen\u00b7wart", "mich", "wie\u00b7de\u00b7rum", "er\u00b7quickt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Denn glaube, da\u00df mein Hertz, der Himmel wird es wissen,", "tokens": ["Denn", "glau\u00b7be", ",", "da\u00df", "mein", "Hertz", ",", "der", "Him\u00b7mel", "wird", "es", "wis\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPOSAT", "NN", "$,", "ART", "NN", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Dir tausend Seuffzer schon statt Pferd und Wagen schickt.", "tokens": ["Dir", "tau\u00b7send", "Seuff\u00b7zer", "schon", "statt", "Pferd", "und", "Wa\u00b7gen", "schickt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "CARD", "NN", "ADV", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}