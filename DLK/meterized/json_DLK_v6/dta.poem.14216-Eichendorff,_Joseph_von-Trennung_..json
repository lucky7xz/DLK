{"dta.poem.14216": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Trennung .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Wie oft, wenn wir im Garten ruhig waren,", "tokens": ["Wie", "oft", ",", "wenn", "wir", "im", "Gar\u00b7ten", "ru\u00b7hig", "wa\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$,", "KOUS", "PPER", "APPRART", "NN", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sagte mein Bruder mir vor vielen Jahren:", "tokens": ["Sag\u00b7te", "mein", "Bru\u00b7der", "mir", "vor", "vie\u00b7len", "Jah\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PPER", "APPR", "PIAT", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "\u201edem sch\u00f6nen Lenz gleicht recht die erste Liebe.", "tokens": ["\u201e", "dem", "sch\u00f6\u00b7nen", "Lenz", "gleicht", "recht", "die", "ers\u00b7te", "Lie\u00b7be", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wann drau\u00dfen neu geschm\u00fcckt die Fr\u00fchlingsb\u00fchne,", "tokens": ["Wann", "drau\u00b7\u00dfen", "neu", "ge\u00b7schm\u00fcckt", "die", "Fr\u00fch\u00b7lings\u00b7b\u00fch\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "VVPP", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die Reiter blitzend unten zieh'n durch's Gr\u00fcne,", "tokens": ["Die", "Rei\u00b7ter", "blit\u00b7zend", "un\u00b7ten", "zieh'n", "durch's", "Gr\u00fc\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADV", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "In blauer Luft die Lerchen lustig schwirren,", "tokens": ["In", "blau\u00b7er", "Luft", "die", "Ler\u00b7chen", "lus\u00b7tig", "schwir\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "L\u00e4\u00dft sie sich weit in's Land hinaus verf\u00fchren.", "tokens": ["L\u00e4\u00dft", "sie", "sich", "weit", "in's", "Land", "hin\u00b7aus", "ver\u00b7f\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADJD", "APPRART", "NN", "APZR", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Fragt nicht wohin, und mag sich gern verirren,", "tokens": ["Fragt", "nicht", "wo\u00b7hin", ",", "und", "mag", "sich", "gern", "ver\u00b7ir\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "PWAV", "$,", "KON", "VMFIN", "PRF", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Den Stimmen folgend, die sie wirrend f\u00fchren.", "tokens": ["Den", "Stim\u00b7men", "fol\u00b7gend", ",", "die", "sie", "wir\u00b7rend", "f\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "PRELS", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Da wendet auf den Feldern sich der Wind,", "tokens": ["Da", "wen\u00b7det", "auf", "den", "Fel\u00b7dern", "sich", "der", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Die V\u00f6gel hoch durch Nebel zieh'n nach Haus;", "tokens": ["Die", "V\u00f6\u00b7gel", "hoch", "durch", "Ne\u00b7bel", "zieh'n", "nach", "Haus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Es wird so still, das sch\u00f6ne Fest ist aus.", "tokens": ["Es", "wird", "so", "still", ",", "das", "sch\u00f6\u00b7ne", "Fest", "ist", "aus", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "ART", "ADJA", "NN", "VAFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Gar weit die Heimath liegt, das sch\u00f6ne Kind", "tokens": ["Gar", "weit", "die", "Hei\u00b7math", "liegt", ",", "das", "sch\u00f6\u00b7ne", "Kind"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "ART", "NN", "VVFIN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Find't nicht nach Hause mehr, nicht weiter fort \u2014", "tokens": ["Find't", "nicht", "nach", "Hau\u00b7se", "mehr", ",", "nicht", "wei\u00b7ter", "fort"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "APPR", "NN", "ADV", "$,", "PTKNEG", "ADV", "PTKVZ", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "H\u00fct' dich, such' fr\u00fch dir einen sichern Port!\u201c", "tokens": ["H\u00fct'", "dich", ",", "such'", "fr\u00fch", "dir", "ei\u00b7nen", "si\u00b7chern", "Port", "!", "\u201c"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "$,", "VVFIN", "ADJD", "PPER", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}