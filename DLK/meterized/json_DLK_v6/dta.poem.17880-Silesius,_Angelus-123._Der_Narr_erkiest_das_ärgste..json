{"dta.poem.17880": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "123.  Der Narr erkiest das \u00e4rgste.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Narr ist/ der den Stok f\u00fcrs Kaisers Burg erkiest;", "tokens": ["Ein", "Narr", "ist", "/", "der", "den", "Stok", "f\u00fcrs", "Kai\u00b7sers", "Burg", "er\u00b7kiest", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$(", "ART", "ART", "NN", "APPRART", "NN", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der lieber in der Welt als in dem Himmel ist.", "tokens": ["Der", "lie\u00b7ber", "in", "der", "Welt", "als", "in", "dem", "Him\u00b7mel", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ART", "NN", "KOKOM", "APPR", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}