{"dta.poem.8362": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Ueberschrift an einen Kirch-  \n hoff.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198782", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Wandrer stehe still! es sind hier heil-\nge Schwellen", "tokens": ["Mein", "Wand\u00b7rer", "ste\u00b7he", "still", "!", "es", "sind", "hier", "heil", "ge", "Schwel\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKVZ", "$.", "PPER", "VAFIN", "ADV", "TRUNC", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Beschaue erst das Bild sieh dies Ge-", "tokens": ["Be\u00b7schau\u00b7e", "erst", "das", "Bild", "sieh", "dies", "Ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN", "VVFIN", "PDS", "TRUNC"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Hier steht ein Todtenkopf, und", "tokens": ["Hier", "steht", "ein", "Tod\u00b7ten\u00b7kopf", ",", "und"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "KON"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie bald ein jeder Mensch der sterblich, sterben", "tokens": ["Wie", "bald", "ein", "je\u00b7der", "Mensch", "der", "sterb\u00b7lich", ",", "ster\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PWAV", "ADV", "ART", "PIAT", "NN", "ART", "ADJD", "$,", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Tritt n\u00e4her nur herbei auf diese Leichen-Auen", "tokens": ["Tritt", "n\u00e4\u00b7her", "nur", "her\u00b7bei", "auf", "die\u00b7se", "Lei\u00b7chen\u00b7Au\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "ADV", "ADV", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wo Hoh und Niedrige, wo Jung und Alte ruhn;", "tokens": ["Wo", "Hoh", "und", "Nied\u00b7ri\u00b7ge", ",", "wo", "Jung", "und", "Al\u00b7te", "ruhn", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "KON", "NN", "$,", "PWAV", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Du kanst in ihrem Grab dein eigen Grabmahl schau-", "tokens": ["Du", "kanst", "in", "ih\u00b7rem", "Grab", "dein", "ei\u00b7gen", "Grab\u00b7mahl", "schau"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "NN", "PPOSAT", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die Klugheit denkt daran bei allen ihren Thun.", "tokens": ["Die", "Klug\u00b7heit", "denkt", "da\u00b7ran", "bei", "al\u00b7len", "ih\u00b7ren", "Thun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PAV", "APPR", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Bedenk wie mancher liegt hier unter deinen F\u00fcs-", "tokens": ["Be\u00b7denk", "wie", "man\u00b7cher", "liegt", "hier", "un\u00b7ter", "dei\u00b7nen", "F\u00fcs"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KOKOM", "PIS", "VVFIN", "ADV", "APPR", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Der an sein Sterblichseyn nie ohn Verdrus ge-", "tokens": ["Der", "an", "sein", "Sterb\u00b7lich\u00b7seyn", "nie", "ohn", "Ver\u00b7drus", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PPOSAT", "NN", "ADV", "APPR", "NE", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Der aber zu der Zeit, dennoch hat sterben m\u00fcs-", "tokens": ["Der", "a\u00b7ber", "zu", "der", "Zeit", ",", "den\u00b7noch", "hat", "ster\u00b7ben", "m\u00fcs"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "ART", "NN", "$,", "ADV", "VAFIN", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Da ihn das Lebensziel zum Todes Port gebracht;", "tokens": ["Da", "ihn", "das", "Le\u00b7bens\u00b7ziel", "zum", "To\u00b7des", "Port", "ge\u00b7bracht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPRART", "NN", "NE", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Stirb deinen S\u00fcnden ab; so kanst du ruhig ster-", "tokens": ["Stirb", "dei\u00b7nen", "S\u00fcn\u00b7den", "ab", ";", "so", "kanst", "du", "ru\u00b7hig", "ster"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPOSAT", "NN", "PTKVZ", "$.", "ADV", "VMFIN", "PPER", "ADJD", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Verlasse dich auf dem, der unser Lebens-F\u00fcrst;", "tokens": ["Ver\u00b7las\u00b7se", "dich", "auf", "dem", ",", "der", "un\u00b7ser", "Le\u00b7bens\u00b7F\u00fcrst", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "$,", "PRELS", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "So kanst du dort die Kron des ewgen Lebens er-", "tokens": ["So", "kanst", "du", "dort", "die", "Kron", "des", "ew\u00b7gen", "Le\u00b7bens", "er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ART", "NN", "ART", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.16": {"text": "Wenn du, wer weis wie bald, allhie zur Leiche", "tokens": ["Wenn", "du", ",", "wer", "weis", "wie", "bald", ",", "all\u00b7hie", "zur", "Lei\u00b7che"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "PWS", "PTKVZ", "KOKOM", "ADV", "$,", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Nim diese Lehr in Acht und geh auf guten Ste-", "tokens": ["Nim", "die\u00b7se", "Lehr", "in", "Acht", "und", "geh", "auf", "gu\u00b7ten", "Ste"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "PDAT", "NN", "APPR", "CARD", "KON", "VVFIN", "APPR", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Geh Wandrer nur getrost bei diesen Gr\u00e4bern fort:", "tokens": ["Geh", "Wand\u00b7rer", "nur", "ge\u00b7trost", "bei", "die\u00b7sen", "Gr\u00e4\u00b7bern", "fort", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADV", "ADJD", "APPR", "PDAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Bedenke da\u00df das Ziel von allen deinen Wegen,", "tokens": ["Be\u00b7den\u00b7ke", "da\u00df", "das", "Ziel", "von", "al\u00b7len", "dei\u00b7nen", "We\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOUS", "ART", "NN", "APPR", "PIAT", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Dich endlich wieder f\u00fchrt zur Ruh an solchen Ort.", "tokens": ["Dich", "end\u00b7lich", "wie\u00b7der", "f\u00fchrt", "zur", "Ruh", "an", "sol\u00b7chen", "Ort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "VVFIN", "APPRART", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}