 "$,", "KOUS", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Geliebter Todten steht.", "tokens": ["Ge\u00b7lieb\u00b7ter", "Tod\u00b7ten", "steht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.18": {"line.1": {"text": "So merken in des Felsen sich'rer Ritze", "tokens": ["So", "mer\u00b7ken", "in", "des", "Fel\u00b7sen", "sich'\u00b7rer", "Rit\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Adlers Kinder nicht,", "tokens": ["Des", "Ad\u00b7lers", "Kin\u00b7der", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKNEG", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df Wogen sich an seinem Fu\u00dfe brechen,", "tokens": ["Da\u00df", "Wo\u00b7gen", "sich", "an", "sei\u00b7nem", "Fu\u00b7\u00dfe", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PRF", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Nordwind ihn bek\u00e4mpft.", "tokens": ["Und", "Nord\u00b7wind", "ihn", "be\u00b7k\u00e4mpft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.19": {"line.1": {"text": "Und war es dir so nicht genug gepr\u00fcfet", "tokens": ["Und", "war", "es", "dir", "so", "nicht", "ge\u00b7nug", "ge\u00b7pr\u00fc\u00b7fet"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "PPER", "ADV", "PTKNEG", "ADV", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": 