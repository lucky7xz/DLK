{"textgrid.poem.41192": {"metadata": {"author": {"name": "Denis, Michael", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sch\u00f6n sangst du, Gottvolk! in die Feiersaiten", "genre": "verse", "period": "N.A.", "pub_year": 1764, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6n sangst du, Gottvolk! in die Feiersaiten", "tokens": ["Sch\u00f6n", "sangst", "du", ",", "Gott\u00b7volk", "!", "in", "die", "Fei\u00b7er\u00b7sai\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "$,", "NN", "$.", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der F\u00fcrstinn Fr\u00f6mmigkeit.", "tokens": ["Der", "F\u00fcrs\u00b7tinn", "Fr\u00f6m\u00b7mig\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mir ward dein Lied in meines Alters Herbste,", "tokens": ["Mir", "ward", "dein", "Lied", "in", "mei\u00b7nes", "Al\u00b7ters", "Herbs\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie warmer Sonnenstral.", "tokens": ["Wie", "war\u00b7mer", "Son\u00b7nen\u00b7stral", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wen liebt Allvater unter Menschenherrschern", "tokens": ["Wen", "liebt", "All\u00b7va\u00b7ter", "un\u00b7ter", "Men\u00b7schen\u00b7herr\u00b7schern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So, wie Theresia?", "tokens": ["So", ",", "wie", "The\u00b7re\u00b7sia", "?"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "NE", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Doch hat er einen unter Menschenherrschern", "tokens": ["Doch", "hat", "er", "ei\u00b7nen", "un\u00b7ter", "Men\u00b7schen\u00b7herr\u00b7schern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ART", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auch so gepr\u00fcft wie Sie?", "tokens": ["Auch", "so", "ge\u00b7pr\u00fcft", "wie", "Sie", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "KOKOM", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Von Ihrer Starkmuth will ich Greise singen,", "tokens": ["Von", "Ih\u00b7rer", "Stark\u00b7muth", "will", "ich", "Grei\u00b7se", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und seiner schweren Hand.", "tokens": ["Und", "sei\u00b7ner", "schwe\u00b7ren", "Hand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er dr\u00fcckte Sie. Sie stand, wie Felsen stehen,", "tokens": ["Er", "dr\u00fcck\u00b7te", "Sie", ".", "Sie", "stand", ",", "wie", "Fel\u00b7sen", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$.", "PPER", "VVFIN", "$,", "PWAV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und hielt die Pr\u00fcfung aus.", "tokens": ["Und", "hielt", "die", "Pr\u00fc\u00b7fung", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Vom Dr\u00e4uen Ihrer Feinde will ich schweigen.", "tokens": ["Vom", "Dr\u00e4u\u00b7en", "Ih\u00b7rer", "Fein\u00b7de", "will", "ich", "schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Freund Bartmar sang davon.", "tokens": ["Freund", "Bart\u00b7mar", "sang", "da\u00b7von", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nur ihres Hauses innerliche Wehen,", "tokens": ["Nur", "ih\u00b7res", "Hau\u00b7ses", "in\u00b7ner\u00b7li\u00b7che", "We\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nur diese sind mein Lied.", "tokens": ["Nur", "die\u00b7se", "sind", "mein", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Vergebens flammt um hohe F\u00fcrstenhallen", "tokens": ["Ver\u00b7ge\u00b7bens", "flammt", "um", "ho\u00b7he", "F\u00fcrs\u00b7ten\u00b7hal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des wachen Schwertes Blitz.", "tokens": ["Des", "wa\u00b7chen", "Schwer\u00b7tes", "Blitz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vergebens l\u00e4rmen bunter Freuden Stimmen", "tokens": ["Ver\u00b7ge\u00b7bens", "l\u00e4r\u00b7men", "bun\u00b7ter", "Freu\u00b7den", "Stim\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Stets um der Gro\u00dfen Ohr.", "tokens": ["Stets", "um", "der", "Gro\u00b7\u00dfen", "Ohr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Unaufgehalten dringt, den Blitz des Schwertes", "tokens": ["Un\u00b7auf\u00b7ge\u00b7hal\u00b7ten", "dringt", ",", "den", "Blitz", "des", "Schwer\u00b7tes"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "$,", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vorbei, der Schmerz hinein.", "tokens": ["Vor\u00b7bei", ",", "der", "Schmerz", "hin\u00b7ein", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ART", "NN", "PTKVZ", "$."], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.3": {"text": "Und \u00f6fter \u00fcbert\u00f6nt der Freuden Stimmen", "tokens": ["Und", "\u00f6f\u00b7ter", "\u00fc\u00b7ber\u00b7t\u00f6nt", "der", "Freu\u00b7den", "Stim\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der bleichen Klage Laut.", "tokens": ["Der", "blei\u00b7chen", "Kla\u00b7ge", "Laut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Ein Gatte war Theresien beschieden,", "tokens": ["Ein", "Gat\u00b7te", "war", "The\u00b7re\u00b7si\u00b7en", "be\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NE", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "War ihres Herzens Lust.", "tokens": ["War", "ih\u00b7res", "Her\u00b7zens", "Lust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich singe nicht, wie sie den Gatten liebte.", "tokens": ["Ich", "sin\u00b7ge", "nicht", ",", "wie", "sie", "den", "Gat\u00b7ten", "lieb\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWAV", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Freund Huldrich sang davon.", "tokens": ["Freund", "Huld\u00b7rich", "sang", "da\u00b7von", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Ein Augenblick! kaum segnen seine Lippen", "tokens": ["Ein", "Au\u00b7gen\u00b7blick", "!", "kaum", "seg\u00b7nen", "sei\u00b7ne", "Lip\u00b7pen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$.", "ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Noch Gattinn, Kinder, Volk.", "tokens": ["Noch", "Gat\u00b7tinn", ",", "Kin\u00b7der", ",", "Volk", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein unversehner Augenblick! Besessen,", "tokens": ["Ein", "un\u00b7ver\u00b7seh\u00b7ner", "Au\u00b7gen\u00b7blick", "!", "Be\u00b7ses\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vermi\u00dft, Gesund, und Todt!", "tokens": ["Ver\u00b7mi\u00dft", ",", "Ge\u00b7sund", ",", "und", "Todt", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Ein J\u00fcngling war des ganzen Volkes Liebe,", "tokens": ["Ein", "J\u00fcng\u00b7ling", "war", "des", "gan\u00b7zen", "Vol\u00b7kes", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der F\u00fcrstinn zweiter Sohn.", "tokens": ["Der", "F\u00fcrs\u00b7tinn", "zwei\u00b7ter", "Sohn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Voll Hoffnung schien des J\u00fcnglings Pfad zum Ruhme,", "tokens": ["Voll", "Hoff\u00b7nung", "schien", "des", "J\u00fcng\u00b7lings", "Pfad", "zum", "Ruh\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "VVFIN", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zum Grabe war's der Pfad.", "tokens": ["Zum", "Gra\u00b7be", "wa\u00b7r's", "der", "Pfad", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.10": {"line.1": {"text": "Der T\u00f6chter eine, m\u00fctterlicher Gaben", "tokens": ["Der", "T\u00f6ch\u00b7ter", "ei\u00b7ne", ",", "m\u00fct\u00b7ter\u00b7li\u00b7cher", "Ga\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ART", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verj\u00fcngtes, \u00e4chtes Bild,", "tokens": ["Ver\u00b7j\u00fcng\u00b7tes", ",", "\u00e4ch\u00b7tes", "Bild", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bot schon die Hand dem k\u00f6niglichen Freier", "tokens": ["Bot", "schon", "die", "Hand", "dem", "k\u00f6\u00b7nig\u00b7li\u00b7chen", "Frei\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN", "ART", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Da griff der Tod darnach.", "tokens": ["Da", "griff", "der", "Tod", "dar\u00b7nach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PAV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Zwo Schnuren, Ihres Josephs jede w\u00fcrdig,", "tokens": ["Zwo", "Schnu\u00b7ren", ",", "Ih\u00b7res", "Jo\u00b7se\u00b7phs", "je\u00b7de", "w\u00fcr\u00b7dig", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "$,", "PPOSAT", "NN", "PIAT", "ADJD", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Erhab'ner F\u00fcrsten Blut,", "tokens": ["Er\u00b7hab'\u00b7ner", "F\u00fcrs\u00b7ten", "Blut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Berief Allvater aus der Schwieger Armen", "tokens": ["Be\u00b7rief", "All\u00b7va\u00b7ter", "aus", "der", "Schwie\u00b7ger", "Ar\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "(ach vor der Zeit!) zu sich.", "tokens": ["(", "ach", "vor", "der", "Zeit", "!", ")", "zu", "sich", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["$(", "ADV", "APPR", "ART", "NN", "$.", "$(", "APPR", "PRF", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.12": {"line.1": {"text": "Doch spro\u00df ein Bl\u00fcmchen von der Ersten H\u00fcgel,", "tokens": ["Doch", "spro\u00df", "ein", "Bl\u00fcm\u00b7chen", "von", "der", "Ers\u00b7ten", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Bl\u00fcmchen fein und zart.", "tokens": ["Ein", "Bl\u00fcm\u00b7chen", "fein", "und", "zart", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nun fing es an, den Knospen aufzuschlie\u00dfen;", "tokens": ["Nun", "fing", "es", "an", ",", "den", "Knos\u00b7pen", "auf\u00b7zu\u00b7schlie\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nun ward es abgem\u00e4ht.", "tokens": ["Nun", "ward", "es", "ab\u00b7ge\u00b7m\u00e4ht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Ha, Tod! in welche Trauerwolken h\u00fcllet", "tokens": ["Ha", ",", "Tod", "!", "in", "wel\u00b7che", "Trau\u00b7er\u00b7wol\u00b7ken", "h\u00fcl\u00b7let"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "NN", "$.", "APPR", "PWAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dein Hauch das Kaiserhaus!", "tokens": ["Dein", "Hauch", "das", "Kai\u00b7ser\u00b7haus", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Kaum kann ich noch durch das verstummte Dunkel", "tokens": ["Kaum", "kann", "ich", "noch", "durch", "das", "ver\u00b7stumm\u00b7te", "Dun\u00b7kel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bis auf die F\u00fcrstinn seh'n.", "tokens": ["Bis", "auf", "die", "F\u00fcrs\u00b7tinn", "seh'", "n."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["APPR", "APPR", "ART", "NN", "VVFIN", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Sie steht, den Blick geheftet an den Himmel,", "tokens": ["Sie", "steht", ",", "den", "Blick", "ge\u00b7hef\u00b7tet", "an", "den", "Him\u00b7mel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Allvaters Priesterinn,", "tokens": ["All\u00b7va\u00b7ters", "Pries\u00b7te\u00b7rinn", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und opfert jedes treu geliebte Leben", "tokens": ["Und", "op\u00b7fert", "je\u00b7des", 