": ["KON", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.29": {"line.1": {"text": "Sie ward gesund, und eben jene Blicke", "tokens": ["Sie", "ward", "ge\u00b7sund", ",", "und", "e\u00b7ben", "je\u00b7ne", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "KON", "ADV", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Sie dem Tode warf,", "tokens": ["Die", "Sie", "dem", "To\u00b7de", "warf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die warf Sie nun, o Leben, dir entgegen,", "tokens": ["Die", "warf", "Sie", "nun", ",", "o", "Le\u00b7ben", ",", "dir", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "$,", "FM", "NN", "$,", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "O Leben, uns're Lust!", "tokens": ["O", "Le\u00b7ben", ",", "un\u00b7s'\u00b7re", "Lust", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.30": {"line.1": {"text": "Doch sing' ich sie, die namenlosen Freuden,", "tokens": ["Doch", "sing'", "ich", "sie", ",", "die", "na\u00b7men\u00b7lo\u00b7sen", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des frohen Vaterlands?", "tokens": ["Des", "fro\u00b7hen", "Va\u00b7ter\u00b7lands", "?"], "token_info": ["word", "word", "word", "punct"