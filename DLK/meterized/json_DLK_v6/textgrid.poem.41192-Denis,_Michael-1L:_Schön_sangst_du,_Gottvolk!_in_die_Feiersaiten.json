rd", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nur Sie lag ruhig da.", "tokens": ["Nur", "Sie", "lag", "ru\u00b7hig", "da", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.26": {"line.1": {"text": "So sah ich oft in meinen Jugendlocken", "tokens": ["So", "sah", "ich", "oft", "in", "mei\u00b7nen", "Ju\u00b7gen\u00b7dlo\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein luftiges Gebirg,", "tokens": ["Ein", "luf\u00b7ti\u00b7ges", "Ge\u00b7birg", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den Gipfel hell, die Mitte neblich, unten", "tokens": ["Den", "Gip\u00b7fel", "hell", ",", "die", "Mit\u00b7te", "neb\u00b7lich", ",", "un\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "ADJD", "$,", "ART", "NN", "ADJD", "$,", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Donnerwolken Nacht.", "tokens": ["Der", "Don\u00b7ner\u00b7wol\u00b7ken", "Nacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.27": {"line.1": {"text": "Sie lag der Himmelsf\u00fcgung unterworfen,", "tokens": ["Sie", "lag", "der", "Him\u00b7mels\u00b7f\u00fc\u00b7gung", "un\u00b7ter\u00b7wor\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "li