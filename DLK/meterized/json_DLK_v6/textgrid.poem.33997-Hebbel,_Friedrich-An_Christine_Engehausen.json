{"textgrid.poem.33997": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "An Christine Engehausen", "genre": "verse", "period": "N.A.", "pub_year": 1846, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du tr\u00e4nkst des Dichters d\u00e4mmernde Gestalten,", "tokens": ["Du", "tr\u00e4nkst", "des", "Dich\u00b7ters", "d\u00e4m\u00b7mern\u00b7de", "Ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die \u00e4ngstlich zwischen Sein und Nichtsein schweben,", "tokens": ["Die", "\u00e4ngst\u00b7lich", "zwi\u00b7schen", "Sein", "und", "Nich\u00b7tsein", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "PPOSAT", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit deinem Blut, und giebst den Schatten Leben,", "tokens": ["Mit", "dei\u00b7nem", "Blut", ",", "und", "giebst", "den", "Schat\u00b7ten", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "KON", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In denen ungebor'ne Seelen walten.", "tokens": ["In", "de\u00b7nen", "un\u00b7ge\u00b7bor'\u00b7ne", "See\u00b7len", "wal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich aber m\u00f6gte nicht zu fr\u00fch' erkalten,", "tokens": ["Ich", "a\u00b7ber", "m\u00f6g\u00b7te", "nicht", "zu", "fr\u00fch'", "er\u00b7kal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "PTKNEG", "PTKA", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Zeit die Form zu dem Gehalt zu geben", "tokens": ["Der", "Zeit", "die", "Form", "zu", "dem", "Ge\u00b7halt", "zu", "ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "APPR", "ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und \u00fcber sich hinaus sie zu erheben", "tokens": ["Und", "\u00fc\u00b7ber", "sich", "hin\u00b7aus", "sie", "zu", "er\u00b7he\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PRF", "APZR", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Durch neuer Sch\u00f6nheit sch\u00fcchternes Entfalten.", "tokens": ["Durch", "neu\u00b7er", "Sch\u00f6n\u00b7heit", "sch\u00fcch\u00b7ter\u00b7nes", "Ent\u00b7fal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch dieses Deutschland wird uns schwer erwarmen,", "tokens": ["Doch", "die\u00b7ses", "Deutschland", "wird", "uns", "schwer", "er\u00b7war\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VAFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und eh' wir's denken, steh'n wir ab, verdrossen,", "tokens": ["Und", "eh'", "wir's", "den\u00b7ken", ",", "steh'n", "wir", "ab", ",", "ver\u00b7dros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "VVINF", "$,", "VVFIN", "PPER", "PTKVZ", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Drum la\u00df uns Eins das Andere belohnen.", "tokens": ["Drum", "la\u00df", "uns", "Eins", "das", "An\u00b7de\u00b7re", "be\u00b7loh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "NN", "ART", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wo treu und fest sich Mann und Weib umarmen,", "tokens": ["Wo", "treu", "und", "fest", "sich", "Mann", "und", "Weib", "um\u00b7ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "KON", "VVFIN", "PRF", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da ist ein Kreis, da ist der Kreis geschlossen,", "tokens": ["Da", "ist", "ein", "Kreis", ",", "da", "ist", "der", "Kreis", "ge\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "ADV", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In dem die h\u00f6chsten Menschenfreuden wohnen.", "tokens": ["In", "dem", "die", "h\u00f6chs\u00b7ten", "Men\u00b7schen\u00b7freu\u00b7den", "woh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}