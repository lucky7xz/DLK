{"textgrid.poem.66678": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Und Liliensinn soll meinen Kampf verkl\u00e4ren,", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und Liliensinn soll meinen Kampf verkl\u00e4ren,", "tokens": ["Und", "Li\u00b7li\u00b7en\u00b7sinn", "soll", "mei\u00b7nen", "Kampf", "ver\u00b7kl\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+----+-+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Denn Kampf wird w\u00e4hren, bis die Flamme loht,", "tokens": ["Denn", "Kampf", "wird", "w\u00e4h\u00b7ren", ",", "bis", "die", "Flam\u00b7me", "loht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "VVINF", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Darin dereinst sich wieder wird geb\u00e4ren", "tokens": ["Da\u00b7rin", "de\u00b7reinst", "sich", "wie\u00b7der", "wird", "ge\u00b7b\u00e4\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "ADV", "PRF", "ADV", "VAFIN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Geheimnisvoll das Leben durch den Tod.", "tokens": ["Ge\u00b7heim\u00b7nis\u00b7voll", "das", "Le\u00b7ben", "durch", "den", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Mich treibt ein Geist aus freiern Liebessph\u00e4ren,", "tokens": ["Mich", "treibt", "ein", "Geist", "aus", "frei\u00b7ern", "Lie\u00b7bes\u00b7sph\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der liegt in Fehde mit dem Geist der Not", "tokens": ["Der", "liegt", "in", "Feh\u00b7de", "mit", "dem", "Geist", "der", "Not"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "NN", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und wirft, bis sich verwandeln diese Z\u00fcge,", "tokens": ["Und", "wirft", ",", "bis", "sich", "ver\u00b7wan\u00b7deln", "die\u00b7se", "Z\u00fc\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PRF", "VVFIN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Den Handschuh hin der Knechtschaft und der L\u00fcge.", "tokens": ["Den", "Hand\u00b7schuh", "hin", "der", "Knecht\u00b7schaft", "und", "der", "L\u00fc\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Viel kleine G\u00f6tzen sind die Abgesandten", "tokens": ["Viel", "klei\u00b7ne", "G\u00f6t\u00b7zen", "sind", "die", "Ab\u00b7ge\u00b7sand\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der gro\u00dfen G\u00f6tzen L\u00fcge, Knechtschaft, Not,", "tokens": ["Der", "gro\u00b7\u00dfen", "G\u00f6t\u00b7zen", "L\u00fc\u00b7ge", ",", "Knecht\u00b7schaft", ",", "Not", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein Schwarm von festverh\u00e4kelten Verwandten,", "tokens": ["Ein", "Schwarm", "von", "fest\u00b7ver\u00b7h\u00e4\u00b7kel\u00b7ten", "Ver\u00b7wand\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gebr\u00fctet von der Nacht, gezeugt vom Kot.", "tokens": ["Ge\u00b7br\u00fc\u00b7tet", "von", "der", "Nacht", ",", "ge\u00b7zeugt", "vom", "Kot", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "$,", "VVPP", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Das sind die ewig aus dem Licht Verbannten,", "tokens": ["Das", "sind", "die", "e\u00b7wig", "aus", "dem", "Licht", "Ver\u00b7bann\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJD", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Verr\u00e4ter Gottes gleich Ischariot \u2013", "tokens": ["Ver\u00b7r\u00e4\u00b7ter", "Got\u00b7tes", "gleich", "I\u00b7scha\u00b7ri\u00b7ot", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ADV", "NE", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Die Riesenwanzen, die der Sterne besten", "tokens": ["Die", "Rie\u00b7sen\u00b7wan\u00b7zen", ",", "die", "der", "Ster\u00b7ne", "bes\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Mit ihrem gr\u00e4ulichen Gestank verpesten.", "tokens": ["Mit", "ih\u00b7rem", "gr\u00e4u\u00b7li\u00b7chen", "Ge\u00b7stank", "ver\u00b7pes\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Sinnbild f\u00fcr Unergr\u00fcndliches wird dauern,", "tokens": ["Sinn\u00b7bild", "f\u00fcr", "Un\u00b7er\u00b7gr\u00fcnd\u00b7li\u00b7ches", "wird", "dau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "VAFIN", "VVINF", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Solang der Mensch sich weltgebunden f\u00fchlt,", "tokens": ["So\u00b7lang", "der", "Mensch", "sich", "welt\u00b7ge\u00b7bun\u00b7den", "f\u00fchlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PRF", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du mu\u00dft vor dem Unendlichen erschauern", "tokens": ["Du", "mu\u00dft", "vor", "dem", "Un\u00b7end\u00b7li\u00b7chen", "er\u00b7schau\u00b7ern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "ART", "NN", "VVINF"], "meter": "-+--++-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und wirst vom R\u00e4tsel gr\u00fcblerisch durchw\u00fchlt.", "tokens": ["Und", "wirst", "vom", "R\u00e4t\u00b7sel", "gr\u00fcb\u00b7ler\u00b7isch", "durch\u00b7w\u00fchlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPRART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Erkenntnis st\u00f6\u00dft an nie erklommne Mauern,", "tokens": ["Er\u00b7kennt\u00b7nis", "st\u00f6\u00dft", "an", "nie", "er\u00b7klomm\u00b7ne", "Mau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Nie wird der Brand der Sehnenden gek\u00fchlt,", "tokens": ["Nie", "wird", "der", "Brand", "der", "Seh\u00b7nen\u00b7den", "ge\u00b7k\u00fchlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Der tiefer brennt als aller Weisen Wissen,", "tokens": ["Der", "tie\u00b7fer", "brennt", "als", "al\u00b7ler", "Wei\u00b7sen", "Wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "KOKOM", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Bis er verlischt in fahlen Finsternissen.", "tokens": ["Bis", "er", "ver\u00b7lischt", "in", "fah\u00b7len", "Fins\u00b7ter\u00b7nis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und ewig werden arme Seelen jammern", "tokens": ["Und", "e\u00b7wig", "wer\u00b7den", "ar\u00b7me", "See\u00b7len", "jam\u00b7mern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VAFIN", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nach reichen G\u00fctern, die unsterblich sind,", "tokens": ["Nach", "rei\u00b7chen", "G\u00fc\u00b7tern", ",", "die", "uns\u00b7terb\u00b7lich", "sind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PRELS", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und bis die Erde geht aus ihren Klammern,", "tokens": ["Und", "bis", "die", "Er\u00b7de", "geht", "aus", "ih\u00b7ren", "Klam\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wird Himmel baun das schwache Menschenkind.", "tokens": ["Wird", "Him\u00b7mel", "baun", "das", "schwa\u00b7che", "Men\u00b7schen\u00b7kind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Den Weltbejublern und den Weltverdammern", "tokens": ["Den", "Welt\u00b7be\u00b7jub\u00b7lern", "und", "den", "Welt\u00b7ver\u00b7dam\u00b7mern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wird vor dem einen Punkt die Brille blind,", "tokens": ["Wird", "vor", "dem", "ei\u00b7nen", "Punkt", "die", "Bril\u00b7le", "blind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "ART", "NN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Sie m\u00fcssen Fluch und Segen unterlassen", "tokens": ["Sie", "m\u00fcs\u00b7sen", "Fluch", "und", "Se\u00b7gen", "un\u00b7ter\u00b7las\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "NN", "KON", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und schweigend nehmen, was sie nicht erfassen.", "tokens": ["Und", "schwei\u00b7gend", "neh\u00b7men", ",", "was", "sie", "nicht", "er\u00b7fas\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVINF", "$,", "PRELS", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Den schlichten Glauben will ich nicht verh\u00f6hnen,", "tokens": ["Den", "schlich\u00b7ten", "Glau\u00b7ben", "will", "ich", "nicht", "ver\u00b7h\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der sich am halben Wissen schlecht gen\u00fcgt,", "tokens": ["Der", "sich", "am", "hal\u00b7ben", "Wis\u00b7sen", "schlecht", "ge\u00b7n\u00fcgt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPRART", "ADJA", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nie sprengt die Pforten auf des Zweiflers St\u00f6hnen,", "tokens": ["Nie", "sprengt", "die", "Pfor\u00b7ten", "auf", "des", "Zweif\u00b7lers", "St\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bis er verzichtet und sich ruhig f\u00fcgt ...", "tokens": ["Bis", "er", "ver\u00b7zich\u00b7tet", "und", "sich", "ru\u00b7hig", "f\u00fcgt", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "KON", "PRF", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Doch Blitz und Donner sollen niederdr\u00f6hnen", "tokens": ["Doch", "Blitz", "und", "Don\u00b7ner", "sol\u00b7len", "nie\u00b7der\u00b7dr\u00f6h\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Auf eine Kirche, die das Volk bel\u00fcgt", "tokens": ["Auf", "ei\u00b7ne", "Kir\u00b7che", ",", "die", "das", "Volk", "be\u00b7l\u00fcgt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und sich samt Oberhaupt und Priesteranden", "tokens": ["Und", "sich", "samt", "O\u00b7ber\u00b7haupt", "und", "Pries\u00b7te\u00b7ran\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PRF", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Stets auf Betrug der Menschheit hat verstanden!", "tokens": ["Stets", "auf", "Be\u00b7trug", "der", "Menschheit", "hat", "ver\u00b7stan\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Mit F\u00e4lschung aufges\u00e4ugt von Anbeginne,", "tokens": ["Mit", "F\u00e4l\u00b7schung", "auf\u00b7ge\u00b7s\u00e4ugt", "von", "An\u00b7be\u00b7gin\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Seit Konstantins erlognem Schenkungsakt", "tokens": ["Seit", "Kons\u00b7tan\u00b7tins", "er\u00b7log\u00b7nem", "Schen\u00b7kungs\u00b7akt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Von Sucht nach Herrschaft schwanger und Gewinne,", "tokens": ["Von", "Sucht", "nach", "Herr\u00b7schaft", "schwan\u00b7ger", "und", "Ge\u00b7win\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "APPR", "NN", "ADJD", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Fett m\u00e4stend sich mit allem, was sie packt,", "tokens": ["Fett", "m\u00e4s\u00b7tend", "sich", "mit", "al\u00b7lem", ",", "was", "sie", "packt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PRF", "APPR", "PIS", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Hat sich die ungeheure schwarze Spinne", "tokens": ["Hat", "sich", "die", "un\u00b7ge\u00b7heu\u00b7re", "schwar\u00b7ze", "Spin\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PRF", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Mit Blut \u00bbder dummen Bauern\u00ab vollgesackt", "tokens": ["Mit", "Blut", "\u00bb", "der", "dum\u00b7men", "Bau\u00b7ern", "\u00ab", "voll\u00b7ge\u00b7sackt"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["APPR", "NN", "$(", "ART", "ADJA", "NN", "$(", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und spannt die schlauverkn\u00fcpften Riesennetze,", "tokens": ["Und", "spannt", "die", "schlau\u00b7ver\u00b7kn\u00fcpf\u00b7ten", "Rie\u00b7sen\u00b7net\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Nach Beute l\u00fcstern wie die fr\u00f6mmste Metze.", "tokens": ["Nach", "Beu\u00b7te", "l\u00fcs\u00b7tern", "wie", "die", "fr\u00f6mms\u00b7te", "Met\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}