{"textgrid.poem.56553": {"metadata": {"author": {"name": "Groth, Klaus", "birth": "N.A.", "death": "N.A."}, "title": "1L: Barfot int Sand, inn raschen Schritt,", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Barfot int Sand, inn raschen Schritt,", "tokens": ["Bar\u00b7fot", "int", "Sand", ",", "inn", "ra\u00b7schen", "Schritt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Den glatten Platen kridenwitt,", "tokens": ["Den", "glat\u00b7ten", "Pla\u00b7ten", "kri\u00b7den\u00b7witt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Stramm opsch\u00f6rt den Linnwullenrock,", "tokens": ["Stramm", "op\u00b7sch\u00f6rt", "den", "Linn\u00b7wul\u00b7len\u00b7rock", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Um Liv so kn\u0119bsch asn Pitschenstock", "tokens": ["Um", "Liv", "so", "kn\u0119bsch", "asn", "Pit\u00b7schen\u00b7stock"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "NN", "ADV", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "\u2013 Man kann \u0119r fl\u00f6di mit de Hann'", "tokens": ["\u2013", "Man", "kann", "\u0119r", "fl\u00f6\u00b7di", "mit", "de", "Hann'"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PIS", "VMFIN", "NE", "NE", "APPR", "NE", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vun een H\u00fcft na de anner spann' \u2013", "tokens": ["Vun", "e\u00b7en", "H\u00fcft", "na", "de", "an\u00b7ner", "spann'", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Den eenen Arm so keit inne Sit,", "tokens": ["Den", "e\u00b7e\u00b7nen", "Arm", "so", "keit", "in\u00b7ne", "Sit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.8": {"text": "As wenn man'n \u00d6hr vun'n Theekann s\u00fcht,", "tokens": ["As", "wenn", "man'n", "\u00d6hr", "vun'n", "Thee\u00b7kann", "s\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Umn witten Hals de gr\u00f6ne Drach:", "tokens": ["Umn", "wit\u00b7ten", "Hals", "de", "gr\u00f6\u00b7ne", "Drach", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "En Diern, dat di de Ogen lach!", "tokens": ["En", "Diern", ",", "dat", "di", "de", "O\u00b7gen", "lach", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PRELS", "NE", "NE", "NE", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "En Strohhot mit en breden Rand,", "tokens": ["En", "Stroh\u00b7hot", "mit", "en", "bre\u00b7den", "Rand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Umt runne Kinn en blassroth Band;", "tokens": ["Umt", "run\u00b7ne", "Kinn", "en", "blass\u00b7roth", "Band", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ADJD", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Dat brune Haar inn dicke Tut", "tokens": ["Dat", "bru\u00b7ne", "Haar", "inn", "di\u00b7cke", "Tut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kikt j\u00fcst as \u00fcnnern S\u00fcnnschirm ut.", "tokens": ["Kikt", "j\u00fcst", "as", "\u00fcn\u00b7nern", "S\u00fcnn\u00b7schirm", "ut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "De Ammers klappt bi jeden Schritt,", "tokens": ["De", "Am\u00b7mers", "klappt", "bi", "je\u00b7den", "Schritt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "ADJD", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "De mischen K\u0119den kl\u0153tert mit,", "tokens": ["De", "mi\u00b7schen", "K\u0119\u00b7den", "kl\u0153tert", "mit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.7": {"text": "Un denn int Seel en l\u00fctten K\u0119tel", "tokens": ["Un", "denn", "int", "Seel", "en", "l\u00fct\u00b7ten", "K\u0119tel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "KON", "FM", "FM", "FM", "ADJA", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "De rasselt as en Bund vull Sl\u0153tel.", "tokens": ["De", "ras\u00b7selt", "as", "en", "Bund", "vull", "Sl\u0153tel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "FM", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Se's fr\u00f6h to Been, dats S\u00fcnndagabnd,", "tokens": ["Se's", "fr\u00f6h", "to", "Be\u00b7en", ",", "dats", "S\u00fcnn\u00b7da\u00b7gabnd", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "$,", "KOUS", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "\u0118r Schatz will na de Koppel kamn;", "tokens": ["\u0118r", "Schatz", "will", "na", "de", "Kop\u00b7pel", "kamn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VMFIN", "NE", "NE", "NE", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "De nimt \u0119r n\u00f6s de Melkdrach af,", "tokens": ["De", "nimt", "\u0119r", "n\u00f6s", "de", "Melk\u00b7drach", "af", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Se sp\u0119lt de Dam un str\u0119vt v\u0153raf. \u2013", "tokens": ["Se", "sp\u0119lt", "de", "Dam", "un", "str\u0119vt", "v\u0153raf", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "FM", "FM", "$.", "$("], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "He sitt un sm\u00f6kt opt Heck bi'n Wall", "tokens": ["He", "sitt", "un", "sm\u00f6kt", "opt", "Heck", "bi'n", "Wall"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "FM", "FM", "FM", "NE", "NE", "NE"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Un kikt al lang' den Snittweg dal.", "tokens": ["Un", "kikt", "al", "lang'", "den", "Snitt\u00b7weg", "dal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "ADV", "ART", "NN", "NE", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Sieh an! dar blitzt se achtern Knick", "tokens": ["Sieh", "an", "!", "dar", "blitzt", "se", "ach\u00b7tern", "Knick"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "PTKVZ", "$.", "PTKVZ", "VVFIN", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Un dreiht herop in Ogenblick.", "tokens": ["Un", "dreiht", "he\u00b7rop", "in", "O\u00b7gen\u00b7blick", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "APPR", "NE", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Se driggt de Drach so steil un nett,", "tokens": ["Se", "driggt", "de", "Drach", "so", "steil", "un", "nett", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Asn Leutnant nich sin Epaulett,", "tokens": ["Asn", "Leut\u00b7nant", "nich", "sin", "E\u00b7pau\u00b7lett", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Un smitt de brune Arm so keit,", "tokens": ["Un", "smitt", "de", "bru\u00b7ne", "Arm", "so", "keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "NE", "ADJA", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "As keen Mamsell opt P\u0119rmark deit.", "tokens": ["As", "ke\u00b7en", "Mam\u00b7sell", "opt", "P\u0119r\u00b7mark", "deit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "\u00bbja, dat mag'k lidn, so hest du't drapen!\u00ab \u2013", "tokens": ["\u00bb", "ja", ",", "dat", "mag'k", "lidn", ",", "so", "hest", "du't", "dra\u00b7pen", "!", "\u00ab", "\u2013"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "PTKANT", "$,", "FM.la", "FM.la", "FM.la", "$,", "ADV", "VVFIN", "ADJD", "VVINF", "$.", "$(", "$("], "meter": "+-++-+-+-", "measure": "unknown.measure.penta"}, "line.10": {"text": "He hett al Dor un Sleetbom apen.", "tokens": ["He", "hett", "al", "Dor", "un", "Sleet\u00b7bom", "a\u00b7pen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}}, "stanza.4": {"line.1": {"text": "\u0118r Drach un Ammer sett se dal,", "tokens": ["\u0118r", "Drach", "un", "Am\u00b7mer", "sett", "se", "dal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "De Hot hangt op den Heckenpahl.", "tokens": ["De", "Hot", "hangt", "op", "den", "He\u00b7cken\u00b7pahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "NE", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nu stiggt se d\u0153r dat lange Gras", "tokens": ["Nu", "stiggt", "se", "d\u0153r", "dat", "lan\u00b7ge", "Gras"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "FM", "FM", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Un schint so witt un hett son Hast", "tokens": ["Un", "schint", "so", "witt", "un", "hett", "son", "Hast"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ADV", "VVFIN", "FM", "VAFIN", "XY", "XY"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.5": {"text": "Un singt so n\u00fcdli \u00fcnner de Koh:", "tokens": ["Un", "singt", "so", "n\u00fcd\u00b7li", "\u00fcn\u00b7ner", "de", "Koh", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "FM", "FM", "FM", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "De Melk sus't sach den Takt darto.", "tokens": ["De", "Melk", "sus't", "sach", "den", "Takt", "dar\u00b7to", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "APPR", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Denn sch\u00fcmt de Ammers vull un vuller,", "tokens": ["Denn", "sch\u00fcmt", "de", "Am\u00b7mers", "vull", "un", "vul\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "FM", "FM", "FM", "FM", "FM", "FM", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Un rattsch! hett Hans se oppe Schuller,", "tokens": ["Un", "rattsch", "!", "hett", "Hans", "se", "op\u00b7pe", "Schul\u00b7ler", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "$.", "VAFIN", "NE", "FM", "FM", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.9": {"text": "Un \u0153wergl\u00fcckli walzt se beid", "tokens": ["Un", "\u0153wer\u00b7gl\u00fc\u00b7ck\u00b7li", "walzt", "se", "beid"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.10": {"text": "Mit Snack un Lachen na de Heid.", "tokens": ["Mit", "Snack", "un", "La\u00b7chen", "na", "de", "Heid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "FM", "FM", "FM", "FM", "FM", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Kamt \u0119r ok v\u0119le Herrn tom\u00f6t", "tokens": ["Kamt", "\u0119r", "ok", "v\u0119le", "Herrn", "to\u00b7m\u00f6t"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Un seht \u0119r na de bloten F\u00f6t", "tokens": ["Un", "seht", "\u0119r", "na", "de", "blo\u00b7ten", "F\u00f6t"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "FM", "FM", "FM", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Un glupt \u0119r n\u0153swis \u00fcnnern Hot \u2013", "tokens": ["Un", "glupt", "\u0119r", "n\u0153s\u00b7wis", "\u00fcn\u00b7nern", "Hot", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Ward se ok eerst en b\u0119ten roth,", "tokens": ["Ward", "se", "ok", "e\u00b7erst", "en", "b\u0119ten", "roth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "FM", "FM", "FM", "FM", "VVFIN", "ADJD", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "So denkt se doch: Wat de wull meenn?", "tokens": ["So", "denkt", "se", "doch", ":", "Wat", "de", "wull", "me\u00b7enn", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "$.", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Ik b\u00fcn so gut as anner Een!", "tokens": ["Ik", "b\u00fcn", "so", "gut", "as", "an\u00b7ner", "E\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "ADJD", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Un lusti h\u00fcppt se \u0153wern Weg,", "tokens": ["Un", "lus\u00b7ti", "h\u00fcppt", "se", "\u0153wern", "Weg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "Kikt in en Koppel \u0153wert Steg:", "tokens": ["Kikt", "in", "en", "Kop\u00b7pel", "\u0153wert", "Steg", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NE", "NE", "VVFIN", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.9": {"text": "\u00bbwullt mit, min Anna? b\u00fcst al klar?\u00ab", "tokens": ["\u00bb", "wullt", "mit", ",", "min", "An\u00b7na", "?", "b\u00fcst", "al", "klar", "?", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "PTKVZ", "$,", "PPOSAT", "NE", "$.", "VVFIN", "ADV", "ADJD", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Un sieh, min Anna is al dar!", "tokens": ["Un", "sieh", ",", "min", "An\u00b7na", "is", "al", "dar", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "FM", "FM", "FM", "ADV", "PTKVZ", 