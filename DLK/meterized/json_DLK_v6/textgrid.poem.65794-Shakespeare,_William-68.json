{"textgrid.poem.65794": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "68", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So ist sein Angesicht die Karte voriger Zeiten,", "tokens": ["So", "ist", "sein", "An\u00b7ge\u00b7sicht", "die", "Kar\u00b7te", "vo\u00b7ri\u00b7ger", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Da Sch\u00f6nheit, wie jetzt Blumen, lebt' und starb;", "tokens": ["Da", "Sch\u00f6n\u00b7heit", ",", "wie", "jetzt", "Blu\u00b7men", ",", "lebt'", "und", "starb", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "PWAV", "ADV", "NN", "$,", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Eh' man um jene Heuchelau\u00dfenseiten", "tokens": ["Eh'", "man", "um", "je\u00b7ne", "Heu\u00b7chel\u00b7au\u00b7\u00dfen\u00b7sei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "F\u00fcr Stirnen Lebender sich noch bewarb:", "tokens": ["F\u00fcr", "Stir\u00b7nen", "Le\u00b7ben\u00b7der", "sich", "noch", "be\u00b7warb", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Eh' man der Toten goldne Locken stahl,", "tokens": ["Eh'", "man", "der", "To\u00b7ten", "gold\u00b7ne", "Lo\u00b7cken", "stahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Das Eigentum der Gr\u00e4ber zu beleben", "tokens": ["Das", "Ei\u00b7gen\u00b7tum", "der", "Gr\u00e4\u00b7ber", "zu", "be\u00b7le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Auf einem zweiten Haupt zum zweiten Mal,", "tokens": ["Auf", "ei\u00b7nem", "zwei\u00b7ten", "Haupt", "zum", "zwei\u00b7ten", "Mal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Eh' toter Reiz sein Vlies zu andrer Putz gegeben.", "tokens": ["Eh'", "to\u00b7ter", "Reiz", "sein", "Vlies", "zu", "an\u00b7drer", "Putz", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Er zeigt der alten heiligen Tage Bild,", "tokens": ["Er", "zeigt", "der", "al\u00b7ten", "hei\u00b7li\u00b7gen", "Ta\u00b7ge", "Bild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Der lautern, zieratlosen, ungemischten,", "tokens": ["Der", "lau\u00b7tern", ",", "zie\u00b7rat\u00b7lo\u00b7sen", ",", "un\u00b7ge\u00b7mischten", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "VVINF", "$,", "VVFIN", "$,", "ADJA", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Die nicht in welker Sommer Gr\u00fcn geh\u00fcllt,", "tokens": ["Die", "nicht", "in", "wel\u00b7ker", "Som\u00b7mer", "Gr\u00fcn", "ge\u00b7h\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "APPR", "PWAT", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Mit altem Raub die Sch\u00f6nheit neu erfrischten:", "tokens": ["Mit", "al\u00b7tem", "Raub", "die", "Sch\u00f6n\u00b7heit", "neu", "er\u00b7frischten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Und ihn als reiche Kart' entwarf Natur,", "tokens": ["Und", "ihn", "als", "rei\u00b7che", "Kart'", "ent\u00b7warf", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "KOUS", "ADJA", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Dem Schein zu zeigen erster Sch\u00f6nheit Spur.", "tokens": ["Dem", "Schein", "zu", "zei\u00b7gen", "ers\u00b7ter", "Sch\u00f6n\u00b7heit", "Spur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}