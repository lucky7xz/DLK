{"textgrid.poem.40377": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Anfang", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbdu bist ein armer Junge\u00ab", "tokens": ["\u00bb", "du", "bist", "ein", "ar\u00b7mer", "Jun\u00b7ge", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "sagt Mutter oft und weint,", "tokens": ["sagt", "Mut\u00b7ter", "oft", "und", "weint", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "ADV", "KON", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "wenn ich Herr Rittersmann spielen will.", "tokens": ["wenn", "ich", "Herr", "Rit\u00b7ters\u00b7mann", "spie\u00b7len", "will", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Aber Vater hat gemeint:", "tokens": ["A\u00b7ber", "Va\u00b7ter", "hat", "ge\u00b7meint", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "\u00bber ist ein kleiner Held!\u00ab", "tokens": ["\u00bb", "er", "ist", "ein", "klei\u00b7ner", "Held", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VAFIN", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Neulich nahm ich ganz einfach", "tokens": ["Neu\u00b7lich", "nahm", "ich", "ganz", "ein\u00b7fach"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "meinen Drachen mit als Schild,", "tokens": ["mei\u00b7nen", "Dra\u00b7chen", "mit", "als", "Schild", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "KOUS", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "und dem reichen Kurt sein Schwesterchen", "tokens": ["und", "dem", "rei\u00b7chen", "Kurt", "sein", "Schwes\u00b7ter\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.4": {"text": "hat mich gek\u00fc\u00dft wie wild:", "tokens": ["hat", "mich", "ge\u00b7k\u00fc\u00dft", "wie", "wild", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "KOKOM", "ADJD", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "\u00bbdu bist ein kleiner Held!\u00ab", "tokens": ["\u00bb", "du", "bist", "ein", "klei\u00b7ner", "Held", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VAFIN", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich lie\u00df meinen Drachen steigen,", "tokens": ["Ich", "lie\u00df", "mei\u00b7nen", "Dra\u00b7chen", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "dann ging es in die Schlacht;", "tokens": ["dann", "ging", "es", "in", "die", "Schlacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "ich wollt meinen Schild blos ", "tokens": ["ich", "wollt", "mei\u00b7nen", "Schild", "blos"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "ADV"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "ich hab ihn selbst gemacht,", "tokens": ["ich", "hab", "ihn", "selbst", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "ich bin ein kleiner Held!", "tokens": ["ich", "bin", "ein", "klei\u00b7ner", "Held", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ich will's schon machen, da\u00df Mutter", "tokens": ["Ich", "will's", "schon", "ma\u00b7chen", ",", "da\u00df", "Mut\u00b7ter"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "VVINF", "$,", "KOUS", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "nicht mehr weint um mich.", "tokens": ["nicht", "mehr", "weint", "um", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVFIN", "APPR", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "O! sie soll mal sehn und lachen,", "tokens": ["O", "!", "sie", "soll", "mal", "sehn", "und", "la\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPER", "VMFIN", "ADV", "VVINF", "KON", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "was ich alles werden kann, ich", "tokens": ["was", "ich", "al\u00b7les", "wer\u00b7den", "kann", ",", "ich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PWS", "PPER", "PIS", "VAINF", "VMFIN", "$,", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "kleiner Held! \u2013", "tokens": ["klei\u00b7ner", "Held", "!", "\u2013"], "token_info": ["word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "$.", "$("], "meter": "+-+", "measure": "trochaic.di"}}}}}