{"textgrid.poem.39343": {"metadata": {"author": {"name": "Lavater, Johann Kaspar", "birth": "N.A.", "death": "N.A."}, "title": "1L: O Menschenherz! Des Gottesglaubens f\u00e4hig!", "genre": "verse", "period": "N.A.", "pub_year": 1770, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Menschenherz! Des Gottesglaubens f\u00e4hig!", "tokens": ["O", "Men\u00b7schen\u00b7herz", "!", "Des", "Got\u00b7tes\u00b7glau\u00b7bens", "f\u00e4\u00b7hig", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die mehr als gro\u00df bist Du! Doch sollst Du nie", "tokens": ["Die", "mehr", "als", "gro\u00df", "bist", "Du", "!", "Doch", "sollst", "Du", "nie"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "KOKOM", "ADJD", "VAFIN", "PPER", "$.", "KON", "VMFIN", "PPER", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In Deiner Gr\u00f6\u00dfe Dich erkennen. Blicke,", "tokens": ["In", "Dei\u00b7ner", "Gr\u00f6\u00b7\u00dfe", "Dich", "er\u00b7ken\u00b7nen", ".", "Bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "VVINF", "$.", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nur schnelle Blick' auf Dich sind Dir verg\u00f6nnet!", "tokens": ["Nur", "schnel\u00b7le", "Blick'", "auf", "Dich", "sind", "Dir", "ver\u00b7g\u00f6n\u00b7net", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "APPR", "PPER", "VAFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Dir ward die Wunderkraft, ein H\u00f6chstes, Be\u00dftes", "tokens": ["Dir", "ward", "die", "Wun\u00b7der\u00b7kraft", ",", "ein", "H\u00f6chs\u00b7tes", ",", "Be\u00df\u00b7tes"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "ART", "ADJA", "$,", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Pers\u00f6nlich Dir, Dir als ein Du zu denken,", "tokens": ["Per\u00b7s\u00f6n\u00b7lich", "Dir", ",", "Dir", "als", "ein", "Du", "zu", "den\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "$,", "PPER", "KOUS", "ART", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Mit diesem Du Dich kindlich zu vereinen,", "tokens": ["Mit", "die\u00b7sem", "Du", "Dich", "kind\u00b7lich", "zu", "ver\u00b7ei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "PPER", "PRF", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Dem hohen Du zu sagen: \u00bbSch\u00f6pfer, Vater,", "tokens": ["Dem", "ho\u00b7hen", "Du", "zu", "sa\u00b7gen", ":", "\u00bb", "Sch\u00f6p\u00b7fer", ",", "Va\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "PPER", "PTKZU", "VVINF", "$.", "$(", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Du bist und bist in mir! Bist, wo ein Seyn ist!", "tokens": ["Du", "bist", "und", "bist", "in", "mir", "!", "Bist", ",", "wo", "ein", "Seyn", "ist", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KON", "VAFIN", "APPR", "PPER", "$.", "VAFIN", "$,", "PWAV", "ART", "NN", "VAFIN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Du Leben, lebst in jedem Leben! Liebe,", "tokens": ["Du", "Le\u00b7ben", ",", "lebst", "in", "je\u00b7dem", "Le\u00b7ben", "!", "Lie\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "NN", "$,", "VVFIN", "APPR", "PIAT", "NN", "$.", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Du liebst in jeder Ich-entstorbnen Liebe!", "tokens": ["Du", "liebst", "in", "je\u00b7der", "Ich\u00b7ents\u00b7torb\u00b7nen", "Lie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Du sprichst mit mir von jedes Weisen Lippen!", "tokens": ["Du", "sprichst", "mit", "mir", "von", "je\u00b7des", "Wei\u00b7sen", "Lip\u00b7pen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "APPR", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Du stralst mich an im reinen Sonnenstrale!", "tokens": ["Du", "stralst", "mich", "an", "im", "rei\u00b7nen", "Son\u00b7nen\u00b7stra\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Du leuchtest mir im mildern Mondesglanze!", "tokens": ["Du", "leuch\u00b7test", "mir", "im", "mil\u00b7dern", "Mon\u00b7des\u00b7glan\u00b7ze", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Die Sterne sind von Deiner Herrlichkeit", "tokens": ["Die", "Ster\u00b7ne", "sind", "von", "Dei\u00b7ner", "Herr\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Entsunkne Funken nur! Die Erd' ein St\u00e4ubchen!", "tokens": ["Ent\u00b7sunk\u00b7ne", "Fun\u00b7ken", "nur", "!", "Die", "Erd'", "ein", "St\u00e4ub\u00b7chen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "$.", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Ein Tropfen ist vor Dir der Ozean!", "tokens": ["Ein", "Trop\u00b7fen", "ist", "vor", "Dir", "der", "O\u00b7ze\u00b7an", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Und Ein Gedanke nur ein Weltsystem!", "tokens": ["Und", "Ein", "Ge\u00b7dan\u00b7ke", "nur", "ein", "Welt\u00b7sys\u00b7tem", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.19": {"text": "Du bist ein Licht, ohn' alle Finsternisse!", "tokens": ["Du", "bist", "ein", "Licht", ",", "ohn'", "al\u00b7le", "Fins\u00b7ter\u00b7nis\u00b7se", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Des Lebens Quell und alles Daseyns Wurzel!", "tokens": ["Des", "Le\u00b7bens", "Quell", "und", "al\u00b7les", "Da\u00b7seyns", "Wur\u00b7zel", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "PIS", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Der Kr\u00e4fte Kraft! Der Mittelpunkt der Sph\u00e4ren!", "tokens": ["Der", "Kr\u00e4f\u00b7te", "Kraft", "!", "Der", "Mit\u00b7tel\u00b7punkt", "der", "Sph\u00e4\u00b7ren", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$.", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Ein ewig Eins, das Allen Alles ist,", "tokens": ["Ein", "e\u00b7wig", "Eins", ",", "das", "Al\u00b7len", "Al\u00b7les", "ist", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,", "PRELS", "PIS", "PIS", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Dasselbe jedem ist und jedem anders\u00ab.", "tokens": ["Das\u00b7sel\u00b7be", "je\u00b7dem", "ist", "und", "je\u00b7dem", "an\u00b7ders", "\u00ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDAT", "PIS", "VAFIN", "KON", "PIS", "ADV", "$(", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Religion! Du Ahnung unsichtbarer", "tokens": ["Re\u00b7li\u00b7gi\u00b7on", "!", "Du", "Ah\u00b7nung", "un\u00b7sicht\u00b7ba\u00b7rer"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "PPER", "NN", "PPOSAT"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Erhabner Geistigkeiten! Vollgef\u00fchl", "tokens": ["Er\u00b7hab\u00b7ner", "Geis\u00b7tig\u00b7kei\u00b7ten", "!", "Voll\u00b7ge\u00b7f\u00fchl"], "token_info": ["word", "word", "punct", "word"], "pos": ["ADJA", "NN", "$.", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Von immer mehr gereifter Gotteskraft", "tokens": ["Von", "im\u00b7mer", "mehr", "ge\u00b7reif\u00b7ter", "Got\u00b7tes\u00b7kraft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In uns und au\u00dfer uns! Du Sinn f\u00fcr's Eine", "tokens": ["In", "uns", "und", "au\u00b7\u00dfer", "uns", "!", "Du", "Sinn", "f\u00fcr's", "Ei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "KON", "APPR", "PPER", "$.", "PPER", "NN", "APPR", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "In Allem dem, was ist und war und seyn wird!", "tokens": ["In", "Al\u00b7lem", "dem", ",", "was", "ist", "und", "war", "und", "seyn", "wird", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ART", "$,", "PWS", "VAFIN", "KON", "VAFIN", "KON", "VAINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Du immer reger Trieb nach Ewigem!", "tokens": ["Du", "im\u00b7mer", "re\u00b7ger", "Trieb", "nach", "E\u00b7wi\u00b7gem", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJA", "NN", "APPR", "ADJA", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.7": {"text": "Du stiller Durst, der stets und nie sich s\u00e4ttigt", "tokens": ["Du", "stil\u00b7ler", "Durst", ",", "der", "stets", "und", "nie", "sich", "s\u00e4t\u00b7tigt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "$,", "PRELS", "ADV", "KON", "ADV", "PRF", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Nach Unerme\u00dflichkeit, die Eins nur ist!", "tokens": ["Nach", "Un\u00b7er\u00b7me\u00df\u00b7lich\u00b7keit", ",", "die", "Eins", "nur", "ist", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ART", "NN", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Nach Unerme\u00dflichkeit, die menschlich ist!", "tokens": ["Nach", "Un\u00b7er\u00b7me\u00df\u00b7lich\u00b7keit", ",", "die", "menschlich", "ist", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Wer sandte Dich, o g\u00f6ttlichster der Triebe", "tokens": ["Wer", "sand\u00b7te", "Dich", ",", "o", "g\u00f6tt\u00b7lichs\u00b7ter", "der", "Trie\u00b7be"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "$,", "FM", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "In uns're Brust, gebaut aus Erd und Wasser?", "tokens": ["In", "un\u00b7s'\u00b7re", "Brust", ",", "ge\u00b7baut", "aus", "Erd", "und", "Was\u00b7ser", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "VVPP", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Wer gab dem Herzen Dich, das in uns schlaget?", "tokens": ["Wer", "gab", "dem", "Her\u00b7zen", "Dich", ",", "das", "in", "uns", "schla\u00b7get", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "PPER", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Bed\u00fcrfni\u00df, Dich, nach unbegr\u00e4nztem Eins,", "tokens": ["Be\u00b7d\u00fcrf\u00b7ni\u00df", ",", "Dich", ",", "nach", "un\u00b7be\u00b7gr\u00e4nz\u00b7tem", "Eins", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Das nicht zu fassen ist und doch umfa\u00dft wird?", "tokens": ["Das", "nicht", "zu", "fas\u00b7sen", "ist", "und", "doch", "um\u00b7fa\u00dft", "wird", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKNEG", "PTKZU", "VVINF", "VAFIN", "KON", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Religion! Du h\u00f6chste Zauberkraft!", "tokens": ["Re\u00b7li\u00b7gi\u00b7on", "!", "Du", "h\u00f6chs\u00b7te", "Zau\u00b7ber\u00b7kraft", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "ADJA", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.16": {"text": "Du Sch\u00f6pferinn des ungeschaffnen Wesens!", "tokens": ["Du", "Sch\u00f6p\u00b7fe\u00b7rinn", "des", "un\u00b7ge\u00b7schaff\u00b7nen", "We\u00b7sens", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Geb\u00e4hrerinn unsterblicher Naturen,", "tokens": ["Ge\u00b7b\u00e4h\u00b7re\u00b7rinn", "uns\u00b7terb\u00b7li\u00b7cher", "Na\u00b7tu\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Die nichts f\u00fcr's Auge sind, und dennoch wahrer", "tokens": ["Die", "nichts", "f\u00fcr's", "Au\u00b7ge", "sind", ",", "und", "den\u00b7noch", "wah\u00b7rer"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PIS", "APPRART", "NN", "VAFIN", "$,", "KON", "ADV", "ADJA"], "meter": "-+-+-+++-+-", "measure": "unknown.measure.hexa"}, "line.19": {"text": "Als Sonnenlicht dem Aug' am Mittag ist!", "tokens": ["Als", "Son\u00b7nen\u00b7licht", "dem", "Aug'", "am", "Mit\u00b7tag", "ist", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "APPRART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Kein Daseyn gleicht dem Daseyn, das des Glaubens", "tokens": ["Kein", "Da\u00b7seyn", "gleicht", "dem", "Da\u00b7seyn", ",", "das", "des", "Glau\u00b7bens"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "ART", "NN", "$,", "PRELS", "ART", "NN"], "meter": "+-+--++--+-", "measure": "trochaic.penta.relaxed"}, "line.21": {"text": "Magie erschafft \u2013 nicht Du, o T\u00e4uscherinn,", "tokens": ["Ma\u00b7gie", "er\u00b7schafft", "\u2013", "nicht", "Du", ",", "o", "T\u00e4u\u00b7sche\u00b7rinn", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVPP", "$(", "PTKNEG", "PPER", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "O Bildersch\u00f6pferinn, der Tr\u00e4ume Mutter \u2013,", "tokens": ["O", "Bil\u00b7der\u00b7sch\u00f6p\u00b7fe\u00b7rinn", ",", "der", "Tr\u00e4u\u00b7me", "Mut\u00b7ter", "\u2013", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NE", "NN", "$,", "ART", "NN", "NN", "$(", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Der \u00e4chte Glaube schafft f\u00fcr inn're Sinnen.", "tokens": ["Der", "\u00e4ch\u00b7te", "Glau\u00b7be", "schafft", "f\u00fcr", "inn'\u00b7re", "Sin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ja, Menschenherz! Du \u00fcbertr