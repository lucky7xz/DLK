{"textgrid.poem.66729": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Die gelbe Rose", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sp\u00e4tsommertag. Berlin in klarer Bl\u00e4ue.", "tokens": ["Sp\u00e4t\u00b7som\u00b7mer\u00b7tag", ".", "Ber\u00b7lin", "in", "kla\u00b7rer", "Bl\u00e4u\u00b7e", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NE", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ihr Gleise sauste die Elektrische.", "tokens": ["Ihr", "Glei\u00b7se", "saus\u00b7te", "die", "E\u00b7lekt\u00b7ri\u00b7sche", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Schaffner zog. Gleich kam die Haltestelle.", "tokens": ["Der", "Schaff\u00b7ner", "zog", ".", "Gleich", "kam", "die", "Hal\u00b7te\u00b7stel\u00b7le", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein zartes Fr\u00e4ulein, ganz in Wei\u00df, stand auf,", "tokens": ["Ein", "zar\u00b7tes", "Fr\u00e4u\u00b7lein", ",", "ganz", "in", "Wei\u00df", ",", "stand", "auf", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "APPR", "NE", "$,", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "So fein und lieblich wie die gelbe Rose,", "tokens": ["So", "fein", "und", "lieb\u00b7lich", "wie", "die", "gel\u00b7be", "Ro\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die locker in dem Schlo\u00df des G\u00fcrtels hing.", "tokens": ["Die", "lo\u00b7cker", "in", "dem", "Schlo\u00df", "des", "G\u00fcr\u00b7tels", "hing", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ein Bremsenruck. Die junge Dame schwankte", "tokens": ["Ein", "Brem\u00b7sen\u00b7ruck", ".", "Die", "jun\u00b7ge", "Da\u00b7me", "schwank\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$.", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ein wenig hin und her, als sie den Wagen", "tokens": ["Ein", "we\u00b7nig", "hin", "und", "her", ",", "als", "sie", "den", "Wa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PIS", "PTKVZ", "KON", "PTKVZ", "$,", "KOUS", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Eilig verlie\u00df. Von der Ersch\u00fctterung", "tokens": ["Ei\u00b7lig", "ver\u00b7lie\u00df", ".", "Von", "der", "Er\u00b7sch\u00fct\u00b7te\u00b7rung"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "$.", "APPR", "ART", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.10": {"text": "Glitt unbemerkt der duftige Schmuck zu Boden.", "tokens": ["Glitt", "un\u00b7be\u00b7merkt", "der", "duf\u00b7ti\u00b7ge", "Schmuck", "zu", "Bo\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Blieb liegen ... Wer denn achtete darauf?", "tokens": ["Blieb", "lie\u00b7gen", "...", "Wer", "denn", "ach\u00b7te\u00b7te", "da\u00b7rauf", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$(", "PWS", "ADV", "VVFIN", "PAV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Das Fr\u00e4ulein winkte mit dem Sonnenschirm", "tokens": ["Das", "Fr\u00e4u\u00b7lein", "wink\u00b7te", "mit", "dem", "Son\u00b7nen\u00b7schirm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Der Freundin, Gru\u00df und leichtes H\u00e4ndesch\u00fctteln \u2013", "tokens": ["Der", "Freun\u00b7din", ",", "Gru\u00df", "und", "leich\u00b7tes", "H\u00e4n\u00b7de\u00b7sch\u00fct\u00b7teln", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Und weiter sauste die Elektrische.", "tokens": ["Und", "wei\u00b7ter", "saus\u00b7te", "die", "E\u00b7lekt\u00b7ri\u00b7sche", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Der Kondukteur, ein junger Mensch, dem hart", "tokens": ["Der", "Kon\u00b7duk\u00b7teur", ",", "ein", "jun\u00b7ger", "Mensch", ",", "dem", "hart"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "$,", "PRELS", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Des Kampfes Furchen schon die Stirn zerschnitten,", "tokens": ["Des", "Kamp\u00b7fes", "Fur\u00b7chen", "schon", "die", "Stirn", "zer\u00b7schnit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Durchschritt sein Reich und hob die Rose rasch", "tokens": ["Durch\u00b7schritt", "sein", "Reich", "und", "hob", "die", "Ro\u00b7se", "rasch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "KON", "VVFIN", "ART", "NN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Vom Fu\u00dfbrett, kehrte zum Perron zur\u00fcck,", "tokens": ["Vom", "Fu\u00df\u00b7brett", ",", "kehr\u00b7te", "zum", "Per\u00b7ron", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "VVFIN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.19": {"text": "Sog einen Augenblick den s\u00fc\u00dfen Hauch", "tokens": ["Sog", "ei\u00b7nen", "Au\u00b7gen\u00b7blick", "den", "s\u00fc\u00b7\u00dfen", "Hauch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Und hielt so freudeheimlich in der Hand", "tokens": ["Und", "hielt", "so", "freu\u00b7de\u00b7heim\u00b7lich", "in", "der", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Den lichtdurchschimmert seidenweichen Kelch ...", "tokens": ["Den", "licht\u00b7durch\u00b7schim\u00b7mert", "sei\u00b7den\u00b7wei\u00b7chen", "Kelch", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Nur ein Moment. Dann steckt er sie behutsam", "tokens": ["Nur", "ein", "Mo\u00b7ment", ".", "Dann", "steckt", "er", "sie", "be\u00b7hut\u00b7sam"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "$.", "ADV", "VVFIN", "PPER", "PPER", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Am R\u00fcckengitter seines Platzes fest,", "tokens": ["Am", "R\u00fc\u00b7cken\u00b7git\u00b7ter", "sei\u00b7nes", "Plat\u00b7zes", "fest", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Wo seltsam sie die N\u00fcchternheit des Raumes", "tokens": ["Wo", "selt\u00b7sam", "sie", "die", "N\u00fcch\u00b7tern\u00b7heit", "des", "Rau\u00b7mes"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "PPER", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Verkl\u00e4rte, nahm die Rolle, zog dem neuen", "tokens": ["Ver\u00b7kl\u00e4r\u00b7te", ",", "nahm", "die", "Rol\u00b7le", ",", "zog", "dem", "neu\u00b7en"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "VVFIN", "ART", "NN", "$,", "VVFIN", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Fahrgast das folgende Billett heraus,", "tokens": ["Fahr\u00b7gast", "das", "fol\u00b7gen\u00b7de", "Bil\u00b7lett", "he\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.27": {"text": "Beugt sich zur\u00fcck: \u00bbGestatten Sie\u00ab, h\u00e4ngt schnell", "tokens": ["Beugt", "sich", "zu\u00b7r\u00fcck", ":", "\u00bb", "Ge\u00b7stat\u00b7ten", "Sie", "\u00ab", ",", "h\u00e4ngt", "schnell"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word"], "pos": ["VVFIN", "PRF", "PTKVZ", "$.", "$(", "NN", "PPER", "$(", "$,", "VVFIN", "ADJD"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.28": {"text": "Die Oberleitung um \u2013 und sausend ging's", "tokens": ["Die", "O\u00b7berl\u00b7ei\u00b7tung", "um", "\u2013", "und", "sau\u00b7send", "ging's"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "$(", "KON", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "In andre Gegend, andre Menschenwelten.", "tokens": ["In", "and\u00b7re", "Ge\u00b7gend", ",", "and\u00b7re", "Men\u00b7schen\u00b7wel\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}