{"textgrid.poem.67639": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du holest, liebe Nachtigall,", "tokens": ["Du", "ho\u00b7lest", ",", "lie\u00b7be", "Nach\u00b7ti\u00b7gall", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So tief aus Herzens Grunde", "tokens": ["So", "tief", "aus", "Her\u00b7zens", "Grun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Den s\u00fc\u00dfen Lieb- und Lobesschall;", "tokens": ["Den", "s\u00fc\u00b7\u00dfen", "Lie\u00b7b", "und", "Lo\u00b7bes\u00b7schall", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "TRUNC", "KON", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und ich mit Herz und Munde", "tokens": ["Und", "ich", "mit", "Herz", "und", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Bin fr\u00fch und sp\u00e4t und weit und breit", "tokens": ["Bin", "fr\u00fch", "und", "sp\u00e4t", "und", "weit", "und", "breit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "KON", "ADJD", "KON", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Stumm allezeit.", "tokens": ["Stumm", "al\u00b7le\u00b7zeit", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Du schwingest, liebe Lerche, Dich", "tokens": ["Du", "schwin\u00b7gest", ",", "lie\u00b7be", "Ler\u00b7che", ",", "Dich"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "$,", "ADJA", "NN", "$,", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So fr\u00f6hlich", "tokens": ["So", "fr\u00f6h\u00b7lich"], "token_info": ["word", "word"], "pos": ["ADV", "ADJD"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Hin in die kalte Morgenluft,", "tokens": ["Hin", "in", "die", "kal\u00b7te", "Mor\u00b7gen\u00b7luft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verlierest Dich", "tokens": ["Ver\u00b7lie\u00b7rest", "Dich"], "token_info": ["word", "word"], "pos": ["VVFIN", "PPER"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "In Weihrauchduft", "tokens": ["In", "Weih\u00b7rauch\u00b7duft"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Und schwebst, ein unsichtbarer Schall,", "tokens": ["Und", "schwebst", ",", "ein", "un\u00b7sicht\u00b7ba\u00b7rer", "Schall", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und singst Natur", "tokens": ["Und", "singst", "Na\u00b7tur"], "token_info": ["word", "word", "word"], "pos": ["KON", "VVFIN", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Und gr\u00fcne Flur \u2013", "tokens": ["Und", "gr\u00fc\u00b7ne", "Flur", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Und ich bin ohne Hall!", "tokens": ["Und", "ich", "bin", "oh\u00b7ne", "Hall", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "So will ich denn mit wehmuthvollem Schweigen,", "tokens": ["So", "will", "ich", "denn", "mit", "weh\u00b7muth\u00b7vol\u00b7lem", "Schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "O Vater der Natur,", "tokens": ["O", "Va\u00b7ter", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auf jeder Deiner Spur", "tokens": ["Auf", "je\u00b7der", "Dei\u00b7ner", "Spur"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Zum stummen Danke mich auf Deinem Fu\u00dftritt neigen.", "tokens": ["Zum", "stum\u00b7men", "Dan\u00b7ke", "mich", "auf", "Dei\u00b7nem", "Fu\u00df\u00b7tritt", "nei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}