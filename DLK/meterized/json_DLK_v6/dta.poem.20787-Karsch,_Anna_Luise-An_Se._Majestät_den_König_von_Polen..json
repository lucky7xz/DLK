{"dta.poem.20787": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "An                \n  Se. Majest\u00e4t den K\u00f6nig von Polen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Blick, Durchlauchtigster August!               ", "tokens": ["Ein", "Blick", ",", "Durch\u00b7lauch\u00b7tigs\u00b7ter", "Au\u00b7gust", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ein Blick nach dem erhabnen Sitze,", "tokens": ["Ein", "Blick", "nach", "dem", "er\u00b7hab\u00b7nen", "Sit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Auf welchen Du, der V\u00f6lker Schutz und Lust,", "tokens": ["Auf", "wel\u00b7chen", "Du", ",", "der", "V\u00f6l\u00b7ker", "Schutz", "und", "Lust", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "PPER", "$,", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dem halben Monden sanft die Spitze", "tokens": ["Dem", "hal\u00b7ben", "Mon\u00b7den", "sanft", "die", "Spit\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJD", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Des K\u00f6niglichen Zepters neigst,", "tokens": ["Des", "K\u00f6\u00b7nig\u00b7li\u00b7chen", "Zep\u00b7ters", "neigst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und Deines Geistes Gr\u00f6\u00dfe zeigst.", "tokens": ["Und", "Dei\u00b7nes", "Geis\u00b7tes", "Gr\u00f6\u00b7\u00dfe", "zeigst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ein solcher Blick entz\u00fcckt Gem\u00fcther,", "tokens": ["Ein", "sol\u00b7cher", "Blick", "ent\u00b7z\u00fcckt", "Ge\u00b7m\u00fc\u00b7ther", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVPP", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die Ruhe unterst\u00fctzt den Thron,", "tokens": ["Die", "Ru\u00b7he", "un\u00b7ter\u00b7st\u00fctzt", "den", "Thron", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Der Friede spricht dem Blutdurst Hohn,", "tokens": ["Der", "Frie\u00b7de", "spricht", "dem", "Blut\u00b7durst", "Hohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Dein Lorbeer prangt mit Sicherheit. Fern weicht der", "tokens": ["Dein", "Lor\u00b7beer", "prangt", "mit", "Si\u00b7cher\u00b7heit", ".", "Fern", "weicht", "der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "NN", "$.", "NN", "VVFIN", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Zwietracht Ungewitter", "tokens": ["Zwiet\u00b7racht", "Un\u00b7ge\u00b7wit\u00b7ter"], "token_info": ["word", "word"], "pos": ["NN", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Dein Land, das froh und ruhig ist;", "tokens": ["Dein", "Land", ",", "das", "froh", "und", "ru\u00b7hig", "ist", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "ADJD", "KON", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schl\u00e4gt in den B\u00fcchern grauer Zeiten", "tokens": ["Schl\u00e4gt", "in", "den", "B\u00fc\u00b7chern", "grau\u00b7er", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Geschichte nach, erstaunt und sieht und liest", "tokens": ["Ge\u00b7schich\u00b7te", "nach", ",", "er\u00b7staunt", "und", "sieht", "und", "liest"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "PTKVZ", "$,", "ADJD", "KON", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wie oft ein feindliches Verbreiten", "tokens": ["Wie", "oft", "ein", "feind\u00b7li\u00b7ches", "Ver\u00b7brei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der L\u00e4nder Zier, der St\u00e4dte Pracht", "tokens": ["Der", "L\u00e4n\u00b7der", "Zier", ",", "der", "St\u00e4d\u00b7te", "Pracht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zur \u00f6den W\u00fcste hat gemacht:", "tokens": ["Zur", "\u00f6\u00b7den", "W\u00fcs\u00b7te", "hat", "ge\u00b7macht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wie Greis und J\u00fcngling hingesunken,", "tokens": ["Wie", "Greis", "und", "J\u00fcng\u00b7ling", "hin\u00b7ge\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ein Schaudern \u00fcberf\u00e4llt Dein Reich,", "tokens": ["Ein", "Schau\u00b7dern", "\u00fc\u00b7berf\u00b7\u00e4llt", "Dein", "Reich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Als f\u00fchlt es noch den S\u00e4belstreich;", "tokens": ["Als", "f\u00fchlt", "es", "noch", "den", "S\u00e4\u00b7bel\u00b7streich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Doch nein, es f\u00fchlt des Friedens Reiz, und wird von", "tokens": ["Doch", "nein", ",", "es", "f\u00fchlt", "des", "Frie\u00b7dens", "Reiz", ",", "und", "wird", "von"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PTKANT", "$,", "PPER", "VVFIN", "ART", "NN", "NE", "$,", "KON", "VAFIN", "APPR"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "seiner Freude trunken.", "tokens": ["sei\u00b7ner", "Freu\u00b7de", "trun\u00b7ken", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Der Scyte kommt, man holt ihn ein,", "tokens": ["Der", "Scy\u00b7te", "kommt", ",", "man", "holt", "ihn", "ein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PIS", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Man dr\u00e4ngt sich um den rothen Bunde,", "tokens": ["Man", "dr\u00e4ngt", "sich", "um", "den", "ro\u00b7then", "Bun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der ihn bedeckt, vorz\u00fcglich nah zu seyn.", "tokens": ["Der", "ihn", "be\u00b7deckt", ",", "vor\u00b7z\u00fcg\u00b7lich", "nah", "zu", "seyn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "$,", "ADJD", "ADJD", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sein Ansehn schl\u00e4gt uns keine Wunde,", "tokens": ["Sein", "An\u00b7sehn", "schl\u00e4gt", "uns", "kei\u00b7ne", "Wun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sein durch die Zeit beschneites Haar", "tokens": ["Sein", "durch", "die", "Zeit", "be\u00b7schnei\u00b7tes", "Haar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zeigt den gef\u00fcrchteten Barbar", "tokens": ["Zeigt", "den", "ge\u00b7f\u00fcrch\u00b7te\u00b7ten", "Bar\u00b7bar"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "In einem freundschaftlichen Bilde.", "tokens": ["In", "ei\u00b7nem", "freund\u00b7schaft\u00b7li\u00b7chen", "Bil\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Das Volk steht da, und staunt ihn an,", "tokens": ["Das", "Volk", "steht", "da", ",", "und", "staunt", "ihn", "an", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,", "KON", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und spricht: o seht, der Muselmann", "tokens": ["Und", "spricht", ":", "o", "seht", ",", "der", "Mu\u00b7sel\u00b7mann"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "$.", "FM", "VVFIN", "$,", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Ist nicht so grausam, als ihr denkt: das Gute blinket", "tokens": ["Ist", "nicht", "so", "grau\u00b7sam", ",", "als", "ihr", "denkt", ":", "das", "Gu\u00b7te", "blin\u00b7ket"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PTKNEG", "ADV", "ADJD", "$,", "KOUS", "PPER", "VVFIN", "$.", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "durch das Wilde.", "tokens": ["durch", "das", "Wil\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "Man sah ihn auf die Stufen gehn,", "tokens": ["Man", "sah", "ihn", "auf", "die", "Stu\u00b7fen", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die, Herr, zu Deinem Throne f\u00fchren;", "tokens": ["Die", ",", "Herr", ",", "zu", "Dei\u00b7nem", "Thro\u00b7ne", "f\u00fch\u00b7ren", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "NN", "$,", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Doch, o Monarch! hier bleibt die Muse stehn,", "tokens": ["Doch", ",", "o", "Mon\u00b7arch", "!", "hier", "bleibt", "die", "Mu\u00b7se", "stehn", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "FM", "NN", "$.", "ADV", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nur stille Ehrfurcht mu\u00df sie r\u00fchren,", "tokens": ["Nur", "stil\u00b7le", "Ehr\u00b7furcht", "mu\u00df", "sie", "r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Im Geist hat sie nur hingeblickt,", "tokens": ["Im", "Geist", "hat", "sie", "nur", "hin\u00b7ge\u00b7blickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie Stambol sich vor Dir geb\u00fcckt.", "tokens": ["Wie", "Stam\u00b7bol", "sich", "vor", "Dir", "ge\u00b7b\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PRF", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die L\u00fcfte lispelten die Worte,", "tokens": ["Die", "L\u00fcf\u00b7te", "lis\u00b7pel\u00b7ten", "die", "Wor\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.8": {"text": "Durch die der F\u00fcrst vom Morgenland", "tokens": ["Durch", "die", "der", "F\u00fcrst", "vom", "Mor\u00b7gen\u00b7land"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Sich neu an Deine Freundschaft band:", "tokens": ["Sich", "neu", "an", "Dei\u00b7ne", "Freund\u00b7schaft", "band", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Dein Adler, der die Schwingen hub, der \u00fcberschattete", "tokens": ["Dein", "Ad\u00b7ler", ",", "der", "die", "Schwin\u00b7gen", "hub", ",", "der", "\u00fc\u00b7bersc\u00b7hat\u00b7te\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.11": {"text": "die Pforte.", "tokens": ["die", "Pfor\u00b7te", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.5": {"line.1": {"text": "O K\u00f6nig! \u2014 durchs Ger\u00e4usch bet\u00e4ubt", "tokens": ["O", "K\u00f6\u00b7nig", "!", "durchs", "Ge\u00b7r\u00e4usch", "be\u00b7t\u00e4ubt"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$.", "$(", "APPRART", "NN", "VVPP"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Vernahm die Muse nicht das Sprechen,", "tokens": ["Ver\u00b7nahm", "die", "Mu\u00b7se", "nicht", "das", "Spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Was feierlich sich tief in Marmor schreibt,", "tokens": ["Was", "fei\u00b7er\u00b7lich", "sich", "tief", "in", "Mar\u00b7mor", 