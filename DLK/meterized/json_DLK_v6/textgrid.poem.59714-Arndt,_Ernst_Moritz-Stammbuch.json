{"textgrid.poem.59714": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Stammbuch", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbfrei das Wort aus voller Brust!", "tokens": ["\u00bb", "frei", "das", "Wort", "aus", "vol\u00b7ler", "Brust", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus der Scheide frisch die Klinge!", "tokens": ["Aus", "der", "Schei\u00b7de", "frisch", "die", "Klin\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Das ist Jugendmut und Lust,", "tokens": ["Das", "ist", "Ju\u00b7gend\u00b7mut", "und", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Das ist Leben guter Dinge.\u00ab", "tokens": ["Das", "ist", "Le\u00b7ben", "gu\u00b7ter", "Din\u00b7ge", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VAFIN", "NN", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "O du Glanz vom Morgenrot!", "tokens": ["O", "du", "Glanz", "vom", "Mor\u00b7gen\u00b7rot", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "O du Jugendheldensage!", "tokens": ["O", "du", "Ju\u00b7gend\u00b7hel\u00b7den\u00b7sa\u00b7ge", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach! wie schleppt dich matt und tot,", "tokens": ["Ach", "!", "wie", "schleppt", "dich", "matt", "und", "tot", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PWAV", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Langsam tot der Gang der Tage!", "tokens": ["Lang\u00b7sam", "tot", "der", "Gang", "der", "Ta\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "K\u00e4lter rollt des Blutes Tanz,", "tokens": ["K\u00e4l\u00b7ter", "rollt", "des", "Blu\u00b7tes", "Tanz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Stiller wandeln hin die Jahre,", "tokens": ["Stil\u00b7ler", "wan\u00b7deln", "hin", "die", "Jah\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und bald liegt der ganze Glanz", "tokens": ["Und", "bald", "liegt", "der", "gan\u00b7ze", "Glanz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Welk und farblos auf der Bahre.", "tokens": ["Welk", "und", "farb\u00b7los", "auf", "der", "Bah\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Nein doch! ruf' ich, aber nein!", "tokens": ["Nein", "doch", "!", "ruf'", "ich", ",", "a\u00b7ber", "nein", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "$.", "VVFIN", "PPER", "$,", "ADV", "PTKANT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Weg mit deinem Hohn, Erfahrung!", "tokens": ["Weg", "mit", "dei\u00b7nem", "Hohn", ",", "Er\u00b7fah\u00b7rung", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Lasse nimmer weg mir schrein", "tokens": ["Las\u00b7se", "nim\u00b7mer", "weg", "mir", "schrein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "ADV", "PPER", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Heilige Herzensoffenbarung.", "tokens": ["Hei\u00b7li\u00b7ge", "Her\u00b7zen\u00b7sof\u00b7fen\u00b7ba\u00b7rung", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.5": {"line.1": {"text": "Schiltst du, da\u00df im Nebeldunst", "tokens": ["Schiltst", "du", ",", "da\u00df", "im", "Ne\u00b7bel\u00b7dunst"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Meine bunten V\u00f6gel fliegen,", "tokens": ["Mei\u00b7ne", "bun\u00b7ten", "V\u00f6\u00b7gel", "flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Weis' ich dir die hohe Kunst,", "tokens": ["Weis'", "ich", "dir", "die", "ho\u00b7he", "Kunst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Die sie lehrt das Licht ersiegen.", "tokens": ["Die", "sie", "lehrt", "das", "Licht", "er\u00b7sie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Sch\u00fcttelst du nur faule Frucht", "tokens": ["Sch\u00fct\u00b7telst", "du", "nur", "fau\u00b7le", "Frucht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Von dem kahlen Lebensbaume,", "tokens": ["Von", "dem", "kah\u00b7len", "Le\u00b7bens\u00b7bau\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mitten in der Tage Flucht", "tokens": ["Mit\u00b7ten", "in", "der", "Ta\u00b7ge", "Flucht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Halt' ich fest am Jugendtraume.", "tokens": ["Halt'", "ich", "fest", "am", "Ju\u00b7gend\u00b7trau\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}