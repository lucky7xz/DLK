{"textgrid.poem.35941": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Von meinem Fenster hoch hinab", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von meinem Fenster hoch hinab", "tokens": ["Von", "mei\u00b7nem", "Fens\u00b7ter", "hoch", "hin\u00b7ab"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie scheint die Welt mir klein! \u2013", "tokens": ["Wie", "scheint", "die", "Welt", "mir", "klein", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "PPER", "ADJD", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da unten gehn sie auf und ab,", "tokens": ["Da", "un\u00b7ten", "gehn", "sie", "auf", "und", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Tr\u00e4gt Jeder seine Pein.", "tokens": ["Tr\u00e4gt", "Je\u00b7der", "sei\u00b7ne", "Pein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Hat Jeder seinen eig'nen Lauf", "tokens": ["Hat", "Je\u00b7der", "sei\u00b7nen", "eig'\u00b7nen", "Lauf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PIS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und seinen eig'nen Sinn. \u2013", "tokens": ["Und", "sei\u00b7nen", "eig'\u00b7nen", "Sinn", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ob Menschen wandeln ab und auf,", "tokens": ["Ob", "Men\u00b7schen", "wan\u00b7deln", "ab", "und", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wei\u00df Keiner, wer ich bin.", "tokens": ["Wei\u00df", "Kei\u00b7ner", ",", "wer", "ich", "bin", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "PWS", "PPER", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich schau in weite Ferne klar,", "tokens": ["Ich", "schau", "in", "wei\u00b7te", "Fer\u00b7ne", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Winde h\u00f6r' ich wehn.", "tokens": ["Die", "Win\u00b7de", "h\u00f6r'", "ich", "wehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mir ist, als h\u00e4tt' ich hundert Jahr", "tokens": ["Mir", "ist", ",", "als", "h\u00e4tt'", "ich", "hun\u00b7dert", "Jahr"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "KOKOM", "VAFIN", "PPER", "CARD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von diesem Thurm gesehn.", "tokens": ["Von", "die\u00b7sem", "Thurm", "ge\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Bin von der Erde weit getrennt,", "tokens": ["Bin", "von", "der", "Er\u00b7de", "weit", "ge\u00b7trennt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Liegt Alles unter mir;", "tokens": ["Liegt", "Al\u00b7les", "un\u00b7ter", "mir", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch Flamme, die im Herzen brennt,", "tokens": ["Doch", "Flam\u00b7me", ",", "die", "im", "Her\u00b7zen", "brennt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie gl\u00fcht und bl\u00fcht auch hier. \u2013", "tokens": ["Sie", "gl\u00fcht", "und", "bl\u00fcht", "auch", "hier", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ADV", "ADV", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "\u00bbund bist du, Seele, losgetrennt,", "tokens": ["\u00bb", "und", "bist", "du", ",", "See\u00b7le", ",", "los\u00b7ge\u00b7trennt", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["$(", "KON", "VAFIN", "PPER", "$,", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Erde Blumen fern:", "tokens": ["Der", "Er\u00b7de", "Blu\u00b7men", "fern", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Flamme, die im Busen brennt,", "tokens": ["Die", "Flam\u00b7me", ",", "die", "im", "Bu\u00b7sen", "brennt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie schlage hoch dem ", "tokens": ["Sie", "schla\u00b7ge", "hoch", "dem"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "ART"], "meter": "-+-+-", "measure": "iambic.di"}}}}}