{"dta.poem.18468": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Ehrlicher nahme.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Njmm dein ger\u00fccht in acht mehr/ als wol gold und geld/", "tokens": ["Njmm", "dein", "ge\u00b7r\u00fccht", "in", "acht", "mehr", "/", "als", "wol", "gold", "und", "geld", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "APPR", "CARD", "ADV", "$(", "KOKOM", "ADV", "ADJD", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil es dein bester schatz ist auf der gantzen welt/", "tokens": ["Weil", "es", "dein", "bes\u00b7ter", "schatz", "ist", "auf", "der", "gant\u00b7zen", "welt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJA", "NN", "VAFIN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Geld das verlohren geht/ wird wieder offt gefunden/", "tokens": ["Geld", "das", "ver\u00b7loh\u00b7ren", "geht", "/", "wird", "wie\u00b7der", "offt", "ge\u00b7fun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "VVFIN", "$(", "VAFIN", "ADV", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein guter nahme nie/ der einmahl ist verschwunden.", "tokens": ["Ein", "gu\u00b7ter", "nah\u00b7me", "nie", "/", "der", "ein\u00b7mahl", "ist", "ver\u00b7schwun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$(", "ART", "ADV", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}