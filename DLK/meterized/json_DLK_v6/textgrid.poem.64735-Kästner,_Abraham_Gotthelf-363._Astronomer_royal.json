{"textgrid.poem.64735": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "363. Astronomer royal", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Den ", "tokens": ["Den"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Den Titel, den er noch sich selbst zu geben wagt,", "tokens": ["Den", "Ti\u00b7tel", ",", "den", "er", "noch", "sich", "selbst", "zu", "ge\u00b7ben", "wagt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "PRF", "ADV", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wenn er damit sich vor dem Bogen weist,", "tokens": ["Wenn", "er", "da\u00b7mit", "sich", "vor", "dem", "Bo\u00b7gen", "weist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PAV", "PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Den zweyten, da er nun acht Jahr' Professor hei\u00dft,", "tokens": ["Den", "zwey\u00b7ten", ",", "da", "er", "nun", "acht", "Jahr'", "Pro\u00b7fes\u00b7sor", "hei\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "KOUS", "PPER", "ADV", "CARD", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So wird er uns wohl was von Sternkunst lehren?", "tokens": ["So", "wird", "er", "uns", "wohl", "was", "von", "Stern\u00b7kunst", "leh\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "ADV", "PWS", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Nein, Verse sind's; man fragt, wem sie geh\u00f6ren?", "tokens": ["Nein", ",", "Ver\u00b7se", "sin\u00b7d's", ";", "man", "fragt", ",", "wem", "sie", "ge\u00b7h\u00f6\u00b7ren", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NN", "VAFIN", "$.", "PIS", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "+-----+-+-+-", "measure": "dactylic.init"}}}}}