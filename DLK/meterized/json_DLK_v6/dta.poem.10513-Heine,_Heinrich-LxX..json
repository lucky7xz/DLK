{"dta.poem.10513": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "LxX.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Das wei\u00df Gott, wo sich die tolle", "tokens": ["Das", "wei\u00df", "Gott", ",", "wo", "sich", "die", "tol\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "NN", "$,", "PWAV", "PRF", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dirne einquartieret hat;", "tokens": ["Dir\u00b7ne", "ein\u00b7quar\u00b7tie\u00b7ret", "hat", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fluchend, in dem Regenwetter,", "tokens": ["Flu\u00b7chend", ",", "in", "dem", "Re\u00b7gen\u00b7wet\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lauf' ich durch die ganze Stadt.", "tokens": ["Lauf'", "ich", "durch", "die", "gan\u00b7ze", "Stadt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Bin ich doch von einem Gasthof", "tokens": ["Bin", "ich", "doch", "von", "ei\u00b7nem", "Gast\u00b7hof"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nach dem andern hingerannt,", "tokens": ["Nach", "dem", "an\u00b7dern", "hin\u00b7ge\u00b7rannt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und an jeden groben Kellner", "tokens": ["Und", "an", "je\u00b7den", "gro\u00b7ben", "Kell\u00b7ner"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hab' ich mich umsonst gewandt.", "tokens": ["Hab'", "ich", "mich", "um\u00b7sonst", "ge\u00b7wandt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PRF", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Da erblick\u2019 ich sie am Fenster,", "tokens": ["Da", "er\u00b7blick'", "ich", "sie", "am", "Fens\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPRART", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Und sie winkt und kichert hell.", "tokens": ["Und", "sie", "winkt", "und", "ki\u00b7chert", "hell", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "KON", "VVFIN", "ADJD", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Konnt' ich wissen, du bewohntest,", "tokens": ["Konnt'", "ich", "wis\u00b7sen", ",", "du", "be\u00b7wohn\u00b7test", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$,", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "M\u00e4dchen, solches Pracht-Hotel!", "tokens": ["M\u00e4d\u00b7chen", ",", "sol\u00b7ches", "Pracht\u00b7Ho\u00b7tel", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PIAT", "NN", "$."], "meter": "+-+-++-", "measure": "unknown.measure.tetra"}}}}}