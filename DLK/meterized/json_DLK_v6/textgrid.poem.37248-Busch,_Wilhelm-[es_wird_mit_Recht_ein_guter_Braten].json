{"textgrid.poem.37248": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "[es wird mit Recht ein guter Braten]", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es wird mit Recht ein guter Braten", "tokens": ["Es", "wird", "mit", "Recht", "ein", "gu\u00b7ter", "Bra\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gerechnet zu den guten Taten;", "tokens": ["Ge\u00b7rech\u00b7net", "zu", "den", "gu\u00b7ten", "Ta\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und da\u00df man ihn geh\u00f6rig mache,", "tokens": ["Und", "da\u00df", "man", "ihn", "ge\u00b7h\u00f6\u00b7rig", "ma\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ist weibliche Charaktersache.", "tokens": ["Ist", "weib\u00b7li\u00b7che", "Cha\u00b7rak\u00b7ter\u00b7sa\u00b7che", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ein braves M\u00e4dchen braucht dazu", "tokens": ["Ein", "bra\u00b7ves", "M\u00e4d\u00b7chen", "braucht", "da\u00b7zu"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mal erstens reine Seelenruh,", "tokens": ["Mal", "ers\u00b7tens", "rei\u00b7ne", "See\u00b7len\u00b7ruh", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da\u00df bei Verwendung der Gew\u00fcrze", "tokens": ["Da\u00df", "bei", "Ver\u00b7wen\u00b7dung", "der", "Ge\u00b7w\u00fcr\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Sie sich nicht hastig \u00fcberst\u00fcrze.", "tokens": ["Sie", "sich", "nicht", "has\u00b7tig", "\u00fc\u00b7bers\u00b7t\u00fcr\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "PTKNEG", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Dann zweitens braucht sie Sinnigkeit,", "tokens": ["Dann", "zwei\u00b7tens", "braucht", "sie", "Sin\u00b7nig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Ja, sozusagen Innigkeit,", "tokens": ["Ja", ",", "so\u00b7zu\u00b7sa\u00b7gen", "In\u00b7nig\u00b7keit", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Damit sie alles appetitlich,", "tokens": ["Da\u00b7mit", "sie", "al\u00b7les", "ap\u00b7pe\u00b7tit\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Bald so, bald so und recht gem\u00fctlich", "tokens": ["Bald", "so", ",", "bald", "so", "und", "recht", "ge\u00b7m\u00fct\u00b7lich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "ADV", "ADV", "KON", "ADJD", "ADJD"], "meter": "-----+-+-", "measure": "unknown.measure.di"}, "line.13": {"text": "Begie\u00dfen, drehn und wenden k\u00f6nne,", "tokens": ["Be\u00b7gie\u00b7\u00dfen", ",", "drehn", "und", "wen\u00b7den", "k\u00f6n\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "CARD", "KON", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Da\u00df an der Sache nichts verbrenne.", "tokens": ["Da\u00df", "an", "der", "Sa\u00b7che", "nichts", "ver\u00b7bren\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "In Summa braucht sie Herzensg\u00fcte,", "tokens": ["In", "Sum\u00b7ma", "braucht", "sie", "Her\u00b7zens\u00b7g\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Ein sanftes Sorgen im Gem\u00fcte,", "tokens": ["Ein", "sanf\u00b7tes", "Sor\u00b7gen", "im", "Ge\u00b7m\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Fast etwas Liebe insofern,", "tokens": ["Fast", "et\u00b7was", "Lie\u00b7be", "in\u00b7so\u00b7fern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "F\u00fcr all die h\u00fcbschen, edlen Herrn,", "tokens": ["F\u00fcr", "all", "die", "h\u00fcb\u00b7schen", ",", "ed\u00b7len", "Herrn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Die diesen Braten essen sollen", "tokens": ["Die", "die\u00b7sen", "Bra\u00b7ten", "es\u00b7sen", "sol\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PDAT", "NN", "VVINF", "VMFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Und immer gern was Gutes wollen.", "tokens": ["Und", "im\u00b7mer", "gern", "was", "Gu\u00b7tes", "wol\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "PWS", "NN", "VMFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Ich wei\u00df, da\u00df hier ein jeder spricht:", "tokens": ["Ich", "wei\u00df", ",", "da\u00df", "hier", "ein", "je\u00b7der", "spricht", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ADV", "ART", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Ein b\u00f6ses M\u00e4dchen kann es nicht.", "tokens": ["Ein", "b\u00f6\u00b7ses", "M\u00e4d\u00b7chen", "kann", "es", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Drum hab ich mir auch stets gedacht", "tokens": ["Drum", "hab", "ich", "mir", "auch", "stets", "ge\u00b7dacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "PPER", "PPER", "ADV", "ADV", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Zu Haus und anderw\u00e4rts:", "tokens": ["Zu", "Haus", "und", "an\u00b7der\u00b7w\u00e4rts", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.25": {"text": "Wer einen guten Braten macht,", "tokens": ["Wer", "ei\u00b7nen", "gu\u00b7ten", "Bra\u00b7ten", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Hat auch ein gutes Herz.", "tokens": ["Hat", "auch", "ein", "gu\u00b7tes", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}