{"dta.poem.11452": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Vermischte Getichte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Seht, seht, wie strotzt Asinius,", "tokens": ["Seht", ",", "seht", ",", "wie", "strotzt", "A\u00b7si\u00b7nius", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "PWAV", "VVFIN", "NE", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Weil er nach hofe kommen mu\u00df!", "tokens": ["Weil", "er", "nach", "ho\u00b7fe", "kom\u00b7men", "mu\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn er versteht noch nicht, was alle bauren sagen,", "tokens": ["Denn", "er", "ver\u00b7steht", "noch", "nicht", ",", "was", "al\u00b7le", "bau\u00b7ren", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "PTKNEG", "$,", "PRELS", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und t\u00e4glich die erfahrung weist:", "tokens": ["Und", "t\u00e4g\u00b7lich", "die", "er\u00b7fah\u00b7rung", "weist", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df man die esel nicht, als nur zum s\u00e4cke tragen", "tokens": ["Da\u00df", "man", "die", "e\u00b7sel", "nicht", ",", "als", "nur", "zum", "s\u00e4\u00b7cke", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ART", "NN", "PTKNEG", "$,", "KOUS", "ADV", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Nach hofe kommen heist.", "tokens": ["Nach", "ho\u00b7fe", "kom\u00b7men", "heist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}