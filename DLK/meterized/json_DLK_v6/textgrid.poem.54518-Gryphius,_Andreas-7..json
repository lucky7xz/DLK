{"textgrid.poem.54518": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Di\u00df ist das heilige Feur/ das Jesus wolt entz\u00fcnden/", "tokens": ["Di\u00df", "ist", "das", "hei\u00b7li\u00b7ge", "Feur", "/", "das", "Je\u00b7sus", "wolt", "ent\u00b7z\u00fcn\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$(", "ART", "NE", "VMFIN", "VVINF", "$("], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Indem was irrdisch ist verlodert und vergeh't/", "tokens": ["In\u00b7dem", "was", "irr\u00b7disch", "ist", "ver\u00b7lo\u00b7dert", "und", "ver\u00b7geh't", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "VAFIN", "VVPP", "KON", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Durch da\u00df was tod/ sich regt/ was kalt/ in Hitze steht/", "tokens": ["Durch", "da\u00df", "was", "tod", "/", "sich", "regt", "/", "was", "kalt", "/", "in", "Hit\u00b7ze", "steht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PIS", "NN", "$(", "PRF", "VVFIN", "$(", "PWS", "ADJD", "$(", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vor dem die schwartze Nacht des zagens mu\u00df verschwinden:", "tokens": ["Vor", "dem", "die", "schwart\u00b7ze", "Nacht", "des", "za\u00b7gens", "mu\u00df", "ver\u00b7schwin\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "ADJA", "NN", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der ists durch den sich Gott mit Menschen wil verbinden/", "tokens": ["Der", "ists", "durch", "den", "sich", "Gott", "mit", "Men\u00b7schen", "wil", "ver\u00b7bin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "PRELS", "PRF", "NN", "APPR", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Di\u00df ist der Perlen-Thau/ der frischen Morgenr\u00f6th:", "tokens": ["Di\u00df", "ist", "der", "Per\u00b7len\u00b7\u00b7T\u00b7hau", "/", "der", "fri\u00b7schen", "Mor\u00b7gen\u00b7r\u00f6th", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$(", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Der die verwelckte Seel erfrischt/ was liegt/ erh\u00f6ht.", "tokens": ["Der", "die", "ver\u00b7welck\u00b7te", "Seel", "er\u00b7frischt", "/", "was", "liegt", "/", "er\u00b7h\u00f6ht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN", "$(", "PWS", "VVFIN", "$(", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Di\u00df ist der Trost durch den wir Muth im Zagen finden:", "tokens": ["Di\u00df", "ist", "der", "Trost", "durch", "den", "wir", "Muth", "im", "Za\u00b7gen", "fin\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "APPR", "PRELS", "PPER", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Als er die Welt erschuff schwebt er still auf der Flut/", "tokens": ["Als", "er", "die", "Welt", "er\u00b7schuff", "schwebt", "er", "still", "auf", "der", "Flut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NE", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$("], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Jtzt kommt er in dem Sturm/ die Erde zu erneuen/", "tokens": ["Jtzt", "kommt", "er", "in", "dem", "Sturm", "/", "die", "Er\u00b7de", "zu", "er\u00b7neu\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$(", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Dort war es angesehn auf ein verg\u00e4nglich Gut.", "tokens": ["Dort", "war", "es", "an\u00b7ge\u00b7sehn", "auf", "ein", "ver\u00b7g\u00e4ng\u00b7lich", "Gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVPP", "APPR", "ART", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Jtzt baut er was da sol die Ewigkeit erfreuen.", "tokens": ["Jtzt", "baut", "er", "was", "da", "sol", "die", "E\u00b7wig\u00b7keit", "er\u00b7freu\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "ADV", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Er kam auf Wasser dort bey finster d\u00fcstre Nacht/", "tokens": ["Er", "kam", "auf", "Was\u00b7ser", "dort", "bey", "fins\u00b7ter", "d\u00fcst\u00b7re", "Nacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ADV", "APPR", "ADJA", "VVFIN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Er kommt bey Tag in Feur/ da Er die Kirche macht.", "tokens": ["Er", "kommt", "bey", "Tag", "in", "Feur", "/", "da", "Er", "die", "Kir\u00b7che", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "APPR", "NN", "$(", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}