{"dta.poem.1086": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Die 4. Antistrophe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1618", "urn": "urn:nbn:de:kobv:b4-20628-9", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wje der starcke Thetische sohn", "tokens": ["Wie", "der", "star\u00b7cke", "The\u00b7ti\u00b7sche", "sohn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Allerhand Ritterspihl geliebet/", "tokens": ["Al\u00b7ler\u00b7hand", "Rit\u00b7ter\u00b7spihl", "ge\u00b7lie\u00b7bet", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Vnd sich t\u00e4glich darin ge\u00fcbet/", "tokens": ["Vnd", "sich", "t\u00e4g\u00b7lich", "da\u00b7rin", "ge\u00b7\u00fc\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "ADJD", "PAV", "VVPP", "$("], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Trachtend stehts nach der ehren-cron:", "tokens": ["Trach\u00b7tend", "stehts", "nach", "der", "eh\u00b7ren\u00b7cron", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "Als wolgeschickt zu rechter zeit", "tokens": ["Als", "wol\u00b7ge\u00b7schickt", "zu", "rech\u00b7ter", "zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zu dantzen/ ringen/ vnd turnieren/", "tokens": ["Zu", "dant\u00b7zen", "/", "rin\u00b7gen", "/", "vnd", "tur\u00b7nie\u00b7ren", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$(", "VVFIN", "$(", "KON", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Als vnver gleichlich in dem streit", "tokens": ["Als", "vn\u00b7ver", "gleich\u00b7lich", "in", "dem", "streit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Vber den feind zu triumfieren:", "tokens": ["Vber", "den", "feind", "zu", "tri\u00b7um\u00b7fie\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Also hat deiner wafen glantz", "tokens": ["Al\u00b7so", "hat", "dei\u00b7ner", "wa\u00b7fen", "glantz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "NN"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.10": {"text": "In schimpf den sig oft weggef\u00fchret/", "tokens": ["In", "schimpf", "den", "sig", "oft", "weg\u00b7ge\u00b7f\u00fch\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ART", "ADJD", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Vnd den schwaissigen Lorb\u00f6r-crantz", "tokens": ["Vnd", "den", "schwais\u00b7si\u00b7gen", "Lor\u00b7b\u00f6r\u00b7crantz"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.12": {"text": "In ernst mit deinem haupt gezieret.", "tokens": ["In", "ernst", "mit", "dei\u00b7nem", "haupt", "ge\u00b7zie\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}