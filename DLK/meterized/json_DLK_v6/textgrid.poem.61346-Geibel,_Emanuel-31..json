{"textgrid.poem.61346": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "31.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Warum dies Buch mir so mi\u00dff\u00e4llt?", "tokens": ["Wa\u00b7rum", "dies", "Buch", "mir", "so", "mi\u00df\u00b7f\u00e4llt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "NN", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Just, weil es Wahrheit auch enth\u00e4lt;", "tokens": ["Just", ",", "weil", "es", "Wahr\u00b7heit", "auch", "ent\u00b7h\u00e4lt", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "NN", "ADV", "VVFIN", "$."], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn br\u00e4cht' es nichts als eitel L\u00fcgen,", "tokens": ["Denn", "br\u00e4cht'", "es", "nichts", "als", "ei\u00b7tel", "L\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIS", "KOKOM", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wer lie\u00dfe sich davon betr\u00fcgen!", "tokens": ["Wer", "lie\u00b7\u00dfe", "sich", "da\u00b7von", "be\u00b7tr\u00fc\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PRF", "PAV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}