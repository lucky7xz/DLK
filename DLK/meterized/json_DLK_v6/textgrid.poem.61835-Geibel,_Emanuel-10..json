{"textgrid.poem.61835": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "10.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "'s ist eben manchen Leuten eigen,", "tokens": ["'s", "ist", "e\u00b7ben", "man\u00b7chen", "Leu\u00b7ten", "ei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PIAT", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Da\u00df ihnen Schlichtes nicht ger\u00e4t;", "tokens": ["Da\u00df", "ih\u00b7nen", "Schlich\u00b7tes", "nicht", "ge\u00b7r\u00e4t", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie m\u00fcssen immer ins Fenster steigen,", "tokens": ["Sie", "m\u00fcs\u00b7sen", "im\u00b7mer", "ins", "Fens\u00b7ter", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Auch wenn die Haust\u00fcr offen steht.", "tokens": ["Auch", "wenn", "die", "Haus\u00b7t\u00fcr", "of\u00b7fen", "steht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}