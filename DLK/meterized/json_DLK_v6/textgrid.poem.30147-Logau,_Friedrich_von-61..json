{"textgrid.poem.30147": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "61.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Tod, der alles sterbt, den sterbt ein gut Ger\u00fcchte,", "tokens": ["Der", "Tod", ",", "der", "al\u00b7les", "sterbt", ",", "den", "sterbt", "ein", "gut", "Ge\u00b7r\u00fcch\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIS", "VVFIN", "$,", "ART", "VVFIN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das stirbt, wann gleich die Welt mu\u00df sterben, doch mit nichte,", "tokens": ["Das", "stirbt", ",", "wann", "gleich", "die", "Welt", "mu\u00df", "ster\u00b7ben", ",", "doch", "mit", "nich\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "PWAV", "ADV", "ART", "NN", "VMFIN", "VVINF", "$,", "ADV", "APPR", "PIS", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Besteht und hat den Ruhm f\u00fcr Gottes Angesichte.", "tokens": ["Be\u00b7steht", "und", "hat", "den", "Ruhm", "f\u00fcr", "Got\u00b7tes", "An\u00b7ge\u00b7sich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VAFIN", "ART", "NN", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}