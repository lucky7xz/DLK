{"dta.poem.19674": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Aus der Zeit ,  wo die Sch\u00e4fereyen \u00fcberhand  \n nahmen .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519168", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00e4fer. Mein Freund! Ein guter Freund,               ", "tokens": ["Sch\u00e4\u00b7fer", ".", "Mein", "Freund", "!", "Ein", "gu\u00b7ter", "Freund", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPOSAT", "NN", "$.", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Der hier versp\u00e4tet weint,", "tokens": ["Der", "hier", "ver\u00b7sp\u00e4\u00b7tet", "weint", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Erbittet sich zur Gnad", "tokens": ["Er\u00b7bit\u00b7tet", "sich", "zur", "Gnad"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Hier eine Ruhestadt,", "tokens": ["Hier", "ei\u00b7ne", "Ru\u00b7he\u00b7stadt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Weil er von diesem Ort", "tokens": ["Weil", "er", "von", "die\u00b7sem", "Ort"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PDAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Nicht mehr kann reisen fort.", "tokens": ["Nicht", "mehr", "kann", "rei\u00b7sen", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VMFIN", "VVINF", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Nachtw\u00e4chter. Wer seyd ihr?             ", "tokens": ["Nacht\u00b7w\u00e4ch\u00b7ter", ".", "Wer", "seyd", "ihr", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PWS", "VAFIN", "PPER", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Sch\u00e4fer. Ich bin ein treuer Hirt,               ", "tokens": ["Sch\u00e4\u00b7fer", ".", "Ich", "bin", "ein", "treu\u00b7er", "Hirt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Aus Liebe und Begierd,", "tokens": ["Aus", "Lie\u00b7be", "und", "Be\u00b7gierd", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Seht an mein Hirtenstab,", "tokens": ["Seht", "an", "mein", "Hir\u00b7ten\u00b7stab", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Den ich in H\u00e4nden hab,", "tokens": ["Den", "ich", "in", "H\u00e4n\u00b7den", "hab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPR", "NN", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Damit weid ich die Heerd", "tokens": ["Da\u00b7mit", "weid", "ich", "die", "Heerd"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "ART", "NN"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Wies mich der Vater lehrt.", "tokens": ["Wies", "mich", "der", "Va\u00b7ter", "lehrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Nachtw\u00e4chter. Wen sucht ihr?             ", "tokens": ["Nacht\u00b7w\u00e4ch\u00b7ter", ".", "Wen", "sucht", "ihr", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PWS", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Sch\u00e4fer. Ich such aus treuem Sinn               ", "tokens": ["Sch\u00e4\u00b7fer", ".", "Ich", "such", "aus", "treu\u00b7em", "Sinn"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "PPER", "PRF", "APPR", "ADJA", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Die edle Sch\u00e4ferinn,", "tokens": ["Die", "ed\u00b7le", "Sch\u00e4\u00b7fe\u00b7rinn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die sich von meiner Heerd,", "tokens": ["Die", "sich", "von", "mei\u00b7ner", "Heerd", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "So schn\u00f6d hinweggekehrt,", "tokens": ["So", "schn\u00f6d", "hin\u00b7weg\u00b7ge\u00b7kehrt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und sich in dieser Stadt", "tokens": ["Und", "sich", "in", "die\u00b7ser", "Stadt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PRF", "APPR", "PDAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Gewi\u00df verloren hat.", "tokens": ["Ge\u00b7wi\u00df", "ver\u00b7lo\u00b7ren", "hat", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Nachtw\u00e4chter. Wenn ihr ein Sch\u00e4fer seyd, so ge-               ", "tokens": ["Nacht\u00b7w\u00e4ch\u00b7ter", ".", "Wenn", "ihr", "ein", "Sch\u00e4\u00b7fer", "seyd", ",", "so", "ge"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "$.", "KOUS", "PPER", "ART", "NN", "VAFIN", "$,", "ADV", "TRUNC"], "meter": "++--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "h\u00f6rt ihr zu eurer Heerd, wie bald ists", "tokens": ["h\u00f6rt", "ihr", "zu", "eu\u00b7rer", "Heerd", ",", "wie", "bald", "ists"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,", "PWAV", "ADV", "VAFIN"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "geschehen, da\u00df ein Wolf kommt und zer-", "tokens": ["ge\u00b7sche\u00b7hen", ",", "da\u00df", "ein", "Wolf", "kommt", "und", "zer"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "KOUS", "ART", "NE", "VVFIN", "KON", "TRUNC"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "trennt die ganze Heerd.", "tokens": ["trennt", "die", "gan\u00b7ze", "Heerd", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Sch\u00e4fer. Wenn schon die ganze Heerd               ", "tokens": ["Sch\u00e4\u00b7fer", ".", "Wenn", "schon", "die", "gan\u00b7ze", "Heerd"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "KOUS", "ADV", "ART", "ADJA", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Von ihm zertrennet w\u00e4r,", "tokens": ["Von", "ihm", "zer\u00b7tren\u00b7net", "w\u00e4r", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So w\u00e4r es nicht so viel,", "tokens": ["So", "w\u00e4r", "es", "nicht", "so", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKNEG", "ADV", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Als wenn ich ohne Ziel", "tokens": ["Als", "wenn", "ich", "oh\u00b7ne", "Ziel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "PPER", "APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sollt ohne Sch\u00e4frin seyn,", "tokens": ["Sollt", "oh\u00b7ne", "Sch\u00e4f\u00b7rin", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "NN", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Und nunmehr ganz allein.", "tokens": ["Und", "nun\u00b7mehr", "ganz", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Nachtw\u00e4chter. Ihr werdet schon eine andre finden,               ", "tokens": ["Nacht\u00b7w\u00e4ch\u00b7ter", ".", "Ihr", "wer\u00b7det", "schon", "ei\u00b7ne", "and\u00b7re", "fin\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VAFIN", "ADV", "ART", "PIS", "VVINF", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Was braucht ihr der so nachzulaufen?", "tokens": ["Was", "braucht", "ihr", "der", "so", "nach\u00b7zu\u00b7lau\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ist sie so gewaltig sch\u00f6n?", "tokens": ["Ist", "sie", "so", "ge\u00b7wal\u00b7tig", "sch\u00f6n", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Sch\u00e4fer. Sie ist vortreflich sch\u00f6n               ", "tokens": ["Sch\u00e4\u00b7fer", ".", "Sie", "ist", "vor\u00b7tre\u00b7flich", "sch\u00f6n"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$.", "PPER", "VAFIN", "ADJD", "ADJD"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Wie eine G\u00f6tterin,", "tokens": ["Wie", "ei\u00b7ne", "G\u00f6t\u00b7te\u00b7rin", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihr Auge ist wie Feur,", "tokens": ["Ihr", "Au\u00b7ge", "ist", "wie", "Feur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "KOKOM", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Das macht sie mir so theur,", "tokens": ["Das", "macht", "sie", "mir", "so", "theur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die liebliche Gestalt", "tokens": ["Die", "lieb\u00b7li\u00b7che", "Ge\u00b7stalt"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Ist wie man Venus mahlt.", "tokens": ["Ist", "wie", "man", "Ve\u00b7nus", "mahlt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "PIS", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Nachtw\u00e4chter. Von Venus mag ich gar nichts wis-               ", "tokens": ["Nacht\u00b7w\u00e4ch\u00b7ter", ".", "Von", "Ve\u00b7nus", "mag", "ich", "gar", "nichts", "wis"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "APPR", "NE", "VMFIN", "PPER", "ADV", "PIS", "TRUNC"], "meter": "++--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "sen, Korporal heraus und Bursche", "tokens": ["sen", ",", "Kor\u00b7po\u00b7ral", "he\u00b7raus", "und", "Bur\u00b7sche"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "NN", "PTKVZ", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "ins Gewehr und f\u00fchrt den Kerl ans Licht.", "tokens": ["ins", "Ge\u00b7wehr", "und", "f\u00fchrt", "den", "Kerl", "ans", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.11": {"line.1": {"text": "Sch\u00e4fer. Gewalt geht stets vor Recht,               ", "tokens": ["Sch\u00e4\u00b7fer", ".", "Ge\u00b7walt", "geht", "stets", "vor", "Recht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "NN", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Mein Treu bezahlt man schlecht,", "tokens": ["Mein", "Treu", "be\u00b7zahlt", "man", "schlecht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PIS", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich such die ganze Nacht,", "tokens": ["Ich", "such", "die", "gan\u00b7ze", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Man f\u00fchrt mich ", "tokens": ["Man", "f\u00fchrt", "mich"], "token_info": ["word", "word", "word"], "pos": ["PIS", "VVFIN", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Adje man f\u00fchrt mich hin", "tokens": ["Ad\u00b7je", "man", "f\u00fchrt", "mich", "hin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "PIS", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "O edle Sch\u00e4ferin.", "tokens": ["O", "ed\u00b7le", "Sch\u00e4\u00b7fe\u00b7rin", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Nachtw\u00e4chter. Licht her, Kerl was winkt er mir? &#8212;               ", "tokens": ["Nacht\u00b7w\u00e4ch\u00b7ter", ".", "Licht", "her", ",", "Kerl", "was", "winkt", "er", "mir", "?", "&#8212;"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "XML_entity"], "pos": ["NN", "$.", "NN", "PTKVZ", "$,", "NN", "PWS", "VVFIN", "PPER", "PPER", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ach ihre Majest\u00e4t! Sie sind es! \u2014 Gnade,", "tokens": ["Ach", "ih\u00b7re", "Ma\u00b7jes\u00b7t\u00e4t", "!", "Sie", "sind", "es", "!", "Gna\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["ITJ", "PPOSAT", "NN", "$.", "PPER", "VAFIN", "PPER", "$.", "$(", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "machen sie einen treuen alten Diener nicht un-", "tokens": ["ma\u00b7chen", "sie", "ei\u00b7nen", "treu\u00b7en", "al\u00b7ten", "Die\u00b7ner", "nicht", "un"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "ADJA", "NN", "PTKNEG", "TRUNC"], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}, "line.4": {"text": "gl\u00fccklich!", "tokens": ["gl\u00fcck\u00b7lich", "!"], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.13": {"line.1": {"text": "Sch\u00e4fer. Ihr sollt mirs nicht ansehn,               ", "tokens": ["Sch\u00e4\u00b7fer", ".", "Ihr", "sollt", "mirs", "nicht", "an\u00b7sehn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPER", "VMFIN", "NE", "PTKNEG", "VVINF", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Ihr k\u00f6nnt mirs nicht ansehn,", "tokens": ["Ihr", "k\u00f6nnt", "mirs", "nicht", "an\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein Sch\u00e4fer will ich seyn,", "tokens": ["Ein", "Sch\u00e4\u00b7fer", "will", "ich", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VMFIN", "PPER", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ein Sch\u00e4fer ganz allein,", "tokens": ["Ein", "Sch\u00e4\u00b7fer", "ganz", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ihr seyd einf\u00e4ltge Schaf,", "tokens": ["Ihr", "seyd", "ein\u00b7f\u00e4lt\u00b7ge", "Schaf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Und ich erla\u00df die Straf.", "tokens": ["Und", "ich", "er\u00b7la\u00df", "die", "Straf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}