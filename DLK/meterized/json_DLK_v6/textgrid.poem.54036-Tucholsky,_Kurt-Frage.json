{"textgrid.poem.54036": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Frage", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es laufen vor Premieren", "tokens": ["Es", "lau\u00b7fen", "vor", "Pre\u00b7mie\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ger\u00fcchte durch die Stadt:", "tokens": ["Ge\u00b7r\u00fcch\u00b7te", "durch", "die", "Stadt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nun kommt, was man in Sph\u00e4ren", "tokens": ["Nun", "kommt", ",", "was", "man", "in", "Sph\u00e4\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "PRELS", "PIS", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "noch nicht gesehen hat.", "tokens": ["noch", "nicht", "ge\u00b7se\u00b7hen", "hat", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Doch hat der Rummel sich gelegt", "tokens": ["Doch", "hat", "der", "Rum\u00b7mel", "sich", "ge\u00b7legt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ART", "NN", "PRF", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "\u2013 so aufgeregt, so aufgeregt \u2013", "tokens": ["\u2013", "so", "auf\u00b7ge\u00b7regt", ",", "so", "auf\u00b7ge\u00b7regt", "\u2013"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ADV", "ADJD", "$,", "ADV", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "dann frag ich still, so leis ich kann:", "tokens": ["dann", "frag", "ich", "still", ",", "so", "leis", "ich", "kann", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "ADV", "ADJD", "PPER", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "\u00bbund dazu ziehn Sie 'n Smoking an \u2013?\u00ab", "tokens": ["\u00bb", "und", "da\u00b7zu", "ziehn", "Sie", "'n", "Smo\u00b7king", "an", "\u2013", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "KON", "PAV", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$(", "$.", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Es steigen gro\u00dfe B\u00e4lle,", "tokens": ["Es", "stei\u00b7gen", "gro\u00b7\u00dfe", "B\u00e4l\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "und die Plakate schrein.", "tokens": ["und", "die", "Pla\u00b7ka\u00b7te", "schrein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Man mu\u00df auf alle F\u00e4lle", "tokens": ["Man", "mu\u00df", "auf", "al\u00b7le", "F\u00e4l\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "APPR", "PIAT", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "da reingetreten sein.", "tokens": ["da", "rein\u00b7ge\u00b7tre\u00b7ten", "sein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der Sekt ist warm, die Garderobe kalt.", "tokens": ["Der", "Sekt", "ist", "warm", ",", "die", "Gar\u00b7de\u00b7ro\u00b7be", "kalt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "\u00bbich glaube, Lo, nun gehn wir bald . . . \u00ab", "tokens": ["\u00bb", "ich", "glau\u00b7be", ",", "Lo", ",", "nun", "gehn", "wir", "bald", ".", ".", ".", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "$,", "NE", "$,", "ADV", "VVFIN", "PPER", "ADV", "$.", "$.", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Zu Hause sehn sich alle an:", "tokens": ["Zu", "Hau\u00b7se", "sehn", "sich", "al\u00b7le", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PRF", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "\u00bbund dazu ziehn wir 'n Smoking an \u2013?\u00ab", "tokens": ["\u00bb", "und", "da\u00b7zu", "ziehn", "wir", "'n", "Smo\u00b7king", "an", "\u2013", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "KON", "PAV", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$(", "$.", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Es prangt in den Journalen", "tokens": ["Es", "prangt", "in", "den", "Jour\u00b7na\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "das Bildnis einer Frau.", "tokens": ["das", "Bild\u00b7nis", "ei\u00b7ner", "Frau", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sch\u00f6n ist sie angemalen,", "tokens": ["Sch\u00f6n", "ist", "sie", "an\u00b7ge\u00b7ma\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "hellrosa, beige und blau.", "tokens": ["hell\u00b7ro\u00b7sa", ",", "bei\u00b7ge", "und", "blau", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADJA", "KON", "ADJD", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Dir gl\u00fcckts . . . ihr Widerstand erschlafft . . .", "tokens": ["Dir", "gl\u00fcckts", ".", ".", ".", "ihr", "Wi\u00b7der\u00b7stand", "er\u00b7schlafft", ".", ".", "."], "token_info": ["word", "word", "punct", "punct", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VVFIN", "$.", "$.", "$.", "PPOSAT", "NN", "VVPP", "$.", "$.", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Na, fabelhaft! Na, fabelhaft?", "tokens": ["Na", ",", "fa\u00b7bel\u00b7haft", "!", "Na", ",", "fa\u00b7bel\u00b7haft", "?"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ITJ", "$,", "ADJD", "$.", "ITJ", "$,", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Grau ist der Morgen . . . welk der Strau\u00df . . .", "tokens": ["Grau", "ist", "der", "Mor\u00b7gen", ".", ".", ".", "welk", "der", "Strau\u00df", ".", ".", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$.", "$.", "$.", "ADJD", "ART", "NE", "$.", "$.", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und dazu zieh ich 'n Smoking aus \u2013?", "tokens": ["Und", "da\u00b7zu", "zieh", "ich", "'n", "Smo\u00b7king", "aus", "\u2013", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PAV", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$(", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Willst du nach oben schweben,", "tokens": ["Willst", "du", "nach", "o\u00b7ben", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "f\u00e4llst du auf den Popo.", "tokens": ["f\u00e4llst", "du", "auf", "den", "Po\u00b7po", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und \u00fcberhaupt das Leben,", "tokens": ["Und", "\u00fc\u00b7ber\u00b7haupt", "das", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "es ist gemeinhin so:", "tokens": ["es", "ist", "ge\u00b7mein\u00b7hin", "so", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Erst viel Geschrei und m\u00e4chtiger Zimt.", "tokens": ["Erst", "viel", "Ge\u00b7schrei", "und", "m\u00e4ch\u00b7ti\u00b7ger", "Zimt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "KON", "ADJA", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Sieh nur, wie alles Karten nimmt!", "tokens": ["Sieh", "nur", ",", "wie", "al\u00b7les", "Kar\u00b7ten", "nimmt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "PWAV", "PIS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Aber mehrstenteils, o Smokingmann:", "tokens": ["A\u00b7ber", "mehrs\u00b7ten\u00b7teils", ",", "o", "Smo\u00b7king\u00b7mann", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "FM", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Zieh ihn gar nicht erst an! Zieh ihn gar nicht erst an \u2013!", "tokens": ["Zieh", "ihn", "gar", "nicht", "erst", "an", "!", "Zieh", "ihn", "gar", "nicht", "erst", "an", "\u2013", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKNEG", "ADV", "PTKVZ", "$.", "VVFIN", "PPER", "ADV", "PTKNEG", "ADV", "APPR", "$(", "$."], "meter": "-+-+-+---+-+", "measure": "unknown.measure.penta"}}}}}