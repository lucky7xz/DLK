{"textgrid.poem.65762": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "36", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gesteh' ich's nur: gesondert bleiben wir,", "tokens": ["Ge\u00b7steh'", "ich's", "nur", ":", "ge\u00b7son\u00b7dert", "blei\u00b7ben", "wir", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "ADV", "$.", "VVPP", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie auch unteilbar unsre Herzen schlagen.", "tokens": ["Wie", "auch", "un\u00b7teil\u00b7bar", "uns\u00b7re", "Her\u00b7zen", "schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So kann ich ohne H\u00fclfe dann von dir", "tokens": ["So", "kann", "ich", "oh\u00b7ne", "H\u00fcl\u00b7fe", "dann", "von", "dir"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "NN", "ADV", "APPR", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Flecken meines eignen Wesens tragen.", "tokens": ["Die", "Fle\u00b7cken", "mei\u00b7nes", "eig\u00b7nen", "We\u00b7sens", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "In unsern Herzen ist nur ein Gef\u00fchl,", "tokens": ["In", "un\u00b7sern", "Her\u00b7zen", "ist", "nur", "ein", "Ge\u00b7f\u00fchl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "In unsern Leben zwistiger Verdru\u00df:", "tokens": ["In", "un\u00b7sern", "Le\u00b7ben", "zwis\u00b7ti\u00b7ger", "Ver\u00b7dru\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Zwar irrt er nicht der Liebe reines Ziel,", "tokens": ["Zwar", "irrt", "er", "nicht", "der", "Lie\u00b7be", "rei\u00b7nes", "Ziel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Doch s\u00fc\u00dfe Stunden raubt er dem Genu\u00df.", "tokens": ["Doch", "s\u00fc\u00b7\u00dfe", "Stun\u00b7den", "raubt", "er", "dem", "Ge\u00b7nu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Nicht \u00fcberall darf ich mich zu dir kehren,", "tokens": ["Nicht", "\u00fc\u00b7be\u00b7rall", "darf", "ich", "mich", "zu", "dir", "keh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VMFIN", "PPER", "PRF", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Wo mein beweint Vergehn dir Schmach zu bringen schien;", "tokens": ["Wo", "mein", "be\u00b7weint", "Ver\u00b7gehn", "dir", "Schmach", "zu", "brin\u00b7gen", "schien", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "VVFIN", "NN", "PPER", "NN", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Noch du mit \u00f6ffentlicher Gunst mich ehren,", "tokens": ["Noch", "du", "mit", "\u00f6f\u00b7fent\u00b7li\u00b7cher", "Gunst", "mich", "eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "APPR", "ADJA", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.12": {"text": "Willst du nicht deinem Namen Ehr' entziehn.", "tokens": ["Willst", "du", "nicht", "dei\u00b7nem", "Na\u00b7men", "Ehr'", "ent\u00b7ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "PPOSAT", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Doch, tu' es nicht! Ich halte so dich wert,", "tokens": ["Doch", ",", "tu'", "es", "nicht", "!", "Ich", "hal\u00b7te", "so", "dich", "wert", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "PPER", "PTKNEG", "$.", "PPER", "VVFIN", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Da\u00df, wie du selbst, mein auch dein Ruf geh\u00f6rt.", "tokens": ["Da\u00df", ",", "wie", "du", "selbst", ",", "mein", "auch", "dein", "Ruf", "ge\u00b7h\u00f6rt", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PWAV", "PPER", "ADV", "$,", "PPOSAT", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}