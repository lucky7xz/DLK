{"textgrid.poem.39775": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Morgenbegegnung", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie ist an mir vorbeigegangen,", "tokens": ["Sie", "ist", "an", "mir", "vor\u00b7bei\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit fl\u00fccht'gem Gru\u00dfe schnell und kalt,", "tokens": ["Mit", "fl\u00fccht'\u00b7gem", "Gru\u00b7\u00dfe", "schnell", "und", "kalt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kein Schimmer flog auf ihre Wangen,", "tokens": ["Kein", "Schim\u00b7mer", "flog", "auf", "ih\u00b7re", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Kein Licht durchz\u00fcckte die Gestalt.", "tokens": ["Kein", "Licht", "durch\u00b7z\u00fcck\u00b7te", "die", "Ge\u00b7stalt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie anders haben meine Tr\u00e4ume", "tokens": ["Wie", "an\u00b7ders", "ha\u00b7ben", "mei\u00b7ne", "Tr\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mir sie noch diese Nacht gestellt!", "tokens": ["Mir", "sie", "noch", "die\u00b7se", "Nacht", "ge\u00b7stellt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "ADV", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da wandelte sie durch die B\u00e4ume,", "tokens": ["Da", "wan\u00b7del\u00b7te", "sie", "durch", "die", "B\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So ganz von Freundlichkeit erhellt;", "tokens": ["So", "ganz", "von", "Freund\u00b7lich\u00b7keit", "er\u00b7hellt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und neigte sich zu s\u00fc\u00dfem Gr\u00fc\u00dfen", "tokens": ["Und", "neig\u00b7te", "sich", "zu", "s\u00fc\u00b7\u00dfem", "Gr\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit so getreuem Angesicht:", "tokens": ["Mit", "so", "ge\u00b7treu\u00b7em", "An\u00b7ge\u00b7sicht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "$."], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.3": {"text": "Ich sah den Himmel sich erschlie\u00dfen,", "tokens": ["Ich", "sah", "den", "Him\u00b7mel", "sich", "er\u00b7schlie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich zweifelte, ich schwankte nicht.", "tokens": ["Ich", "zwei\u00b7fel\u00b7te", ",", "ich", "schwank\u00b7te", "nicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich wei\u00df nicht, wie es ist ergangen,", "tokens": ["Ich", "wei\u00df", "nicht", ",", "wie", "es", "ist", "er\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWAV", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich lag ihr in dem zarten Arm;", "tokens": ["Ich", "lag", "ihr", "in", "dem", "zar\u00b7ten", "Arm", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich hielt das theure Kind umfangen,", "tokens": ["Ich", "hielt", "das", "theu\u00b7re", "Kind", "um\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fchlt' all ihr Leben voll und warm.", "tokens": ["F\u00fchlt'", "all", "ihr", "Le\u00b7ben", "voll", "und", "warm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "PPOSAT", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Mir duftete der Strau\u00df entgegen,", "tokens": ["Mir", "duf\u00b7te\u00b7te", "der", "Strau\u00df", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NE", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der keusche W\u00e4chter ihrer Brust,", "tokens": ["Der", "keu\u00b7sche", "W\u00e4ch\u00b7ter", "ih\u00b7rer", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es war mir ihres Herzens Regen,", "tokens": ["Es", "war", "mir", "ih\u00b7res", "Her\u00b7zens", "Re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "All ihre Seele mir bewu\u00dft.", "tokens": ["All", "ih\u00b7re", "See\u00b7le", "mir", "be\u00b7wu\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "So hielt der Traum mich still und lange", "tokens": ["So", "hielt", "der", "Traum", "mich", "still", "und", "lan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "ADJD", "KON", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Versenkt in ruhigem Genu\u00df.", "tokens": ["Ver\u00b7senkt", "in", "ru\u00b7hi\u00b7gem", "Ge\u00b7nu\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-++--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Da endlich dr\u00fcckt' er Wang' an Wange", "tokens": ["Da", "end\u00b7lich", "dr\u00fcckt'", "er", "Wang'", "an", "Wan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und schied mit einem sel'gen Ku\u00df.", "tokens": ["Und", "schied", "mit", "ei\u00b7nem", "sel'\u00b7gen", "Ku\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Ich war erwacht, ich eilt' ins Freie,", "tokens": ["Ich", "war", "er\u00b7wacht", ",", "ich", "eilt'", "ins", "Frei\u00b7e", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Seltsam bewegt von Lieb' und Wahn;", "tokens": ["Selt\u00b7sam", "be\u00b7wegt", "von", "Lieb'", "und", "Wahn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Und, wie im Schlaf, so kam aufs Neue", "tokens": ["Und", ",", "wie", "im", "Schlaf", ",", "so", "kam", "aufs", "Neu\u00b7e"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$,", "PWAV", "APPRART", "NN", "$,", "ADV", "VVFIN", "APPRART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Alsbald die S\u00fc\u00dfe mir heran.", "tokens": ["Als\u00b7bald", "die", "S\u00fc\u00b7\u00dfe", "mir", "he\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Wie dacht' ich meines Traumes wieder!", "tokens": ["Wie", "dacht'", "ich", "mei\u00b7nes", "Trau\u00b7mes", "wie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Meint', Alles w\u00e4re so geschehn;", "tokens": ["Meint'", ",", "Al\u00b7les", "w\u00e4\u00b7re", "so", "ge\u00b7schehn", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PIS", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich schlug versch\u00e4mt die Augen nieder,", "tokens": ["Ich", "schlug", "ver\u00b7sch\u00e4mt", "die", "Au\u00b7gen", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und wagte kaum, sie anzusehn.", "tokens": ["Und", "wag\u00b7te", "kaum", ",", "sie", "an\u00b7zu\u00b7sehn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$,", "PPER", "VVIZU", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Ich schielte bang nach meinem Gl\u00fccke \u2013", "tokens": ["Ich", "schiel\u00b7te", "bang", "nach", "mei\u00b7nem", "Gl\u00fc\u00b7cke", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie sah nicht auf, sie winkte kaum:", "tokens": ["Sie", "sah", "nicht", "auf", ",", "sie", "wink\u00b7te", "kaum", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PTKVZ", "$,", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ach, diese Lippen, diese Blicke,", "tokens": ["Ach", ",", "die\u00b7se", "Lip\u00b7pen", ",", "die\u00b7se", "Bli\u00b7cke", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "PDAT", "NN", "$,", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie wissen nichts von meinem Traum!", "tokens": ["Sie", "wis\u00b7sen", "nichts", "von", "mei\u00b7nem", "Traum", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Geh' denn, du armes Lied, und sage,", "tokens": ["Geh'", "denn", ",", "du", "ar\u00b7mes", "Lied", ",", "und", "sa\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "KON", "$,", "PPER", "ADJA", "NN", "$,", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was ich ihr stets verschweigen mu\u00df!", "tokens": ["Was", "ich", "ihr", "stets", "ver\u00b7schwei\u00b7gen", "mu\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPER", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Erfleh' von Ihr mit deiner Klage", "tokens": ["Er\u00b7fleh'", "von", "Ihr", "mit", "dei\u00b7ner", "Kla\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nur einen holden Morgengru\u00df!", "tokens": ["Nur", "ei\u00b7nen", "hol\u00b7den", "Mor\u00b7gen\u00b7gru\u00df", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "So wird der Traum mir zur Geschichte,", "tokens": ["So", "wird", "der", "Traum", "mir", "zur", "Ge\u00b7schich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und tritt ins wache Leben ein:", "tokens": ["Und", "tritt", "ins", "wa\u00b7che", "Le\u00b7ben", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Seh' ich auf ihrem Angesichte", "tokens": ["Seh'", "ich", "auf", "ih\u00b7rem", "An\u00b7ge\u00b7sich\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den kurzen, lieben Widerschein.", "tokens": ["Den", "kur\u00b7zen", ",", "lie\u00b7ben", "Wi\u00b7der\u00b7schein", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}