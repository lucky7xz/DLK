{"textgrid.poem.44961": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Priccialdi", "genre": "verse", "period": "N.A.", "pub_year": 1831, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was bl\u00e4sest du, ungl\u00fccklicher Tamino?", "tokens": ["Was", "bl\u00e4\u00b7sest", "du", ",", "un\u00b7gl\u00fcck\u00b7li\u00b7cher", "Ta\u00b7mi\u00b7no", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "ADJA", "NE", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Meinst du, weil Eingeweihte wohnen hier?", "tokens": ["Meinst", "du", ",", "weil", "Ein\u00b7ge\u00b7weih\u00b7te", "woh\u00b7nen", "hier", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie aber blasen selbst auf ihren H\u00f6rnern,", "tokens": ["Sie", "a\u00b7ber", "bla\u00b7sen", "selbst", "auf", "ih\u00b7ren", "H\u00f6r\u00b7nern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Affen nur und B\u00e4ren lauschen dir.", "tokens": ["Und", "Af\u00b7fen", "nur", "und", "B\u00e4\u00b7ren", "lau\u00b7schen", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "KON", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}