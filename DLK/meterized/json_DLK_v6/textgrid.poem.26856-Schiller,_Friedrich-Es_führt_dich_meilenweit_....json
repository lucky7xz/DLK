{"textgrid.poem.26856": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Es f\u00fchrt dich meilenweit ...", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es f\u00fchrt dich meilenweit von dannen", "tokens": ["Es", "f\u00fchrt", "dich", "mei\u00b7len\u00b7weit", "von", "dan\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und bleibt doch stets an seinem Ort,", "tokens": ["Und", "bleibt", "doch", "stets", "an", "sei\u00b7nem", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es hat nicht Fl\u00fcgel auszuspannen", "tokens": ["Es", "hat", "nicht", "Fl\u00fc\u00b7gel", "aus\u00b7zu\u00b7span\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PTKNEG", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und tr\u00e4gt dich durch die L\u00fcfte fort.", "tokens": ["Und", "tr\u00e4gt", "dich", "durch", "die", "L\u00fcf\u00b7te", "fort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es ist die allerschnellste F\u00e4hre,", "tokens": ["Es", "ist", "die", "al\u00b7ler\u00b7schnells\u00b7te", "F\u00e4h\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die jemals einen Wandrer trug,", "tokens": ["Die", "je\u00b7mals", "ei\u00b7nen", "Wand\u00b7rer", "trug", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und durch das gr\u00f6\u00dfte aller Meere", "tokens": ["Und", "durch", "das", "gr\u00f6\u00df\u00b7te", "al\u00b7ler", "Mee\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Tr\u00e4gt es dich mit Gedankenflug,", "tokens": ["Tr\u00e4gt", "es", "dich", "mit", "Ge\u00b7dan\u00b7ken\u00b7flug", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "NN", "$,"], "meter": "+-++-+-+", "measure": "unknown.measure.penta"}, "line.9": {"text": "Ihm ist ein Augenblick genug!", "tokens": ["Ihm", "ist", "ein", "Au\u00b7gen\u00b7blick", "ge\u00b7nug", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dies leichte Schiff, das mit Gedankenschnelle", "tokens": ["Dies", "leich\u00b7te", "Schiff", ",", "das", "mit", "Ge\u00b7dan\u00b7ken\u00b7schnel\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "ADJA", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mich durch die L\u00fcfte ruhig tr\u00e4gt,", "tokens": ["Mich", "durch", "die", "L\u00fcf\u00b7te", "ru\u00b7hig", "tr\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sich selbst nicht von dem Ort bewegt,", "tokens": ["Sich", "selbst", "nicht", "von", "dem", "Ort", "be\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "PTKNEG", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Sehrohr ists, das in die Ferne", "tokens": ["Das", "Seh\u00b7rohr", "ists", ",", "das", "in", "die", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Den Blick befl\u00fcgelt bis ins Land der Sterne.", "tokens": ["Den", "Blick", "be\u00b7fl\u00fc\u00b7gelt", "bis", "ins", "Land", "der", "Ster\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}