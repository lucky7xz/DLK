{"dta.poem.3270": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Der Morgen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1755", "urn": "urn:nbn:de:kobv:b4-20614-6", "language": ["de:0.99"], "booktitle": "Uz, Johann Peter: Lyrische und andere Gedichte. 2. Aufl. Ansbach, 1755."}, "poem": {"stanza.1": {"line.1": {"text": "Auf! auf! weil schon Aurora lacht;", "tokens": ["Auf", "!", "auf", "!", "weil", "schon", "Au\u00b7ro\u00b7ra", "lacht", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "PTKVZ", "$.", "KOUS", "ADV", "NE", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Jhr Gatten junger Sch\u00f6nen!", "tokens": ["Ihr", "Gat\u00b7ten", "jun\u00b7ger", "Sch\u00f6\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Jhr m\u00fc\u00dft nunmehr, nach fauler Nacht,", "tokens": ["Ihr", "m\u00fc\u00dft", "nun\u00b7mehr", ",", "nach", "fau\u00b7ler", "Nacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Gott der Ehe fr\u00f6hnen.", "tokens": ["Dem", "Gott", "der", "E\u00b7he", "fr\u00f6h\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Erneuert den verliebten Zwist,", "tokens": ["Er\u00b7neu\u00b7ert", "den", "ver\u00b7lieb\u00b7ten", "Zwist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der s\u00fcsser, als die Eintracht ist,", "tokens": ["Der", "s\u00fcs\u00b7ser", ",", "als", "die", "Ein\u00b7tracht", "ist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "KOUS", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Nach der sich Alte sehnen.", "tokens": ["Nach", "der", "sich", "Al\u00b7te", "seh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PRF", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ists m\u00f6glich, da\u00df, geweckt von Lust,", "tokens": ["Ists", "m\u00f6g\u00b7lich", ",", "da\u00df", ",", "ge\u00b7weckt", "von", "Lust", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "$,", "VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Gatte nicht erwache?", "tokens": ["Ein", "Gat\u00b7te", "nicht", "er\u00b7wa\u00b7che", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df eine nahe Liljen-Brust", "tokens": ["Da\u00df", "ei\u00b7ne", "na\u00b7he", "Lil\u00b7jen\u00b7Brust"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Jhn nicht gesch\u00e4ftig mache?", "tokens": ["Jhn", "nicht", "ge\u00b7sch\u00e4f\u00b7tig", "ma\u00b7che", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Inde\u00df schwebt um der Gattinn Haupt", "tokens": ["In\u00b7de\u00df", "schwebt", "um", "der", "Gat\u00b7tinn", "Haupt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Der Morgentraum, mit Mohn umlaubt;", "tokens": ["Der", "Mor\u00b7gen\u00b7traum", ",", "mit", "Mohn", "um\u00b7laubt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Jhr tr\u00e4umt von eitel Rache.", "tokens": ["Ihr", "tr\u00e4umt", "von", "ei\u00b7tel", "Ra\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Dort, wo Cytherens waches Kind", "tokens": ["Dort", ",", "wo", "Cy\u00b7the\u00b7rens", "wa\u00b7ches", "Kind"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "NE", "ADJA", "NN"], "meter": "+----+-+", "measure": "dactylic.init"}, "line.2": {"text": "Den Schlaf vom Bette scheuchet;", "tokens": ["Den", "Schlaf", "vom", "Bet\u00b7te", "scheu\u00b7chet", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dort rauschts, wie wann ein Morgenwind", "tokens": ["Dort", "rauschts", ",", "wie", "wann", "ein", "Mor\u00b7gen\u00b7wind"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "PWAV", "PWAV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bethautes Laub durchstreichet.", "tokens": ["Be\u00b7thau\u00b7tes", "Laub", "durch\u00b7strei\u00b7chet", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dort lauscht auch meine Muse nun,", "tokens": ["Dort", "lauscht", "auch", "mei\u00b7ne", "Mu\u00b7se", "nun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die, wie die M\u00e4dchen alle thun,", "tokens": ["Die", ",", "wie", "die", "M\u00e4d\u00b7chen", "al\u00b7le", "thun", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PWAV", "ART", "NN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Verliebte gern beschleichet.", "tokens": ["Ver\u00b7lieb\u00b7te", "gern", "be\u00b7schlei\u00b7chet", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Der Vorhang weicht: welch reizend Weib!", "tokens": ["Der", "Vor\u00b7hang", "weicht", ":", "welch", "rei\u00b7zend", "Weib", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "PWAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich sehe Venus liegen,", "tokens": ["Ich", "se\u00b7he", "Ve\u00b7nus", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und leichten Flohr den Marmorleib", "tokens": ["Und", "leich\u00b7ten", "Flohr", "den", "Mar\u00b7mor\u00b7leib"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verr\u00e4therisch umfliegen.", "tokens": ["Ver\u00b7r\u00e4\u00b7the\u00b7risch", "um\u00b7flie\u00b7gen", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wie sucht ihr Blick, der kriegrisch gl\u00fcht,", "tokens": ["Wie", "sucht", "ihr", "Blick", ",", "der", "krie\u00b7grisch", "gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie sucht er, wenn der Streit verzieht,", "tokens": ["Wie", "sucht", "er", ",", "wenn", "der", "Streit", "ver\u00b7zieht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Streit, Gegner und Vergn\u00fcgen!", "tokens": ["Streit", ",", "Geg\u00b7ner", "und", "Ver\u00b7gn\u00fc\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Du itzo noch verliebtes Paar,", "tokens": ["Du", "it\u00b7zo", "noch", "ver\u00b7lieb\u00b7tes", "Paar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was mangelt deinem Gl\u00fccke?", "tokens": ["Was", "man\u00b7gelt", "dei\u00b7nem", "Gl\u00fc\u00b7cke", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich werde selbst entz\u00fcckt, gewahr,", "tokens": ["Ich", "wer\u00b7de", "selbst", "ent\u00b7z\u00fcckt", ",", "ge\u00b7wahr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df Hymen auch entz\u00fccke.", "tokens": ["Da\u00df", "Hy\u00b7men", "auch", "ent\u00b7z\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Muse sieht hinweg und weicht:", "tokens": ["Die", "Mu\u00b7se", "sieht", "hin\u00b7weg", "und", "weicht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Doch manchmal und verstohlen schleicht", "tokens": ["Doch", "manch\u00b7mal", "und", "ver\u00b7stoh\u00b7len", "schleicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "KON", "VVINF", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ein halber Blick zur\u00fccke.", "tokens": ["Ein", "hal\u00b7ber", "Blick", "zu\u00b7r\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}