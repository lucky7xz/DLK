{"textgrid.poem.45829": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[schnell nieder mit der alten Welt]", "genre": "verse", "period": "N.A.", "pub_year": 1799, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schnell nieder mit der alten Welt,", "tokens": ["Schnell", "nie\u00b7der", "mit", "der", "al\u00b7ten", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die neue zu erbauen.", "tokens": ["Die", "neu\u00b7e", "zu", "er\u00b7bau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der, dem die Liebe sich gesellt,", "tokens": ["Der", ",", "dem", "die", "Lie\u00b7be", "sich", "ge\u00b7sellt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PRELS", "ART", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Darf nicht nach Tr\u00fcmmern schauen.", "tokens": ["Darf", "nicht", "nach", "Tr\u00fcm\u00b7mern", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Aus Kraft und nicht aus Reue dringt,", "tokens": ["Aus", "Kraft", "und", "nicht", "aus", "Reu\u00b7e", "dringt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "PTKNEG", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Was die Vergangenheit verschlingt.", "tokens": ["Was", "die", "Ver\u00b7gan\u00b7gen\u00b7heit", "ver\u00b7schlingt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}