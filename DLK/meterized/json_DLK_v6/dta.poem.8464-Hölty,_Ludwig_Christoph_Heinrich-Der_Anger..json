{"dta.poem.8464": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Anger.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1783", "urn": "urn:nbn:de:kobv:b4-200905192568", "language": ["de:0.99"], "booktitle": "H\u00f6lty, Ludwig Christoph Heinrich: Gedichte. Hamburg, 1783."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Anger, welchen fr\u00fch und spat", "tokens": ["Mein", "An\u00b7ger", ",", "wel\u00b7chen", "fr\u00fch", "und", "spat"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PWAT", "ADJD", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein allerliebstes M\u00e4dchen trat", "tokens": ["Ein", "al\u00b7ler\u00b7liebs\u00b7tes", "M\u00e4d\u00b7chen", "trat"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit ihren weissen F\u00fcssen,", "tokens": ["Mit", "ih\u00b7ren", "weis\u00b7sen", "F\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit Zitterklang", "tokens": ["Mit", "Zit\u00b7ter\u00b7klang"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Und mit Gesang", "tokens": ["Und", "mit", "Ge\u00b7sang"], "token_info": ["word", "word", "word"], "pos": ["KON", "APPR", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Werd' ich dich oft begr\u00fcssen.", "tokens": ["Werd'", "ich", "dich", "oft", "be\u00b7gr\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Oft werd' ich in dein frisches Gras,", "tokens": ["Oft", "werd'", "ich", "in", "dein", "fri\u00b7sches", "Gras", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo sie mit mir am Schleedorn sass,", "tokens": ["Wo", "sie", "mit", "mir", "am", "Schlee\u00b7dorn", "sass", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im Abendrothe kommen,", "tokens": ["Im", "A\u00b7ben\u00b7dro\u00b7the", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und fingen dann,", "tokens": ["Und", "fin\u00b7gen", "dann", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "So gut ich kann,", "tokens": ["So", "gut", "ich", "kann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "VMFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Der Reinen und der Frommen.", "tokens": ["Der", "Rei\u00b7nen", "und", "der", "From\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Mit deinen Blumen, weiss und roth,", "tokens": ["Mit", "dei\u00b7nen", "Blu\u00b7men", ",", "weiss", "und", "roth", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Werd' ich, begl\u00e4nzt vom Abendroth,", "tokens": ["Werd'", "ich", ",", "be\u00b7gl\u00e4nzt", "vom", "A\u00b7ben\u00b7droth", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "VVPP", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die blanke Zitter kr\u00f6nen,", "tokens": ["Die", "blan\u00b7ke", "Zit\u00b7ter", "kr\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und weinen nass", "tokens": ["Und", "wei\u00b7nen", "nass"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Das gr\u00fcne Gras", "tokens": ["Das", "gr\u00fc\u00b7ne", "Gras"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Beim Namen meiner Sch\u00f6nen.", "tokens": ["Beim", "Na\u00b7men", "mei\u00b7ner", "Sch\u00f6\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Gieb doch, o lieber gr\u00fcner Plan,", "tokens": ["Gieb", "doch", ",", "o", "lie\u00b7ber", "gr\u00fc\u00b7ner", "Plan", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "FM", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Am Ende meiner Lebensbahn,", "tokens": ["Am", "En\u00b7de", "mei\u00b7ner", "Le\u00b7bens\u00b7bahn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bei dieser Murmelquelle,", "tokens": ["Bei", "die\u00b7ser", "Mur\u00b7mel\u00b7quel\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wo V\u00f6gelein", "tokens": ["Wo", "V\u00f6\u00b7ge\u00b7lein"], "token_info": ["word", "word"], "pos": ["PWAV", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Des Mais sich freun,", "tokens": ["Des", "Mais", "sich", "freun", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Mir meine Ruhestelle.", "tokens": ["Mir", "mei\u00b7ne", "Ru\u00b7he\u00b7stel\u00b7le", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}