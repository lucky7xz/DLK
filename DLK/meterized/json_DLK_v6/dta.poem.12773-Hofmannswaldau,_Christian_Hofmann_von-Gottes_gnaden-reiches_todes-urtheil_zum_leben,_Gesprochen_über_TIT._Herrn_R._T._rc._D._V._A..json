{"dta.poem.12773": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Gottes gnaden-reiches todes-urtheil  \n zum leben,  \n  Gesprochen \u00fcber  \n  TIT.  Herrn R. T. rc.  \n  D.  V. A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der sch\u00f6ppen-stuhl, ein kleinod dieser stadt,", "tokens": ["Der", "sch\u00f6p\u00b7pen\u00b7stuhl", ",", "ein", "klei\u00b7nod", "die\u00b7ser", "stadt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dergleichen man kaum hat,", "tokens": ["Derg\u00b7lei\u00b7chen", "man", "kaum", "hat", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "PIS", "ADV", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und fremde selbst als ihr oracul ehren;", "tokens": ["Und", "frem\u00b7de", "selbst", "als", "ihr", "o\u00b7ra\u00b7cul", "eh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "ADV", "KOUS", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das sonsten im gericht", "tokens": ["Das", "sons\u00b7ten", "im", "ge\u00b7richt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die todes-urthel spricht,", "tokens": ["Die", "to\u00b7des\u00b7ur\u00b7thel", "spricht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Mu\u00df itzo selbst dergleichen ausspruch h\u00f6ren.", "tokens": ["Mu\u00df", "it\u00b7zo", "selbst", "derg\u00b7lei\u00b7chen", "aus\u00b7spruch", "h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADV", "PIS", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wie geht es zu? Herr Teller, der allhier", "tokens": ["Wie", "geht", "es", "zu", "?", "Herr", "Tel\u00b7ler", ",", "der", "all\u00b7hier"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "PTKVZ", "$.", "NN", "NN", "$,", "PRELS", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Gesessen f\u00fcr und f\u00fcr,", "tokens": ["Ge\u00b7ses\u00b7sen", "f\u00fcr", "und", "f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "KON", "APPR", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und vielen hat das leben abgesprochen;", "tokens": ["Und", "vie\u00b7len", "hat", "das", "le\u00b7ben", "ab\u00b7ge\u00b7spro\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "PDS", "VVINF", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mein Freund, der liebe mann,", "tokens": ["Mein", "Freund", ",", "der", "lie\u00b7be", "mann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "H\u00f6rt itzund selber an,", "tokens": ["H\u00f6rt", "it\u00b7zund", "sel\u00b7ber", "an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Da\u00df \u00fcber ihn auch sey der stab gebrochen.", "tokens": ["Da\u00df", "\u00fc\u00b7ber", "ihn", "auch", "sey", "der", "stab", "ge\u00b7bro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPER", "ADV", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wem that er was? er lebte fromm und still;", "tokens": ["Wem", "that", "er", "was", "?", "er", "leb\u00b7te", "fromm", "und", "still", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PIS", "$.", "PPER", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Es war sein wunsch und will,", "tokens": ["Es", "war", "sein", "wunsch", "und", "will", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "ADJD", "KON", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gott, und zugleich dem n\u00e4chsten recht zu dienen:", "tokens": ["Gott", ",", "und", "zu\u00b7gleich", "dem", "n\u00e4chs\u00b7ten", "recht", "zu", "die\u00b7nen", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "ADV", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des vatern theurer geist", "tokens": ["Des", "va\u00b7tern", "theu\u00b7rer", "geist"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Hat sich in ihm erweist:", "tokens": ["Hat", "sich", "in", "ihm", "er\u00b7weist", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Jhm ist zu bald der letzte tag erschienen.", "tokens": ["Jhm", "ist", "zu", "bald", "der", "letz\u00b7te", "tag", "er\u00b7schie\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Der Werthe Mann, wie hat er sich erbaut,", "tokens": ["Der", "Wert\u00b7he", "Mann", ",", "wie", "hat", "er", "sich", "er\u00b7baut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PWAV", "VAFIN", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und wacker umgeschaut,", "tokens": ["Und", "wa\u00b7cker", "um\u00b7ge\u00b7schaut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als Sud und West ihn willig aufgenommen;", "tokens": ["Als", "Sud", "und", "West", "ihn", "wil\u00b7lig", "auf\u00b7ge\u00b7nom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "KON", "VVFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Er reiste hin und her", "tokens": ["Er", "reis\u00b7te", "hin", "und", "her"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Zu land und \u00fcber meer,", "tokens": ["Zu", "land", "und", "\u00fc\u00b7ber", "meer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Und war gesund vorl\u00e4ngsten wiederkommen.", "tokens": ["Und", "war", "ge\u00b7sund", "vor\u00b7l\u00e4ngs\u00b7ten", "wie\u00b7der\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADJD", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Er nahm auch drauf an gl\u00fcck und ehren zu,", "tokens": ["Er", "nahm", "auch", "drauf", "an", "gl\u00fcck", "und", "eh\u00b7ren", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PAV", "APPR", "NN", "KON", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und sa\u00df in guter ruh,", "tokens": ["Und", "sa\u00df", "in", "gu\u00b7ter", "ruh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vergn\u00fcgt in GOtt, und seiner perlen-crone;", "tokens": ["Ver\u00b7gn\u00fcgt", "in", "Gott", ",", "und", "sei\u00b7ner", "per\u00b7len\u00b7cro\u00b7ne", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$,", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Herr Berlichs hohes hau\u00df", "tokens": ["Herr", "Ber\u00b7lichs", "ho\u00b7hes", "hau\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "NE", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Gab ihm den schatz heraus,", "tokens": ["Gab", "ihm", "den", "schatz", "he\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Und GOtt vielmehr, der tugend selbst zum lohne.", "tokens": ["Und", "Gott", "viel\u00b7mehr", ",", "der", "tu\u00b7gend", "selbst", "zum", "loh\u00b7ne", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "$,", "ART", "NN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Ach ein verlust, der kaum zu sch\u00e4tzen war,", "tokens": ["Ach", "ein", "ver\u00b7lust", ",", "der", "kaum", "zu", "sch\u00e4t\u00b7zen", "war", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ART", "VVFIN", "$,", "PRELS", "ADV", "PTKZU", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Bey ihrer todten-bahr,", "tokens": ["Bey", "ih\u00b7rer", "tod\u00b7ten\u00b7bahr", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als dieser schatz ihm zeitlich war genommen,", "tokens": ["Als", "die\u00b7ser", "schatz", "ihm", "zeit\u00b7lich", "war", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "VVFIN", "PPER", "ADJD", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gott aber f\u00fchrt ihn an,", "tokens": ["Gott", "a\u00b7ber", "f\u00fchrt", "ihn", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da\u00df er kam auf die bahn,", "tokens": ["Da\u00df", "er", "kam", "auf", "die", "bahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Da er hernach dergleichen hat bekommen.", "tokens": ["Da", "er", "her\u00b7nach", "derg\u00b7lei\u00b7chen", "hat", "be\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PIS", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "In Lau\u00dfnitz liegt die sch\u00f6n\u2019 und liebe stadt,", "tokens": ["In", "Lau\u00df\u00b7nitz", "liegt", "die", "sch\u00f6n'", "und", "lie\u00b7be", "stadt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "ART", "ADJD", "KON", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mein Zittau, das ihm hat", "tokens": ["Mein", "Zit\u00b7tau", ",", "das", "ihm", "hat"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "VAFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den andern schatz, von perlen-art, gegeben;", "tokens": ["Den", "an\u00b7dern", "schatz", ",", "von", "per\u00b7len\u00b7art", ",", "ge\u00b7ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "NE", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Herr Harttichs edles hau\u00df", "tokens": ["Herr", "Hart\u00b7tichs", "ed\u00b7les", "hau\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "NE", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sieht wie der himmel aus;", "tokens": ["Sieht", "wie", "der", "him\u00b7mel", "aus", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Wer darein freyt, kan wohl auf erden leben.", "tokens": ["Wer", "da\u00b7rein", "freyt", ",", "kan", "wohl", "auf", "er\u00b7den", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PAV", "VVFIN", "$,", "VMFIN", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Ach aber! itzt sitzt sie, die Werthe Frau,", "tokens": ["Ach", "a\u00b7ber", "!", "itzt", "sitzt", "sie", ",", "die", "Wert\u00b7he", "Frau", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "$.", "ADV", "VVFIN", "PPER", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "In keiner g\u00f6ldnen au:", "tokens": ["In", "kei\u00b7ner", "g\u00f6ld\u00b7nen", "au", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Jhr ehstand ist zum bittern wehstand worden;", "tokens": ["Ihr", "eh\u00b7stand", "ist", "zum", "bit\u00b7tern", "weh\u00b7stand", "wor\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPRART", "ADJA", "NN", "VAPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die kinder neben ihr", "tokens": ["Die", "kin\u00b7der", "ne\u00b7ben", "ihr"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Beklagen f\u00fcr und f\u00fcr", "tokens": ["Be\u00b7kla\u00b7gen", "f\u00fcr", "und", "f\u00fcr"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "KON", "APPR"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Der wittwen noth und ihren w\u00e4ysen-orden.", "tokens": ["Der", "witt\u00b7wen", "noth", "und", "ih\u00b7ren", "w\u00e4y\u00b7sen\u00b7or\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Gott aber ists, der alles hat gemacht,", "tokens": ["Gott", "a\u00b7ber", "ists", ",", "der", "al\u00b7les", "hat", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VAFIN", "$,", "PRELS", "PIS", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wenn an ihn wird gedacht,", "tokens": ["Wenn", "an", "ihn", "wird", "ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "L\u00e4st sich allhier kein todtes-urtheil h\u00f6ren.", "tokens": ["L\u00e4st", "sich", "all\u00b7hier", "kein", "tod\u00b7tes\u00b7urt\u00b7heil", "h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "PIAT", "NN", "VVINF", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Es ist durch Christi blut", "tokens": ["Es", "ist", "durch", "Chris\u00b7ti", "blut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "NE", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Schon alles mit ihm gut;", "tokens": ["Schon", "al\u00b7les", "mit", "ihm", "gut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "APPR", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "So sitzen sie, wie er, in neuen ehren.", "tokens": ["So", "sit\u00b7zen", "sie", ",", "wie", "er", ",", "in", "neu\u00b7en", "eh\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "PPER", "$,", "APPR", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}