{"dta.poem.9538": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "An seine augen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr augen/ seht forthin nicht Lisimennen an.", "tokens": ["Ihr", "au\u00b7gen", "/", "seht", "for\u00b7thin", "nicht", "Li\u00b7si\u00b7men\u00b7nen", "an", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "VVFIN", "ADV", "PTKNEG", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Jhr augen/ die ich euch mir nur zur marter trage/", "tokens": ["Ihr", "au\u00b7gen", "/", "die", "ich", "euch", "mir", "nur", "zur", "mar\u00b7ter", "tra\u00b7ge", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "PRELS", "PPER", "PPER", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich schwere/ da\u00df ich euch bey Cypris thron verklage/", "tokens": ["Ich", "schwe\u00b7re", "/", "da\u00df", "ich", "euch", "bey", "Cyp\u00b7ris", "thron", "ver\u00b7kla\u00b7ge", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "PPER", "PRF", "APPR", "NE", "NE", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So fern eur lichter blitz mich nicht erretten kan/", "tokens": ["So", "fern", "eur", "lich\u00b7ter", "blitz", "mich", "nicht", "er\u00b7ret\u00b7ten", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPOSAT", "NN", "VVIMP", "PPER", "PTKNEG", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Jhr habet allzuviel mir itzt schon angethan/", "tokens": ["Ihr", "ha\u00b7bet", "all\u00b7zu\u00b7viel", "mir", "itzt", "schon", "an\u00b7ge\u00b7than", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "PPER", "ADV", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Jhr augen/ da\u00df ichs euch aus grund des hertzens sage/", "tokens": ["Ihr", "au\u00b7gen", "/", "da\u00df", "ichs", "euch", "aus", "grund", "des", "hert\u00b7zens", "sa\u00b7ge", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "KOUS", "PIS", "PPER", "APPR", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Jhr schafft mir wenig lust/ nur lauter angst und plage.", "tokens": ["Ihr", "schafft", "mir", "we\u00b7nig", "lust", "/", "nur", "lau\u00b7ter", "angst", "und", "pla\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "VVFIN", "$(", "ADV", "PIAT", "NN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ich trete nur durch euch auf diese marter-bahn.", "tokens": ["Ich", "tre\u00b7te", "nur", "durch", "euch", "auf", "die\u00b7se", "mar\u00b7ter\u00b7bahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPER", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wie ist es/ kan ich euch ihr augen nicht bezwingen?", "tokens": ["Wie", "ist", "es", "/", "kan", "ich", "euch", "ihr", "au\u00b7gen", "nicht", "be\u00b7zwin\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "$(", "VMFIN", "PPER", "PPER", "PPOSAT", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ach nein! ich kan nicht mehr/ hemt ich gleich euren blitz/", "tokens": ["Ach", "nein", "!", "ich", "kan", "nicht", "mehr", "/", "hemt", "ich", "gleich", "eu\u00b7ren", "blitz", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKANT", "$.", "PPER", "VMFIN", "PTKNEG", "ADV", "$(", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Es w\u00fcrde Lisimen sich dennoch in euch dringen/", "tokens": ["Es", "w\u00fcr\u00b7de", "Li\u00b7si\u00b7men", "sich", "den\u00b7noch", "in", "euch", "drin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NE", "PRF", "ADV", "APPR", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wo sie nicht allbereit schon in dem hertzen sitzt.", "tokens": ["Wo", "sie", "nicht", "all\u00b7be\u00b7reit", "schon", "in", "dem", "hert\u00b7zen", "sitzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PTKNEG", "ADV", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "R\u00e4cht/ r\u00e4cht ihr augen euch/ kan sie sich in euch spielen/", "tokens": ["R\u00e4cht", "/", "r\u00e4cht", "ihr", "au\u00b7gen", "euch", "/", "kan", "sie", "sich", "in", "euch", "spie\u00b7len", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "VVFIN", "PPOSAT", "NN", "PPER", "$(", "VMFIN", "PPER", "PRF", "APPR", "PPER", "VVINF", "$("], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "So lasset Lisimen auch gleiche schmertzen f\u00fchlen.", "tokens": ["So", "las\u00b7set", "Li\u00b7si\u00b7men", "auch", "glei\u00b7che", "schmert\u00b7zen", "f\u00fch\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "ADV", "ADJA", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}